      "utterance": [
        {
          "id": "MDRW2100024966.1.1",
          "form": "커피 대용으로 커피맛나는 물을 마시던데",
          "original_form": "커피 대용으로 커피맛나는 물을 마시던데",
          "speaker_id": "1",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100024966.1.2",
          "form": "그거랑 비슷한가요?",
          "original_form": "그거랑 비슷한가요?",
          "speaker_id": "1",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100024966.1.3",
          "form": "아 제가 그런거같아요...",
          "original_form": "아 제가 그런거같아요...",
          "speaker_id": "2",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100024966.1.4",
          "form": "ㅇㅅㅇ!!",
          "original_form": "ㅇㅅㅇ!!",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024966.1.5",
          "form": "커피그만마셔야하는데 연하게 타서",
          "original_form": "커피그만마셔야하는데 연하게 타서",
          "speaker_id": "2",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024966.1.6",
          "form": "물처럼 마셔요 ㅋㅋㅋㅋ",
          "original_form": "물처럼 마셔요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024966.1.7",
          "form": "아이구",
          "original_form": "아이구",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024966.1.8",
          "form": "ㅋㅋㅋㅋ그거 한번 알아보셔서",
          "original_form": "ㅋㅋㅋㅋ그거 한번 알아보셔서",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024966.1.9",
          "form": "드셔보세요 카페인도 안 들어갔대요 커피맛나는데",
          "original_form": "드셔보세요 카페인도 안 들어갔대요 커피맛나는데",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024966.1.10",
          "form": "오...좋아요 카페인 없으면 더 좋을거같아요",
          "original_form": "오...좋아요 카페인 없으면 더 좋을거같아요",
          "speaker_id": "2",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024966.1.11",
          "form": "하도 많이마셔가지고 ㅠ",
          "original_form": "하도 많이마셔가지고 ㅠ",
          "speaker_id": "2",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100024966.1.12",
          "form": "ㅠㅠ전사실",
          "original_form": "ㅠㅠ전사실",
          "speaker_id": "1",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100024966.1.13",
          "form": "카페인이 잘 안 받아가지고",
          "original_form": "카페인이 잘 안 받아가지고",
          "speaker_id": "1",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100024966.1.14",
          "form": "커피다 습관인거 같아요",
          "original_form": "커피다 습관인거 같아요",
          "speaker_id": "2",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100024966.1.15",
          "form": "커피를 더 안 좋아하는 것도 있는 것 같아요",
          "original_form": "커피를 더 안 좋아하는 것도 있는 것 같아요",
          "speaker_id": "1",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100024966.1.16",
          "form": "맞아요 그렇다더라구요",
          "original_form": "맞아요 그렇다더라구요",
          "speaker_id": "1",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100024966.1.17",
          "form": "중독...",
          "original_form": "중독...",
          "speaker_id": "1",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100024966.1.18",
          "form": "아 그쵸 ㅇㅇ 그런분들도 계시죵",
          "original_form": "아 그쵸 ㅇㅇ 그런분들도 계시죵",
          "speaker_id": "2",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100024966.1.19",
          "form": "카페인에 민감하면 막 가슴도 두근거리고 머리도 아프다던데",
          "original_form": "카페인에 민감하면 막 가슴도 두근거리고 머리도 아프다던데",
          "speaker_id": "2",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100024966.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100024966.1.21",
          "form": "머리는",
          "original_form": "머리는",
          "speaker_id": "1",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100024966.1.22",
          "form": "초콜렛 먹어도 아플 때도 있고 후",
          "original_form": "초콜렛 먹어도 아플 때도 있고 후",
          "speaker_id": "1",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100024966.1.23",
          "form": "정말 괴로워요",
          "original_form": "정말 괴로워요",
          "speaker_id": "1",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100024966.1.24",
          "form": "그럼 카페가시면 주로 뭐드세요??",
          "original_form": "그럼 카페가시면 주로 뭐드세요??",
          "speaker_id": "2",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100024966.1.25",
          "form": "저 디카페인 있는 카페면 디카페인으로 먹는데",
          "original_form": "저 디카페인 있는 카페면 디카페인으로 먹는데",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024966.1.26",
          "form": "별로 없으니까",
          "original_form": "별로 없으니까",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024966.1.27",
          "form": "초딩마냥 과일쥬스를 마십니다",
          "original_form": "초딩마냥 과일쥬스를 마십니다",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024966.1.28",
          "form": "아아 근데 진짜 요샌 별거별거에 다 카페인이 들어가서",
          "original_form": "아아 근데 진짜 요샌 별거별거에 다 카페인이 들어가서",
          "speaker_id": "2",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100024966.1.29",
          "form": "맞아여ㅠㅠ",
          "original_form": "맞아여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100024966.1.30",
          "form": "과일주스아니면 진짜 대안이 별로 없겠어요 ㅠ",
          "original_form": "과일주스아니면 진짜 대안이 별로 없겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100024966.1.31",
          "form": "근데 보통 또 카페에서는",
          "original_form": "근데 보통 또 카페에서는",
          "speaker_id": "1",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100024966.1.32",
          "form": "생과일 갈아주는 게 아니라 시럽 넣어서 탄산넣고 에이드 파는 집이 많아서",
          "original_form": "생과일 갈아주는 게 아니라 시럽 넣어서 탄산넣고 에이드 파는 집이 많아서",
          "speaker_id": "1",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024966.1.33",
          "form": "넘 슬퍼용...",
          "original_form": "넘 슬퍼용...",
          "speaker_id": "1",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024966.1.34",
          "form": "스무디 이런 건 생과일 들어가는 종류가 많던데",
          "original_form": "스무디 이런 건 생과일 들어가는 종류가 많던데",
          "speaker_id": "1",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024966.1.35",
          "form": "꼴에 또 요거트도 안 좋아해서 안 먹어요ㅠ",
          "original_form": "꼴에 또 요거트도 안 좋아해서 안 먹어요ㅠ",
          "speaker_id": "1",
          "time": "20210831 21:34"
        }
      ]
    }
