      "utterance": [
        {
          "id": "MDRW2100018045.1.1",
          "form": "tv보는거 좋아하세요?",
          "original_form": "tv보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018045.1.2",
          "form": "네! 여행프로들 좋아해요",
          "original_form": "네! 여행프로들 좋아해요",
          "speaker_id": "2",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100018045.1.3",
          "form": "백종원 스트리트 푸드 파이터 재밌게 봤어요 ㅋㅋㅋ",
          "original_form": "백종원 스트리트 푸드 파이터 재밌게 봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100018045.1.4",
          "form": "name1은 어떤 프로 좋아하세요?",
          "original_form": "&name1&은 어떤 프로 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100018045.1.5",
          "form": "저는 드라마를 자주봐요!",
          "original_form": "저는 드라마를 자주봐요!",
          "speaker_id": "1",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100018045.1.6",
          "form": "최근에 보는 드라마 있으세요?",
          "original_form": "최근에 보는 드라마 있으세요?",
          "speaker_id": "2",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100018045.1.7",
          "form": "보이스4 재밌게 봤고",
          "original_form": "보이스4 재밌게 봤고",
          "speaker_id": "1",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100018045.1.8",
          "form": "속아도 꿈결이 재밌네요ㅋㅋ",
          "original_form": "속아도 꿈결이 재밌네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100018045.1.9",
          "form": "드라마 보시나요?",
          "original_form": "드라마 보시나요?",
          "speaker_id": "1",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100018045.1.10",
          "form": "전 마지막으로 봤던 드라마가 오월의 청춘인데",
          "original_form": "전 마지막으로 봤던 드라마가 오월의 청춘인데",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100018045.1.11",
          "form": "제가 막 챙겨보는 편이 아니라 중간중간만 봤어요",
          "original_form": "제가 막 챙겨보는 편이 아니라 중간중간만 봤어요",
          "speaker_id": "2",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100018045.1.12",
          "form": "드라마는 전부 챙겨보는게 은근 힘들어요",
          "original_form": "드라마는 전부 챙겨보는게 은근 힘들어요",
          "speaker_id": "1",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100018045.1.13",
          "form": "그쵸 요즘은 그래서 웨이브나 티빙도 같이 쓰고있어요",
          "original_form": "그쵸 요즘은 그래서 웨이브나 티빙도 같이 쓰고있어요",
          "speaker_id": "2",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100018045.1.14",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "1",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100018045.1.15",
          "form": "웨이브를 자주보는데 해외꺼도 볼 수 있어서 편하고 좋아요",
          "original_form": "웨이브를 자주보는데 해외꺼도 볼 수 있어서 편하고 좋아요",
          "speaker_id": "1",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100018045.1.16",
          "form": "그쵸 처음에는 넷플릭스로 입문했는데",
          "original_form": "그쵸 처음에는 넷플릭스로 입문했는데",
          "speaker_id": "2",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018045.1.17",
          "form": "생각보다 없는게 많아서 오히려 지금은 티빙이랑 웨이브를 더 많이 쓰는거같아요",
          "original_form": "생각보다 없는게 많아서 오히려 지금은 티빙이랑 웨이브를 더 많이 쓰는거같아요",
          "speaker_id": "2",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018045.1.18",
          "form": "맞아요 넷플릭스는 많은데 뭔가 없어요",
          "original_form": "맞아요 넷플릭스는 많은데 뭔가 없어요",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018045.1.19",
          "form": "그쵸 ㅋㅋㅋㅋ 정작 볼 거리는 얼마 안되는거같아요",
          "original_form": "그쵸 ㅋㅋㅋㅋ 정작 볼 거리는 얼마 안되는거같아요",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018045.1.20",
          "form": "진짜요ㅋㅋ 왜지? 싶을정도로 많은데 말이죠ㅋㅋ",
          "original_form": "진짜요ㅋㅋ 왜지? 싶을정도로 많은데 말이죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100018045.1.21",
          "form": "웨이브가 더 볼게 많은데 보고싶은건 또 없어요..",
          "original_form": "웨이브가 더 볼게 많은데 보고싶은건 또 없어요..",
          "speaker_id": "1",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100018045.1.22",
          "form": "다들 인기있는거 하나씩은 빼놓고 서비스하는거같아요 ㅋㅋㅋ",
          "original_form": "다들 인기있는거 하나씩은 빼놓고 서비스하는거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018045.1.23",
          "form": "이러다가 왓챠까지 다 가입하게 생겼어요",
          "original_form": "이러다가 왓챠까지 다 가입하게 생겼어요",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018045.1.24",
          "form": "진짜요ㅋㅋ 다들 쇼부 본 거 같은 느낌이여요",
          "original_form": "진짜요ㅋㅋ 다들 쇼부 본 거 같은 느낌이여요",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018045.1.25",
          "form": "맘같아서는 다 가입해두고 싶어요ㅋㅋ",
          "original_form": "맘같아서는 다 가입해두고 싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:32"
        }
