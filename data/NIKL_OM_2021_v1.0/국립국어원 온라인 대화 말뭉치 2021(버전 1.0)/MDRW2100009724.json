      "utterance": [
        {
          "id": "MDRW2100009724.1.1",
          "form": "반려동물 혹시 키우시나요",
          "original_form": "반려동물 혹시 키우시나요",
          "speaker_id": "1",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100009724.1.2",
          "form": "네 강아지요.ㅜㅜ",
          "original_form": "네 강아지요.ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 21:02"
        },
        {
          "id": "MDRW2100009724.1.3",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210824 21:02"
        },
        {
          "id": "MDRW2100009724.1.4",
          "form": "네 두마리 키우고 있어요.",
          "original_form": "네 두마리 키우고 있어요.",
          "speaker_id": "1",
          "time": "20210824 21:02"
        },
        {
          "id": "MDRW2100009724.1.5",
          "form": "무슨 종인가요",
          "original_form": "무슨 종인가요",
          "speaker_id": "1",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100009724.1.6",
          "form": "믹스에요. 믹스는 똑똑이사랑입니다",
          "original_form": "믹스에요. 믹스는 똑똑이사랑입니다",
          "speaker_id": "2",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100009724.1.7",
          "form": "그쵸. 믹스가 건강해요",
          "original_form": "그쵸. 믹스가 건강해요",
          "speaker_id": "1",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100009724.1.8",
          "form": "섞인만큼 귀엽기도 더 귀엽고",
          "original_form": "섞인만큼 귀엽기도 더 귀엽고",
          "speaker_id": "1",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100009724.1.9",
          "form": "몇마리 키우세요?",
          "original_form": "몇마리 키우세요?",
          "speaker_id": "1",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100009724.1.10",
          "form": "앗 저도 두마리에요",
          "original_form": "앗 저도 두마리에요",
          "speaker_id": "2",
          "time": "20210824 21:07"
        },
        {
          "id": "MDRW2100009724.1.11",
          "form": "역시 한마리는 조금 외롭달까?",
          "original_form": "역시 한마리는 조금 외롭달까?",
          "speaker_id": "2",
          "time": "20210824 21:07"
        },
        {
          "id": "MDRW2100009724.1.12",
          "form": "근데 또 둘이 친한듯 안친한듯 ㅋㅋㅋㅋ",
          "original_form": "근데 또 둘이 친한듯 안친한듯 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:07"
        },
        {
          "id": "MDRW2100009724.1.13",
          "form": "ㅎㅎ 안 싸우면 친한 거지요",
          "original_form": "ㅎㅎ 안 싸우면 친한 거지요",
          "speaker_id": "1",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100009724.1.14",
          "form": "우리집 강아지들도 소닭 관계 같아요",
          "original_form": "우리집 강아지들도 소닭 관계 같아요",
          "speaker_id": "1",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100009724.1.15",
          "form": "아이들은 사료 잘 먹나요",
          "original_form": "아이들은 사료 잘 먹나요",
          "speaker_id": "1",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100009724.1.16",
          "form": "거의 애원하죠. 강아지들은 예쁨 받으려고 사료 먹는 느낌이에요ㅋㅋ",
          "original_form": "거의 애원하죠. 강아지들은 예쁨 받으려고 사료 먹는 느낌이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100009724.1.17",
          "form": "ㅎㅎ 기호성 좋은 사료 어디 없나요",
          "original_form": "ㅎㅎ 기호성 좋은 사료 어디 없나요",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100009724.1.18",
          "form": "사료로 인식하는 순간.. 맛있는 사료는 없어지는 거 같아요ㅋㅋ",
          "original_form": "사료로 인식하는 순간.. 맛있는 사료는 없어지는 거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100009724.1.19",
          "form": "그쵸ㅜ 샘플 잘 먹는다고 방심하면 안되더라고요",
          "original_form": "그쵸ㅜ 샘플 잘 먹는다고 방심하면 안되더라고요",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100009724.1.20",
          "form": "그래도 추천할 사료 없으신가요",
          "original_form": "그래도 추천할 사료 없으신가요",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100009724.1.21",
          "form": "제스트 사료 조금 좋아했어요",
          "original_form": "제스트 사료 조금 좋아했어요",
          "speaker_id": "2",
          "time": "20210826 00:49"
        },
        {
          "id": "MDRW2100009724.1.22",
          "form": "직접 먹어봤는데 약간 초콜렛 맛 났어요",
          "original_form": "직접 먹어봤는데 약간 초콜렛 맛 났어요",
          "speaker_id": "2",
          "time": "20210826 00:49"
        },
        {
          "id": "MDRW2100009724.1.23",
          "form": "오오 한번 먹여봐야겠어요",
          "original_form": "오오 한번 먹여봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100009724.1.24",
          "form": "초콜릿 맛이라니 궁금하네요",
          "original_form": "초콜릿 맛이라니 궁금하네요",
          "speaker_id": "1",
          "time": "20210826 00:51"
        }
      ]
    }
