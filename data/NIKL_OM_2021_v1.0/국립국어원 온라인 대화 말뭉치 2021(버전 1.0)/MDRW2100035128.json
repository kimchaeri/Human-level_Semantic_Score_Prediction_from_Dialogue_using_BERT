      "utterance": [
        {
          "id": "MDRW2100035128.1.1",
          "form": "최근껀 뭐있으세요??",
          "original_form": "최근껀 뭐있으세요??",
          "speaker_id": "2",
          "time": "20210829 03:37"
        },
        {
          "id": "MDRW2100035128.1.2",
          "form": "재밌는거 좋아하시면",
          "original_form": "재밌는거 좋아하시면",
          "speaker_id": "1",
          "time": "20210829 03:37"
        },
        {
          "id": "MDRW2100035128.1.3",
          "form": "드라마는 아니고 영환데",
          "original_form": "드라마는 아니고 영환데",
          "speaker_id": "1",
          "time": "20210829 03:37"
        },
        {
          "id": "MDRW2100035128.1.4",
          "form": "스파이랑 \r\n나를 차버린 스파이",
          "original_form": "스파이랑 \r\n나를 차버린 스파이",
          "speaker_id": "1",
          "time": "20210829 03:37"
        },
        {
          "id": "MDRW2100035128.1.5",
          "form": "이렇게 두개 추천드려용",
          "original_form": "이렇게 두개 추천드려용",
          "speaker_id": "1",
          "time": "20210829 03:37"
        },
        {
          "id": "MDRW2100035128.1.6",
          "form": "어 ㅋㅋㅋ",
          "original_form": "어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:37"
        },
        {
          "id": "MDRW2100035128.1.7",
          "form": "이거 2에요?",
          "original_form": "이거 2에요?",
          "speaker_id": "2",
          "time": "20210829 03:37"
        },
        {
          "id": "MDRW2100035128.1.8",
          "form": "보셨아용?ㅋㅋㅋ",
          "original_form": "보셨아용?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:37"
        },
        {
          "id": "MDRW2100035128.1.9",
          "form": "요새영화에요?",
          "original_form": "요새영화에요?",
          "speaker_id": "2",
          "time": "20210829 03:37"
        },
        {
          "id": "MDRW2100035128.1.10",
          "form": "둘다 2는 안나왔구 단편이에용!",
          "original_form": "둘다 2는 안나왔구 단편이에용!",
          "speaker_id": "1",
          "time": "20210829 03:37"
        },
        {
          "id": "MDRW2100035128.1.11",
          "form": "본거같아서 ㅋㅋㅋ",
          "original_form": "본거같아서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:37"
        },
        {
          "id": "MDRW2100035128.1.12",
          "form": "음 스파이라는 영화는 15년도쯤 나왔구",
          "original_form": "음 스파이라는 영화는 15년도쯤 나왔구",
          "speaker_id": "1",
          "time": "20210829 03:38"
        },
        {
          "id": "MDRW2100035128.1.13",
          "form": "나를 차버린 스파이",
          "original_form": "나를 차버린 스파이",
          "speaker_id": "2",
          "time": "20210829 03:38"
        },
        {
          "id": "MDRW2100035128.1.14",
          "form": "나를 차버린 스파이는 19년도??",
          "original_form": "나를 차버린 스파이는 19년도??",
          "speaker_id": "1",
          "time": "20210829 03:38"
        },
        {
          "id": "MDRW2100035128.1.15",
          "form": "둘이 유럽가서 유럽 배낭여행하는 여자들 여권뺏는 장면이 있는데",
          "original_form": "둘이 유럽가서 유럽 배낭여행하는 여자들 여권뺏는 장면이 있는데",
          "speaker_id": "1",
          "time": "20210829 03:38"
        },
        {
          "id": "MDRW2100035128.1.16",
          "form": "너무 웃겨욬ㅋㅋㅋ",
          "original_form": "너무 웃겨욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:38"
        },
        {
          "id": "MDRW2100035128.1.17",
          "form": "포스터찾아보니깐",
          "original_form": "포스터찾아보니깐",
          "speaker_id": "2",
          "time": "20210829 03:39"
        },
        {
          "id": "MDRW2100035128.1.18",
          "form": "안본거같아요 ㅋㅋㅋ봐야겟어요 ㄹㅎ",
          "original_form": "안본거같아요 ㅋㅋㅋ봐야겟어요 ㄹㅎ",
          "speaker_id": "2",
          "time": "20210829 03:39"
        },
        {
          "id": "MDRW2100035128.1.19",
          "form": "둘다 추천드립니당ㅋㅋㅋ 가볍게 보기 좋아용",
          "original_form": "둘다 추천드립니당ㅋㅋㅋ 가볍게 보기 좋아용",
          "speaker_id": "1",
          "time": "20210829 03:39"
        },
        {
          "id": "MDRW2100035128.1.20",
          "form": "킬러의보디가드같이",
          "original_form": "킬러의보디가드같이",
          "speaker_id": "1",
          "time": "20210829 03:39"
        },
        {
          "id": "MDRW2100035128.1.21",
          "form": "넵냅 !!",
