      "utterance": [
        {
          "id": "MDRW2100010447.1.1",
          "form": "다른 장르도 즐겨보시는 거 있으신가요?",
          "original_form": "다른 장르도 즐겨보시는 거 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 18:21"
        },
        {
          "id": "MDRW2100010447.1.2",
          "form": "소설도 많이 보는데",
          "original_form": "소설도 많이 보는데",
          "speaker_id": "2",
          "time": "20210823 18:22"
        },
        {
          "id": "MDRW2100010447.1.3",
          "form": "대받 어떤거 보시나요",
          "original_form": "대받 어떤거 보시나요",
          "speaker_id": "1",
          "time": "20210823 18:22"
        },
        {
          "id": "MDRW2100010447.1.4",
          "form": "웹소는 현대 판타지나 로판 많이 봐요",
          "original_form": "웹소는 현대 판타지나 로판 많이 봐요",
          "speaker_id": "2",
          "time": "20210823 18:23"
        },
        {
          "id": "MDRW2100010447.1.5",
          "form": "그냥 소설도 괜찮아요",
          "original_form": "그냥 소설도 괜찮아요",
          "speaker_id": "1",
          "time": "20210823 18:23"
        },
        {
          "id": "MDRW2100010447.1.6",
          "form": "달러구트의 꿈 백화점",
          "original_form": "달러구트의 꿈 백화점",
          "speaker_id": "1",
          "time": "20210823 18:23"
        },
        {
          "id": "MDRW2100010447.1.7",
          "form": "얼마전에 e북으로 서비스하길래",
          "original_form": "얼마전에 e북으로 서비스하길래",
          "speaker_id": "1",
          "time": "20210823 18:23"
        },
        {
          "id": "MDRW2100010447.1.8",
          "form": "달리기 시작했어요",
          "original_form": "달리기 시작했어요",
          "speaker_id": "1",
          "time": "20210823 18:23"
        },
        {
          "id": "MDRW2100010447.1.9",
          "form": "장르가 어떻게 되나요?",
          "original_form": "장르가 어떻게 되나요?",
          "speaker_id": "2",
          "time": "20210823 18:23"
        },
        {
          "id": "MDRW2100010447.1.10",
          "form": "그거 판타지일겁니다!!",
          "original_form": "그거 판타지일겁니다!!",
          "speaker_id": "1",
          "time": "20210823 18:23"
        },
        {
          "id": "MDRW2100010447.1.11",
          "form": "호평받아서 달리기 시작했어요",
          "original_form": "호평받아서 달리기 시작했어요",
          "speaker_id": "1",
          "time": "20210823 18:23"
        },
        {
          "id": "MDRW2100010447.1.12",
          "form": "헉 한 번 찾아봐야겠어요",
          "original_form": "헉 한 번 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210823 18:23"
        },
        {
          "id": "MDRW2100010447.1.13",
          "form": "좋아하는 소설 있으세요?!",
          "original_form": "좋아하는 소설 있으세요?!",
          "speaker_id": "1",
          "time": "20210823 18:24"
        },
        {
          "id": "MDRW2100010447.1.14",
          "form": "종이책으로는 소설 책 많이 읽었어요",
          "original_form": "종이책으로는 소설 책 많이 읽었어요",
          "speaker_id": "2",
          "time": "20210823 18:24"
        },
        {
          "id": "MDRW2100010447.1.15",
          "form": "히가시노 게이고 작품 좋아해요",
          "original_form": "히가시노 게이고 작품 좋아해요",
          "speaker_id": "2",
          "time": "20210823 18:24"
        },
        {
          "id": "MDRW2100010447.1.16",
          "form": "아니 추리 소설이요!",
          "original_form": "아니 추리 소설이요!",
          "speaker_id": "2",
          "time": "20210823 18:24"
        },
        {
          "id": "MDRW2100010447.1.17",
          "form": "허헙!!!!",
          "original_form": "허헙!!!!",
          "speaker_id": "1",
          "time": "20210823 18:24"
        },
        {
          "id": "MDRW2100010447.1.18",
          "form": "추리소설!!!!",
          "original_form": "추리소설!!!!",
          "speaker_id": "1",
          "time": "20210823 18:24"
        },
        {
          "id": "MDRW2100010447.1.19",
          "form": "그냥 소설이라고만 보냈네요ㅋㅋㅋ",
          "original_form": "그냥 소설이라고만 보냈네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:24"
        },
        {
          "id": "MDRW2100010447.1.20",
          "form": "추리 소설 좋아하시나요?",
          "original_form": "추리 소설 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210823 18:24"
        },
        {
          "id": "MDRW2100010447.1.21",
          "form": "저 추리..어 미스터리도 추리에 포함될까요",
          "original_form": "저 추리..어 미스터리도 추리에 포함될까요",
          "speaker_id": "1",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010447.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010447.1.23",
          "form": "미스터리 책도 좋아해요",
          "original_form": "미스터리 책도 좋아해요",
          "speaker_id": "2",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010447.1.24",
          "form": "재밌는 작품 있나요?",
          "original_form": "재밌는 작품 있나요?",
          "speaker_id": "2",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010447.1.25",
          "form": "지금 생각나는건",
          "original_form": "지금 생각나는건",
          "speaker_id": "1",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010447.1.26",
          "form": "그 별이지던 밤인가",
          "original_form": "그 별이지던 밤인가",
          "speaker_id": "1",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010447.1.27",
          "form": "그 장례식 때 쪽지를 발견했는데",
          "original_form": "그 장례식 때 쪽지를 발견했는데",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010447.1.28",
          "form": "딸을 해친 범인이 따로 있다고",
          "original_form": "딸을 해친 범인이 따로 있다고",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010447.1.29",
          "form": "아나 나쁜말이람서 안보내지네여",
          "original_form": "아나 나쁜말이람서 안보내지네여",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010447.1.30",
          "form": "그래서 아빠가 범인찾는데",
          "original_form": "그래서 아빠가 범인찾는데",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010447.1.31",
          "form": "범인 누군지 뻔한데 감정선이 좋았어요",
          "original_form": "범인 누군지 뻔한데 감정선이 좋았어요",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010447.1.32",
          "form": "당신의 별이 사라지던 밤",
          "original_form": "당신의 별이 사라지던 밤",
          "speaker_id": "2",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100010447.1.33",
          "form": "이건 가요?",
          "original_form": "이건 가요?",
          "speaker_id": "2",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100010447.1.34",
          "form": "네네넵",
          "original_form": "네네넵",
          "speaker_id": "1",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100010447.1.35",
          "form": "마장요!!!",
          "original_form": "마장요!!!",
          "speaker_id": "1",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100010447.1.36",
          "form": "오 재밌을 거 같아요",
          "original_form": "오 재밌을 거 같아요",
          "speaker_id": "2",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100010447.1.37",
          "form": "추리 소설 읽다보시면",
          "original_form": "추리 소설 읽다보시면",
          "speaker_id": "1",
          "time": "20210823 18:27"
        }
      ]
