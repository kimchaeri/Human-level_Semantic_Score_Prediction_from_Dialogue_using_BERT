      "utterance": [
        {
          "id": "MDRW2100003866.1.1",
          "form": "운동은 가끔 링피트 하고 산책만 해요.",
          "original_form": "운동은 가끔 링피트 하고 산책만 해요.",
          "speaker_id": "2",
          "time": "20210821 13:06"
        },
        {
          "id": "MDRW2100003866.1.2",
          "form": "링피트는 머죠",
          "original_form": "링피트는 머죠",
          "speaker_id": "1",
          "time": "20210821 13:06"
        },
        {
          "id": "MDRW2100003866.1.3",
          "form": "닌텐도 게임사에서 운동 좀 하라고 내 놓은 운동 게임이에요.",
          "original_form": "닌텐도 게임사에서 운동 좀 하라고 내 놓은 운동 게임이에요.",
          "speaker_id": "2",
          "time": "20210821 13:07"
        },
        {
          "id": "MDRW2100003866.1.4",
          "form": "아 광고봤어요",
          "original_form": "아 광고봤어요",
          "speaker_id": "1",
          "time": "20210821 13:07"
        },
        {
          "id": "MDRW2100003866.1.5",
          "form": "근력운동 보다는 유산소라는데 저는 운동면에서는 팔방으로 다 부족하기때문인지 정말 30분만 해도 힘들더라구요.",
          "original_form": "근력운동 보다는 유산소라는데 저는 운동면에서는 팔방으로 다 부족하기때문인지 정말 30분만 해도 힘들더라구요.",
          "speaker_id": "2",
          "time": "20210821 13:16"
        },
        {
          "id": "MDRW2100003866.1.6",
          "form": "저는 홈트해요",
          "original_form": "저는 홈트해요",
          "speaker_id": "1",
          "time": "20210821 13:17"
        },
        {
          "id": "MDRW2100003866.1.7",
          "form": "와 홈트는 보통 어떤 루틴으로 하세요?",
          "original_form": "와 홈트는 보통 어떤 루틴으로 하세요?",
          "speaker_id": "2",
          "time": "20210821 13:17"
        },
        {
          "id": "MDRW2100003866.1.8",
          "form": "제가 헬스 문외한이라 시작도 못하겠더라구요.",
          "original_form": "제가 헬스 문외한이라 시작도 못하겠더라구요.",
          "speaker_id": "2",
          "time": "20210821 13:18"
        },
        {
          "id": "MDRW2100003866.1.9",
          "form": "유투브 보면서 해요",
          "original_form": "유투브 보면서 해요",
          "speaker_id": "1",
          "time": "20210821 13:18"
        },
        {
          "id": "MDRW2100003866.1.10",
          "form": "추천해주실수 있나요?",
          "original_form": "추천해주실수 있나요?",
          "speaker_id": "2",
          "time": "20210821 13:18"
        },
        {
          "id": "MDRW2100003866.1.11",
          "form": "땅크봐요",
          "original_form": "땅크봐요",
          "speaker_id": "1",
          "time": "20210821 13:19"
        },
        {
          "id": "MDRW2100003866.1.12",
          "form": "감사합니다. 저도 저녁에 보러가야겠어요.",
          "original_form": "감사합니다. 저도 저녁에 보러가야겠어요.",
          "speaker_id": "2",
          "time": "20210821 13:28"
        },
        {
          "id": "MDRW2100003866.1.13",
          "form": "잼나요",
          "original_form": "잼나요",
          "speaker_id": "1",
          "time": "20210821 13:28"
        },
        {
          "id": "MDRW2100003866.1.14",
          "form": "꾸준히 해야죠",
          "original_form": "꾸준히 해야죠",
          "speaker_id": "1",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100003866.1.15",
          "form": "지금도",
          "original_form": "지금도",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100003866.1.16",
          "form": "운동은 정말 꾸준히 하는게 관건인 것 같아요.",
          "original_form": "운동은 정말 꾸준히 하는게 관건인 것 같아요.",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100003866.1.17",
          "form": "주말만 되면 결심이 무너집니다...",
          "original_form": "주말만 되면 결심이 무너집니다...",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100003866.1.18",
          "form": "ㅎㅎ 그렇죠",
          "original_form": "ㅎㅎ 그렇죠",
          "speaker_id": "1",
          "time": "20210821 22:22"
        }
      ]
