        },
        {
          "id": "MDRW2100014003.1.2",
          "form": "말해봐요",
          "original_form": "말해봐요",
          "speaker_id": "1",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100014003.1.3",
          "form": "닭을 키우다니",
          "original_form": "닭을 키우다니",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014003.1.4",
          "form": "복날에 삼계탕을 먹으러 갔는대요",
          "original_form": "복날에 삼계탕을 먹으러 갔는대요",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014003.1.5",
          "form": "으잉",
          "original_form": "으잉",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014003.1.6",
          "form": "먹고 나오니까 차도에 병아리와 닭사이의 못생긴게",
          "original_form": "먹고 나오니까 차도에 병아리와 닭사이의 못생긴게",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014003.1.7",
          "form": "있더라고요",
          "original_form": "있더라고요",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014003.1.8",
          "form": "그래서 위험하니까 우선 잡아서",
          "original_form": "그래서 위험하니까 우선 잡아서",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014003.1.9",
          "form": "삼계탕집거냐고 물어봤는대",
          "original_form": "삼계탕집거냐고 물어봤는대",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014003.1.10",
          "form": "설마",
          "original_form": "설마",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014003.1.11",
          "form": "먼소리냐고 우리는 죽은거밖에 취급안한다하는데",
          "original_form": "먼소리냐고 우리는 죽은거밖에 취급안한다하는데",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014003.1.12",
          "form": "그래서 데려왔죠",
          "original_form": "그래서 데려왔죠",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014003.1.13",
          "form": "어디서온거지 그럼",
          "original_form": "어디서온거지 그럼",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014003.1.14",
          "form": "그래서요",
          "original_form": "그래서요",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014003.1.15",
          "form": "근데 저희는 그 암탉인줄알고 데려왔거든요",
          "original_form": "근데 저희는 그 암탉인줄알고 데려왔거든요",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014003.1.16",
          "form": "모르겠어요",
          "original_form": "모르겠어요",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014003.1.17",
          "form": "근데 수컷인거에요",
          "original_form": "근데 수컷인거에요",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014003.1.18",
          "form": "멋지겠네",
          "original_form": "멋지겠네",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100014003.1.19",
          "form": "닭 얻으려다가 모닝콜만 얻은거죠",
          "original_form": "닭 얻으려다가 모닝콜만 얻은거죠",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100014003.1.20",
          "form": "ㅋㅋㅋㅋ 시끄럽던가요",
          "original_form": "ㅋㅋㅋㅋ 시끄럽던가요",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100014003.1.21",
          "form": "3시부터 우는데",
          "original_form": "3시부터 우는데",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100014003.1.22",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100014003.1.23",
          "form": "근데 또 못울게할수는 없어서",
