      "utterance": [
        {
          "id": "MDRW2100002742.1.1",
          "form": "너는 악동뮤지션 노래 들어봤니",
          "original_form": "너는 악동뮤지션 노래 들어봤니",
          "speaker_id": "1",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002742.1.2",
          "form": "신곡이 나온더야?",
          "original_form": "신곡이 나온더야?",
          "speaker_id": "2",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002742.1.3",
          "form": "응 신곡이라고 하기엔",
          "original_form": "응 신곡이라고 하기엔",
          "speaker_id": "1",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002742.1.4",
          "form": "난 아직 들어본 적 없어.",
          "original_form": "난 아직 들어본 적 없어.",
          "speaker_id": "2",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002742.1.5",
          "form": "좋야?",
          "original_form": "좋야?",
          "speaker_id": "2",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002742.1.6",
          "form": "저번달이긴해",
          "original_form": "저번달이긴해",
          "speaker_id": "1",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002742.1.7",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "2",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002742.1.8",
          "form": "전혀 몰랐어",
          "original_form": "전혀 몰랐어",
          "speaker_id": "2",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002742.1.9",
          "form": "아니 나는 옛날스타일이 더 좋은 것 같아",
          "original_form": "아니 나는 옛날스타일이 더 좋은 것 같아",
          "speaker_id": "1",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002742.1.10",
          "form": "나는 꽤 그들을 좋아한다고 생각했었어",
          "original_form": "나는 꽤 그들을 좋아한다고 생각했었어",
          "speaker_id": "2",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002742.1.11",
          "form": "나도 한번 들어봐야재",
          "original_form": "나도 한번 들어봐야재",
          "speaker_id": "2",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002742.1.12",
          "form": "구래도 새로운 느낌이니까 들업하!",
          "original_form": "구래도 새로운 느낌이니까 들업하!",
          "speaker_id": "1",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002742.1.13",
          "form": "그래서 그들이 방송에 많이 나오ㅑㅆ던거구나",
          "original_form": "그래서 그들이 방송에 많이 나오ㅑㅆ던거구나",
          "speaker_id": "2",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002742.1.14",
          "form": "그래!",
          "original_form": "그래!",
          "speaker_id": "2",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002742.1.15",
          "form": "들어보고 후기를 알려주ㅗ",
          "original_form": "들어보고 후기를 알려주ㅗ",
          "speaker_id": "1",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002742.1.16",
          "form": "알겠어. 기대된다",
          "original_form": "알겠어. 기대된다",
          "speaker_id": "2",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002742.1.17",
          "form": "아 홍보하러 나왔구나",
          "original_form": "아 홍보하러 나왔구나",
          "speaker_id": "1",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002742.1.18",
          "form": "나는 그들의 엘피도 갖고싶어",
          "original_form": "나는 그들의 엘피도 갖고싶어",
          "speaker_id": "2",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002742.1.19",
          "form": "역시 가수들은 컴백하면 티비에서 홍보하는군",
          "original_form": "역시 가수들은 컴백하면 티비에서 홍보하는군",
          "speaker_id": "1",
          "time": "20210820 15:14"
        }
      ]
    }
