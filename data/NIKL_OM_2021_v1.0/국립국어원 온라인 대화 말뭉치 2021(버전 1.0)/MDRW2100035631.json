      "utterance": [
        {
          "id": "MDRW2100035631.1.1",
          "form": "근데 왠만한 사람들은 이미 다 보고간듯 ㅋㅋㅋ",
          "original_form": "근데 왠만한 사람들은 이미 다 보고간듯 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100035631.1.2",
          "form": "ㅇㅇ동네사람들 다 보고간듯",
          "original_form": "ㅇㅇ동네사람들 다 보고간듯",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100035631.1.3",
          "form": "집토끼 name1님도 보고갔으니 ㅋㅋ",
          "original_form": "집토끼 &name1&님도 보고갔으니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100035631.1.4",
          "form": "ㅇㅇ딱 9/1되면 완전 한산할듯 ㅋㅋ",
          "original_form": "ㅇㅇ딱 9/1되면 완전 한산할듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035631.1.5",
          "form": "지자체 힘내라ㅜㅜ",
          "original_form": "지자체 힘내라ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100035631.1.6",
          "form": "ㅋㅋ유료화의 힘",
          "original_form": "ㅋㅋ유료화의 힘",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035631.1.7",
          "form": "나중에 검색한번해봐요",
          "original_form": "나중에 검색한번해봐요",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035631.1.8",
          "form": "대왕암 공원 출렁다리임 ㅋ",
          "original_form": "대왕암 공원 출렁다리임 ㅋ",
          "speaker_id": "1",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100035631.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100035631.1.10",
          "form": "출렁다리라니 안 안전해보이는데요",
          "original_form": "출렁다리라니 안 안전해보이는데요",
          "speaker_id": "2",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100035631.1.11",
          "form": "제법 그럴듯함",
          "original_form": "제법 그럴듯함",
          "speaker_id": "1",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100035631.1.12",
          "form": "담력시험하러 친구들올듯",
          "original_form": "담력시험하러 친구들올듯",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035631.1.13",
          "form": "사진들도 다들 멋있게 찍으셨더랍",
          "original_form": "사진들도 다들 멋있게 찍으셨더랍",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035631.1.14",
          "form": "ㅋㅋ부럽다 잘찍는사람",
          "original_form": "ㅋㅋ부럽다 잘찍는사람",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035631.1.15",
          "form": "그정도는 아님 ㅋㅋ담력정도는",
          "original_form": "그정도는 아님 ㅋㅋ담력정도는",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100035631.1.16",
          "form": "애기들도 걸음 ㅋㅋ",
          "original_form": "애기들도 걸음 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100035631.1.17",
          "form": "우아",
          "original_form": "우아",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100035631.1.18",
          "form": "애기들도ㅋㅋ 안전하네",
          "original_form": "애기들도ㅋㅋ 안전하네",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100035631.1.19",
          "form": "ㅇㅇ진짜 위험하면 나이제한이나 키제한이 있었을듭",
          "original_form": "ㅇㅇ진짜 위험하면 나이제한이나 키제한이 있었을듭",
          "speaker_id": "1",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100035631.1.20",
          "form": "유원지 기구도아니고ㅋㅋ",
          "original_form": "유원지 기구도아니고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100035631.1.21",
          "form": "담에 풍경사진 인증 부탁함니다",
          "original_form": "담에 풍경사진 인증 부탁함니다",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100035631.1.22",
          "form": "아니 난거기서 그거못하겠던데",
          "original_form": "아니 난거기서 그거못하겠던데",
