      "utterance": [
        {
          "id": "MDRW2100035232.1.1",
          "form": "남궁민이 남주 박하선이 여주다",
          "original_form": "남궁민이 남주 박하선이 여주다",
          "speaker_id": "2",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100035232.1.2",
          "form": "이번에 좀 드라마 떠야 될껀데, 엠비씨 계속 망함",
          "original_form": "이번에 좀 드라마 떠야 될껀데, 엠비씨 계속 망함",
          "speaker_id": "1",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100035232.1.3",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100035232.1.4",
          "form": "박하선 사격 잘하네",
          "original_form": "박하선 사격 잘하네",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100035232.1.5",
          "form": "그렇다",
          "original_form": "그렇다",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100035232.1.6",
          "form": "매니저 너무 떠는데 10점이네",
          "original_form": "매니저 너무 떠는데 10점이네",
          "speaker_id": "1",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100035232.1.7",
          "form": "ㅋㅋ군대까지 포병으로  갔다왓는데 더 못하면 좀 그렇겠지",
          "original_form": "ㅋㅋ군대까지 포병으로  갔다왓는데 더 못하면 좀 그렇겠지",
          "speaker_id": "2",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100035232.1.8",
          "form": "너무 긴장했나?  박하선이 더 잘했네",
          "original_form": "너무 긴장했나?  박하선이 더 잘했네",
          "speaker_id": "1",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100035232.1.9",
          "form": "10점은 운이갑다 ㅋㅋ점수차가 많다 ㅋㅋ",
          "original_form": "10점은 운이갑다 ㅋㅋ점수차가 많다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100035232.1.10",
          "form": "이게 완전 집중을 잘해야 되네",
          "original_form": "이게 완전 집중을 잘해야 되네",
          "speaker_id": "1",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100035232.1.11",
          "form": "그러게 저 플라잉 요가하다가 팔 빠지겠다",
          "original_form": "그러게 저 플라잉 요가하다가 팔 빠지겠다",
          "speaker_id": "2",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100035232.1.12",
          "form": "그러게.  운동을 좋아하네",
          "original_form": "그러게.  운동을 좋아하네",
          "speaker_id": "1",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100035232.1.13",
          "form": "연예인은 운동으로 멀어지면 안될듯하다",
          "original_form": "연예인은 운동으로 멀어지면 안될듯하다",
