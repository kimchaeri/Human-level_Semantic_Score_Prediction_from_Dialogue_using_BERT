      "utterance": [
        {
          "id": "MDRW2100033023.1.1",
          "form": "아ㅋㅋ근데 이거주제가 넘 어려워서",
          "original_form": "아ㅋㅋ근데 이거주제가 넘 어려워서",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033023.1.2",
          "form": "시사에 어두운저는ㅋㅋㅋ",
          "original_form": "시사에 어두운저는ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033023.1.3",
          "form": "우와 근데 저 처음 봐요",
          "original_form": "우와 근데 저 처음 봐요",
          "speaker_id": "1",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033023.1.4",
          "form": "지금 약간",
          "original_form": "지금 약간",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033023.1.5",
          "form": "검색해도 어렵네요",
          "original_form": "검색해도 어렵네요",
          "speaker_id": "2",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033023.1.6",
          "form": "오픽 보는 기분인데요 ㅋㅋㅋㅋ",
          "original_form": "오픽 보는 기분인데요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033023.1.7",
          "form": "한국어로 하는 오픽 ㅋㅋㅋ",
          "original_form": "한국어로 하는 오픽 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033023.1.8",
          "form": "일단 미국 전문가들은",
          "original_form": "일단 미국 전문가들은",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033023.1.9",
          "form": "한국 참여를 부정적으로 보고 있다고 하네요?",
          "original_form": "한국 참여를 부정적으로 보고 있다고 하네요?",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033023.1.10",
          "form": "헐ㅋㅋㅋ그렇군요",
          "original_form": "헐ㅋㅋㅋ그렇군요",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100033023.1.11",
          "form": "46년에 시작된거보니",
          "original_form": "46년에 시작된거보니",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100033023.1.12",
          "form": "오래됐네요",
          "original_form": "오래됐네요",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100033023.1.13",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100033023.1.14",
          "form": "아니 이렇게 오래된건데",
          "original_form": "아니 이렇게 오래된건데",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100033023.1.15",
          "form": "왜 몰랐을까요?",
          "original_form": "왜 몰랐을까요?",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100033023.1.16",
          "form": "우리나라는 또 미국이랑 중국 사이에 꼈네요ㅜㅜ",
          "original_form": "우리나라는 또 미국이랑 중국 사이에 꼈네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100033023.1.17",
          "form": "이게 기밀정보 동맹체라고하는거보니",
          "original_form": "이게 기밀정보 동맹체라고하는거보니",
          "speaker_id": "2",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033023.1.18",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033023.1.19",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033023.1.20",
          "form": "국가안보",
          "original_form": "국가안보",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033023.1.21",
          "form": "미국이 굳이 한국을 끼워줄필요는 없을거같기도하네요",
          "original_form": "미국이 굳이 한국을 끼워줄필요는 없을거같기도하네요",
          "speaker_id": "2",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033023.1.22",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033023.1.23",
          "form": "북한 관련한 문제일까요?",
          "original_form": "북한 관련한 문제일까요?",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033023.1.24",
          "form": "하긴...",
          "original_form": "하긴...",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033023.1.25",
          "form": "우리나라는 너무 약하긴 해요",
          "original_form": "우리나라는 너무 약하긴 해요",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033023.1.26",
          "form": "미영캐호주 뉴질랜드 5개국참여중이라네요",
          "original_form": "미영캐호주 뉴질랜드 5개국참여중이라네요",
          "speaker_id": "2",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033023.1.27",
          "form": "이런ㅋㅋ어려워요",
          "original_form": "이런ㅋㅋ어려워요",
          "speaker_id": "2",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033023.1.28",
          "form": "오호라......",
          "original_form": "오호라......",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033023.1.29",
          "form": "그러게요 그래도",
          "original_form": "그러게요 그래도",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033023.1.30",
          "form": "알고 있으면 좋을 것 같네요!!!",
          "original_form": "알고 있으면 좋을 것 같네요!!!",
          "speaker_id": "1",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100033023.1.31",
          "form": "그러게요 똑똑해지는 기분이네요",
          "original_form": "그러게요 똑똑해지는 기분이네요",
          "speaker_id": "2",
          "time": "20210906 14:08"
        }
      ]
