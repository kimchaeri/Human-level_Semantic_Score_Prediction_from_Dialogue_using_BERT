      "utterance": [
        {
          "id": "MDRW2100036273.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100036273.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100036273.1.3",
          "form": "오늘 저녁은 무엇을 드셨나요?",
          "original_form": "오늘 저녁은 무엇을 드셨나요?",
          "speaker_id": "2",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100036273.1.4",
          "form": "저는 김치찌개요 들디님은요??",
          "original_form": "저는 김치찌개요 들디님은요??",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100036273.1.5",
          "form": "저는 저녁에 간단하게",
          "original_form": "저는 저녁에 간단하게",
          "speaker_id": "2",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100036273.1.6",
          "form": "두유에다가 시리얼을 말아먹었어요 ㅎㅎ",
          "original_form": "두유에다가 시리얼을 말아먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100036273.1.7",
          "form": "다이어트 중이신가요??",
          "original_form": "다이어트 중이신가요??",
          "speaker_id": "1",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100036273.1.8",
          "form": "아 다이어트는 아니고",
          "original_form": "아 다이어트는 아니고",
          "speaker_id": "2",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100036273.1.9",
          "form": "시리얼 먹고",
          "original_form": "시리얼 먹고",
          "speaker_id": "2",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100036273.1.10",
          "form": "만두도 6개 먹었습니다!",
          "original_form": "만두도 6개 먹었습니다!",
          "speaker_id": "2",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100036273.1.11",
          "form": "바빠서 먹어놓고도 까먹네요 ㅎㅎㅎ",
          "original_form": "바빠서 먹어놓고도 까먹네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100036273.1.12",
          "form": "아ㅋㅋㅋ 맛있는거 드셨네요 어떤 만두 드셨나요??",
          "original_form": "아ㅋㅋㅋ 맛있는거 드셨네요 어떤 만두 드셨나요??",
          "speaker_id": "1",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100036273.1.13",
          "form": "씨유에서 파는",
          "original_form": "씨유에서 파는",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100036273.1.14",
          "form": "2+1하는 백종원 만두였어요!",
          "original_form": "2+1하는 백종원 만두였어요!",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100036273.1.15",
          "form": "크기는 작지만 맛은 좋았답니다 ㅎㅎ",
          "original_form": "크기는 작지만 맛은 좋았답니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100036273.1.16",
          "form": "name1님은 김치찌개를 댁에서 만들어 드신건가요?",
          "original_form": "&name1&님은 김치찌개를 댁에서 만들어 드신건가요?",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036273.1.17",
          "form": "아니요 배달시킨거 먹었어요",
          "original_form": "아니요 배달시킨거 먹었어요",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100036273.1.18",
          "form": "고기만두였나요??",
          "original_form": "고기만두였나요??",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100036273.1.19",
          "form": "네네 고기만두랑 해물만두 골고루 샀는데",
          "original_form": "네네 고기만두랑 해물만두 골고루 샀는데",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100036273.1.20",
          "form": "선배랑 반반씩 나눠먹었어요!",
          "original_form": "선배랑 반반씩 나눠먹었어요!",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100036273.1.21",
          "form": "주로 식사를 어디에서 하시나요?",
          "original_form": "주로 식사를 어디에서 하시나요?",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100036273.1.22",
          "form": "저는 주로 집이요",
          "original_form": "저는 주로 집이요",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100036273.1.23",
          "form": "name2님은요??",
          "original_form": "&name2&님은요??",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100036273.1.24",
          "form": "저도 쉴 때는 주로 집에서 먹는데",
          "original_form": "저도 쉴 때는 주로 집에서 먹는데",
          "speaker_id": "2",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100036273.1.25",
          "form": "평일에는 제가 대학원생이라...",
          "original_form": "평일에는 제가 대학원생이라...",
          "speaker_id": "2",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100036273.1.26",
          "form": "학교에서 두 끼를 먹습니다!",
          "original_form": "학교에서 두 끼를 먹습니다!",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036273.1.27",
