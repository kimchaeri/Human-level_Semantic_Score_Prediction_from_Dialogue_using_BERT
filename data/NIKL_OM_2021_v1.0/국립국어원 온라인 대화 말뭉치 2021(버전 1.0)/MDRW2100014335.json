      "utterance": [
        {
          "id": "MDRW2100014335.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014335.1.2",
          "form": "오늘 날씨가 참 좋죠?",
          "original_form": "오늘 날씨가 참 좋죠?",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014335.1.3",
          "form": "거기 지역은 좋으신가봐요",
          "original_form": "거기 지역은 좋으신가봐요",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014335.1.4",
          "form": "여긴 비가 내리네요",
          "original_form": "여긴 비가 내리네요",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014335.1.5",
          "form": "이츠뤠인?",
          "original_form": "이츠뤠인?",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014335.1.6",
          "form": "오마이갓",
          "original_form": "오마이갓",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014335.1.7",
          "form": "장마기간마다 장화를 살까 고민을 해요",
          "original_form": "장마기간마다 장화를 살까 고민을 해요",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014335.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014335.1.9",
          "form": "부산은 태풍때문에",
          "original_form": "부산은 태풍때문에",
          "speaker_id": "2",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014335.1.10",
          "form": "난리라고 하더라구요",
          "original_form": "난리라고 하더라구요",
          "speaker_id": "2",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014335.1.11",
          "form": "맞아요 거긴 도로가 워터파크더라구요",
          "original_form": "맞아요 거긴 도로가 워터파크더라구요",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014335.1.12",
          "form": "name1네는 괜찮으신가요?",
          "original_form": "&name1&네는 괜찮으신가요?",
          "speaker_id": "2",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014335.1.13",
          "form": "이번주내내 비소식이긴한데 부산만큼 심하지는 않아요",
          "original_form": "이번주내내 비소식이긴한데 부산만큼 심하지는 않아요",
          "speaker_id": "1",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014335.1.14",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014335.1.15",
          "form": "name1은 비오는 날 좋아하시나요?",
          "original_form": "&name1&은 비오는 날 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014335.1.16",
          "form": "저는 좋아해요",
          "original_form": "저는 좋아해요",
          "speaker_id": "2",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014335.1.17",
          "form": "비내리는호남선~~~",
          "original_form": "비내리는호남선~~~",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014335.1.18",
          "form": "남행열차에~~~~",
          "original_form": "남행열차에~~~~",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014335.1.19",
          "form": "저는 밖에 나갈일 있으면 안좋아해요",
          "original_form": "저는 밖에 나갈일 있으면 안좋아해요",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014335.1.20",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014335.1.21",
          "form": "제가 또 흥에",
          "original_form": "제가 또 흥에",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014335.1.22",
          "form": "비올때 버스 타보셨나요?",
          "original_form": "비올때 버스 타보셨나요?",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014335.1.23",
          "form": "겨웠네요",
          "original_form": "겨웠네요",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014335.1.24",
          "form": "다들 우산을 왜 안접는지 모르겠네요",
          "original_form": "다들 우산을 왜 안접는지 모르겠네요",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014335.1.25",
          "form": "네 많이 질척거리죠",
          "original_form": "네 많이 질척거리죠",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014335.1.26",
          "form": "이슬비올때 산책하거나",
          "original_form": "이슬비올때 산책하거나",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014335.1.27",
          "form": "창문으로 비 내리는걸",
          "original_form": "창문으로 비 내리는걸",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014335.1.28",
          "form": "구경하면서",
          "original_form": "구경하면서",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014335.1.29",
          "form": "뜨거운 아메리카노",
          "original_form": "뜨거운 아메리카노",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014335.1.30",
          "form": "우비랑 장화신고 걸어보고 싶긴해요",
          "original_form": "우비랑 장화신고 걸어보고 싶긴해요",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014335.1.31",
          "form": "이게 삶이죠",
          "original_form": "이게 삶이죠",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014335.1.32",
          "form": "제가 코로나를 정말 싫어하는데",
          "original_form": "제가 코로나를 정말 싫어하는데",
          "speaker_id": "2",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014335.1.33",
          "form": "좋아진점이 있다면",
          "original_form": "좋아진점이 있다면",
          "speaker_id": "2",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014335.1.34",
          "form": "미세먼지가",
          "original_form": "미세먼지가",
          "speaker_id": "2",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014335.1.35",
          "form": "없어졌다는거에요",
          "original_form": "없어졌다는거에요",
          "speaker_id": "2",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014335.1.36",
          "form": "파란하늘 정말 이쁘지 않나요?",
          "original_form": "파란하늘 정말 이쁘지 않나요?",
          "speaker_id": "2",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014335.1.37",
          "form": "미세먼지 최악이죠~",
          "original_form": "미세먼지 최악이죠~",
          "speaker_id": "1",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014335.1.38",
          "form": "거긴 비가 오고있지만요",
          "original_form": "거긴 비가 오고있지만요",
          "speaker_id": "2",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014335.1.39",
          "form": "제가 비염끼가 있어서 너무 괴로워요",
          "original_form": "제가 비염끼가 있어서 너무 괴로워요",
          "speaker_id": "1",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014335.1.40",
          "form": "아이고 저런",
          "original_form": "아이고 저런",
          "speaker_id": "2",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014335.1.41",
          "form": "오 방금 번개가 쳤어요!",
          "original_form": "오 방금 번개가 쳤어요!",
          "speaker_id": "1",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014335.1.42",
          "form": "우르릉 쾅",
          "original_form": "우르릉 쾅",
          "speaker_id": "2",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014335.1.43",
          "form": "저는 번개 치는 소리도 좋아한답니다",
          "original_form": "저는 번개 치는 소리도 좋아한답니다",
          "speaker_id": "2",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014335.1.44",
          "form": "점심에는 그렇게 찌더니",
          "original_form": "점심에는 그렇게 찌더니",
          "speaker_id": "2",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014335.1.45",
          "form": "지금은 선선하니 좋네요",
          "original_form": "지금은 선선하니 좋네요",
          "speaker_id": "2",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014335.1.46",
          "form": "거기는 비가 오고 번개가 치겠지만",
          "original_form": "거기는 비가 오고 번개가 치겠지만",
          "speaker_id": "2",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014335.1.47",
          "form": "저랑 취향이 정 반대시네요",
          "original_form": "저랑 취향이 정 반대시네요",
          "speaker_id": "1",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014335.1.48",
          "form": "비오는날은 파전이 맛있다는거 말고 좋은점을 모르겠어요",
          "original_form": "비오는날은 파전이 맛있다는거 말고 좋은점을 모르겠어요",
          "speaker_id": "1",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014335.1.49",
          "form": "그쪽은 많이 더운가요?",
          "original_form": "그쪽은 많이 더운가요?",
          "speaker_id": "1",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014335.1.50",
          "form": "아뇨 시원하다구요",
          "original_form": "아뇨 시원하다구요",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014335.1.51",
          "form": "집중",
          "original_form": "집중",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014335.1.52",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아 시원하시구나",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아 시원하시구나",
          "speaker_id": "1",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014335.1.53",
          "form": "저는 더워서 나시 입고있어요",
          "original_form": "저는 더워서 나시 입고있어요",
          "speaker_id": "1",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014335.1.54",
          "form": "습기가 많아서",
          "original_form": "습기가 많아서",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014335.1.55",
          "form": "덥죠",
          "original_form": "덥죠",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014335.1.56",
          "form": "비오는날은 참 에어컨 틀기가 애매해요",
          "original_form": "비오는날은 참 에어컨 틀기가 애매해요",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014335.1.57",
          "form": "온도는 낮은데",
          "original_form": "온도는 낮은데",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014335.1.58",
          "form": "습기가 차니",
          "original_form": "습기가 차니",
          "speaker_id": "2",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014335.1.59",
          "form": "이럴때일수록",
          "original_form": "이럴때일수록",
          "speaker_id": "2",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014335.1.60",
          "form": "저희집은 엄마가 더위를 많이타서 최근들어 항상 틀고 있어요",
          "original_form": "저희집은 엄마가 더위를 많이타서 최근들어 항상 틀고 있어요",
          "speaker_id": "1",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014335.1.61",
          "form": "제습으로",
          "original_form": "제습으로",
          "speaker_id": "2",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014335.1.62",
          "form": "틀어놔야하는데",
          "original_form": "틀어놔야하는데",
          "speaker_id": "2",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014335.1.63",
          "form": "한국은 날씨가 에어콘이 필수인거같아요",
          "original_form": "한국은 날씨가 에어콘이 필수인거같아요",
          "speaker_id": "1",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014335.1.64",
          "form": "일기예보 보시는 편인가요?",
          "original_form": "일기예보 보시는 편인가요?",
          "speaker_id": "1",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014335.1.65",
          "form": "아 그럼요",
          "original_form": "아 그럼요",
          "speaker_id": "2",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014335.1.66",
          "form": "근데 요즘은 보나안보나",
          "original_form": "근데 요즘은 보나안보나",
          "speaker_id": "2",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014335.1.67",
          "form": "맞은적이 별로 없어서",
          "original_form": "맞은적이 별로 없어서",
          "speaker_id": "2",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014335.1.68",
          "form": "보는 의미가 없는거 같기도 해요",
          "original_form": "보는 의미가 없는거 같기도 해요",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
