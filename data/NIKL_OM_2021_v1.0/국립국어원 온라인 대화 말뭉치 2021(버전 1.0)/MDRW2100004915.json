      "utterance": [
        {
          "id": "MDRW2100004915.1.1",
          "form": "하시는 게임이 있나요?",
          "original_form": "하시는 게임이 있나요?",
          "speaker_id": "2",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004915.1.2",
          "form": "저는 컴퓨터게임 예전에 테일즈런너라는 게임을 했었는데 지금은 안 하고",
          "original_form": "저는 컴퓨터게임 예전에 테일즈런너라는 게임을 했었는데 지금은 안 하고",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004915.1.3",
          "form": "휴대폰 게임으로 프로젝트 메이크 오버라는 게임을 해요",
          "original_form": "휴대폰 게임으로 프로젝트 메이크 오버라는 게임을 해요",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004915.1.4",
          "form": "애니팡같은건데 게임을 깨면 받은 돈으리ㅡ",
          "original_form": "애니팡같은건데 게임을 깨면 받은 돈으리ㅡ",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004915.1.5",
          "form": "돈으로",
          "original_form": "돈으로",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004915.1.6",
          "form": "집과 스타일이 망가진 사람들을 고쳐주는 게임이에요",
          "original_form": "집과 스타일이 망가진 사람들을 고쳐주는 게임이에요",
          "speaker_id": "1",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004915.1.7",
          "form": "우와 테런 저도알아요 친구들 어깨너머로 자주 구경했었죠",
          "original_form": "우와 테런 저도알아요 친구들 어깨너머로 자주 구경했었죠",
          "speaker_id": "2",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004915.1.8",
          "form": "지금 하시는게임은 꿈의정원과 비슷하시네요",
          "original_form": "지금 하시는게임은 꿈의정원과 비슷하시네요",
          "speaker_id": "2",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004915.1.9",
          "form": "재밌을거같아요",
          "original_form": "재밌을거같아요",
          "speaker_id": "2",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004915.1.10",
          "form": "넵 맞아요",
          "original_form": "넵 맞아요",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100004915.1.11",
          "form": "name2님은 어떤 게임 하시나요?",
          "original_form": "&name2&님은 어떤 게임 하시나요?",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100004915.1.12",
          "form": "저는 여러가지 게임을 즐겨해요 최근에 재밌게 하고있는건",
          "original_form": "저는 여러가지 게임을 즐겨해요 최근에 재밌게 하고있는건",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100004915.1.13",
          "form": "돈스타브 투게더에요",
          "original_form": "돈스타브 투게더에요",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100004915.1.14",
          "form": "얼마전까진 스위치 모동숲도 재밌게했어요",
          "original_form": "얼마전까진 스위치 모동숲도 재밌게했어요",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100004915.1.15",
          "form": "아하 저도 동숲 좋아해요",
          "original_form": "아하 저도 동숲 좋아해요",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100004915.1.16",
          "form": "놀러오세요 동물의 숲 때만 열심히 했지만ㅋㅋㅋ",
          "original_form": "놀러오세요 동물의 숲 때만 열심히 했지만ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100004915.1.17",
          "form": "돈스타브는 어떤 겜이에요??",
          "original_form": "돈스타브는 어떤 겜이에요??",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100004915.1.18",
          "form": "저두요ㅋㄱㅋㄱㄱ",
          "original_form": "저두요ㅋㄱㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100004915.1.19",
          "form": "돈스타브는 생존게임이에요 생존에 필요한 여러가지 자원을모아",
          "original_form": "돈스타브는 생존게임이에요 생존에 필요한 여러가지 자원을모아",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100004915.1.20",
          "form": "적의 침입을 막고 기지를 건설하고 요리를 해먹고 살아남는 게임이에요",
          "original_form": "적의 침입을 막고 기지를 건설하고 요리를 해먹고 살아남는 게임이에요",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100004915.1.21",
          "form": "게임 그래픽? 그림이 호불호가 많이 갈리는편이에요",
          "original_form": "게임 그래픽? 그림이 호불호가 많이 갈리는편이에요",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100004915.1.22",
          "form": "그 밖에 초반 게임 진입장벽이 높고 어렵지만 부딪히다보면 쉽게 알게되고 굉장히 재미있어요",
          "original_form": "그 밖에 초반 게임 진입장벽이 높고 어렵지만 부딪히다보면 쉽게 알게되고 굉장히 재미있어요",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100004915.1.23",
          "form": "아하 스팀게임인가요?",
          "original_form": "아하 스팀게임인가요?",
          "speaker_id": "1",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100004915.1.24",
          "form": "재미있을 것 같네요",
          "original_form": "재미있을 것 같네요",
          "speaker_id": "1",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100004915.1.25",
          "form": "투게더는 말그대로 친구들과 함께할수있어서 더욱 재밌답니다",
          "original_form": "투게더는 말그대로 친구들과 함께할수있어서 더욱 재밌답니다",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100004915.1.26",
          "form": "네 스팀게임이에요",
          "original_form": "네 스팀게임이에요",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100004915.1.27",
          "form": "돈스타브 회사가 게임에 애정을 가지고 오류수정과 업데이트를 꾸준히 해줘서 좋아요",
          "original_form": "돈스타브 회사가 게임에 애정을 가지고 오류수정과 업데이트를 꾸준히 해줘서 좋아요",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100004915.1.28",
          "form": "생각해뒀다가 다음에 꼭 해봐야겠어요",
          "original_form": "생각해뒀다가 다음에 꼭 해봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100004915.1.29",
          "form": "저는 방금 오랜만에 템플런을 깔았는데",
          "original_form": "저는 방금 오랜만에 템플런을 깔았는데",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100004915.1.30",
          "form": "좋아요! 카페같은 곳을 보면 공략하기 쉬울거에요",
          "original_form": "좋아요! 카페같은 곳을 보면 공략하기 쉬울거에요",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100004915.1.31",
          "form": "템플런 정말 재밌게 했던 기억이있어요",
          "original_form": "템플런 정말 재밌게 했던 기억이있어요",
          "speaker_id": "2",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100004915.1.32",
          "form": "10주년이라고 10000코인을 주네요",
          "original_form": "10주년이라고 10000코인을 주네요",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100004915.1.33",
          "form": "맞아요 추억이 새록새록 돋네요",
          "original_form": "맞아요 추억이 새록새록 돋네요",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100004915.1.34",
          "form": "친구 아이팟ㅋㅋㅋ을 빌려서 열심히 했었는데..",
          "original_form": "친구 아이팟ㅋㅋㅋ을 빌려서 열심히 했었는데..",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100004915.1.35",
          "form": "큰 폰으로 하니까 신선하네요",
          "original_form": "큰 폰으로 하니까 신선하네요",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100004915.1.36",
          "form": "와아 만코인이면 가치가 높은건가요?  기억이 잘안나요",
          "original_form": "와아 만코인이면 가치가 높은건가요?  기억이 잘안나요",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100004915.1.37",
          "form": "아직도 있다니 신기하네요",
          "original_form": "아직도 있다니 신기하네요",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100004915.1.38",
          "form": "저도 깔아서 해봐야겠어요",
          "original_form": "저도 깔아서 해봐야겠어요",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100004915.1.39",
          "form": "가장 비싼 캐릭터가 25000코인이에요!! 꽤 많이 주죠",
          "original_form": "가장 비싼 캐릭터가 25000코인이에요!! 꽤 많이 주죠",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100004915.1.40",
          "form": "그쵸 아직도 살아남았다니 대단한 게임..",
          "original_form": "그쵸 아직도 살아남았다니 대단한 게임..",
