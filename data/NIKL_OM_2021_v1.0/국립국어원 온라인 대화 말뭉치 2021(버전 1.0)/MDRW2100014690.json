        {
          "id": "MDRW2100014690.1.1",
          "form": "아....님..",
          "original_form": "아....님..",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014690.1.2",
          "form": "랑종 보셨나요?",
          "original_form": "랑종 보셨나요?",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014690.1.3",
          "form": "저 그거 무섭기보단 역겨워서 중간탈출했는데",
          "original_form": "저 그거 무섭기보단 역겨워서 중간탈출했는데",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014690.1.4",
          "form": "예?!!!",
          "original_form": "예?!!!",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014690.1.5",
          "form": "저는 안봤어요",
          "original_form": "저는 안봤어요",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014690.1.6",
          "form": "저 최근에 본게 보스베이비 2",
          "original_form": "저 최근에 본게 보스베이비 2",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014690.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 씁 귀여워.",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 씁 귀여워.",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014690.1.8",
          "form": "쏘큩",
          "original_form": "쏘큩",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014690.1.9",
          "form": "님 그 랑종 안보셨구나 2021 제일 잘한일 베스트1위로 올라갑니다",
          "original_form": "님 그 랑종 안보셨구나 2021 제일 잘한일 베스트1위로 올라갑니다",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014690.1.10",
          "form": "돈아끼고 눈아끼고",
          "original_form": "돈아끼고 눈아끼고",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014690.1.11",
          "form": "ㅋㅋㅋㅋㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014690.1.12",
          "form": "전.....결제해서 봤거든요",
          "original_form": "전.....결제해서 봤거든요",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014690.1.13",
          "form": "그정도냐고욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그정도냐고욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014690.1.14",
          "form": "근데 고어에 가깝다해서 안봤어요",
          "original_form": "근데 고어에 가깝다해서 안봤어요",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014690.1.15",
          "form": "스포...해도 되나요?",
          "original_form": "스포...해도 되나요?",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014690.1.16",
          "form": "전 다른 건 아 웩웩 이러면서 봤는데 그 장면 나오자마자 아 탈출 못본다 최악의소비. 이랬거든요",
          "original_form": "전 다른 건 아 웩웩 이러면서 봤는데 그 장면 나오자마자 아 탈출 못본다 최악의소비. 이랬거든요",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014690.1.17",
          "form": "그 강아지요..?",
          "original_form": "그 강아지요..?",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014690.1.18",
          "form": "해주세요",
          "original_form": "해주세요",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014690.1.19",
          "form": "그",
          "original_form": "그",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014690.1.20",
          "form": "주인공이 있거든요 근데 얘가 귀신?악령이 씌인단말이에요",
          "original_form": "주인공이 있거든요 근데 얘가 귀신?악령이 씌인단말이에요",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014690.1.21",
          "form": "근데 얘가 악령같은게 몸에 들어오면 보통 걍 사람해치고 이러는게 다인데",
          "original_form": "근데 얘가 악령같은게 몸에 들어오면 보통 걍 사람해치고 이러는게 다인데",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014690.1.22",
          "form": "아 씁 얘가 갑자기 옷을",
          "original_form": "아 씁 얘가 갑자기 옷을",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014690.1.23",
          "form": "ㅂㅓㅅ거든요?",
          "original_form": "ㅂㅓㅅ거든요?",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014690.1.24",
          "form": "근데 그 상태로 앞에 있는 삼촌 손을 끌어다가 자기 ㄱr슴을 만ㅈi라고 하면서",
          "original_form": "근데 그 상태로 앞에 있는 삼촌 손을 끌어다가 자기 ㄱr슴을 만ㅈi라고 하면서",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014690.1.25",
          "form": "으 저장면보고 난 못본다. 이러면서 껏어요",
          "original_form": "으 저장면보고 난 못본다. 이러면서 껏어요",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014690.1.26",
          "form": "하 진짜 최악의 영화 무서움보다 역겨운데 하도 무섭다해서......",
          "original_form": "하 진짜 최악의 영화 무서움보다 역겨운데 하도 무섭다해서......",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014690.1.27",
          "form": "쓰읍...",
          "original_form": "쓰읍...",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014690.1.28",
