      "utterance": [
        {
          "id": "MDRW2100030065.1.1",
          "form": "우는 냥이 보면 걱정도되고",
          "original_form": "우는 냥이 보면 걱정도되고",
          "speaker_id": "1",
          "time": "20210903 20:31"
        },
        {
          "id": "MDRW2100030065.1.2",
          "form": "그냥 길냥이봐도 엄청 걱정되죠 ㅠ",
          "original_form": "그냥 길냥이봐도 엄청 걱정되죠 ㅠ",
          "speaker_id": "1",
          "time": "20210903 20:31"
        },
        {
          "id": "MDRW2100030065.1.3",
          "form": "어찌살아갈까싶고 ㅠㅠ",
          "original_form": "어찌살아갈까싶고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 20:31"
        },
        {
          "id": "MDRW2100030065.1.4",
          "form": "사람들이 너무 잔인해서",
          "original_form": "사람들이 너무 잔인해서",
          "speaker_id": "1",
          "time": "20210903 20:31"
        },
        {
          "id": "MDRW2100030065.1.5",
          "form": "맞아요 이제 겨울도되가는데ㅠㅠ",
          "original_form": "맞아요 이제 겨울도되가는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 20:32"
        },
        {
          "id": "MDRW2100030065.1.6",
          "form": "날도추워지고...",
          "original_form": "날도추워지고...",
          "speaker_id": "2",
          "time": "20210903 20:32"
        },
        {
          "id": "MDRW2100030065.1.7",
          "form": "고양이들 싫어하는건 그러려니해도 해치는건 진짜 너무한거같아요",
          "original_form": "고양이들 싫어하는건 그러려니해도 해치는건 진짜 너무한거같아요",
          "speaker_id": "2",
          "time": "20210903 20:33"
        },
        {
          "id": "MDRW2100030065.1.8",
          "form": "그쵸오 ㅠㅠㅠ",
          "original_form": "그쵸오 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 20:33"
        },
        {
          "id": "MDRW2100030065.1.9",
          "form": "해치는건진짜 너무해요 ㅠㅠ",
          "original_form": "해치는건진짜 너무해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 20:33"
        },
        {
          "id": "MDRW2100030065.1.10",
          "form": "겨울 냥이가 얼마나 귀여운데 ㅠㅜㅜ",
          "original_form": "겨울 냥이가 얼마나 귀여운데 ㅠㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 20:33"
        },
        {
          "id": "MDRW2100030065.1.11",
          "form": "겨울에 고양이들 막 추워서 털도 복실해지고",
          "original_form": "겨울에 고양이들 막 추워서 털도 복실해지고",
          "speaker_id": "2",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100030065.1.12",
          "form": "털 복실복실 졸귀죠!",
          "original_form": "털 복실복실 졸귀죠!",
          "speaker_id": "1",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100030065.1.13",
          "form": "많이 긴친구들은 덜추워보이긴 하더라고요",
          "original_form": "많이 긴친구들은 덜추워보이긴 하더라고요",
          "speaker_id": "2",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100030065.1.14",
          "form": "맞아옄ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아까말한 메인쿤은 ㅋㅋㅋㅋ절대안추울거같아요",
          "original_form": "맞아옄ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아까말한 메인쿤은 ㅋㅋㅋㅋ절대안추울거같아요",
          "speaker_id": "1",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100030065.1.15",
          "form": "오히려 여름에 더워보이지..",
          "original_form": "오히려 여름에 더워보이지..",
          "speaker_id": "1",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100030065.1.16",
          "form": "맞아요ㅋㅋ 한국이면 더...",
          "original_form": "맞아요ㅋㅋ 한국이면 더...",
          "speaker_id": "2",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100030065.1.17",
          "form": "습하고더워서 그런친구들 진짜 완전 쪄서 힘들거같아요",
          "original_form": "습하고더워서 그런친구들 진짜 완전 쪄서 힘들거같아요",
          "speaker_id": "2",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100030065.1.18",
          "form": "으어",
          "original_form": "으어",
          "speaker_id": "1",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100030065.1.19",
          "form": "사람도 습해서 죽을거같은데",
          "original_form": "사람도 습해서 죽을거같은데",
          "speaker_id": "1",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100030065.1.20",
          "form": "고양이도 그렇겠네요",
          "original_form": "고양이도 그렇겠네요",
          "speaker_id": "1",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100030065.1.21",
          "form": "털이 많으니 많은만큼 더운...",
          "original_form": "털이 많으니 많은만큼 더운...",
          "speaker_id": "2",
          "time": "20210903 20:43"
        },
        {
          "id": "MDRW2100030065.1.22",
          "form": "짧은 친구들도 더울날씨인데",
          "original_form": "짧은 친구들도 더울날씨인데",
          "speaker_id": "2",
          "time": "20210903 20:43"
        },
        {
          "id": "MDRW2100030065.1.23",
          "form": "털 긴 애들은 추운곳이 고향이래서 더위 더탈거같고 그래요",
          "original_form": "털 긴 애들은 추운곳이 고향이래서 더위 더탈거같고 그래요",
          "speaker_id": "2",
          "time": "20210903 20:43"
        },
        {
          "id": "MDRW2100030065.1.24",
          "form": "그럴듯해요 ㅋㅋㅋ 진짜",
          "original_form": "그럴듯해요 ㅋㅋㅋ 진짜",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100030065.1.25",
          "form": "그런친구들 미용같은거로 털을 어떻게 밀어주나 싶기는한데",
          "original_form": "그런친구들 미용같은거로 털을 어떻게 밀어주나 싶기는한데",
          "speaker_id": "2",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100030065.1.26",
          "form": "강아지처럼 아주 짧게 밀은 고양이는 별로 못본거같네요",
          "original_form": "강아지처럼 아주 짧게 밀은 고양이는 별로 못본거같네요",
          "speaker_id": "2",
          "time": "20210903 20:49"
        },
        {
          "id": "MDRW2100030065.1.27",
          "form": "친구는 털을 밀더라구요",
          "original_form": "친구는 털을 밀더라구요",
          "speaker_id": "1",
          "time": "20210903 20:49"
        },
        {
          "id": "MDRW2100030065.1.28",
          "form": "메인쿤 키워요",
          "original_form": "메인쿤 키워요",
          "speaker_id": "1",
          "time": "20210903 20:49"
        },
        {
          "id": "MDRW2100030065.1.29",
          "form": "여름마다 다 밀어요",
          "original_form": "여름마다 다 밀어요",
          "speaker_id": "1",
          "time": "20210903 20:49"
        },
        {
          "id": "MDRW2100030065.1.30",
          "form": "우와 그럼 몸통을 다 밀어주는건가요?",
          "original_form": "우와 그럼 몸통을 다 밀어주는건가요?",
          "speaker_id": "2",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100030065.1.31",
          "form": "아님 덜덥게 배같은곳만 미는거려나요",
          "original_form": "아님 덜덥게 배같은곳만 미는거려나요",
          "speaker_id": "2",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100030065.1.32",
          "form": "네 몸통을  다",
          "original_form": "네 몸통을  다",
          "speaker_id": "1",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100030065.1.33",
          "form": "얼굴빼고 다요",
          "original_form": "얼굴빼고 다요",
          "speaker_id": "1",
          "time": "20210903 20:50"
        }
      ]
    }
