      "utterance": [
        {
          "id": "MDRW2100001890.1.1",
          "form": "이제는 제법 선선해졌당",
          "original_form": "이제는 제법 선선해졌당",
          "speaker_id": "1",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100001890.1.2",
          "form": "그러게 새벽엔 이제 춥더라고",
          "original_form": "그러게 새벽엔 이제 춥더라고",
          "speaker_id": "2",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001890.1.3",
          "form": "문 닫고 자야해",
          "original_form": "문 닫고 자야해",
          "speaker_id": "1",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001890.1.4",
          "form": "어제 잊고 문안닫아서",
          "original_form": "어제 잊고 문안닫아서",
          "speaker_id": "2",
          "time": "20210819 23:01"
        },
        {
          "id": "MDRW2100001890.1.5",
          "form": "목아프더라",
          "original_form": "목아프더라",
          "speaker_id": "2",
          "time": "20210819 23:01"
        },
        {
          "id": "MDRW2100001890.1.6",
          "form": "맞어ㅠㅠ 오늘도시원하긴한데뭔가 닫으면 더울것 같노",
          "original_form": "맞어ㅠㅠ 오늘도시원하긴한데뭔가 닫으면 더울것 같노",
          "speaker_id": "1",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100001890.1.7",
          "form": "오늘도 새벽에 춥던데ㅜㅜ",
          "original_form": "오늘도 새벽에 춥던데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 07:41"
        },
        {
          "id": "MDRW2100001890.1.8",
          "form": "잘잤나",
          "original_form": "잘잤나",
          "speaker_id": "2",
          "time": "20210820 09:56"
        },
        {
          "id": "MDRW2100001890.1.9",
          "form": "응 문닫고 잘잤당ㅋㅋ",
          "original_form": "응 문닫고 잘잤당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 11:47"
        },
        {
          "id": "MDRW2100001890.1.10",
          "form": "아깐  비오더만 지금 햇빛쨍쨍이고",
          "original_form": "아깐  비오더만 지금 햇빛쨍쨍이고",
          "speaker_id": "2",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100001890.1.11",
          "form": "그르네ㅠㅠ덥다ㅠㅠ",
          "original_form": "그르네ㅠㅠ덥다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100001890.1.12",
          "form": "아 차에 에어콘틀고 오니 집에 올라가기 싫으네 더워서",
          "original_form": "아 차에 에어콘틀고 오니 집에 올라가기 싫으네 더워서",
          "speaker_id": "2",
          "time": "20210820 12:22"
        },
        {
          "id": "MDRW2100001890.1.13",
          "form": "그르이ㅠ오늘은덥다ㅜ순간 에어컨틀까? 고민함",
          "original_form": "그르이ㅠ오늘은덥다ㅜ순간 에어컨틀까? 고민함",
          "speaker_id": "1",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100001890.1.14",
          "form": "회사에선 에어콘 틀고있었다ㅋㅋ",
          "original_form": "회사에선 에어콘 틀고있었다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100001890.1.15",
          "form": "좋은 회사구나ㅎㅎ",
          "original_form": "좋은 회사구나ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 12:53"
        },
        {
          "id": "MDRW2100001890.1.16",
          "form": "응 ㅋㅋㅋ 저녁엔 또 선선하네ㅋ",
          "original_form": "응 ㅋㅋㅋ 저녁엔 또 선선하네ㅋ",
