      "utterance": [
        {
          "id": "MDRW2100006860.1.1",
          "form": "봐든게 있나요",
          "original_form": "봐든게 있나요",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100006860.1.2",
          "form": "해밀턴같은 동그란거 생각하다가요",
          "original_form": "해밀턴같은 동그란거 생각하다가요",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006860.1.3",
          "form": "동그란 빵이 흔한가요",
          "original_form": "동그란 빵이 흔한가요",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006860.1.4",
          "form": "맞아요 머핀류의 동그런걸 넣어야하는데 편해보이기내요",
          "original_form": "맞아요 머핀류의 동그런걸 넣어야하는데 편해보이기내요",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006860.1.5",
          "form": "저는 네모가 좋아요",
          "original_form": "저는 네모가 좋아요",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006860.1.6",
          "form": "파니니 그릴 같은거도 봐두긴했어요",
          "original_form": "파니니 그릴 같은거도 봐두긴했어요",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006860.1.7",
          "form": "누르면되니 간편하죠",
          "original_form": "누르면되니 간편하죠",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100006860.1.8",
          "form": "그런데 꾸준히 해 먹을지 자신이 없네요 ㅋ",
          "original_form": "그런데 꾸준히 해 먹을지 자신이 없네요 ㅋ",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100006860.1.9",
          "form": "뭐든 그게 문제에요",
          "original_form": "뭐든 그게 문제에요",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100006860.1.10",
          "form": "초창기엔 부지런히 하고 나중엔 방치하고요",
          "original_form": "초창기엔 부지런히 하고 나중엔 방치하고요",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100006860.1.11",
          "form": "그래도 본전뽑는다면요",
          "original_form": "그래도 본전뽑는다면요",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100006860.1.12",
          "form": "그 생각으로 사볼까 생각해요",
          "original_form": "그 생각으로 사볼까 생각해요",
          "speaker_id": "1",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100006860.1.13",
          "form": "저도 구입하는건 좋아요",
          "original_form": "저도 구입하는건 좋아요",
          "speaker_id": "2",
          "time": "20210908 10:55"
