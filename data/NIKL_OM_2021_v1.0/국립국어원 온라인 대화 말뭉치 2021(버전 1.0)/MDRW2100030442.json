      "utterance": [
        {
          "id": "MDRW2100030442.1.1",
          "form": "집안일 하니",
          "original_form": "집안일 하니",
          "speaker_id": "1",
          "time": "20210904 13:18"
        },
        {
          "id": "MDRW2100030442.1.2",
          "form": "하지",
          "original_form": "하지",
          "speaker_id": "2",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030442.1.3",
          "form": "청소기 돌리는거랑 고양이 밥하고 물 챙겨주는거랑",
          "original_form": "청소기 돌리는거랑 고양이 밥하고 물 챙겨주는거랑",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030442.1.4",
          "form": "오 고양이 밥",
          "original_form": "오 고양이 밥",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030442.1.5",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030442.1.6",
          "form": "저는 설거지 합니다",
          "original_form": "저는 설거지 합니다",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030442.1.7",
          "form": "청소기도 돌리구..",
          "original_form": "청소기도 돌리구..",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030442.1.8",
          "form": "이열~~",
          "original_form": "이열~~",
          "speaker_id": "2",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100030442.1.9",
          "form": "설거지는,,,시키면 해 막 뭐시기 엄마 손다쳤거나 집에 나밖에 없거나",
          "original_form": "설거지는,,,시키면 해 막 뭐시기 엄마 손다쳤거나 집에 나밖에 없거나",
          "speaker_id": "2",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100030442.1.10",
          "form": "아하 저는 걍 엄마가 하라면 하는 편...",
          "original_form": "아하 저는 걍 엄마가 하라면 하는 편...",
          "speaker_id": "1",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100030442.1.11",
          "form": "엄마는 내가 하면 그 뭐시기 그거래 만족이 안된대",
          "original_form": "엄마는 내가 하면 그 뭐시기 그거래 만족이 안된대",
          "speaker_id": "2",
          "time": "20210904 14:24"
        },
        {
          "id": "MDRW2100030442.1.12",
          "form": "엄마 기준으로 만족울 할 수 없음",
          "original_form": "엄마 기준으로 만족울 할 수 없음",
          "speaker_id": "1",
          "time": "20210904 14:24"
        },
        {
          "id": "MDRW2100030442.1.13",
          "form": "우리 엄만 그냥 넘어가던데",
          "original_form": "우리 엄만 그냥 넘어가던데",
          "speaker_id": "1",
          "time": "20210904 14:24"
        },
        {
          "id": "MDRW2100030442.1.14",
          "form": "했다는거에 의의를 두시는거 아닐까",
          "original_form": "했다는거에 의의를 두시는거 아닐까",
          "speaker_id": "2",
          "time": "20210904 14:46"
        },
        {
          "id": "MDRW2100030442.1.15",
          "form": "ㅋㅋㅋㅋ울엄마 만족 못해서 시켜놓고 화내 잉 ㅠ",
          "original_form": "ㅋㅋㅋㅋ울엄마 만족 못해서 시켜놓고 화내 잉 ㅠ",
          "speaker_id": "2",
          "time": "20210904 14:46"
        },
        {
          "id": "MDRW2100030442.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030442.1.17",
          "form": "아근데 우리 엄마도 좀 안 만적스러운거 같드라...",
          "original_form": "아근데 우리 엄마도 좀 안 만적스러운거 같드라...",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030442.1.18",
          "form": "약간 내가 동생에게 길 시키고 결과물 보면 마음에 안드는것 같은",
          "original_form": "약간 내가 동생에게 길 시키고 결과물 보면 마음에 안드는것 같은",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030442.1.19",
          "form": "그런 느낌인듯",
          "original_form": "그런 느낌인듯",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030442.1.20",
          "form": "그렇지 그렇지",
          "original_form": "그렇지 그렇지",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
