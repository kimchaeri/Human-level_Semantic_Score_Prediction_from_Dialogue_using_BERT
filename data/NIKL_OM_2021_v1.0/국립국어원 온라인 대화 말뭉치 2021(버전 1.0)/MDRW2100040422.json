      "utterance": [
        {
          "id": "MDRW2100040422.1.1",
          "form": "3시에 12시? 얼핏봣어",
          "original_form": "3시에 12시? 얼핏봣어",
          "speaker_id": "2",
          "time": "20210904 19:22"
        },
        {
          "id": "MDRW2100040422.1.2",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210904 19:22"
        },
        {
          "id": "MDRW2100040422.1.3",
          "form": "좋은..옷..이 있는지 모르겠다",
          "original_form": "좋은..옷..이 있는지 모르겠다",
          "speaker_id": "2",
          "time": "20210904 19:23"
        },
        {
          "id": "MDRW2100040422.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:23"
        },
        {
          "id": "MDRW2100040422.1.5",
          "form": "ㅎㅎㅎ......",
          "original_form": "ㅎㅎㅎ......",
          "speaker_id": "2",
          "time": "20210904 19:23"
        },
        {
          "id": "MDRW2100040422.1.6",
          "form": "치마가 맞으려나...",
          "original_form": "치마가 맞으려나...",
          "speaker_id": "2",
          "time": "20210904 19:23"
        },
        {
          "id": "MDRW2100040422.1.7",
          "form": "걍 깔끔하게 잘어울리는거 입고와",
          "original_form": "걍 깔끔하게 잘어울리는거 입고와",
          "speaker_id": "1",
          "time": "20210904 19:23"
        },
        {
          "id": "MDRW2100040422.1.8",
          "form": "아닝 난 슬렉스 입을거야 ㅋㅋㅋ",
          "original_form": "아닝 난 슬렉스 입을거야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:23"
        },
        {
          "id": "MDRW2100040422.1.9",
          "form": "사진 많이 찍자",
          "original_form": "사진 많이 찍자",
          "speaker_id": "1",
          "time": "20210904 19:23"
        },
        {
          "id": "MDRW2100040422.1.10",
          "form": "바지에 슬랙스..출근복장인데 위는 블라우스가 맞으려나",
          "original_form": "바지에 슬랙스..출근복장인데 위는 블라우스가 맞으려나",
          "speaker_id": "2",
          "time": "20210904 19:23"
        },
        {
          "id": "MDRW2100040422.1.11",
          "form": "ㅇㅇ 조아조아",
          "original_form": "ㅇㅇ 조아조아",
          "speaker_id": "2",
          "time": "20210904 19:23"
        },
        {
          "id": "MDRW2100040422.1.12",
          "form": "블라우스 잇으면 ㅇㅇ",
          "original_form": "블라우스 잇으면 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210904 19:23"
        },
        {
          "id": "MDRW2100040422.1.13",
          "form": "애매하지믄 7부길이 있어서",
          "original_form": "애매하지믄 7부길이 있어서",
          "speaker_id": "2",
          "time": "20210904 19:23"
        },
        {
          "id": "MDRW2100040422.1.14",
          "form": "ㅇㅇ 남는건 사진이더라",
          "original_form": "ㅇㅇ 남는건 사진이더라",
          "speaker_id": "1",
          "time": "20210904 19:24"
        },
        {
          "id": "MDRW2100040422.1.15",
          "form": "그치 사진기 가져가야겎ㄷ",
          "original_form": "그치 사진기 가져가야겎ㄷ",
          "speaker_id": "2",
          "time": "20210904 19:24"
        },
        {
          "id": "MDRW2100040422.1.16",
          "form": "셀카봉은 안되겠지만",
          "original_form": "셀카봉은 안되겠지만",
          "speaker_id": "2",
          "time": "20210904 19:24"
        },
        {
          "id": "MDRW2100040422.1.17",
          "form": "아니 머래",
          "original_form": "아니 머래",
          "speaker_id": "2",
          "time": "20210904 19:24"
        },
        {
          "id": "MDRW2100040422.1.18",
          "form": "셀카로 찍긴 어려운 카메라지만",
          "original_form": "셀카로 찍긴 어려운 카메라지만",
          "speaker_id": "2",
          "time": "20210904 19:24"
        },
        {
          "id": "MDRW2100040422.1.19",
          "form": "셀카봉도 좋긴 한데",
          "original_form": "셀카봉도 좋긴 한데",
          "speaker_id": "1",
          "time": "20210904 19:26"
        },
        {
          "id": "MDRW2100040422.1.20",
          "form": "삼각대 되는거야?",
          "original_form": "삼각대 되는거야?",
          "speaker_id": "1",
          "time": "20210904 19:26"
        },
        {
          "id": "MDRW2100040422.1.21",
          "form": "삼각대가 없어.....",
          "original_form": "삼각대가 없어.....",
          "speaker_id": "2",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100040422.1.22",
          "form": "되긴..하겠지?",
          "original_form": "되긴..하겠지?",
          "speaker_id": "2",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100040422.1.23",
          "form": "소니 알파 6천인가 그거거던",
          "original_form": "소니 알파 6천인가 그거거던",
          "speaker_id": "2",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100040422.1.24",
          "form": "나중에 짐쌀때",
          "original_form": "나중에 짐쌀때",
          "speaker_id": "1",
          "time": "20210904 20:21"
        },
        {
          "id": "MDRW2100040422.1.25",
          "form": "함 펼쳐지나 봐",
          "original_form": "함 펼쳐지나 봐",
