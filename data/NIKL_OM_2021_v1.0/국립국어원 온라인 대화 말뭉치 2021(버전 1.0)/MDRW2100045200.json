      "utterance": [
        {
          "id": "MDRW2100045200.1.1",
          "form": "어디로 가고싶어요?",
          "original_form": "어디로 가고싶어요?",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045200.1.2",
          "form": "바다",
          "original_form": "바다",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045200.1.3",
          "form": "바다로~~~",
          "original_form": "바다로~~~",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045200.1.4",
          "form": "동해요 서해요 남해요?",
          "original_form": "동해요 서해요 남해요?",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045200.1.5",
          "form": "동해 남해",
          "original_form": "동해 남해",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045200.1.6",
          "form": "파란게조아요",
          "original_form": "파란게조아요",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045200.1.7",
          "form": "물 예쁜데여",
          "original_form": "물 예쁜데여",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045200.1.8",
          "form": "서해도 노을 매력이 있어서 좋던데ㅜㅠ",
          "original_form": "서해도 노을 매력이 있어서 좋던데ㅜㅠ",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045200.1.9",
          "form": "그건 글쵸 근데 동해남해 가기가",
          "original_form": "그건 글쵸 근데 동해남해 가기가",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045200.1.10",
          "form": "더어려워서용",
          "original_form": "더어려워서용",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045200.1.11",
          "form": "파란바다보고파용",
          "original_form": "파란바다보고파용",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045200.1.12",
          "form": "흠...ㅜ 가까움좋겟다",
          "original_form": "흠...ㅜ 가까움좋겟다",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045200.1.13",
          "form": "저도 그렇긴해요 바다보이는데서 살아보고싶어요",
          "original_form": "저도 그렇긴해요 바다보이는데서 살아보고싶어요",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045200.1.14",
          "form": "창문을 열었는데 바다가 보이는....그런곳에서..",
          "original_form": "창문을 열었는데 바다가 보이는....그런곳에서..",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045200.1.15",
          "form": "허걱...너무좋죠",
          "original_form": "허걱...너무좋죠",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045200.1.16",
          "form": "아 주말에 갓다오고싶은데",
          "original_form": "아 주말에 갓다오고싶은데",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045200.1.17",
          "form": "마땅치않네용",
          "original_form": "마땅치않네용",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045200.1.18",
          "form": "제가 ..돈만 많으면.. 바닷가에 집하나 별장으로 갖고 싶네여 ㅎㅎ",
          "original_form": "제가 ..돈만 많으면.. 바닷가에 집하나 별장으로 갖고 싶네여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045200.1.19",
          "form": "name2님 돈 많았음 좋겟네요",
          "original_form": "&name2&님 돈 많았음 좋겟네요",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045200.1.20",
          "form": "놀러가게",
          "original_form": "놀러가게",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045200.1.21",
          "form": "그르게요.. 친구들한데도 별장 빌려주고~~ 제 로망이네여~~",
          "original_form": "그르게요.. 친구들한데도 별장 빌려주고~~ 제 로망이네여~~",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045200.1.22",
          "form": "크....아름다워요 제발 로또됩시다",
          "original_form": "크....아름다워요 제발 로또됩시다",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045200.1.23",
          "form": "네 그럼 풀빌라로 바다 맘껏 보게 해줄게요",
          "original_form": "네 그럼 풀빌라로 바다 맘껏 보게 해줄게요",
