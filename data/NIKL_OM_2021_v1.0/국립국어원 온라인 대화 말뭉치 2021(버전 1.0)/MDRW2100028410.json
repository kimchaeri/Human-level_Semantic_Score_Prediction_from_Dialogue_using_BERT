      "utterance": [
        {
          "id": "MDRW2100028410.1.1",
          "form": "오늘 저 코스로 공을 몇번을 보내는 건지",
          "original_form": "오늘 저 코스로 공을 몇번을 보내는 건지",
          "speaker_id": "1",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100028410.1.2",
          "form": "다행인 건 병살 한번도 안 당함",
          "original_form": "다행인 건 병살 한번도 안 당함",
          "speaker_id": "1",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100028410.1.3",
          "form": "정말다행이다",
          "original_form": "정말다행이다",
          "speaker_id": "2",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100028410.1.4",
          "form": "보경소닉이네",
          "original_form": "보경소닉이네",
          "speaker_id": "1",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100028410.1.5",
          "form": "열심히 뛰었네",
          "original_form": "열심히 뛰었네",
          "speaker_id": "1",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100028410.1.6",
          "form": "ㄱㄴㄲ",
          "original_form": "ㄱㄴㄲ",
          "speaker_id": "2",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100028410.1.7",
          "form": "헐~~님 홍창기 그라운드홈런도 못봤겠네요",
          "original_form": "헐~~님 홍창기 그라운드홈런도 못봤겠네요",
          "speaker_id": "1",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028410.1.8",
          "form": "근데 못 보는 게 나았을듯 그날 못이김",
          "original_form": "근데 못 보는 게 나았을듯 그날 못이김",
          "speaker_id": "1",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028410.1.9",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028410.1.10",
          "form": "방금 하라 틀어줘서 봄",
          "original_form": "방금 하라 틀어줘서 봄",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028410.1.11",
          "form": "진짜웅장",
          "original_form": "진짜웅장",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028410.1.12",
          "form": "홍창기는 신이냐?",
          "original_form": "홍창기는 신이냐?",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028410.1.13",
          "form": "아무래도 신이 하늘의 홍창기인 편이죠",
          "original_form": "아무래도 신이 하늘의 홍창기인 편이죠",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100028410.1.14",
          "form": "아; 그런 편이죠",
          "original_form": "아; 그런 편이죠",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100028410.1.15",
          "form": "그 게임 진짜 이겼어야 했는데...",
          "original_form": "그 게임 진짜 이겼어야 했는데...",
          "speaker_id": "1",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028410.1.16",
          "form": "누가 연장전 없앴냐",
          "original_form": "누가 연장전 없앴냐",
          "speaker_id": "1",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028410.1.17",
          "form": "홍창기가 그라운드홈런까지 쳤는데",
          "original_form": "홍창기가 그라운드홈런까지 쳤는데",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028410.1.18",
          "form": "그날 심지어 서교수님이 홈런침",
          "original_form": "그날 심지어 서교수님이 홈런침",
          "speaker_id": "1",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028410.1.19",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028410.1.20",
          "form": "왜 못 이긴 거야",
          "original_form": "왜 못 이긴 거야",
          "speaker_id": "2",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028410.1.21",
          "form": "걍 뭔가... 안 풀렸던",
          "original_form": "걍 뭔가... 안 풀렸던",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028410.1.22",
          "form": "3점이 다 홈런으로 나고 그 다음에 더 못 친 것도 문제고...",
          "original_form": "3점이 다 홈런으로 나고 그 다음에 더 못 친 것도 문제고...",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028410.1.23",
          "form": "우석이가 1점 줘서 무승부로 끝났을 거임...",
          "original_form": "우석이가 1점 줘서 무승부로 끝났을 거임...",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028410.1.24",
          "form": "에휴 글쿠만",
          "original_form": "에휴 글쿠만",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028410.1.25",
          "form": "아까워라",
          "original_form": "아까워라",
          "speaker_id": "2",
