      "utterance": [
        {
          "id": "MDRW2100033010.1.1",
          "form": "밥드셨어요?",
          "original_form": "밥드셨어요?",
          "speaker_id": "1",
          "time": "20210906 13:38"
        },
        {
          "id": "MDRW2100033010.1.2",
          "form": "전 시리얼이요",
          "original_form": "전 시리얼이요",
          "speaker_id": "2",
          "time": "20210906 13:38"
        },
        {
          "id": "MDRW2100033010.1.3",
          "form": "저는 소고기랑 에그타르트 먹었어요~",
          "original_form": "저는 소고기랑 에그타르트 먹었어요~",
          "speaker_id": "1",
          "time": "20210906 13:38"
        },
        {
          "id": "MDRW2100033010.1.4",
          "form": "너무 맛있는거 드셨네요",
          "original_form": "너무 맛있는거 드셨네요",
          "speaker_id": "2",
          "time": "20210906 13:39"
        },
        {
          "id": "MDRW2100033010.1.5",
          "form": "좋아는 가수있어요?",
          "original_form": "좋아는 가수있어요?",
          "speaker_id": "1",
          "time": "20210906 13:41"
        },
        {
          "id": "MDRW2100033010.1.6",
          "form": "아이유요",
          "original_form": "아이유요",
          "speaker_id": "2",
          "time": "20210906 13:42"
        },
        {
          "id": "MDRW2100033010.1.7",
          "form": "저도 아이유 좋아해요 콘서트가본적있어요???",
          "original_form": "저도 아이유 좋아해요 콘서트가본적있어요???",
          "speaker_id": "1",
          "time": "20210906 13:43"
        },
        {
          "id": "MDRW2100033010.1.8",
          "form": "아뇨 콘서트는 못가봤네요",
          "original_form": "아뇨 콘서트는 못가봤네요",
          "speaker_id": "2",
          "time": "20210906 13:45"
        },
        {
          "id": "MDRW2100033010.1.9",
          "form": "저는 아이유 미아 부를 때 좋아했어요\r\n이 정도까지 인기 많아 질줄은~",
          "original_form": "저는 아이유 미아 부를 때 좋아했어요\r\n이 정도까지 인기 많아 질줄은~",
          "speaker_id": "1",
          "time": "20210906 13:47"
        },
        {
          "id": "MDRW2100033010.1.10",
          "form": "연기도 잘하고 인기가 많죠ㅋㅋ",
          "original_form": "연기도 잘하고 인기가 많죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:48"
        },
        {
          "id": "MDRW2100033010.1.11",
          "form": "스포티파이 들어요? 아님 멜론??",
          "original_form": "스포티파이 들어요? 아님 멜론??",
          "speaker_id": "1",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100033010.1.12",
          "form": "멜론이요",
          "original_form": "멜론이요",
          "speaker_id": "2",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100033010.1.13",
          "form": "스트리밍 돌려요?",
          "original_form": "스트리밍 돌려요?",
          "speaker_id": "1",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100033010.1.14",
          "form": "네 맞아요ㅋ",
          "original_form": "네 맞아요ㅋ",
          "speaker_id": "2",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100033010.1.15",
          "form": "누구해요??",
          "original_form": "누구해요??",
          "speaker_id": "1",
