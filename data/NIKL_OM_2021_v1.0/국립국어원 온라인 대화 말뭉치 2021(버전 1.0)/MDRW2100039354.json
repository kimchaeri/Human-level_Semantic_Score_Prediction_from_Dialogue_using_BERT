      "utterance": [
        {
          "id": "MDRW2100039354.1.1",
          "form": "집안일 하시나용 ?",
          "original_form": "집안일 하시나용 ?",
          "speaker_id": "1",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100039354.1.2",
          "form": "네 합니당",
          "original_form": "네 합니당",
          "speaker_id": "2",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100039354.1.3",
          "form": "주말에도 밀린 집안일ㅜㅜ",
          "original_form": "주말에도 밀린 집안일ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100039354.1.4",
          "form": "제말입니다 .. ㅜㅜ 그래도 주말이 더조아용 ... 출근보다는ㅋㅋㅋㅋㅋ",
          "original_form": "제말입니다 .. ㅜㅜ 그래도 주말이 더조아용 ... 출근보다는ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100039354.1.5",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "2",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100039354.1.6",
          "form": "집안일 뭐가 제일 싫으신가요ㅋㅋ",
          "original_form": "집안일 뭐가 제일 싫으신가요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:45"
        },
        {
          "id": "MDRW2100039354.1.7",
          "form": "흠 .. 뭐가 딱히싫은건아니구 ..",
          "original_form": "흠 .. 뭐가 딱히싫은건아니구 ..",
          "speaker_id": "1",
          "time": "20210903 16:45"
        },
        {
          "id": "MDRW2100039354.1.8",
          "form": "전반적으로 다 ..",
          "original_form": "전반적으로 다 ..",
          "speaker_id": "1",
          "time": "20210903 16:45"
        },
        {
          "id": "MDRW2100039354.1.9",
