      "utterance": [
        {
          "id": "MDRW2100047471.1.1",
          "form": "크크 당면사리도 너무 맛있죠ㅎㅎㅎ",
          "original_form": "크크 당면사리도 너무 맛있죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100047471.1.2",
          "form": "그런 음식에는 면사리 안넣으면 너무 아쉬워요",
          "original_form": "그런 음식에는 면사리 안넣으면 너무 아쉬워요",
          "speaker_id": "2",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100047471.1.3",
          "form": "크으 저랑같네요",
          "original_form": "크으 저랑같네요",
          "speaker_id": "1",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100047471.1.4",
          "form": "저 면킬러예요",
          "original_form": "저 면킬러예요",
          "speaker_id": "1",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100047471.1.5",
          "form": "저두요ㅠㅜㅜ 면들 너무 사랑해요...",
          "original_form": "저두요ㅠㅜㅜ 면들 너무 사랑해요...",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100047471.1.6",
          "form": "종류도 다양하구ㅠㅜ",
          "original_form": "종류도 다양하구ㅠㅜ",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100047471.1.7",
          "form": "크 저도요",
          "original_form": "크 저도요",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100047471.1.8",
          "form": "뭘 아시는군요",
          "original_form": "뭘 아시는군요",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100047471.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ제일 좋아하는 면요리 있으세요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ제일 좋아하는 면요리 있으세요?",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100047471.1.10",
          "form": "저는 아무래도",
          "original_form": "저는 아무래도",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100047471.1.11",
          "form": "이게 면요리는아닌데",
          "original_form": "이게 면요리는아닌데",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100047471.1.12",
          "form": "닭한마리 먹고나서 그 육수국물에",
          "original_form": "닭한마리 먹고나서 그 육수국물에",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100047471.1.13",
          "form": "칼국수사리 추가해먹는게 그렇게좋더라고요ㅋㅋㅋㅋ",
          "original_form": "칼국수사리 추가해먹는게 그렇게좋더라고요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100047471.1.14",
          "form": "으악 너무좋죠ㅜㅠㅠ",
          "original_form": "으악 너무좋죠ㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100047471.1.15",
          "form": "사실 전 샤브샤브나 닭한마리같은거 먹을때",
          "original_form": "사실 전 샤브샤브나 닭한마리같은거 먹을때",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100047471.1.16",
          "form": "마지막 칼국수사리를 위해서 먹는다고 볼수있을정도...!",
          "original_form": "마지막 칼국수사리를 위해서 먹는다고 볼수있을정도...!",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100047471.1.17",
          "form": "등촌샤브칼국수도 그래서 좋아해요",
          "original_form": "등촌샤브칼국수도 그래서 좋아해요",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100047471.1.18",
          "form": "님 저랑 입맛소울메이트네요",
          "original_form": "님 저랑 입맛소울메이트네요",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100047471.1.19",
          "form": "전요",
          "original_form": "전요",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100047471.1.20",
          "form": "샤브샤브나 닭한마리에 칼국수사리안먹는사람이랑은",
          "original_form": "샤브샤브나 닭한마리에 칼국수사리안먹는사람이랑은",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100047471.1.21",
          "form": "같이 밥먹으면 너무슬퍼요 등촌도요ㅋㅋㅋㅋ",
          "original_form": "같이 밥먹으면 너무슬퍼요 등촌도요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100047471.1.22",
          "form": "아니....그런사람이 존재한가요ㅠㅠㅠㅠ",
          "original_form": "아니....그런사람이 존재한가요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100047471.1.23",
          "form": "얘기만 들어도 슬픈데요....?",
          "original_form": "얘기만 들어도 슬픈데요....?",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100047471.1.24",
          "form": "어떻게 칼국수를 포기하죠ㅋㅋㅋㅋㅋ",
          "original_form": "어떻게 칼국수를 포기하죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100047471.1.25",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100047471.1.26",
          "form": "그런사람들이",
          "original_form": "그런사람들이",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100047471.1.27",
          "form": "있더라고요!",
          "original_form": "있더라고요!",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100047471.1.28",
          "form": "헉 너모 불쌍한 사람들ㅠㅠ 그런 맛있는걸....!!",
          "original_form": "헉 너모 불쌍한 사람들ㅠㅠ 그런 맛있는걸....!!",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100047471.1.29",
          "form": "갑자기 비빔국수 먹고싶내요!!",
          "original_form": "갑자기 비빔국수 먹고싶내요!!",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100047471.1.30",
          "form": "이시간에...!",
          "original_form": "이시간에...!",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100047471.1.31",
          "form": "ㅋㅋㅋ 잔치vs비빔",
          "original_form": "ㅋㅋㅋ 잔치vs비빔",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100047471.1.32",
          "form": "전 무족건 비빔!!!ㅋㄱㄱㄱㄲ",
          "original_form": "전 무족건 비빔!!!ㅋㄱㄱㄱㄲ",
          "speaker_id": "2",
          "time": "20210826 23:28"
        },
        {
