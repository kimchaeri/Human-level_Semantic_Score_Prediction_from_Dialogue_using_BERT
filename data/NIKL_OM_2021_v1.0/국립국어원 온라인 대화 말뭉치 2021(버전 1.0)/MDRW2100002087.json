      "utterance": [
        {
          "id": "MDRW2100002087.1.1",
          "form": "안녕하세요 학교 다니시나요?",
          "original_form": "안녕하세요 학교 다니시나요?",
          "speaker_id": "1",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002087.1.2",
          "form": "아니요 학교다니시나요?",
          "original_form": "아니요 학교다니시나요?",
          "speaker_id": "2",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002087.1.3",
          "form": "저도 안다녀요!",
          "original_form": "저도 안다녀요!",
          "speaker_id": "1",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002087.1.4",
          "form": "안다녀서 과제 같은 거 없어서 좋은데",
          "original_form": "안다녀서 과제 같은 거 없어서 좋은데",
          "speaker_id": "1",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002087.1.5",
          "form": "가끔은 학교가 그리운 거 같아요",
          "original_form": "가끔은 학교가 그리운 거 같아요",
          "speaker_id": "1",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002087.1.6",
          "form": "어떤점이 그리운가요?",
          "original_form": "어떤점이 그리운가요?",
          "speaker_id": "2",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002087.1.7",
          "form": "그냥 아무 걱정없이 놀던거요?",
          "original_form": "그냥 아무 걱정없이 놀던거요?",
          "speaker_id": "1",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002087.1.8",
          "form": "그런건 진짜 너무 그리워요..",
          "original_form": "그런건 진짜 너무 그리워요..",
          "speaker_id": "2",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002087.1.9",
          "form": "분명 그때도 고민과 걱정 있던 거 같은데 지금 생각하니 생각이 안나요",
          "original_form": "분명 그때도 고민과 걱정 있던 거 같은데 지금 생각하니 생각이 안나요",
          "speaker_id": "1",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002087.1.10",
          "form": "맞아요 너무 그립네요",
          "original_form": "맞아요 너무 그립네요",
          "speaker_id": "1",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002087.1.11",
          "form": "그럼 가장 재밌었던 추억은 뭐가있나요?",
          "original_form": "그럼 가장 재밌었던 추억은 뭐가있나요?",
          "speaker_id": "2",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002087.1.12",
          "form": "그냥 쉬는시간이나 공강 때 친구들하고 논 추억이요ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 쉬는시간이나 공강 때 친구들하고 논 추억이요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002087.1.13",
          "form": "왜 그게 그리운지 지원님은요?",
          "original_form": "왜 그게 그리운지 지원님은요?",
          "speaker_id": "1",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002087.1.14",
          "form": "저도 비슷한거같아요.\r\n공강이나 자체휴강 이런거",
          "original_form": "저도 비슷한거같아요.\r\n공강이나 자체휴강 이런거",
          "speaker_id": "2",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100002087.1.15",
          "form": "학생때만 할 수 있는 특권같은거잖아요",
          "original_form": "학생때만 할 수 있는 특권같은거잖아요",
          "speaker_id": "2",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100002087.1.16",
          "form": "딱 그 때만 느낄 수 있는 그런 감성?",
          "original_form": "딱 그 때만 느낄 수 있는 그런 감성?",
          "speaker_id": "2",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002087.1.17",
          "form": "맞아요ㅋㅋㅋㅋㅋㅋㅋ 자체휴강 안해봤는데",
          "original_form": "맞아요ㅋㅋㅋㅋㅋㅋㅋ 자체휴강 안해봤는데",
          "speaker_id": "1",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002087.1.18",
          "form": "해볼껄도 싶고 ㅎㅎ",
          "original_form": "해볼껄도 싶고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002087.1.19",
          "form": "에이 한번 쯤은 해봐야죠",
          "original_form": "에이 한번 쯤은 해봐야죠",
          "speaker_id": "2",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002087.1.20",
          "form": "근데.. 생각해보면 학점과 맞바꾸기엔 .. 무모했죠",
          "original_form": "근데.. 생각해보면 학점과 맞바꾸기엔 .. 무모했죠",
          "speaker_id": "2",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002087.1.21",
          "form": "흑흑흑",
          "original_form": "흑흑흑",
          "speaker_id": "2",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100002087.1.22",
          "form": "맞아요.... 내 학점... 돌려내....",
          "original_form": "맞아요.... 내 학점... 돌려내....",
          "speaker_id": "1",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100002087.1.23",
          "form": "그래도 시험공부랑 생각하면 .....",
          "original_form": "그래도 시험공부랑 생각하면 .....",
          "speaker_id": "1",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100002087.1.24",
          "form": "끔찍하죠",
          "original_form": "끔찍하죠",
          "speaker_id": "2",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100002087.1.25",
          "form": "어찌했나몰라",
          "original_form": "어찌했나몰라",
          "speaker_id": "2",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100002087.1.26",
          "form": "진짜 돌아가라면 두 번 못해요..",
          "original_form": "진짜 돌아가라면 두 번 못해요..",
          "speaker_id": "2",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100002087.1.27",
          "form": "노는건 백번을 하래도 하는데",
          "original_form": "노는건 백번을 하래도 하는데",
          "speaker_id": "2",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100002087.1.28",
          "form": "맞아요 공부는 넣어둘래요 ㅎ ㅎㅎ 진심",
          "original_form": "맞아요 공부는 넣어둘래요 ㅎ ㅎㅎ 진심",
