      "utterance": [
        {
          "id": "MDRW2100041934.1.1",
          "form": "동물 키우시나요",
          "original_form": "동물 키우시나요",
          "speaker_id": "2",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100041934.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100041934.1.3",
          "form": "혹시 좋아하시는 동물 있으세요?",
          "original_form": "혹시 좋아하시는 동물 있으세요?",
          "speaker_id": "2",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100041934.1.4",
          "form": "키워보고싶은 동물이라거나",
          "original_form": "키워보고싶은 동물이라거나",
          "speaker_id": "2",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100041934.1.5",
          "form": "물고기키워봤네요",
          "original_form": "물고기키워봤네요",
          "speaker_id": "1",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100041934.1.6",
          "form": "아 어떤 종이요?",
          "original_form": "아 어떤 종이요?",
          "speaker_id": "2",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100041934.1.7",
          "form": "구피요..",
          "original_form": "구피요..",
          "speaker_id": "1",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100041934.1.8",
          "form": "앗 귀여웠을거같아요",
          "original_form": "앗 귀여웠을거같아요",
          "speaker_id": "2",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100041934.1.9",
          "form": "이름도 붙여주셨나요?",
          "original_form": "이름도 붙여주셨나요?",
          "speaker_id": "2",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100041934.1.10",
          "form": "종이 구피예요",
          "original_form": "종이 구피예요",
          "speaker_id": "1",
          "time": "20210907 18:39"
        },
        {
          "id": "MDRW2100041934.1.11",
          "form": "넵넵 알아요",
          "original_form": "넵넵 알아요",
          "speaker_id": "2",
          "time": "20210907 18:39"
        },
        {
          "id": "MDRW2100041934.1.12",
          "form": "이름이 따로 있었나해서요",
          "original_form": "이름이 따로 있었나해서요",
          "speaker_id": "2",
          "time": "20210907 18:39"
        },
        {
          "id": "MDRW2100041934.1.13",
          "form": "아니요.이름은없었네요",
          "original_form": "아니요.이름은없었네요",
          "speaker_id": "1",
          "time": "20210907 18:43"
        },
        {
          "id": "MDRW2100041934.1.14",
          "form": "구피는 초보자도 키우기 쉽나요?",
          "original_form": "구피는 초보자도 키우기 쉽나요?",
          "speaker_id": "2",
          "time": "20210907 18:44"
        },
        {
          "id": "MDRW2100041934.1.15",
          "form": "네..완전쉬워요",
          "original_form": "네..완전쉬워요",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100041934.1.16",
          "form": "아 어떤거는 물온도도 세심히봐주고",
          "original_form": "아 어떤거는 물온도도 세심히봐주고",
          "speaker_id": "2",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100041934.1.17",
          "form": "신경쓸게 많다고 하더라구요",
          "original_form": "신경쓸게 많다고 하더라구요",
          "speaker_id": "2",
          "time": "20210907 18:50"
        }
