      "utterance": [
        {
          "id": "MDRW2100029982.1.1",
          "form": "너 전시회 가본 적 있어?",
          "original_form": "너 전시회 가본 적 있어?",
          "speaker_id": "1",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100029982.1.2",
          "form": "헐 전시회",
          "original_form": "헐 전시회",
          "speaker_id": "2",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100029982.1.3",
          "form": "초등학생때 방학숙제 하려고 간게 마지막임",
          "original_form": "초등학생때 방학숙제 하려고 간게 마지막임",
          "speaker_id": "2",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100029982.1.4",
          "form": "어디 갔었는데?",
          "original_form": "어디 갔었는데?",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100029982.1.5",
          "form": "초딩땐 엄청 다녔었는데",
          "original_form": "초딩땐 엄청 다녔었는데",
          "speaker_id": "2",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100029982.1.6",
          "form": "인체의 신비 전시회나",
          "original_form": "인체의 신비 전시회나",
          "speaker_id": "2",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100029982.1.7",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100029982.1.8",
          "form": "투탕카멘의 보물이나...",
          "original_form": "투탕카멘의 보물이나...",
          "speaker_id": "2",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100029982.1.9",
          "form": "그런 전사회도 잇군",
          "original_form": "그런 전사회도 잇군",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100029982.1.10",
          "form": "뭐시기 중동국가 보물전시회같은거",
          "original_form": "뭐시기 중동국가 보물전시회같은거",
          "speaker_id": "2",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100029982.1.11",
          "form": "나도 리히텐슈타인 왕가의 보물전이었나?",
          "original_form": "나도 리히텐슈타인 왕가의 보물전이었나?",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100029982.1.12",
          "form": "전시회가 특별히 컨셉잡고 하는것만 포함이 아니면 대영박물관도 가봤어",
          "original_form": "전시회가 특별히 컨셉잡고 하는것만 포함이 아니면 대영박물관도 가봤어",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100029982.1.13",
          "form": "헐 그랬을걸",
          "original_form": "헐 그랬을걸",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100029982.1.14",
          "form": "작년에인가 제작년에 가봤어",
          "original_form": "작년에인가 제작년에 가봤어",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100029982.1.15",
          "form": "아 아닌가",
          "original_form": "아 아닌가",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100029982.1.16",
          "form": "와ㅏ 대영박물관 우와",
          "original_form": "와ㅏ 대영박물관 우와",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100029982.1.17",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100029982.1.18",
          "form": "멋지다!",
          "original_form": "멋지다!",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100029982.1.19",
          "form": "대영박물관",
          "original_form": "대영박물관",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100029982.1.20",
          "form": "영국에 잇는거 아닌가?",
          "original_form": "영국에 잇는거 아닌가?",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100029982.1.21",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100029982.1.22",
          "form": "그건 중학생때였다",
          "original_form": "그건 중학생때였다",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100029982.1.23",
          "form": "자기네 나라 물건이 얼마 없어서 입장료도 못받아 거기",
          "original_form": "자기네 나라 물건이 얼마 없어서 입장료도 못받아 거기",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100029982.1.24",
          "form": "우와 나도 대영박물관 가보고 싶어....",
          "original_form": "우와 나도 대영박물관 가보고 싶어....",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100029982.1.25",
          "form": "진짜 장난아니게 넓다",
          "original_form": "진짜 장난아니게 넓다",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100029982.1.26",
          "form": "한국관도 있어",
          "original_form": "한국관도 있어",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100029982.1.27",
          "form": "좋아하는 영화 잇니?",
          "original_form": "좋아하는 영화 잇니?",
          "speaker_id": "1",
          "time": "20210904 18:43"
        }
      ]
    }
  ]
