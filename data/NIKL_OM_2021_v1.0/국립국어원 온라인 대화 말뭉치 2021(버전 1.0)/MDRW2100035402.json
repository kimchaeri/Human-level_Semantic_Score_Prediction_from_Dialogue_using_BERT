      "utterance": [
        {
          "id": "MDRW2100035402.1.1",
          "form": "주로 어떤 잔소리 하세요??",
          "original_form": "주로 어떤 잔소리 하세요??",
          "speaker_id": "1",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100035402.1.2",
          "form": "뭐 엄마 일 도우라는둥",
          "original_form": "뭐 엄마 일 도우라는둥",
          "speaker_id": "2",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100035402.1.3",
          "form": "시비걸어요",
          "original_form": "시비걸어요",
          "speaker_id": "2",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100035402.1.4",
          "form": "켁 ㅠㅠ 혹시 몇살이세요??",
          "original_form": "켁 ㅠㅠ 혹시 몇살이세요??",
          "speaker_id": "1",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100035402.1.5",
          "form": "30대 후반요",
          "original_form": "30대 후반요",
          "speaker_id": "2",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100035402.1.6",
          "form": "입으로 저 저평가하고",
          "original_form": "입으로 저 저평가하고",
          "speaker_id": "2",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100035402.1.7",
          "form": "입으로 엄마돕는...",
          "original_form": "입으로 엄마돕는...",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100035402.1.8",
          "form": "그게 가장의 특권인듯요 ㅋ",
          "original_form": "그게 가장의 특권인듯요 ㅋ",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100035402.1.9",
          "form": "내 밥은 못먹게 불편주고",
          "original_form": "내 밥은 못먹게 불편주고",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100035402.1.10",
          "form": "ㅠㅠㅠ 아 저랑 비슷하시네요ㅠ 부모님 입장에서는 나이 있는데 결혼을 아직 안하셔서.. 그런신건가ㅠㅠ",
          "original_form": "ㅠㅠㅠ 아 저랑 비슷하시네요ㅠ 부모님 입장에서는 나이 있는데 결혼을 아직 안하셔서.. 그런신건가ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100035402.1.11",
          "form": "집에서 막 있으니 답답하고 뭐그런 잔소리들이겠네요 ㅠㅠㅠ",
          "original_form": "집에서 막 있으니 답답하고 뭐그런 잔소리들이겠네요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100035402.1.12",
          "form": "네 그냥 저한테",
          "original_form": "네 그냥 저한테",
          "speaker_id": "2",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100035402.1.13",
          "form": "꼬투리잡아서 시비거는거라 느끼거든요 저는",
          "original_form": "꼬투리잡아서 시비거는거라 느끼거든요 저는",
          "speaker_id": "2",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100035402.1.14",
          "form": "이미 듣는 사람이 그렇게 받아들이는 말이면",
          "original_form": "이미 듣는 사람이 그렇게 받아들이는 말이면",
          "speaker_id": "2",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100035402.1.15",
          "form": "표현을 잘 못하시는것 같네요 ㅎㅎ",
          "original_form": "표현을 잘 못하시는것 같네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100035402.1.16",
          "form": "서로 감정만 상하고요",
          "original_form": "서로 감정만 상하고요",
          "speaker_id": "2",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100035402.1.17",
          "form": "맞아요 ㅎㅎ 그것도 맞는 말이죠",
          "original_form": "맞아요 ㅎㅎ 그것도 맞는 말이죠",
          "speaker_id": "1",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100035402.1.18",
          "form": "ㅠㅠ 하고나면 근데 부모님도 후회하셨을지도..저도 애한테 소리한번 지르고 나면 아차 싶은데..ㅎㅎㅎ",
          "original_form": "ㅠㅠ 하고나면 근데 부모님도 후회하셨을지도..저도 애한테 소리한번 지르고 나면 아차 싶은데..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100035402.1.19",
          "form": "아뇨 저희부모님은",
          "original_form": "아뇨 저희부모님은",
          "speaker_id": "2",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100035402.1.20",
          "form": "좋은 말로해서 후회할사람이 아닌거같아여",
          "original_form": "좋은 말로해서 후회할사람이 아닌거같아여",
          "speaker_id": "2",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100035402.1.21",
          "form": "아 ㅋㅋ 이런.ㅠㅠ",
          "original_form": "아 ㅋㅋ 이런.ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100035402.1.22",
          "form": "말씀대로 표현을 못하는거 같긴 한데",
          "original_form": "말씀대로 표현을 못하는거 같긴 한데",
          "speaker_id": "2",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100035402.1.23",
          "form": "그건 분명히 있을거게요",
          "original_form": "그건 분명히 있을거게요",
          "speaker_id": "1",
          "time": "20210829 16:44"
        }
      ]
    }
  ]
