      "utterance": [
        {
          "id": "MDRW2100000769.1.1",
          "form": "저녁 뭐드세요?",
          "original_form": "저녁 뭐드세요?",
          "speaker_id": "1",
          "time": "20210819 19:07"
        },
        {
          "id": "MDRW2100000769.1.2",
          "form": "저는 오늘은 라면끓여먹을것같아요ㅋㅋㅋ",
          "original_form": "저는 오늘은 라면끓여먹을것같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100000769.1.3",
          "form": "꽃비님은 뭐드세요?",
          "original_form": "꽃비님은 뭐드세요?",
          "speaker_id": "2",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100000769.1.4",
          "form": "오 라면~간편하니 좋네요",
          "original_form": "오 라면~간편하니 좋네요",
          "speaker_id": "1",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100000769.1.5",
          "form": "저는 제육볶음 해 먹으려고요~ㅎ",
          "original_form": "저는 제육볶음 해 먹으려고요~ㅎ",
          "speaker_id": "1",
          "time": "20210819 19:09"
        },
        {
          "id": "MDRW2100000769.1.6",
          "form": "제육볶음!!! 맛있겠어요",
          "original_form": "제육볶음!!! 맛있겠어요",
          "speaker_id": "2",
          "time": "20210819 19:23"
        },
        {
          "id": "MDRW2100000769.1.7",
          "form": "직접해서 드시나요?",
          "original_form": "직접해서 드시나요?",
          "speaker_id": "2",
          "time": "20210819 19:23"
        },
        {
          "id": "MDRW2100000769.1.8",
          "form": "네~직접해 먹었어요",
          "original_form": "네~직접해 먹었어요",
          "speaker_id": "1",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100000769.1.9",
          "form": "맛있으셨겠어요!!",
          "original_form": "맛있으셨겠어요!!",
          "speaker_id": "2",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100000769.1.10",
          "form": "니니님은 뭐 드셨나요?",
          "original_form": "니니님은 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100000769.1.11",
          "form": "저는 내일 연차여서 남이섬 놀러갈려고요",
          "original_form": "저는 내일 연차여서 남이섬 놀러갈려고요",
          "speaker_id": "2",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100000769.1.12",
          "form": "앗 저는 라면먹고 복숭아 먹고있어요",
          "original_form": "앗 저는 라면먹고 복숭아 먹고있어요",
          "speaker_id": "2",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100000769.1.13",
          "form": "아~복숭아 맛있겠어요",
          "original_form": "아~복숭아 맛있겠어요",
          "speaker_id": "1",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100000769.1.14",
          "form": "요즘 복숭아철이라서 자주 사먹고있어요",
          "original_form": "요즘 복숭아철이라서 자주 사먹고있어요",
          "speaker_id": "2",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100000769.1.15",
          "form": "딱복이에요?",
          "original_form": "딱복이에요?",
          "speaker_id": "1",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100000769.1.16",
          "form": "딱복이요!!!",
          "original_form": "딱복이요!!!",
          "speaker_id": "2",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100000769.1.17",
          "form": "저는 딱복이 좋아해요",
          "original_form": "저는 딱복이 좋아해요",
          "speaker_id": "2",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100000769.1.18",
          "form": "아~~저는 복숭아는 다 좋아해요 ㅎㅎ",
          "original_form": "아~~저는 복숭아는 다 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:35"
        },
        {
