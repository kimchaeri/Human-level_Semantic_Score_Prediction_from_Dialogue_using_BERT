      "utterance": [
        {
          "id": "MDRW2100046839.1.1",
          "form": "저 요즘 애니에 빠져있어요",
          "original_form": "저 요즘 애니에 빠져있어요",
          "speaker_id": "1",
          "time": "20211105 01:30"
        },
        {
          "id": "MDRW2100046839.1.2",
          "form": "엇 어떤 애니 보시나요?!",
          "original_form": "엇 어떤 애니 보시나요?!",
          "speaker_id": "2",
          "time": "20211105 10:14"
        },
        {
          "id": "MDRW2100046839.1.3",
          "form": "저는 애니를 잘 몰라서",
          "original_form": "저는 애니를 잘 몰라서",
          "speaker_id": "2",
          "time": "20211105 10:14"
        },
        {
          "id": "MDRW2100046839.1.4",
          "form": "하이큐랑 진격의거인 이정도만 들어봤어요 ㅎ",
          "original_form": "하이큐랑 진격의거인 이정도만 들어봤어요 ㅎ",
          "speaker_id": "2",
          "time": "20211105 10:14"
        },
        {
          "id": "MDRW2100046839.1.5",
          "form": "진격의거인 재밌죠!",
          "original_form": "진격의거인 재밌죠!",
          "speaker_id": "1",
          "time": "20211105 10:41"
        },
        {
          "id": "MDRW2100046839.1.6",
          "form": "진격의거인 실사판봤어요?",
          "original_form": "진격의거인 실사판봤어요?",
          "speaker_id": "1",
          "time": "20211105 10:41"
        },
        {
          "id": "MDRW2100046839.1.7",
          "form": "실사판?",
          "original_form": "실사판{emoji:…}?",
          "speaker_id": "2",
          "time": "20211105 10:41"
        },
        {
          "id": "MDRW2100046839.1.8",
          "form": "영화요",
          "original_form": "영화요",
          "speaker_id": "1",
          "time": "20211105 10:41"
        },
        {
          "id": "MDRW2100046839.1.9",
          "form": "실사판도 있나요.?",
          "original_form": "실사판도 있나요{emoji:…}.?",
          "speaker_id": "2",
          "time": "20211105 10:41"
        },
        {
          "id": "MDRW2100046839.1.10",
          "form": "아니요 ㅠㅠ 뭔가 상상이 안가는데 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니요 ㅠㅠ 뭔가 상상이 안가는데 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 10:41"
        },
        {
          "id": "MDRW2100046839.1.11",
          "form": "영화로나와서 개봉했는데",
          "original_form": "영화로나와서 개봉했는데",
          "speaker_id": "1",
          "time": "20211105 10:41"
        },
        {
          "id": "MDRW2100046839.1.12",
          "form": "상상이상의 저퀄리티",
          "original_form": "상상이상의 저퀄리티",
          "speaker_id": "1",
          "time": "20211105 10:42"
        },
        {
          "id": "MDRW2100046839.1.13",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20211105 10:42"
        },
        {
          "id": "MDRW2100046839.1.14",
          "form": "",
          "original_form": "{share:info}",
          "speaker_id": "2",
          "time": "20211105 10:42"
        },
        {
          "id": "MDRW2100046839.1.15",
          "form": "래요",
          "original_form": "래요",
          "speaker_id": "1",
          "time": "20211105 10:42"
        },
        {
          "id": "MDRW2100046839.1.16",
          "form": "맞아요 저거",
          "original_form": "맞아요 저거",
          "speaker_id": "1",
          "time": "20211105 10:42"
        },
        {
          "id": "MDRW2100046839.1.17",
          "form": "이건가욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이건가욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 10:42"
        },
        {
          "id": "MDRW2100046839.1.18",
          "form": "상상 이상의 저퀄리티 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "상상 이상의 저퀄리티 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 10:42"
        },
        {
          "id": "MDRW2100046839.1.19",
          "form": "역시 애니는 2d일때 아름다운것",
          "original_form": "역시 애니는 2d일때 아름다운것",
          "speaker_id": "2",
          "time": "20211105 10:42"
        },
        {
          "id": "MDRW2100046839.1.20",
          "form": "일본에서는 이미성공한 애니를 저렇게 많이 만든데요",
          "original_form": "일본에서는 이미성공한 애니를 저렇게 많이 만든데요",
          "speaker_id": "1",
          "time": "20211105 10:42"
        },
        {
          "id": "MDRW2100046839.1.21",
          "form": "맞아요 2D가 제일 아름다워요",
          "original_form": "맞아요 2D가 제일 아름다워요",
          "speaker_id": "1",
          "time": "20211105 10:43"
        },
        {
          "id": "MDRW2100046839.1.22",
          "form": "귀멸의칼날은 보셨나요?",
          "original_form": "귀멸의칼날은 보셨나요?",
          "speaker_id": "1",
          "time": "20211105 10:43"
        },
        {
