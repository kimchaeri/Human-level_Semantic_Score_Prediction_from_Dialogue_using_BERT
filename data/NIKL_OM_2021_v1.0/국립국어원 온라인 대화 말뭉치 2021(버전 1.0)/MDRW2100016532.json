      "utterance": [
        {
          "id": "MDRW2100016532.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210826 13:32"
        },
        {
          "id": "MDRW2100016532.1.2",
          "form": "안녕하세요 카페 자주 다니세요?",
          "original_form": "안녕하세요 카페 자주 다니세요?",
          "speaker_id": "2",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016532.1.3",
          "form": "네 카페 자주가요! 커피 좋아해서요!",
          "original_form": "네 카페 자주가요! 커피 좋아해서요!",
          "speaker_id": "1",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100016532.1.4",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100016532.1.5",
          "form": "저도 자주 가는 편이에요",
          "original_form": "저도 자주 가는 편이에요",
          "speaker_id": "2",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100016532.1.6",
          "form": "프랜차이즈로 가세요?",
          "original_form": "프랜차이즈로 가세요?",
          "speaker_id": "2",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100016532.1.7",
          "form": "네네 프차도 가고 개인카페 핫플도 찾아가구 그래요 ㅋㅋㅋㅋ",
          "original_form": "네네 프차도 가고 개인카페 핫플도 찾아가구 그래요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016532.1.8",
          "form": "어디 주로 이용하세요?",
          "original_form": "어디 주로 이용하세요?",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016532.1.9",
          "form": "제가 바닷가에 살거든요",
          "original_form": "제가 바닷가에 살거든요",
          "speaker_id": "2",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016532.1.10",
          "form": "주변에 예쁜카페가 많아요",
          "original_form": "주변에 예쁜카페가 많아요",
          "speaker_id": "2",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016532.1.11",
          "form": "그런데 제일 많이 가는 곳은 스타벅스에요",
          "original_form": "그런데 제일 많이 가는 곳은 스타벅스에요",
          "speaker_id": "2",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016532.1.12",
          "form": "바닷가!!! 뷰예쁜 카페 많겟어요 ㅠㅠ",
          "original_form": "바닷가!!! 뷰예쁜 카페 많겟어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016532.1.13",
          "form": "네 그렇긴 한대",
          "original_form": "네 그렇긴 한대",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016532.1.14",
          "form": "요즘은 다 비슷비슷핫 컨셉의 카페들이고",
          "original_form": "요즘은 다 비슷비슷핫 컨셉의 카페들이고",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016532.1.15",
          "form": "가격은 엄청 높아지고",
          "original_form": "가격은 엄청 높아지고",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016532.1.16",
          "form": "맛은 가격 못따라가는것 같고",
          "original_form": "맛은 가격 못따라가는것 같고",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016532.1.17",
          "form": "그래서 결국 스타벅스 이용하게 되더라고요",
          "original_form": "그래서 결국 스타벅스 이용하게 되더라고요",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016532.1.18",
          "form": "카페가면 가자 잘 드시는 음로는?",
          "original_form": "카페가면 가자 잘 드시는 음로는?",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016532.1.19",
          "form": "아아 맞아요 맛보다 자리값느낌으루 비싸더라구요",
          "original_form": "아아 맞아요 맛보다 자리값느낌으루 비싸더라구요",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016532.1.20",
          "form": "오히려 스벅이 합리적인느낌!",
          "original_form": "오히려 스벅이 합리적인느낌!",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016532.1.21",
          "form": "저는 아아 좋아해요! 가끔 라떼",
          "original_form": "저는 아아 좋아해요! 가끔 라떼",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016532.1.22",
          "form": "저도 아아",
          "original_form": "저도 아아",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016532.1.23",
          "form": "칼로리가 낮아서",
          "original_form": "칼로리가 낮아서",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016532.1.24",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016532.1.25",
          "form": "스벅 라떼는 제입에는 안맞아서",
          "original_form": "스벅 라떼는 제입에는 안맞아서",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016532.1.26",
          "form": "맞아요 스벅라떼 좀 연해서 좀 그래여 ㅋㅋㅋ",
          "original_form": "맞아요 스벅라떼 좀 연해서 좀 그래여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016532.1.27",
          "form": "디저트는 드세요?",
          "original_form": "디저트는 드세요?",
          "speaker_id": "1",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016532.1.28",
          "form": "디저트는 잘 안먹어요",
          "original_form": "디저트는 잘 안먹어요",
          "speaker_id": "2",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100016532.1.29",
          "form": "그리고 요즘은 집에서 네스프레소 머신 사용해서",
          "original_form": "그리고 요즘은 집에서 네스프레소 머신 사용해서",
          "speaker_id": "2",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016532.1.30",
          "form": "카페를 잘 안가슨것 같아요",
          "original_form": "카페를 잘 안가슨것 같아요",
          "speaker_id": "2",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016532.1.31",
          "form": "오 머신사용하시는군요",
          "original_form": "오 머신사용하시는군요",
          "speaker_id": "1",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016532.1.32",
          "form": "저는 모카포트 이용해요!",
          "original_form": "저는 모카포트 이용해요!",
          "speaker_id": "1",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016532.1.33",
          "form": "그게 뭐에요?",
          "original_form": "그게 뭐에요?",
          "speaker_id": "2",
          "time": "20210826 14:20"
        }
      ]
    }
