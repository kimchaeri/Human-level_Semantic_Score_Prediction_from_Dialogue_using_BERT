      "utterance": [
        {
          "id": "MDRW2100017420.1.1",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017420.1.2",
          "form": "네 간단하게 고구마 먹었어요",
          "original_form": "네 간단하게 고구마 먹었어요",
          "speaker_id": "2",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100017420.1.3",
          "form": "오 저녁식사는 간단하게 드시는구나",
          "original_form": "오 저녁식사는 간단하게 드시는구나",
          "speaker_id": "1",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017420.1.4",
          "form": "요새 살이 너무 쪄서요 ㅠ",
          "original_form": "요새 살이 너무 쪄서요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017420.1.5",
          "form": "뭐드셨어요",
          "original_form": "뭐드셨어요",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017420.1.6",
          "form": "저도 평소엔 간단히 먹는데",
          "original_form": "저도 평소엔 간단히 먹는데",
          "speaker_id": "1",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017420.1.7",
          "form": "오늘은 보쌈 먹었어요",
          "original_form": "오늘은 보쌈 먹었어요",
          "speaker_id": "1",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017420.1.8",
          "form": "맛있었겠어요 ㅠㅠ",
          "original_form": "맛있었겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017420.1.9",
          "form": "네 맛있게 먹었어요",
          "original_form": "네 맛있게 먹었어요",
          "speaker_id": "1",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017420.1.10",
          "form": "단백질 메뉴이니 건강에도 좋겠죠?",
          "original_form": "단백질 메뉴이니 건강에도 좋겠죠?",
          "speaker_id": "1",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017420.1.11",
          "form": "그럴거같아요 ㅋㅋㅋ",
          "original_form": "그럴거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017420.1.12",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017420.1.13",
          "form": "저는 가리는 것 없는데",
          "original_form": "저는 가리는 것 없는데",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017420.1.14",
          "form": "일식이나 태국음식 좋아해요",
          "original_form": "일식이나 태국음식 좋아해요",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017420.1.15",
          "form": "둘다 넘 맛있죠",
          "original_form": "둘다 넘 맛있죠",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017420.1.16",
          "form": "초밥도 좋아하세요?",
          "original_form": "초밥도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017420.1.17",
          "form": "초밥 엄청 좋아해요",
          "original_form": "초밥 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017420.1.18",
          "form": "장염 걸렸을때도 먹을 정도로ㅎㅎ",
          "original_form": "장염 걸렸을때도 먹을 정도로ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017420.1.19",
          "form": "와 ㅋㅋㅋ 어떤 초밥이요?",
          "original_form": "와 ㅋㅋㅋ 어떤 초밥이요?",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017420.1.20",
          "form": "연어초밥이랑",
          "original_form": "연어초밥이랑",
          "speaker_id": "1",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017420.1.21",
          "form": "한치도 좋아하고",
          "original_form": "한치도 좋아하고",
          "speaker_id": "1",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017420.1.22",
          "form": "연어맛있죠",
          "original_form": "연어맛있죠",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017420.1.23",
          "form": "계란초밥도요",
          "original_form": "계란초밥도요",
          "speaker_id": "1",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017420.1.24",
          "form": "저도 계란초밥 너무 좋아해요 ㅋㅋㅋ",
          "original_form": "저도 계란초밥 너무 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:33"
        }
      ]
