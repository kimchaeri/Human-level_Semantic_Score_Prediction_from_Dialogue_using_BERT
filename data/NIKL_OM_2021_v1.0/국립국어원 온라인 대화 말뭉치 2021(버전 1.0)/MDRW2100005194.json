      "utterance": [
        {
          "id": "MDRW2100005194.1.1",
          "form": "티비 보나?",
          "original_form": "티비 보나?",
          "speaker_id": "2",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005194.1.2",
          "form": "아니 아빠가 보고 있어ㅋ",
          "original_form": "아니 아빠가 보고 있어ㅋ",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005194.1.3",
          "form": "아빠 뭐 보시는데?",
          "original_form": "아빠 뭐 보시는데?",
          "speaker_id": "2",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005194.1.4",
          "form": "멘탈리스트 보시네",
          "original_form": "멘탈리스트 보시네",
          "speaker_id": "1",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005194.1.5",
          "form": "오~ 외국드라마 맞지",
          "original_form": "오~ 외국드라마 맞지",
          "speaker_id": "2",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005194.1.6",
          "form": "어 외국드라마 많이 보는편",
          "original_form": "어 외국드라마 많이 보는편",
          "speaker_id": "1",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005194.1.7",
          "form": "추리물이라고 해야하나 시리즈물",
          "original_form": "추리물이라고 해야하나 시리즈물",
          "speaker_id": "2",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005194.1.8",
          "form": "글치 CSI 같은거",
          "original_form": "글치 CSI 같은거",
          "speaker_id": "1",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005194.1.9",
          "form": "나두 많이 봤는데 이제 별로 하우스랑 크리미널 많이 봤는데",
          "original_form": "나두 많이 봤는데 이제 별로 하우스랑 크리미널 많이 봤는데",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005194.1.10",
          "form": "난 시리즈물은 한번 보면 계속 봐야 되니깐 좀 그래",
          "original_form": "난 시리즈물은 한번 보면 계속 봐야 되니깐 좀 그래",
          "speaker_id": "1",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005194.1.11",
          "form": "그냥 난 이어지는 내용으로 안봐서 이편보고 저편보고 했다",
          "original_form": "그냥 난 이어지는 내용으로 안봐서 이편보고 저편보고 했다",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005194.1.12",
          "form": "보통 한편에 다른 얘기니깐 상관은 없겠다",
          "original_form": "보통 한편에 다른 얘기니깐 상관은 없겠다",
          "speaker_id": "1",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100005194.1.13",
          "form": "엉 그래서 몇번씩 본거 같다 티비에서 방송해주는데로 보니까",
          "original_form": "엉 그래서 몇번씩 본거 같다 티비에서 방송해주는데로 보니까",
          "speaker_id": "2",
          "time": "20210821 21:11"
        }
      ]
