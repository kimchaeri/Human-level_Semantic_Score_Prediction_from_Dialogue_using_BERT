      "utterance": [
        {
          "id": "MDRW2100016146.1.1",
          "form": "점심메뉴 정하신거있으신가요",
          "original_form": "점심메뉴 정하신거있으신가요",
          "speaker_id": "1",
          "time": "20210826 10:17"
        },
        {
          "id": "MDRW2100016146.1.2",
          "form": "저는 떡볶이요ㅎ",
          "original_form": "저는 떡볶이요ㅎ",
          "speaker_id": "2",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016146.1.3",
          "form": "뭐드시나요?",
          "original_form": "뭐드시나요?",
          "speaker_id": "2",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016146.1.4",
          "form": "떡볶이 너무 좋네요",
          "original_form": "떡볶이 너무 좋네요",
          "speaker_id": "1",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016146.1.5",
          "form": "저 아마 라면..?",
          "original_form": "저 아마 라면..?",
          "speaker_id": "1",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016146.1.6",
          "form": "오늘은 좀 간단히 먹고싶어서요ㅎ",
          "original_form": "오늘은 좀 간단히 먹고싶어서요ㅎ",
          "speaker_id": "1",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016146.1.7",
          "form": "아ㅎ라면도 좋죠ㅎ",
          "original_form": "아ㅎ라면도 좋죠ㅎ",
          "speaker_id": "2",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016146.1.8",
          "form": "떡볶이 배달시켜드시나요 만들어드시나요?",
          "original_form": "떡볶이 배달시켜드시나요 만들어드시나요?",
          "speaker_id": "1",
          "time": "20210826 10:20"
        },
        {
          "id": "MDRW2100016146.1.9",
          "form": "배달요ㅇ",
          "original_form": "배달요ㅇ",
          "speaker_id": "2",
          "time": "20210826 10:20"
        },
        {
          "id": "MDRW2100016146.1.10",
          "form": "일하고있어서ㅎ",
          "original_form": "일하고있어서ㅎ",
          "speaker_id": "2",
          "time": "20210826 10:20"
        },
        {
          "id": "MDRW2100016146.1.11",
          "form": "자주시켜먹는 곳이 있나요?",
          "original_form": "자주시켜먹는 곳이 있나요?",
          "speaker_id": "1",
          "time": "20210826 10:20"
        },
        {
          "id": "MDRW2100016146.1.12",
          "form": "아뇨 정한곳은없어요",
          "original_form": "아뇨 정한곳은없어요",
          "speaker_id": "2",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100016146.1.13",
          "form": "배민자주써요",
          "original_form": "배민자주써요",
          "speaker_id": "2",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100016146.1.14",
          "form": "프랜차이즈 주로 시켜드세요?",
          "original_form": "프랜차이즈 주로 시켜드세요?",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100016146.1.15",
          "form": "이것저것요ㅎ  좋은곳 있나요?",
          "original_form": "이것저것요ㅎ  좋은곳 있나요?",
          "speaker_id": "2",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100016146.1.16",
          "form": "어.. 저는 떡볶이는 그냥 로컬맛집에서 시켜먹어서..",
          "original_form": "어.. 저는 떡볶이는 그냥 로컬맛집에서 시켜먹어서..",
          "speaker_id": "1",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100016146.1.17",
          "form": "엽떡이나 그런건 너무 자극적이라 잘 못먹어요",
          "original_form": "엽떡이나 그런건 너무 자극적이라 잘 못먹어요",
          "speaker_id": "1",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100016146.1.18",
          "form": "name2은 매운거 잘 드시나요",
          "original_form": "&name2&은 매운거 잘 드시나요",
          "speaker_id": "1",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100016146.1.19",
          "form": "아니요 매운맛은 잘못먹어요",
          "original_form": "아니요 매운맛은 잘못먹어요",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016146.1.20",
          "form": "초딩입맛",
          "original_form": "초딩입맛",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016146.1.21",
          "form": "저도 그래요ㅎ",
          "original_form": "저도 그래요ㅎ",
          "speaker_id": "1",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016146.1.22",
          "form": "매운건 먹고나면 항상 후회해서",
