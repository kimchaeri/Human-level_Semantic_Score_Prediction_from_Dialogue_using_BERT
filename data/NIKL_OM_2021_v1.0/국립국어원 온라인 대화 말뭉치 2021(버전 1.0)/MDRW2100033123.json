      "utterance": [
        {
          "id": "MDRW2100033123.1.1",
          "form": "날씨가 비가오니깐",
          "original_form": "날씨가 비가오니깐",
          "speaker_id": "1",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100033123.1.2",
          "form": "치킨이생각나는하루",
          "original_form": "치킨이생각나는하루",
          "speaker_id": "1",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100033123.1.3",
          "form": "종교 믿으세요? 치킨교? ㅋㅋ",
          "original_form": "종교 믿으세요? 치킨교? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100033123.1.4",
          "form": "저는 치킨없으면 못살거든요",
          "original_form": "저는 치킨없으면 못살거든요",
          "speaker_id": "1",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100033123.1.5",
          "form": "1인1닭을 좋아하기때문에",
          "original_form": "1인1닭을 좋아하기때문에",
          "speaker_id": "1",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100033123.1.6",
          "form": "주말에 햄버거랑 치킨먹었어요",
          "original_form": "주말에 햄버거랑 치킨먹었어요",
          "speaker_id": "1",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100033123.1.7",
          "form": "KFC꺼",
          "original_form": "KFC꺼",
          "speaker_id": "1",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100033123.1.8",
          "form": "저도 치킨마루",
          "original_form": "저도 치킨마루",
          "speaker_id": "2",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100033123.1.9",
          "form": "주말에 흡입",
          "original_form": "주말에 흡입",
          "speaker_id": "2",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100033123.1.10",
          "form": "모임도 하시나요?",
          "original_form": "모임도 하시나요?",
          "speaker_id": "2",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100033123.1.11",
          "form": "영화모임있어요",
          "original_form": "영화모임있어요",
          "speaker_id": "1",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100033123.1.12",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "1",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100033123.1.13",
          "form": "아 저는 딱히 없는데",
          "original_form": "아 저는 딱히 없는데",
          "speaker_id": "2",
          "time": "20210906 18:35"
        },
        {
          "id": "MDRW2100033123.1.14",
          "form": "하고싶어요",
          "original_form": "하고싶어요",
          "speaker_id": "2",
          "time": "20210906 18:35"
        },
        {
          "id": "MDRW2100033123.1.15",
          "form": "영화좋아하세요?",
          "original_form": "영화좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 18:35"
        },
        {
          "id": "MDRW2100033123.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210906 18:35"
        },
        {
          "id": "MDRW2100033123.1.17",
          "form": "당연하죠!!!",
          "original_form": "당연하죠!!!",
          "speaker_id": "1",
          "time": "20210907 07:03"
        },
        {
          "id": "MDRW2100033123.1.18",
          "form": "아이어맨죽었을때 전 울었어요ㅠㅠ",
          "original_form": "아이어맨죽었을때 전 울었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 07:03"
        },
        {
          "id": "MDRW2100033123.1.19",
          "form": "얼마나 슬프던지",
          "original_form": "얼마나 슬프던지",
          "speaker_id": "1",
          "time": "20210907 07:03"
        },
        {
          "id": "MDRW2100033123.1.20",
          "form": "ㅋㅋ 악ㅋㅋ",
          "original_form": "ㅋㅋ 악ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 07:03"
        },
        {
          "id": "MDRW2100033123.1.21",
          "form": "죽는거 너무 싫어요ㅜㅠ",
          "original_form": "죽는거 너무 싫어요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 07:03"
        },
        {
          "id": "MDRW2100033123.1.22",
          "form": "그니깐요ㅠㅠ주인공이 죽었자나요ㅠㅠ",
          "original_form": "그니깐요ㅠㅠ주인공이 죽었자나요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 07:04"
        },
        {
          "id": "MDRW2100033123.1.23",
          "form": "이번에 또 나오자나요",
          "original_form": "이번에 또 나오자나요",
          "speaker_id": "2",
          "time": "20210907 07:04"
        },
        {
          "id": "MDRW2100033123.1.24",
          "form": "스파이더맨이 그 뒤를 이어가줘야죠",
          "original_form": "스파이더맨이 그 뒤를 이어가줘야죠",
          "speaker_id": "1",
          "time": "20210907 07:04"
        },
        {
          "id": "MDRW2100033123.1.25",
          "form": "스파이디 궁금하네요",
          "original_form": "스파이디 궁금하네요",
          "speaker_id": "2",
          "time": "20210907 07:04"
        },
        {
          "id": "MDRW2100033123.1.26",
          "form": "그니깐요!!! 완전 기대중",
          "original_form": "그니깐요!!! 완전 기대중",
          "speaker_id": "1",
          "time": "20210907 07:04"
        },
        {
          "id": "MDRW2100033123.1.27",
          "form": "그러기엔 포스가 좀 약하,ㅣ만!!!",
          "original_form": "그러기엔 포스가 좀 약하,ㅣ만!!!",
          "speaker_id": "2",
          "time": "20210907 07:04"
        },
        {
          "id": "MDRW2100033123.1.28",
          "form": "엑스맨시리즈도 다봤어요",
          "original_form": "엑스맨시리즈도 다봤어요",
          "speaker_id": "1",
          "time": "20210907 07:04"
        },
        {
          "id": "MDRW2100033123.1.29",
          "form": "앆ㅋㅋㅋ포스가",
          "original_form": "앆ㅋㅋㅋ포스가",
          "speaker_id": "1",
          "time": "20210907 07:05"
        },
        {
          "id": "MDRW2100033123.1.30",
          "form": "이번에 역대애들 다 나오죠?",
          "original_form": "이번에 역대애들 다 나오죠?",
          "speaker_id": "2",
          "time": "20210907 07:05"
        },
        {
          "id": "MDRW2100033123.1.31",
          "form": "스파이디 스포봤어요",
          "original_form": "스파이디 스포봤어요",
          "speaker_id": "2",
          "time": "20210907 07:05"
        }
      ]
    }
