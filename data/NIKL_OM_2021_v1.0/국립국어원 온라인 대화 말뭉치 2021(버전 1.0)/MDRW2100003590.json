        {
          "id": "MDRW2100003590.1.1",
          "form": "요즘 운동하세요?",
          "original_form": "요즘 운동하세요?",
          "speaker_id": "2",
          "time": "20210821 03:08"
        },
        {
          "id": "MDRW2100003590.1.2",
          "form": "전 플라잉요가 다니고있어요!",
          "original_form": "전 플라잉요가 다니고있어요!",
          "speaker_id": "1",
          "time": "20210821 03:08"
        },
        {
          "id": "MDRW2100003590.1.3",
          "form": "오  그거 배워보고싶던데 재밌나요?",
          "original_form": "오  그거 배워보고싶던데 재밌나요?",
          "speaker_id": "2",
          "time": "20210821 03:09"
        },
        {
          "id": "MDRW2100003590.1.4",
          "form": "너무 아파요",
          "original_form": "너무 아파요",
          "speaker_id": "1",
          "time": "20210821 03:09"
        },
        {
          "id": "MDRW2100003590.1.5",
          "form": "할때마다 멍들어서 하기싫어요...",
          "original_form": "할때마다 멍들어서 하기싫어요...",
          "speaker_id": "1",
          "time": "20210821 03:10"
        },
        {
          "id": "MDRW2100003590.1.6",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210821 03:10"
        },
        {
          "id": "MDRW2100003590.1.7",
          "form": "매달려서 해서그런가요?",
          "original_form": "매달려서 해서그런가요?",
          "speaker_id": "2",
          "time": "20210821 03:10"
        },
        {
          "id": "MDRW2100003590.1.8",
          "form": "네 재미있는거보다",
          "original_form": "네 재미있는거보다",
          "speaker_id": "1",
          "time": "20210821 03:10"
        },
        {
          "id": "MDRW2100003590.1.9",
          "form": "아픈게 더 커서 ...ㅠㅠ",
          "original_form": "아픈게 더 커서 ...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 03:10"
        },
        {
          "id": "MDRW2100003590.1.10",
          "form": "그런거같아요 제 몸무게를 다 견뎌야하니가..",
          "original_form": "그런거같아요 제 몸무게를 다 견뎌야하니가..",
          "speaker_id": "1",
          "time": "20210821 03:10"
        },
        {
          "id": "MDRW2100003590.1.11",
          "form": "혈액순환이 안돼서 그럴까요?",
          "original_form": "혈액순환이 안돼서 그럴까요?",
          "speaker_id": "2",
          "time": "20210821 03:10"
        },
        {
          "id": "MDRW2100003590.1.12",
          "form": "아 플라잉요가 해먹이",
          "original_form": "아 플라잉요가 해먹이",
          "speaker_id": "1",
          "time": "20210821 03:11"
        },
        {
          "id": "MDRW2100003590.1.13",
          "form": "너무 아파요",
          "original_form": "너무 아파요",
          "speaker_id": "1",
          "time": "20210821 03:11"
        },
        {
          "id": "MDRW2100003590.1.14",
          "form": "날씬해도 멍드는건 마찬가지인거같아요",
          "original_form": "날씬해도 멍드는건 마찬가지인거같아요",
          "speaker_id": "1",
          "time": "20210821 03:11"
        },
        {
          "id": "MDRW2100003590.1.15",
          "form": "보기엔 쉬워보이는데 아닌가보네요",
          "original_form": "보기엔 쉬워보이는데 아닌가보네요",
          "speaker_id": "2",
          "time": "20210821 03:11"
        },
        {
          "id": "MDRW2100003590.1.16",
          "form": "저는 날씬하지않아서 더 멍이 많이들어요",
          "original_form": "저는 날씬하지않아서 더 멍이 많이들어요",
          "speaker_id": "1",
          "time": "20210821 03:11"
        },
        {
          "id": "MDRW2100003590.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ 네 어려워요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 네 어려워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 03:11"
        },
        {
          "id": "MDRW2100003590.1.18",
          "form": "그래서 하기싫어요 ..",
          "original_form": "그래서 하기싫어요 ..",
          "speaker_id": "1",
          "time": "20210821 03:11"
        },
        {
          "id": "MDRW2100003590.1.19",
          "form": "name2님은 최근에 운동 하고있으신가요?",
          "original_form": "&name2&님은 최근에 운동 하고있으신가요?",
          "speaker_id": "1",
          "time": "20210821 03:11"
        },
        {
          "id": "MDRW2100003590.1.20",
          "form": "ㅎㅎ 전 줌바 하다가 코로나 터지고 암것도안해요",
          "original_form": "ㅎㅎ 전 줌바 하다가 코로나 터지고 암것도안해요",
          "speaker_id": "2",
          "time": "20210821 03:12"
        },
        {
          "id": "MDRW2100003590.1.21",
          "form": "우와 줌바는 어떤가요?",
          "original_form": "우와 줌바는 어떤가요?",
          "speaker_id": "1",
          "time": "20210821 03:12"
        },
        {
          "id": "MDRW2100003590.1.22",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "1",
          "time": "20210821 03:13"
        },
        {
          "id": "MDRW2100003590.1.23",
          "form": "줌바 잼난데 완전 추천해요",
          "original_form": "줌바 잼난데 완전 추천해요",
          "speaker_id": "2",
          "time": "20210821 03:13"
        },
        {
          "id": "MDRW2100003590.1.24",
          "form": "우와 살도 빠지나요?",
          "original_form": "우와 살도 빠지나요?",
          "speaker_id": "1",
          "time": "20210821 03:13"
        },
        {
          "id": "MDRW2100003590.1.25",
          "form": "라틴 레게 그런장르 음악틀고 춤추는거라",
          "original_form": "라틴 레게 그런장르 음악틀고 춤추는거라",
          "speaker_id": "2",
          "time": "20210821 03:13"
        },
        {
          "id": "MDRW2100003590.1.26",
          "form": "땀도 많이나고 한시간에 열량소모가 많거든요",
          "original_form": "땀도 많이나고 한시간에 열량소모가 많거든요",
          "speaker_id": "2",
          "time": "20210821 03:14"
        },
        {
          "id": "MDRW2100003590.1.27",
          "form": "우와 말만들어도 재밌겠어요",
          "original_form": "우와 말만들어도 재밌겠어요",
