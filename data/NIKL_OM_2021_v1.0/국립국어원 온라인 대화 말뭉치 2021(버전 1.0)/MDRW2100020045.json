      "utterance": [
        {
          "id": "MDRW2100020045.1.1",
          "form": "저는 그래서 탱커 더 많이 해요",
          "original_form": "저는 그래서 탱커 더 많이 해요",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020045.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ제가 죽였다는 거 너무 웃기네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ제가 죽였다는 거 너무 웃기네요",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020045.1.3",
          "form": "ㅋㅋㅋㅋㅋ진짜일지도",
          "original_form": "ㅋㅋㅋㅋㅋ진짜일지도",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020045.1.4",
          "form": "거의 빠대만 돌려서 만났을수도 있겠네요 ㅋㅋ",
          "original_form": "거의 빠대만 돌려서 만났을수도 있겠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020045.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ그러니까요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러니까요",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020045.1.6",
          "form": "피씨방에서 하세요? 집에서 하세요?",
          "original_form": "피씨방에서 하세요? 집에서 하세요?",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020045.1.7",
          "form": "집에 데스크탑이 없어서 피시방이요ㅠ",
          "original_form": "집에 데스크탑이 없어서 피시방이요ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020045.1.8",
          "form": "저도 피시방이요 데스크탑 진짜 사고싶어요",
          "original_form": "저도 피시방이요 데스크탑 진짜 사고싶어요",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020045.1.9",
          "form": "요즘 피시방 10시 제한 ㅠㅠ",
          "original_form": "요즘 피시방 10시 제한 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020045.1.10",
          "form": "맞아요ㅜ 시간 제한이 너무... 흑",
          "original_form": "맞아요ㅜ 시간 제한이 너무... 흑",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020045.1.11",
          "form": "그니까요 ㅠ 옵치 한판당 시간 길어서 빡쳐요 ㅋㅋ",
          "original_form": "그니까요 ㅠ 옵치 한판당 시간 길어서 빡쳐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020045.1.12",
          "form": "저는 요즘 디바나 라인하네요 자리아도 재밌어요",
          "original_form": "저는 요즘 디바나 라인하네요 자리아도 재밌어요",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020045.1.13",
          "form": "자리야 재밌어요",
          "original_form": "자리야 재밌어요",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020045.1.14",
          "form": "저는 디바 하면 바로 터져서ㅜ 관리를 못하겠더라고요",
          "original_form": "저는 디바 하면 바로 터져서ㅜ 관리를 못하겠더라고요",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020045.1.15",
          "form": "아 잘터지죠 ㅋㅋㅋ",
          "original_form": "아 잘터지죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020045.1.16",
          "form": "저는 라인을 젤 잘하고싶은데 너무 어려워요 ㅋㅋ",
          "original_form": "저는 라인을 젤 잘하고싶은데 너무 어려워요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020045.1.17",
          "form": "라인 어려워요ㅠ 정말",
          "original_form": "라인 어려워요ㅠ 정말",
          "speaker_id": "1",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020045.1.18",
          "form": "그냥 고기방패가 되는 기분이에오",
          "original_form": "그냥 고기방패가 되는 기분이에오",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100020045.1.19",
          "form": "ㅋㅋ그니까요 방벽드는게 생각보다 어려움",
          "original_form": "ㅋㅋ그니까요 방벽드는게 생각보다 어려움",
          "speaker_id": "2",
          "time": "20210827 23:31"
        }
      ]
