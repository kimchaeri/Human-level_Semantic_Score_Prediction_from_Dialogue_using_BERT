      "utterance": [
        {
          "id": "MDRW2100008051.1.1",
          "form": "이번여름 참 더웠는데 잘 보내셨나요?",
          "original_form": "이번여름 참 더웠는데 잘 보내셨나요?",
          "speaker_id": "1",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008051.1.2",
          "form": "안녕하세요! 이번에 너무 더웠죠!",
          "original_form": "안녕하세요! 이번에 너무 더웠죠!",
          "speaker_id": "2",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008051.1.3",
          "form": "마스크 안 낀 부분만 다 타고..",
          "original_form": "마스크 안 낀 부분만 다 타고..",
          "speaker_id": "2",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008051.1.4",
          "form": "진짜 너무 더웠어요 에어컨 안키고 살수가 없을정도로요",
          "original_form": "진짜 너무 더웠어요 에어컨 안키고 살수가 없을정도로요",
          "speaker_id": "1",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100008051.1.5",
          "form": "발도 샌달모양이나 슬리퍼 모양으로 탔어요ㅋㅋㅋ",
          "original_form": "발도 샌달모양이나 슬리퍼 모양으로 탔어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100008051.1.6",
          "form": "맞아욬ㅋㅋㅋㅋ 너무 빨리 더워지기도 했구요..",
          "original_form": "맞아욬ㅋㅋㅋㅋ 너무 빨리 더워지기도 했구요..",
          "speaker_id": "2",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100008051.1.7",
          "form": "여름에 비도 자주 안오지 않았나요",
          "original_form": "여름에 비도 자주 안오지 않았나요",
          "speaker_id": "2",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100008051.1.8",
          "form": "네 그래서 그런가 요즘 몰아서 오네요",
          "original_form": "네 그래서 그런가 요즘 몰아서 오네요",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008051.1.9",
          "form": "이번주 비많이 왔죠",
          "original_form": "이번주 비많이 왔죠",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008051.1.10",
          "form": "네 완전 하늘에 구멍뚫린 거 같아요",
          "original_form": "네 완전 하늘에 구멍뚫린 거 같아요",
          "speaker_id": "2",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008051.1.11",
          "form": "또 금방 추워질 거 같아요",
          "original_form": "또 금방 추워질 거 같아요",
          "speaker_id": "2",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008051.1.12",
          "form": "또 금방 롱패딩만 둘둘 입고 다니겠네요",
          "original_form": "또 금방 롱패딩만 둘둘 입고 다니겠네요",
          "speaker_id": "1",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100008051.1.13",
          "form": "이렇게 더운데 몇달안남았다니...",
          "original_form": "이렇게 더운데 몇달안남았다니...",
          "speaker_id": "1",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100008051.1.14",
          "form": "롱패딩 또 입을 생각 하니 뜨악하네요",
          "original_form": "롱패딩 또 입을 생각 하니 뜨악하네요",
          "speaker_id": "2",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100008051.1.15",
          "form": "그래도 여름 마스크보다 겨울 마스크가 나은거같아요 ㅋㅋ ㅠ",
          "original_form": "그래도 여름 마스크보다 겨울 마스크가 나은거같아요 ㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100008051.1.16",
          "form": "여름엔 마스크안에 숨차고 땀나서 너무 힘들었어요ㅠ",
          "original_form": "여름엔 마스크안에 숨차고 땀나서 너무 힘들었어요ㅠ",
          "speaker_id": "1",
          "time": "20210822 14:02"
        },
        {
          "id": "MDRW2100008051.1.17",
          "form": "겨울에는 그래도 얼굴이 덜추운 느낌...?",
          "original_form": "겨울에는 그래도 얼굴이 덜추운 느낌...?",
          "speaker_id": "1",
          "time": "20210822 14:03"
        },
        {
          "id": "MDRW2100008051.1.18",
          "form": "맞아요! 그리고 마스크가 따뜻해서.. 기분나쁘지 않더라구요",
          "original_form": "맞아요! 그리고 마스크가 따뜻해서.. 기분나쁘지 않더라구요",
          "speaker_id": "2",
          "time": "20210822 14:05"
        },
        {
          "id": "MDRW2100008051.1.19",
          "form": "여름에 턱에 계속 여드름났어요 땀차서 ㅋㅋㅋㅋ",
          "original_form": "여름에 턱에 계속 여드름났어요 땀차서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:05"
        },
        {
          "id": "MDRW2100008051.1.20",
          "form": "앜ㅋㅋㄲㅋ저두요",
          "original_form": "앜ㅋㅋㄲㅋ저두요",
          "speaker_id": "1",
          "time": "20210822 14:07"
        },
        {
          "id": "MDRW2100008051.1.21",
          "form": "저는 여름보다는 겨울이좋아서",
          "original_form": "저는 여름보다는 겨울이좋아서",
          "speaker_id": "1",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100008051.1.22",
          "form": "그래도 겨울이 기다려져요",
          "original_form": "그래도 겨울이 기다려져요",
          "speaker_id": "1",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100008051.1.23",
          "form": "맞아요 포장마차에서 오뎅먹고싶어요",
          "original_form": "맞아요 포장마차에서 오뎅먹고싶어요",
          "speaker_id": "2",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100008051.1.24",
          "form": "겨울간식 맛있어요 ㅜㅜ",
          "original_form": "겨울간식 맛있어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100008051.1.25",
          "form": "타코야끼 !! 운이 좋아야 만날수있지만요",
          "original_form": "타코야끼 !! 운이 좋아야 만날수있지만요",
          "speaker_id": "1",
          "time": "20210822 14:10"
        }
      ]
    }
