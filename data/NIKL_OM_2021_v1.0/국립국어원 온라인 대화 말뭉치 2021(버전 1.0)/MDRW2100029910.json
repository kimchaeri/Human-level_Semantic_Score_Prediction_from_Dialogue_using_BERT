      "utterance": [
        {
          "id": "MDRW2100029910.1.1",
          "form": "그리고 서로 안다치게해주는 배려심",
          "original_form": "그리고 서로 안다치게해주는 배려심",
          "speaker_id": "2",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029910.1.2",
          "form": "퇴근몇시에하시나여",
          "original_form": "퇴근몇시에하시나여",
          "speaker_id": "1",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029910.1.3",
          "form": "저는 다섯시만 기둘림다",
          "original_form": "저는 다섯시만 기둘림다",
          "speaker_id": "1",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029910.1.4",
          "form": "6시인데",
          "original_form": "6시인데",
          "speaker_id": "2",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029910.1.5",
          "form": "유연 근무제라",
          "original_form": "유연 근무제라",
          "speaker_id": "2",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029910.1.6",
          "form": "들썩들썩해요",
          "original_form": "들썩들썩해요",
          "speaker_id": "1",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029910.1.7",
          "form": "4시에 퇴근해도 되영",
          "original_form": "4시에 퇴근해도 되영",
          "speaker_id": "2",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029910.1.8",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029910.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029910.1.10",
          "form": "궁디 들썩들썩",
          "original_form": "궁디 들썩들썩",
          "speaker_id": "2",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029910.1.11",
          "form": "동감입니다",
          "original_form": "동감입니다",
          "speaker_id": "2",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029910.1.12",
          "form": "월급루팡이에요",
          "original_form": "월급루팡이에요",
          "speaker_id": "1",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029910.1.13",
          "form": "집가고싶슴다",
          "original_form": "집가고싶슴다",
          "speaker_id": "1",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029910.1.14",
          "form": "유연근무제 신기하네요",
          "original_form": "유연근무제 신기하네요",
          "speaker_id": "1",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029910.1.15",
          "form": "10시 4시에",
          "original_form": "10시 4시에",
          "speaker_id": "2",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100029910.1.16",
          "form": "코어타임만 지키면되여",
          "original_form": "코어타임만 지키면되여",
          "speaker_id": "2",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100029910.1.17",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100029910.1.18",
          "form": "쩌네요",
          "original_form": "쩌네요",
          "speaker_id": "1",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100029910.1.19",
          "form": "무슨일하서요?",
          "original_form": "무슨일하서요?",
          "speaker_id": "2",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100029910.1.20",
          "form": "병원에서일해요",
          "original_form": "병원에서일해요",
          "speaker_id": "1",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100029910.1.21",
          "form": "ㅋㅋ저도 부럽다는소리 많이듣는데",
          "original_form": "ㅋㅋ저도 부럽다는소리 많이듣는데",
          "speaker_id": "1",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029910.1.22",
          "form": "거기회사도 좋으네여",
          "original_form": "거기회사도 좋으네여",
          "speaker_id": "1",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029910.1.23",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029910.1.24",
          "form": "병원",
          "original_form": "병원",
          "speaker_id": "2",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029910.1.25",
          "form": "좋은데 잘모르는곳",
          "original_form": "좋은데 잘모르는곳",
          "speaker_id": "2",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029910.1.26",
          "form": "근무환경 넘좋아서 일한지오래됏어요",
          "original_form": "근무환경 넘좋아서 일한지오래됏어요",
          "speaker_id": "1",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029910.1.27",
          "form": "진짜 모르실겁니다 ㅋㅋ",
          "original_form": "진짜 모르실겁니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029910.1.28",
