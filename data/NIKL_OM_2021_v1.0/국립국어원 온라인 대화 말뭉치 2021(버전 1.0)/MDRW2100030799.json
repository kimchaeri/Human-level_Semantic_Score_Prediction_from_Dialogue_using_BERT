      "utterance": [
        {
          "id": "MDRW2100030799.1.1",
          "form": "예전에 연극 본거 재밓ㅅ었는데",
          "original_form": "예전에 연극 본거 재밓ㅅ었는데",
          "speaker_id": "2",
          "time": "20210904 12:41"
        },
        {
          "id": "MDRW2100030799.1.2",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210904 12:41"
        },
        {
          "id": "MDRW2100030799.1.3",
          "form": "그리스에서 남자들 나오는거",
          "original_form": "그리스에서 남자들 나오는거",
          "speaker_id": "2",
          "time": "20210904 12:41"
        },
        {
          "id": "MDRW2100030799.1.4",
          "form": "노래도 부르고",
          "original_form": "노래도 부르고",
          "speaker_id": "2",
          "time": "20210904 12:41"
        },
        {
          "id": "MDRW2100030799.1.5",
          "form": "춤도 추고",
          "original_form": "춤도 추고",
          "speaker_id": "2",
          "time": "20210904 12:41"
        },
        {
          "id": "MDRW2100030799.1.6",
          "form": "뮤지컬이라고 그러던가",
          "original_form": "뮤지컬이라고 그러던가",
          "speaker_id": "1",
          "time": "20210904 12:41"
        },
        {
          "id": "MDRW2100030799.1.7",
          "form": "저도 뮤지컬 좋아해요",
          "original_form": "저도 뮤지컬 좋아해요",
          "speaker_id": "1",
          "time": "20210904 12:42"
        },
        {
          "id": "MDRW2100030799.1.8",
          "form": "아트홀 가서 봤었어",
          "original_form": "아트홀 가서 봤었어",
          "speaker_id": "2",
          "time": "20210904 12:42"
        },
        {
          "id": "MDRW2100030799.1.9",
          "form": "아트홀 맞나",
          "original_form": "아트홀 맞나",
          "speaker_id": "2",
          "time": "20210904 12:42"
        },
        {
          "id": "MDRW2100030799.1.10",
          "form": "백운아트홀이요",
          "original_form": "백운아트홀이요",
          "speaker_id": "1",
          "time": "20210904 12:42"
        },
        {
          "id": "MDRW2100030799.1.11",
          "form": "공짜로 보여줘서 좋긴해요 ㅋㅋㅋ",
          "original_form": "공짜로 보여줘서 좋긴해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:42"
        },
        {
          "id": "MDRW2100030799.1.12",
          "form": "한물간 것들 많이 들어오지만",
          "original_form": "한물간 것들 많이 들어오지만",
          "speaker_id": "1",
          "time": "20210904 12:42"
        },
        {
          "id": "MDRW2100030799.1.13",
          "form": "그래도 무료로 보니까 좋지",
          "original_form": "그래도 무료로 보니까 좋지",
          "speaker_id": "2",
          "time": "20210904 12:42"
        },
        {
          "id": "MDRW2100030799.1.14",
          "form": "너는 그때 안봤니",
          "original_form": "너는 그때 안봤니",
          "speaker_id": "2",
          "time": "20210904 12:42"
        },
        {
          "id": "MDRW2100030799.1.15",
          "form": "저는 그때 야자했으니까 ㅎㅎ",
          "original_form": "저는 그때 야자했으니까 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100030799.1.16",
          "form": "공부 열심히 했겠구나",
          "original_form": "공부 열심히 했겠구나",
          "speaker_id": "2",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100030799.1.17",
          "form": "일부러 안보려고 한건 아니고...",
          "original_form": "일부러 안보려고 한건 아니고...",
          "speaker_id": "1",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100030799.1.18",
          "form": "못본거죠",
          "original_form": "못본거죠",
          "speaker_id": "1",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100030799.1.19",
          "form": "재밌었는데 아쉽다",
          "original_form": "재밌었는데 아쉽다",
          "speaker_id": "2",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100030799.1.20",
          "form": "그러게요 ㅠ 저도 아래에서 본적 별로 없는데",
          "original_form": "그러게요 ㅠ 저도 아래에서 본적 별로 없는데",
          "speaker_id": "1",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100030799.1.21",
          "form": "극장에서 뮤지컬 본거 별로 없지 않아?",
          "original_form": "극장에서 뮤지컬 본거 별로 없지 않아?",
          "speaker_id": "2",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100030799.1.22",
          "form": "아무래도 인프라가 적으니까요",
          "original_form": "아무래도 인프라가 적으니까요",
          "speaker_id": "1",
          "time": "20210904 12:44"
        }
      ]
