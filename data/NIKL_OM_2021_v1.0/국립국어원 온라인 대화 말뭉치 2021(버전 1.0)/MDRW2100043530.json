      "utterance": [
        {
          "id": "MDRW2100043530.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043530.1.2",
          "form": "무슨 음료 좋아하세용?",
          "original_form": "무슨 음료 좋아하세용?",
          "speaker_id": "1",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043530.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043530.1.4",
          "form": "저는 탄산음료요ㅠ",
          "original_form": "저는 탄산음료요ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043530.1.5",
          "form": "탄산음료 좋죠! 저도 좋아해요",
          "original_form": "탄산음료 좋죠! 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043530.1.6",
          "form": "어떤걸 가장 좋아하시나요?",
          "original_form": "어떤걸 가장 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043530.1.7",
          "form": "콜라요!",
          "original_form": "콜라요!",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043530.1.8",
          "form": "근데 요즘은 그 사이다 복숭아맛에 빠져있어요",
          "original_form": "근데 요즘은 그 사이다 복숭아맛에 빠져있어요",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043530.1.9",
          "form": "펩시? 코카콜라?",
          "original_form": "펩시? 코카콜라?",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043530.1.10",
          "form": "아앗 ㅋㅋㅋㅋㅋ",
          "original_form": "아앗 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043530.1.11",
          "form": "펩시요",
          "original_form": "펩시요",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043530.1.12",
          "form": "그것두 맛있죠 ㅎㅎ",
          "original_form": "그것두 맛있죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043530.1.13",
          "form": "코카콜라도 좋긴 한데...",
          "original_form": "코카콜라도 좋긴 한데...",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043530.1.14",
          "form": "오우 펩시파 처음봐요!",
          "original_form": "오우 펩시파 처음봐요!",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043530.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043530.1.16",
          "form": "신기해용 ㅋㅋㅋ",
          "original_form": "신기해용 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043530.1.17",
          "form": "주변에 없더라구요 사실 근데 눈 감고 모를 것 같아요",
          "original_form": "주변에 없더라구요 사실 근데 눈 감고 모를 것 같아요",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043530.1.18",
          "form": "아항 ㅠ 다들 그렇게 말하는데 한번 실험해보고싶긴해요",
          "original_form": "아항 ㅠ 다들 그렇게 말하는데 한번 실험해보고싶긴해요",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043530.1.19",
          "form": "분명 다르거든요 ㅋㅋㅋㅋ",
          "original_form": "분명 다르거든요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043530.1.20",
          "form": "ㅋㅋㅋㅋ그쵸",
          "original_form": "ㅋㅋㅋㅋ그쵸",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043530.1.21",
          "form": "그냥 기분 탓인 것 같기도 하구...",
          "original_form": "그냥 기분 탓인 것 같기도 하구...",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043530.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 누군가 보조가 필요래눀ㅋㅋㅋㅋ 실험을 위해",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 누군가 보조가 필요래눀ㅋㅋㅋㅋ 실험을 위해",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043530.1.23",
          "form": "아 친구들 만나서 약간 게임 식으로 하면 재미있을 거 같아요 ㅋㅋㅋㅋ",
          "original_form": "아 친구들 만나서 약간 게임 식으로 하면 재미있을 거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043530.1.24",
          "form": "아 그렇네요 ㅎㅎㅎㅎ 한번해보고싶긴해요",
          "original_form": "아 그렇네요 ㅎㅎㅎㅎ 한번해보고싶긴해요",
          "speaker_id": "1",
          "time": "20210907 19:38"
        }
      ]
    }
  ]
