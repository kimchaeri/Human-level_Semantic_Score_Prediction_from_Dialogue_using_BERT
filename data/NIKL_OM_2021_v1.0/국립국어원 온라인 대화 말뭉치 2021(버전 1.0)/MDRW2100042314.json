      "utterance": [
        {
          "id": "MDRW2100042314.1.1",
          "form": "대답이 없는 거 보니",
          "original_form": "대답이 없는 거 보니",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042314.1.2",
          "form": "저녁은 뭐 먹었어요",
          "original_form": "저녁은 뭐 먹었어요",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042314.1.3",
          "form": "먹고 있구만",
          "original_form": "먹고 있구만",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042314.1.4",
          "form": "애들은 자요",
          "original_form": "애들은 자요",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042314.1.5",
          "form": "아직 안 왔어요ㅋㅋㅋㅋ",
          "original_form": "아직 안 왔어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042314.1.6",
          "form": "아직 안왔나ㅋㅋㅋㅋ",
          "original_form": "아직 안왔나ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042314.1.7",
          "form": "인 슈 했어요ㅋㅋㅋ",
          "original_form": "인 슈 했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042314.1.8",
          "form": "저녁에 어제 먹고 나면 탕수육이랑",
          "original_form": "저녁에 어제 먹고 나면 탕수육이랑",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042314.1.9",
          "form": "너무 배가 고프네요",
          "original_form": "너무 배가 고프네요",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042314.1.10",
          "form": "반찬 나무 숟가락에서",
          "original_form": "반찬 나무 숟가락에서",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042314.1.11",
          "form": "반찬 사왔는 거 먹었지",
          "original_form": "반찬 사왔는 거 먹었지",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042314.1.12",
          "form": "탕수육이 남아요?",
          "original_form": "탕수육이 남아요?",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042314.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042314.1.14",
          "form": "나무 숟가락 오늘의 메뉴는 뭔가요",
          "original_form": "나무 숟가락 오늘의 메뉴는 뭔가요",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042314.1.15",
          "form": "조금 남았다",
          "original_form": "조금 남았다",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042314.1.16",
          "form": "안 그래도 동태탕 샀는데 우리 신랑",
          "original_form": "안 그래도 동태탕 샀는데 우리 신랑",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042314.1.17",
          "form": "싱겁다고ㅋㅋㅋ",
          "original_form": "싱겁다고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042314.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㄱ 그래 거기 좀 심심하지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㄱ 그래 거기 좀 심심하지",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042314.1.19",
          "form": "건강식이라 생각해라",
          "original_form": "건강식이라 생각해라",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042314.1.20",
          "form": "다시 다를 좀 넣어줘야 되겠어요",
          "original_form": "다시 다를 좀 넣어줘야 되겠어요",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042314.1.21",
          "form": "아 내가 웃긴 얘기 해줄까??",
          "original_form": "아 내가 웃긴 얘기 해줄까??",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042314.1.22",
          "form": "애들 반찬은 괜찮은 듯",
          "original_form": "애들 반찬은 괜찮은 듯",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042314.1.23",
          "form": "뭐 예용",
          "original_form": "뭐 예용",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042314.1.24",
          "form": "내가  우리 name3",
          "original_form": "내가  우리 &name3&",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042314.1.25",
          "form": "학교 밥 맛있다고 했잖아",
          "original_form": "학교 밥 맛있다고 했잖아",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042314.1.26",
          "form": "야 비야",
          "original_form": "야 비야",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042314.1.27",
          "form": "맛있지요 맛있지",
          "original_form": "맛있지요 맛있지",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042314.1.28",
          "form": "유치원 밥은 맛없다고 했었거든",
          "original_form": "유치원 밥은 맛없다고 했었거든",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042314.1.29",
          "form": "다시 단 가요",
          "original_form": "다시 단 가요",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042314.1.30",
          "form": "그래서 나는 진짜 우리 name3이 학교 밥이 맛있다고 생각하고 먹는 줄 알았어",
          "original_form": "그래서 나는 진짜 우리 &name3&이 학교 밥이 맛있다고 생각하고 먹는 줄 알았어",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042314.1.31",
          "form": "아니 그게 아니고",
          "original_form": "아니 그게 아니고",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042314.1.32",
          "form": "우리 원반에 다리 다쳐가지고 휠체어 타고 다니는 친구가 있거든",
          "original_form": "우리 원반에 다리 다쳐가지고 휠체어 타고 다니는 친구가 있거든",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042314.1.33",
          "form": "뭘까요",
          "original_form": "뭘까요",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042314.1.34",
          "form": "걔 엄마가",
          "original_form": "걔 엄마가",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042314.1.35",
          "form": "밀어줘야 돼서",
          "original_form": "밀어줘야 돼서",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042314.1.36",
          "form": "급식실을 같이 가거든",
          "original_form": "급식실을 같이 가거든",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042314.1.37",
          "form": "그 엄마가 풍경채 사는데",
          "original_form": "그 엄마가 풍경채 사는데",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042314.1.38",
          "form": "아 엄마가 같이 급식실을 간다고요",
          "original_form": "아 엄마가 같이 급식실을 간다고요",
          "speaker_id": "2",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042314.1.39",
          "form": "어느 날 놀이터에서 나를 보더니",
          "original_form": "어느 날 놀이터에서 나를 보더니",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042314.1.40",
          "form": "어 엄마가 같이",
          "original_form": "어 엄마가 같이",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042314.1.41",
          "form": "선생님이 계속 걔를 밀어줘야되니까",
          "original_form": "선생님이 계속 걔를 밀어줘야되니까",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042314.1.42",
          "form": "자 언니 흥분하지 말고ㅋㅋㅋ",
          "original_form": "자 언니 흥분하지 말고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:38"
        }
      ]
    }
