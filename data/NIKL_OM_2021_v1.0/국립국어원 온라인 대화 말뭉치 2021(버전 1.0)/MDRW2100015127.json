      "utterance": [
        {
          "id": "MDRW2100015127.1.1",
          "form": "유튜브 많이 보시나요?",
          "original_form": "유튜브 많이 보시나요?",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015127.1.2",
          "form": "아뇨 유투브도 길어서 인스타짤로봐요",
          "original_form": "아뇨 유투브도 길어서 인스타짤로봐요",
          "speaker_id": "1",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015127.1.3",
          "form": "그나마 보는게 십오야 정도",
          "original_form": "그나마 보는게 십오야 정도",
          "speaker_id": "1",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015127.1.4",
          "form": "아 전 아직 못 봤는데 그거 재밌다고 하더라구요!",
          "original_form": "아 전 아직 못 봤는데 그거 재밌다고 하더라구요!",
          "speaker_id": "2",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015127.1.5",
          "form": "출장십오야 구구즈 편 보고 숨넘어갈뻔",
          "original_form": "출장십오야 구구즈 편 보고 숨넘어갈뻔",
          "speaker_id": "1",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015127.1.6",
          "form": "아 그거 꼭 봐야되는데 아직도 못 봤어요",
          "original_form": "아 그거 꼭 봐야되는데 아직도 못 봤어요",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015127.1.7",
          "form": "짤 몇개만 보고ㅜㅜㅋㅋ",
          "original_form": "짤 몇개만 보고ㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015127.1.8",
          "form": "정경호 너무 귀엽게 나오더라구요",
          "original_form": "정경호 너무 귀엽게 나오더라구요",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015127.1.9",
          "form": "꼭 봐야겠네요ㅎㅎ",
          "original_form": "꼭 봐야겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015127.1.10",
          "form": "방탄소년단 나온것도 재밌었다던데",
          "original_form": "방탄소년단 나온것도 재밌었다던데",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015127.1.11",
          "form": "그편은 방탄자체콘텐츠 분량때문인지 짤린게 많더라구요",
          "original_form": "그편은 방탄자체콘텐츠 분량때문인지 짤린게 많더라구요",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015127.1.12",
          "form": "최근에 씽크홀편도 재밌었어요",
          "original_form": "최근에 씽크홀편도 재밌었어요",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015127.1.13",
          "form": "아 중간에 짤리면 재미가 덜 하겠네요",
          "original_form": "아 중간에 짤리면 재미가 덜 하겠네요",
          "speaker_id": "2",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015127.1.14",
          "form": "다 봐야겠네요ㅎㅎ",
          "original_form": "다 봐야겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015127.1.15",
          "form": "넷플릭스나 왓챠같은건 보시나요?",
          "original_form": "넷플릭스나 왓챠같은건 보시나요?",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015127.1.16",
          "form": "넹 둘다 구독하고 있어요",
          "original_form": "넹 둘다 구독하고 있어요",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015127.1.17",
          "form": "아 전 왓챠에서 왕좌의 게임만 보고 지금은 넷플릭스만 구독중이에요",
          "original_form": "아 전 왓챠에서 왕좌의 게임만 보고 지금은 넷플릭스만 구독중이에요",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015127.1.18",
          "form": "요즘 볼만한 것 있을까요?",
          "original_form": "요즘 볼만한 것 있을까요?",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015127.1.19",
          "form": "왓챠요?",
          "original_form": "왓챠요?",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015127.1.20",
          "form": "넷플이면 전 생존 막이런거 좋아해서 원헌드레드 재밌게 봤어요",
          "original_form": "넷플이면 전 생존 막이런거 좋아해서 원헌드레드 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015127.1.21",
          "form": "아님 풀러하우스",
          "original_form": "아님 풀러하우스",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015127.1.22",
          "form": "이건 미국시트콤인데 아이들이 너무 귀여워요",
          "original_form": "이건 미국시트콤인데 아이들이 너무 귀여워요",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015127.1.23",
          "form": "오 둘 다 처음 들어봤어요!",
          "original_form": "오 둘 다 처음 들어봤어요!",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015127.1.24",
          "form": "넷플릭스 한창 열심히 보다가 이제는 볼게 별로 없더라구요ㅋㅋ",
          "original_form": "넷플릭스 한창 열심히 보다가 이제는 볼게 별로 없더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015127.1.25",
          "form": "걍 제 취향이요",
          "original_form": "걍 제 취향이요",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015127.1.26",
          "form": "풀러하우스 찾아봐야겠네요",
          "original_form": "풀러하우스 찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015127.1.27",
          "form": "모던패밀리 정말 재밌게 봤는데",
          "original_form": "모던패밀리 정말 재밌게 봤는데",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015127.1.28",
          "form": "맨날 찜콩만하죠 뭐",
          "original_form": "맨날 찜콩만하죠 뭐",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015127.1.29",
          "form": "그거 엄청 유명하잖아요",
          "original_form": "그거 엄청 유명하잖아요",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015127.1.30",
          "form": "거기에 코믹적인 요소가 더한듯",
          "original_form": "거기에 코믹적인 요소가 더한듯",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015127.1.31",
          "form": "오 그거보다 더 코믹하면 정말 재밌겠네요!",
          "original_form": "오 그거보다 더 코믹하면 정말 재밌겠네요!",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015127.1.32",
          "form": "추천 감사해요ㅎㅎ",
          "original_form": "추천 감사해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015127.1.33",
          "form": "저도 추천해주세요",
          "original_form": "저도 추천해주세요",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015127.1.34",
          "form": "아 저는 요즘 안 본지 오래돼서ㅜㅜ",
          "original_form": "아 저는 요즘 안 본지 오래돼서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015127.1.35",
          "form": "이제 넷플릭스에 없는건지ㅜㅜ",
          "original_form": "이제 넷플릭스에 없는건지ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015127.1.36",
          "form": "넘 유명하긴 하지만 종이의 집 정말 재밌게 봤어요!",
          "original_form": "넘 유명하긴 하지만 종이의 집 정말 재밌게 봤어요!",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015127.1.37",
          "form": "종이의 집 시즌 2에서 접었는데 새시즌 나온다고 해서 재도전 하려구요",
          "original_form": "종이의 집 시즌 2에서 접었는데 새시즌 나온다고 해서 재도전 하려구요",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015127.1.38",
          "form": "아아 사실 시즌 1,2가 제일 재밌긴 했어요ㅎㅎ",
          "original_form": "아아 사실 시즌 1,2가 제일 재밌긴 했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:38"
        }
      ]
    }
  ]
