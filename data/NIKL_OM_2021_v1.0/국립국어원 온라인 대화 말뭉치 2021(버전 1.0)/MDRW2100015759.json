      "utterance": [
        {
          "id": "MDRW2100015759.1.1",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 02:20"
        },
        {
          "id": "MDRW2100015759.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 02:20"
        },
        {
          "id": "MDRW2100015759.1.3",
          "form": "어떤 종목 좋아하세요?",
          "original_form": "어떤 종목 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 02:20"
        },
        {
          "id": "MDRW2100015759.1.4",
          "form": "저는 여자배구 좋아해요.",
          "original_form": "저는 여자배구 좋아해요.",
          "speaker_id": "2",
          "time": "20210826 02:20"
        },
        {
          "id": "MDRW2100015759.1.5",
          "form": "여자배구 지난 도쿄올림픽 때 봤어요!!",
          "original_form": "여자배구 지난 도쿄올림픽 때 봤어요!!",
          "speaker_id": "1",
          "time": "20210826 02:21"
        },
        {
          "id": "MDRW2100015759.1.6",
          "form": "어떤 팀 응원하세요?",
          "original_form": "어떤 팀 응원하세요?",
          "speaker_id": "1",
          "time": "20210826 02:21"
        },
        {
          "id": "MDRW2100015759.1.7",
          "form": "딱히 어느 팀을 응원하지는 않아요.",
          "original_form": "딱히 어느 팀을 응원하지는 않아요.",
          "speaker_id": "2",
          "time": "20210826 02:21"
        },
        {
          "id": "MDRW2100015759.1.8",
          "form": "그냥 약한 팀 응원해요",
          "original_form": "그냥 약한 팀 응원해요",
          "speaker_id": "2",
          "time": "20210826 02:21"
        },
        {
          "id": "MDRW2100015759.1.9",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
          "time": "20210826 02:21"
        },
        {
          "id": "MDRW2100015759.1.10",
          "form": "저는 지난번 김연경선수 있을 때 흥국 경기 봤어요",
          "original_form": "저는 지난번 김연경선수 있을 때 흥국 경기 봤어요",
          "speaker_id": "1",
          "time": "20210826 02:21"
        },
        {
          "id": "MDRW2100015759.1.11",
          "form": "저도 김연경 선수 좋아해요",
          "original_form": "저도 김연경 선수 좋아해요",
          "speaker_id": "2",
          "time": "20210826 02:22"
        },
        {
          "id": "MDRW2100015759.1.12",
          "form": "시원시원하고",
          "original_form": "시원시원하고",
          "speaker_id": "2",
          "time": "20210826 02:22"
        },
        {
          "id": "MDRW2100015759.1.13",
          "form": "맞아요. 중국 가서 아쉬워요.",
          "original_form": "맞아요. 중국 가서 아쉬워요.",
          "speaker_id": "1",
          "time": "20210826 02:22"
        },
        {
          "id": "MDRW2100015759.1.14",
          "form": "그래도 한국에서 고생 많이 하셨으니까...",
          "original_form": "그래도 한국에서 고생 많이 하셨으니까...",
          "speaker_id": "1",
          "time": "20210826 02:22"
        },
        {
          "id": "MDRW2100015759.1.15",
          "form": "김연경 선수 떠나고 쌍둥이 자매도 외국가면",
          "original_form": "김연경 선수 떠나고 쌍둥이 자매도 외국가면",
          "speaker_id": "2",
          "time": "20210826 02:22"
        },
        {
          "id": "MDRW2100015759.1.16",
          "form": "흥국이 힘들 것 같네요.",
          "original_form": "흥국이 힘들 것 같네요.",
          "speaker_id": "2",
          "time": "20210826 02:22"
        },
        {
          "id": "MDRW2100015759.1.17",
          "form": "지난 시즌까지는 최강팀이었는데",
          "original_form": "지난 시즌까지는 최강팀이었는데",
          "speaker_id": "1",
          "time": "20210826 02:23"
        },
        {
          "id": "MDRW2100015759.1.18",
          "form": "아쉽네요.",
          "original_form": "아쉽네요.",
          "speaker_id": "1",
          "time": "20210826 02:23"
        },
        {
          "id": "MDRW2100015759.1.19",
          "form": "여자배구 말고 어떤 종목 좋아하세요?",
          "original_form": "여자배구 말고 어떤 종목 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 02:23"
        },
        {
          "id": "MDRW2100015759.1.20",
          "form": "저는 야구요",
          "original_form": "저는 야구요",
          "speaker_id": "1",
          "time": "20210826 02:23"
        },
        {
          "id": "MDRW2100015759.1.21",
          "form": "응원하는 팀 있으세요?",
          "original_form": "응원하는 팀 있으세요?",
          "speaker_id": "2",
          "time": "20210826 02:23"
        },
        {
          "id": "MDRW2100015759.1.22",
          "form": "두산 좋아해요.",
          "original_form": "두산 좋아해요.",
          "speaker_id": "1",
          "time": "20210826 02:24"
        }
      ]
