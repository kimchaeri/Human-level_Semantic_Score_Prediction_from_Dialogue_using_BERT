      "utterance": [
        {
          "id": "MDRW2100040301.1.1",
          "form": "쥐포 구워 먹고 싶네요",
          "original_form": "쥐포 구워 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100040301.1.2",
          "form": "아기 포도 맛있죠",
          "original_form": "아기 포도 맛있죠",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100040301.1.3",
          "form": "아귀포",
          "original_form": "아귀포",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100040301.1.4",
          "form": "먹태도 먹고 싶고요",
          "original_form": "먹태도 먹고 싶고요",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100040301.1.5",
          "form": "나쁜 말이 뭐였을까요",
          "original_form": "나쁜 말이 뭐였을까요",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100040301.1.6",
          "form": "ㅋㅋㅋㅈㅋ",
          "original_form": "ㅋㅋㅋㅈㅋ",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100040301.1.7",
          "form": "과연 무엇이었을까요",
          "original_form": "과연 무엇이었을까요",
          "speaker_id": "2",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100040301.1.8",
          "form": "아귀포를 아기 포도라고 하는 게 너무 웃겨요",
          "original_form": "아귀포를 아기 포도라고 하는 게 너무 웃겨요",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100040301.1.9",
          "form": "발음이 구려요",
          "original_form": "발음이 구려요",
          "speaker_id": "2",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100040301.1.10",
          "form": "인식이 구린가",
          "original_form": "인식이 구린가",
          "speaker_id": "2",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100040301.1.11",
          "form": "너도 맛있는데",
          "original_form": "너도 맛있는데",
          "speaker_id": "2",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100040301.1.12",
          "form": "제 발음도 구린가 봐요",
          "original_form": "제 발음도 구린가 봐요",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100040301.1.13",
          "form": "그래도 편하고 좋아요",
          "original_form": "그래도 편하고 좋아요",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100040301.1.14",
          "form": "먹 태 먹으러 가야 되는데",
          "original_form": "먹 태 먹으러 가야 되는데",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100040301.1.15",
          "form": "편해서 좋죠 먹태 언제 먹으러 가나요",
          "original_form": "편해서 좋죠 먹태 언제 먹으러 가나요",
          "speaker_id": "2",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100040301.1.16",
          "form": "할맥 먹태가 제일 맛있는 것 같아요",
          "original_form": "할맥 먹태가 제일 맛있는 것 같아요",
          "speaker_id": "2",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100040301.1.17",
          "form": "우리 이 차 맞고 약속 잡을까요",
          "original_form": "우리 이 차 맞고 약속 잡을까요",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100040301.1.18",
          "form": "추석 때 어디 가세요",
          "original_form": "추석 때 어디 가세요",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100040301.1.19",
          "form": "매우 좋은 생각입니다 아무데도 안 가요",
          "original_form": "매우 좋은 생각입니다 아무데도 안 가요",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100040301.1.20",
          "form": "아니면 추석 때 한 잔 해요",
          "original_form": "아니면 추석 때 한 잔 해요",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100040301.1.21",
          "form": "추석 음식 또 먹게 되겠네요",
          "original_form": "추석 음식 또 먹게 되겠네요",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100040301.1.22",
          "form": "매우 좋은 생각입니다",
          "original_form": "매우 좋은 생각입니다",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100040301.1.23",
          "form": "우리 집은 추석 음식 안에서 좋아요",
          "original_form": "우리 집은 추석 음식 안에서 좋아요",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100040301.1.24",
          "form": "추석 음식추석 급식 안 하세요",
          "original_form": "추석 음식추석 급식 안 하세요",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100040301.1.25",
          "form": "추석 음식 싫어요 싫어요",
          "original_form": "추석 음식 싫어요 싫어요",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100040301.1.26",
          "form": "느끼해요",
          "original_form": "느끼해요",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100040301.1.27",
          "form": "한 번만 먹으면 맛있는데",
          "original_form": "한 번만 먹으면 맛있는데",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100040301.1.28",
          "form": "계속 먹어야 하니 질리죠",
          "original_form": "계속 먹어야 하니 질리죠",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100040301.1.29",
          "form": "근데 마지막에 먹는  짜그리 탕은 좋아요",
          "original_form": "근데 마지막에 먹는  짜그리 탕은 좋아요",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100040301.1.30",
          "form": "맞아요 그리고 쥐포튀김 맛있어요",
          "original_form": "맞아요 그리고 쥐포튀김 맛있어요",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100040301.1.31",
          "form": "부경대 앞에서 팔았는데",
          "original_form": "부경대 앞에서 팔았는데",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100040301.1.32",
          "form": "할머니 혼자서 요리하시는 맛집이 있었어요",
          "original_form": "할머니 혼자서 요리하시는 맛집이 있었어요",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100040301.1.33",
          "form": "포도 맛있고 튀김도 맛있는데 쥐포를 튀김이 얼마나 맛있겠어요",
          "original_form": "포도 맛있고 튀김도 맛있는데 쥐포를 튀김이 얼마나 맛있겠어요",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100040301.1.34",
          "form": "어디 였죠 제가 알 수도 있어요 제가 거기 나왔거든요",
          "original_form": "어디 였죠 제가 알 수도 있어요 제가 거기 나왔거든요",
          "speaker_id": "2",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100040301.1.35",
          "form": "짝을 이 탕을 해주시는데 그거 먹으로 구경 돼 갔었어요",
          "original_form": "짝을 이 탕을 해주시는데 그거 먹으로 구경 돼 갔었어요",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100040301.1.36",
          "form": "자막걸리도 있었어요",
          "original_form": "자막걸리도 있었어요",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100040301.1.37",
          "form": "지금도 있을까요 다음에 같이 한번가 봐요",
          "original_form": "지금도 있을까요 다음에 같이 한번가 봐요",
          "speaker_id": "2",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100040301.1.38",
          "form": "유자 막걸리 짜그리타 오징어 입 조림",
          "original_form": "유자 막걸리 짜그리타 오징어 입 조림",
          "speaker_id": "1",
          "time": "20210904 15:54"
        }
      ]
