      "utterance": [
        {
          "id": "MDRW2100023252.1.1",
          "form": "고등일땐 좀 그런 애들이 있었는데 제가 선택과목이거든요",
          "original_form": "고등일땐 좀 그런 애들이 있었는데 제가 선택과목이거든요",
          "speaker_id": "2",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023252.1.2",
          "form": "선택과목이면",
          "original_form": "선택과목이면",
          "speaker_id": "1",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023252.1.3",
          "form": "그래서 뭐 현실적으로 어쩔수없지 하는 생각이 들어요",
          "original_form": "그래서 뭐 현실적으로 어쩔수없지 하는 생각이 들어요",
          "speaker_id": "2",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023252.1.4",
          "form": "과학에서 화학/물리/ 이런거요?",
          "original_form": "과학에서 화학/물리/ 이런거요?",
          "speaker_id": "1",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023252.1.5",
          "form": "필요한 애들만 들으니까",
          "original_form": "필요한 애들만 들으니까",
          "speaker_id": "2",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023252.1.6",
          "form": "네네 그런 과목",
          "original_form": "네네 그런 과목",
          "speaker_id": "2",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023252.1.7",
          "form": "지들이 원해서 들었는데 수업 참여 안해요?",
          "original_form": "지들이 원해서 들었는데 수업 참여 안해요?",
          "speaker_id": "1",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023252.1.8",
          "form": "원하지 않는 애들!",
          "original_form": "원하지 않는 애들!",
          "speaker_id": "2",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023252.1.9",
          "form": "헉!",
          "original_form": "헉!",
          "speaker_id": "1",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023252.1.10",
          "form": "내신으로 선택은 했는데",
          "original_form": "내신으로 선택은 했는데",
          "speaker_id": "2",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023252.1.11",
          "form": "수능에선 안 치는 애들은",
          "original_form": "수능에선 안 치는 애들은",
          "speaker_id": "2",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023252.1.12",
          "form": "아 그런 아이들이 있구나....",
          "original_form": "아 그런 아이들이 있구나....",
          "speaker_id": "1",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023252.1.13",
          "form": "제가 뭐 끝까지 들으라고 하기보다는 좀 체력 아껴서 다른 과목 공부하는게 더 현명한 거기도 하고 ㅋㅋ",
          "original_form": "제가 뭐 끝까지 들으라고 하기보다는 좀 체력 아껴서 다른 과목 공부하는게 더 현명한 거기도 하고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023252.1.14",
          "form": "우리 분명 직장 이야기 중인데 뭔가 주제가 학교생활인 것 같은 건 기분탓이죠?",
          "original_form": "우리 분명 직장 이야기 중인데 뭔가 주제가 학교생활인 것 같은 건 기분탓이죠?",
          "speaker_id": "1",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023252.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023252.1.16",
          "form": "학생들이랑 라포형성 잘 돼요?",
          "original_form": "학생들이랑 라포형성 잘 돼요?",
          "speaker_id": "1",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100023252.1.17",
          "form": "직업정신을 야기하는데여 ㅜ머ㅜ",
          "original_form": "직업정신을 야기하는데여 ㅜ머ㅜ",
          "speaker_id": "2",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100023252.1.18",
          "form": "여고 일할땐 라포형성 최대였는디",
          "original_form": "여고 일할땐 라포형성 최대였는디",
          "speaker_id": "2",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100023252.1.19",
          "form": "남중 일하는 지금 관심사 너무 달라서",
          "original_form": "남중 일하는 지금 관심사 너무 달라서",
          "speaker_id": "2",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100023252.1.20",
          "form": "담임 역할이 경게가 뚜렷하지 않은 것 같아서 잘모르겟어요 궁금해요",
          "original_form": "담임 역할이 경게가 뚜렷하지 않은 것 같아서 잘모르겟어요 궁금해요",
          "speaker_id": "1",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100023252.1.21",
          "form": "친해지는데 어려워요 ㅋㅋㅋㅋㅋㅋ물론 친근감 표시는 하지만 ㅋㅋㅋㅋ",
          "original_form": "친해지는데 어려워요 ㅋㅋㅋㅋㅋㅋ물론 친근감 표시는 하지만 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100023252.1.22",
          "form": "남학생들이 선생님 많이 좋아할 것 같아요",
          "original_form": "남학생들이 선생님 많이 좋아할 것 같아요",
          "speaker_id": "1",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100023252.1.23",
          "form": "전 지금 담임 안하는 중이라",
          "original_form": "전 지금 담임 안하는 중이라",
          "speaker_id": "2",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100023252.1.24",
          "form": "그냥 교과시간에만 만나는데",
          "original_form": "그냥 교과시간에만 만나는데",
          "speaker_id": "2",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023252.1.25",
          "form": "그럼 서로 좋은 모습만 볼 수 있으니 친하게 지내고 ㅋㅋㅋ",
          "original_form": "그럼 서로 좋은 모습만 볼 수 있으니 친하게 지내고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023252.1.26",
          "form": "담임이면 교실 들어갔을때 잔소리 좀 얹게되고 ㅋㅋㅋ",
          "original_form": "담임이면 교실 들어갔을때 잔소리 좀 얹게되고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023252.1.27",
          "form": "아ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023252.1.28",
          "form": "그럼 학교선생님들 담임 선호하는 편인가요 아니면 비선호?",
          "original_form": "그럼 학교선생님들 담임 선호하는 편인가요 아니면 비선호?",
          "speaker_id": "1",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100023252.1.29",
          "form": "다들 일반학교에선 비선호인데",
          "original_form": "다들 일반학교에선 비선호인데",
          "speaker_id": "2",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100023252.1.30",
          "form": "오.. 비선호하는구나",
          "original_form": "오.. 비선호하는구나",
          "speaker_id": "1",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100023252.1.31",
          "form": "혁신학교라 담임들 업무 아예 안줘서",
          "original_form": "혁신학교라 담임들 업무 아예 안줘서",
          "speaker_id": "2",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100023252.1.32",
          "form": "아 그건 좋네요 ㅋㅋㅋ",
          "original_form": "아 그건 좋네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100023252.1.33",
          "form": "여긴 담임 선호해요",
          "original_form": "여긴 담임 선호해요",
          "speaker_id": "2",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100023252.1.34",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100023252.1.35",
          "form": "그러면 일반 학교의 경우에는요",
          "original_form": "그러면 일반 학교의 경우에는요",
          "speaker_id": "1",
          "time": "20210829 23:25"
        },
        {
          "id": "MDRW2100023252.1.36",
          "form": "선생님이 담임 맡으면 어떤 점이 어려워요?",
          "original_form": "선생님이 담임 맡으면 어떤 점이 어려워요?",
          "speaker_id": "1",
          "time": "20210829 23:25"
        }
      ]
