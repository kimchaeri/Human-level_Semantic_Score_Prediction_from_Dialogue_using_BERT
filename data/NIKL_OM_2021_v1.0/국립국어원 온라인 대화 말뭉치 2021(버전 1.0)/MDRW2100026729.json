      "utterance": [
        {
          "id": "MDRW2100026729.1.1",
          "form": "제가쓰는 온라인커뮤니티라면",
          "original_form": "제가쓰는 온라인커뮤니티라면",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026729.1.2",
          "form": "에타 맛게....?",
          "original_form": "에타 맛게....?",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026729.1.3",
          "form": "맛게가 뭐에요?",
          "original_form": "맛게가 뭐에요?",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026729.1.4",
          "form": "에타 이용을 안해봐서 ㅋㅋㅋㅋ",
          "original_form": "에타 이용을 안해봐서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026729.1.5",
          "form": "맛집게시판이요!!!",
          "original_form": "맛집게시판이요!!!",
          "speaker_id": "2",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026729.1.6",
          "form": "아아 ㅎㅎ",
          "original_form": "아아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100026729.1.7",
          "form": "거기서 맛집 정보 공유하는거에요?:)",
          "original_form": "거기서 맛집 정보 공유하는거에요?:)",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100026729.1.8",
          "form": "좋은 게시판이네요 ㅋㅋ",
          "original_form": "좋은 게시판이네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100026729.1.9",
          "form": "네네 맛집정보공유에요ㅋㅋ",
          "original_form": "네네 맛집정보공유에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100026729.1.10",
          "form": "최고에요 저에게 가장 필요한게시판ㅋㅋ",
          "original_form": "최고에요 저에게 가장 필요한게시판ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100026729.1.11",
          "form": "가입하고싶은생각이 절로들어요",
          "original_form": "가입하고싶은생각이 절로들어요",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100026729.1.12",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100026729.1.13",
          "form": "비원 커뮤니티는 안하셔요?:)",
          "original_form": "비원 커뮤니티는 안하셔요?:)",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100026729.1.14",
          "form": "해요!!!",
          "original_form": "해요!!!",
          "speaker_id": "2",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100026729.1.15",
          "form": "가끔 해요ㅋㅋㅋ",
          "original_form": "가끔 해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100026729.1.16",
          "form": "ㅋㅋㅋ저두요 가끔",
          "original_form": "ㅋㅋㅋ저두요 가끔",
          "speaker_id": "1",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100026729.1.17",
          "form": "커뮤 분위기가 때론 무섭지만",
          "original_form": "커뮤 분위기가 때론 무섭지만",
          "speaker_id": "1",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100026729.1.18",
          "form": "그래도 역시 이화 여성들이 모여있는곳이라 좋아요",
          "original_form": "그래도 역시 이화 여성들이 모여있는곳이라 좋아요",
          "speaker_id": "1",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100026729.1.19",
          "form": "맞아요ㅋㅋㅋ 가끔 싸움나지만",
          "original_form": "맞아요ㅋㅋㅋ 가끔 싸움나지만",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100026729.1.20",
          "form": "대부분의 경우는 개인을 존중해주는 분위기라",
          "original_form": "대부분의 경우는 개인을 존중해주는 분위기라",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100026729.1.21",
          "form": "너무좋아요ㅋㅋ",
          "original_form": "너무좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100026729.1.22",
          "form": "맞아요오",
          "original_form": "맞아요오",
          "speaker_id": "1",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100026729.1.23",
          "form": "에타는 대학 기반이에요??",
          "original_form": "에타는 대학 기반이에요??",
          "speaker_id": "1",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100026729.1.24",
          "form": "들어만봐서..ㅎㅎ",
          "original_form": "들어만봐서..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100026729.1.25",
          "form": "대학 기반이 학교별로 커뮤니티가 형성돼있냐는 뜻인가요???",
          "original_form": "대학 기반이 학교별로 커뮤니티가 형성돼있냐는 뜻인가요???",
          "speaker_id": "2",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100026729.1.26",
          "form": "이 뜻이면 맞아요!!!",
          "original_form": "이 뜻이면 맞아요!!!",
