      "utterance": [
        {
          "id": "MDRW2100035731.1.1",
          "form": "name1님 백신 맞으셨나요?",
          "original_form": "&name1&님 백신 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100035731.1.2",
          "form": "저 좀있으면 2차맞아요",
          "original_form": "저 좀있으면 2차맞아요",
          "speaker_id": "1",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100035731.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100035731.1.4",
          "form": "헐 빠르시네요! 저는 10월에 2차 맞을 예정입니다 ㅎ.ㅎ",
          "original_form": "헐 빠르시네요! 저는 10월에 2차 맞을 예정입니다 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100035731.1.5",
          "form": "네 사전예약으로 맞아서 좀이르게맞게됐어요",
          "original_form": "네 사전예약으로 맞아서 좀이르게맞게됐어요",
          "speaker_id": "1",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100035731.1.6",
          "form": "어떤거 맞으세요",
          "original_form": "어떤거 맞으세요",
          "speaker_id": "1",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100035731.1.7",
          "form": "저 화이자 맞을 예정이에용! name1님은요?",
          "original_form": "저 화이자 맞을 예정이에용! &name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100035731.1.8",
          "form": "1차는 그럼최근에 맞으셨나요?",
          "original_form": "1차는 그럼최근에 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100035731.1.9",
          "form": "네!24일에 맞았어요! 후유증 있으셨나요?",
          "original_form": "네!24일에 맞았어요! 후유증 있으셨나요?",
          "speaker_id": "2",
          "time": "20210830 00:00"
        },
        {
          "id": "MDRW2100035731.1.10",
          "form": "저 과하게 불안해하는성향있어서 그런지 괜히 가슴쪽이뻐근하더라구여",
          "original_form": "저 과하게 불안해하는성향있어서 그런지 괜히 가슴쪽이뻐근하더라구여",
          "speaker_id": "1",
          "time": "20210830 00:00"
        },
        {
          "id": "MDRW2100035731.1.11",
          "form": "그래서 약먹고하니까 괜찮았어요",
          "original_form": "그래서 약먹고하니까 괜찮았어요",
          "speaker_id": "1",
          "time": "20210830 00:01"
        },
        {
          "id": "MDRW2100035731.1.12",
          "form": "name2님은여",
          "original_form": "&name2&님은여",
          "speaker_id": "1",
          "time": "20210830 00:01"
        },
        {
          "id": "MDRW2100035731.1.13",
          "form": "아 저는 소화불량 빼고는 괜찮더라고요!ㅋㅋㅋㅋㅎㅎ",
          "original_form": "아 저는 소화불량 빼고는 괜찮더라고요!ㅋㅋㅋㅋㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:06"
        },
        {
          "id": "MDRW2100035731.1.14",
          "form": "팔이 뻐근한거랑...신기하게 이건 이틀이상 안가더라고요",
          "original_form": "팔이 뻐근한거랑...신기하게 이건 이틀이상 안가더라고요",
          "speaker_id": "2",
          "time": "20210830 00:06"
        },
        {
          "id": "MDRW2100035731.1.15",
          "form": "오 신기하네요 ㅋㅋㅋㅋ전 식욕이왕성해졌었는데",
          "original_form": "오 신기하네요 ㅋㅋㅋㅋ전 식욕이왕성해졌었는데",
          "speaker_id": "1",
          "time": "20210830 00:12"
        },
        {
          "id": "MDRW2100035731.1.16",
          "form": "백신핑계로 엄청시켜먹었어요",
          "original_form": "백신핑계로 엄청시켜먹었어요",
          "speaker_id": "1",
          "time": "20210830 00:13"
        },
        {
          "id": "MDRW2100035731.1.17",
          "form": "헐 저도요",
          "original_form": "헐 저도요",
          "speaker_id": "2",
          "time": "20210830 08:58"
        },
        {
          "id": "MDRW2100035731.1.18",
          "form": "백신핑계로 너무 먹어서 저... 식도염 왔습니다",
          "original_form": "백신핑계로 너무 먹어서 저... 식도염 왔습니다",
          "speaker_id": "2",
          "time": "20210830 08:58"
        },
        {
          "id": "MDRW2100035731.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
