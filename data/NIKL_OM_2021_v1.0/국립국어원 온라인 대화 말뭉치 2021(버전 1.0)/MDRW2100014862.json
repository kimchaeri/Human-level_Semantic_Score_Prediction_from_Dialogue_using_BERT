      "utterance": [
        {
          "id": "MDRW2100014862.1.1",
          "form": "엄마 나 단발머리 하고싶다고 했잖아",
          "original_form": "엄마 나 단발머리 하고싶다고 했잖아",
          "speaker_id": "1",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014862.1.2",
          "form": "응 미용실 같이가자",
          "original_form": "응 미용실 같이가자",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014862.1.3",
          "form": "요즘 코로나 때문에 미용실 가기 무섭다",
          "original_form": "요즘 코로나 때문에 미용실 가기 무섭다",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014862.1.4",
          "form": "그러게 운동도 다녀야한다며 못하고있네",
          "original_form": "그러게 운동도 다녀야한다며 못하고있네",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014862.1.5",
          "form": "응 건강챙기려면 운동 해야하는데 운동하다 코로나 걸려서 건강상할까봐 못하고있어 아이러니하네",
          "original_form": "응 건강챙기려면 운동 해야하는데 운동하다 코로나 걸려서 건강상할까봐 못하고있어 아이러니하네",
          "speaker_id": "1",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014862.1.6",
          "form": "나가서 산책이라도 하는건 어때?",
          "original_form": "나가서 산책이라도 하는건 어때?",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014862.1.7",
          "form": "더워서 산책은 힘들엉",
          "original_form": "더워서 산책은 힘들엉",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014862.1.8",
          "form": "곧 선선해지니까 그때 해",
          "original_form": "곧 선선해지니까 그때 해",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014862.1.9",
          "form": "알겠어 같이할래?",
          "original_form": "알겠어 같이할래?",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014862.1.10",
          "form": "좋아 아침산책이 건강에 좋대",
          "original_form": "좋아 아침산책이 건강에 좋대",
          "speaker_id": "2",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014862.1.11",
          "form": "난 못일어날수도있어",
          "original_form": "난 못일어날수도있어",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014862.1.12",
          "form": "깨워줄게",
          "original_form": "깨워줄게",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014862.1.13",
          "form": "운동은 참 쉽지않구나",
          "original_form": "운동은 참 쉽지않구나",
          "speaker_id": "1",
          "time": "20210825 21:05"
