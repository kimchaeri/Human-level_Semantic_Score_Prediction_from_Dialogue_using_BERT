      "utterance": [
        {
          "id": "MDRW2100018227.1.1",
          "form": "운동하시나요?",
          "original_form": "운동하시나요?",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018227.1.2",
          "form": "숨쉬기운동만 해요ㅎ",
          "original_form": "숨쉬기운동만 해요ㅎ",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018227.1.3",
          "form": "님은 하는 운동이 있으신가요",
          "original_form": "님은 하는 운동이 있으신가요",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018227.1.4",
          "form": "저는 그냥 런닝머신에서 조금씩 걷는 것을 한답니다",
          "original_form": "저는 그냥 런닝머신에서 조금씩 걷는 것을 한답니다",
          "speaker_id": "1",
          "time": "20210827 08:43"
        },
        {
          "id": "MDRW2100018227.1.5",
          "form": "와 집에서요? 아님 헬스장?",
          "original_form": "와 집에서요? 아님 헬스장?",
          "speaker_id": "2",
          "time": "20210827 08:44"
        },
        {
          "id": "MDRW2100018227.1.6",
          "form": "집에서요. 엄마가 본인이 한다고 사 놓고 빨래걸이로 쓰려고 해서 아까워서 가끔 걸어요",
          "original_form": "집에서요. 엄마가 본인이 한다고 사 놓고 빨래걸이로 쓰려고 해서 아까워서 가끔 걸어요",
          "speaker_id": "1",
          "time": "20210827 08:48"
        },
        {
          "id": "MDRW2100018227.1.7",
          "form": "저가 밖에 나가는 걸 귀찮아 해서 걷는 것도 그냥 집에서 해요 하하",
          "original_form": "저가 밖에 나가는 걸 귀찮아 해서 걷는 것도 그냥 집에서 해요 하하",
          "speaker_id": "1",
          "time": "20210827 08:49"
        },
        {
          "id": "MDRW2100018227.1.8",
          "form": "우리집은 빨래걸이로 쓰다 결국 다른집에 보냈어요ㅜ",
          "original_form": "우리집은 빨래걸이로 쓰다 결국 다른집에 보냈어요ㅜ",
          "speaker_id": "2",
          "time": "20210827 08:50"
        },
        {
          "id": "MDRW2100018227.1.9",
          "form": "쓰지도 않으면서 보내기는 또 아쉽더라고요",
          "original_form": "쓰지도 않으면서 보내기는 또 아쉽더라고요",
          "speaker_id": "2",
          "time": "20210827 08:50"
        },
        {
          "id": "MDRW2100018227.1.10",
          "form": "name1처럼 써줄걸",
          "original_form": "&name1&처럼 써줄걸",
          "speaker_id": "2",
          "time": "20210827 08:50"
        },
        {
          "id": "MDRW2100018227.1.11",
          "form": "집에 운동기구 뭐가 있나요",
          "original_form": "집에 운동기구 뭐가 있나요",
          "speaker_id": "2",
          "time": "20210827 08:50"
        },
        {
          "id": "MDRW2100018227.1.12",
          "form": "아이고 그러셨군요",
          "original_form": "아이고 그러셨군요",
          "speaker_id": "1",
          "time": "20210827 08:59"
        },
        {
          "id": "MDRW2100018227.1.13",
          "form": "운동기구는 런닝머신이 끝이네요",
          "original_form": "운동기구는 런닝머신이 끝이네요",
          "speaker_id": "1",
          "time": "20210827 08:59"
        },
        {
          "id": "MDRW2100018227.1.14",
          "form": "아 위에 올라가서 허리 트위스트 하는 기구 있네요",
          "original_form": "아 위에 올라가서 허리 트위스트 하는 기구 있네요",
          "speaker_id": "1",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018227.1.15",
          "form": "어떤 운동 기구인지 아시려나",
          "original_form": "어떤 운동 기구인지 아시려나",
          "speaker_id": "1",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018227.1.16",
          "form": "아 그거 효과 좋은가요?",
          "original_form": "아 그거 효과 좋은가요?",
          "speaker_id": "2",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018227.1.17",
          "form": "네네 알아요",
          "original_form": "네네 알아요",
          "speaker_id": "2",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018227.1.18",
          "form": "그거를 이제 하루 이틀 하다가 안해서 하하하",
          "original_form": "그거를 이제 하루 이틀 하다가 안해서 하하하",
          "speaker_id": "1",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018227.1.19",
          "form": "딱 한번 길게 일주일 한 적 있는데 허리가 탄탄해지긴해요",
          "original_form": "딱 한번 길게 일주일 한 적 있는데 허리가 탄탄해지긴해요",
          "speaker_id": "1",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018227.1.20",
          "form": "옆구리도 엄청 땡기고",
          "original_form": "옆구리도 엄청 땡기고",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018227.1.21",
          "form": "ㅎㅎㅎ 홈쇼핑보며 혹했는데",
          "original_form": "ㅎㅎㅎ 홈쇼핑보며 혹했는데",
          "speaker_id": "2",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018227.1.22",
          "form": "하하하 그쵸",
          "original_form": "하하하 그쵸",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018227.1.23",
          "form": "오오 효과 있네요 그럼!",
          "original_form": "오오 효과 있네요 그럼!",
          "speaker_id": "2",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018227.1.24",
          "form": "그런건가요 하하",
          "original_form": "그런건가요 하하",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018227.1.25",
          "form": "요새는 또 스트레칭이 중요하다고 하더군요",
          "original_form": "요새는 또 스트레칭이 중요하다고 하더군요",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018227.1.26",
          "form": "그래서 마사지 볼 같은 것도 샀어요",
          "original_form": "그래서 마사지 볼 같은 것도 샀어요",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018227.1.27",
          "form": "은근 집에 운동기구가 많네요",
          "original_form": "은근 집에 운동기구가 많네요",
          "speaker_id": "2",
          "time": "20210827 09:02"
        }
      ]
    }
  ]
