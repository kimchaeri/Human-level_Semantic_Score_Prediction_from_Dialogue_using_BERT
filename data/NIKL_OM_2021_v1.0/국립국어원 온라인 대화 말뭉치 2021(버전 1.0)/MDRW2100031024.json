      "utterance": [
        {
          "id": "MDRW2100031024.1.1",
          "form": "피부관리같은거 어떻게하세요?",
          "original_form": "피부관리같은거 어떻게하세요?",
          "speaker_id": "1",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100031024.1.2",
          "form": "작년에피부과가서",
          "original_form": "작년에피부과가서",
          "speaker_id": "2",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100031024.1.3",
          "form": "레이저 받으셨나요",
          "original_form": "레이저 받으셨나요",
          "speaker_id": "1",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100031024.1.4",
          "form": "6회권으로 한달에한번씩했는데 효과가없어요",
          "original_form": "6회권으로 한달에한번씩했는데 효과가없어요",
          "speaker_id": "2",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100031024.1.5",
          "form": "앗 그렇군요",
          "original_form": "앗 그렇군요",
          "speaker_id": "1",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100031024.1.6",
          "form": "저는 상태가 워낙안좋아서그런가",
          "original_form": "저는 상태가 워낙안좋아서그런가",
          "speaker_id": "1",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100031024.1.7",
          "form": "바르고 관리를 해야하는데 안하다보니",
          "original_form": "바르고 관리를 해야하는데 안하다보니",
          "speaker_id": "2",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100031024.1.8",
          "form": "레블라이트했더니 눈에 확띄게 좋아졌어요",
          "original_form": "레블라이트했더니 눈에 확띄게 좋아졌어요",
          "speaker_id": "1",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100031024.1.9",
          "form": "돈만 버렸어요",
          "original_form": "돈만 버렸어요",
          "speaker_id": "2",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100031024.1.10",
          "form": "그게뭐예요?",
          "original_form": "그게뭐예요?",
          "speaker_id": "2",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100031024.1.11",
          "form": "레이저의 한 종류인데",
          "original_form": "레이저의 한 종류인데",
          "speaker_id": "1",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100031024.1.12",
          "form": "피부미백도 되고 다크써클홍조가 없어지더라구요",
          "original_form": "피부미백도 되고 다크써클홍조가 없어지더라구요",
          "speaker_id": "1",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100031024.1.13",
          "form": "피부과에선 뭐뭐뭐한다고 얘기하는데 하나도 모르겠더라구요",
          "original_form": "피부과에선 뭐뭐뭐한다고 얘기하는데 하나도 모르겠더라구요",
          "speaker_id": "2",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100031024.1.14",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100031024.1.15",
          "form": "한번해선안되는거죠?",
          "original_form": "한번해선안되는거죠?",
          "speaker_id": "2",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100031024.1.16",
          "form": "네 보통 5회부터 효과가나는데요",
          "original_form": "네 보통 5회부터 효과가나는데요",
          "speaker_id": "1",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100031024.1.17",
          "form": "여러번해야효과를보는듯해요..그러고 계속 관리하고",
          "original_form": "여러번해야효과를보는듯해요..그러고 계속 관리하고",
          "speaker_id": "2",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100031024.1.18",
          "form": "친구 엄마는그거하고 기미가 없어졌대요",
          "original_form": "친구 엄마는그거하고 기미가 없어졌대요",
          "speaker_id": "1",
          "time": "20210904 15:41"
        },
        {
          "id": "MDRW2100031024.1.19",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
          "time": "20210904 15:41"
        },
        {
          "id": "MDRW2100031024.1.20",
          "form": "맞아요 그리고 레이저마다 효과도 천차만별이ㄱᆢ",
          "original_form": "맞아요 그리고 레이저마다 효과도 천차만별이ㄱᆢ",
          "speaker_id": "1",
          "time": "20210904 15:41"
        },
        {
          "id": "MDRW2100031024.1.21",
          "form": "돈만 날린것도 많아요",
          "original_form": "돈만 날린것도 많아요",
          "speaker_id": "1",
