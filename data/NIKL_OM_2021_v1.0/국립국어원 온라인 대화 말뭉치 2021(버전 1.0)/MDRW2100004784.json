      "utterance": [
        {
          "id": "MDRW2100004784.1.1",
          "form": "종교 활동하세요?",
          "original_form": "종교 활동하세요?",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100004784.1.2",
          "form": "아니요 무교예요",
          "original_form": "아니요 무교예요",
          "speaker_id": "2",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100004784.1.3",
          "form": "불교에 가깝다고 해야되나 ㅎ",
          "original_form": "불교에 가깝다고 해야되나 ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100004784.1.4",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "2",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100004784.1.5",
          "form": "저도 비슷해요ㅎ",
          "original_form": "저도 비슷해요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100004784.1.6",
          "form": "저희 아버님은 기독교, 어머님은 불교",
          "original_form": "저희 아버님은 기독교, 어머님은 불교",
          "speaker_id": "1",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100004784.1.7",
          "form": "뭘믿어야할지몰라 전 무교요ㅎ",
          "original_form": "뭘믿어야할지몰라 전 무교요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100004784.1.8",
          "form": "엇 두분이 종교가 다르시면 문제 없으신가요?",
          "original_form": "엇 두분이 종교가 다르시면 문제 없으신가요?",
          "speaker_id": "2",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100004784.1.9",
          "form": "한분이 바꾸시지 않나요?ㅋ",
          "original_form": "한분이 바꾸시지 않나요?ㅋ",
          "speaker_id": "2",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100004784.1.10",
          "form": "아버님이 바꾸시질 않아요ㅎㅎ",
          "original_form": "아버님이 바꾸시질 않아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100004784.1.11",
          "form": "그래서 저도 무교이지만 어머님 따라 불교이긴해요ㅎ",
          "original_form": "그래서 저도 무교이지만 어머님 따라 불교이긴해요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100004784.1.12",
          "form": "반반이라고하면 정확하겠네요ㅎ",
          "original_form": "반반이라고하면 정확하겠네요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100004784.1.13",
          "form": "근데 name2님은 왜 불교쪽에 가까우세요?",
          "original_form": "근데 &name2&님은 왜 불교쪽에 가까우세요?",
          "speaker_id": "1",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100004784.1.14",
          "form": "할머니가 가끔 절에 다니셨는데",
          "original_form": "할머니가 가끔 절에 다니셨는데",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100004784.1.15",
          "form": "돌아가시고는 종교활동하시는분이 없으니 무교가 맞을듯요ㅎ",
          "original_form": "돌아가시고는 종교활동하시는분이 없으니 무교가 맞을듯요ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100004784.1.16",
          "form": "제사는 지내요",
          "original_form": "제사는 지내요",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100004784.1.17",
          "form": "에고 제가 괜한이야기를...",
          "original_form": "에고 제가 괜한이야기를...",
          "speaker_id": "1",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100004784.1.18",
          "form": "제사는 지내야죠ㅎ 전 종교를 떠나서 제사는 꼭 해야한다고 생각해요",
          "original_form": "제사는 지내야죠ㅎ 전 종교를 떠나서 제사는 꼭 해야한다고 생각해요",
          "speaker_id": "1",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100004784.1.19",
          "form": "그게 예의인것같아요ㅎ 유교사상이라고 하죠",
          "original_form": "그게 예의인것같아요ㅎ 유교사상이라고 하죠",
          "speaker_id": "1",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100004784.1.20",
          "form": "그럼 아버님이 기독교신데 차례나 제사는 어찌하세요?",
          "original_form": "그럼 아버님이 기독교신데 차례나 제사는 어찌하세요?",
          "speaker_id": "2",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100004784.1.21",
          "form": "어머님의 파워가 더쎄시려나요",
          "original_form": "어머님의 파워가 더쎄시려나요",
          "speaker_id": "2",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100004784.1.22",
          "form": "그건아니고ㅎ",
          "original_form": "그건아니고ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100004784.1.23",
          "form": "제사는 다 지내요ㅎ",
          "original_form": "제사는 다 지내요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100004784.1.24",
          "form": "저희 할머니가 기독교이신대도 제사는 꼬박꼬박 지내세요ㅎ",
          "original_form": "저희 할머니가 기독교이신대도 제사는 꼬박꼬박 지내세요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100004784.1.25",
          "form": "어르신들은 그런가봐요ㅎ 종교를 떠나서ㅎ 제사는 꼭하세요",
          "original_form": "어르신들은 그런가봐요ㅎ 종교를 떠나서ㅎ 제사는 꼭하세요",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100004784.1.26",
          "form": "엥? 기독교신데 제사를지내세요?ㅋㅋ",
          "original_form": "엥? 기독교신데 제사를지내세요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100004784.1.27",
          "form": "첨봐요 그런경우는",
          "original_form": "첨봐요 그런경우는",
          "speaker_id": "2",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100004784.1.28",
          "form": "천주교 아니시구요?",
          "original_form": "천주교 아니시구요?",
          "speaker_id": "2",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100004784.1.29",
          "form": "ㅎㅎ기독교맞아요ㅎㅎ",
          "original_form": "ㅎㅎ기독교맞아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100004784.1.30",
          "form": "이상한건가요?ㅎㅎ",
          "original_form": "이상한건가요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100004784.1.31",
          "form": "네 ㅋ 기독교는 그런경우 못봐서",
          "original_form": "네 ㅋ 기독교는 그런경우 못봐서",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100004784.1.32",
          "form": "제사음식도 안드시던데",
          "original_form": "제사음식도 안드시던데",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100004784.1.33",
          "form": "ㅎㅎ 저희쪽은 믿음이 절실하지 않나봐요~^^",
          "original_form": "ㅎㅎ 저희쪽은 믿음이 절실하지 않나봐요~^^",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100004784.1.34",
          "form": "아무래도 돌아가신분에 대한 예의때문이겟죠ㅎ",
          "original_form": "아무래도 돌아가신분에 대한 예의때문이겟죠ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100004784.1.35",
          "form": "그런가보네요^^ 종교가 뭐 중요한가요",
          "original_form": "그런가보네요^^ 종교가 뭐 중요한가요",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100004784.1.36",
          "form": "나자신을 믿어야지",
          "original_form": "나자신을 믿어야지",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100004784.1.37",
          "form": "내가 중요하죠ㅎ 암요ㅎ",
          "original_form": "내가 중요하죠ㅎ 암요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100004784.1.38",
          "form": "근데 저도 나자신을 믿는 사람인데ㅎ",
          "original_form": "근데 저도 나자신을 믿는 사람인데ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100004784.1.39",
          "form": "힘든시기가오면 어쩔수없이 잡신이든 뭐든 찾더라구요ㅎ",
          "original_form": "힘든시기가오면 어쩔수없이 잡신이든 뭐든 찾더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100004784.1.40",
          "form": "전 근데 사주보는거 좋아해요 미신 ㅋ운세 요런거",
          "original_form": "전 근데 사주보는거 좋아해요 미신 ㅋ운세 요런거",
          "speaker_id": "2",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100004784.1.41",
          "form": "믿음이 있음 마음의 안정이 오긴하는듯요",
          "original_form": "믿음이 있음 마음의 안정이 오긴하는듯요",
          "speaker_id": "2",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100004784.1.42",
          "form": "사주는 아무래도 맞는것같애요ㅎ",
          "original_form": "사주는 아무래도 맞는것같애요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100004784.1.43",
          "form": "저도 안믿는데ㅎㅎ 저한테 역마살이 있다고 하던데...",
          "original_form": "저도 안믿는데ㅎㅎ 저한테 역마살이 있다고 하던데...",
          "speaker_id": "1",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100004784.1.44",
          "form": "성격은좀 맞는듯요",
          "original_form": "성격은좀 맞는듯요",
          "speaker_id": "2",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100004784.1.45",
          "form": "아무래도 그분은 신들리신듯요ㅎ",
          "original_form": "아무래도 그분은 신들리신듯요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100004784.1.46",
          "form": "아 제 성격이 그래요?",
          "original_form": "아 제 성격이 그래요?",
          "speaker_id": "1",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100004784.1.47",
          "form": "아니요ㅋ사주보면 성격이 나오는거 같단얘기",
          "original_form": "아니요ㅋ사주보면 성격이 나오는거 같단얘기",
