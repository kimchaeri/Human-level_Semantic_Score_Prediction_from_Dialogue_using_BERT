      "utterance": [
        {
          "id": "MDRW2100041147.1.1",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "1",
          "time": "20210905 18:01"
        },
        {
          "id": "MDRW2100041147.1.2",
          "form": "아직 안했어요 뭐 드시나요",
          "original_form": "아직 안했어요 뭐 드시나요",
          "speaker_id": "2",
          "time": "20210905 18:04"
        },
        {
          "id": "MDRW2100041147.1.3",
          "form": "전 일찍 치킨 먹었어요ㅋㅋ",
          "original_form": "전 일찍 치킨 먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:04"
        },
        {
          "id": "MDRW2100041147.1.4",
          "form": "오 점저를 드셨나요",
          "original_form": "오 점저를 드셨나요",
          "speaker_id": "2",
          "time": "20210905 18:05"
        },
        {
          "id": "MDRW2100041147.1.5",
          "form": "그럼 저녁 뛰어넘으시나여",
          "original_form": "그럼 저녁 뛰어넘으시나여",
          "speaker_id": "2",
          "time": "20210905 18:05"
        },
        {
          "id": "MDRW2100041147.1.6",
          "form": "5시쯤 먹어서 넘 배불러요",
          "original_form": "5시쯤 먹어서 넘 배불러요",
          "speaker_id": "1",
          "time": "20210905 18:06"
        },
        {
          "id": "MDRW2100041147.1.7",
          "form": "아하 무슨 치킨 드셨어요",
          "original_form": "아하 무슨 치킨 드셨어요",
          "speaker_id": "2",
          "time": "20210905 18:10"
        },
        {
          "id": "MDRW2100041147.1.8",
          "form": "치킨마루로 3마리 먹었어요ㅋㅋ",
          "original_form": "치킨마루로 3마리 먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:13"
        },
        {
          "id": "MDRW2100041147.1.9",
          "form": "캬 3마리나 드세요?",
          "original_form": "캬 3마리나 드세요?",
          "speaker_id": "2",
          "time": "20210905 18:14"
        },
        {
          "id": "MDRW2100041147.1.10",
          "form": "전 방금 심심이로 받은",
          "original_form": "전 방금 심심이로 받은",
          "speaker_id": "2",
          "time": "20210905 18:14"
        },
        {
          "id": "MDRW2100041147.1.11",
          "form": "굽네치킨 시켰어요",
          "original_form": "굽네치킨 시켰어요",
          "speaker_id": "2",
          "time": "20210905 18:15"
        },
        {
          "id": "MDRW2100041147.1.12",
          "form": "ㅋㅋ대박",
          "original_form": "ㅋㅋ대박",
          "speaker_id": "1",
          "time": "20210905 18:15"
        },
        {
          "id": "MDRW2100041147.1.13",
          "form": "저는 아직ㅋㅋㅋ",
          "original_form": "저는 아직ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100041147.1.14",
          "form": "식구 4명이서 먹었어요",
          "original_form": "식구 4명이서 먹었어요",
          "speaker_id": "1",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100041147.1.15",
          "form": "오오 4명이서 3마리군요",
          "original_form": "오오 4명이서 3마리군요",
          "speaker_id": "2",
          "time": "20210905 18:25"
        },
        {
          "id": "MDRW2100041147.1.16",
          "form": "저희 집은 사실 3명이서 한마리 먹어욬ㅋㅋㅋㅋ",
          "original_form": "저희 집은 사실 3명이서 한마리 먹어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:25"
        },
        {
          "id": "MDRW2100041147.1.17",
          "form": "ㅋㅋ 저흰 대식가... ㅋㅋ",
          "original_form": "ㅋㅋ 저흰 대식가... ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:26"
        },
        {
          "id": "MDRW2100041147.1.18",
          "form": "식비 많이 들겠네요 ㅋㅋㅋ",
          "original_form": "식비 많이 들겠네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:28"
        },
        {
          "id": "MDRW2100041147.1.19",
          "form": "ㅋㅋ 많이 벌어야해욪ㅋㅋ",
          "original_form": "ㅋㅋ 많이 벌어야해욪ㅋㅋ",
