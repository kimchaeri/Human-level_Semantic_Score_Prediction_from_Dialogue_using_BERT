      "utterance": [
        {
          "id": "MDRW2100036667.1.1",
          "form": "아침 드셨나요?!",
          "original_form": "아침 드셨나요?!",
          "speaker_id": "2",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036667.1.2",
          "form": "넵! 쥬스 마셨어용!",
          "original_form": "넵! 쥬스 마셨어용!",
          "speaker_id": "1",
          "time": "20210831 09:55"
        },
        {
          "id": "MDRW2100036667.1.3",
          "form": "name2님은용?",
          "original_form": "&name2&님은용?",
          "speaker_id": "1",
          "time": "20210831 09:55"
        },
        {
          "id": "MDRW2100036667.1.4",
          "form": "저는 시리얼 두 그릇 먹고",
          "original_form": "저는 시리얼 두 그릇 먹고",
          "speaker_id": "2",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100036667.1.5",
          "form": "뻗었다가 지금 일어나버렸어요 ^.^",
          "original_form": "뻗었다가 지금 일어나버렸어요 ^.^",
          "speaker_id": "2",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100036667.1.6",
          "form": "점심.. 먹으려고요!",
          "original_form": "점심.. 먹으려고요!",
          "speaker_id": "2",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100036667.1.7",
          "form": "어떤거 드셨나욥?!",
          "original_form": "어떤거 드셨나욥?!",
          "speaker_id": "1",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100036667.1.8",
          "form": "저 연어 구워먹었어요ㅁㅁㅁ",
          "original_form": "저 연어 구워먹었어요ㅁㅁㅁ",
          "speaker_id": "2",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100036667.1.9",
          "form": "name1님 연어 좋아하세요?",
          "original_form": "&name1&님 연어 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100036667.1.10",
          "form": "크으 넹!",
          "original_form": "크으 넹!",
          "speaker_id": "1",
          "time": "20210831 18:17"
        },
        {
          "id": "MDRW2100036667.1.11",
          "form": "저는 연어는 생이 더 좋더라구요",
          "original_form": "저는 연어는 생이 더 좋더라구요",
          "speaker_id": "1",
          "time": "20210831 18:17"
        },
        {
          "id": "MDRW2100036667.1.12",
          "form": "헐럴 저도요! 근데 ㅠ 요새 여름이라",
          "original_form": "헐럴 저도요! 근데 ㅠ 요새 여름이라",
          "speaker_id": "2",
          "time": "20210831 19:20"
        },
        {
          "id": "MDRW2100036667.1.13",
          "form": "생으로 먹었다 탈날까봐 구워먹었어요",
          "original_form": "생으로 먹었다 탈날까봐 구워먹었어요",
          "speaker_id": "2",
          "time": "20210831 19:20"
        },
        {
          "id": "MDRW2100036667.1.14",
          "form": "아ㅠㅠ 맞죠 여름은 회를 먹고싶어도 걱정되서 못먹어요ㅜㅠㅜ",
          "original_form": "아ㅠㅠ 맞죠 여름은 회를 먹고싶어도 걱정되서 못먹어요ㅜㅠㅜ",
          "speaker_id": "1",
          "time": "20210831 19:21"
        },
        {
          "id": "MDRW2100036667.1.15",
          "form": "요즘 날좋아져서 회먹을 생각에 가득차있어요ㅋㅋㅋ",
          "original_form": "요즘 날좋아져서 회먹을 생각에 가득차있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:21"
        },
        {
          "id": "MDRW2100036667.1.16",
          "form": "헛헛 저도요 ㅎ.ㅎ",
          "original_form": "헛헛 저도요 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210831 19:23"
        },
        {
          "id": "MDRW2100036667.1.17",
          "form": "name1님은 어떤 회가 제일 좋으셍ㅎ?",
          "original_form": "&name1&님은 어떤 회가 제일 좋으셍ㅎ?",
          "speaker_id": "2",
          "time": "20210831 19:23"
        },
        {
          "id": "MDRW2100036667.1.18",
          "form": "광어요!",
          "original_form": "광어요!",
          "speaker_id": "1",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100036667.1.19",
          "form": "씹는 식감이 너무 맛있어용",
          "original_form": "씹는 식감이 너무 맛있어용",
          "speaker_id": "1",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100036667.1.20",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100036667.1.21",
          "form": "저는 연어만 먹어요 ㅎ.ㅎ !!",
          "original_form": "저는 연어만 먹어요 ㅎ.ㅎ !!",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100036667.1.22",
          "form": "광어가 그 하얀? 껍질 회 맞나요!?",
          "original_form": "광어가 그 하얀? 껍질 회 맞나요!?",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100036667.1.23",
          "form": "네 속은 하얗구 껍질은 검은?!",
          "original_form": "네 속은 하얗구 껍질은 검은?!",
          "speaker_id": "1",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100036667.1.24",
          "form": "헛 네네! 부모님이 좋아하셔소",
          "original_form": "헛 네네! 부모님이 좋아하셔소",
          "speaker_id": "2",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100036667.1.25",
          "form": "네네 부모님이 좋아하셔서",
          "original_form": "네네 부모님이 좋아하셔서",
          "speaker_id": "2",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100036667.1.26",
          "form": "부모님은 광어 저는 연어 엄청 많이!",
          "original_form": "부모님은 광어 저는 연어 엄청 많이!",
          "speaker_id": "2",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100036667.1.27",
          "form": "먹어요 ㅎ.ㅎ",
          "original_form": "먹어요 ㅎ.ㅎ",
          "speaker_id": "2",
