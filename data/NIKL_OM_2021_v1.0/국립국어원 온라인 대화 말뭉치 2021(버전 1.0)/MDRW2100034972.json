      "utterance": [
        {
          "id": "MDRW2100034972.1.1",
          "form": "name2님이 키우는 오리는 몇개월인가요?",
          "original_form": "&name2&님이 키우는 오리는 몇개월인가요?",
          "speaker_id": "1",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100034972.1.2",
          "form": "아 저 비염때문에 오리는 못키우고 있어여ㅠㅠ",
          "original_form": "아 저 비염때문에 오리는 못키우고 있어여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100034972.1.3",
          "form": "그래서 맨날 오리동영상 찾아봐요ㅋㅋㅋㅋ",
          "original_form": "그래서 맨날 오리동영상 찾아봐요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100034972.1.4",
          "form": "name1님은 동물 키우세요?",
          "original_form": "&name1&님은 동물 키우세요?",
          "speaker_id": "2",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100034972.1.5",
          "form": "아뇨 오리를 좋아하신다구 해서 ㅋㅋ",
          "original_form": "아뇨 오리를 좋아하신다구 해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100034972.1.6",
          "form": "여쭤봤어요",
          "original_form": "여쭤봤어요",
          "speaker_id": "1",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100034972.1.7",
          "form": "근데 오리는 새다보니깐",
          "original_form": "근데 오리는 새다보니깐",
          "speaker_id": "1",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100034972.1.8",
          "form": "변이 줄줄 나오기때문에",
          "original_form": "변이 줄줄 나오기때문에",
          "speaker_id": "1",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100034972.1.9",
          "form": "풀어놓고 키우면 계속 덩을 치워야하니 그게 좀 힘들거에요",
          "original_form": "풀어놓고 키우면 계속 덩을 치워야하니 그게 좀 힘들거에요",
          "speaker_id": "1",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100034972.1.10",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034972.1.11",
          "form": "되게 잘 아시네요!",
          "original_form": "되게 잘 아시네요!",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034972.1.12",
          "form": "맞아요 새라 괄약근이 없어서",
          "original_form": "맞아요 새라 괄약근이 없어서",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034972.1.13",
          "form": "배변 훈련이 불가능해요",
          "original_form": "배변 훈련이 불가능해요",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034972.1.14",
          "form": "네 새는 날아가야 하기에 평생 지릴수 밖에 없는,,",
          "original_form": "네 새는 날아가야 하기에 평생 지릴수 밖에 없는,,",
          "speaker_id": "1",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034972.1.15",
          "form": "제 집 근처에 늘 비둘기가",
          "original_form": "제 집 근처에 늘 비둘기가",
          "speaker_id": "1",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100034972.1.16",
          "form": "지려서 자 알아요,, 손이 안 닿는곳에 싼 똥때문에 ㅠㅠ 난간이 삭고 있어요,,,",
          "original_form": "지려서 자 알아요,, 손이 안 닿는곳에 싼 똥때문에 ㅠㅠ 난간이 삭고 있어요,,,",
          "speaker_id": "1",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100034972.1.17",
          "form": "평생 지릴수밖에없는ㅋㅋㅋㅋㅋㅋ",
          "original_form": "평생 지릴수밖에없는ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100034972.1.18",
          "form": "아 너무 웃겨요ㅋㅋㅋㅋㅋ",
          "original_form": "아 너무 웃겨요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100034972.1.19",
          "form": "비둘기 똥",
          "original_form": "비둘기 똥",
          "speaker_id": "2",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100034972.1.20",
          "form": "ㅋㅋ자유의 상징이 새잖아요 날아가니간",
          "original_form": "ㅋㅋ자유의 상징이 새잖아요 날아가니간",
          "speaker_id": "1",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100034972.1.21",
          "form": "독성있다고 이거",
          "original_form": "독성있다고 이거",
          "speaker_id": "2",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100034972.1.22",
          "form": "깨끗하게닦기 힘들다고 하던데ㅠㅠ",
          "original_form": "깨끗하게닦기 힘들다고 하던데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100034972.1.23",
          "form": "몸이 가볍게 날아가려면 숙변이 없어야 되고 지리게되니,,참,,,",
          "original_form": "몸이 가볍게 날아가려면 숙변이 없어야 되고 지리게되니,,참,,,",
          "speaker_id": "1",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100034972.1.24",
          "form": "그러게요ㅠㅠ",
          "original_form": "그러게요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100034972.1.25",
          "form": "네 그리고 비둘기가 장소 집착이 있어서 게속 싸서 똥이 쌓이기때문에",
          "original_form": "네 그리고 비둘기가 장소 집착이 있어서 게속 싸서 똥이 쌓이기때문에",
          "speaker_id": "1",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100034972.1.26",
          "form": "아 헐",
          "original_form": "아 헐",
          "speaker_id": "2",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100034972.1.27",
          "form": "전 사실 조류중에 비둘기 혐오자입니다",
          "original_form": "전 사실 조류중에 비둘기 혐오자입니다",
          "speaker_id": "1",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100034972.1.28",
          "form": "하지만 애완새는 좋아해요",
          "original_form": "하지만 애완새는 좋아해요",
          "speaker_id": "1",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100034972.1.29",
          "form": "ㅠㅠㅠ 비둘기가 유해동물인가",
          "original_form": "ㅠㅠㅠ 비둘기가 유해동물인가",
          "speaker_id": "2",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100034972.1.30",
          "form": "그렇죠?",
          "original_form": "그렇죠?",
          "speaker_id": "2",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100034972.1.31",
          "form": "똥을 집에서 치우잖아요 ㅠㅠ",
          "original_form": "똥을 집에서 치우잖아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100034972.1.32",
          "form": "주인들이 알아서 잘 관리해주고 ㅋㅋ",
          "original_form": "주인들이 알아서 잘 관리해주고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034972.1.33",
          "form": "그리고 오리는 주둥이가 특히 귀엽죠",
          "original_form": "그리고 오리는 주둥이가 특히 귀엽죠",
          "speaker_id": "1",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034972.1.34",
          "form": "이빨이 없어서",
          "original_form": "이빨이 없어서",
          "speaker_id": "2",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034972.1.35",
          "form": "부리로 겨우 공격하는게",
          "original_form": "부리로 겨우 공격하는게",
          "speaker_id": "2",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034972.1.36",
          "form": "하찮아요ㅋㅋㅋㅋㅋ",
          "original_form": "하찮아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034972.1.37",
          "form": "그리고 좀 앵무새같은 새는 눈이랑 부리가 무서운데",
          "original_form": "그리고 좀 앵무새같은 새는 눈이랑 부리가 무서운데",
          "speaker_id": "1",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100034972.1.38",
          "form": "오리는 눈이 작고 까맣고 주둥이가 넙적해서 친근감가는 얼굴이에요",
          "original_form": "오리는 눈이 작고 까맣고 주둥이가 넙적해서 친근감가는 얼굴이에요",
          "speaker_id": "1",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100034972.1.39",
          "form": "앗 마자용 살짝 무섭죠ㅋㅋㅋ",
          "original_form": "앗 마자용 살짝 무섭죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100034972.1.40",
          "form": "앗 귀여워해주셔서 다행이에요",
          "original_form": "앗 귀여워해주셔서 다행이에요",
          "speaker_id": "2",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100034972.1.41",
          "form": "유튜브 도시오리",
          "original_form": "유튜브 도시오리",
          "speaker_id": "2",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100034972.1.42",
          "form": "앵무새도 싫어하는건 아닌데 살짝무섭죠",
          "original_form": "앵무새도 싫어하는건 아닌데 살짝무섭죠",
          "speaker_id": "1",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100034972.1.43",
          "form": "채널 추천드려용",
          "original_form": "채널 추천드려용",
          "speaker_id": "2",
          "time": "20210828 23:15"
        },
        {
