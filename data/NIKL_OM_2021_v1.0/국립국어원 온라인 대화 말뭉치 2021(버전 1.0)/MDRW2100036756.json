      "utterance": [
        {
          "id": "MDRW2100036756.1.1",
          "form": "안까져요?껍질?",
          "original_form": "안까져요?껍질?",
          "speaker_id": "1",
          "time": "20210831 11:25"
        },
        {
          "id": "MDRW2100036756.1.2",
          "form": "복숭아요?",
          "original_form": "복숭아요?",
          "speaker_id": "2",
          "time": "20210831 11:27"
        },
        {
          "id": "MDRW2100036756.1.3",
          "form": "바나나ㅋㅋ껍질채냉동 아까ㅋㅋ",
          "original_form": "바나나ㅋㅋ껍질채냉동 아까ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100036756.1.4",
          "form": "훅 지나가서",
          "original_form": "훅 지나가서",
          "speaker_id": "1",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100036756.1.5",
          "form": "아아 얼리면",
          "original_form": "아아 얼리면",
          "speaker_id": "2",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100036756.1.6",
          "form": "껍질이 안까질거잖아요",
          "original_form": "껍질이 안까질거잖아요",
          "speaker_id": "2",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100036756.1.7",
          "form": "악ㅋㅋ깍아먹으면안되나요",
          "original_form": "악ㅋㅋ깍아먹으면안되나요",
          "speaker_id": "1",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100036756.1.8",
          "form": "넘 당연한데 얼리기전엔 생각하기힘들얼어요",
          "original_form": "넘 당연한데 얼리기전엔 생각하기힘들얼어요",
          "speaker_id": "2",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100036756.1.9",
          "form": "잘안돼요 다 얼어서요",
          "original_form": "잘안돼요 다 얼어서요",
          "speaker_id": "2",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100036756.1.10",
          "form": "ㅋㅋㅋㅋㅋ. 이런ㅎㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ. 이런ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100036756.1.11",
          "form": "까서 얼리는게 좋아요",
          "original_form": "까서 얼리는게 좋아요",
          "speaker_id": "2",
          "time": "20210831 11:35"
        },
        {
          "id": "MDRW2100036756.1.12",
          "form": "그렇겠네요ㅋ예전에 저도안까그넣은적 있는것같은데.ㅋ",
          "original_form": "그렇겠네요ㅋ예전에 저도안까그넣은적 있는것같은데.ㅋ",
          "speaker_id": "1",
          "time": "20210831 11:41"
        },
        {
          "id": "MDRW2100036756.1.13",
          "form": "갑자기 과일타르트 먹고싶네요",
          "original_form": "갑자기 과일타르트 먹고싶네요",
          "speaker_id": "2",
          "time": "20210831 11:41"
        },
        {
          "id": "MDRW2100036756.1.14",
          "form": "예쁘게 생겼더라구요",
          "original_form": "예쁘게 생겼더라구요",
          "speaker_id": "2",
          "time": "20210831 11:42"
        },
        {
          "id": "MDRW2100036756.1.15",
          "form": "과일타르트는 한번도 안먹어봤는데",
          "original_form": "과일타르트는 한번도 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210831 11:53"
        },
        {
          "id": "MDRW2100036756.1.16",
          "form": "맛있을거같아요",
          "original_form": "맛있을거같아요",
          "speaker_id": "2",
          "time": "20210831 11:54"
        },
        {
          "id": "MDRW2100036756.1.17",
          "form": "근데 그 타르트란게 부스러기가 엄청 나오는거같던데요;;;",
          "original_form": "근데 그 타르트란게 부스러기가 엄청 나오는거같던데요;;;",
