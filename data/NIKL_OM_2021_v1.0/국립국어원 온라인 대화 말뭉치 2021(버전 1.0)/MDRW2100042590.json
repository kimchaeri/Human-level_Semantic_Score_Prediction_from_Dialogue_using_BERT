      "utterance": [
        {
          "id": "MDRW2100042590.1.1",
          "form": "심지어",
          "original_form": "심지어",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042590.1.2",
          "form": "주가 엄청 올라서",
          "original_form": "주가 엄청 올라서",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042590.1.3",
          "form": "15만원 찍었었음 ㅎ",
          "original_form": "15만원 찍었었음 ㅎ",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042590.1.4",
          "form": "ㅇㅇㅎ",
          "original_form": "ㅇㅇㅎ",
          "speaker_id": "1",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042590.1.5",
          "form": "연기금이 엄청 사주더라",
          "original_form": "연기금이 엄청 사주더라",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042590.1.6",
          "form": "근데 그것도 싫엇음",
          "original_form": "근데 그것도 싫엇음",
          "speaker_id": "2",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100042590.1.7",
          "form": "거기다도 럽프듀만큼은 아니지만",
          "original_form": "거기다도 럽프듀만큼은 아니지만",
          "speaker_id": "1",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100042590.1.8",
          "form": "돈 좀 썼었는데",
          "original_form": "돈 좀 썼었는데",
          "speaker_id": "1",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100042590.1.9",
          "form": "나 거기다 투자하기 싫으니까",
          "original_form": "나 거기다 투자하기 싫으니까",
          "speaker_id": "2",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100042590.1.10",
          "form": "왜 이세상에는 대표가 입을 다물고있는 갓겜이",
          "original_form": "왜 이세상에는 대표가 입을 다물고있는 갓겜이",
          "speaker_id": "1",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100042590.1.11",
          "form": "없는걸까",
          "original_form": "없는걸까",
          "speaker_id": "1",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100042590.1.12",
          "form": "대표가 되려면 자아가 비대해야해서..?",
          "original_form": "대표가 되려면 자아가 비대해야해서..?",
          "speaker_id": "2",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100042590.1.13",
          "form": "ㅎㅏ",
          "original_form": "ㅎㅏ",
          "speaker_id": "1",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042590.1.14",
          "form": "양립할수없구나",
          "original_form": "양립할수없구나",
          "speaker_id": "1",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042590.1.15",
          "form": "그래.......",
          "original_form": "그래.......",
          "speaker_id": "1",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042590.1.16",
          "form": "슬프다",
          "original_form": "슬프다",
          "speaker_id": "1",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042590.1.17",
          "form": "ㅋㅋㅋㅋㅋ그렇지...",
          "original_form": "ㅋㅋㅋㅋㅋ그렇지...",
          "speaker_id": "2",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042590.1.18",
          "form": "암튼",
          "original_form": "암튼",
          "speaker_id": "2",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042590.1.19",
          "form": "로오히 회사",
          "original_form": "로오히 회사",
          "speaker_id": "2",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042590.1.20",
          "form": "로오히 초대박쳐서 상장 준비중이라며",
          "original_form": "로오히 초대박쳐서 상장 준비중이라며",
          "speaker_id": "2",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042590.1.21",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042590.1.22",
          "form": "진짜 꼴보기 싫다",
          "original_form": "진짜 꼴보기 싫다",
          "speaker_id": "1",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042590.1.23",
          "form": "ㅇㅇ ㅎ",
          "original_form": "ㅇㅇ ㅎ",
          "speaker_id": "2",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100042590.1.24",
          "form": "매출 100억 찍엇다고",
          "original_form": "매출 100억 찍엇다고",
          "speaker_id": "2",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100042590.1.25",
          "form": "신나서 입털던데 진심 보기싫었음",
          "original_form": "신나서 입털던데 진심 보기싫었음",
          "speaker_id": "2",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100042590.1.26",
          "form": "ㅎㅏ",
          "original_form": "ㅎㅏ",
          "speaker_id": "1",
          "time": "20210906 23:14"
        },
        {
          "id": "MDRW2100042590.1.27",
          "form": "내가 럽프듀만큼 좋아할순없지만",
          "original_form": "내가 럽프듀만큼 좋아할순없지만",
          "speaker_id": "1",
          "time": "20210906 23:14"
        },
        {
          "id": "MDRW2100042590.1.28",
          "form": "그래도 신야근이",
          "original_form": "그래도 신야근이",
          "speaker_id": "1",
          "time": "20210906 23:14"
        },
        {
          "id": "MDRW2100042590.1.29",
          "form": "좀 발붙일 게임이",
          "original_form": "좀 발붙일 게임이",
          "speaker_id": "1",
          "time": "20210906 23:14"
        },
        {
          "id": "MDRW2100042590.1.30",
          "form": "되주었으면",
          "original_form": "되주었으면",
          "speaker_id": "1",
          "time": "20210906 23:14"
        },
        {
          "id": "MDRW2100042590.1.31",
          "form": "했는데....",
          "original_form": "했는데....",
          "speaker_id": "1",
          "time": "20210906 23:14"
        },
        {
          "id": "MDRW2100042590.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 올해의 대 사기극 느낌인데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 올해의 대 사기극 느낌인데",
          "speaker_id": "2",
          "time": "20210906 23:14"
        },
        {
          "id": "MDRW2100042590.1.33",
          "form": "지금 분위기로는 ㅋㅋㅋㅋ",
          "original_form": "지금 분위기로는 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100042590.1.34",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100042590.1.35",
          "form": "이래서 인디겜 못믿는것 맞는데",
          "original_form": "이래서 인디겜 못믿는것 맞는데",
          "speaker_id": "1",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100042590.1.36",
          "form": "1억씩이나 투자받고도",
          "original_form": "1억씩이나 투자받고도",
          "speaker_id": "1",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100042590.1.37",
          "form": "인디겜이 인ㄷ겜할줄이야",
          "original_form": "인디겜이 인ㄷ겜할줄이야",
          "speaker_id": "1",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100042590.1.38",
          "form": "그니까 ㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:16"
        },
        {
          "id": "MDRW2100042590.1.39",
          "form": "심지어 출시도 한참 미뤘으면서....",
          "original_form": "심지어 출시도 한참 미뤘으면서....",
