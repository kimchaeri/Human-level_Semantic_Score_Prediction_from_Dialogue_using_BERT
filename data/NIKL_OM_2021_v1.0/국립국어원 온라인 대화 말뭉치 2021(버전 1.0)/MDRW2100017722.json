      },
      "utterance": [
        {
          "id": "MDRW2100017722.1.1",
          "form": "항상 힘들지. 퇴근도 늦고.",
          "original_form": "항상 힘들지. 퇴근도 늦고.",
          "speaker_id": "2",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100017722.1.2",
          "form": "과장님은 결국 오늘도 안 나옴??",
          "original_form": "과장님은 결국 오늘도 안 나옴??",
          "speaker_id": "1",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100017722.1.3",
          "form": "응. 다른 데로 이직했대.",
          "original_form": "응. 다른 데로 이직했대.",
          "speaker_id": "2",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100017722.1.4",
          "form": "요즘 그 회사 왜 그러냐",
          "original_form": "요즘 그 회사 왜 그러냐",
          "speaker_id": "1",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100017722.1.5",
          "form": "그 청소부 아주머니도 그렇고..",
          "original_form": "그 청소부 아주머니도 그렇고..",
          "speaker_id": "1",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100017722.1.6",
          "form": "그러게 말이야. 직원들이 다들 속 썩이네.",
          "original_form": "그러게 말이야. 직원들이 다들 속 썩이네.",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017722.1.7",
          "form": "그래서 과장 자리는 어떻게 하기로 했어?",
          "original_form": "그래서 과장 자리는 어떻게 하기로 했어?",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017722.1.8",
          "form": "일단 사람 구한다고 공고 올렸어.",
          "original_form": "일단 사람 구한다고 공고 올렸어.",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017722.1.9",
          "form": "내일 면접 보기로 하고.",
          "original_form": "내일 면접 보기로 하고.",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017722.1.10",
          "form": "지원자가 있나 보네~그나마 다행이다",
          "original_form": "지원자가 있나 보네~그나마 다행이다",
          "speaker_id": "1",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017722.1.11",
          "form": "직원이 너무 자주 바뀌니까 정신없어.",
          "original_form": "직원이 너무 자주 바뀌니까 정신없어.",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017722.1.12",
          "form": "내 일도 바쁜데 말야.",
          "original_form": "내 일도 바쁜데 말야.",
          "speaker_id": "2",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017722.1.13",
          "form": "왜? 말일이라서 처리할 일이 많아?",
          "original_form": "왜? 말일이라서 처리할 일이 많아?",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017722.1.14",
          "form": "응 세금계산도 해야되고 은행도 다녀와야 돼.",
          "original_form": "응 세금계산도 해야되고 은행도 다녀와야 돼.",
          "speaker_id": "2",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100017722.1.15",
          "form": "바빠.",
          "original_form": "바빠.",
          "speaker_id": "2",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100017722.1.16",
          "form": "아 은행 업무도 봐야 되는 구나ㅠ",
          "original_form": "아 은행 업무도 봐야 되는 구나ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017722.1.17",
          "form": "과장 얼른 채용해.. 업무 분담이라도 하게ㅠ",
          "original_form": "과장 얼른 채용해.. 업무 분담이라도 하게ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100017722.1.18",
          "form": "응. 내일 면접이닠가 바로 뽑을 거야.",
          "original_form": "응. 내일 면접이닠가 바로 뽑을 거야.",
          "speaker_id": "2",
          "time": "20210826 20:58"
        }
      ]
