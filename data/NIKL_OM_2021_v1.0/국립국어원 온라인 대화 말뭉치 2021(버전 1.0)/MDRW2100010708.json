      "utterance": [
        {
          "id": "MDRW2100010708.1.1",
          "form": "나는 아직도 학교를 다니고 있지",
          "original_form": "나는 아직도 학교를 다니고 있지",
          "speaker_id": "2",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010708.1.2",
          "form": "나도 학교가고싶다",
          "original_form": "나도 학교가고싶다",
          "speaker_id": "1",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010708.1.3",
          "form": "아직도 학교를 다닌다는게 스트레스...",
          "original_form": "아직도 학교를 다닌다는게 스트레스...",
          "speaker_id": "2",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010708.1.4",
          "form": "아아 나 담주부터 그거야",
          "original_form": "아아 나 담주부터 그거야",
          "speaker_id": "2",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010708.1.5",
          "form": "학교 가애됨",
          "original_form": "학교 가애됨",
          "speaker_id": "2",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010708.1.6",
          "form": "수강신청은 잘 햇는가",
          "original_form": "수강신청은 잘 햇는가",
          "speaker_id": "1",
          "time": "20210823 20:55"
        },
        {
          "id": "MDRW2100010708.1.7",
          "form": "나도 수업듣고싶다",
          "original_form": "나도 수업듣고싶다",
          "speaker_id": "1",
          "time": "20210823 20:56"
        },
        {
          "id": "MDRW2100010708.1.8",
          "form": "도강하고싶다",
          "original_form": "도강하고싶다",
          "speaker_id": "1",
          "time": "20210823 20:56"
        },
        {
          "id": "MDRW2100010708.1.9",
          "form": "개살구",
          "original_form": "개살구",
          "speaker_id": "2",
          "time": "20210823 20:56"
        },
        {
          "id": "MDRW2100010708.1.10",
          "form": "도강이라뇨",
          "original_form": "도강이라뇨",
          "speaker_id": "2",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010708.1.11",
          "form": "한번 해보세요",
          "original_form": "한번 해보세요",
          "speaker_id": "2",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010708.1.12",
          "form": "둑훈둑훈 도강생활",
          "original_form": "둑훈둑훈 도강생활",
          "speaker_id": "2",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010708.1.13",
          "form": "하버드에서 도강한사람",
          "original_form": "하버드에서 도강한사람",
          "speaker_id": "1",
          "time": "20210823 20:57"
        },
        {
