      "utterance": [
        {
          "id": "MDRW2100002995.1.1",
          "form": "이주제괜찬나요?",
          "original_form": "이주제괜찬나요?",
          "speaker_id": "1",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002995.1.2",
          "form": "네  편하신거하세요",
          "original_form": "네  편하신거하세요",
          "speaker_id": "2",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002995.1.3",
          "form": "저부터소개하죠",
          "original_form": "저부터소개하죠",
          "speaker_id": "1",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002995.1.4",
          "form": "대학생이지만 소상공인이구 사업자 등록증 있구 온라인판매한지  4년차네요",
          "original_form": "대학생이지만 소상공인이구 사업자 등록증 있구 온라인판매한지  4년차네요",
          "speaker_id": "1",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002995.1.5",
          "form": "어떤걸 판매하시나요",
          "original_form": "어떤걸 판매하시나요",
          "speaker_id": "2",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002995.1.6",
          "form": "농대나와서 농산물 식물구근팔다 접구",
          "original_form": "농대나와서 농산물 식물구근팔다 접구",
          "speaker_id": "1",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002995.1.7",
          "form": "동전 지폐 파네용",
          "original_form": "동전 지폐 파네용",
          "speaker_id": "1",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002995.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002995.1.9",
          "form": "본인은 뭣 하시나요",
          "original_form": "본인은 뭣 하시나요",
          "speaker_id": "1",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002995.1.10",
          "form": "농산물 어떤거 파셨어요?",
          "original_form": "농산물 어떤거 파셨어요?",
          "speaker_id": "2",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002995.1.11",
          "form": "저는 지금 잠시 쉬고있어요",
          "original_form": "저는 지금 잠시 쉬고있어요",
          "speaker_id": "2",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002995.1.12",
          "form": "토마토 같은거 중개 도소매",
          "original_form": "토마토 같은거 중개 도소매",
          "speaker_id": "1",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002995.1.13",
          "form": "도소매의 어려운 점은 없었나요?",
          "original_form": "도소매의 어려운 점은 없었나요?",
          "speaker_id": "2",
          "time": "20210820 17:27"
        },
        {
          "id": "MDRW2100002995.1.14",
          "form": "크게 어려움 없고 단지",
          "original_form": "크게 어려움 없고 단지",
          "speaker_id": "1",
          "time": "20210820 17:27"
        },
        {
          "id": "MDRW2100002995.1.15",
          "form": "택배보내는과정 손해인지 이익인지만 판단하면",
          "original_form": "택배보내는과정 손해인지 이익인지만 판단하면",
          "speaker_id": "1",
          "time": "20210820 17:27"
        },
        {
          "id": "MDRW2100002995.1.16",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "2",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100002995.1.17",
          "form": "농산물이면 손실이 많았겠어요",
          "original_form": "농산물이면 손실이 많았겠어요",
          "speaker_id": "2",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100002995.1.18",
          "form": "동전 지폐가 구하귀쉽고 취급 편해서 좋아요",
          "original_form": "동전 지폐가 구하귀쉽고 취급 편해서 좋아요",
          "speaker_id": "1",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100002995.1.19",
          "form": "보통 이동중에 잘 상하니까",
          "original_form": "보통 이동중에 잘 상하니까",
          "speaker_id": "2",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100002995.1.20",
          "form": "아뇽 ㅣ달 100  만벌었네요",
          "original_form": "아뇽 ㅣ달 100  만벌었네요",
          "speaker_id": "1",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100002995.1.21",
          "form": "농장주에게 밴드올려도되는지허락받구",
          "original_form": "농장주에게 밴드올려도되는지허락받구",
          "speaker_id": "1",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100002995.1.22",
          "form": "그정도면 괜찮은 편인가요?",
          "original_form": "그정도면 괜찮은 편인가요?",
          "speaker_id": "2",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100002995.1.23",
          "form": "벌이 묻는거에요",
          "original_form": "벌이 묻는거에요",
          "speaker_id": "2",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002995.1.24",
          "form": "저작권있으니  가격 마진생각해서올리면  농장에주소등주면거기서바로 보내니",
          "original_form": "저작권있으니  가격 마진생각해서올리면  농장에주소등주면거기서바로 보내니",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002995.1.25",
          "form": "손해 귀찬은건 없죠 ㅋ",
          "original_form": "손해 귀찬은건 없죠 ㅋ",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002995.1.26",
          "form": "아 저작권이 있구나 새로운 사실을 알아가네요",
          "original_form": "아 저작권이 있구나 새로운 사실을 알아가네요",
          "speaker_id": "2",
          "time": "20210820 17:30"
        }
      ]
    }
  ]
