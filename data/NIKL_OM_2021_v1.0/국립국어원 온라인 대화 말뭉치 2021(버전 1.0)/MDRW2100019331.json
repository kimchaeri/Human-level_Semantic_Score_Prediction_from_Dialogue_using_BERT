      "utterance": [
        {
          "id": "MDRW2100019331.1.1",
          "form": "앗 공연",
          "original_form": "앗 공연",
          "speaker_id": "1",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100019331.1.2",
          "form": "요즘 공연이며 전시회며 많이 줄어서 너무 속상하지않나요ㅜㅜ",
          "original_form": "요즘 공연이며 전시회며 많이 줄어서 너무 속상하지않나요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100019331.1.3",
          "form": "맞아요 ㅠㅠㅠ",
          "original_form": "맞아요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 19:54"
        },
        {
          "id": "MDRW2100019331.1.4",
          "form": "표 사도 다 취소되고 그러더라구요",
          "original_form": "표 사도 다 취소되고 그러더라구요",
          "speaker_id": "2",
          "time": "20210827 19:55"
        },
        {
          "id": "MDRW2100019331.1.5",
          "form": "그니까요... 좀 속상해요ㅜㅜ",
          "original_form": "그니까요... 좀 속상해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019331.1.6",
          "form": "저 원래 일주일에 두번 전시회 다니던 사람인뎈ㅋㅋ...",
          "original_form": "저 원래 일주일에 두번 전시회 다니던 사람인뎈ㅋㅋ...",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019331.1.7",
          "form": "무슨 전시회 다니셨어요??",
          "original_form": "무슨 전시회 다니셨어요??",
          "speaker_id": "2",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019331.1.8",
          "form": "저는 그림 위주로 많이 다녔어요!",
          "original_form": "저는 그림 위주로 많이 다녔어요!",
          "speaker_id": "1",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100019331.1.9",
          "form": "그림보면서 멍때리는걸 좋아해서 ㅋㅋㅋㅋ",
          "original_form": "그림보면서 멍때리는걸 좋아해서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100019331.1.10",
          "form": "저는 음악회 자주 다녔는데",
          "original_form": "저는 음악회 자주 다녔는데",
          "speaker_id": "2",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019331.1.11",
          "form": "코로나 때문에 ㅠㅠㅠ",
          "original_form": "코로나 때문에 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019331.1.12",
          "form": "아이고ㅜㅜㅜ 음악회는 더더욱 안열죠 참...",
          "original_form": "아이고ㅜㅜㅜ 음악회는 더더욱 안열죠 참...",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019331.1.13",
          "form": "얼른 이것저것 열었음 좋겠는디",
          "original_form": "얼른 이것저것 열었음 좋겠는디",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019331.1.14",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019331.1.15",
          "form": "뮤직컬도 좋아했는데",
          "original_form": "뮤직컬도 좋아했는데",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019331.1.16",
          "form": "안그래도 어려운 티켓팅이 더 어려워져서",
          "original_form": "안그래도 어려운 티켓팅이 더 어려워져서",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019331.1.17",
          "form": "포기하고 있어요 흑흑",
          "original_form": "포기하고 있어요 흑흑",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019331.1.18",
          "form": "앜.... 여전히 피켓팅이군요",
          "original_form": "앜.... 여전히 피켓팅이군요",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019331.1.19",
          "form": "뮤지컬은 열리나요 요새??",
          "original_form": "뮤지컬은 열리나요 요새??",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019331.1.20",
          "form": "열리기는 하는데 4단계라 잘 모르겠어요",
          "original_form": "열리기는 하는데 4단계라 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100019331.1.21",
          "form": "포기한지 오래라 ㅠㅠ",
          "original_form": "포기한지 오래라 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100019331.1.22",
          "form": "아이고ㅜㅜ 하김 저도 전시회 포기했으니까요...",
          "original_form": "아이고ㅜㅜ 하김 저도 전시회 포기했으니까요...",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019331.1.23",
          "form": "전시회는 예약 받는 그런것도 없이 아예 안하나요??",
          "original_form": "전시회는 예약 받는 그런것도 없이 아예 안하나요??",
          "speaker_id": "2",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100019331.1.24",
          "form": "예약 받아서 소규모로 진행하는 것 같던데 전 구냥 포기했어요...",
          "original_form": "예약 받아서 소규모로 진행하는 것 같던데 전 구냥 포기했어요...",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019331.1.25",
          "form": "아무때나 쏙 들어가서 보는 맛이 있었는디",
          "original_form": "아무때나 쏙 들어가서 보는 맛이 있었는디",
          "speaker_id": "1",
          "time": "20210827 21:20"
        }
      ]
    }
  ]
