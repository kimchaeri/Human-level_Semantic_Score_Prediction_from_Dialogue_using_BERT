      "utterance": [
        {
          "id": "MDRW2100008654.1.1",
          "form": "전 되도록 요즘 1만보이상 걸어요",
          "original_form": "전 되도록 요즘 1만보이상 걸어요",
          "speaker_id": "2",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100008654.1.2",
          "form": "2만보도 걷고요",
          "original_form": "2만보도 걷고요",
          "speaker_id": "2",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100008654.1.3",
          "form": "저도 그래야 하는데 다리가 안아프세요??ㅠㅠ저는 다리가 아프더라구요",
          "original_form": "저도 그래야 하는데 다리가 안아프세요??ㅠㅠ저는 다리가 아프더라구요",
          "speaker_id": "1",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100008654.1.4",
          "form": "다리아파요ㅜ그리고 위가무거워서 무릎도 아픈거같아요",
          "original_form": "다리아파요ㅜ그리고 위가무거워서 무릎도 아픈거같아요",
          "speaker_id": "2",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008654.1.5",
          "form": "상반신에 살이 많으니 하반신이 고생한다며 으헝ㅜ",
          "original_form": "상반신에 살이 많으니 하반신이 고생한다며 으헝ㅜ",
          "speaker_id": "2",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008654.1.6",
          "form": "아 저는 하비라ㅠㅜ자전거를 타야하나 생각중이에요",
          "original_form": "아 저는 하비라ㅠㅜ자전거를 타야하나 생각중이에요",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008654.1.7",
          "form": "전 그냥 전부비만",
          "original_form": "전 그냥 전부비만",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008654.1.8",
          "form": "ㅜㅜㅜ",
          "original_form": "ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008654.1.9",
          "form": "월욜부턴 진짜 도시락 싸다니게요",
          "original_form": "월욜부턴 진짜 도시락 싸다니게요",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008654.1.10",
          "form": "저도 그러려구요ㅠㅠ 사먹으면 살이 쪄서ㅎㅎ",
          "original_form": "저도 그러려구요ㅠㅠ 사먹으면 살이 쪄서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 17:52"
        },
        {
          "id": "MDRW2100008654.1.11",
          "form": "그리고 돈도나가고",
          "original_form": "그리고 돈도나가고",
          "speaker_id": "2",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008654.1.12",
          "form": "저는 현타도 오더라고요",
          "original_form": "저는 현타도 오더라고요",
          "speaker_id": "2",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008654.1.13",
          "form": "이돈을 이리 쓰는것이 아니었는데 ㅋㅋ",
          "original_form": "이돈을 이리 쓰는것이 아니었는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008654.1.14",
          "form": "맞아요 건강에 좋지두 않구..",
          "original_form": "맞아요 건강에 좋지두 않구..",
          "speaker_id": "1",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008654.1.15",
          "form": "단백질 쉐이크 사놓고 한번도 안먹어서",
          "original_form": "단백질 쉐이크 사놓고 한번도 안먹어서",
          "speaker_id": "2",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008654.1.16",
          "form": "낼부턴 하루 한번은 먹으려고요",
          "original_form": "낼부턴 하루 한번은 먹으려고요",
          "speaker_id": "2",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008654.1.17",
          "form": "아 저는 샐러드 이제 진짜 먹어야겠어요ㅎㅎ",
          "original_form": "아 저는 샐러드 이제 진짜 먹어야겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008654.1.18",
          "form": "샐러드 준비가 귀찮지 않나요",
          "original_form": "샐러드 준비가 귀찮지 않나요",
          "speaker_id": "2",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008654.1.19",
          "form": "저는 일단 쉐이크랑 달걀삶았어요 ㅋㅋ",
          "original_form": "저는 일단 쉐이크랑 달걀삶았어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008654.1.20",
          "form": "샐러드..그냥 사려구요ㅎㅎ 우유랑ㅋㅋ",
          "original_form": "샐러드..그냥 사려구요ㅎㅎ 우유랑ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008654.1.21",
          "form": "요새 배송 서비스 같은거도 있으니까요 ㅋㅋ",
