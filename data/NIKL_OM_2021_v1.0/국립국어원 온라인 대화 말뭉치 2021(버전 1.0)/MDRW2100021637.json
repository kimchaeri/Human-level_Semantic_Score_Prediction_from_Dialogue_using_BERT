      "utterance": [
        {
          "id": "MDRW2100021637.1.1",
          "form": "게임인 거 맘에 드셨나바요",
          "original_form": "게임인 거 맘에 드셨나바요",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021637.1.2",
          "form": "예스으",
          "original_form": "예스으",
          "speaker_id": "1",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021637.1.3",
          "form": "딱히 할거 업자나",
          "original_form": "딱히 할거 업자나",
          "speaker_id": "1",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021637.1.4",
          "form": "나의 쿠킹덤 노동기가 맘에 드시나바요",
          "original_form": "나의 쿠킹덤 노동기가 맘에 드시나바요",
          "speaker_id": "2",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021637.1.5",
          "form": "노동싫어하면서 게임에선 노동 열시미 하네여",
          "original_form": "노동싫어하면서 게임에선 노동 열시미 하네여",
          "speaker_id": "1",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100021637.1.6",
          "form": "내가 노동하지 않잖아 대리 노동 개꿀",
          "original_form": "내가 노동하지 않잖아 대리 노동 개꿀",
          "speaker_id": "2",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100021637.1.7",
          "form": "ㅋㅋㅋㅋ쿠킹덤만 해?",
          "original_form": "ㅋㅋㅋㅋ쿠킹덤만 해?",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100021637.1.8",
          "form": "그렇지 퍼드도 콜라보 기간에만 하고 다시 들어가지도 않아",
          "original_form": "그렇지 퍼드도 콜라보 기간에만 하고 다시 들어가지도 않아",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100021637.1.9",
          "form": "퍼드 아직도 함?",
          "original_form": "퍼드 아직도 함?",
          "speaker_id": "1",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100021637.1.10",
          "form": "콜라보 생각나서 잠깐 했었지",
          "original_form": "콜라보 생각나서 잠깐 했었지",
          "speaker_id": "2",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100021637.1.11",
          "form": "여기서 퍼드 하는 사람 만났는데",
          "original_form": "여기서 퍼드 하는 사람 만났는데",
          "speaker_id": "1",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100021637.1.12",
          "form": "와 한퍼드는 사람 없다던데 일퍼드신가",
          "original_form": "와 한퍼드는 사람 없다던데 일퍼드신가",
          "speaker_id": "2",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100021637.1.13",
          "form": "그건 모르겠네",
          "original_form": "그건 모르겠네",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021637.1.14",
          "form": "재밌게 하고 있댔음ㅋㅋㅋ",
          "original_form": "재밌게 하고 있댔음ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021637.1.15",
          "form": "재밌다면 오케이입니다",
          "original_form": "재밌다면 오케이입니다",
          "speaker_id": "2",
          "time": "20210828 21:25"
