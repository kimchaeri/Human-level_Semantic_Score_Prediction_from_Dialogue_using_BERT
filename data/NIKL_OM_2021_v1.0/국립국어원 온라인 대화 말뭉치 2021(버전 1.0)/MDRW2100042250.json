      "utterance": [
        {
          "id": "MDRW2100042250.1.1",
          "form": "친구있는데먼저갈거같아요 호주!",
          "original_form": "친구있는데먼저갈거같아요 호주!",
          "speaker_id": "2",
          "time": "20210906 21:01"
        },
        {
          "id": "MDRW2100042250.1.2",
          "form": "간다면ㅋㅋㅋ",
          "original_form": "간다면ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:01"
        },
        {
          "id": "MDRW2100042250.1.3",
          "form": "말씀드린데중에선 미국먼저가고싶오요!",
          "original_form": "말씀드린데중에선 미국먼저가고싶오요!",
          "speaker_id": "2",
          "time": "20210906 21:02"
        },
        {
          "id": "MDRW2100042250.1.4",
          "form": "유럽은 몽가 무섭더라구요",
          "original_form": "유럽은 몽가 무섭더라구요",
          "speaker_id": "2",
          "time": "20210906 21:02"
        },
        {
          "id": "MDRW2100042250.1.5",
          "form": "친구가 프랑스에있을때 인종차별을 많이당해서그런지ㅜㅜ",
          "original_form": "친구가 프랑스에있을때 인종차별을 많이당해서그런지ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 21:02"
        },
        {
          "id": "MDRW2100042250.1.6",
          "form": "맞아요ㅜㅠ저도 캐나다 한번 더 갔다가 패키지로 유럽갈까...싶고요ㅋㅋ",
          "original_form": "맞아요ㅜㅠ저도 캐나다 한번 더 갔다가 패키지로 유럽갈까...싶고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:04"
        },
        {
          "id": "MDRW2100042250.1.7",
          "form": "무서워요 집시랑 도둑들ㅋㅈㅋ",
          "original_form": "무서워요 집시랑 도둑들ㅋㅈㅋ",
          "speaker_id": "1",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100042250.1.8",
          "form": "맞아요ㄷㄷㄷ",
          "original_form": "맞아요ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042250.1.9",
          "form": "그냥막채가더라구요",
          "original_form": "그냥막채가더라구요",
          "speaker_id": "2",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042250.1.10",
          "form": "에펠탑 최고래요",
          "original_form": "에펠탑 최고래요",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042250.1.11",
          "form": "그리고 파리 냄새도 최고라고ㅋㅋ",
          "original_form": "그리고 파리 냄새도 최고라고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042250.1.12",
          "form": "으 맞아요",
          "original_form": "으 맞아요",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042250.1.13",
          "form": "불친절 냄새 도둑ㅜㅜ",
          "original_form": "불친절 냄새 도둑ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042250.1.14",
          "form": "근데도 참 멋져보이니 마성인가봐요",
          "original_form": "근데도 참 멋져보이니 마성인가봐요",
          "speaker_id": "1",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042250.1.15",
          "form": "에펠탑 센강보고 크로와상과 에스프레소 로망입니다",
          "original_form": "에펠탑 센강보고 크로와상과 에스프레소 로망입니다",
          "speaker_id": "1",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100042250.1.16",
          "form": "그건그래요ㅜㅜ 바게트~~",
          "original_form": "그건그래요ㅜㅜ 바게트~~",
          "speaker_id": "2",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042250.1.17",
          "form": "유럽은 커피가 그리 맛있다고!!",
          "original_form": "유럽은 커피가 그리 맛있다고!!",
          "speaker_id": "1",
          "time": "20210907 09:28"
        },
        {
          "id": "MDRW2100042250.1.18",
          "form": "맞아요 아아는 안판다는 정통의 맛ㅋㅋ",
          "original_form": "맞아요 아아는 안판다는 정통의 맛ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:30"
        },
        {
          "id": "MDRW2100042250.1.19",
          "form": "ㅋㅋ 근데 진짜 맛있다고 자부심가질만하대요",
          "original_form": "ㅋㅋ 근데 진짜 맛있다고 자부심가질만하대요",
          "speaker_id": "1",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100042250.1.20",
          "form": "쓰레기라고 아아ㅜㅡㅜ",
          "original_form": "쓰레기라고 아아ㅜㅡㅜ",
          "speaker_id": "1",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100042250.1.21",
          "form": "우왕 가보고싶어져요ㅜㅜ",
          "original_form": "우왕 가보고싶어져요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 09:33"
        },
        {
          "id": "MDRW2100042250.1.22",
          "form": "그러니까요 에스프레소에 각설탕 넣고 드링킹",
          "original_form": "그러니까요 에스프레소에 각설탕 넣고 드링킹",
          "speaker_id": "1",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042250.1.23",
          "form": "으악 그냥 스벅에서 그렇게 마시는것도 좋아하는데",
          "original_form": "으악 그냥 스벅에서 그렇게 마시는것도 좋아하는데",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042250.1.24",
          "form": "거기선얼마나맛있을까요ㅜㅜ",
          "original_form": "거기선얼마나맛있을까요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042250.1.25",
          "form": "맞아요 분위기부터 먹고들어갈거같아요",
          "original_form": "맞아요 분위기부터 먹고들어갈거같아요",
