      "utterance": [
        {
          "id": "MDRW2100045457.1.1",
          "form": "연애하세요?",
          "original_form": "연애하세요?",
          "speaker_id": "1",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045457.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045457.1.3",
          "form": "아 요즘은 비혼도 대세죠",
          "original_form": "아 요즘은 비혼도 대세죠",
          "speaker_id": "1",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045457.1.4",
          "form": "비혼이 대세라니",
          "original_form": "비혼이 대세라니",
          "speaker_id": "2",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045457.1.5",
          "form": "세상 말세네요",
          "original_form": "세상 말세네요",
          "speaker_id": "2",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045457.1.6",
          "form": "주변에도 연애결혼 안하는사람 많아요",
          "original_form": "주변에도 연애결혼 안하는사람 많아요",
          "speaker_id": "1",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045457.1.7",
          "form": "와.. 으....",
          "original_form": "와.. 으....",
          "speaker_id": "1",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045457.1.8",
          "form": "근대 맞아요",
          "original_form": "근대 맞아요",
          "speaker_id": "1",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045457.1.9",
          "form": "아니 세상이 말세인건 맞잖아요",
          "original_form": "아니 세상이 말세인건 맞잖아요",
          "speaker_id": "2",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045457.1.10",
          "form": "그래요.",
          "original_form": "그래요.",
          "speaker_id": "2",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045457.1.11",
          "form": "말세가되서 종의 본능이 위협받으면",
          "original_form": "말세가되서 종의 본능이 위협받으면",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045457.1.12",
          "form": "저도 3포자인가",
          "original_form": "저도 3포자인가",
          "speaker_id": "2",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045457.1.13",
          "form": "그래요",
          "original_form": "그래요",
          "speaker_id": "2",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045457.1.14",
          "form": "자손을 적게남기고",
          "original_form": "자손을 적게남기고",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045457.1.15",
          "form": "연애출산이 적어져요",
          "original_form": "연애출산이 적어져요",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045457.1.16",
          "form": "어쩔 수 없는 순리죠",
          "original_form": "어쩔 수 없는 순리죠",
          "speaker_id": "2",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045457.1.17",
          "form": "지금 2개인데 하난 뭐예요",
          "original_form": "지금 2개인데 하난 뭐예요",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045457.1.18",
          "form": "집?",
          "original_form": "집?",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045457.1.19",
          "form": "인생?",
          "original_form": "인생?",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045457.1.20",
          "form": "아 집은 포기 못하죠",
          "original_form": "아 집은 포기 못하죠",
          "speaker_id": "2",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045457.1.21",
          "form": "인생도 포기 못하죠",
          "original_form": "인생도 포기 못하죠",
          "speaker_id": "2",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045457.1.22",
          "form": "인생초기한거네요 그럼",
          "original_form": "인생초기한거네요 그럼",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045457.1.23",
          "form": "남자를 포기했어요",
          "original_form": "남자를 포기했어요",
          "speaker_id": "2",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045457.1.24",
          "form": "아 ㅋㅋ 늦음",
          "original_form": "아 ㅋㅋ 늦음",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045457.1.25",
          "form": "연애랑 중복 아니예요?",
          "original_form": "연애랑 중복 아니예요?",
          "speaker_id": "1",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045457.1.26",
          "form": "아니 이 사람이 ㅋㅋㅋㅋㅋ",
          "original_form": "아니 이 사람이 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
