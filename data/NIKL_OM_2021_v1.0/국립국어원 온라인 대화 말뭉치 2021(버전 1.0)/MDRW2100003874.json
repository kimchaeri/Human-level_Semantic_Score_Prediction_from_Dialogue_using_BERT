        },
        {
          "id": "MDRW2100003874.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210821 13:09"
        },
        {
          "id": "MDRW2100003874.1.3",
          "form": "저는 무교입니다.",
          "original_form": "저는 무교입니다.",
          "speaker_id": "2",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003874.1.4",
          "form": "종교 있으신가요?",
          "original_form": "종교 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003874.1.5",
          "form": "저도 없어서",
          "original_form": "저도 없어서",
          "speaker_id": "1",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003874.1.6",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "2",
          "time": "20210821 13:42"
        },
        {
          "id": "MDRW2100003874.1.7",
          "form": "절이랑 교회에 몇번 가봤어요",
          "original_form": "절이랑 교회에 몇번 가봤어요",
          "speaker_id": "2",
          "time": "20210821 13:42"
        },
        {
          "id": "MDRW2100003874.1.8",
          "form": "부모님은 절이요",
          "original_form": "부모님은 절이요",
          "speaker_id": "1",
          "time": "20210821 13:43"
        },
        {
          "id": "MDRW2100003874.1.9",
          "form": "이모는 기독교에요.",
          "original_form": "이모는 기독교에요.",
          "speaker_id": "2",
          "time": "20210821 13:44"
        },
        {
          "id": "MDRW2100003874.1.10",
          "form": "주말은 휴식을 하는 편이라",
          "original_form": "주말은 휴식을 하는 편이라",
          "speaker_id": "1",
          "time": "20210821 13:45"
        },
        {
          "id": "MDRW2100003874.1.11",
          "form": "주일예배 힘들죠",
          "original_form": "주일예배 힘들죠",
          "speaker_id": "2",
          "time": "20210821 13:49"
        },
        {
          "id": "MDRW2100003874.1.12",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210821 13:51"
        },
        {
          "id": "MDRW2100003874.1.13",
          "form": "석가탄신일에 쉬어서 좋아요",
          "original_form": "석가탄신일에 쉬어서 좋아요",
          "speaker_id": "2",
          "time": "20210821 14:06"
        },
        {
          "id": "MDRW2100003874.1.14",
          "form": "크리스마스도 좋구요",
          "original_form": "크리스마스도 좋구요",
          "speaker_id": "2",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100003874.1.15",
          "form": "그건 좋죠",
          "original_form": "그건 좋죠",
          "speaker_id": "1",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100003874.1.16",
          "form": "뉴스보니 이슬람교는 무서워요",
          "original_form": "뉴스보니 이슬람교는 무서워요",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100003874.1.17",
