      "utterance": [
        {
          "id": "MDRW2100042981.1.1",
          "form": "비가 계속 오네요 ㅠㅠ",
          "original_form": "비가 계속 오네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 12:27"
        },
        {
          "id": "MDRW2100042981.1.2",
          "form": "그러게요 ㅠㅠ 언제쯤 그칠지..ㅠㅠ",
          "original_form": "그러게요 ㅠㅠ 언제쯤 그칠지..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100042981.1.3",
          "form": "낼은 그쳤으면 좋겠어요",
          "original_form": "낼은 그쳤으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100042981.1.4",
          "form": "네네 요몇일은 좋았는데요 ㅠ",
          "original_form": "네네 요몇일은 좋았는데요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100042981.1.5",
          "form": "맞아요 완전 초가을 날씨였는데 아쉬웡ㅅ",
          "original_form": "맞아요 완전 초가을 날씨였는데 아쉬웡ㅅ",
          "speaker_id": "2",
          "time": "20210907 15:33"
        },
        {
          "id": "MDRW2100042981.1.6",
          "form": "비가 그치면 산책 하고싶네요!!",
          "original_form": "비가 그치면 산책 하고싶네요!!",
          "speaker_id": "1",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100042981.1.7",
          "form": "비 그치셨나용?",
          "original_form": "비 그치셨나용?",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100042981.1.8",
          "form": "지금 살짝 내리는거 같아요 !!",
          "original_form": "지금 살짝 내리는거 같아요 !!",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100042981.1.9",
          "form": "비오고 있어용?",
          "original_form": "비오고 있어용?",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100042981.1.10",
          "form": "네 ..! 여긴 아직 비오고있엉ㅅ!!",
          "original_form": "네 ..! 여긴 아직 비오고있엉ㅅ!!",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100042981.1.11",
          "form": "내일도 올거같지 않아요??",
          "original_form": "내일도 올거같지 않아요??",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100042981.1.12",
          "form": "제발 멈췄으묜 졸겠어요 ㅠㅠ",
          "original_form": "제발 멈췄으묜 졸겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100042981.1.13",
          "form": "내일은 출근해여라는뎅",
          "original_form": "내일은 출근해여라는뎅",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100042981.1.14",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100042981.1.15",
          "form": "맞아요 ㅠㅠ 출근길에 비오면 진짜 힘들어요 ㅋㅋ",
          "original_form": "맞아요 ㅠㅠ 출근길에 비오면 진짜 힘들어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100042981.1.16",
          "form": "내일 제발 비오지 않게ㅠ해주세요#!~!~!",
          "original_form": "내일 제발 비오지 않게ㅠ해주세요#!~!~!",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100042981.1.17",
          "form": "내일 외출 계획 있으세요?",
          "original_form": "내일 외출 계획 있으세요?",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100042981.1.18",
          "form": "다행히 내일은 쉬는날이에요 ㅋㅋ",
          "original_form": "다행히 내일은 쉬는날이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100042981.1.19",
          "form": "와 너무 부러유ㅓ요 ㅠㅠㅠ",
          "original_form": "와 너무 부러유ㅓ요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100042981.1.20",
          "form": "저도 제발 내일 쉬게해주세요 ㅠ ㅠ",
          "original_form": "저도 제발 내일 쉬게해주세요 ㅠ ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100042981.1.21",
          "form": "비까지 오면 정말 너무 가기싫을거같어요",
