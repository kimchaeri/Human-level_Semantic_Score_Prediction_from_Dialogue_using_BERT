        {
          "id": "MDRW2100020524.1.1",
          "form": "게임해요?",
          "original_form": "게임해요?",
          "speaker_id": "1",
          "time": "20210828 08:57"
        },
        {
          "id": "MDRW2100020524.1.2",
          "form": "아니요ㅜㅠ 게임좋아하시나요?",
          "original_form": "아니요ㅜㅠ 게임좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 09:00"
        },
        {
          "id": "MDRW2100020524.1.3",
          "form": "폰겜이요",
          "original_form": "폰겜이요",
          "speaker_id": "1",
          "time": "20210828 09:11"
        },
        {
          "id": "MDRW2100020524.1.4",
          "form": "폰게임ㅋㅋ 예전에 애니팡 수준정도만... ㅋㅋ 최근에 쿠키도 쫌하긴했어요",
          "original_form": "폰게임ㅋㅋ 예전에 애니팡 수준정도만... ㅋㅋ 최근에 쿠키도 쫌하긴했어요",
          "speaker_id": "2",
          "time": "20210828 09:13"
        },
        {
          "id": "MDRW2100020524.1.5",
          "form": "브롤이요",
          "original_form": "브롤이요",
          "speaker_id": "1",
          "time": "20210828 09:13"
        },
        {
          "id": "MDRW2100020524.1.6",
          "form": "브롤이요?ㅜㅜ 게임 문외한입니다ㅠ",
          "original_form": "브롤이요?ㅜㅜ 게임 문외한입니다ㅠ",
          "speaker_id": "2",
          "time": "20210828 09:13"
        },
        {
          "id": "MDRW2100020524.1.7",
          "form": "초딩겜이여",
          "original_form": "초딩겜이여",
          "speaker_id": "1",
          "time": "20210828 09:14"
        },
        {
          "id": "MDRW2100020524.1.8",
          "form": "초딩게임이요? 검색해볼게요",
          "original_form": "초딩게임이요? 검색해볼게요",
          "speaker_id": "2",
          "time": "20210828 09:15"
        },
        {
          "id": "MDRW2100020524.1.9",
          "form": "재밌어요?",
          "original_form": "재밌어요?",
          "speaker_id": "2",
          "time": "20210828 09:15"
        },
        {
          "id": "MDRW2100020524.1.10",
          "form": "얘때문에 해요",
          "original_form": "얘때문에 해요",
          "speaker_id": "1",
          "time": "20210828 09:19"
        },
        {
          "id": "MDRW2100020524.1.11",
          "form": "브롤스타즈?? 이 게임 맞아요?",
          "original_form": "브롤스타즈?? 이 게임 맞아요?",
          "speaker_id": "2",
