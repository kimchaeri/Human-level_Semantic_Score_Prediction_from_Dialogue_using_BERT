        },
        {
          "id": "MDRW2100020524.1.12",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210828 09:22"
        },
        {
          "id": "MDRW2100020524.1.13",
