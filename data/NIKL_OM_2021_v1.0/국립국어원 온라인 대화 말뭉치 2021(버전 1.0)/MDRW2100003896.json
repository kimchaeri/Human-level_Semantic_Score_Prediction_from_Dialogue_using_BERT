      "utterance": [
        {
          "id": "MDRW2100003896.1.1",
          "form": "보는 프로 있어요?",
          "original_form": "보는 프로 있어요?",
          "speaker_id": "1",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003896.1.2",
          "form": "전 ㅋㅋ펜트 하우스요",
          "original_form": "전 ㅋㅋ펜트 하우스요",
          "speaker_id": "2",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003896.1.3",
          "form": "놀면 뭐하니는 시간 놓쳐서 요새 못봤네요",
          "original_form": "놀면 뭐하니는 시간 놓쳐서 요새 못봤네요",
          "speaker_id": "2",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003896.1.4",
          "form": "뭐 보세요?",
          "original_form": "뭐 보세요?",
          "speaker_id": "2",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003896.1.5",
          "form": "런닝맨요",
          "original_form": "런닝맨요",
          "speaker_id": "1",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003896.1.6",
          "form": "오 저도 예전에 봤었어요",
          "original_form": "오 저도 예전에 봤었어요",
          "speaker_id": "2",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003896.1.7",
          "form": "가끔 재방봐요",
          "original_form": "가끔 재방봐요",
          "speaker_id": "2",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003896.1.8",
          "form": "예능 좋아 하시나봐요",
          "original_form": "예능 좋아 하시나봐요",
          "speaker_id": "2",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003896.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 13:26"
        },
        {
          "id": "MDRW2100003896.1.10",
          "form": "전 전참시 나혼산 봐요",
          "original_form": "전 전참시 나혼산 봐요",
          "speaker_id": "2",
          "time": "20210821 13:27"
        },
        {
          "id": "MDRW2100003896.1.11",
          "form": "아 것도 잼나요",
          "original_form": "아 것도 잼나요",
          "speaker_id": "1",
          "time": "20210821 13:28"
        },
        {
          "id": "MDRW2100003896.1.12",
          "form": "그죠 우세윤 넘 웃겼어요",
          "original_form": "그죠 우세윤 넘 웃겼어요",
          "speaker_id": "2",
          "time": "20210821 13:30"
        },
        {
          "id": "MDRW2100003896.1.13",
          "form": "기안은 좀 엉뚱하고 짠하고요",
          "original_form": "기안은 좀 엉뚱하고 짠하고요",
          "speaker_id": "2",
          "time": "20210821 13:30"
        },
        {
          "id": "MDRW2100003896.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 13:31"
        },
        {
          "id": "MDRW2100003896.1.15",
          "form": "드라마는 뭐보세요",
          "original_form": "드라마는 뭐보세요",
          "speaker_id": "2",
          "time": "20210821 13:34"
        },
        {
          "id": "MDRW2100003896.1.16",
          "form": "드라마는 별로 ㅋㅋ",
          "original_form": "드라마는 별로 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 13:34"
        },
        {
          "id": "MDRW2100003896.1.17",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "2",
          "time": "20210821 13:35"
        },
        {
          "id": "MDRW2100003896.1.18",
          "form": "노래는 뭐 좋아 하세요?",
          "original_form": "노래는 뭐 좋아 하세요?",
          "speaker_id": "2",
          "time": "20210821 13:35"
        },
        {
          "id": "MDRW2100003896.1.19",
          "form": "전 요새 sg 워너비 다시 좋아 졌어요",
          "original_form": "전 요새 sg 워너비 다시 좋아 졌어요",
          "speaker_id": "2",
          "time": "20210821 13:35"
        },
        {
