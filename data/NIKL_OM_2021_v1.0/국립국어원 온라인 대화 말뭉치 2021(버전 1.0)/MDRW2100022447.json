      "utterance": [
        {
          "id": "MDRW2100022447.1.1",
          "form": "그럼 키우는 동물은 없겠네요\r\n저는 물고기 키우고 있어요",
          "original_form": "그럼 키우는 동물은 없겠네요\r\n저는 물고기 키우고 있어요",
          "speaker_id": "1",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100022447.1.2",
          "form": "아뇨 있어요!",
          "original_form": "아뇨 있어요!",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100022447.1.3",
          "form": "고양이 키우고 있습니다 ㅎㅎ",
          "original_form": "고양이 키우고 있습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100022447.1.4",
          "form": "오호 고양이 어울리네요",
          "original_form": "오호 고양이 어울리네요",
          "speaker_id": "1",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100022447.1.5",
          "form": "고양이 한마리인가요?",
          "original_form": "고양이 한마리인가요?",
          "speaker_id": "1",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100022447.1.6",
          "form": "네 한마리 키우고있어용",
          "original_form": "네 한마리 키우고있어용",
          "speaker_id": "2",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100022447.1.7",
          "form": "귀엽겠네요",
          "original_form": "귀엽겠네요",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022447.1.8",
          "form": "전 예전에 강아지 키웠어요",
          "original_form": "전 예전에 강아지 키웠어요",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022447.1.9",
          "form": "강아지도 귀엽겠네요 ㅠㅠ",
          "original_form": "강아지도 귀엽겠네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022447.1.10",
          "form": "말티즈였는데 귀여웠죠",
          "original_form": "말티즈였는데 귀여웠죠",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022447.1.11",
          "form": "그런데 강아지는 손이 많이 가요",
          "original_form": "그런데 강아지는 손이 많이 가요",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022447.1.12",
          "form": "아기 키우는 거랑 비슷하죠",
          "original_form": "아기 키우는 거랑 비슷하죠",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022447.1.13",
          "form": "손이 많이 간다고 얘기들은 많이들 하시드라고용..",
          "original_form": "손이 많이 간다고 얘기들은 많이들 하시드라고용..",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022447.1.14",
          "form": "그래서 전 고양이를 키우고있죠! 좀 손이 상대적으로 덜가는지라",
          "original_form": "그래서 전 고양이를 키우고있죠! 좀 손이 상대적으로 덜가는지라",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022447.1.15",
          "form": "그렇게 들었어요",
          "original_form": "그렇게 들었어요",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022447.1.16",
          "form": "그런데 저는고양이보다 더 손이 안가는 물고기로 넘어왔답니다 ㅎㅎ",
          "original_form": "그런데 저는고양이보다 더 손이 안가는 물고기로 넘어왔답니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022447.1.17",
          "form": "아하 그렇네요 ㅋㅋㅋ",
          "original_form": "아하 그렇네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022447.1.18",
          "form": "처음엔 물고기를 무슨 재미로 키우나 했는데 몇년 키워보니 욘석들도 나름 귀엽더군요",
          "original_form": "처음엔 물고기를 무슨 재미로 키우나 했는데 몇년 키워보니 욘석들도 나름 귀엽더군요",
          "speaker_id": "1",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022447.1.19",
          "form": "밥 준다고 다가가면 막 앞으로 나와서 날라고 꼬리지느러미 흔들고 그래요",
          "original_form": "밥 준다고 다가가면 막 앞으로 나와서 날라고 꼬리지느러미 흔들고 그래요",
          "speaker_id": "1",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022447.1.20",
          "form": "오 귀엽네요 ㅋㅋㅋ",
          "original_form": "오 귀엽네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:24"
        },
        {
          "id": "MDRW2100022447.1.21",
          "form": "물론 헤엄치느라 흔드는 꼬리겠지만   볼땐 강아지 같아서 귀여워요",
          "original_form": "물론 헤엄치느라 흔드는 꼬리겠지만   볼땐 강아지 같아서 귀여워요",
          "speaker_id": "1",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100022447.1.22",
          "form": "그리고 조용해서 그것도 장점이죠",
          "original_form": "그리고 조용해서 그것도 장점이죠",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022447.1.23",
          "form": "고양이도 조용한 편이죠?",
          "original_form": "고양이도 조용한 편이죠?",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022447.1.24",
          "form": "가끔 놀아달라고 하거나 간식달라고 할때빼곤",
          "original_form": "가끔 놀아달라고 하거나 간식달라고 할때빼곤",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022447.1.25",
          "form": "조용하죵",
          "original_form": "조용하죵",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022447.1.26",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210829 15:51"
        }
      ]
    }
  ]
