      "utterance": [
        {
          "id": "MDRW2100009423.1.1",
          "form": "주말이 다 지나갔습니다....  월요일입니다 이제 곧....",
          "original_form": "주말이 다 지나갔습니다....  월요일입니다 이제 곧....",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009423.1.2",
          "form": "월요일 너무 사랑합니다ㅋㅋㅋㅋㅋ",
          "original_form": "월요일 너무 사랑합니다ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009423.1.3",
          "form": "한 명이라도 출근하면",
          "original_form": "한 명이라도 출근하면",
          "speaker_id": "2",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009423.1.4",
          "form": "제 일은 좀 줄어드는거 같아요ㅋㅋ",
          "original_form": "제 일은 좀 줄어드는거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009423.1.5",
          "form": "ㅋㅋㅋㅋㅋ 전 아닙니다",
          "original_form": "ㅋㅋㅋㅋㅋ 전 아닙니다",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009423.1.6",
          "form": "너무 싫어요 월요일",
          "original_form": "너무 싫어요 월요일",
          "speaker_id": "1",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009423.1.7",
          "form": "전업주부신가요?",
          "original_form": "전업주부신가요?",
          "speaker_id": "1",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009423.1.8",
          "form": "네:) 지금은 전업주부에요",
          "original_form": "네:) 지금은 전업주부에요",
          "speaker_id": "2",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009423.1.9",
          "form": "집에서 놀면서\r\n이 여섯글자를 제일 싫어하는",
          "original_form": "집에서 놀면서\r\n이 여섯글자를 제일 싫어하는",
          "speaker_id": "2",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009423.1.10",
          "form": "전업주부입니다ㅋㅋㅋㅋㅈ",
          "original_form": "전업주부입니다ㅋㅋㅋㅋㅈ",
          "speaker_id": "2",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009423.1.11",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009423.1.12",
          "form": "재택근무하는 남편은요?",
          "original_form": "재택근무하는 남편은요?",
          "speaker_id": "1",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009423.1.13",
          "form": "재택근무없는 회사에\r\n큰 절 올리려구요ㅋㅋㅋㅋ",
          "original_form": "재택근무없는 회사에\r\n큰 절 올리려구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009423.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ 그렇게 싫으세요?ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그렇게 싫으세요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009423.1.15",
          "form": "삼시세.끼 고민하는게 너무 괴로워요ㅠㅠ",
          "original_form": "삼시세.끼 고민하는게 너무 괴로워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009423.1.16",
          "form": "그렇겠네요...ㅜㅜ",
          "original_form": "그렇겠네요...ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100009423.1.17",
          "form": "집안일에 육아까지 너무 힘드시겠네요",
          "original_form": "집안일에 육아까지 너무 힘드시겠네요",
          "speaker_id": "1",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100009423.1.18",
          "form": "아직 아이들이 어리니까",
          "original_form": "아직 아이들이 어리니까",
          "speaker_id": "2",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009423.1.19",
          "form": "반찬을 하더라도\r\n아이들용 어른용을",
          "original_form": "반찬을 하더라도\r\n아이들용 어른용을",
          "speaker_id": "2",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009423.1.20",
          "form": "따로 해야되다보니ㅠㅠ",
          "original_form": "따로 해야되다보니ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009423.1.21",
          "form": "주말에는 부엌에서 사는거 같아요ㅋ",
          "original_form": "주말에는 부엌에서 사는거 같아요ㅋ",
          "speaker_id": "2",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009423.1.22",
          "form": "그와중에 이걸 하시는거에요?",
          "original_form": "그와중에 이걸 하시는거에요?",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009423.1.23",
          "form": "지금은 아이들 자는 시간이고",
          "original_form": "지금은 아이들 자는 시간이고",
          "speaker_id": "2",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009423.1.24",
          "form": "간간히 짬날때ㅋㅋ",
          "original_form": "간간히 짬날때ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009423.1.25",
          "form": "쉬는시간에 하시는군요 ㅋㅋ",
          "original_form": "쉬는시간에 하시는군요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009423.1.26",
          "form": "엄마는 참 대단한거 같아요",
          "original_form": "엄마는 참 대단한거 같아요",
          "speaker_id": "1",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009423.1.27",
          "form": "그럼요! 엄마는 위대합니다ㅋㅋㅋ",
          "original_form": "그럼요! 엄마는 위대합니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009423.1.28",
          "form": "안힘드세요? 일욜인데",
          "original_form": "안힘드세요? 일욜인데",
          "speaker_id": "1",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009423.1.29",
          "form": "쉬시지도 못하고",
          "original_form": "쉬시지도 못하고",
          "speaker_id": "1",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009423.1.30",
          "form": "그래도 주말에는 \r\n낮잠타임을 줘서\r\n오늘도 푹 잤어요ㅋㅋ",
          "original_form": "그래도 주말에는 \r\n낮잠타임을 줘서\r\n오늘도 푹 잤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009423.1.31",
          "form": "name1님은 결혼하심\r\n집안일 같이 하실건가요?",
          "original_form": "&name1&님은 결혼하심\r\n집안일 같이 하실건가요?",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009423.1.32",
          "form": "최대한 해주려고 하는데",
          "original_form": "최대한 해주려고 하는데",
          "speaker_id": "1",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009423.1.33",
          "form": "그게 그렇게 잘 될런지....",
          "original_form": "그게 그렇게 잘 될런지....",
          "speaker_id": "1",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009423.1.34",
          "form": "남편분이 많이 도와주세요?",
          "original_form": "남편분이 많이 도와주세요?",
          "speaker_id": "1",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009423.1.35",
          "form": "네:) 많이 도와주고\r\n잔소리도 그만큼 하는 스탈입니다ㅋㅋ",
          "original_form": "네:) 많이 도와주고\r\n잔소리도 그만큼 하는 스탈입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009423.1.36",
          "form": "ㅋㅋㅋㅋㅋ 제여친이 제일 싫어하는게 제 잔소리인데",
          "original_form": "ㅋㅋㅋㅋㅋ 제여친이 제일 싫어하는게 제 잔소리인데",
          "speaker_id": "1",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009423.1.37",
          "form": "저도 그럴듯...",
          "original_form": "저도 그럴듯...",
          "speaker_id": "1",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009423.1.38",
          "form": "헐 여자친구분 늦지 않았어요",
          "original_form": "헐 여자친구분 늦지 않았어요",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009423.1.39",
          "form": "어서 돔황챠!!!",
          "original_form": "어서 돔황챠!!!",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009423.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009423.1.41",
          "form": "이미 늦었죠",
          "original_form": "이미 늦었죠",
          "speaker_id": "1",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009423.1.42",
          "form": "애 둘 키우는거 친구들보니 진짜 만만치 않던데",
          "original_form": "애 둘 키우는거 친구들보니 진짜 만만치 않던데",
          "speaker_id": "1",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009423.1.43",
          "form": "아직 늦지 않았죠ㅋㅋ",
          "original_form": "아직 늦지 않았죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009423.1.44",
          "form": "대단하십니다",
          "original_form": "대단하십니다",
          "speaker_id": "1",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009423.1.45",
          "form": "이와중에 셋째 타령해서\r\n진짜 .... 하아...",
          "original_form": "이와중에 셋째 타령해서\r\n진짜 .... 하아...",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009423.1.46",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009423.1.47",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009423.1.48",
          "form": "애국자",
          "original_form": "애국자",
          "speaker_id": "1",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009423.1.49",
          "form": "것도 셋째도 아들이면 좋겠다고 해서",
          "original_form": "것도 셋째도 아들이면 좋겠다고 해서",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009423.1.50",
          "form": "임신도 출산도 육아도\r\n다 본인이 할 거 아님 \r\n그 입 다물라고 했어요",
          "original_form": "임신도 출산도 육아도\r\n다 본인이 할 거 아님 \r\n그 입 다물라고 했어요",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009423.1.51",
          "form": "그건... 맘대로 안되죠...",
          "original_form": "그건... 맘대로 안되죠...",
          "speaker_id": "1",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009423.1.52",
          "form": "ㅋㅋㅋㅋ 지금 아이들 다 남자아이인가요?",
          "original_form": "ㅋㅋㅋㅋ 지금 아이들 다 남자아이인가요?",
          "speaker_id": "1",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009423.1.53",
          "form": "네ㅋㅋ 둘 다 아들입니다",
          "original_form": "네ㅋㅋ 둘 다 아들입니다",
          "speaker_id": "2",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009423.1.54",
          "form": "힘드시겠군여",
          "original_form": "힘드시겠군여",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009423.1.55",
          "form": "아들을 키우다보면 입이 거칠어진대요",
          "original_form": "아들을 키우다보면 입이 거칠어진대요",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009423.1.56",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009423.1.57",
          "form": "그 말에 공감합니다 ㅋㅋㅋㅋ",
          "original_form": "그 말에 공감합니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009423.1.58",
          "form": "저 되게 여자여자 하다는말\r\n많이 듣고 살았는데요",
          "original_form": "저 되게 여자여자 하다는말\r\n많이 듣고 살았는데요",
          "speaker_id": "2",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009423.1.59",
          "form": "그런 제 안에 헐크가 있더라구요",
          "original_form": "그런 제 안에 헐크가 있더라구요",
