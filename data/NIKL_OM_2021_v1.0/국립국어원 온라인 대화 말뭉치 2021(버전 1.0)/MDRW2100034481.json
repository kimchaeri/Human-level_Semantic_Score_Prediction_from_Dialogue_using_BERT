        },
        {
          "id": "MDRW2100034481.1.2",
          "form": "식사하셨어요??",
          "original_form": "식사하셨어요??",
          "speaker_id": "1",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100034481.1.3",
          "form": "네 name1님은요?",
          "original_form": "네 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100034481.1.4",
          "form": "방금했어요!",
          "original_form": "방금했어요!",
          "speaker_id": "1",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100034481.1.5",
          "form": "뭐드셨나요",
          "original_form": "뭐드셨나요",
          "speaker_id": "1",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100034481.1.6",
          "form": "떡만둣국 먹엇어요 ㅋㅋ",
          "original_form": "떡만둣국 먹엇어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100034481.1.7",
          "form": "name1님은 뭐 만드셧어요?",
          "original_form": "&name1&님은 뭐 만드셧어요?",
          "speaker_id": "2",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100034481.1.8",
          "form": "오늘 만두데인가요",
          "original_form": "오늘 만두데인가요",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100034481.1.9",
          "form": "제친구도 만두먹었다더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "제친구도 만두먹었다더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100034481.1.10",
          "form": "저는 닭갈비요",
          "original_form": "저는 닭갈비요",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100034481.1.11",
          "form": "만두가 맛잇고 만만해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "만두가 맛잇고 만만해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034481.1.12",
          "form": "와 닭갈비",
          "original_form": "와 닭갈비",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034481.1.13",
          "form": "맛잇겟다ㅠㅠ",
          "original_form": "맛잇겟다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034481.1.14",
          "form": "만만한 만두 ㅋㅋㅋㅋㅋ",
          "original_form": "만만한 만두 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100034481.1.15",
          "form": "어제먹다 남은건데도 맛나요 ㅠㅠ",
          "original_form": "어제먹다 남은건데도 맛나요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100034481.1.16",
          "form": "밥 볶아드셧어요?",
          "original_form": "밥 볶아드셧어요?",
          "speaker_id": "2",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100034481.1.17",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100034481.1.18",
          "form": "한국인은 밥심..!!",
          "original_form": "한국인은 밥심..!!",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100034481.1.19",
          "form": "맛잇겟다 ㅋㅋㅋㅋ",
          "original_form": "맛잇겟다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100034481.1.20",
          "form": "점심 먹엇는데도 침흘리고 잇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "점심 먹엇는데도 침흘리고 잇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100034481.1.21",
          "form": "앜ㅋㅋ",
          "original_form": "앜ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100034481.1.22",
          "form": "음식은 먹어도먹어도 안 질리더라구여 저는ㅋㅋㅋ",
          "original_form": "음식은 먹어도먹어도 안 질리더라구여 저는ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100034481.1.23",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100034481.1.24",
          "form": "점심먹으면서 저녁 메뉴 얘기도 가능 ㅋㅋㅋㅋㅋ",
          "original_form": "점심먹으면서 저녁 메뉴 얘기도 가능 ㅋㅋㅋㅋㅋ",
