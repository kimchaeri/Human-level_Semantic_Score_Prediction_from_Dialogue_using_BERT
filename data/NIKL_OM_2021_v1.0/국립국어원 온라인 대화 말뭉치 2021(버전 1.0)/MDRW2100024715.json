        },
        {
          "id": "MDRW2100024715.1.2",
          "form": "방금 분석글 봤는데",
          "original_form": "방금 분석글 봤는데",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024715.1.3",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024715.1.4",
          "form": "얘네가 원래 5성부터 공개를 하거든?",
          "original_form": "얘네가 원래 5성부터 공개를 하거든?",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024715.1.5",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024715.1.6",
          "form": "지난번에 2.0 업뎃 예정돼있을때는",
          "original_form": "지난번에 2.0 업뎃 예정돼있을때는",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024715.1.7",
          "form": "라이덴 요이미야 먼저 공개하고",
          "original_form": "라이덴 요이미야 먼저 공개하고",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024715.1.8",
          "form": "그담에 사유였단말야?",
          "original_form": "그담에 사유였단말야?",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024715.1.9",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024715.1.10",
          "form": "근데 이번에 2.1 확정이고 2.2 가챠 캐릭터가 미정인 상태에서",
          "original_form": "근데 이번에 2.1 확정이고 2.2 가챠 캐릭터가 미정인 상태에서",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024715.1.11",
          "form": "헐 복각이 아니라고?",
          "original_form": "헐 복각이 아니라고?",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024715.1.12",
          "form": "4성 토마만 올라왔잔ㄹ아?",
          "original_form": "4성 토마만 올라왔잔ㄹ아?",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024715.1.13",
          "form": "ㄴㄴ",
          "original_form": "ㄴㄴ",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024715.1.14",
          "form": "그럼?",
          "original_form": "그럼?",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024715.1.15",
          "form": "이대로 다음 캐가 4성 고로면",
          "original_form": "이대로 다음 캐가 4성 고로면",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024715.1.16",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024715.1.17",
          "form": "2.2는 통으로 복각",
          "original_form": "2.2는 통으로 복각",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024715.1.18",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024715.1.19",
          "form": "5성 둘 복각할건데",
          "original_form": "5성 둘 복각할건데",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024715.1.20",
          "form": "그게 과연 누구일지....",
          "original_form": "그게 과연 누구일지....",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024715.1.21",
          "form": "제발소",
          "original_form": "제발소",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024715.1.22",
          "form": "유력한건 감우랑 알베도이긴 해...",
          "original_form": "유력한건 감우랑 알베도이긴 해...",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024715.1.23",
          "form": "제발제발제발",
          "original_form": "제발제발제발",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024715.1.24",
          "form": "ㅇㅏ 둘다 싫어....",
          "original_form": "ㅇㅏ 둘다 싫어....",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024715.1.25",
          "form": "아니 알베도는 심지어",
          "original_form": "아니 알베도는 심지어",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024715.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024715.1.27",
          "form": "써 뵈지도 못했어",
          "original_form": "써 뵈지도 못했어",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024715.1.28",
          "form": "걔 얼음이야?",
          "original_form": "걔 얼음이야?",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024715.1.29",
          "form": "걔 무슨 원손지도 몰라",
          "original_form": "걔 무슨 원손지도 몰라",
          "speaker_id": "2",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024715.1.30",
          "form": "바위 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "바위 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100024715.1.31",
          "form": "나름 나쁘진 않대 ㅋㅋㅋㅋ",
          "original_form": "나름 나쁘진 않대 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100024715.1.32",
          "form": "알베도??",
          "original_form": "알베도??",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100024715.1.33",
          "form": "바위야???",
          "original_form": "바위야???",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100024715.1.34",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100024715.1.35",
          "form": "너무 의외다......",
          "original_form": "너무 의외다......",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100024715.1.36",
          "form": "아 그래서",
          "original_form": "아 그래서",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100024715.1.37",
          "form": "4바위 조합에",
          "original_form": "4바위 조합에",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100024715.1.38",
          "form": "알베도 끼워도",
          "original_form": "알베도 끼워도",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100024715.1.39",
          "form": "된다고 써있었구나",
          "original_form": "된다고 써있었구나",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100024715.1.40",
          "form": "멍청짓이네",
          "original_form": "멍청짓이네",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100024715.1.41",
          "form": "나 그리고 이제",
          "original_form": "나 그리고 이제",
          "speaker_id": "1",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024715.1.42",
          "form": "아포칼립스에서 주인공이 구르기만 하다가",
          "original_form": "아포칼립스에서 주인공이 구르기만 하다가",
          "speaker_id": "1",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024715.1.43",
          "form": "다음시즌을 기다려주세요!",
          "original_form": "다음시즌을 기다려주세요!",
          "speaker_id": "1",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024715.1.44",
          "form": "ㅎㅏ",
          "original_form": "ㅎㅏ",
          "speaker_id": "2",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024715.1.45",
          "form": "하는 내용 그만보고싶어...ㅎ....",
          "original_form": "하는 내용 그만보고싶어...ㅎ....",
          "speaker_id": "1",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024715.1.46",
          "form": "듣기만 해도",
          "original_form": "듣기만 해도",
          "speaker_id": "2",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024715.1.47",
          "form": "갑갑하다",
          "original_form": "갑갑하다",
          "speaker_id": "2",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024715.1.48",
          "form": "중겜들은 다 그런가?",
          "original_form": "중겜들은 다 그런가?",
          "speaker_id": "2",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024715.1.49",
          "form": "왜 그런걸 좋아하지....?",
          "original_form": "왜 그런걸 좋아하지....?",
          "speaker_id": "2",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024715.1.50",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024715.1.51",
          "form": "최소한 그럴거면",
          "original_form": "최소한 그럴거면",
          "speaker_id": "1",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024715.1.52",
          "form": "중간중간에 좀 환기시켜주는 구간이 있어야지",
          "original_form": "중간중간에 좀 환기시켜주는 구간이 있어야지",
          "speaker_id": "1",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100024715.1.53",
          "form": "아니 적어도",
          "original_form": "아니 적어도",
          "speaker_id": "2",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100024715.1.54",
          "form": "뭐 나아지는거 없이 계속 나락으로만 가니까",
          "original_form": "뭐 나아지는거 없이 계속 나락으로만 가니까",
          "speaker_id": "1",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100024715.1.55",
          "form": "1.로맨스임",
          "original_form": "1.로맨스임",
          "speaker_id": "2",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100024715.1.56",
          "form": "게임을 하고 있는데 사람이 피곤해...",
          "original_form": "게임을 하고 있는데 사람이 피곤해...",
          "speaker_id": "1",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100024715.1.57",
          "form": "2.스토리가 기승전결이 있음",
          "original_form": "2.스토리가 기승전결이 있음",
          "speaker_id": "2",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100024715.1.58",
          "form": "정도는",
          "original_form": "정도는",
          "speaker_id": "2",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100024715.1.59",
          "form": "맞춰야하는거",
          "original_form": "맞춰야하는거",
          "speaker_id": "2",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100024715.1.60",
          "form": "아닐까",
          "original_form": "아닐까",
          "speaker_id": "2",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100024715.1.61",
          "form": "그러니까 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그러니까 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100024715.1.62",
          "form": "걍 맥락도 없이",
          "original_form": "걍 맥락도 없이",
          "speaker_id": "2",
          "time": "20210831 17:54"
        }
      ]
