      "utterance": [
        {
          "id": "MDRW2100046202.1.1",
          "form": "가을이라 옷 쇼핑해야할것같아요",
          "original_form": "가을이라 옷 쇼핑해야할것같아요",
          "speaker_id": "1",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046202.1.2",
          "form": "저두요ㅠㅠ",
          "original_form": "저두요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046202.1.3",
          "form": "가을옷은 이상하게 많이 없네요ㅠㅠ",
          "original_form": "가을옷은 이상하게 많이 없네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046202.1.4",
          "form": "약간 저는 그냥 셔츠 이런거 좋아해요",
          "original_form": "약간 저는 그냥 셔츠 이런거 좋아해요",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046202.1.5",
          "form": "오! 저두요!",
          "original_form": "오! 저두요!",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046202.1.6",
          "form": "셔츠에 스웨터나 가디건 입는것도 정말 좋아해요ㅠ",
          "original_form": "셔츠에 스웨터나 가디건 입는것도 정말 좋아해요ㅠ",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046202.1.7",
          "form": "저도 가디건 너무좋아요!",
          "original_form": "저도 가디건 너무좋아요!",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046202.1.8",
          "form": "가디건은 푹신푹신하고 부드럽기도해서 좋은데",
          "original_form": "가디건은 푹신푹신하고 부드럽기도해서 좋은데",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046202.1.9",
          "form": "바람이 좀만 불면 추워서 오래 못입는게 아쉬워요ㅠㅠ",
          "original_form": "바람이 좀만 불면 추워서 오래 못입는게 아쉬워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046202.1.10",
          "form": "맞아요 ㅠㅠ 한 한달도못입고",
          "original_form": "맞아요 ㅠㅠ 한 한달도못입고",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046202.1.11",
          "form": "패딩으로넘어가죠",
          "original_form": "패딩으로넘어가죠",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046202.1.12",
          "form": "패딩도 겨울에 다리 시린것 때문에 롱패딩이 생기기 전으로는 못돌아가겠어요ㅠㅠ",
          "original_form": "패딩도 겨울에 다리 시린것 때문에 롱패딩이 생기기 전으로는 못돌아가겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046202.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요  눈올때도 편하고",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요  눈올때도 편하고",
          "speaker_id": "1",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046202.1.14",
          "form": "맞아욬ㅋㅋ 김밥처럼 보이면 어때욬ㅋㅋㅋ 따숩고 편한뎈ㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋ 김밥처럼 보이면 어때욬ㅋㅋㅋ 따숩고 편한뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100046202.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ얼죽코는 모를거에요 이 따스함",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ얼죽코는 모를거에요 이 따스함",
          "speaker_id": "1",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046202.1.16",
          "form": "맞아욬ㅋㅋㅋ 코트가 아무리 따수워도 패딩만은 못하죠ㅠㅠ",
          "original_form": "맞아욬ㅋㅋㅋ 코트가 아무리 따수워도 패딩만은 못하죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046202.1.17",
          "form": "맞아요 ㅋㅋㅋ공강되네요",
          "original_form": "맞아요 ㅋㅋㅋ공강되네요",
          "speaker_id": "1",
          "time": "20210908 11:28"
        }
      ]
