      "utterance": [
        {
          "id": "MDRW2100002438.1.1",
          "form": "학교 다닐 때 과제 잘 하셨나요?",
          "original_form": "학교 다닐 때 과제 잘 하셨나요?",
          "speaker_id": "2",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002438.1.2",
          "form": "전 사실 잘 안했습니다ㅎ",
          "original_form": "전 사실 잘 안했습니다ㅎ",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002438.1.3",
          "form": "어린시절 숙제도 잘 안했어요",
          "original_form": "어린시절 숙제도 잘 안했어요",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002438.1.4",
          "form": "아 ㅎ 어린시절에 저도",
          "original_form": "아 ㅎ 어린시절에 저도",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002438.1.5",
          "form": "방학숙제는 밀리기 일수였죠",
          "original_form": "방학숙제는 밀리기 일수였죠",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002438.1.6",
          "form": "ㅎㅎㅎ 사람 사는 거 다 똑같죠",
          "original_form": "ㅎㅎㅎ 사람 사는 거 다 똑같죠",
          "speaker_id": "1",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002438.1.7",
          "form": "그 중에 일기가 제일 싫었어요",
          "original_form": "그 중에 일기가 제일 싫었어요",
          "speaker_id": "1",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002438.1.8",
          "form": "정말... 일기는.... 최악이죠",
          "original_form": "정말... 일기는.... 최악이죠",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002438.1.9",
          "form": "그래서 매잉 밀려썼어요 ㅎㅎ",
          "original_form": "그래서 매잉 밀려썼어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002438.1.10",
          "form": "name1님도요?",
          "original_form": "&name1&님도요?",
          "speaker_id": "2",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002438.1.11",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002438.1.12",
          "form": "그리고 날씨생각안난다고 안썼어요",
          "original_form": "그리고 날씨생각안난다고 안썼어요",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002438.1.13",
          "form": "날씨 그게 뭐라고 그거 부터 삐끗하면 쓰기싫어져요",
          "original_form": "날씨 그게 뭐라고 그거 부터 삐끗하면 쓰기싫어져요",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002438.1.14",
          "form": "아 너무나도 공감입니다",
          "original_form": "아 너무나도 공감입니다",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002438.1.15",
          "form": "지금 생각하면 일기 쓰는 걸 왜이리 싫어했는지 ㅎㅎ",
          "original_form": "지금 생각하면 일기 쓰는 걸 왜이리 싫어했는지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002438.1.16",
          "form": "숙제라 그랬나봐요",
          "original_form": "숙제라 그랬나봐요",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002438.1.17",
          "form": "약간 그것도있어요",
          "original_form": "약간 그것도있어요",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002438.1.18",
          "form": "내 생각을 누가 훔쳐보는기분",
          "original_form": "내 생각을 누가 훔쳐보는기분",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002438.1.19",
          "form": "그게 좀 싫었던것도 있어요",
          "original_form": "그게 좀 싫었던것도 있어요",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002438.1.20",
          "form": "맞아요 선생님이 검사했는데",
          "original_form": "맞아요 선생님이 검사했는데",
          "speaker_id": "2",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002438.1.21",
          "form": "지금 생각해도 의아하네요",
          "original_form": "지금 생각해도 의아하네요",
          "speaker_id": "2",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002438.1.22",
          "form": "일기숙제 검사라니...",
          "original_form": "일기숙제 검사라니...",
          "speaker_id": "2",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002438.1.23",
          "form": "그게 약간 생각발달?이런거랑 맞춤법 검사한다고 했던거같은데",
          "original_form": "그게 약간 생각발달?이런거랑 맞춤법 검사한다고 했던거같은데",
          "speaker_id": "1",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002438.1.24",
          "form": "의도는 좋으나.. 음...ㅎ",
          "original_form": "의도는 좋으나.. 음...ㅎ",
          "speaker_id": "1",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002438.1.25",
          "form": "아하 의도는 좋지만 정말 별로인 숙제에여 ㅠㅠ",
          "original_form": "아하 의도는 좋지만 정말 별로인 숙제에여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002438.1.26",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002438.1.27",
          "form": "아마 다시 돌아가서 쓰라해도 전 잘 안쓸 거 같아요",
          "original_form": "아마 다시 돌아가서 쓰라해도 전 잘 안쓸 거 같아요",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002438.1.28",
          "form": "오 그런 패기 좋네요 ㅎ",
          "original_form": "오 그런 패기 좋네요 ㅎ",
