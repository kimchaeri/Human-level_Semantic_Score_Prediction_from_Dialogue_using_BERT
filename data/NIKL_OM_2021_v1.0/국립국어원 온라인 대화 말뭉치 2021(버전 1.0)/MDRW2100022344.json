      "utterance": [
        {
          "id": "MDRW2100022344.1.1",
          "form": "내년 목표로 하고 있어요!",
          "original_form": "내년 목표로 하고 있어요!",
          "speaker_id": "1",
          "time": "20210829 13:07"
        },
        {
          "id": "MDRW2100022344.1.2",
          "form": "어우 경쟁률 장난 아니겠네요ᅲᅲᅲ",
          "original_form": "어우 경쟁률 장난 아니겠네요ᅲᅲᅲ",
          "speaker_id": "2",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022344.1.3",
          "form": "어느 지역으로 지원하시나요??",
          "original_form": "어느 지역으로 지원하시나요??",
          "speaker_id": "2",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022344.1.4",
          "form": "네ㅜㅜ",
          "original_form": "네ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022344.1.5",
          "form": "대전이요!",
          "original_form": "대전이요!",
          "speaker_id": "1",
          "time": "20210829 13:17"
        },
        {
          "id": "MDRW2100022344.1.6",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 13:17"
        },
        {
          "id": "MDRW2100022344.1.7",
          "form": "아 저는 경기권으로 지원할 예정입니다!!",
          "original_form": "아 저는 경기권으로 지원할 예정입니다!!",
          "speaker_id": "2",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022344.1.8",
          "form": "오오 경기.. 멋지십니닷",
          "original_form": "오오 경기.. 멋지십니닷",
          "speaker_id": "1",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022344.1.9",
          "form": "경기에 살아서 경기 지원하는 건데요 뭐ㅎㅎ",
          "original_form": "경기에 살아서 경기 지원하는 건데요 뭐ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:25"
        },
        {
          "id": "MDRW2100022344.1.10",
          "form": "일행은 자격 조건같은 거 없나요??",
          "original_form": "일행은 자격 조건같은 거 없나요??",
          "speaker_id": "2",
          "time": "20210829 13:25"
        },
        {
          "id": "MDRW2100022344.1.11",
          "form": "핳 그게 부럽심닷!",
          "original_form": "핳 그게 부럽심닷!",
          "speaker_id": "1",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100022344.1.12",
          "form": "네! 없더라구요~",
          "original_form": "네! 없더라구요~",
          "speaker_id": "1",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100022344.1.13",
          "form": "제 전공이랑 같은것도 없고 제일 많이 뽑기에.. 일행으로 했어요",
          "original_form": "제 전공이랑 같은것도 없고 제일 많이 뽑기에.. 일행으로 했어요",
          "speaker_id": "1",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100022344.1.14",
          "form": "오 전공이 뭔지 여쭤봐도 될까요??",
          "original_form": "오 전공이 뭔지 여쭤봐도 될까요??",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022344.1.15",
          "form": "저 영상이에요!",
          "original_form": "저 영상이에요!",
          "speaker_id": "1",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100022344.1.16",
          "form": "우와 저 영상학과 첨 봐요!!",
          "original_form": "우와 저 영상학과 첨 봐요!!",
          "speaker_id": "2",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022344.1.17",
          "form": "영상학과는 보통 어디로 취직 많이 해요??",
          "original_form": "영상학과는 보통 어디로 취직 많이 해요??",
          "speaker_id": "2",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022344.1.18",
          "form": "실습을 외주로 많이 갔어요!",
          "original_form": "실습을 외주로 많이 갔어요!",
          "speaker_id": "1",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022344.1.19",
          "form": "편집이나 다큐쪽도 갔고 뉴스도 많이 가요!",
          "original_form": "편집이나 다큐쪽도 갔고 뉴스도 많이 가요!",
          "speaker_id": "1",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022344.1.20",
          "form": "우와 멋지네요!!",
          "original_form": "우와 멋지네요!!",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022344.1.21",
          "form": "보통 방송국으로 많이 가나봐요!!",
          "original_form": "보통 방송국으로 많이 가나봐요!!",
