      "utterance": [
        {
          "id": "MDRW2100041445.1.1",
          "form": "ㄹㅇ....사실 벤티를 준다면",
          "original_form": "ㄹㅇ....사실 벤티를 준다면",
          "speaker_id": "2",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100041445.1.2",
          "form": "소는 안줘도 돼",
          "original_form": "소는 안줘도 돼",
          "speaker_id": "2",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100041445.1.3",
          "form": "해등절까지 객잔에 앉아서 도나 닦으라지",
          "original_form": "해등절까지 객잔에 앉아서 도나 닦으라지",
          "speaker_id": "2",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100041445.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ그정도냐궄ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그정도냐궄ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100041445.1.5",
          "form": "나 아짇도",
          "original_form": "나 아짇도",
          "speaker_id": "1",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100041445.1.6",
          "form": "울림풀 퀘스트 하는중임...",
          "original_form": "울림풀 퀘스트 하는중임...",
          "speaker_id": "1",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100041445.1.7",
          "form": "엌",
          "original_form": "엌",
          "speaker_id": "2",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100041445.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ울림풀",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ울림풀",
          "speaker_id": "2",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100041445.1.9",
          "form": "세이라이섬에 엄청 많아",
          "original_form": "세이라이섬에 엄청 많아",
          "speaker_id": "2",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100041445.1.10",
          "form": "이거 어디 모여잇어?",
          "original_form": "이거 어디 모여잇어?",
          "speaker_id": "1",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100041445.1.11",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100041445.1.12",
          "form": "전기장판 없는곳에도 꽤 있어",
          "original_form": "전기장판 없는곳에도 꽤 있어",
          "speaker_id": "2",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041445.1.13",
          "form": "아직 안열음.....",
          "original_form": "아직 안열음.....",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041445.1.14",
          "form": "함 가봐",
          "original_form": "함 가봐",
          "speaker_id": "2",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041445.1.15",
          "form": "그래야겠다...",
          "original_form": "그래야겠다...",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041445.1.16",
          "form": "나 다 깨고 갔는데",
          "original_form": "나 다 깨고 갔는데",
          "speaker_id": "2",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041445.1.17",
          "form": "허무하게 많았어",
          "original_form": "허무하게 많았어",
          "speaker_id": "2",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041445.1.18",
          "form": "와타츠미에 남은 퀘스트 하느라 아직 손도 안댐",
          "original_form": "와타츠미에 남은 퀘스트 하느라 아직 손도 안댐",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041445.1.19",
          "form": "그리고 타타라스나쪽?",
          "original_form": "그리고 타타라스나쪽?",
          "speaker_id": "2",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041445.1.20",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041445.1.21",
          "form": "우인단 너무많아",
          "original_form": "우인단 너무많아",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041445.1.22",
          "form": "그 협곡같은 강변이랑 미카게 용광로 있던 주변쯤",
          "original_form": "그 협곡같은 강변이랑 미카게 용광로 있던 주변쯤",
          "speaker_id": "2",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041445.1.23",
          "form": "그건 그래.....",
          "original_form": "그건 그래.....",
          "speaker_id": "2",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041445.1.24",
          "form": "거긴 채집하러갔다가 꼭 전투걸려서 나옴..",
          "original_form": "거긴 채집하러갔다가 꼭 전투걸려서 나옴..",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041445.1.25",
          "form": "협곡은 전기장판 못없애나??",
          "original_form": "협곡은 전기장판 못없애나??",
          "speaker_id": "1",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100041445.1.26",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100041445.1.27",
          "form": "아니 나도 잘 몰라 ㅋㅋㅋㅋ",
          "original_form": "아니 나도 잘 몰라 ㅋㅋㅋㅋ",
