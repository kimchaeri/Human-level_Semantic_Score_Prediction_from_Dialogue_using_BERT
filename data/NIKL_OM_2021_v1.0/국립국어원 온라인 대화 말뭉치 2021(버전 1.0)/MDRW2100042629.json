      "utterance": [
        {
          "id": "MDRW2100042629.1.1",
          "form": "좋아하는 프로잖아",
          "original_form": "좋아하는 프로잖아",
          "speaker_id": "2",
          "time": "20210906 23:57"
        },
        {
          "id": "MDRW2100042629.1.2",
          "form": "엉 바빠서 못봄",
          "original_form": "엉 바빠서 못봄",
          "speaker_id": "1",
          "time": "20210906 23:57"
        },
        {
          "id": "MDRW2100042629.1.3",
          "form": "재방송 많이 하니까 낼 벼ㆍ",
          "original_form": "재방송 많이 하니까 낼 벼ㆍ",
          "speaker_id": "2",
          "time": "20210906 23:57"
        },
        {
          "id": "MDRW2100042629.1.4",
          "form": "그래야겠어",
          "original_form": "그래야겠어",
          "speaker_id": "1",
          "time": "20210906 23:57"
        },
        {
          "id": "MDRW2100042629.1.5",
          "form": "게스트는 누군지 모르겠네",
          "original_form": "게스트는 누군지 모르겠네",
          "speaker_id": "2",
          "time": "20210906 23:57"
        },
        {
          "id": "MDRW2100042629.1.6",
          "form": "응 지난주 허경환이었는데",
          "original_form": "응 지난주 허경환이었는데",
          "speaker_id": "1",
          "time": "20210906 23:58"
        },
        {
          "id": "MDRW2100042629.1.7",
          "form": "그럼 이번주도 그렇겠지",
          "original_form": "그럼 이번주도 그렇겠지",
          "speaker_id": "2",
          "time": "20210906 23:58"
        },
        {
          "id": "MDRW2100042629.1.8",
          "form": "2주에 걸쳐 방송하니 그럴수도",
          "original_form": "2주에 걸쳐 방송하니 그럴수도",
          "speaker_id": "1",
          "time": "20210906 23:58"
        },
        {
          "id": "MDRW2100042629.1.9",
          "form": "검색해봐",
          "original_form": "검색해봐",
          "speaker_id": "2",
          "time": "20210906 23:58"
        },
        {
          "id": "MDRW2100042629.1.10",
          "form": "그러까",
          "original_form": "그러까",
          "speaker_id": "1",
          "time": "20210906 23:58"
        },
        {
          "id": "MDRW2100042629.1.11",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "2",
          "time": "20210906 23:58"
        },
        {
          "id": "MDRW2100042629.1.12",
          "form": "잠깐만",
          "original_form": "잠깐만",
          "speaker_id": "1",
          "time": "20210906 23:58"
        },
        {
          "id": "MDRW2100042629.1.13",
          "form": "누구야 게스트가",
          "original_form": "누구야 게스트가",
          "speaker_id": "2",
          "time": "20210907 00:00"
        },
        {
          "id": "MDRW2100042629.1.14",
          "form": "지난주에 이어서 허경환하고 동료",
          "original_form": "지난주에 이어서 허경환하고 동료",
          "speaker_id": "1",
          "time": "20210907 00:00"
        },
        {
          "id": "MDRW2100042629.1.15",
          "form": "아~",
          "original_form": "아~",
          "speaker_id": "2",
          "time": "20210907 00:00"
        },
        {
          "id": "MDRW2100042629.1.16",
          "form": "안정환이 재밌는데",
          "original_form": "안정환이 재밌는데",
          "speaker_id": "1",
          "time": "20210907 00:01"
        },
        {
          "id": "MDRW2100042629.1.17",
          "form": "왜! 허경환 편도 재밌을수도 있다",
          "original_form": "왜! 허경환 편도 재밌을수도 있다",
          "speaker_id": "2",
          "time": "20210907 00:01"
        },
        {
          "id": "MDRW2100042629.1.18",
          "form": "그런가",
          "original_form": "그런가",
          "speaker_id": "1",
          "time": "20210907 00:02"
        },
        {
          "id": "MDRW2100042629.1.19",
          "form": "엉 보고  얘기해주고",
          "original_form": "엉 보고  얘기해주고",
          "speaker_id": "2",
          "time": "20210907 00:02"
        },
        {
          "id": "MDRW2100042629.1.20",
          "form": "ㅋㅋ그게뭐야",
          "original_form": "ㅋㅋ그게뭐야",
          "speaker_id": "1",
