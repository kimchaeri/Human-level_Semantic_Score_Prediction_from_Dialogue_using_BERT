      "utterance": [
        {
          "id": "MDRW2100034629.1.1",
          "form": "헉 저도 치즈감자도 진짜 좋아해요!",
          "original_form": "헉 저도 치즈감자도 진짜 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034629.1.2",
          "form": "어니언 치즈 감자!!",
          "original_form": "어니언 치즈 감자!!",
          "speaker_id": "2",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034629.1.3",
          "form": "하 너무 맛있죠!!",
          "original_form": "하 너무 맛있죠!!",
          "speaker_id": "1",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034629.1.4",
          "form": "어떤 쩝쩝박사가 만든 메뉴인지",
          "original_form": "어떤 쩝쩝박사가 만든 메뉴인지",
          "speaker_id": "2",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034629.1.5",
          "form": "진짜 jmt!!!",
          "original_form": "진짜 jmt!!!",
          "speaker_id": "2",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034629.1.6",
          "form": "근데 그거 점바점 많이 타더라고요",
          "original_form": "근데 그거 점바점 많이 타더라고요",
          "speaker_id": "1",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034629.1.7",
          "form": "그쳐!! 상주고싶어요",
          "original_form": "그쳐!! 상주고싶어요",
          "speaker_id": "1",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034629.1.8",
          "form": "헉 진짜요??",
          "original_form": "헉 진짜요??",
          "speaker_id": "2",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034629.1.9",
          "form": "그냥 감튀에 치즈소스에 가루 올리는건데...",
          "original_form": "그냥 감튀에 치즈소스에 가루 올리는건데...",
          "speaker_id": "2",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100034629.1.10",
          "form": "그걸 애끼는 점주들이 있나보네요ㅠㅠㅠㅠ",
          "original_form": "그걸 애끼는 점주들이 있나보네요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100034629.1.11",
          "form": "네 ㅠㅠㅠ",
          "original_form": "네 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100034629.1.12",
          "form": "소스를 너무 아껴요",
          "original_form": "소스를 너무 아껴요",
          "speaker_id": "1",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100034629.1.13",
          "form": "심지어 어니언가루도 실수로 흘린만큼 준답니다...",
          "original_form": "심지어 어니언가루도 실수로 흘린만큼 준답니다...",
          "speaker_id": "1",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100034629.1.14",
          "form": "허...",
          "original_form": "허...",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034629.1.15",
          "form": "정말 쪼잔해요!!!",
          "original_form": "정말 쪼잔해요!!!",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034629.1.16",
          "form": "이러니 계모터치 소리를 듣죠",
          "original_form": "이러니 계모터치 소리를 듣죠",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034629.1.17",
          "form": "그러니깐요!",
          "original_form": "그러니깐요!",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034629.1.18",
          "form": "그래서 배달비가 4500이지만 더 먼데서 시켜요..ㅠㅠ",
          "original_form": "그래서 배달비가 4500이지만 더 먼데서 시켜요..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034629.1.19",
          "form": "헉....ㅠㅠㅠㅠㅠ",
          "original_form": "헉....ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 17:13"
        },
        {
          "id": "MDRW2100034629.1.20",
