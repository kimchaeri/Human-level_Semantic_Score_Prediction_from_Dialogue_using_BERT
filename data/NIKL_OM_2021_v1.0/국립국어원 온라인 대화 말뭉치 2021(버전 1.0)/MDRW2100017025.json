        },
        {
          "id": "MDRW2100017025.1.3",
          "form": "괜찮아",
          "original_form": "괜찮아",
          "speaker_id": "2",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100017025.1.4",
          "form": "30도도 더워",
          "original_form": "30도도 더워",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100017025.1.5",
          "form": "내가 오키나와갔을때가",
          "original_form": "내가 오키나와갔을때가",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100017025.1.6",
          "form": "30도 내외였는데",
          "original_form": "30도 내외였는데",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100017025.1.7",
          "form": "습해서 땀이 온몸에 들러붙잖아",
          "original_form": "습해서 땀이 온몸에 들러붙잖아",
          "speaker_id": "1",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100017025.1.8",
          "form": "사실 30도 보다 낮을 때가 많아",
          "original_form": "사실 30도 보다 낮을 때가 많아",
          "speaker_id": "2",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100017025.1.9",
          "form": "조금만 움직여도 피부에 물이 가득찬느임",
          "original_form": "조금만 움직여도 피부에 물이 가득찬느임",
          "speaker_id": "1",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100017025.1.10",
          "form": "우기라서",
          "original_form": "우기라서",
          "speaker_id": "2",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100017025.1.11",
