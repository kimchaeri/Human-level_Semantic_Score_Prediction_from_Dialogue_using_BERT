      "utterance": [
        {
          "id": "MDRW2100019330.1.1",
          "form": "근데 돈 많이 벌꺼면",
          "original_form": "근데 돈 많이 벌꺼면",
          "speaker_id": "2",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019330.1.2",
          "form": "공무유ㅗㄴ은 다른거로 돈버는거 아예안돼?",
          "original_form": "공무유ㅗㄴ은 다른거로 돈버는거 아예안돼?",
          "speaker_id": "1",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019330.1.3",
          "form": "애초에 공무원 하면 안된다더라 ㅋㅋㅋㅋㅋ",
          "original_form": "애초에 공무원 하면 안된다더라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019330.1.4",
          "form": "응 아마 안될껄",
          "original_form": "응 아마 안될껄",
          "speaker_id": "2",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019330.1.5",
          "form": "아 맞음ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 맞음ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019330.1.6",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "1",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019330.1.7",
          "form": "그 선생님들 유튜브 하는것도",
          "original_form": "그 선생님들 유튜브 하는것도",
          "speaker_id": "2",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019330.1.8",
          "form": "교육청에 다 신고 한다던데",
          "original_form": "교육청에 다 신고 한다던데",
          "speaker_id": "2",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019330.1.9",
          "form": "허락맡으면 된다며",
          "original_form": "허락맡으면 된다며",
          "speaker_id": "1",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019330.1.10",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019330.1.11",
          "form": "근데 솔직히 유튜브도",
          "original_form": "근데 솔직히 유튜브도",
          "speaker_id": "2",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019330.1.12",
          "form": "말이 쉽지",
          "original_form": "말이 쉽지",
          "speaker_id": "2",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019330.1.13",
          "form": "난 조회수 1",
          "original_form": "난 조회수 1",
          "speaker_id": "2",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019330.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019330.1.15",
          "form": "이런거 보면 마음 아파서 바로 접을듯...",
          "original_form": "이런거 보면 마음 아파서 바로 접을듯...",
          "speaker_id": "2",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019330.1.16",
          "form": "나도 악플 하나라도 달리면",
          "original_form": "나도 악플 하나라도 달리면",
          "speaker_id": "1",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019330.1.17",
          "form": "완전 안절부절못하다가",
          "original_form": "완전 안절부절못하다가",
          "speaker_id": "1",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019330.1.18",
          "form": "영상삭제할듯ㅋㅋㅋㅋㅋ",
          "original_form": "영상삭제할듯ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019330.1.19",
          "form": "내가 봤을때는 일단 블로그나 다른 sns로 팬층 만들고",
          "original_form": "내가 봤을때는 일단 블로그나 다른 sns로 팬층 만들고",
          "speaker_id": "2",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019330.1.20",
          "form": "그 다음에 유튜브 시작해야 그나마 구독자 생기고",
          "original_form": "그 다음에 유튜브 시작해야 그나마 구독자 생기고",
          "speaker_id": "2",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019330.1.21",
          "form": "팬 생기는것 같더러",
          "original_form": "팬 생기는것 같더러",
          "speaker_id": "2",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019330.1.22",
          "form": "라",
          "original_form": "라",
          "speaker_id": "2",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019330.1.23",
          "form": "난 그쪽으로는 재주가없으니가",
          "original_form": "난 그쪽으로는 재주가없으니가",
          "speaker_id": "1",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019330.1.24",
          "form": "경제공부 열심히해야지....",
          "original_form": "경제공부 열심히해야지....",
          "speaker_id": "1",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019330.1.25",
          "form": "나도...",
          "original_form": "나도...",
          "speaker_id": "2",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019330.1.26",
          "form": "로설 써서 대박나고 싶다",
          "original_form": "로설 써서 대박나고 싶다",
