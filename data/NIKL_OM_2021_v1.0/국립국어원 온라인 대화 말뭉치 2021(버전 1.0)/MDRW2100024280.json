      "utterance": [
        {
          "id": "MDRW2100024280.1.1",
          "form": "너 커뮤니티 해?",
          "original_form": "너 커뮤니티 해?",
          "speaker_id": "2",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024280.1.2",
          "form": "아어 티비에서도 많이 나오더라",
          "original_form": "아어 티비에서도 많이 나오더라",
          "speaker_id": "2",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024280.1.3",
          "form": "나는 하다말다가 해",
          "original_form": "나는 하다말다가 해",
          "speaker_id": "1",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024280.1.4",
          "form": "요즘은 커뮤니티용어와 일상용어가 혼재되는거같아",
          "original_form": "요즘은 커뮤니티용어와 일상용어가 혼재되는거같아",
          "speaker_id": "2",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024280.1.5",
          "form": "너는 어떤 유행어가 재밌니?",
          "original_form": "너는 어떤 유행어가 재밌니?",
          "speaker_id": "1",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024280.1.6",
          "form": "너 깍깍주시술 알아?",
          "original_form": "너 깍깍주시술 알아?",
          "speaker_id": "2",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024280.1.7",
          "form": "맞춤법 잘 모르는 사람들이 나오는데 웃기더라",
          "original_form": "맞춤법 잘 모르는 사람들이 나오는데 웃기더라",
          "speaker_id": "2",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024280.1.8",
          "form": "ㅠㅠ응 그 오타는 너무 웃겨",
          "original_form": "ㅠㅠ응 그 오타는 너무 웃겨",
          "speaker_id": "1",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024280.1.9",
          "form": "깎아 줄 수 있냐는 멀이야",
          "original_form": "깎아 줄 수 있냐는 멀이야",
          "speaker_id": "2",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024280.1.10",
          "form": "오타였을까?",
          "original_form": "오타였을까?",
          "speaker_id": "2",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024280.1.11",
          "form": "오타라고 생각하자...",
          "original_form": "오타라고 생각하자...",
          "speaker_id": "1",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024280.1.12",
          "form": "깍깍주시술 너무 웃기다",
          "original_form": "깍깍주시술 너무 웃기다",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024280.1.13",
          "form": "너 웃음지뢰라는 말은 알아?",
          "original_form": "너 웃음지뢰라는 말은 알아?",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024280.1.14",
          "form": "이것도 커뮤니티 용어인가?",
          "original_form": "이것도 커뮤니티 용어인가?",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024280.1.15",
          "form": "어어 그 단어는 참 잘만든 것 같아",
          "original_form": "어어 그 단어는 참 잘만든 것 같아",
          "speaker_id": "1",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024280.1.16",
          "form": "그리고 흐린눈이라고 알아?",
          "original_form": "그리고 흐린눈이라고 알아?",
          "speaker_id": "1",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024280.1.17",
          "form": "내친구들도 이제 다같이 쓰고있어",
          "original_form": "내친구들도 이제 다같이 쓰고있어",
          "speaker_id": "1",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024280.1.18",
          "form": "맞아. 깍깍주시술도 내 웃음지뢰야",
          "original_form": "맞아. 깍깍주시술도 내 웃음지뢰야",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024280.1.19",
          "form": "흐린눈 근데 잘 설명을 해줘얗9",
          "original_form": "흐린눈 근데 잘 설명을 해줘얗9",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024280.1.20",
          "form": "못알아듣는 친구들이 들으면 정말 갑분싸란자",
          "original_form": "못알아듣는 친구들이 들으면 정말 갑분싸란자",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024280.1.21",
          "form": "맞아 커뮤니티 용어를 남발하다간",
          "original_form": "맞아 커뮤니티 용어를 남발하다간",
          "speaker_id": "1",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024280.1.22",
          "form": "갑분싸는 한물 간 용어지?",
          "original_form": "갑분싸는 한물 간 용어지?",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024280.1.23",
          "form": "모임에서 소외될 수도 있어",
          "original_form": "모임에서 소외될 수도 있어",
          "speaker_id": "1",
          "time": "20210831 00:02"
        }
      ]
    }
