      "utterance": [
        {
          "id": "MDRW2100023663.1.1",
          "form": "헐... 좋다 ..평화롭다...",
          "original_form": "헐... 좋다 ..평화롭다...",
          "speaker_id": "2",
          "time": "20210830 14:09"
        },
        {
          "id": "MDRW2100023663.1.2",
          "form": "당케 부럽...",
          "original_form": "당케 부럽...",
          "speaker_id": "2",
          "time": "20210830 14:09"
        },
        {
          "id": "MDRW2100023663.1.3",
          "form": "샛별제과 다녀오셨어여?",
          "original_form": "샛별제과 다녀오셨어여?",
          "speaker_id": "2",
          "time": "20210830 14:09"
        },
        {
          "id": "MDRW2100023663.1.4",
          "form": "ㅋㅋㅋ그티만 교육중이라 너무졸리네용...",
          "original_form": "ㅋㅋㅋ그티만 교육중이라 너무졸리네용...",
          "speaker_id": "1",
          "time": "20210830 14:33"
        },
        {
          "id": "MDRW2100023663.1.5",
          "form": "아 셋별제과 가려고했는데",
          "original_form": "아 셋별제과 가려고했는데",
          "speaker_id": "1",
          "time": "20210830 14:33"
        },
        {
          "id": "MDRW2100023663.1.6",
          "form": "기프티콘이 주말까지인것들이있어서",
          "original_form": "기프티콘이 주말까지인것들이있어서",
          "speaker_id": "1",
          "time": "20210830 14:33"
        },
        {
          "id": "MDRW2100023663.1.7",
          "form": "파스쿠치에사 사왔어요",
          "original_form": "파스쿠치에사 사왔어요",
          "speaker_id": "1",
          "time": "20210830 14:33"
        },
        {
          "id": "MDRW2100023663.1.8",
          "form": "꺅 파스쿠찌!! 맛나겠다ㅜㅜㅜ",
          "original_form": "꺅 파스쿠찌!! 맛나겠다ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 14:36"
        },
        {
          "id": "MDRW2100023663.1.9",
          "form": "집에서 교육받으면서...티와 케잌이라뇨..",
          "original_form": "집에서 교육받으면서...티와 케잌이라뇨..",
          "speaker_id": "2",
          "time": "20210830 14:36"
        },
        {
          "id": "MDRW2100023663.1.10",
          "form": "넘나 퐌타스틱 골져스",
          "original_form": "넘나 퐌타스틱 골져스",
          "speaker_id": "2",
          "time": "20210830 14:37"
        },
        {
          "id": "MDRW2100023663.1.11",
          "form": "ㅋㅋㅋ졸려서 자꾸 뭐 가져다 먹게되네용 배뷰른데도 ㅋㅋㄴ잠깰라구 크크",
          "original_form": "ㅋㅋㅋ졸려서 자꾸 뭐 가져다 먹게되네용 배뷰른데도 ㅋㅋㄴ잠깰라구 크크",
          "speaker_id": "1",
          "time": "20210830 14:39"
        },
        {
          "id": "MDRW2100023663.1.12",
          "form": "아 너무 부럽다...!! 저는 뭐 먹다가도 메신저 소리 띠링나면 갑자기 너무 스트레스 하...",
          "original_form": "아 너무 부럽다...!! 저는 뭐 먹다가도 메신저 소리 띠링나면 갑자기 너무 스트레스 하...",
          "speaker_id": "2",
          "time": "20210830 15:03"
        },
        {
          "id": "MDRW2100023663.1.13",
          "form": "아 저 주말에 마켓컬리에서 파운드 산거와서",
          "original_form": "아 저 주말에 마켓컬리에서 파운드 산거와서",
          "speaker_id": "2",
          "time": "20210830 15:03"
        },
        {
          "id": "MDRW2100023663.1.14",
          "form": "그거 먹을거에오 지금",
          "original_form": "그거 먹을거에오 지금",
          "speaker_id": "2",
          "time": "20210830 15:03"
        },
        {
          "id": "MDRW2100023663.1.15",
          "form": "저도 뭐 ㅋㅋㅋ나중에 전달교육하려면 다시공부해야할듯요 ㅠ 흑ㅋㅋㅋㅋㅋ",
          "original_form": "저도 뭐 ㅋㅋㅋ나중에 전달교육하려면 다시공부해야할듯요 ㅠ 흑ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:08"
        },
        {
          "id": "MDRW2100023663.1.16",
          "form": "오 마켓컬리 파운드!!! 어떤거 사셨어요?",
          "original_form": "오 마켓컬리 파운드!!! 어떤거 사셨어요?",
          "speaker_id": "1",
          "time": "20210830 15:08"
        },
        {
          "id": "MDRW2100023663.1.17",
          "form": "모카랑 후르츠랑 크림치즈요!!",
          "original_form": "모카랑 후르츠랑 크림치즈요!!",
          "speaker_id": "2",
          "time": "20210830 15:13"
        },
        {
          "id": "MDRW2100023663.1.18",
          "form": "앞에2개는 이미먹어서없고",
          "original_form": "앞에2개는 이미먹어서없고",
          "speaker_id": "2",
          "time": "20210830 15:13"
        },
        {
          "id": "MDRW2100023663.1.19",
          "form": "크림치즈먹을거에용",
          "original_form": "크림치즈먹을거에용",
          "speaker_id": "2",
          "time": "20210830 15:13"
        },
        {
          "id": "MDRW2100023663.1.20",
          "form": "헐 대박ㅋㅋ후르츠 파운드에는 어떤 과일들 들어있었어요?",
          "original_form": "헐 대박ㅋㅋ후르츠 파운드에는 어떤 과일들 들어있었어요?",
          "speaker_id": "1",
          "time": "20210830 16:54"
        },
        {
          "id": "MDRW2100023663.1.21",
          "form": "근데 걍 건과일 들어있는 ㅋㅋㅋ",
          "original_form": "근데 걍 건과일 들어있는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:54"
        },
        {
          "id": "MDRW2100023663.1.22",
          "form": "그래도 후르츠가 젤 맛났어요",
          "original_form": "그래도 후르츠가 젤 맛났어요",
          "speaker_id": "2",
          "time": "20210830 16:55"
        },
        {
          "id": "MDRW2100023663.1.23",
          "form": "아항 건과일이었군요 ㅋㅋㅋ 크 커피랑 먹고싶네요",
          "original_form": "아항 건과일이었군요 ㅋㅋㅋ 크 커피랑 먹고싶네요",
          "speaker_id": "1",
          "time": "20210830 16:55"
        },
        {
          "id": "MDRW2100023663.1.24",
          "form": "저는 아까 홍차 마시다가",
          "original_form": "저는 아까 홍차 마시다가",
          "speaker_id": "1",
          "time": "20210830 16:55"
        },
        {
          "id": "MDRW2100023663.1.25",
          "form": "저두 커피랑 먹었답니다...!",
          "original_form": "저두 커피랑 먹었답니다...!",
          "speaker_id": "2",
          "time": "20210830 16:55"
        },
        {
          "id": "MDRW2100023663.1.26",
          "form": "아 백신맞은지 얼마안됐지하고 차는 남기고 케익만먹었어요",
          "original_form": "아 백신맞은지 얼마안됐지하고 차는 남기고 케익만먹었어요",
          "speaker_id": "1",
