      "utterance": [
        {
          "id": "MDRW2100012519.1.1",
          "form": "요즘도 알바 알아봐?",
          "original_form": "요즘도 알바 알아봐?",
          "speaker_id": "1",
          "time": "20210824 22:43"
        },
        {
          "id": "MDRW2100012519.1.2",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210824 22:43"
        },
        {
          "id": "MDRW2100012519.1.3",
          "form": "무슨 알바?",
          "original_form": "무슨 알바?",
          "speaker_id": "1",
          "time": "20210824 22:43"
        },
        {
          "id": "MDRW2100012519.1.4",
          "form": "단기 알바나 재택알바",
          "original_form": "단기 알바나 재택알바",
          "speaker_id": "2",
          "time": "20210824 22:43"
        },
        {
          "id": "MDRW2100012519.1.5",
          "form": "재택알바는 돈 안되지않아?",
          "original_form": "재택알바는 돈 안되지않아?",
          "speaker_id": "1",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012519.1.6",
          "form": "아니야",
          "original_form": "아니야",
          "speaker_id": "2",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012519.1.7",
          "form": "꽤 쏠쏠한데?",
          "original_form": "꽤 쏠쏠한데?",
          "speaker_id": "2",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012519.1.8",
          "form": "어떤 알바하는데?",
          "original_form": "어떤 알바하는데?",
          "speaker_id": "1",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012519.1.9",
          "form": "친구랑 대화하는거 녹음해서 보내는거",
          "original_form": "친구랑 대화하는거 녹음해서 보내는거",
          "speaker_id": "2",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012519.1.10",
          "form": "시급제야?",
          "original_form": "시급제야?",
          "speaker_id": "1",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012519.1.11",
          "form": "아니 건당으로 쳐줘",
          "original_form": "아니 건당으로 쳐줘",
          "speaker_id": "2",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012519.1.12",
          "form": "한줄씩 쳐줘?",
          "original_form": "한줄씩 쳐줘?",
          "speaker_id": "1",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012519.1.13",
          "form": "아니 5분이 한건이야",
          "original_form": "아니 5분이 한건이야",
          "speaker_id": "2",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012519.1.14",
          "form": "한 건에 얼마줘?",
          "original_form": "한 건에 얼마줘?",
          "speaker_id": "1",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012519.1.15",
          "form": "1500원이라서 시급으로 18000원인데 그만큼 받긴힘들어",
          "original_form": "1500원이라서 시급으로 18000원인데 그만큼 받긴힘들어",
          "speaker_id": "2",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100012519.1.16",
          "form": "뭐가 힘든데?",
          "original_form": "뭐가 힘든데?",
          "speaker_id": "1",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100012519.1.17",
          "form": "장소가 까다롭거든",
          "original_form": "장소가 까다롭거든",
          "speaker_id": "2",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100012519.1.18",
          "form": "어디서 해야하는데?",
          "original_form": "어디서 해야하는데?",
          "speaker_id": "1",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100012519.1.19",
          "form": "울림도 없고 엄청 조용한 곳에서 해야해",
          "original_form": "울림도 없고 엄청 조용한 곳에서 해야해",
          "speaker_id": "2",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100012519.1.20",
          "form": "그래서 어디서 하는데?",
          "original_form": "그래서 어디서 하는데?",
          "speaker_id": "1",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100012519.1.21",
          "form": "우리는 자동차 안",
          "original_form": "우리는 자동차 안",
          "speaker_id": "2",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100012519.1.22",
          "form": "안덥니?",
          "original_form": "안덥니?",
          "speaker_id": "1",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100012519.1.23",
          "form": "에어컨 키면 소리나서 너무 더워",
          "original_form": "에어컨 키면 소리나서 너무 더워",
          "speaker_id": "2",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100012519.1.24",
          "form": "계속할거야?",
          "original_form": "계속할거야?",
          "speaker_id": "1",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100012519.1.25",
          "form": "그래도 편하게 할 수 있어서 하게",
          "original_form": "그래도 편하게 할 수 있어서 하게",
          "speaker_id": "2",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100012519.1.26",
          "form": "그래 그럼",
          "original_form": "그래 그럼",
          "speaker_id": "1",
          "time": "20210824 22:47"
        }
      ]
    }
