      "utterance": [
        {
          "id": "MDRW2100025793.1.1",
          "form": "응응!",
          "original_form": "응응!",
          "speaker_id": "2",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100025793.1.2",
          "form": "시라노 또 안한대?",
          "original_form": "시라노 또 안한대?",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100025793.1.3",
          "form": "아 원래 올해 왔어야 되는데 얘기가 없더라ㅠㅠ",
          "original_form": "아 원래 올해 왔어야 되는데 얘기가 없더라ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100025793.1.4",
          "form": "아 ㅠㅠ왜지?",
          "original_form": "아 ㅠㅠ왜지?",
          "speaker_id": "1",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100025793.1.5",
          "form": "잘 안팔려서 그런가...? 사실 그렇게 인기가 많지는 않았오ㅠㅠ",
          "original_form": "잘 안팔려서 그런가...? 사실 그렇게 인기가 많지는 않았오ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100025793.1.6",
          "form": "나 조형균으로 보고싶은데ㅠㅠ",
          "original_form": "나 조형균으로 보고싶은데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100025793.1.7",
          "form": "나도 나도ㅠㅠ 조형균으로 또 보고싶어..",
          "original_form": "나도 나도ㅠㅠ 조형균으로 또 보고싶어..",
          "speaker_id": "2",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100025793.1.8",
          "form": "조형균 춤도 잘춘다며",
          "original_form": "조형균 춤도 잘춘다며",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100025793.1.9",
          "form": "어 비보이 출신이래ㅋㅋ",
          "original_form": "어 비보이 출신이래ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100025793.1.10",
          "form": "비보이?ㅋㅋㅋㅋㅋ진심?",
          "original_form": "비보이?ㅋㅋㅋㅋㅋ진심?",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100025793.1.11",
          "form": "응응ㅋㅋㅋ 그렇다고 하더라 그래서 춤 진짜 잘춰",
          "original_form": "응응ㅋㅋㅋ 그렇다고 하더라 그래서 춤 진짜 잘춰",
          "speaker_id": "2",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100025793.1.12",
          "form": "아니 나는 유튜브 뮤지컬 클립만 봤는뎈ㅋㅋㅋ그정도면 뮤지컬 댄스는 껌이겠다",
          "original_form": "아니 나는 유튜브 뮤지컬 클립만 봤는뎈ㅋㅋㅋ그정도면 뮤지컬 댄스는 껌이겠다",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025793.1.13",
          "form": "그니깐ㅋㅋㅋ 게다가 시라노는 그렇게 빡센 안무는 딱히 없더라고",
          "original_form": "그니깐ㅋㅋㅋ 게다가 시라노는 그렇게 빡센 안무는 딱히 없더라고",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025793.1.14",
          "form": "아 여름되니까 킹키부츠 또 보고 싶다",
          "original_form": "아 여름되니까 킹키부츠 또 보고 싶다",
