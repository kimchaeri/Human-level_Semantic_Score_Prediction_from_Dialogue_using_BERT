      "utterance": [
        {
          "id": "MDRW2100024065.1.1",
          "form": "혹시 좋아하시는 국내여행지 있으세요?",
          "original_form": "혹시 좋아하시는 국내여행지 있으세요?",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024065.1.2",
          "form": "저는.강릉이요 ㅋㅋ 강릉에 살았던 적이 있어서 제2의 고향같고 좋더라구요",
          "original_form": "저는.강릉이요 ㅋㅋ 강릉에 살았던 적이 있어서 제2의 고향같고 좋더라구요",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024065.1.3",
          "form": "바다도 예쁘구요",
          "original_form": "바다도 예쁘구요",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024065.1.4",
          "form": "담양도 좋았어요",
          "original_form": "담양도 좋았어요",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024065.1.5",
          "form": "강릉! 너무좋죠!",
          "original_form": "강릉! 너무좋죠!",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024065.1.6",
          "form": "외할머니가 강원도 사셔서ㅎㅎ",
          "original_form": "외할머니가 강원도 사셔서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024065.1.7",
          "form": "담양은 추천해주실곳이 있나요?",
          "original_form": "담양은 추천해주실곳이 있나요?",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024065.1.8",
          "form": "넘 유명하지만 죽녹원이요 ㅋㅋ",
          "original_form": "넘 유명하지만 죽녹원이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:53"
        },
        {
          "id": "MDRW2100024065.1.9",
          "form": "아! 안가봤어요ㅜㅜ",
          "original_form": "아! 안가봤어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100024065.1.10",
          "form": "숲을 좋아해서... 여름에 가면 넘 좋더라구요ㅠ",
          "original_form": "숲을 좋아해서... 여름에 가면 넘 좋더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100024065.1.11",
          "form": "여름에 더 좋아요?",
          "original_form": "여름에 더 좋아요?",
          "speaker_id": "1",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100024065.1.12",
          "form": "나중에 한 번 가보세요 ㅋㅋ 전라도 음식 좋아하세요?",
          "original_form": "나중에 한 번 가보세요 ㅋㅋ 전라도 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100024065.1.13",
          "form": "덥진않나요?",
          "original_form": "덥진않나요?",
          "speaker_id": "1",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100024065.1.14",
          "form": "네! 아무래도 그 때 녹음이 우거지니까요",
          "original_form": "네! 아무래도 그 때 녹음이 우거지니까요",
          "speaker_id": "2",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100024065.1.15",
          "form": "음식은 다 좋아요ㅋㅋ",
          "original_form": "음식은 다 좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100024065.1.16",
          "form": "근데 대나무 안에 그늘이라 너무 한여름 아니면 좋아요",
          "original_form": "근데 대나무 안에 그늘이라 너무 한여름 아니면 좋아요",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100024065.1.17",
          "form": "요즘도 괜찮겠네요",
          "original_form": "요즘도 괜찮겠네요",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100024065.1.18",
          "form": "우와 내년 여름휴가때 생각해봐야겠네요",
          "original_form": "우와 내년 여름휴가때 생각해봐야겠네요",
          "speaker_id": "1",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100024065.1.19",
          "form": "전라도 여행은 역시 맛집...",
          "original_form": "전라도 여행은 역시 맛집...",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100024065.1.20",
          "form": "전라도는 왠지..쌀도 맛있더라구요ㅋ",
          "original_form": "전라도는 왠지..쌀도 맛있더라구요ㅋ",
          "speaker_id": "1",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100024065.1.21",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100024065.1.22",
          "form": "저 진짜 맛집 안 정하고 그냥 대충 택시타구",
          "original_form": "저 진짜 맛집 안 정하고 그냥 대충 택시타구",
          "speaker_id": "2",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100024065.1.23",
          "form": "기사님 여기 뭐가 맛있어요 해서 내려주시는 데 갔는데 다 성공이었어요 ㅋㅋ",
          "original_form": "기사님 여기 뭐가 맛있어요 해서 내려주시는 데 갔는데 다 성공이었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:57"
        }
      ]
    }
