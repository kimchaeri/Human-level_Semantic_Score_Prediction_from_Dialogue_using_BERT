      "utterance": [
        {
          "id": "MDRW2100002335.1.1",
          "form": "반려동물 키우고 있나요?",
          "original_form": "반려동물 키우고 있나요?",
          "speaker_id": "2",
          "time": "20210820 09:52"
        },
        {
          "id": "MDRW2100002335.1.2",
          "form": "예전에 키웠는데 \r\n사고로 하늘나라 갔어요",
          "original_form": "예전에 키웠는데 \r\n사고로 하늘나라 갔어요",
          "speaker_id": "1",
          "time": "20210820 10:12"
        },
        {
          "id": "MDRW2100002335.1.3",
          "form": "name2님은 반려동물 키우셔요?",
          "original_form": "&name2&님은 반려동물 키우셔요?",
          "speaker_id": "1",
          "time": "20210820 10:14"
        },
        {
          "id": "MDRW2100002335.1.4",
          "form": "아이고 괜한 이야기 꺼낸건지... 사고라니...",
          "original_form": "아이고 괜한 이야기 꺼낸건지... 사고라니...",
          "speaker_id": "2",
          "time": "20210820 10:39"
        },
        {
          "id": "MDRW2100002335.1.5",
          "form": "제가 키우는 건 아니고 동생이 멍멍이 한 마리 키워요!",
          "original_form": "제가 키우는 건 아니고 동생이 멍멍이 한 마리 키워요!",
          "speaker_id": "2",
          "time": "20210820 10:39"
        },
        {
          "id": "MDRW2100002335.1.6",
          "form": "아네요 ㅎㅎㅎ 지금괜찮아요",
          "original_form": "아네요 ㅎㅎㅎ 지금괜찮아요",
          "speaker_id": "1",
          "time": "20210820 10:41"
        },
        {
          "id": "MDRW2100002335.1.7",
          "form": "요즘은 다른분들 키우는거 보는거 듣는거료\r\n대리만족 중이에요 ㅎㅎ",
          "original_form": "요즘은 다른분들 키우는거 보는거 듣는거료\r\n대리만족 중이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 10:41"
        },
        {
          "id": "MDRW2100002335.1.8",
          "form": "가끔 집에 놀러오면 귀여워요",
          "original_form": "가끔 집에 놀러오면 귀여워요",
          "speaker_id": "2",
          "time": "20210820 10:43"
        },
        {
          "id": "MDRW2100002335.1.9",
          "form": "갈색 푸들 키우거든요",
          "original_form": "갈색 푸들 키우거든요",
          "speaker_id": "2",
          "time": "20210820 10:44"
        },
        {
          "id": "MDRW2100002335.1.10",
          "form": "아 넘 귀엽겠어요 ㅎㅎ \r\n제 지인도 갈색 푸들이라 이름이 초코에욮ㅎㅎ",
          "original_form": "아 넘 귀엽겠어요 ㅎㅎ \r\n제 지인도 갈색 푸들이라 이름이 초코에욮ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 11:04"
        },
        {
          "id": "MDRW2100002335.1.11",
          "form": "초코라니... 빵실빵실 귀엽겠어요",
          "original_form": "초코라니... 빵실빵실 귀엽겠어요",
          "speaker_id": "2",
          "time": "20210820 11:09"
        },
        {
          "id": "MDRW2100002335.1.12",
          "form": "은근 관종이더라구요",
          "original_form": "은근 관종이더라구요",
          "speaker_id": "2",
          "time": "20210820 11:09"
        },
        {
          "id": "MDRW2100002335.1.13",
          "form": "ㅎㅎㅎㅎ 다시키우고 싶은맘이 큰데\r\n이별이 힘들어서  힘들더라구요",
          "original_form": "ㅎㅎㅎㅎ 다시키우고 싶은맘이 큰데\r\n이별이 힘들어서  힘들더라구요",
          "speaker_id": "1",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002335.1.14",
          "form": "맞아요 이별 오는게 너무 슬퍼요",
          "original_form": "맞아요 이별 오는게 너무 슬퍼요",
          "speaker_id": "2",
          "time": "20210820 11:29"
        },
        {
          "id": "MDRW2100002335.1.15",
          "form": "조금더 시간이 필요 한가봐요 ㅎㅎ",
          "original_form": "조금더 시간이 필요 한가봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100002335.1.16",
          "form": "가족이랑 마찬가지니깐요 그럴수있죠",
          "original_form": "가족이랑 마찬가지니깐요 그럴수있죠",
          "speaker_id": "2",
          "time": "20210820 11:34"
        },
        {
          "id": "MDRW2100002335.1.17",
          "form": "ㅎㅎㅎ 저는 원래  반려동물 안좋아했는데\r\n어쩔수없는 선택으로 만난건데\r\n지내다보니 정이 엄청 들더라구요",
          "original_form": "ㅎㅎㅎ 저는 원래  반려동물 안좋아했는데\r\n어쩔수없는 선택으로 만난건데\r\n지내다보니 정이 엄청 들더라구요",
          "speaker_id": "1",
          "time": "20210820 11:38"
        },
        {
          "id": "MDRW2100002335.1.18",
          "form": "맞아요 정이 무서워요",
          "original_form": "맞아요 정이 무서워요",
          "speaker_id": "2",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002335.1.19",
          "form": "그래서 더 신중해야 하구요 ㅎㅎ",
          "original_form": "그래서 더 신중해야 하구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 14:15"
        }
      ]
