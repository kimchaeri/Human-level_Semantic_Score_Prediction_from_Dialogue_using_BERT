      "utterance": [
        {
          "id": "MDRW2100041383.1.1",
          "form": "후기 엄청 많은데서 샀는데 실패했어요 ㅠ",
          "original_form": "후기 엄청 많은데서 샀는데 실패했어요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100041383.1.2",
          "form": "아이고ㅠㅠ 아까워라ㅠㅠ",
          "original_form": "아이고ㅠㅠ 아까워라ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100041383.1.3",
          "form": "한국인은...김치없이 밥먹기란 힘들어요ㅋㅋㅋㅠㅠ",
          "original_form": "한국인은...김치없이 밥먹기란 힘들어요ㅋㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100041383.1.4",
          "form": "그래서 저 진짜 힘들었어요 겨우겨우 먹어서 이제 반쪽 남았어요",
          "original_form": "그래서 저 진짜 힘들었어요 겨우겨우 먹어서 이제 반쪽 남았어요",
          "speaker_id": "1",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100041383.1.5",
          "form": "비비고 이런데서 시킬까요?",
          "original_form": "비비고 이런데서 시킬까요?",
          "speaker_id": "1",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100041383.1.6",
          "form": "아이고ㅠ 입맛에 안맞는 음식 먹는것만큼 힘든게 없죠ㅠ",
          "original_form": "아이고ㅠ 입맛에 안맞는 음식 먹는것만큼 힘든게 없죠ㅠ",
          "speaker_id": "2",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100041383.1.7",
          "form": "비비고가 대중적으로 다들 잘먹는거 같아요!",
          "original_form": "비비고가 대중적으로 다들 잘먹는거 같아요!",
          "speaker_id": "2",
          "time": "20210905 22:55"
        },
        {
          "id": "MDRW2100041383.1.8",
          "form": "좋아요 그럼 비비고 시켜야겠어요 ㅋㅋㅋ 감사합니당",
          "original_form": "좋아요 그럼 비비고 시켜야겠어요 ㅋㅋㅋ 감사합니당",
          "speaker_id": "1",
          "time": "20210905 22:56"
        },
        {
          "id": "MDRW2100041383.1.9",
          "form": "최근에 배달 시켜드신거 있어요?",
          "original_form": "최근에 배달 시켜드신거 있어요?",
          "speaker_id": "1",
          "time": "20210905 22:56"
        },
        {
          "id": "MDRW2100041383.1.10",
          "form": "역시나... 엽떡이요ㅋㅋㅋㅋ",
          "original_form": "역시나... 엽떡이요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100041383.1.11",
          "form": "매운거 잘 드세요?",
          "original_form": "매운거 잘 드세요?",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100041383.1.12",
          "form": "좀 먹긴해요!",
          "original_form": "좀 먹긴해요!",
          "speaker_id": "2",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100041383.1.13",
          "form": "불닭이 맵긴하지만 좋아하는 정도랄까...?",
          "original_form": "불닭이 맵긴하지만 좋아하는 정도랄까...?",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041383.1.14",
          "form": "허얼",
          "original_form": "허얼",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041383.1.15",
          "form": "신이시군요",
          "original_form": "신이시군요",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041383.1.16",
          "form": "사람이 아니셨어요",
          "original_form": "사람이 아니셨어요",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041383.1.17",
          "form": "앜ㅋㅋㅋㅋ신라면 드실수 있으세요?ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ신라면 드실수 있으세요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041383.1.18",
          "form": "앗 그정도는 먹어요 다행히...ㅎㅎㅎㅎㅎㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗 그정도는 먹어요 다행히...ㅎㅎㅎㅎㅎㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041383.1.19",
          "form": "그 이상은 어렵습니다",
          "original_form": "그 이상은 어렵습니다",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041383.1.20",
          "form": "아ㅋㅋㅋ보통 약간 매운거 못드시는 분들은 신라면이나 진라면으로 이야기 하시는거 같더라고요ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ보통 약간 매운거 못드시는 분들은 신라면이나 진라면으로 이야기 하시는거 같더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041383.1.21",
          "form": "아 저를 시험하신거옄ㅅ군요!ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 저를 시험하신거옄ㅅ군요!ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100041383.1.22",
          "form": "시험한게 아니라ㅋㅋㅋ 저는 다 먹으니까 잘 못먹는다는 말이 감이 안 와서 저거를 기준으로 물어봤어욬ㅋㅋㅋㅋ",
          "original_form": "시험한게 아니라ㅋㅋㅋ 저는 다 먹으니까 잘 못먹는다는 말이 감이 안 와서 저거를 기준으로 물어봤어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041383.1.23",
          "form": "갑자기 궁금한게 신라면 진라면 남자라면",
          "original_form": "갑자기 궁금한게 신라면 진라면 남자라면",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041383.1.24",
          "form": "얘네 매운걸로 서열이 어떻게 되나요??",
          "original_form": "얘네 매운걸로 서열이 어떻게 되나요??",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041383.1.25",
          "form": "ㅎㅎㅎㅎ 그러시군요!",
          "original_form": "ㅎㅎㅎㅎ 그러시군요!",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100041383.1.26",
          "form": "남자라면은 처음 들어봐요!",
          "original_form": "남자라면은 처음 들어봐요!",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100041383.1.27",
          "form": "열라면은 먹어봤는데",
          "original_form": "열라면은 먹어봤는데",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100041383.1.28",
          "form": "열라면 신라면 진라면 이렇게요",
          "original_form": "열라면 신라면 진라면 이렇게요",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100041383.1.29",
          "form": "아그렇군요?!",
          "original_form": "아그렇군요?!",
          "speaker_id": "2",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100041383.1.30",
          "form": "남자라면도 종종 맵다고 하시는 분들 있더라고요!",
          "original_form": "남자라면도 종종 맵다고 하시는 분들 있더라고요!",
          "speaker_id": "2",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100041383.1.31",
          "form": "신라면보다 맵다고 하던가요?",
          "original_form": "신라면보다 맵다고 하던가요?",
          "speaker_id": "1",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100041383.1.32",
          "form": "남자라면 먹어보고 싶은데요 약간 승부욕이 올라와요",
          "original_form": "남자라면 먹어보고 싶은데요 약간 승부욕이 올라와요",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041383.1.33",
          "form": "이게 좀 매워요ㅋㅋㅋㅋㅋ숨겨진 매운라면입니다",
          "original_form": "이게 좀 매워요ㅋㅋㅋㅋㅋ숨겨진 매운라면입니다",
          "speaker_id": "2",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100041383.1.34",
          "form": "토핑?으로 마늘이 늘어있어요",
          "original_form": "토핑?으로 마늘이 늘어있어요",
          "speaker_id": "2",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100041383.1.35",
          "form": "악 name2님한테 좀 매운거면 안되겠어요",
          "original_form": "악 &name2&님한테 좀 매운거면 안되겠어요",
          "speaker_id": "1",
          "time": "20210905 23:31"
        },
        {
          "id": "MDRW2100041383.1.36",
          "form": "제가 졌어요",
          "original_form": "제가 졌어요",
          "speaker_id": "1",
          "time": "20210905 23:31"
        },
        {
          "id": "MDRW2100041383.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:31"
        },
        {
          "id": "MDRW2100041383.1.38",
          "form": "ㅋㅋㅋㅋㅋ끓일때도 매운냄새가 올라오는데 종종 재채기가 나와요...ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ끓일때도 매운냄새가 올라오는데 종종 재채기가 나와요...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:32"
        },
        {
          "id": "MDRW2100041383.1.39",
          "form": "안돼안됔ㅋㅋㅋㅋㅋㅋ",
          "original_form": "안돼안됔ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:36"
        },
        {
          "id": "MDRW2100041383.1.40",
          "form": "무서워요ㅋㅋㅋㅋㅋ갸아아아",
          "original_form": "무서워요ㅋㅋㅋㅋㅋ갸아아아",
