      "utterance": [
        {
          "id": "MDRW2100024899.1.1",
          "form": "네 집안일 했어요!",
          "original_form": "네 집안일 했어요!",
          "speaker_id": "2",
          "time": "20210831 20:47"
        },
        {
          "id": "MDRW2100024899.1.2",
          "form": "얼마나 되셔요?",
          "original_form": "얼마나 되셔요?",
          "speaker_id": "1",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024899.1.3",
          "form": "2일에 1번씩 해요!",
          "original_form": "2일에 1번씩 해요!",
          "speaker_id": "2",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100024899.1.4",
          "form": "자주 하시넹",
          "original_form": "자주 하시넹",
          "speaker_id": "1",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100024899.1.5",
          "form": "네 좀 자주하는 편이에요",
          "original_form": "네 좀 자주하는 편이에요",
          "speaker_id": "2",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100024899.1.6",
          "form": "먼지가 집에 자주 쌓여서 ㅠㅠ",
          "original_form": "먼지가 집에 자주 쌓여서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100024899.1.7",
          "form": "부지런하시네여",
          "original_form": "부지런하시네여",
          "speaker_id": "1",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100024899.1.8",
          "form": "청소를 자주 하니",
          "original_form": "청소를 자주 하니",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024899.1.9",
          "form": "부지런해졌어요 ㅎㅎ",
          "original_form": "부지런해졌어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024899.1.10",
          "form": "힘들지 않을까요?",
          "original_form": "힘들지 않을까요?",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100024899.1.11",
          "form": "힘들긴하지만",
          "original_form": "힘들긴하지만",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100024899.1.12",
          "form": "청소하고 나고",
          "original_form": "청소하고 나고",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100024899.1.13",
          "form": "깨끗해진 바닥을 보면",
          "original_form": "깨끗해진 바닥을 보면",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100024899.1.14",
          "form": "성취감덕분에 다 사라지더라고요 ㅎㅎ",
          "original_form": "성취감덕분에 다 사라지더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100024899.1.15",
          "form": "그렇게많이하세요?",
          "original_form": "그렇게많이하세요?",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100024899.1.16",
          "form": "네 자주 하죵",
          "original_form": "네 자주 하죵",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100024899.1.17",
          "form": "부지런하시네여",
          "original_form": "부지런하시네여",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100024899.1.18",
          "form": "네 ㅎㅎ 그렇게 된거같아요",
          "original_form": "네 ㅎㅎ 그렇게 된거같아요",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
