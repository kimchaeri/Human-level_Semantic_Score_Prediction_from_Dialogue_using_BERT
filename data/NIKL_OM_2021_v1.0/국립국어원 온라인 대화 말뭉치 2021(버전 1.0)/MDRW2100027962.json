      "utterance": [
        {
          "id": "MDRW2100027962.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100027962.1.2",
          "form": "봉사 활동 해 봤어요?",
          "original_form": "봉사 활동 해 봤어요?",
          "speaker_id": "2",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100027962.1.3",
          "form": "학교 다닐 때요",
          "original_form": "학교 다닐 때요",
          "speaker_id": "1",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100027962.1.4",
          "form": "저는 주말에 아이들을 돌보는 봉사를 했었는데 코로나 이후로는 중단을 했어요",
          "original_form": "저는 주말에 아이들을 돌보는 봉사를 했었는데 코로나 이후로는 중단을 했어요",
          "speaker_id": "2",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100027962.1.5",
          "form": "뜻깊은 일을 하셨네요",
          "original_form": "뜻깊은 일을 하셨네요",
          "speaker_id": "1",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100027962.1.6",
          "form": "아이들이 눈에 밟혀서 한번 시작하면 계속 하게 돼요",
          "original_form": "아이들이 눈에 밟혀서 한번 시작하면 계속 하게 돼요",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100027962.1.7",
          "form": "축복 받아야 할 존재들인데",
          "original_form": "축복 받아야 할 존재들인데",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100027962.1.8",
          "form": "슬프네요",
          "original_form": "슬프네요",
          "speaker_id": "1",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100027962.1.9",
          "form": "아이들이 애정과 관심을 받고 자라야 정상적으로 성장할 수 있을 텐데",
          "original_form": "아이들이 애정과 관심을 받고 자라야 정상적으로 성장할 수 있을 텐데",
          "speaker_id": "2",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100027962.1.10",
          "form": "너무 안타깝죠",
          "original_form": "너무 안타깝죠",
          "speaker_id": "2",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100027962.1.11",
          "form": "정말로 그러네요",
          "original_form": "정말로 그러네요",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027962.1.12",
          "form": "그럼 가서 뭐 하시는 거예요?",
          "original_form": "그럼 가서 뭐 하시는 거예요?",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027962.1.13",
          "form": "분유도 먹이고 아이들 옷 세탁도 하고",
          "original_form": "분유도 먹이고 아이들 옷 세탁도 하고",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027962.1.14",
          "form": "목욕도 시키고 안아주거나 놀아주거나",
          "original_form": "목욕도 시키고 안아주거나 놀아주거나",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027962.1.15",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027962.1.16",
          "form": "이렇게 필요한 걸 그때 그때 해요",
          "original_form": "이렇게 필요한 걸 그때 그때 해요",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
