      "utterance": [
        {
          "id": "MDRW2100002169.1.1",
          "form": "코로나 예방 접종은 하셨나요?",
          "original_form": "코로나 예방 접종은 하셨나요?",
          "speaker_id": "2",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002169.1.2",
          "form": "1차완료상태고 2차한2주뒤에 맞는것같아요",
          "original_form": "1차완료상태고 2차한2주뒤에 맞는것같아요",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002169.1.3",
          "form": "name2님은 하셨나요?",
          "original_form": "&name2&님은 하셨나요?",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002169.1.4",
          "form": "전 얀센이라 끝인데",
          "original_form": "전 얀센이라 끝인데",
          "speaker_id": "2",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002169.1.5",
          "form": "후회중요 한번이라 효과가 있을지",
          "original_form": "후회중요 한번이라 효과가 있을지",
          "speaker_id": "2",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002169.1.6",
          "form": "오 얀센한방이라 편하네여",
          "original_form": "오 얀센한방이라 편하네여",
          "speaker_id": "1",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002169.1.7",
          "form": "전 모더나맞았는데 1차만 맞고끝내고싶은맘이 커요 ㅋㅋㅋ....",
          "original_form": "전 모더나맞았는데 1차만 맞고끝내고싶은맘이 커요 ㅋㅋㅋ....",
          "speaker_id": "1",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002169.1.8",
          "form": "효과가 미지수래요",
          "original_form": "효과가 미지수래요",
          "speaker_id": "2",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002169.1.9",
          "form": "그러시더라고요 다들",
          "original_form": "그러시더라고요 다들",
          "speaker_id": "2",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002169.1.10",
          "form": "헉ㅜㅜ미지수라니",
          "original_form": "헉ㅜㅜ미지수라니",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002169.1.11",
          "form": "1차는 사전예약이라 진짜 얼결에 맞았거든요",
          "original_form": "1차는 사전예약이라 진짜 얼결에 맞았거든요",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002169.1.12",
          "form": "막 걱정할틈도없이",
          "original_form": "막 걱정할틈도없이",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002169.1.13",
          "form": "맞아요 그냥가서 암생각없이 맞았죠",
          "original_form": "맞아요 그냥가서 암생각없이 맞았죠",
          "speaker_id": "2",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002169.1.14",
          "form": "근데 2차는 텀이길고 기다리다보니 오만걱정이다들어요",
          "original_form": "근데 2차는 텀이길고 기다리다보니 오만걱정이다들어요",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002169.1.15",
          "form": "그게 문제예요",
          "original_form": "그게 문제예요",
          "speaker_id": "2",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002169.1.16",
          "form": "ㅜㅜ2차가 더아프다기도하고 괜히불안해져서",
          "original_form": "ㅜㅜ2차가 더아프다기도하고 괜히불안해져서",
          "speaker_id": "1",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002169.1.17",
          "form": "이 백신들이 원래있던 백신들이아니니까요",
          "original_form": "이 백신들이 원래있던 백신들이아니니까요",
          "speaker_id": "1",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002169.1.18",
          "form": "넘 급하게 만들어진건아닌지ㅜ",
          "original_form": "넘 급하게 만들어진건아닌지ㅜ",
          "speaker_id": "1",
          "time": "20210820 00:26"
        },
        {
          "id": "MDRW2100002169.1.19",
          "form": "그렇죠 2차가 아프다더라구요 하지만 괜찮을 거예요",
          "original_form": "그렇죠 2차가 아프다더라구요 하지만 괜찮을 거예요",
          "speaker_id": "2",
          "time": "20210820 00:26"
        },
        {
          "id": "MDRW2100002169.1.20",
          "form": "급하게 했죠 저희집 다완료 멀쩡해요",
          "original_form": "급하게 했죠 저희집 다완료 멀쩡해요",
          "speaker_id": "2",
          "time": "20210820 00:26"
        },
        {
          "id": "MDRW2100002169.1.21",
          "form": "오 그러니 좀 안심되네여",
          "original_form": "오 그러니 좀 안심되네여",
          "speaker_id": "1",
          "time": "20210820 00:26"
        },
        {
          "id": "MDRW2100002169.1.22",
          "form": "저도 맘을 좀 비워야겠어요",
          "original_form": "저도 맘을 좀 비워야겠어요",
          "speaker_id": "1",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002169.1.23",
          "form": "아스트라 얀센 화이자 골고루 맞았어요",
          "original_form": "아스트라 얀센 화이자 골고루 맞았어요",
          "speaker_id": "2",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002169.1.24",
          "form": "네 맘편히 하세요",
          "original_form": "네 맘편히 하세요",
          "speaker_id": "2",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002169.1.25",
          "form": "큰애도 고3이라 화이자 2차했는데 이틀 좀아프다 말았어요",
          "original_form": "큰애도 고3이라 화이자 2차했는데 이틀 좀아프다 말았어요",
          "speaker_id": "2",
          "time": "20210820 00:29"
        }
      ]
    }
