      "utterance": [
        {
          "id": "MDRW2100044559.1.1",
          "form": "반려동물기르시나요?",
          "original_form": "반려동물기르시나요?",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044559.1.2",
          "form": "아뇨ㅜ",
          "original_form": "아뇨ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044559.1.3",
          "form": "저두요ㅜ",
          "original_form": "저두요ㅜ",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044559.1.4",
          "form": "렌선집사에요ㅜ",
          "original_form": "렌선집사에요ㅜ",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044559.1.5",
          "form": "저두 랜선집신요ㅜ",
          "original_form": "저두 랜선집신요ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044559.1.6",
          "form": "고양이 너무귀여운데요",
          "original_form": "고양이 너무귀여운데요",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044559.1.7",
          "form": "책임지기 무거워서요ㅜ",
          "original_form": "책임지기 무거워서요ㅜ",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044559.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044559.1.9",
          "form": "저두 마찬가지예요",
          "original_form": "저두 마찬가지예요",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044559.1.10",
          "form": "어떤고양이좋아하세요",
          "original_form": "어떤고양이좋아하세요",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044559.1.11",
          "form": "아 구체적인 종을 좋아하기보단",
          "original_form": "아 구체적인 종을 좋아하기보단",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044559.1.12",
          "form": "초보랜선집사입니당",
          "original_form": "초보랜선집사입니당",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044559.1.13",
          "form": "그러시군요 저는 노란고양이좋아해요",
          "original_form": "그러시군요 저는 노란고양이좋아해요",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044559.1.14",
          "form": "치즈태비요!",
          "original_form": "치즈태비요!",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044559.1.15",
          "form": "아아 찾아보니 엄청 매력있네요",
          "original_form": "아아 찾아보니 엄청 매력있네요",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044559.1.16",
          "form": "그쵸ㅠ 저는 한국 고양이들 사랑합니다ㅜ",
          "original_form": "그쵸ㅠ 저는 한국 고양이들 사랑합니다ㅜ",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044559.1.17",
          "form": "네네 고양이는 사랑이죠",
          "original_form": "네네 고양이는 사랑이죠",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044559.1.18",
          "form": "강아지도 좋지만 저는 역시 고양이가좋아요ㅎㅎ",
          "original_form": "강아지도 좋지만 저는 역시 고양이가좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044559.1.19",
          "form": "네 저두 요즘 고양이의 매력에 빠져드는 중",
          "original_form": "네 저두 요즘 고양이의 매력에 빠져드는 중",
          "speaker_id": "2",
          "time": "20210907 23:37"
        }
