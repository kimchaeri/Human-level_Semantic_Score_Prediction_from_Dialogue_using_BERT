      "utterance": [
        {
          "id": "MDRW2100019652.1.1",
          "form": "name1님 스포츠 관람 좋아하시나요?",
          "original_form": "&name1&님 스포츠 관람 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100019652.1.2",
          "form": "저는 월드컵이나 올림픽 이런거 보는거는 좋아해요 ㅋㅋ",
          "original_form": "저는 월드컵이나 올림픽 이런거 보는거는 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100019652.1.3",
          "form": "헛 저도요! 최애 종목 뭐였어요!?",
          "original_form": "헛 저도요! 최애 종목 뭐였어요!?",
          "speaker_id": "2",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019652.1.4",
          "form": "저는 축구를 원래 제일좋아해서 그거요 ㅋㅋㅋ",
          "original_form": "저는 축구를 원래 제일좋아해서 그거요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019652.1.5",
          "form": "엇 그렇구나!!!! 저는 이번에 배구가 그렇게 재밌더라고요!",
          "original_form": "엇 그렇구나!!!! 저는 이번에 배구가 그렇게 재밌더라고요!",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019652.1.6",
          "form": "인기많더라구요!",
          "original_form": "인기많더라구요!",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019652.1.7",
          "form": "저는 막경기만 조금 봤어요",
          "original_form": "저는 막경기만 조금 봤어요",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019652.1.8",
          "form": "ㅎㅎ 대단하더라고요",
          "original_form": "ㅎㅎ 대단하더라고요",
          "speaker_id": "2",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019652.1.9",
          "form": "야구장은 자주 가시나요?",
          "original_form": "야구장은 자주 가시나요?",
          "speaker_id": "2",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019652.1.10",
          "form": "전 직관해본적은 없어요 ㅜㅜ",
          "original_form": "전 직관해본적은 없어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019652.1.11",
          "form": "가보셨나요?",
          "original_form": "가보셨나요?",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019652.1.12",
          "form": "네! 코로나 전에는 자주 갔었어요",
          "original_form": "네! 코로나 전에는 자주 갔었어요",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019652.1.13",
          "form": "야구장 치맥을 엄청 좋아해서 ㅎㅎ",
          "original_form": "야구장 치맥을 엄청 좋아해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019652.1.14",
          "form": "와거기서 먹으면 더 꿀맛일거같아요",
          "original_form": "와거기서 먹으면 더 꿀맛일거같아요",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019652.1.15",
          "form": "맞아요! 야구룰은 잘 모르지만 분위기 때문에",
          "original_form": "맞아요! 야구룰은 잘 모르지만 분위기 때문에",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019652.1.16",
          "form": "항상 즐겁게 보고 와요",
          "original_form": "항상 즐겁게 보고 와요",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019652.1.17",
          "form": "같이 응원하면 분위기타서 재밌겠어요!",
          "original_form": "같이 응원하면 분위기타서 재밌겠어요!",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019652.1.18",
          "form": "네! 춤추는 분들도 있어서 재밌어요 ㅎㅎ",
          "original_form": "네! 춤추는 분들도 있어서 재밌어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:12"
        }
      ]
