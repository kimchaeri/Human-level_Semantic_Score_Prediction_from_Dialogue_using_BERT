      "utterance": [
        {
          "id": "MDRW2100025692.1.1",
          "form": "난 필릭스가 좋더랑",
          "original_form": "난 필릭스가 좋더랑",
          "speaker_id": "2",
          "time": "20210901 15:59"
        },
        {
          "id": "MDRW2100025692.1.2",
          "form": "필릭스가 최애야?ㅋㅋㅋ",
          "original_form": "필릭스가 최애야?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:00"
        },
        {
          "id": "MDRW2100025692.1.3",
          "form": "걍 귀엽? 내스타일은 방찬ㅋㅋ",
          "original_form": "걍 귀엽? 내스타일은 방찬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:00"
        },
        {
          "id": "MDRW2100025692.1.4",
          "form": "너 스타일이 뭔데?ㅋㅋㅋ",
          "original_form": "너 스타일이 뭔데?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:00"
        },
        {
          "id": "MDRW2100025692.1.5",
          "form": "나 몸좋은애^^ㅋㅋㅋ\r\n필릭스는 귀엽게생겼는데 목소리가 진짜 짱낮아",
          "original_form": "나 몸좋은애^^ㅋㅋㅋ\r\n필릭스는 귀엽게생겼는데 목소리가 진짜 짱낮아",
          "speaker_id": "2",
          "time": "20210901 16:00"
        },
        {
          "id": "MDRW2100025692.1.6",
          "form": "방찬 좋짘ㅋㅋ",
          "original_form": "방찬 좋짘ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:00"
        },
        {
          "id": "MDRW2100025692.1.7",
          "form": "근데 97년생이구나...",
          "original_form": "근데 97년생이구나...",
          "speaker_id": "2",
          "time": "20210901 16:00"
        },
        {
          "id": "MDRW2100025692.1.8",
          "form": "맞아 진짜 얼굴이랑 딴 판이야 목소리가",
          "original_form": "맞아 진짜 얼굴이랑 딴 판이야 목소리가",
          "speaker_id": "1",
          "time": "20210901 16:01"
        },
        {
          "id": "MDRW2100025692.1.9",
          "form": "방찬?",
          "original_form": "방찬?",
          "speaker_id": "1",
          "time": "20210901 16:01"
        },
        {
          "id": "MDRW2100025692.1.10",
          "form": "응 방찬ㅋㅋ큐ㅠㅠ",
          "original_form": "응 방찬ㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:01"
        },
        {
          "id": "MDRW2100025692.1.11",
          "form": "다섯살 차이 밖에 안나넼ㅋㅋ",
          "original_form": "다섯살 차이 밖에 안나넼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:01"
        },
        {
          "id": "MDRW2100025692.1.12",
          "form": "아 맞지 5살이면 뭐~\r\n아 나 그때 꽂혔던 그륩",
          "original_form": "아 맞지 5살이면 뭐~\r\n아 나 그때 꽂혔던 그륩",
          "speaker_id": "2",
          "time": "20210901 16:01"
        },
        {
          "id": "MDRW2100025692.1.13",
          "form": "에이티즈는 어떻게 된거니?",
          "original_form": "에이티즈는 어떻게 된거니?",
          "speaker_id": "1",
          "time": "20210901 16:01"
        },
        {
          "id": "MDRW2100025692.1.14",
          "form": "이제 에이티즈는 관심없어?",
          "original_form": "이제 에이티즈는 관심없어?",
          "speaker_id": "1",
          "time": "20210901 16:01"
        },
        {
          "id": "MDRW2100025692.1.15",
          "form": "아 맞아ㅋㅋㅋ\r\n이젠 찾아보지는 않는? 요즘 대탈출이 제일 재믺어ㅋㅋ",
          "original_form": "아 맞아ㅋㅋㅋ\r\n이젠 찾아보지는 않는? 요즘 대탈출이 제일 재믺어ㅋㅋ",
          "speaker_id": "2",
