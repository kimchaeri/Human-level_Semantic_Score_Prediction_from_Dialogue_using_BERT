      "utterance": [
        {
          "id": "MDRW2100026578.1.1",
          "form": "비가 또 오네요",
          "original_form": "비가 또 오네요",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026578.1.2",
          "form": "글게여 맨날와요",
          "original_form": "글게여 맨날와요",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026578.1.3",
          "form": "요즘 너무 많이 오네요ㅜㅜ",
          "original_form": "요즘 너무 많이 오네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026578.1.4",
          "form": "네 습해서시러요ㅠ",
          "original_form": "네 습해서시러요ㅠ",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026578.1.5",
          "form": "빨래가 잘 안 말라서 싫긴해요",
          "original_form": "빨래가 잘 안 말라서 싫긴해요",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026578.1.6",
          "form": "언제까지 오려나요",
          "original_form": "언제까지 오려나요",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026578.1.7",
          "form": "글게여 ㅠ 비멈추면 완전 가을일듯",
          "original_form": "글게여 ㅠ 비멈추면 완전 가을일듯",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026578.1.8",
          "form": "기온이 확 시원해질것같아요",
          "original_form": "기온이 확 시원해질것같아요",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026578.1.9",
          "form": "어느 계절을 가장 좋아하세요?",
          "original_form": "어느 계절을 가장 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026578.1.10",
          "form": "가을이용 ㅋㅋㅋㅋ",
          "original_form": "가을이용 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026578.1.11",
          "form": "이유가 있나요?ㅎㅎ",
          "original_form": "이유가 있나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026578.1.12",
          "form": "ㅋㅋ ㅋ젤 쾌적해요",
          "original_form": "ㅋㅋ ㅋ젤 쾌적해요",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026578.1.13",
          "form": "봄은 미세먼지ㅠㅠㅠ",
          "original_form": "봄은 미세먼지ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026578.1.14",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026578.1.15",
          "form": "저는 봄 좋아했는데 미세먼지때문에 극혐",
          "original_form": "저는 봄 좋아했는데 미세먼지때문에 극혐",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026578.1.16",
          "form": "비염이 심해서ㅋㅋ",
          "original_form": "비염이 심해서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026578.1.17",
          "form": "마자요 저도 ㅠ",
          "original_form": "마자요 저도 ㅠ",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026578.1.18",
          "form": "점점 그러네요 해가갈수록",
          "original_form": "점점 그러네요 해가갈수록",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026578.1.19",
          "form": "미세먼지 정말 심각한 것같아요",
          "original_form": "미세먼지 정말 심각한 것같아요",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026578.1.20",
          "form": "그니까요 이 중국넘들",
          "original_form": "그니까요 이 중국넘들",
          "speaker_id": "2",
          "time": "20210901 23:15"
        }
      ]
