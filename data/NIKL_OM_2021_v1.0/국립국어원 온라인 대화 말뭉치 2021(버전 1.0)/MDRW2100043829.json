      "utterance": [
        {
          "id": "MDRW2100043829.1.1",
          "form": "교촌에 간장인가 맛있던데",
          "original_form": "교촌에 간장인가 맛있던데",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043829.1.2",
          "form": "그런 비슷한 맛인데 더 달달한 느낌?",
          "original_form": "그런 비슷한 맛인데 더 달달한 느낌?",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043829.1.3",
          "form": "글고 비에이치씨 특유의 향? 맛? 나",
          "original_form": "글고 비에이치씨 특유의 향? 맛? 나",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043829.1.4",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043829.1.5",
          "form": "나근데 저번ㅇㅔ 고바먹고 배탈났었늠 흑",
          "original_form": "나근데 저번ㅇㅔ 고바먹고 배탈났었늠 흑",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043829.1.6",
          "form": "엥 고바삭 손절하자 그냥",
          "original_form": "엥 고바삭 손절하자 그냥",
          "speaker_id": "2",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043829.1.7",
          "form": "다른 치킨으로 갈아타",
          "original_form": "다른 치킨으로 갈아타",
          "speaker_id": "2",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043829.1.8",
          "form": "개맛있는데 않되",
          "original_form": "개맛있는데 않되",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043829.1.9",
          "form": "넌 치킨이 그렇게 쉽니?",
          "original_form": "넌 치킨이 그렇게 쉽니?",
          "speaker_id": "1",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043829.1.10",
          "form": "세상에 고바삭보다 맛있는 치킨 엄청 많아",
          "original_form": "세상에 고바삭보다 맛있는 치킨 엄청 많아",
          "speaker_id": "2",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043829.1.11",
          "form": "하지만 최애치킨에 오를 수 읶는 건 고바뿐이야",
          "original_form": "하지만 최애치킨에 오를 수 읶는 건 고바뿐이야",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043829.1.12",
          "form": "아니. 고바는 그저 스쳐지나가는 치킨.",
          "original_form": "아니. 고바는 그저 스쳐지나가는 치킨.",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043829.1.13",
          "form": "굽네에서 순살 시키면 진짜 야둘야들하고 맛있어",
          "original_form": "굽네에서 순살 시키면 진짜 야둘야들하고 맛있어",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043829.1.14",
          "form": "고바 의견 인정 못해",
          "original_form": "고바 의견 인정 못해",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043829.1.15",
          "form": "굽네 순살 다릿살이라 맛있어 ㄹㅇ",
          "original_form": "굽네 순살 다릿살이라 맛있어 ㄹㅇ",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043829.1.16",
          "form": "ㅁㅈ 야들야들 갈비천왕 순살로 먹고 깜짝 놀람",
          "original_form": "ㅁㅈ 야들야들 갈비천왕 순살로 먹고 깜짝 놀람",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043829.1.17",
          "form": "오늘 밥 너무 만ㄹ이 먹어서 배불러",
          "original_form": "오늘 밥 너무 만ㄹ이 먹어서 배불러",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043829.1.18",
          "form": "맞아 덕분에 잘마셧오",
          "original_form": "맞아 덕분에 잘마셧오",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100043829.1.19",
          "form": "태리로제vs떡슐랭로제",
          "original_form": "태리로제vs떡슐랭로제",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100043829.1.20",
          "form": "태리로제",
          "original_form": "태리로제",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100043829.1.21",
          "form": "아뭔가 나는 배달떡볶이같은 떡볶이 진짜 안맞는듯",
          "original_form": "아뭔가 나는 배달떡볶이같은 떡볶이 진짜 안맞는듯",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100043829.1.22",
          "form": "그 ㅇ다 똑같이 생긴 밀떡이 넘 맛없는것같아",
          "original_form": "그 ㅇ다 똑같이 생긴 밀떡이 넘 맛없는것같아",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100043829.1.23",
          "form": "오늘도 그냥.. 그냥 먹었어",
          "original_form": "오늘도 그냥.. 그냥 먹었어",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100043829.1.24",
          "form": "맛없는건 아닌데 그냥.. 그렇다.. 하면서 먹음",
          "original_form": "맛없는건 아닌데 그냥.. 그렇다.. 하면서 먹음",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100043829.1.25",
          "form": "근데 태리로제 나도 저번주에 먹었는데 진짜 맛있드라",
          "original_form": "근데 태리로제 나도 저번주에 먹었는데 진짜 맛있드라",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100043829.1.26",
          "form": "그치 맛 ㄱㅊ지",
          "original_form": "그치 맛 ㄱㅊ지",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100043829.1.27",
          "form": "꾸덕꾸덕 돌았음 존맛",
          "original_form": "꾸덕꾸덕 돌았음 존맛",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100043829.1.28",
          "form": "나 거기 투움바도 먹어보고싶어",
          "original_form": "나 거기 투움바도 먹어보고싶어",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100043829.1.29",
          "form": "재난지원금으로 이것저것 먹울거 생각하니까 벌써 행봇하다",
          "original_form": "재난지원금으로 이것저것 먹울거 생각하니까 벌써 행봇하다",
