      "utterance": [
        {
          "id": "MDRW2100035453.1.1",
          "form": "요샌 클라우드를 많이마셨어요~ 클라우드드ㄹㅐ프트 나온뒤로",
          "original_form": "요샌 클라우드를 많이마셨어요~ 클라우드드ㄹㅐ프트 나온뒤로",
          "speaker_id": "2",
          "time": "20210829 23:01"
        },
        {
          "id": "MDRW2100035453.1.2",
          "form": "커피좋아하세요~?",
          "original_form": "커피좋아하세요~?",
          "speaker_id": "2",
          "time": "20210829 23:01"
        },
        {
          "id": "MDRW2100035453.1.3",
          "form": "전 맥주는 잘 몰라서 4개 만원 편의점 세일할때 아무거나 골라요",
          "original_form": "전 맥주는 잘 몰라서 4개 만원 편의점 세일할때 아무거나 골라요",
          "speaker_id": "1",
          "time": "20210829 23:01"
        },
        {
          "id": "MDRW2100035453.1.4",
          "form": "커피는 좋아해요",
          "original_form": "커피는 좋아해요",
          "speaker_id": "1",
          "time": "20210829 23:01"
        },
        {
          "id": "MDRW2100035453.1.5",
          "form": "제 필수품입니다",
          "original_form": "제 필수품입니다",
          "speaker_id": "1",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100035453.1.6",
          "form": "저도요 ㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100035453.1.7",
          "form": "좋아하는브랜드가있으세요~? 아님 홈카페?",
          "original_form": "좋아하는브랜드가있으세요~? 아님 홈카페?",
          "speaker_id": "2",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100035453.1.8",
          "form": "전 사람만나야 할뗀 이디야 이런데여도 다 잘가고요",
          "original_form": "전 사람만나야 할뗀 이디야 이런데여도 다 잘가고요",
          "speaker_id": "1",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100035453.1.9",
          "form": "맛으로 먹는건 집에서 무난하게 라바짜 원두를 먹어요",
          "original_form": "맛으로 먹는건 집에서 무난하게 라바짜 원두를 먹어요",
          "speaker_id": "1",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100035453.1.10",
          "form": "라바짜 지금도 인지 모르겟지만 전에 맥카페 원두가 라바짜였던거같은데~",
          "original_form": "라바짜 지금도 인지 모르겟지만 전에 맥카페 원두가 라바짜였던거같은데~",
          "speaker_id": "2",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100035453.1.11",
          "form": "전 원두를 크게 선호하능건없엇서 그냥",
          "original_form": "전 원두를 크게 선호하능건없엇서 그냥",
          "speaker_id": "2",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100035453.1.12",
          "form": "커피면다좋아요 ㅎㅎ 너무 산미가 강하지만 않으면 !",
          "original_form": "커피면다좋아요 ㅎㅎ 너무 산미가 강하지만 않으면 !",
          "speaker_id": "2",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100035453.1.13",
          "form": "라바짜가 언제사도 맛이 비슷해서",
          "original_form": "라바짜가 언제사도 맛이 비슷해서",
          "speaker_id": "1",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100035453.1.14",
          "form": "좋았어요",
          "original_form": "좋았어요",
          "speaker_id": "1",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100035453.1.15",
          "form": "로스터리 카페 제품 국산걸 좀 사봤는데",
          "original_form": "로스터리 카페 제품 국산걸 좀 사봤는데",
          "speaker_id": "1",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100035453.1.16",
          "form": "그것도 중요하죠!",
          "original_form": "그것도 중요하죠!",
          "speaker_id": "2",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100035453.1.17",
          "form": "같은 맛을 지키는데는 좀적었어요",
          "original_form": "같은 맛을 지키는데는 좀적었어요",
          "speaker_id": "1",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100035453.1.18",
          "form": "라바짜는 싼 라인은 비슷하게 싼 맛.. 중간은 중간맛",
          "original_form": "라바짜는 싼 라인은 비슷하게 싼 맛.. 중간은 중간맛",
          "speaker_id": "1",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100035453.1.19",
          "form": "예상가능한 맛이에요",
          "original_form": "예상가능한 맛이에요",
          "speaker_id": "1",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100035453.1.20",
          "form": "편차가잇엇군요~",
          "original_form": "편차가잇엇군요~",
          "speaker_id": "2",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100035453.1.21",
          "form": "네 규모가 작을수록",
          "original_form": "네 규모가 작을수록",
          "speaker_id": "1",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100035453.1.22",
          "form": "블렌딩할 양이 적으니깐 편차가 커요",
          "original_form": "블렌딩할 양이 적으니깐 편차가 커요",
          "speaker_id": "1",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100035453.1.23",
          "form": "님은 원두를 뭘로 가세요?",
          "original_form": "님은 원두를 뭘로 가세요?",
          "speaker_id": "1",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100035453.1.24",
          "form": "그렇군요~",
          "original_form": "그렇군요~",
          "speaker_id": "2",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100035453.1.25",
          "form": "전 원두보고 마시는건아니라",
          "original_form": "전 원두보고 마시는건아니라",
          "speaker_id": "2",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100035453.1.26",
          "form": "그냥 매일 나갓을때 그날에 따라 주변에 커피숍에서 테이크아웃해용!",
          "original_form": "그냥 매일 나갓을때 그날에 따라 주변에 커피숍에서 테이크아웃해용!",
          "speaker_id": "2",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100035453.1.27",
          "form": "그게 사실 제일 편해요",
          "original_form": "그게 사실 제일 편해요",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100035453.1.28",
          "form": "내려먹기가 귀찮긴해요 ㅋㅋㅋㅋㅋ",
          "original_form": "내려먹기가 귀찮긴해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100035453.1.29",
          "form": "어떤 커피를 좋아하세요?",
          "original_form": "어떤 커피를 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100035453.1.30",
          "form": "라떼 카푸치노 이런 종류중에서요",
          "original_form": "라떼 카푸치노 이런 종류중에서요",
          "speaker_id": "1",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100035453.1.31",
          "form": "전 그냥 아아요 !!",
          "original_form": "전 그냥 아아요 !!",
          "speaker_id": "2",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100035453.1.32",
          "form": "아메리카노만 거의마셔요! 정말 가끔 땡길때 라떼",
          "original_form": "아메리카노만 거의마셔요! 정말 가끔 땡길때 라떼",
          "speaker_id": "2",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100035453.1.33",
          "form": "클래식하네요! 전 라떼요",
          "original_form": "클래식하네요! 전 라떼요",
          "speaker_id": "1",
          "time": "20210829 23:12"
        }
      ]
    }
