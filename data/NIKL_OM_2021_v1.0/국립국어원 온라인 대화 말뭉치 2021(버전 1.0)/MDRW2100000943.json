      "utterance": [
        {
          "id": "MDRW2100000943.1.1",
          "form": "나 봉사 한번도 안가봤는데",
          "original_form": "나 봉사 한번도 안가봤는데",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100000943.1.2",
          "form": "교육봉사 이런거 말고",
          "original_form": "교육봉사 이런거 말고",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100000943.1.3",
          "form": "근데 유기견보호센터 이런데서는 해보고싶음",
          "original_form": "근데 유기견보호센터 이런데서는 해보고싶음",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100000943.1.4",
          "form": "음 나는 중학생때 몇번 정화봉사 가본적 있는듯",
          "original_form": "음 나는 중학생때 몇번 정화봉사 가본적 있는듯",
          "speaker_id": "1",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100000943.1.5",
          "form": "대학생 되고나서는 나도 교육봉사만",
          "original_form": "대학생 되고나서는 나도 교육봉사만",
          "speaker_id": "1",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100000943.1.6",
          "form": "유기견봉사 맞아...",
          "original_form": "유기견봉사 맞아...",
          "speaker_id": "1",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100000943.1.7",
          "form": "근데 그런 봉사는 보통 시내랑 좀 멀리있으니까",
          "original_form": "근데 그런 봉사는 보통 시내랑 좀 멀리있으니까",
          "speaker_id": "1",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100000943.1.8",
          "form": "정화봉사 신기하네",
          "original_form": "정화봉사 신기하네",
          "speaker_id": "2",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100000943.1.9",
          "form": "말이 거창하지 그냥 청소지뭐 ㅋㅋㅋㅋㅋ",
          "original_form": "말이 거창하지 그냥 청소지뭐 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100000943.1.10",
          "form": "포항에 유기견센터 거기 가보려고 나중에 언젠간",
          "original_form": "포항에 유기견센터 거기 가보려고 나중에 언젠간",
          "speaker_id": "2",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100000943.1.11",
          "form": "나는 그냥 휴지줍기만 해봄",
          "original_form": "나는 그냥 휴지줍기만 해봄",
          "speaker_id": "2",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100000943.1.12",
          "form": "오 나도 같이가자",
          "original_form": "오 나도 같이가자",
          "speaker_id": "1",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100000943.1.13",
          "form": "포항에 있구나 그래도 가까운편이네",
          "original_form": "포항에 있구나 그래도 가까운편이네",
          "speaker_id": "1",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100000943.1.14",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100000943.1.15",
          "form": "강아지들 사진 보니까 귀엽더라",
          "original_form": "강아지들 사진 보니까 귀엽더라",
          "speaker_id": "2",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100000943.1.16",
          "form": "ㅋㅋㅋㅋ시간 많을때 해볼껄 그랬다",
          "original_form": "ㅋㅋㅋㅋ시간 많을때 해볼껄 그랬다",
          "speaker_id": "1",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100000943.1.17",
          "form": "나도 ㅋㅋㅋㅋ 시간이 안나네",
          "original_form": "나도 ㅋㅋㅋㅋ 시간이 안나네",
          "speaker_id": "2",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100000943.1.18",
          "form": "그러면 일단 네이버 해피빔 기부나 하자",
          "original_form": "그러면 일단 네이버 해피빔 기부나 하자",
          "speaker_id": "2",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100000943.1.19",
          "form": "네이버 리뷰쓰면 주는거..",
          "original_form": "네이버 리뷰쓰면 주는거..",
          "speaker_id": "2",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100000943.1.20",
          "form": "ㅋㅋ아 나도 콩 많이 모았음",
          "original_form": "ㅋㅋ아 나도 콩 많이 모았음",
          "speaker_id": "1",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100000943.1.21",
          "form": "빨리 기부고",
          "original_form": "빨리 기부고",
          "speaker_id": "2",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100000943.1.22",
          "form": "나는 환경이나 동물 그런걸로 함",
          "original_form": "나는 환경이나 동물 그런걸로 함",
          "speaker_id": "2",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100000943.1.23",
          "form": "나는 미혼모 이런곳!",
          "original_form": "나는 미혼모 이런곳!",
          "speaker_id": "1",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100000943.1.24",
          "form": "나도 그거 가끔한다",
          "original_form": "나도 그거 가끔한다",
          "speaker_id": "2",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100000943.1.25",
          "form": "미혼모쪽 잘 안모이더라 기부금이",
          "original_form": "미혼모쪽 잘 안모이더라 기부금이",
          "speaker_id": "2",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100000943.1.26",
          "form": "하고싶은 봉사가 있니?",
          "original_form": "하고싶은 봉사가 있니?",
          "speaker_id": "1",
          "time": "20210824 00:10"
        },
        {
          "id": "MDRW2100000943.1.27",
          "form": "나는 무료급식 봉사를 꼭 가보고싶어",
          "original_form": "나는 무료급식 봉사를 꼭 가보고싶어",
          "speaker_id": "1",
          "time": "20210824 00:11"
        }
      ]
    }
