      "utterance": [
        {
          "id": "MDRW2100002106.1.1",
          "form": "요즘 하시는 운동있으신가요?",
          "original_form": "요즘 하시는 운동있으신가요?",
          "speaker_id": "1",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002106.1.2",
          "form": "달리기 하고있어요",
          "original_form": "달리기 하고있어요",
          "speaker_id": "2",
          "time": "20210820 20:15"
        },
        {
          "id": "MDRW2100002106.1.3",
          "form": "달리기 건강에 좋죠",
          "original_form": "달리기 건강에 좋죠",
          "speaker_id": "1",
          "time": "20210820 20:16"
        },
        {
          "id": "MDRW2100002106.1.4",
          "form": "헬스장 가시는거예요?",
          "original_form": "헬스장 가시는거예요?",
          "speaker_id": "1",
          "time": "20210820 20:16"
        },
        {
          "id": "MDRW2100002106.1.5",
          "form": "아뇨 바로 집 앞에 공원에서 달려요",
          "original_form": "아뇨 바로 집 앞에 공원에서 달려요",
          "speaker_id": "2",
          "time": "20210820 20:18"
        },
        {
          "id": "MDRW2100002106.1.6",
          "form": "운동 하시는거 있으세요?",
          "original_form": "운동 하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210820 20:18"
        },
        {
          "id": "MDRW2100002106.1.7",
          "form": "전에는 줌바랑 스피닝했었는데 요즘엔 암것도 안해요ㅎ",
          "original_form": "전에는 줌바랑 스피닝했었는데 요즘엔 암것도 안해요ㅎ",
          "speaker_id": "1",
          "time": "20210820 20:19"
        },
        {
          "id": "MDRW2100002106.1.8",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210820 20:21"
        },
        {
          "id": "MDRW2100002106.1.9",
          "form": "혹시 어디 아픈덴 없어요?",
          "original_form": "혹시 어디 아픈덴 없어요?",
          "speaker_id": "2",
          "time": "20210820 20:21"
        },
        {
          "id": "MDRW2100002106.1.10",
          "form": "만성피로말고는 딱히 아픈데는 없네요",
          "original_form": "만성피로말고는 딱히 아픈데는 없네요",
          "speaker_id": "1",
          "time": "20210820 20:22"
        },
        {
          "id": "MDRW2100002106.1.11",
          "form": "그래도 건강한 편인듯요",
          "original_form": "그래도 건강한 편인듯요",
          "speaker_id": "1",
          "time": "20210820 20:22"
        },
        {
          "id": "MDRW2100002106.1.12",
          "form": "전 오늘 치과 다녀왔는데",
          "original_form": "전 오늘 치과 다녀왔는데",
          "speaker_id": "2",
          "time": "20210820 20:24"
        },
        {
          "id": "MDRW2100002106.1.13",
          "form": "치아 사이에 고무박고왔어요..",
          "original_form": "치아 사이에 고무박고왔어요..",
          "speaker_id": "2",
          "time": "20210820 20:24"
        },
        {
          "id": "MDRW2100002106.1.14",
          "form": "아 어디 아프신거예요? 고무는왜요?",
          "original_form": "아 어디 아프신거예요? 고무는왜요?",
          "speaker_id": "1",
          "time": "20210820 20:24"
        },
        {
          "id": "MDRW2100002106.1.15",
          "form": "아뇨 아픈건 아니고",
          "original_form": "아뇨 아픈건 아니고",
          "speaker_id": "2",
          "time": "20210820 20:24"
        },
        {
          "id": "MDRW2100002106.1.16",
          "form": "교정할건데 공간확장 해야한데서...",
          "original_form": "교정할건데 공간확장 해야한데서...",
          "speaker_id": "2",
          "time": "20210820 20:24"
        },
        {
          "id": "MDRW2100002106.1.17",
          "form": "살짝 아프네요",
          "original_form": "살짝 아프네요",
          "speaker_id": "2",
          "time": "20210820 20:25"
        },
        {
          "id": "MDRW2100002106.1.18",
          "form": "아 교정하시는군요",
          "original_form": "아 교정하시는군요",
          "speaker_id": "1",
          "time": "20210820 20:25"
        },
        {
          "id": "MDRW2100002106.1.19",
          "form": "치과는 가기 넘 무서워요",
          "original_form": "치과는 가기 넘 무서워요",
          "speaker_id": "1",
          "time": "20210820 20:26"
        },
        {
          "id": "MDRW2100002106.1.20",
          "form": "혹시 치과 마취주사 맞아보셨어요?",
          "original_form": "혹시 치과 마취주사 맞아보셨어요?",
          "speaker_id": "2",
          "time": "20210820 20:27"
        },
        {
          "id": "MDRW2100002106.1.21",
          "form": "사랑니빼야하는데 무섭네요..",
          "original_form": "사랑니빼야하는데 무섭네요..",
          "speaker_id": "2",
          "time": "20210820 20:27"
        },
        {
          "id": "MDRW2100002106.1.22",
          "form": "네 몇달전에 사랑니 뽑았어요^^",
          "original_form": "네 몇달전에 사랑니 뽑았어요^^",
          "speaker_id": "1",
          "time": "20210820 20:27"
        },
        {
          "id": "MDRW2100002106.1.23",
          "form": "뽑는것보다 마취가 더아픈듯요",
          "original_form": "뽑는것보다 마취가 더아픈듯요",
