      "utterance": [
        {
          "id": "MDRW2100039188.1.1",
          "form": "거기우동사리 넣으면 꿀맛",
          "original_form": "거기우동사리 넣으면 꿀맛",
          "speaker_id": "1",
          "time": "20210903 13:22"
        },
        {
          "id": "MDRW2100039188.1.2",
          "form": "아 곱창전골도 좋죠",
          "original_form": "아 곱창전골도 좋죠",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100039188.1.3",
          "form": "우동사리까지 보태서 먹고",
          "original_form": "우동사리까지 보태서 먹고",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100039188.1.4",
          "form": "나중에 밥도 넣어서 볶아먹으면 캬~~",
          "original_form": "나중에 밥도 넣어서 볶아먹으면 캬~~",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100039188.1.5",
          "form": "배는 안고픈데 먹는 이야기하니 먹고싶어지네요",
          "original_form": "배는 안고픈데 먹는 이야기하니 먹고싶어지네요",
          "speaker_id": "2",
          "time": "20210903 15:51"
        },
        {
          "id": "MDRW2100039188.1.6",
          "form": "네저도요 ㅋ\r\n면과 고기는 세트에요",
          "original_form": "네저도요 ㅋ\r\n면과 고기는 세트에요",
          "speaker_id": "1",
          "time": "20210903 15:51"
        },
        {
          "id": "MDRW2100039188.1.7",
          "form": "아 그러고보니 맞네요 ㅎ",
          "original_form": "아 그러고보니 맞네요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100039188.1.8",
          "form": "부대찌개에도 라면사리",
          "original_form": "부대찌개에도 라면사리",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100039188.1.9",
          "form": "세뚜세뚜 ㅎ",
          "original_form": "세뚜세뚜 ㅎ",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100039188.1.10",
          "form": "보쌈족발은 막국수죠",
          "original_form": "보쌈족발은 막국수죠",
          "speaker_id": "1",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100039188.1.11",
          "form": "캬~~맞아요 맞아요",
          "original_form": "캬~~맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100039188.1.12",
          "form": "갈비엔 냉면이고요",
          "original_form": "갈비엔 냉면이고요",
          "speaker_id": "1",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100039188.1.13",
          "form": "생각해보니 면과 고기의 조합이 많네요",
          "original_form": "생각해보니 면과 고기의 조합이 많네요",
          "speaker_id": "2",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100039188.1.14",
          "form": "네 잘어울린다는 거겟죠",
          "original_form": "네 잘어울린다는 거겟죠",
          "speaker_id": "1",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100039188.1.15",
          "form": "그러니 다들 찾는거죵 ㅎ",
          "original_form": "그러니 다들 찾는거죵 ㅎ",
          "speaker_id": "2",
          "time": "20210903 15:56"
        },
        {
          "id": "MDRW2100039188.1.16",
          "form": "오늘저녁은 또 무얼먹을지 ㅎㅎ",
          "original_form": "오늘저녁은 또 무얼먹을지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 15:56"
        },
        {
          "id": "MDRW2100039188.1.17",
          "form": "전 쭈꾸미 볶아먹으려고요",
          "original_form": "전 쭈꾸미 볶아먹으려고요",
          "speaker_id": "1",
          "time": "20210903 15:56"
        },
        {
          "id": "MDRW2100039188.1.18",
          "form": "오 쭈꾸미 맵지않아요?",
          "original_form": "오 쭈꾸미 맵지않아요?",
          "speaker_id": "2",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039188.1.19",
          "form": "저는 김치볶음밥했어요",
          "original_form": "저는 김치볶음밥했어요",
          "speaker_id": "2",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039188.1.20",
          "form": "매운데 맛있어요",
          "original_form": "매운데 맛있어요",
          "speaker_id": "1",
          "time": "20210903 18:12"
        },
        {
          "id": "MDRW2100039188.1.21",
          "form": "매울것같아서 아예 생각도 안한 메뉴예요",
          "original_form": "매울것같아서 아예 생각도 안한 메뉴예요",
          "speaker_id": "2",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100039188.1.22",
          "form": "스팸에 김치볶음밥은 언제먹어도 맛난거같아요 ㅎ",
          "original_form": "스팸에 김치볶음밥은 언제먹어도 맛난거같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100039188.1.23",
          "form": "낙지볶음보다는 나아서 자주 사먹어요\r\n스팸은 진리죠",
          "original_form": "낙지볶음보다는 나아서 자주 사먹어요\r\n스팸은 진리죠",
          "speaker_id": "1",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100039188.1.24",
          "form": "스팸은 상추만 싸먹어도 맛나요ㅋ",
          "original_form": "스팸은 상추만 싸먹어도 맛나요ㅋ",
          "speaker_id": "1",
          "time": "20210903 18:19"
        }
      ]
    }
