      "utterance": [
        {
          "id": "MDRW2100016559.1.1",
          "form": "그거방탈출게임나온거봤음?",
          "original_form": "그거방탈출게임나온거봤음?",
          "speaker_id": "1",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100016559.1.2",
          "form": "리얼크라임씬",
          "original_form": "리얼크라임씬",
          "speaker_id": "1",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100016559.1.3",
          "form": "엥 그게 뭐야??",
          "original_form": "엥 그게 뭐야??",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016559.1.4",
          "form": "모바일 게임이야?",
          "original_form": "모바일 게임이야?",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016559.1.5",
          "form": "ㅇㅇ뭔가너취향일거같음",
          "original_form": "ㅇㅇ뭔가너취향일거같음",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016559.1.6",
          "form": "9월1일부터유료고",
          "original_form": "9월1일부터유료고",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016559.1.7",
          "form": "지금은무료래",
          "original_form": "지금은무료래",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016559.1.8",
          "form": "헐 일단 다운 받아봐야겠네",
          "original_form": "헐 일단 다운 받아봐야겠네",
          "speaker_id": "2",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100016559.1.9",
          "form": "어떤 류야??",
          "original_form": "어떤 류야??",
          "speaker_id": "2",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100016559.1.10",
          "form": "같은거?",
          "original_form": "같은거?",
          "speaker_id": "1",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100016559.1.11",
          "form": "추리도하고그러나봐",
          "original_form": "추리도하고그러나봐",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016559.1.12",
          "form": "약간사람모아서하는방탈출",
          "original_form": "약간사람모아서하는방탈출",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016559.1.13",
          "form": "오오옹 재밌겠다",
          "original_form": "오오옹 재밌겠다",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016559.1.14",
          "form": "ㅇㅇ너스팀게임해본적있어?",
          "original_form": "ㅇㅇ너스팀게임해본적있어?",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016559.1.15",
          "form": "아니 안해봤어",
          "original_form": "아니 안해봤어",
          "speaker_id": "2",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016559.1.16",
          "form": "세일할 때 쟁겨두기만 해봤어",
          "original_form": "세일할 때 쟁겨두기만 해봤어",
          "speaker_id": "2",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016559.1.17",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016559.1.18",
          "form": "뭐뭐",
          "original_form": "뭐뭐",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016559.1.19",
          "form": "사려고?",
          "original_form": "사려고?",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016559.1.20",
          "form": "사려는게 아니고 특별 세일 할 때 있잖아",
          "original_form": "사려는게 아니고 특별 세일 할 때 있잖아",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016559.1.21",
          "form": "ㄷ그땨 사뒀어",
          "original_form": "ㄷ그땨 사뒀어",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016559.1.22",
          "form": "리틀 나이트 메어",
          "original_form": "리틀 나이트 메어",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016559.1.23",
          "form": "ㅇㅇ아",
          "original_form": "ㅇㅇ아",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016559.1.24",
          "form": "리틀나이트메어는뭔데",
          "original_form": "리틀나이트메어는뭔데",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016559.1.25",
          "form": "추리게임?",
          "original_form": "추리게임?",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016559.1.26",
          "form": "아니 뭐라 그러냐 그거",
          "original_form": "아니 뭐라 그러냐 그거",
          "speaker_id": "2",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016559.1.27",
          "form": "어드벤처 탈출 게임?",
          "original_form": "어드벤처 탈출 게임?",
          "speaker_id": "2",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016559.1.28",
          "form": "딱니취향일듯",
          "original_form": "딱니취향일듯",
          "speaker_id": "1",
          "time": "20210826 13:58"
        }
      ]
    }
