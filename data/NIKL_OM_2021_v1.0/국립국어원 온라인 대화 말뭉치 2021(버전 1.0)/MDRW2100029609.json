      "utterance": [
        {
          "id": "MDRW2100029609.1.1",
          "form": "저는 게임은 패드로해요",
          "original_form": "저는 게임은 패드로해요",
          "speaker_id": "2",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100029609.1.2",
          "form": "크게보이거든요",
          "original_form": "크게보이거든요",
          "speaker_id": "2",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100029609.1.3",
          "form": "오 패드좋지요",
          "original_form": "오 패드좋지요",
          "speaker_id": "1",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100029609.1.4",
          "form": "관련 어플 다운받아서",
          "original_form": "관련 어플 다운받아서",
          "speaker_id": "2",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100029609.1.5",
          "form": "패드 게임하려 사셨나요",
          "original_form": "패드 게임하려 사셨나요",
          "speaker_id": "1",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100029609.1.6",
          "form": "스킨 공짜로 입힐수 있어요",
          "original_form": "스킨 공짜로 입힐수 있어요",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029609.1.7",
          "form": "일땜에 샀는데",
          "original_form": "일땜에 샀는데",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029609.1.8",
          "form": "게임하려는 목적이 많이커요",
          "original_form": "게임하려는 목적이 많이커요",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029609.1.9",
          "form": "스킨은 배경을 얘기하는듯요",
          "original_form": "스킨은 배경을 얘기하는듯요",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029609.1.10",
          "form": "낯섭니다",
          "original_form": "낯섭니다",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029609.1.11",
          "form": "옷입니다",
          "original_form": "옷입니다",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029609.1.12",
          "form": "개이득이군요. 일석이조",
          "original_form": "개이득이군요. 일석이조",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029609.1.13",
          "form": "캐릭터 외양입니다",
          "original_form": "캐릭터 외양입니다",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029609.1.14",
          "form": "공짜로 바꿀수있어서",
          "original_form": "공짜로 바꿀수있어서",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029609.1.15",
          "form": "무슨옷 입혔나요",
          "original_form": "무슨옷 입혔나요",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029609.1.16",
          "form": "매우좋아요",
          "original_form": "매우좋아요",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029609.1.17",
          "form": "보노보노옷도 있고",
          "original_form": "보노보노옷도 있고",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029609.1.18",
          "form": "인기많은 스킨이 뭔가요",
          "original_form": "인기많은 스킨이 뭔가요",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029609.1.19",
          "form": "펭수 옷도 있어요",
          "original_form": "펭수 옷도 있어요",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029609.1.20",
          "form": "매번 바껴요",
          "original_form": "매번 바껴요",
          "speaker_id": "2",
