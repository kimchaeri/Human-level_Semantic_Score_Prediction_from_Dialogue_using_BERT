        {
          "id": "MDRW2100021102.1.1",
          "form": "여행!!!",
          "original_form": "여행!!!",
          "speaker_id": "2",
          "time": "20210828 15:58"
        },
        {
          "id": "MDRW2100021102.1.2",
          "form": "코로나 때문에 여행 못간지 되게 오래된 기분이에요 ㅋㅋㅋ",
          "original_form": "코로나 때문에 여행 못간지 되게 오래된 기분이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:58"
        },
        {
          "id": "MDRW2100021102.1.3",
          "form": "어머 여행 ㅜㅜ",
          "original_form": "어머 여행 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100021102.1.4",
          "form": "코로나가 이렇게 오래 갈 줄 누가 알았겠어요 그쵸?",
          "original_form": "코로나가 이렇게 오래 갈 줄 누가 알았겠어요 그쵸?",
          "speaker_id": "1",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100021102.1.5",
          "form": "그러게 말입니다",
          "original_form": "그러게 말입니다",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100021102.1.6",
          "form": "국내 여행지는",
          "original_form": "국내 여행지는",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100021102.1.7",
          "form": "제주도가 그렇게 가고싶어요 ㅎㅎ",
          "original_form": "제주도가 그렇게 가고싶어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100021102.1.8",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100021102.1.9",
          "form": "저도 제주도 다시 또 가고 싶어요 ㅜㅜ",
          "original_form": "저도 제주도 다시 또 가고 싶어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100021102.1.10",
          "form": "전에 마지막 여행이 제주도였거든요",
          "original_form": "전에 마지막 여행이 제주도였거든요",
          "speaker_id": "1",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100021102.1.11",
          "form": "제주도 정말 좋았겠어요...",
          "original_form": "제주도 정말 좋았겠어요...",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100021102.1.12",
          "form": "비행기 타고 어디든 가고싶네요!",
          "original_form": "비행기 타고 어디든 가고싶네요!",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100021102.1.13",
          "form": "몇 박 하고 오셨나요?",
          "original_form": "몇 박 하고 오셨나요?",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100021102.1.14",
          "form": "기억이 가물가물한데 2박3일인가 3박4일 정도요!",
          "original_form": "기억이 가물가물한데 2박3일인가 3박4일 정도요!",
          "speaker_id": "1",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100021102.1.15",
          "form": "거기 테마파크랑 오픈형 동물원이 기억에 남아서 또 가고싶어요 ㅜ",
          "original_form": "거기 테마파크랑 오픈형 동물원이 기억에 남아서 또 가고싶어요 ㅜ",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100021102.1.16",
          "form": "우와 말만 들어도 너무 신나요",
          "original_form": "우와 말만 들어도 너무 신나요",
          "speaker_id": "2",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100021102.1.17",
          "form": "저도 가보고싶어요... 따흐흑",
          "original_form": "저도 가보고싶어요... 따흐흑",
          "speaker_id": "2",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100021102.1.18",
          "form": "아니면 부산이라도...",
          "original_form": "아니면 부산이라도...",
          "speaker_id": "2",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100021102.1.19",
          "form": "저 부산 한 번도 안가봤거든요..",
          "original_form": "저 부산 한 번도 안가봤거든요..",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100021102.1.20",
          "form": "부산~ 막상 가보면 별거 없어요 ㅜㅜ",
          "original_form": "부산~ 막상 가보면 별거 없어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100021102.1.21",
          "form": "그래도 광안리 바닷가 경치는 좋더라구요!",
          "original_form": "그래도 광안리 바닷가 경치는 좋더라구요!",
          "speaker_id": "1",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100021102.1.22",
          "form": "아 진짜요???",
          "original_form": "아 진짜요???",
          "speaker_id": "2",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100021102.1.23",
          "form": "센텀시티 이쪽",
          "original_form": "센텀시티 이쪽",
          "speaker_id": "2",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100021102.1.24",
          "form": "완전... 고층건물 많고 삐까뻔쩍 하다고 해서",
          "original_form": "완전... 고층건물 많고 삐까뻔쩍 하다고 해서",
          "speaker_id": "2",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100021102.1.25",
          "form": "가고싶은 곳 중 하나예요 ㅎㅎㅎ",
          "original_form": "가고싶은 곳 중 하나예요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100021102.1.26",
          "form": "name1님은 어디 가고싶으신 곳 없으세요?",
          "original_form": "&name1&님은 어디 가고싶으신 곳 없으세요?",
          "speaker_id": "2",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100021102.1.27",
          "form": "저는 그냥 호텔 가고 싶네요 호캉스요",
          "original_form": "저는 그냥 호텔 가고 싶네요 호캉스요",
          "speaker_id": "1",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100021102.1.28",
          "form": "지역은 상관없고 그냥 시설 좋아서 힐링하고 싶어요",
          "original_form": "지역은 상관없고 그냥 시설 좋아서 힐링하고 싶어요",
          "speaker_id": "1",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100021102.1.29",
          "form": "그러게요 일상만 벗어날 수 있다면",
          "original_form": "그러게요 일상만 벗어날 수 있다면",
          "speaker_id": "2",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100021102.1.30",
          "form": "어디든 좋을 것 같습니다 ㅎㅎ",
          "original_form": "어디든 좋을 것 같습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100021102.1.31",
          "form": "그쵸 그래서 그냥 혼자만의 힐링 하고 싶네요",
          "original_form": "그쵸 그래서 그냥 혼자만의 힐링 하고 싶네요",
          "speaker_id": "1",
          "time": "20210829 23:54"
        }
      ]
