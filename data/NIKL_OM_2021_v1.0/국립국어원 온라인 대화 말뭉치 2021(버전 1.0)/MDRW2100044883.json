      "utterance": [
        {
          "id": "MDRW2100044883.1.1",
          "form": "쇼핑자주하세요?",
          "original_form": "쇼핑자주하세요?",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044883.1.2",
          "form": "매일할게 생기네요ㅋ",
          "original_form": "매일할게 생기네요ㅋ",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044883.1.3",
          "form": "저두요 곧 십일절이라 아끼고있어요ㅋㅋ",
          "original_form": "저두요 곧 십일절이라 아끼고있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044883.1.4",
          "form": "자꾸 여기저기서 하라고 이벤트를 하네요ㅋ",
          "original_form": "자꾸 여기저기서 하라고 이벤트를 하네요ㅋ",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044883.1.5",
          "form": "마자여 또 가격비교하다가도 시간순삭!",
          "original_form": "마자여 또 가격비교하다가도 시간순삭!",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044883.1.6",
          "form": "어제 뭐사셨어요ㅋㅋ",
          "original_form": "어제 뭐사셨어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044883.1.7",
          "form": "아몬드요",
          "original_form": "아몬드요",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044883.1.8",
          "form": "장도 오만어치나 배달시키고ㅋㅋ",
          "original_form": "장도 오만어치나 배달시키고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044883.1.9",
          "form": "와 오만원이면 혜택쎄게주겠는걸여ㅋㅋㅋ",
          "original_form": "와 오만원이면 혜택쎄게주겠는걸여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044883.1.10",
          "form": "저는 선없는줄넘기 샀는데",
          "original_form": "저는 선없는줄넘기 샀는데",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044883.1.11",
          "form": "택배도안뜯었어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "택배도안뜯었어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044883.1.12",
          "form": "ㅋㅋㅋ 사놓고 원래바로 안뜯져",
          "original_form": "ㅋㅋㅋ 사놓고 원래바로 안뜯져",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044883.1.13",
          "form": "추석선물도 혹시 준비하시나여?",
          "original_form": "추석선물도 혹시 준비하시나여?",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044883.1.14",
          "form": "전 선물은 따로 안해요 현금으로ㅋ",
          "original_form": "전 선물은 따로 안해요 현금으로ㅋ",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044883.1.15",
          "form": "현금이 최고인듯요ㅋ 회사에서도 돈으로주면좋겠어요",
          "original_form": "현금이 최고인듯요ㅋ 회사에서도 돈으로주면좋겠어요",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044883.1.16",
          "form": "매번 샴푸세트입니다ㅋㅋㅋㄲㅋ",
          "original_form": "매번 샴푸세트입니다ㅋㅋㅋㄲㅋ",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044883.1.17",
          "form": "저도 어제 그거 하나들고왓어요\n무거워서 혼낫네여",
          "original_form": "저도 어제 그거 하나들고왓어요\n무거워서 혼낫네여",
          "speaker_id": "2",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100044883.1.18",
          "form": "덕분에 샴푸구매는 안하네요 그래도 ㅎㅎ",
          "original_form": "덕분에 샴푸구매는 안하네요 그래도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:04"
        }
