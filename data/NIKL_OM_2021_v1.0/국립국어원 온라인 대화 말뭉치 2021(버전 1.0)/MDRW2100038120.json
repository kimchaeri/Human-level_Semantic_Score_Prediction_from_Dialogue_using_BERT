      "utterance": [
        {
          "id": "MDRW2100038120.1.1",
          "form": "가격은 좀 있는데",
          "original_form": "가격은 좀 있는데",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100038120.1.2",
          "form": "크리스찬 한센 유산균인데",
          "original_form": "크리스찬 한센 유산균인데",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100038120.1.3",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038120.1.4",
          "form": "첨들어봐여",
          "original_form": "첨들어봐여",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038120.1.5",
          "form": "알약으로 되어있거든요?",
          "original_form": "알약으로 되어있거든요?",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038120.1.6",
          "form": "빛 받으면 안되고",
          "original_form": "빛 받으면 안되고",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038120.1.7",
          "form": "알약이면편하겠네요",
          "original_form": "알약이면편하겠네요",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038120.1.8",
          "form": "냉장보관하는게 좋은 유산균인데",
          "original_form": "냉장보관하는게 좋은 유산균인데",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038120.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038120.1.10",
          "form": "호오",
          "original_form": "호오",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038120.1.11",
          "form": "근데 저 이거 먹고 광명찾았어요...!!!",
          "original_form": "근데 저 이거 먹고 광명찾았어요...!!!",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038120.1.12",
          "form": "광명이라니! 어떻게달랐어요",
          "original_form": "광명이라니! 어떻게달랐어요",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038120.1.13",
          "form": "좀 더러운 이야기지만",
          "original_form": "좀 더러운 이야기지만",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038120.1.14",
          "form": "저도 과민성대장증후군이어서",
          "original_form": "저도 과민성대장증후군이어서",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038120.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ괜찮ㅇ아여",
          "original_form": "ㅋㅋㅋㅋㅋㅋ괜찮ㅇ아여",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038120.1.16",
          "form": "형체가 있는 변을 본 적이 거의 없었거든요...?",
          "original_form": "형체가 있는 변을 본 적이 거의 없었거든요...?",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038120.1.17",
          "form": "흑흑...알죠ㅜㅜ",
          "original_form": "흑흑...알죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038120.1.18",
          "form": "근데 저거 먹고나서 드디어",
          "original_form": "근데 저거 먹고나서 드디어",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038120.1.19",
          "form": "구렁이를 낳았어요....!!!",
          "original_form": "구렁이를 낳았어요....!!!",
          "speaker_id": "2",
          "time": "20210901 23:26"
        }
      ]
