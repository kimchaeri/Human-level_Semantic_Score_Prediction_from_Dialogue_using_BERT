      "utterance": [
        {
          "id": "MDRW2100045189.1.1",
          "form": "ㅇㅇㅇ 그것도 에케비..",
          "original_form": "ㅇㅇㅇ 그것도 에케비..",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045189.1.2",
          "form": "그래서 사쿠라가 ㅇㅠ투버 소속사들어간다그랬을대",
          "original_form": "그래서 사쿠라가 ㅇㅠ투버 소속사들어간다그랬을대",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045189.1.3",
          "form": "그래 유투버를 하지그러냐했는데",
          "original_form": "그래 유투버를 하지그러냐했는데",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045189.1.4",
          "form": "역시나",
          "original_form": "역시나",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045189.1.5",
          "form": "유튜버 소속사?",
          "original_form": "유튜버 소속사?",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045189.1.6",
          "form": "ㅇㅇ일본에서 그쪽이란 계약한다글썌었어",
          "original_form": "ㅇㅇ일본에서 그쪽이란 계약한다글썌었어",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045189.1.7",
          "form": "결국엔 하이브..",
          "original_form": "결국엔 하이브..",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045189.1.8",
          "form": "다들 여배우 소속사가냐",
          "original_form": "다들 여배우 소속사가냐",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045189.1.9",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045189.1.10",
          "form": "했는데",
          "original_form": "했는데",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045189.1.11",
          "form": "결국 ㅋㅋㅋㅎㅎㅎㅎㅎ",
          "original_form": "결국 ㅋㅋㅋㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045189.1.12",
          "form": "힛뱅피그네",
          "original_form": "힛뱅피그네",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045189.1.13",
          "form": "아니왜지",
          "original_form": "아니왜지",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045189.1.14",
          "form": "나믿에이를 왜데려가지",
          "original_form": "나믿에이를 왜데려가지",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045189.1.15",
          "form": "그것도 주작돌",
          "original_form": "그것도 주작돌",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045189.1.16",
          "form": "중국인이 제일 싫긴 하지만",
          "original_form": "중국인이 제일 싫긴 하지만",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045189.1.17",
          "form": "일본도 싫거든요",
          "original_form": "일본도 싫거든요",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045189.1.18",
          "form": "당근이져",
          "original_form": "당근이져",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045189.1.19",
          "form": "일본도 만만치않아",
          "original_form": "일본도 만만치않아",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045189.1.20",
          "form": "요새 중국한테 쩌밀렸는데",
          "original_form": "요새 중국한테 쩌밀렸는데",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045189.1.21",
          "form": "구관이명관이라고",
          "original_form": "구관이명관이라고",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045189.1.22",
          "form": "잘되면 잘되는대로 사쿠라와 닛뽄과 제이뽑뿌덕에 흥했다고",
          "original_form": "잘되면 잘되는대로 사쿠라와 닛뽄과 제이뽑뿌덕에 흥했다고",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045189.1.23",
          "form": "",
          "original_form": "{censored}",
