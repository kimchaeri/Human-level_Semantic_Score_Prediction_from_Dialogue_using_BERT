      "utterance": [
        {
          "id": "MDRW2100044421.1.1",
          "form": "좋아해요! 자주 드세요?",
          "original_form": "좋아해요! 자주 드세요?",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044421.1.2",
          "form": "하루에 몇잔 드시나요?",
          "original_form": "하루에 몇잔 드시나요?",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044421.1.3",
          "form": "두세잔이요 님은요?",
          "original_form": "두세잔이요 님은요?",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044421.1.4",
          "form": "저도 두세잔",
          "original_form": "저도 두세잔",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044421.1.5",
          "form": "주로 어떤 음료드세요",
          "original_form": "주로 어떤 음료드세요",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044421.1.6",
          "form": "주로 뜨아",
          "original_form": "주로 뜨아",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044421.1.7",
          "form": "전 아아파예요",
          "original_form": "전 아아파예요",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044421.1.8",
          "form": "여름에도 뜨아인가요",
          "original_form": "여름에도 뜨아인가요",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044421.1.9",
          "form": "아하 얼죽아ㅋ기",
          "original_form": "아하 얼죽아ㅋ기",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044421.1.10",
          "form": "네 여름에도 뜨아에요",
          "original_form": "네 여름에도 뜨아에요",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044421.1.11",
          "form": "ㄷㄷ독한 분이시군요",
          "original_form": "ㄷㄷ독한 분이시군요",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044421.1.12",
          "form": "님도 만만치 않으신데용",
          "original_form": "님도 만만치 않으신데용",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044421.1.13",
          "form": "커피 외에 좋아하시는 음료는?",
          "original_form": "커피 외에 좋아하시는 음료는?",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044421.1.14",
          "form": "흑당밀크티요!",
          "original_form": "흑당밀크티요!",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044421.1.15",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044421.1.16",
          "form": "헐 대박 저도요",
          "original_form": "헐 대박 저도요",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044421.1.17",
          "form": "어디거 좋아하세요?",
          "original_form": "어디거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044421.1.18",
          "form": "카페일리터 아시나요",
          "original_form": "카페일리터 아시나요",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044421.1.19",
          "form": "알죠 저희집 근처에도 있어요. 흑당밀크티에 와플 자주먹어요ㅋㅋ",
          "original_form": "알죠 저희집 근처에도 있어요. 흑당밀크티에 와플 자주먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044421.1.20",
          "form": "단 거 좋아하세요?",
          "original_form": "단 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044421.1.21",
          "form": "잘 못먹어요ㅜ",
          "original_form": "잘 못먹어요ㅜ",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044421.1.22",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044421.1.23",
          "form": "저는 뭐.. 엄청 잘 먹죠",
          "original_form": "저는 뭐.. 엄청 잘 먹죠",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044421.1.24",
          "form": "케이크 다섯개 가능",
          "original_form": "케이크 다섯개 가능",
