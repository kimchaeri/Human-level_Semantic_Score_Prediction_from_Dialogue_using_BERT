        },
        {
          "id": "MDRW2100030250.1.2",
          "form": "냉면이죠",
          "original_form": "냉면이죠",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100030250.1.3",
          "form": "화화는 서비스가 좋아요",
          "original_form": "화화는 서비스가 좋아요",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100030250.1.4",
          "form": "비냉인가요 물냉인가요",
          "original_form": "비냉인가요 물냉인가요",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100030250.1.5",
          "form": "양도 푸지고요",
          "original_form": "양도 푸지고요",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100030250.1.6",
          "form": "체인점인데 살짝씩 달라묘",
          "original_form": "체인점인데 살짝씩 달라묘",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100030250.1.7",
          "form": "비냉이요  식초 많이",
          "original_form": "비냉이요  식초 많이",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030250.1.8",
          "form": "대신동지점은맛나묘",
          "original_form": "대신동지점은맛나묘",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030250.1.9",
          "form": "대신동이 맛집이지요",
          "original_form": "대신동이 맛집이지요",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030250.1.10",
          "form": "줄서서 먹었ㄴㅇㄴ데",
          "original_form": "줄서서 먹었ㄴㅇㄴ데",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030250.1.11",
          "form": "안간지오래됐네요",
          "original_form": "안간지오래됐네요",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030250.1.12",
          "form": "요즘도 줄서나요",
          "original_form": "요즘도 줄서나요",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030250.1.13",
          "form": "신평에도 밌구요",
          "original_form": "신평에도 밌구요",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030250.1.14",
          "form": "아뇨 줄안서요",
          "original_form": "아뇨 줄안서요",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030250.1.15",
          "form": "고기냄샌ㅇㄴ 죽입니다",
          "original_form": "고기냄샌ㅇㄴ 죽입니다",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030250.1.16",
          "form": "신평에 맛집많죠",
          "original_form": "신평에 맛집많죠",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030250.1.17",
          "form": "영진국밥가고싶네요",
          "original_form": "영진국밥가고싶네요",
          "speaker_id": "2",
          "time": "20210903 23:17"
