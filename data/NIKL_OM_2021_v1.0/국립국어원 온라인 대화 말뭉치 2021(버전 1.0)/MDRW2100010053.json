      "utterance": [
        {
          "id": "MDRW2100010053.1.1",
          "form": "수페르가 세일하길래",
          "original_form": "수페르가 세일하길래",
          "speaker_id": "2",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010053.1.2",
          "form": "두켤레 샀지롱",
          "original_form": "두켤레 샀지롱",
          "speaker_id": "2",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010053.1.3",
          "form": "두켤레나요?",
          "original_form": "두켤레나요?",
          "speaker_id": "1",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010053.1.4",
          "form": "응 근데 두켤레에 3만원",
          "original_form": "응 근데 두켤레에 3만원",
          "speaker_id": "2",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010053.1.5",
          "form": "와 싸게샀네요",
          "original_form": "와 싸게샀네요",
          "speaker_id": "1",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100010053.1.6",
          "form": "누구거 샀어?",
          "original_form": "누구거 샀어?",
          "speaker_id": "1",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100010053.1.7",
          "form": "내거ㅎㅎㅎㅎ",
          "original_form": "내거ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100010053.1.8",
          "form": "넌 운동화많잖아ㅎㅎ",
          "original_form": "넌 운동화많잖아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100010053.1.9",
          "form": "흥ㅠㅜㅜㅜ",
          "original_form": "흥ㅠㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 13:38"
        },
        {
          "id": "MDRW2100010053.1.10",
          "form": "코듀로이 겨울용 하나랑",
          "original_form": "코듀로이 겨울용 하나랑",
          "speaker_id": "2",
          "time": "20210823 13:38"
        },
        {
          "id": "MDRW2100010053.1.11",
          "form": "무슨 소가죽 하나샀는데",
          "original_form": "무슨 소가죽 하나샀는데",
          "speaker_id": "2",
          "time": "20210823 13:38"
        },
        {
          "id": "MDRW2100010053.1.12",
          "form": "빨리 신어보고싶다",
          "original_form": "빨리 신어보고싶다",
          "speaker_id": "2",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100010053.1.13",
          "form": "부럽다 내것도 사주지",
          "original_form": "부럽다 내것도 사주지",
          "speaker_id": "1",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100010053.1.14",
          "form": "넌 몇달전에 사줬잖아",
          "original_form": "넌 몇달전에 사줬잖아",
          "speaker_id": "2",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100010053.1.15",
          "form": "그래두...",
          "original_form": "그래두...",
          "speaker_id": "1",
          "time": "20210823 13:41"
        },
        {
          "id": "MDRW2100010053.1.16",
          "form": "다양하게신으면 좋자나",
          "original_form": "다양하게신으면 좋자나",
          "speaker_id": "1",
          "time": "20210823 13:41"
        },
        {
          "id": "MDRW2100010053.1.17",
          "form": "알겠어 아직 세일기간 안끝났으니까",
          "original_form": "알겠어 아직 세일기간 안끝났으니까",
          "speaker_id": "2",
          "time": "20210823 13:41"
        },
        {
          "id": "MDRW2100010053.1.18",
          "form": "사줄까?",
          "original_form": "사줄까?",
          "speaker_id": "2",
          "time": "20210823 13:41"
        },
        {
          "id": "MDRW2100010053.1.19",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "1",
          "time": "20210823 13:41"
        },
        {
          "id": "MDRW2100010053.1.20",
          "form": "네네네네",
          "original_form": "네네네네",
          "speaker_id": "1",
          "time": "20210823 13:42"
        },
        {
          "id": "MDRW2100010053.1.21",
          "form": "사주세요",
          "original_form": "사주세요",
          "speaker_id": "1",
