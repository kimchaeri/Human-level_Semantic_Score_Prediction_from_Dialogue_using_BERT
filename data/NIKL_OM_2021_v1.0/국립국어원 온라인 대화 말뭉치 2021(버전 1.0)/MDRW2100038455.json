      "utterance": [
        {
          "id": "MDRW2100038455.1.1",
          "form": "새벽의저를욕하며ㅋㅋㄱㅋ으으으",
          "original_form": "새벽의저를욕하며ㅋㅋㄱㅋ으으으",
          "speaker_id": "2",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100038455.1.2",
          "form": "가을옷은장만해두셨나용?",
          "original_form": "가을옷은장만해두셨나용?",
          "speaker_id": "2",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100038455.1.3",
          "form": "저는 땀쟁이라서",
          "original_form": "저는 땀쟁이라서",
          "speaker_id": "1",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100038455.1.4",
          "form": "아직 여름옷 입고인어욤",
          "original_form": "아직 여름옷 입고인어욤",
          "speaker_id": "1",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100038455.1.5",
          "form": "가을옷은 추석쯤 본격적으로 지를까해요!!!!",
          "original_form": "가을옷은 추석쯤 본격적으로 지를까해요!!!!",
          "speaker_id": "1",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100038455.1.6",
          "form": "날씨봐가면서!!!!",
          "original_form": "날씨봐가면서!!!!",
          "speaker_id": "1",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100038455.1.7",
          "form": "자켓사구싶아요",
          "original_form": "자켓사구싶아요",
          "speaker_id": "1",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100038455.1.8",
          "form": "저두여름옷입니다ㅋㅋㄱ오늘은쪼끔쌀쌀하네요ㅋㅋㄱㅋㅋ\r\n자켓이쁘지요ㅠㅡㅠ",
          "original_form": "저두여름옷입니다ㅋㅋㄱ오늘은쪼끔쌀쌀하네요ㅋㅋㄱㅋㅋ\r\n자켓이쁘지요ㅠㅡㅠ",
          "speaker_id": "2",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100038455.1.9",
          "form": "아침에 나오면",
          "original_form": "아침에 나오면",
          "speaker_id": "1",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100038455.1.10",
          "form": "쌀쌀하더라구용",
          "original_form": "쌀쌀하더라구용",
          "speaker_id": "1",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100038455.1.11",
          "form": "가을이오긴오나봐요  구두잘신으면 이쁜구두도사고싶은데 발이도저히버티질못해요ㅡ..",
          "original_form": "가을이오긴오나봐요  구두잘신으면 이쁜구두도사고싶은데 발이도저히버티질못해요ㅡ..",
          "speaker_id": "2",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100038455.1.12",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100038455.1.13",
          "form": "맞아요 발도아프고 무릎도아파요",
          "original_form": "맞아요 발도아프고 무릎도아파요",
          "speaker_id": "1",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100038455.1.14",
          "form": "대학생때 아무생각없이 혹사시켜서 흑흑",
          "original_form": "대학생때 아무생각없이 혹사시켜서 흑흑",
          "speaker_id": "1",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100038455.1.15",
          "form": "아니그땐 막 구두신고 뛰고그랬거든요??!!  \r\n지금은 걷기도힘들어요...",
          "original_form": "아니그땐 막 구두신고 뛰고그랬거든요??!!  \r\n지금은 걷기도힘들어요...",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038455.1.16",
          "form": "ㅋㅋㄱㅋㅋㄱㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㄱㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038455.1.17",
          "form": "한번맘먹고신고나가면",
          "original_form": "한번맘먹고신고나가면",
          "speaker_id": "2",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100038455.1.18",
          "form": "집에올땐그냥맨발로올때도많아요 발더러워지든말든",
          "original_form": "집에올땐그냥맨발로올때도많아요 발더러워지든말든",
          "speaker_id": "2",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100038455.1.19",
          "form": "하 대학생때 클럽갓다가 집에들어올때",
          "original_form": "하 대학생때 클럽갓다가 집에들어올때",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100038455.1.20",
          "form": "맨발로왓어요....",
          "original_form": "맨발로왓어요....",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100038455.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100038455.1.22",
          "form": "와 클럽..잘나가셨어...",
          "original_form": "와 클럽..잘나가셨어...",
          "speaker_id": "2",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100038455.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100038455.1.24",
          "form": "그때 무릎이랑 발쫌 애껴놓을껄 왜그랫을까요ㅠㅠ",
          "original_form": "그때 무릎이랑 발쫌 애껴놓을껄 왜그랫을까요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100038455.1.25",
          "form": "그니까요..파스겁나사붙여야해요지금..",
          "original_form": "그니까요..파스겁나사붙여야해요지금..",
          "speaker_id": "2",
          "time": "20210902 16:31"
