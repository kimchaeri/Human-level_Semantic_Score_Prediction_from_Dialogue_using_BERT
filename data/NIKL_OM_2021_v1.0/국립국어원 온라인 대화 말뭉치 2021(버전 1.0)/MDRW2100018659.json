      "utterance": [
        {
          "id": "MDRW2100018659.1.1",
          "form": "저도 원래 게하로 잘 안가거든요",
          "original_form": "저도 원래 게하로 잘 안가거든요",
          "speaker_id": "2",
          "time": "20210827 10:07"
        },
        {
          "id": "MDRW2100018659.1.2",
          "form": "한번 가봤는데 파티가 진짜 너무 재밌는거에요",
          "original_form": "한번 가봤는데 파티가 진짜 너무 재밌는거에요",
          "speaker_id": "2",
          "time": "20210827 10:07"
        },
        {
          "id": "MDRW2100018659.1.3",
          "form": "궁금하다",
          "original_form": "궁금하다",
          "speaker_id": "1",
          "time": "20210827 10:08"
        },
        {
          "id": "MDRW2100018659.1.4",
          "form": "무슨파티해써요",
          "original_form": "무슨파티해써요",
          "speaker_id": "1",
          "time": "20210827 10:08"
        },
        {
          "id": "MDRW2100018659.1.5",
          "form": "일박이일이었는데 둘째날엔 파티에서 만난 사람들이랑 같이 밥먹으러가고, 카페갔다가 비와서 숙소 돌아와서 게임했어요 ㅋㅋ",
          "original_form": "일박이일이었는데 둘째날엔 파티에서 만난 사람들이랑 같이 밥먹으러가고, 카페갔다가 비와서 숙소 돌아와서 게임했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:08"
        },
        {
          "id": "MDRW2100018659.1.6",
          "form": "모르는 사람들이랑 즐기는 하루",
          "original_form": "모르는 사람들이랑 즐기는 하루",
          "speaker_id": "1",
          "time": "20210827 10:08"
        },
        {
          "id": "MDRW2100018659.1.7",
          "form": "꽤 재밌게써요",
          "original_form": "꽤 재밌게써요",
          "speaker_id": "1",
          "time": "20210827 10:08"
        },
        {
          "id": "MDRW2100018659.1.8",
          "form": "그냥 술마시면서 대화하는데 서로서로 자리 옮기면서 재밌더라구요",
          "original_form": "그냥 술마시면서 대화하는데 서로서로 자리 옮기면서 재밌더라구요",
          "speaker_id": "2",
          "time": "20210827 10:08"
        },
        {
          "id": "MDRW2100018659.1.9",
          "form": "게임은 뭐했나요",
          "original_form": "게임은 뭐했나요",
          "speaker_id": "1",
          "time": "20210827 10:08"
        },
        {
          "id": "MDRW2100018659.1.10",
          "form": "술게임하셨나",
          "original_form": "술게임하셨나",
          "speaker_id": "1",
          "time": "20210827 10:08"
        },
        {
          "id": "MDRW2100018659.1.11",
          "form": "게임은 맨정신에 한거라 ㅋㅋㅋㅋㅋ",
          "original_form": "게임은 맨정신에 한거라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:09"
        },
        {
          "id": "MDRW2100018659.1.12",
          "form": "클레오파트라 같은거 했어요...",
          "original_form": "클레오파트라 같은거 했어요...",
          "speaker_id": "2",
          "time": "20210827 10:09"
        },
        {
          "id": "MDRW2100018659.1.13",
          "form": "클레오 파트라 클레오파트라",
          "original_form": "클레오 파트라 클레오파트라",
          "speaker_id": "1",
          "time": "20210827 10:09"
        },
        {
          "id": "MDRW2100018659.1.14",
          "form": "세상에서 제일가는 포테이토칩",
          "original_form": "세상에서 제일가는 포테이토칩",
          "speaker_id": "1",
          "time": "20210827 10:09"
        },
        {
          "id": "MDRW2100018659.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋㄱㅋㄱㅋㄱㅋㅋㄱㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋㄱㅋㄱㅋㄱㅋㅋㄱㅋㄱ",
          "speaker_id": "2",
          "time": "20210827 10:09"
        },
        {
          "id": "MDRW2100018659.1.16",
          "form": "이기셨나요",
          "original_form": "이기셨나요",
          "speaker_id": "1",
          "time": "20210827 10:09"
        },
        {
          "id": "MDRW2100018659.1.17",
          "form": "아녀.. 게임 못해요..",
          "original_form": "아녀.. 게임 못해요..",
          "speaker_id": "2",
          "time": "20210827 10:09"
        },
        {
          "id": "MDRW2100018659.1.18",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "1",
          "time": "20210827 10:09"
        },
        {
          "id": "MDRW2100018659.1.19",
          "form": "벌칙같은건있었어요?",
          "original_form": "벌칙같은건있었어요?",
          "speaker_id": "1",
          "time": "20210827 10:10"
        },
        {
          "id": "MDRW2100018659.1.20",
          "form": "손목맞기!!",
          "original_form": "손목맞기!!",
          "speaker_id": "2",
          "time": "20210827 10:10"
        },
        {
          "id": "MDRW2100018659.1.21",
          "form": "아아악",
          "original_form": "아아악",
          "speaker_id": "1",
          "time": "20210827 10:10"
        },
        {
          "id": "MDRW2100018659.1.22",
          "form": "근데 진짜 코로나 끝나면 게하 파티 한번 다녀와보세요",
          "original_form": "근데 진짜 코로나 끝나면 게하 파티 한번 다녀와보세요",
          "speaker_id": "2",
          "time": "20210827 10:10"
        },
        {
          "id": "MDRW2100018659.1.23",
          "form": "아프겠다",
          "original_form": "아프겠다",
          "speaker_id": "1",
          "time": "20210827 10:10"
        },
        {
          "id": "MDRW2100018659.1.24",
          "form": "진짜 재밌어요",
          "original_form": "진짜 재밌어요",
          "speaker_id": "2",
          "time": "20210827 10:10"
        },
        {
          "id": "MDRW2100018659.1.25",
          "form": "네 꼭 한번 가봐야겠어요",
          "original_form": "네 꼭 한번 가봐야겠어요",
          "speaker_id": "1",
          "time": "20210827 10:10"
        }
      ]
