      "utterance": [
        {
          "id": "MDRW2100015303.1.1",
          "form": "안녕하세요 어떤 음식 좋아하세요?",
          "original_form": "안녕하세요 어떤 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015303.1.2",
          "form": "파스타용",
          "original_form": "파스타용",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015303.1.3",
          "form": "name1님은용",
          "original_form": "&name1&님은용",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015303.1.4",
          "form": "저는 샤브샤브요",
          "original_form": "저는 샤브샤브요",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015303.1.5",
          "form": "양식 좋아하세요?",
          "original_form": "양식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015303.1.6",
          "form": "네네네 조아해요 사실 다좋아해요",
          "original_form": "네네네 조아해요 사실 다좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015303.1.7",
          "form": "샤브샤브도 좋아해요",
          "original_form": "샤브샤브도 좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015303.1.8",
          "form": "오늘 저녁은 뭐 드셨어요?",
          "original_form": "오늘 저녁은 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015303.1.9",
          "form": "등촌샤브샤브같은덴 볶음밥이 맛잇고",
          "original_form": "등촌샤브샤브같은덴 볶음밥이 맛잇고",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015303.1.10",
          "form": "오늘저녁 감바스요",
          "original_form": "오늘저녁 감바스요",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015303.1.11",
          "form": "맛있으셨겠네요",
          "original_form": "맛있으셨겠네요",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015303.1.12",
          "form": "넹 님은요 ㅋㅋ",
          "original_form": "넹 님은요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015303.1.13",
          "form": "저는 김치찌개먹었어요",
          "original_form": "저는 김치찌개먹었어요",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015303.1.14",
          "form": "고기 듬뿍이요 ㅎㅎ",
          "original_form": "고기 듬뿍이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015303.1.15",
          "form": "오우 고기듬뿍 좋다",
          "original_form": "오우 고기듬뿍 좋다",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015303.1.16",
          "form": "해드셨어요?",
          "original_form": "해드셨어요?",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015303.1.17",
          "form": "네 요리못하지만 그래도 해먹었어요 name2님은 요리잘하시나요?",
          "original_form": "네 요리못하지만 그래도 해먹었어요 &name2&님은 요리잘하시나요?",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015303.1.18",
          "form": "ㅋㅋ 잘한다고 하긴 애매해요 거의 돼있는걸 해먹어서 ㅋㅋ 반조리라고하죠",
          "original_form": "ㅋㅋ 잘한다고 하긴 애매해요 거의 돼있는걸 해먹어서 ㅋㅋ 반조리라고하죠",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015303.1.19",
          "form": "잘하시는 요리있나요",
          "original_form": "잘하시는 요리있나요",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015303.1.20",
          "form": "그나마 김치볶음밥이요 ㅎ",
          "original_form": "그나마 김치볶음밥이요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 22:44"
        }
