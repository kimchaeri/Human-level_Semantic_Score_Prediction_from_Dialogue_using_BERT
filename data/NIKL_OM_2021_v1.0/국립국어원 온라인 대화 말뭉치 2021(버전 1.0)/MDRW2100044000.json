        {
          "id": "MDRW2100044000.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100044000.1.2",
          "form": "코로나끝남 어디 여행가고싶으세요?",
          "original_form": "코로나끝남 어디 여행가고싶으세요?",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100044000.1.3",
          "form": "해외여행이라면 가까운 대만 가보고 싶어요",
          "original_form": "해외여행이라면 가까운 대만 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044000.1.4",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044000.1.5",
          "form": "대만 디저트 그립네요",
          "original_form": "대만 디저트 그립네요",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044000.1.6",
          "form": "아직 한 번도 못 가봤거든요",
          "original_form": "아직 한 번도 못 가봤거든요",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044000.1.7",
          "form": "대만에 맛있는 거 많다 그러더라구요 ㅋㅋ",
          "original_form": "대만에 맛있는 거 많다 그러더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044000.1.8",
          "form": "ㅎㅎ육우면같은거 좋아하시면!!",
          "original_form": "ㅎㅎ육우면같은거 좋아하시면!!",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044000.1.9",
          "form": "저는 향신료가 안맞긴했어요",
          "original_form": "저는 향신료가 안맞긴했어요",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044000.1.10",
          "form": "ㅋㅋ 향신료 때문에 반응이 갈리긴 하죠",
          "original_form": "ㅋㅋ 향신료 때문에 반응이 갈리긴 하죠",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044000.1.11",
          "form": "대만 가셨을 때 어디어디 가셨어요?",
          "original_form": "대만 가셨을 때 어디어디 가셨어요?",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044000.1.12",
          "form": "타이베이 근처에 오래있었어요:)",
          "original_form": "타이베이 근처에 오래있었어요:)",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044000.1.13",
          "form": "그럼 대중교통으로만 다닐만 하셨겠어요",
          "original_form": "그럼 대중교통으로만 다닐만 하셨겠어요",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044000.1.14",
          "form": "네~지하철이 우리나라만큼 잘돼있더라구요",
          "original_form": "네~지하철이 우리나라만큼 잘돼있더라구요",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044000.1.15",
          "form": "외국 지하철 타면 또 재밌더라구요 ㅋㅋ",
          "original_form": "외국 지하철 타면 또 재밌더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044000.1.16",
          "form": "맞아요 더러운데가 많긴하지만 ㅜ",
          "original_form": "맞아요 더러운데가 많긴하지만 ㅜ",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044000.1.17",
          "form": "여행갈수록 한국 최고에요",
          "original_form": "여행갈수록 한국 최고에요",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044000.1.18",
          "form": "ㅋㅋ 그건 그래요",
          "original_form": "ㅋㅋ 그건 그래요",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044000.1.19",
          "form": "깨끗하고 빠르고",
          "original_form": "깨끗하고 빠르고",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
