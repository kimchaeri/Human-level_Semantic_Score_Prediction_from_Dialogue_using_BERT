      "utterance": [
        {
          "id": "MDRW2100033876.1.1",
          "form": "고르기 너무 어려운데요ㅎ",
          "original_form": "고르기 너무 어려운데요ㅎ",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100033876.1.2",
          "form": "목살이 좀더 품질을 타니 만만한 삼겹살을 고를게요",
          "original_form": "목살이 좀더 품질을 타니 만만한 삼겹살을 고를게요",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100033876.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100033876.1.4",
          "form": "ㅋㅋㅋ 저도 삼겹살이랍니다ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 저도 삼겹살이랍니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100033876.1.5",
          "form": "흑돼지는 어떠세요?",
          "original_form": "흑돼지는 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100033876.1.6",
          "form": "없어서 못 먹죠",
          "original_form": "없어서 못 먹죠",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100033876.1.7",
          "form": "멜젓 꼭 있어야 해요!!",
          "original_form": "멜젓 꼭 있어야 해요!!",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100033876.1.8",
          "form": "어우 너무좋죠",
          "original_form": "어우 너무좋죠",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033876.1.9",
          "form": "저는 제주도에서 흑돼지먹고싶어요",
          "original_form": "저는 제주도에서 흑돼지먹고싶어요",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033876.1.10",
          "form": "제주도 가면 칠돈가 꼭 들렀는데",
          "original_form": "제주도 가면 칠돈가 꼭 들렀는데",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100033876.1.11",
          "form": "코로나 이후로 못가서 더 그립네요ㅜ",
          "original_form": "코로나 이후로 못가서 더 그립네요ㅜ",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100033876.1.12",
          "form": "연돈 혹시 가보셨나요",
          "original_form": "연돈 혹시 가보셨나요",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100033876.1.13",
          "form": "아니요 연돈은",
          "original_form": "아니요 연돈은",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100033876.1.14",
          "form": "못가봤네요ㅠ",
          "original_form": "못가봤네요ㅠ",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100033876.1.15",
          "form": "가보셯ㅇㄴ어요?",
          "original_form": "가보셯ㅇㄴ어요?",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100033876.1.16",
          "form": "저도 못가봤어요",
          "original_form": "저도 못가봤어요",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100033876.1.17",
          "form": "근데 제주 여행가서 너무 시간 허비하기는 그래서 평생 못가볼 것 같아요",
          "original_form": "근데 제주 여행가서 너무 시간 허비하기는 그래서 평생 못가볼 것 같아요",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100033876.1.18",
          "form": "서울만 되어도 주말에 시도해보겠는데ㅜ",
          "original_form": "서울만 되어도 주말에 시도해보겠는데ㅜ",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100033876.1.19",
          "form": "맞아요ㅋㅋㅋ 아예 한달살기하지않는이상",
          "original_form": "맞아요ㅋㅋㅋ 아예 한달살기하지않는이상",
          "speaker_id": "1",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100033876.1.20",
          "form": "맛집은 웨이팅이너무심해요",
          "original_form": "맛집은 웨이팅이너무심해요",
          "speaker_id": "1",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100033876.1.21",
          "form": "그래도 인기가 유지되는걸 보면 연돈이 대단하긴 한가봐요",
          "original_form": "그래도 인기가 유지되는걸 보면 연돈이 대단하긴 한가봐요",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100033876.1.22",
          "form": "티비에서 과정도 보여주고 그랬는데 다른데서 따라하지도  않고",
          "original_form": "티비에서 과정도 보여주고 그랬는데 다른데서 따라하지도  않고",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100033876.1.23",
          "form": "손이 많이 가서 그런가",
          "original_form": "손이 많이 가서 그런가",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100033876.1.24",
          "form": "나중에 연돈2호점 서울점이 나오길 고대해요",
          "original_form": "나중에 연돈2호점 서울점이 나오길 고대해요",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100033876.1.25",
          "form": "그러게요ㄷㄷ어마어마하게 맛있나봐요",
          "original_form": "그러게요ㄷㄷ어마어마하게 맛있나봐요",
          "speaker_id": "1",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100033876.1.26",
          "form": "무슨맛일지 진짜궁금해요ㅋㅋㅋ",
          "original_form": "무슨맛일지 진짜궁금해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100033876.1.27",
          "form": "연돈은 숟가락으로도 잘리는 부드러움이랬었는데",
          "original_form": "연돈은 숟가락으로도 잘리는 부드러움이랬었는데",
          "speaker_id": "2",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100033876.1.28",
          "form": "쫄깃한 흑돼지로는 무슨 식감을 냈을지ㅎ",
          "original_form": "쫄깃한 흑돼지로는 무슨 식감을 냈을지ㅎ",
          "speaker_id": "2",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100033876.1.29",
          "form": "말하고보니 돈가스가 당기네요",
          "original_form": "말하고보니 돈가스가 당기네요",
          "speaker_id": "2",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100033876.1.30",
          "form": "와우 숟가락으로요?",
          "original_form": "와우 숟가락으로요?",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100033876.1.31",
          "form": "대단하네요ㅋㅋㅋㅋ",
          "original_form": "대단하네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100033876.1.32",
          "form": "사실 저는",
          "original_form": "사실 저는",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100033876.1.33",
          "form": "옛날왕돈까스도좋아해요",
          "original_form": "옛날왕돈까스도좋아해요",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100033876.1.34",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100033876.1.35",
          "form": "실은 일식돈까스보다 왕돈까스파예요",
          "original_form": "실은 일식돈까스보다 왕돈까스파예요",
          "speaker_id": "2",
          "time": "20210825 15:27"
        }
      ]
