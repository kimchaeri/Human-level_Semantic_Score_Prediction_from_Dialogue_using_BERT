      "utterance": [
        {
          "id": "MDRW2100046221.1.1",
          "form": "초밥너무좋죠",
          "original_form": "초밥너무좋죠",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100046221.1.2",
          "form": "초밥 너무 맛있는데 너무 빨리 소화돼요ㅠ",
          "original_form": "초밥 너무 맛있는데 너무 빨리 소화돼요ㅠ",
          "speaker_id": "1",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046221.1.3",
          "form": "저는 그래서 회전초밥집 이런거가면많이 먹게되요",
          "original_form": "저는 그래서 회전초밥집 이런거가면많이 먹게되요",
          "speaker_id": "2",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046221.1.4",
          "form": "저 진짜 접시 쌓아놓고 먹어요",
          "original_form": "저 진짜 접시 쌓아놓고 먹어요",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046221.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046221.1.6",
          "form": "평소에도 많이 드시는 편이세요?",
          "original_form": "평소에도 많이 드시는 편이세요?",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046221.1.7",
          "form": "좋아하는건 많이 먹는거같아요!",
          "original_form": "좋아하는건 많이 먹는거같아요!",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046221.1.8",
          "form": "아하!!",
          "original_form": "아하!!",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046221.1.9",
          "form": "전 원래 뷔페가면 7~8접시 이렇게 먹을 정도로 많이 먹었거든요",
          "original_form": "전 원래 뷔페가면 7~8접시 이렇게 먹을 정도로 많이 먹었거든요",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046221.1.10",
          "form": "그러다가 5접시 이렇게까지 줄더라고요ㅡ... 늙어버렸어요",
          "original_form": "그러다가 5접시 이렇게까지 줄더라고요ㅡ... 늙어버렸어요",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046221.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ10대때 진짜잘먹었던것같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ10대때 진짜잘먹었던것같아요",
          "speaker_id": "2",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046221.1.12",
          "form": "진짜 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100046221.1.13",
          "form": "엄청먹었어요",
          "original_form": "엄청먹었어요",
          "speaker_id": "1",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100046221.1.14",
          "form": "아침 점심 저녁 야식을 먹어도 살이 안쪘었는데 그때는...",
          "original_form": "아침 점심 저녁 야식을 먹어도 살이 안쪘었는데 그때는...",
          "speaker_id": "1",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100046221.1.15",
          "form": "지금은 왜이럴까요",
          "original_form": "지금은 왜이럴까요",
          "speaker_id": "1",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100046221.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그러니까말이에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그러니까말이에요",
          "speaker_id": "2",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046221.1.17",
          "form": "뷔페가고싶네요",
          "original_form": "뷔페가고싶네요",
          "speaker_id": "2",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046221.1.18",
          "form": "어디 뷔페 제일 좋아하세요?",
          "original_form": "어디 뷔페 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046221.1.19",
          "form": "전 빕스보단 애슐리가 좋더라고요",
          "original_form": "전 빕스보단 애슐리가 좋더라고요",
          "speaker_id": "1",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046221.1.20",
          "form": "저도 애슐리를제일많이간것같아요",
          "original_form": "저도 애슐리를제일많이간것같아요",
          "speaker_id": "2",
          "time": "20210908 11:29"
        },
        {
          "id": "MDRW2100046221.1.21",
          "form": "테마가뚜렷해서",
          "original_form": "테마가뚜렷해서",
          "speaker_id": "2",
          "time": "20210908 11:29"
        },
        {
          "id": "MDRW2100046221.1.22",
          "form": "진짜 너무 맛있어요ㅠ",
          "original_form": "진짜 너무 맛있어요ㅠ",
          "speaker_id": "1",
          "time": "20210908 11:29"
        },
        {
          "id": "MDRW2100046221.1.23",
          "form": "근데 애슐리 스테이크는 정말 아니더라구요ㅠㅋㅋ",
          "original_form": "근데 애슐리 스테이크는 정말 아니더라구요ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:29"
        },
        {
          "id": "MDRW2100046221.1.24",
          "form": "앗 ㅋㅋㅋ안먹어봤어요  어떤대요?",
          "original_form": "앗 ㅋㅋㅋ안먹어봤어요  어떤대요?",
          "speaker_id": "2",
          "time": "20210908 11:29"
        },
        {
          "id": "MDRW2100046221.1.25",
          "form": "스테이크를 잘 접해보지 못한 시절에  처음 먹고",
          "original_form": "스테이크를 잘 접해보지 못한 시절에  처음 먹고",
          "speaker_id": "1",
          "time": "20210908 11:30"
        },
        {
          "id": "MDRW2100046221.1.26",
          "form": "스테이크는 이런건가 정말 실망했었어요",
          "original_form": "스테이크는 이런건가 정말 실망했었어요",
          "speaker_id": "1",
          "time": "20210908 11:31"
        }
      ]
    }
  ]
