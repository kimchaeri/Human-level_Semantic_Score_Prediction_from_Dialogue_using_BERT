      "utterance": [
        {
          "id": "MDRW2100027458.1.1",
          "form": "형부랑 말안하나?",
          "original_form": "형부랑 말안하나?",
          "speaker_id": "2",
          "time": "20210902 12:47"
        },
        {
          "id": "MDRW2100027458.1.2",
          "form": "부동산건때문에 전화해서 말하고 오빠 짜증난다고 전화 확 끊었더니 다시 전화와서 미안하다 사과하데",
          "original_form": "부동산건때문에 전화해서 말하고 오빠 짜증난다고 전화 확 끊었더니 다시 전화와서 미안하다 사과하데",
          "speaker_id": "1",
          "time": "20210902 12:48"
        },
        {
          "id": "MDRW2100027458.1.3",
          "form": "뭐가미안하다고 하던데",
          "original_form": "뭐가미안하다고 하던데",
          "speaker_id": "2",
          "time": "20210902 12:49"
        },
        {
          "id": "MDRW2100027458.1.4",
          "form": "내가 name3때문에 화난줄알더라",
          "original_form": "내가 &name3&때문에 화난줄알더라",
          "speaker_id": "1",
          "time": "20210902 12:50"
        },
        {
          "id": "MDRW2100027458.1.5",
          "form": "본인이 멍해서 상황판단 안 됐다고",
          "original_form": "본인이 멍해서 상황판단 안 됐다고",
          "speaker_id": "1",
          "time": "20210902 12:50"
        },
        {
          "id": "MDRW2100027458.1.6",
          "form": "근데 사과 절대 안하거든...보통..처음 미안하다는 말 들었다",
          "original_form": "근데 사과 절대 안하거든...보통..처음 미안하다는 말 들었다",
          "speaker_id": "1",
          "time": "20210902 12:50"
        },
        {
          "id": "MDRW2100027458.1.7",
          "form": "심하게 잘못은 했나보네",
          "original_form": "심하게 잘못은 했나보네",
          "speaker_id": "2",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027458.1.8",
          "form": "미안하겠지 생일이었는데...",
          "original_form": "미안하겠지 생일이었는데...",
          "speaker_id": "1",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100027458.1.9",
          "form": "뭐 맛난거 먹으러가 주말에",
          "original_form": "뭐 맛난거 먹으러가 주말에",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027458.1.10",
          "form": "됐다 지나갔다ㅜ",
          "original_form": "됐다 지나갔다ㅜ",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027458.1.11",
          "form": "둘이만 나가면 엄마 싫어해",
          "original_form": "둘이만 나가면 엄마 싫어해",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027458.1.12",
          "form": "맞나 ㅜ 선물받아",
          "original_form": "맞나 ㅜ 선물받아",
          "speaker_id": "2",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027458.1.13",
          "form": "선물받고싶은것도 없어",
          "original_form": "선물받고싶은것도 없어",
          "speaker_id": "1",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027458.1.14",
          "form": "나중에 큰거 받아야지",
          "original_form": "나중에 큰거 받아야지",
          "speaker_id": "1",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027458.1.15",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "2",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027458.1.16",
          "form": "오늘 같이 외식했나",
          "original_form": "오늘 같이 외식했나",
