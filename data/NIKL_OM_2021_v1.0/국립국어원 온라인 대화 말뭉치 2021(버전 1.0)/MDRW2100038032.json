        },
        {
          "id": "MDRW2100038032.1.2",
          "form": "왠지",
          "original_form": "왠지",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100038032.1.3",
          "form": "오오오 기대되네 ㅋㅋ",
          "original_form": "오오오 기대되네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100038032.1.4",
          "form": "일하고 있을  때 올 것 같다;",
          "original_form": "일하고 있을  때 올 것 같다;",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100038032.1.5",
          "form": "그냥 흰 거 다 기본 반찬 임ㅋㅋ",
          "original_form": "그냥 흰 거 다 기본 반찬 임ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100038032.1.6",
          "form": "아하....우짜냐 그러면",
          "original_form": "아하....우짜냐 그러면",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100038032.1.7",
          "form": "비밀번호 가르쳐줘야지 뭐ㅋㅋㅋㅋ",
          "original_form": "비밀번호 가르쳐줘야지 뭐ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100038032.1.8",
          "form": "훔쳐갈 게 없네ㅋㅋ",
          "original_form": "훔쳐갈 게 없네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100038032.1.9",
          "form": "ㅋㅋㅋ 왜 없노 많지 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 왜 없노 많지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100038032.1.10",
          "form": "그렇구만 ㅋㅋㅋ",
          "original_form": "그렇구만 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100038032.1.11",
          "form": "아효 이제 잔다 . 둘째",
          "original_form": "아효 이제 잔다 . 둘째",
          "speaker_id": "1",
          "time": "20210901 22:42"
        },
        {
