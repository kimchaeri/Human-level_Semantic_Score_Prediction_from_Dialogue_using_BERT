      "utterance": [
        {
          "id": "MDRW2100034186.1.1",
          "form": "게임하는거 있으세용?",
          "original_form": "게임하는거 있으세용?",
          "speaker_id": "1",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034186.1.2",
          "form": "음 마이리틀셰프라는 폰게임 아세요?ㅋㅋㅋ",
          "original_form": "음 마이리틀셰프라는 폰게임 아세요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034186.1.3",
          "form": "앗 아뇨ㅠㅡㅠ 오늘처음들어봤지만 요리만들고 서빙하구 그런게임인가요?ㅇㅁㅇ?!",
          "original_form": "앗 아뇨ㅠㅡㅠ 오늘처음들어봤지만 요리만들고 서빙하구 그런게임인가요?ㅇㅁㅇ?!",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034186.1.4",
          "form": "네맞아여!!",
          "original_form": "네맞아여!!",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100034186.1.5",
          "form": "아기자기하니 할만해요 귀엽구 ㅋㅋㅋ",
          "original_form": "아기자기하니 할만해요 귀엽구 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100034186.1.6",
          "form": "아기자기한게임 좋아요ㅋㅋㅋ\r\n제가 잠시 심심이에 미쳐(?!)가지구 빨래너는것도 깜빡하고잇었네요ㅋㅋㅋㅋ",
          "original_form": "아기자기한게임 좋아요ㅋㅋㅋ\r\n제가 잠시 심심이에 미쳐(?!)가지구 빨래너는것도 깜빡하고잇었네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100034186.1.7",
          "form": "앗 ㅋㅋㅋㅋ 쉬엄쉬엄하셔요 ㅋㅋㅋ",
          "original_form": "앗 ㅋㅋㅋㅋ 쉬엄쉬엄하셔요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100034186.1.8",
          "form": "그러게요ㅋㅋㅋ 지금약간 이걸 약간 게임하듯이 집착하며 하고있는느낌...ㅋㅋㅋㅋ\r\n저 게임은 오래하셨어용?",
          "original_form": "그러게요ㅋㅋㅋ 지금약간 이걸 약간 게임하듯이 집착하며 하고있는느낌...ㅋㅋㅋㅋ\r\n저 게임은 오래하셨어용?",
          "speaker_id": "1",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100034186.1.9",
          "form": "저두여 ㅋㅋㅋㅋㅋ",
          "original_form": "저두여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100034186.1.10",
          "form": "네 3년 넘은거같아요 ㅋㅋㅋ",
          "original_form": "네 3년 넘은거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100034186.1.11",
          "form": "와 3년이요??!!!!",
          "original_form": "와 3년이요??!!!!",
          "speaker_id": "1",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100034186.1.12",
          "form": "꽤오래하셨다대박",
          "original_form": "꽤오래하셨다대박",
          "speaker_id": "1",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100034186.1.13",
          "form": "엄청 재밌다기보다 할게 없어서요ㅠㅋㅋㅋ",
          "original_form": "엄청 재밌다기보다 할게 없어서요ㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100034186.1.14",
          "form": "맞아요약간습관처럼하게되는게있는거같아요...",
          "original_form": "맞아요약간습관처럼하게되는게있는거같아요...",
          "speaker_id": "1",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100034186.1.15",
          "form": "저도 레알팜이라는 겜 하구있어요..ㅋㅋㅋ",
          "original_form": "저도 레알팜이라는 겜 하구있어요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100034186.1.16",
          "form": "그건 무슨게임인가요??",
          "original_form": "그건 무슨게임인가요??",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100034186.1.17",
          "form": "농사짓는 게임인데 게임을 하면 실제 농산물을 받아볼수있다!  라고 광고하는 게임 혹시 들어본적없으신가요ㅋㅋㅋㅋㅋㅋ\r\n저 그광고에 혹해서하기시작했는데..",
          "original_form": "농사짓는 게임인데 게임을 하면 실제 농산물을 받아볼수있다!  라고 광고하는 게임 혹시 들어본적없으신가요ㅋㅋㅋㅋㅋㅋ\r\n저 그광고에 혹해서하기시작했는데..",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100034186.1.18",
          "form": "헉 들어봤어요! 그게 그거군요",
          "original_form": "헉 들어봤어요! 그게 그거군요",
          "speaker_id": "2",
          "time": "20210827 22:35"
        }
      ]
