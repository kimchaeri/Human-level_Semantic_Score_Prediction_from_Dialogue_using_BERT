      "utterance": [
        {
          "id": "MDRW2100027381.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100027381.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100027381.1.3",
          "form": "뭐 하시는 게임 있으신가요?",
          "original_form": "뭐 하시는 게임 있으신가요?",
          "speaker_id": "2",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100027381.1.4",
          "form": "저는 롤 해요",
          "original_form": "저는 롤 해요",
          "speaker_id": "1",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100027381.1.5",
          "form": "오 롤 유명하죠",
          "original_form": "오 롤 유명하죠",
          "speaker_id": "2",
          "time": "20210902 12:05"
        },
        {
          "id": "MDRW2100027381.1.6",
          "form": "롤은 안해서 미드 탑 정글 이런거밖에 모르긴 하지만 ㅋㅋㅋ",
          "original_form": "롤은 안해서 미드 탑 정글 이런거밖에 모르긴 하지만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:05"
        },
        {
          "id": "MDRW2100027381.1.7",
          "form": "그러면 밀커님은 어떤 게임 주로 하세요?",
          "original_form": "그러면 밀커님은 어떤 게임 주로 하세요?",
          "speaker_id": "1",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100027381.1.8",
          "form": "저는 파이널 판타지 14랑 메이플, 요즘에는 라테일 해요!",
          "original_form": "저는 파이널 판타지 14랑 메이플, 요즘에는 라테일 해요!",
          "speaker_id": "2",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100027381.1.9",
          "form": "보통 RPG게임!!",
          "original_form": "보통 RPG게임!!",
          "speaker_id": "2",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100027381.1.10",
          "form": "라테일은 처음 들어봐요",
          "original_form": "라테일은 처음 들어봐요",
          "speaker_id": "1",
          "time": "20210902 12:08"
        },
        {
          "id": "MDRW2100027381.1.11",
          "form": "어떤 게임인가요",
          "original_form": "어떤 게임인가요",
          "speaker_id": "1",
          "time": "20210902 12:08"
        },
        {
          "id": "MDRW2100027381.1.12",
          "form": "메이플이랑 비슷한데 조금 달라요 횡스크롤게임에 도트/던전 공략/퀘스트 위주의 게임이에요!",
          "original_form": "메이플이랑 비슷한데 조금 달라요 횡스크롤게임에 도트/던전 공략/퀘스트 위주의 게임이에요!",
          "speaker_id": "2",
          "time": "20210902 12:10"
        },
        {
          "id": "MDRW2100027381.1.13",
          "form": "메이플이랑 비슷하면 입문하기 쉽겠네요",
          "original_form": "메이플이랑 비슷하면 입문하기 쉽겠네요",
          "speaker_id": "1",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027381.1.14",
          "form": "저도 메이플을 조금 했어서",
          "original_form": "저도 메이플을 조금 했어서",
          "speaker_id": "1",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027381.1.15",
          "form": "오 ㅎㅎ 맞아요 지금 막 버닝 이벤트 중이라",
          "original_form": "오 ㅎㅎ 맞아요 지금 막 버닝 이벤트 중이라",
          "speaker_id": "2",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027381.1.16",
          "form": "시작하기에도 나쁘지 않으실거예요",
          "original_form": "시작하기에도 나쁘지 않으실거예요",
          "speaker_id": "2",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027381.1.17",
          "form": "메이플에서는 무슨 캐릭터 하셨어요?",
          "original_form": "메이플에서는 무슨 캐릭터 하셨어요?",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100027381.1.18",
          "form": "엔버 했었어요",
          "original_form": "엔버 했었어요",
          "speaker_id": "1",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100027381.1.19",
          "form": "엔버 귀엽죠 아이돌 컨셉이고 타격감도 나쁘지 않았던거같네요",
          "original_form": "엔버 귀엽죠 아이돌 컨셉이고 타격감도 나쁘지 않았던거같네요",
          "speaker_id": "2",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100027381.1.20",
          "form": "근데 코디를 두종류로 해야했어서 불편했던거같아요 ㅋㅋㅋㅠㅠ",
          "original_form": "근데 코디를 두종류로 해야했어서 불편했던거같아요 ㅋㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100027381.1.21",
          "form": "맞아요ㅋㅋ 그래서 진작에 코디 포기했어요",
          "original_form": "맞아요ㅋㅋ 그래서 진작에 코디 포기했어요",
          "speaker_id": "1",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100027381.1.22",
          "form": "게임들 다 가챠 랜덤박스 시스템때문에 돈 많이 드는 ㅠㅠ",
          "original_form": "게임들 다 가챠 랜덤박스 시스템때문에 돈 많이 드는 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027381.1.23",
          "form": "롤에도 그런거 있나요?",
          "original_form": "롤에도 그런거 있나요?",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027381.1.24",
          "form": "있긴 해요",
          "original_form": "있긴 해요",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027381.1.25",
          "form": "주력 컨텐츠가 아닐 뿐이죠",
          "original_form": "주력 컨텐츠가 아닐 뿐이죠",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027381.1.26",
          "form": "오 어떤것들 있나요?",
          "original_form": "오 어떤것들 있나요?",
          "speaker_id": "2",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100027381.1.27",
          "form": "스킨이나 챔피언을 랜덤으로 뽑기도 하고",
          "original_form": "스킨이나 챔피언을 랜덤으로 뽑기도 하고",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027381.1.28",
          "form": "롤체 캐릭터를 가챠로 뽑기도 해요",
          "original_form": "롤체 캐릭터를 가챠로 뽑기도 해요",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027381.1.29",
          "form": "오 다들 있기는 하군요 신기해요 ㅋㅋㅋ",
          "original_form": "오 다들 있기는 하군요 신기해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100027381.1.30",
          "form": "캐릭터 가챠라니 가챠캐릭터가 성능이 좋은편인가요?",
          "original_form": "캐릭터 가챠라니 가챠캐릭터가 성능이 좋은편인가요?",
          "speaker_id": "2",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100027381.1.31",
          "form": "성능은 따로 없어요",
          "original_form": "성능은 따로 없어요",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100027381.1.32",
          "form": "외모만 보고 뽑는거죠ㅋㅋ",
          "original_form": "외모만 보고 뽑는거죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100027381.1.33",
          "form": "아하 ㅋㅋㅋㅋ 그렇군요 다 컨트롤 게임인가봐요!?",
          "original_form": "아하 ㅋㅋㅋㅋ 그렇군요 다 컨트롤 게임인가봐요!?",
          "speaker_id": "2",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100027381.1.34",
          "form": "네ㅠ 완전 피지컬 뇌지컬 게임이라",
          "original_form": "네ㅠ 완전 피지컬 뇌지컬 게임이라",
          "speaker_id": "1",
          "time": "20210902 12:30"
        },
        {
          "id": "MDRW2100027381.1.35",
          "form": "영향 받는게 아예 없어요",
          "original_form": "영향 받는게 아예 없어요",
          "speaker_id": "1",
          "time": "20210902 12:30"
        },
        {
          "id": "MDRW2100027381.1.36",
          "form": "크 ㅠㅠ 하긴 프로 선수들 있는 게임이니까요 캐릭터에 영향받으면",
          "original_form": "크 ㅠㅠ 하긴 프로 선수들 있는 게임이니까요 캐릭터에 영향받으면",
          "speaker_id": "2",
          "time": "20210902 12:31"
        },
        {
          "id": "MDRW2100027381.1.37",
          "form": "한 캐릭터만 계속 나올거같네요",
          "original_form": "한 캐릭터만 계속 나올거같네요",
          "speaker_id": "2",
          "time": "20210902 12:31"
        }
      ]
