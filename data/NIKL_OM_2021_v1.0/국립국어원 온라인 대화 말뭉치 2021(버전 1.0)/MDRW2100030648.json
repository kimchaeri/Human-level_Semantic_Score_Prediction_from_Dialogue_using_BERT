      "utterance": [
        {
          "id": "MDRW2100030648.1.1",
          "form": "네 사서 먹고 있습니다",
          "original_form": "네 사서 먹고 있습니다",
          "speaker_id": "2",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100030648.1.2",
          "form": "만들어 먹기에는 너무 귀찮아서요",
          "original_form": "만들어 먹기에는 너무 귀찮아서요",
          "speaker_id": "2",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100030648.1.3",
          "form": "오늘 사셨나요",
          "original_form": "오늘 사셨나요",
          "speaker_id": "1",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100030648.1.4",
          "form": "감자를 정말 자주 드시네요",
          "original_form": "감자를 정말 자주 드시네요",
          "speaker_id": "2",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100030648.1.5",
          "form": "요새 김밥 사건이 말이 많습니다",
          "original_form": "요새 김밥 사건이 말이 많습니다",
          "speaker_id": "1",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100030648.1.6",
          "form": "네 오늘 샀습니다",
          "original_form": "네 오늘 샀습니다",
          "speaker_id": "2",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100030648.1.7",
          "form": "감자농사 하시는 분이 잔뜩 주셨어요",
          "original_form": "감자농사 하시는 분이 잔뜩 주셨어요",
          "speaker_id": "1",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100030648.1.8",
          "form": "아직 많습니다",
          "original_form": "아직 많습니다",
          "speaker_id": "1",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100030648.1.9",
          "form": "주변에 주시는 분이 있어서 좋겠어요",
          "original_form": "주변에 주시는 분이 있어서 좋겠어요",
          "speaker_id": "2",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100030648.1.10",
          "form": "무슨 김밥인가요",
          "original_form": "무슨 김밥인가요",
          "speaker_id": "1",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100030648.1.11",
          "form": "감자만 줍니다",
          "original_form": "감자만 줍니다",
          "speaker_id": "1",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100030648.1.12",
          "form": "먹을 거 나눠 주는 사람 좋은 사람",
          "original_form": "먹을 거 나눠 주는 사람 좋은 사람",
          "speaker_id": "2",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100030648.1.13",
          "form": "기본 김밥입니다 원조 김밥",
          "original_form": "기본 김밥입니다 원조 김밥",
          "speaker_id": "2",
          "time": "20210904 11:01"
        },
        {
          "id": "MDRW2100030648.1.14",
          "form": "냥코 됐는데 드리고 싶네요",
          "original_form": "냥코 됐는데 드리고 싶네요",
          "speaker_id": "1",
          "time": "20210904 11:01"
        },
        {
          "id": "MDRW2100030648.1.15",
          "form": "양껏있는데",
          "original_form": "양껏있는데",
          "speaker_id": "1",
          "time": "20210904 11:01"
        },
        {
          "id": "MDRW2100030648.1.16",
          "form": "원조 김밥은 얼마입니까",
          "original_form": "원조 김밥은 얼마입니까",
          "speaker_id": "1",
          "time": "20210904 11:01"
        },
        {
          "id": "MDRW2100030648.1.17",
          "form": "근처에 살면 서로 음식 나눠 먹고 좋을 텐데요",
          "original_form": "근처에 살면 서로 음식 나눠 먹고 좋을 텐데요",
          "speaker_id": "2",
          "time": "20210904 11:01"
        },
        {
          "id": "MDRW2100030648.1.18",
          "form": "제가 안 사서 가격을 모르겠습니다",
          "original_form": "제가 안 사서 가격을 모르겠습니다",
          "speaker_id": "2",
          "time": "20210904 11:01"
        },
        {
          "id": "MDRW2100030648.1.19",
          "form": "나눠 먹으면 더 맛있겠네요",
          "original_form": "나눠 먹으면 더 맛있겠네요",
          "speaker_id": "1",
          "time": "20210904 11:01"
        },
        {
          "id": "MDRW2100030648.1.20",
          "form": "기본 김밥인데 3,000원이라고 하시네요",
          "original_form": "기본 김밥인데 3,000원이라고 하시네요",
          "speaker_id": "2",
          "time": "20210904 11:01"
        },
        {
          "id": "MDRW2100030648.1.21",
          "form": "얻어 먹을 셨군요 좋네요",
          "original_form": "얻어 먹을 셨군요 좋네요",
          "speaker_id": "1",
          "time": "20210904 11:01"
        }
