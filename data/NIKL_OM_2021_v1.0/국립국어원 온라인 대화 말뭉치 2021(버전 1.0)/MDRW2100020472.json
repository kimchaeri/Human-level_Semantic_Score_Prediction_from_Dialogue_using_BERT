        },
        {
          "id": "MDRW2100020472.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100020472.1.3",
          "form": "여행 자주 다니시나요?",
          "original_form": "여행 자주 다니시나요?",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100020472.1.4",
          "form": "네 나름이요 ㅎㅎ",
          "original_form": "네 나름이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 06:56"
        },
        {
          "id": "MDRW2100020472.1.5",
          "form": "오 주로 국내 다니세요 해외 다니세요?",
          "original_form": "오 주로 국내 다니세요 해외 다니세요?",
          "speaker_id": "2",
          "time": "20210829 10:25"
        },
        {
          "id": "MDRW2100020472.1.6",
          "form": "원래는 해외 매니아인데",
          "original_form": "원래는 해외 매니아인데",
          "speaker_id": "1",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100020472.1.7",
          "form": "지금은 할 수 없이 국내....ㅠ",
          "original_form": "지금은 할 수 없이 국내....ㅠ",
          "speaker_id": "1",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100020472.1.8",
          "form": "아아 아무래도 시국이ㅠㅠ",
          "original_form": "아아 아무래도 시국이ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 23:37"
        },
        {
          "id": "MDRW2100020472.1.9",
          "form": "해외여행은 어디 다녀오셨나요??",
          "original_form": "해외여행은 어디 다녀오셨나요??",
          "speaker_id": "2",
          "time": "20210829 23:37"
        },
        {
          "id": "MDRW2100020472.1.10",
          "form": "저 되게 많은데 ㅎㅎ",
          "original_form": "저 되게 많은데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:22"
        },
        {
          "id": "MDRW2100020472.1.11",
          "form": "19년 기준으로는",
          "original_form": "19년 기준으로는",
          "speaker_id": "1",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100020472.1.12",
          "form": "홍콩마카오 태국 베트남 대만이요ㅎㅎ",
          "original_form": "홍콩마카오 태국 베트남 대만이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100020472.1.13",
          "form": "와 엄청 많이 가보셨네요!!",
          "original_form": "와 엄청 많이 가보셨네요!!",
          "speaker_id": "2",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100020472.1.14",
          "form": "마카오 화려한가요?? 사진으로만 접해봐서ㅎㅎ",
          "original_form": "마카오 화려한가요?? 사진으로만 접해봐서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100020472.1.15",
          "form": "마카오 좋아요",
          "original_form": "마카오 좋아요",
          "speaker_id": "1",
          "time": "20210830 19:15"
        },
        {
          "id": "MDRW2100020472.1.16",
          "form": "먹을것도 괜찮고",
          "original_form": "먹을것도 괜찮고",
          "speaker_id": "1",
          "time": "20210830 19:15"
        },
        {
          "id": "MDRW2100020472.1.17",
          "form": "카지노 때문에 숙소도 싸서",
          "original_form": "카지노 때문에 숙소도 싸서",
          "speaker_id": "1",
          "time": "20210830 19:15"
        },
        {
          "id": "MDRW2100020472.1.18",
          "form": "오오 돌아다니면서 관광 많이 하셨나요..!!",
          "original_form": "오오 돌아다니면서 관광 많이 하셨나요..!!",
          "speaker_id": "2",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100020472.1.19",
          "form": "네 ㅎㅎ 재밌었어요 ㅎㅎ",
          "original_form": "네 ㅎㅎ 재밌었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:05"
        },
        {
          "id": "MDRW2100020472.1.20",
          "form": "어디 가보셨나요",
          "original_form": "어디 가보셨나요",
          "speaker_id": "1",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100020472.1.21",
          "form": "전 가평이나 강릉 소박하게 다녀온 게 다예요ㅎㅎ",
          "original_form": "전 가평이나 강릉 소박하게 다녀온 게 다예요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100020472.1.22",
          "form": "강릉 좋죠",
          "original_form": "강릉 좋죠",
          "speaker_id": "1",
          "time": "20210831 09:59"
        },
        {
          "id": "MDRW2100020472.1.23",
