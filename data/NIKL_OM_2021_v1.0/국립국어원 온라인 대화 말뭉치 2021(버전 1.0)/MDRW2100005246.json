      "utterance": [
        {
          "id": "MDRW2100005246.1.1",
          "form": "저는 게임을 많이 안하는 편인데",
          "original_form": "저는 게임을 많이 안하는 편인데",
          "speaker_id": "2",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100005246.1.2",
          "form": "요즘 그래도 하고있는건 쿠키런킹덤이에요..ㅎㅎ",
          "original_form": "요즘 그래도 하고있는건 쿠키런킹덤이에요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005246.1.3",
          "form": "킹덤하시는구나",
          "original_form": "킹덤하시는구나",
          "speaker_id": "1",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005246.1.4",
          "form": "그거 한번 빠지면 빠져나올 수 없다고 그래서 시작도 못했어요...",
          "original_form": "그거 한번 빠지면 빠져나올 수 없다고 그래서 시작도 못했어요...",
          "speaker_id": "1",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005246.1.5",
          "form": "저는 초반부터해서 약간 질리긴 했는데..",
          "original_form": "저는 초반부터해서 약간 질리긴 했는데..",
          "speaker_id": "2",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005246.1.6",
          "form": "나중에 다시 시작할때 뒤쳐질까봐 계속 하고있어요",
          "original_form": "나중에 다시 시작할때 뒤쳐질까봐 계속 하고있어요",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005246.1.7",
          "form": "게임 어떤거하세요?",
          "original_form": "게임 어떤거하세요?",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005246.1.8",
          "form": "저는 예전에 쿠키런 카카오 했었어요",
          "original_form": "저는 예전에 쿠키런 카카오 했었어요",
          "speaker_id": "1",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005246.1.9",
          "form": "한때 거기에 파...쳐서 상위 0.1퍼 찍고 그랬어욬ㅋㅋ",
          "original_form": "한때 거기에 파...쳐서 상위 0.1퍼 찍고 그랬어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005246.1.10",
          "form": "쿠키런 저도 예전에 했었어요ㅎㅎ",
          "original_form": "쿠키런 저도 예전에 했었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005246.1.11",
          "form": "엄청 잘하셨나봐요 ㅠㅠ 대단하세요..",
          "original_form": "엄청 잘하셨나봐요 ㅠㅠ 대단하세요..",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005246.1.12",
          "form": "게임 하나에 집착하는 스타일이라 그래요ㅋㅋㅋ ㅠㅠ",
          "original_form": "게임 하나에 집착하는 스타일이라 그래요ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005246.1.13",
          "form": "완전 빠져서 가끔 그럴때 있죠ㅋㅋㅋ",
          "original_form": "완전 빠져서 가끔 그럴때 있죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005246.1.14",
          "form": "PC게임도 하는데",
          "original_form": "PC게임도 하는데",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005246.1.15",
          "form": "스타듀밸리 아세요?",
          "original_form": "스타듀밸리 아세요?",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005246.1.16",
          "form": "저 그거 한때 빠져서 한달에 200시간 했어욬ㅋㅋ",
          "original_form": "저 그거 한때 빠져서 한달에 200시간 했어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005246.1.17",
          "form": "네네 알아요ㅋㅋ",
          "original_form": "네네 알아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005246.1.18",
          "form": "이런 게임빠지면 시간 완전 순삭이잖아요..",
          "original_form": "이런 게임빠지면 시간 완전 순삭이잖아요..",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005246.1.19",
          "form": "ㅋㅋㅋㅋㅋ그렇죠...",
          "original_form": "ㅋㅋㅋㅋㅋ그렇죠...",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005246.1.20",
          "form": "시간이 홀라당",
          "original_form": "시간이 홀라당",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005246.1.21",
          "form": "지금은 모바일게임 많이 하지만 예전에는 컴퓨터 게임 참 많이했어요",
          "original_form": "지금은 모바일게임 많이 하지만 예전에는 컴퓨터 게임 참 많이했어요",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005246.1.22",
          "form": "어렸을때는 크아랑 테일즈런너 젤 많이했었는데..",
          "original_form": "어렸을때는 크아랑 테일즈런너 젤 많이했었는데..",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005246.1.23",
          "form": "혹시 하셨나요..?",
          "original_form": "혹시 하셨나요..?",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005246.1.24",
          "form": "와 저도 크아 엄청 많이 했어요ㅠㅠ",
          "original_form": "와 저도 크아 엄청 많이 했어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005246.1.25",
          "form": "만렙 가까이 찍고 트로피 달고...",
          "original_form": "만렙 가까이 찍고 트로피 달고...",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005246.1.26",
          "form": "크아 진짜 재밌었죠 ㅠㅠ",
          "original_form": "크아 진짜 재밌었죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005246.1.27",
          "form": "맵도 다양하고 둘이서 같이 할수있어서 더 많이 했던 것 같아요",
          "original_form": "맵도 다양하고 둘이서 같이 할수있어서 더 많이 했던 것 같아요",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005246.1.28",
          "form": "맞아요 모드가 많아서 질리지도 않고요",
          "original_form": "맞아요 모드가 많아서 질리지도 않고요",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005246.1.29",
          "form": "진짜 재밌었는데.. 오랜만에 다시 하고싶네요ㅎㅎ",
          "original_form": "진짜 재밌었는데.. 오랜만에 다시 하고싶네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:54"
        }
      ]
