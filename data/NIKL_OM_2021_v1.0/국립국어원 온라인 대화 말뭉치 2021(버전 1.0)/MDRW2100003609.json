      "utterance": [
        {
          "id": "MDRW2100003609.1.1",
          "form": "전 더운거 진......짜 싫어하고",
          "original_form": "전 더운거 진......짜 싫어하고",
          "speaker_id": "2",
          "time": "20210821 08:21"
        },
        {
          "id": "MDRW2100003609.1.2",
          "form": "추운게 좋아요",
          "original_form": "추운게 좋아요",
          "speaker_id": "2",
          "time": "20210821 08:21"
        },
        {
          "id": "MDRW2100003609.1.3",
          "form": "아무래도 그렇죠",
          "original_form": "아무래도 그렇죠",
          "speaker_id": "1",
          "time": "20210821 08:22"
        },
        {
          "id": "MDRW2100003609.1.4",
          "form": "더운건 사람을 미치게하더라구요",
          "original_form": "더운건 사람을 미치게하더라구요",
          "speaker_id": "1",
          "time": "20210821 08:22"
        },
        {
          "id": "MDRW2100003609.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 08:22"
        },
        {
          "id": "MDRW2100003609.1.6",
          "form": "미치겠어요",
          "original_form": "미치겠어요",
          "speaker_id": "2",
          "time": "20210821 08:22"
        },
        {
          "id": "MDRW2100003609.1.7",
          "form": "제가 더위 진짜 많이타서",
          "original_form": "제가 더위 진짜 많이타서",
          "speaker_id": "2",
          "time": "20210821 08:22"
        },
        {
          "id": "MDRW2100003609.1.8",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 08:22"
        },
        {
          "id": "MDRW2100003609.1.9",
          "form": "2018년 여름",
          "original_form": "2018년 여름",
          "speaker_id": "1",
          "time": "20210821 08:22"
        },
        {
          "id": "MDRW2100003609.1.10",
          "form": "기억나시나요",
          "original_form": "기억나시나요",
          "speaker_id": "1",
          "time": "20210821 08:23"
        },
        {
          "id": "MDRW2100003609.1.11",
          "form": "진짜 역대급이었는데",
          "original_form": "진짜 역대급이었는데",
          "speaker_id": "1",
          "time": "20210821 08:23"
        },
        {
          "id": "MDRW2100003609.1.12",
          "form": "근데 그거 아세쇼?",
          "original_form": "근데 그거 아세쇼?",
          "speaker_id": "2",
          "time": "20210821 08:23"
        },
        {
          "id": "MDRW2100003609.1.13",
          "form": "요?",
          "original_form": "요?",
          "speaker_id": "2",
          "time": "20210821 08:23"
        },
        {
          "id": "MDRW2100003609.1.14",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210821 08:23"
        },
        {
          "id": "MDRW2100003609.1.15",
          "form": "올해 7월이 평균기온으로 따지면 2018년보다 더웠대요",
          "original_form": "올해 7월이 평균기온으로 따지면 2018년보다 더웠대요",
          "speaker_id": "2",
          "time": "20210821 08:24"
        },
        {
          "id": "MDRW2100003609.1.16",
          "form": "8월은 아니고",
          "original_form": "8월은 아니고",
          "speaker_id": "2",
          "time": "20210821 08:24"
        },
        {
          "id": "MDRW2100003609.1.17",
          "form": "헐 거짓말",
          "original_form": "헐 거짓말",
          "speaker_id": "1",
          "time": "20210821 08:24"
        },
        {
          "id": "MDRW2100003609.1.18",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210821 08:24"
        },
        {
          "id": "MDRW2100003609.1.19",
          "form": "네 그렇다던데요?",
          "original_form": "네 그렇다던데요?",
          "speaker_id": "2",
          "time": "20210821 08:25"
        },
        {
          "id": "MDRW2100003609.1.20",
          "form": "저 2018년에 자다가 울면서 깼어요 더워서",
          "original_form": "저 2018년에 자다가 울면서 깼어요 더워서",
          "speaker_id": "1",
          "time": "20210821 08:25"
        },
        {
          "id": "MDRW2100003609.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:25"
        },
        {
          "id": "MDRW2100003609.1.22",
          "form": "ㅋㅋㅋ 진짜 너무 화가나는거에요",
          "original_form": "ㅋㅋㅋ 진짜 너무 화가나는거에요",
          "speaker_id": "1",
          "time": "20210821 08:25"
        },
        {
          "id": "MDRW2100003609.1.23",
          "form": "선풍기 틀고 자시지",
          "original_form": "선풍기 틀고 자시지",
          "speaker_id": "2",
          "time": "20210821 08:25"
        },
        {
          "id": "MDRW2100003609.1.24",
          "form": "열받아서 에어컨샀잖아요",
          "original_form": "열받아서 에어컨샀잖아요",
          "speaker_id": "1",
          "time": "20210821 08:25"
        },
        {
          "id": "MDRW2100003609.1.25",
          "form": "그때는 공기가 더워서.. 틀어도 소용이 없었어요",
          "original_form": "그때는 공기가 더워서.. 틀어도 소용이 없었어요",
          "speaker_id": "1",
          "time": "20210821 08:25"
        },
