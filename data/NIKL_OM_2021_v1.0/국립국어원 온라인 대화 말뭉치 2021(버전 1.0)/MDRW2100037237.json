      "utterance": [
        {
          "id": "MDRW2100037237.1.1",
          "form": "크으 더 먹고싶네용ㅋㅋㅋㅋ",
          "original_form": "크으 더 먹고싶네용ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100037237.1.2",
          "form": "name2님 혹시 성인이세요?",
          "original_form": "&name2&님 혹시 성인이세요?",
          "speaker_id": "1",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100037237.1.3",
          "form": "술.. 이랑 같이 먹으면.. 너무 좋길래..",
          "original_form": "술.. 이랑 같이 먹으면.. 너무 좋길래..",
          "speaker_id": "1",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037237.1.4",
          "form": "어릴적 자주가던곳이 리모델링해서 가봐야하는데 말이죠",
          "original_form": "어릴적 자주가던곳이 리모델링해서 가봐야하는데 말이죠",
          "speaker_id": "2",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037237.1.5",
          "form": "네넹,!!",
          "original_form": "네넹,!!",
          "speaker_id": "2",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037237.1.6",
          "form": "아..진짜 좋죠",
          "original_form": "아..진짜 좋죠",
          "speaker_id": "2",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037237.1.7",
          "form": "최고죠",
          "original_form": "최고죠",
          "speaker_id": "2",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037237.1.8",
          "form": "계속 들어가죠",
          "original_form": "계속 들어가죠",
          "speaker_id": "2",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037237.1.9",
          "form": "그니까요 소주랑 먹으면 크으!",
          "original_form": "그니까요 소주랑 먹으면 크으!",
          "speaker_id": "1",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037237.1.10",
          "form": "name2님 주종은 뭔가요!?",
          "original_form": "&name2&님 주종은 뭔가요!?",
          "speaker_id": "1",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037237.1.11",
          "form": "저는 소주나 소맥이용",
          "original_form": "저는 소주나 소맥이용",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100037237.1.12",
          "form": "맥주는 맛을 잘모르겠어요",
          "original_form": "맥주는 맛을 잘모르겠어요",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100037237.1.13",
          "form": "헐헐 저도요",
          "original_form": "헐헐 저도요",
          "speaker_id": "1",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100037237.1.14",
          "form": "소맥은 좋아하는데 맥주는 과일맛이 아니면",
          "original_form": "소맥은 좋아하는데 맥주는 과일맛이 아니면",
          "speaker_id": "1",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100037237.1.15",
          "form": "잘 안 먹어요",
          "original_form": "잘 안 먹어요",
          "speaker_id": "1",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100037237.1.16",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100037237.1.17",
          "form": "생맥은 좀 나은편이지만 캔같은거는 좀 안맞더라구요",
          "original_form": "생맥은 좀 나은편이지만 캔같은거는 좀 안맞더라구요",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037237.1.18",
          "form": "와인은 드시나요?",
          "original_form": "와인은 드시나요?",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037237.1.19",
          "form": "넹! 가끔요",
          "original_form": "넹! 가끔요",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037237.1.20",
          "form": "드시나요?",
          "original_form": "드시나요?",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100037237.1.21",
          "form": "저는 못 먹어요 ㅠㅠ !!!!!!! 와인은 의외로 빨리 취하더라고요",
          "original_form": "저는 못 먹어요 ㅠㅠ !!!!!!! 와인은 의외로 빨리 취하더라고요",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100037237.1.22",
          "form": "주 량 강하신가요!?",
          "original_form": "주 량 강하신가요!?",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100037237.1.23",
          "form": "별로 안강하지만 좋아해용!",
          "original_form": "별로 안강하지만 좋아해용!",
          "speaker_id": "2",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100037237.1.24",
          "form": "헛 저도요 ㅎㅎㅎㅎㅎㅎ !!!",
          "original_form": "헛 저도요 ㅎㅎㅎㅎㅎㅎ !!!",
          "speaker_id": "1",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100037237.1.25",
          "form": "술도 많이 먹고 안주도 많이 목는편..이에요 ㅎ.ㅎ",
          "original_form": "술도 많이 먹고 안주도 많이 목는편..이에요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037237.1.26",
          "form": "저두용 안주빨이라해도 과언이 아니죠ㅋㄱㅋ",
          "original_form": "저두용 안주빨이라해도 과언이 아니죠ㅋㄱㅋ",
