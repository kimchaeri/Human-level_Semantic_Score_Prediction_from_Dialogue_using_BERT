      "utterance": [
        {
          "id": "MDRW2100001922.1.1",
          "form": "길냥이 주려고 사료 샀는데 길냥이를 못 만났어",
          "original_form": "길냥이 주려고 사료 샀는데 길냥이를 못 만났어",
          "speaker_id": "1",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100001922.1.2",
          "form": "헛~ 사료를 또 샀다고?",
          "original_form": "헛~ 사료를 또 샀다고?",
          "speaker_id": "2",
          "time": "20210823 07:11"
        },
        {
          "id": "MDRW2100001922.1.3",
          "form": "길냥이 영양사료래",
          "original_form": "길냥이 영양사료래",
          "speaker_id": "1",
          "time": "20210824 11:29"
        },
        {
          "id": "MDRW2100001922.1.4",
          "form": "영양사료는 비쌀텐데 ㅋㅋ",
          "original_form": "영양사료는 비쌀텐데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:30"
        },
        {
          "id": "MDRW2100001922.1.5",
          "form": "길냥이를 위해서 좋은거 먹이려고",
          "original_form": "길냥이를 위해서 좋은거 먹이려고",
          "speaker_id": "1",
          "time": "20210824 11:30"
        },
        {
          "id": "MDRW2100001922.1.6",
          "form": "길냥이들 호강하는구먼~^^",
          "original_form": "길냥이들 호강하는구먼~^^",
          "speaker_id": "2",
          "time": "20210824 11:30"
        },
        {
          "id": "MDRW2100001922.1.7",
          "form": "불쌍하다 너무 꼬질꼬질해",
          "original_form": "불쌍하다 너무 꼬질꼬질해",
          "speaker_id": "1",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100001922.1.8",
          "form": "요즘은 날씨가 춥진 않아서 다행이다",
          "original_form": "요즘은 날씨가 춥진 않아서 다행이다",
          "speaker_id": "2",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100001922.1.9",
          "form": "샤워를 좀 했으면 좋겠어",
          "original_form": "샤워를 좀 했으면 좋겠어",
          "speaker_id": "1",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100001922.1.10",
          "form": "도시에 사는 애들은 어쩔 수 없다",
          "original_form": "도시에 사는 애들은 어쩔 수 없다",
          "speaker_id": "2",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100001922.1.11",
          "form": "인스타보면 길냥이들이 너무 많아",
          "original_form": "인스타보면 길냥이들이 너무 많아",
          "speaker_id": "1",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100001922.1.12",
          "form": "맨날 차밑으로 다니니까",
          "original_form": "맨날 차밑으로 다니니까",
          "speaker_id": "2",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100001922.1.13",
          "form": "시골에도 많고 없는 데가 없어",
          "original_form": "시골에도 많고 없는 데가 없어",
          "speaker_id": "1",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100001922.1.14",
          "form": "굶은 고양이들은 사료를 허겁지겁 먹더라",
          "original_form": "굶은 고양이들은 사료를 허겁지겁 먹더라",
          "speaker_id": "1",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100001922.1.15",
          "form": "챙겨주는 사람들이 많아져서 ㅋ",
          "original_form": "챙겨주는 사람들이 많아져서 ㅋ",
          "speaker_id": "2",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100001922.1.16",
          "form": "고양이의 품위는 어디갔나",
          "original_form": "고양이의 품위는 어디갔나",
          "speaker_id": "1",
          "time": "20210824 11:32"
        }
      ]
