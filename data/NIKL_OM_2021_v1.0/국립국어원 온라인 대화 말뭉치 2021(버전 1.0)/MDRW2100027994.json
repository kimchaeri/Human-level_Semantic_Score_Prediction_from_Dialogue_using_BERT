      "utterance": [
        {
          "id": "MDRW2100027994.1.1",
          "form": "요즘 맨날 향수 들어",
          "original_form": "요즘 맨날 향수 들어",
          "speaker_id": "1",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027994.1.2",
          "form": "근데 이제 여자가 커버한",
          "original_form": "근데 이제 여자가 커버한",
          "speaker_id": "1",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027994.1.3",
          "form": "웃기다",
          "original_form": "웃기다",
          "speaker_id": "2",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100027994.1.4",
          "form": "당연함",
          "original_form": "당연함",
          "speaker_id": "2",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100027994.1.5",
          "form": "나는... 이달소노래",
          "original_form": "나는... 이달소노래",
          "speaker_id": "2",
          "time": "20210902 16:48"
        },
        {
          "id": "MDRW2100027994.1.6",
          "form": "그건... 좀 지나가서",
          "original_form": "그건... 좀 지나가서",
          "speaker_id": "1",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027994.1.7",
          "form": "요즘은 잔잔한 노래 듣는",
          "original_form": "요즘은 잔잔한 노래 듣는",
          "speaker_id": "1",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027994.1.8",
          "form": "엇갈리는 쿨",
          "original_form": "엇갈리는 쿨",
          "speaker_id": "2",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027994.1.9",
          "form": "나는 이 쿨 돌기 전에 잔잔한 노래 들었음",
          "original_form": "나는 이 쿨 돌기 전에 잔잔한 노래 들었음",
          "speaker_id": "2",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027994.1.10",
          "form": "자우림노래...",
          "original_form": "자우림노래...",
          "speaker_id": "2",
          "time": "20210902 16:50"
        },
        {
          "id": "MDRW2100027994.1.11",
          "form": "그렇구만...",
          "original_form": "그렇구만...",
          "speaker_id": "1",
          "time": "20210902 16:50"
        },
        {
          "id": "MDRW2100027994.1.12",
          "form": "요즘은 거의 일본노래 듣는 듯...",
          "original_form": "요즘은 거의 일본노래 듣는 듯...",
          "speaker_id": "1",
          "time": "20210902 16:50"
        },
        {
          "id": "MDRW2100027994.1.13",
          "form": "하나 추천해보도록.",
          "original_form": "하나 추천해보도록.",
          "speaker_id": "2",
          "time": "20210902 16:50"
        },
        {
          "id": "MDRW2100027994.1.14",
          "form": "위에 썼잖아 향수",
          "original_form": "위에 썼잖아 향수",
          "speaker_id": "1",
          "time": "20210902 16:51"
        },
        {
          "id": "MDRW2100027994.1.15",
          "form": "아 그래",
          "original_form": "아 그래",
          "speaker_id": "2",
          "time": "20210902 16:51"
        },
        {
          "id": "MDRW2100027994.1.16",
          "form": "에이토의 향수",
          "original_form": "에이토의 향수",
          "speaker_id": "1",
          "time": "20210902 16:51"
        },
        {
          "id": "MDRW2100027994.1.17",
          "form": "치면 여러가지 나오는데 여자버전은 다 좋은 듯",
          "original_form": "치면 여러가지 나오는데 여자버전은 다 좋은 듯",
          "speaker_id": "1",
          "time": "20210902 16:51"
        },
        {
          "id": "MDRW2100027994.1.18",
          "form": "원곡은 올드해",
          "original_form": "원곡은 올드해",
          "speaker_id": "1",
          "time": "20210902 16:51"
        },
        {
          "id": "MDRW2100027994.1.19",
          "form": "사실 뭔 노랜지 몰라서 쳐보고 왔는데 한국가곡같은 거 나와서",
          "original_form": "사실 뭔 노랜지 몰라서 쳐보고 왔는데 한국가곡같은 거 나와서",
          "speaker_id": "2",
          "time": "20210902 16:51"
        },
        {
          "id": "MDRW2100027994.1.20",
          "form": "이런 것도 듣는구나 ... 하고 나오는 길이었던",
          "original_form": "이런 것도 듣는구나 ... 하고 나오는 길이었던",
          "speaker_id": "2",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100027994.1.21",
          "form": "향수 너무 많지...",
          "original_form": "향수 너무 많지...",
          "speaker_id": "1",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100027994.1.22",
          "form": "이따 작업하면서 들어보겟음",
          "original_form": "이따 작업하면서 들어보겟음",
          "speaker_id": "2",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100027994.1.23",
          "form": "잔잔하고 좋아요",
          "original_form": "잔잔하고 좋아요",
          "speaker_id": "1",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100027994.1.24",
          "form": "노동요로 들으시나요",
          "original_form": "노동요로 들으시나요",
          "speaker_id": "2",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100027994.1.25",
          "form": "거의? 그냥 다른 영상 고를 때도 브금 느낌으로 틀어놓는 듯(ㅋㅋ)",
          "original_form": "거의? 그냥 다른 영상 고를 때도 브금 느낌으로 틀어놓는 듯(ㅋㅋ)",
          "speaker_id": "1",
          "time": "20210902 16:55"
        },
        {
          "id": "MDRW2100027994.1.26",
          "form": "웃기다(ㅋㅋ)",
          "original_form": "웃기다(ㅋㅋ)",
          "speaker_id": "2",
          "time": "20210902 16:55"
        }
      ]
