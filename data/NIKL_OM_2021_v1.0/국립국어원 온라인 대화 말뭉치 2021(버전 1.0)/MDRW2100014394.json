        },
        {
          "id": "MDRW2100014394.1.2",
          "form": "저녁 드셨어요",
          "original_form": "저녁 드셨어요",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014394.1.3",
          "form": "지금 먹었어요",
          "original_form": "지금 먹었어요",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014394.1.4",
          "form": "바로",
          "original_form": "바로",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014394.1.5",
          "form": "불고기 상추",
          "original_form": "불고기 상추",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014394.1.6",
          "form": "에 먹었어요",
          "original_form": "에 먹었어요",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014394.1.7",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014394.1.8",
          "form": "오 불고기...",
          "original_form": "오 불고기...",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014394.1.9",
          "form": "저는 피자요 ㅋ",
          "original_form": "저는 피자요 ㅋ",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014394.1.10",
          "form": "아 피자 말씀하시니",
          "original_form": "아 피자 말씀하시니",
          "speaker_id": "2",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014394.1.11",
          "form": "오늘 티데이 도미노 50퍼라고 쿠폰 줘서요",
          "original_form": "오늘 티데이 도미노 50퍼라고 쿠폰 줘서요",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014394.1.12",
          "form": "또 먹고싶어요 ㅋㅋ",
          "original_form": "또 먹고싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014394.1.13",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014394.1.14",
          "form": "대박인데요",
          "original_form": "대박인데요",
          "speaker_id": "2",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014394.1.15",
          "form": "아들한테 피자먹을래? 물으니",
          "original_form": "아들한테 피자먹을래? 물으니",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014394.1.16",
          "form": "t가 참 할인이 많아요 ㅎㅎ",
          "original_form": "t가 참 할인이 많아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014394.1.17",
          "form": "바로 네!!",
          "original_form": "바로 네!!",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014394.1.18",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014394.1.19",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014394.1.20",
          "form": "에스케이 아니세용?",
          "original_form": "에스케이 아니세용?",
          "speaker_id": "1",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014394.1.21",
          "form": "애들은 바로 피자입니다",
          "original_form": "애들은 바로 피자입니다",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014394.1.22",
          "form": "네 ㅜㅜ",
          "original_form": "네 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014394.1.23",
          "form": "lg 입니다 ㅋㅋ",
          "original_form": "lg 입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014394.1.24",
          "form": "아...그러시구낭",
          "original_form": "아...그러시구낭",
          "speaker_id": "1",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014394.1.25",
          "form": "던킨 ㅋㅋ공짜로 주더라고요",
          "original_form": "던킨 ㅋㅋ공짜로 주더라고요",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014394.1.26",
          "form": "별거 없어요 ㅋㅋ",
          "original_form": "별거 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014394.1.27",
          "form": "오던킨도 좋네요",
          "original_form": "오던킨도 좋네요",
          "speaker_id": "1",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014394.1.28",
          "form": "무슨 피자요?",
          "original_form": "무슨 피자요?",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014394.1.29",
          "form": "아이들입맛 불고기피자요 ㅎ",
          "original_form": "아이들입맛 불고기피자요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014394.1.30",
          "form": "네 근데 ㅋㅋ던킨 근처에 없어요 ㅋㅋ",
          "original_form": "네 근데 ㅋㅋ던킨 근처에 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014394.1.31",
          "form": "오 저희도요",
          "original_form": "오 저희도요",
          "speaker_id": "2",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014394.1.32",
          "form": "불고기피자 시키는데 ㅋㅋ",
          "original_form": "불고기피자 시키는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014394.1.33",
          "form": "똑같아요",
          "original_form": "똑같아요",
          "speaker_id": "2",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014394.1.34",
          "form": "어딜가나 고기가 들어가야 ㅋㅋ",
          "original_form": "어딜가나 고기가 들어가야 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014394.1.35",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014394.1.36",
          "form": "그러고보니 저도 불고기네요 ㅋㅋ",
          "original_form": "그러고보니 저도 불고기네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014394.1.37",
          "form": "애들이 불고기 맛을 좋아해요",
          "original_form": "애들이 불고기 맛을 좋아해요",
          "speaker_id": "2",
