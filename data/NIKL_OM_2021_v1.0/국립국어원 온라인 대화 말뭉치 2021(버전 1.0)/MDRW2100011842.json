      "utterance": [
        {
          "id": "MDRW2100011842.1.1",
          "form": "해리포터 좋아하세요??",
          "original_form": "해리포터 좋아하세요??",
          "speaker_id": "1",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011842.1.2",
          "form": "네 옛날에 좋아했었어요",
          "original_form": "네 옛날에 좋아했었어요",
          "speaker_id": "2",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011842.1.3",
          "form": "거기에서 파는 해리포터 기념품을 사서 들고다녀보고 싶어요",
          "original_form": "거기에서 파는 해리포터 기념품을 사서 들고다녀보고 싶어요",
          "speaker_id": "1",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011842.1.4",
          "form": "별로 맛은 없다는데 버터맥주 먹어보고싶어요",
          "original_form": "별로 맛은 없다는데 버터맥주 먹어보고싶어요",
          "speaker_id": "2",
          "time": "20210824 16:35"
        },
        {
          "id": "MDRW2100011842.1.5",
          "form": "유니버셜에 가보고싶은 어트랙션 있으세요?",
          "original_form": "유니버셜에 가보고싶은 어트랙션 있으세요?",
          "speaker_id": "1",
          "time": "20210824 16:35"
        },
        {
          "id": "MDRW2100011842.1.6",
          "form": "맞아요 버터맥주도 궁금해요",
          "original_form": "맞아요 버터맥주도 궁금해요",
          "speaker_id": "1",
          "time": "20210824 16:35"
        },
        {
          "id": "MDRW2100011842.1.7",
          "form": "잘 기억은 안나는데 스파이더맨 관련해서",
          "original_form": "잘 기억은 안나는데 스파이더맨 관련해서",
          "speaker_id": "2",
          "time": "20210824 16:35"
        },
        {
          "id": "MDRW2100011842.1.8",
          "form": "재밋는거 있다고 들었던 것 같아요",
          "original_form": "재밋는거 있다고 들었던 것 같아요",
          "speaker_id": "2",
          "time": "20210824 16:35"
        },
        {
          "id": "MDRW2100011842.1.9",
          "form": "오오 뭔가 날아다닐 것 같네요",
          "original_form": "오오 뭔가 날아다닐 것 같네요",
          "speaker_id": "1",
          "time": "20210824 16:35"
        },
        {
          "id": "MDRW2100011842.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 16:35"
        },
        {
          "id": "MDRW2100011842.1.11",
          "form": "뉴욕에서는 어디 다녀오셨나요?",
          "original_form": "뉴욕에서는 어디 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011842.1.12",
          "form": "그리고 퍼레이드도 궁금해요",
          "original_form": "그리고 퍼레이드도 궁금해요",
          "speaker_id": "2",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011842.1.13",
          "form": "브로드웨이에서 뮤지컬 봤고",
          "original_form": "브로드웨이에서 뮤지컬 봤고",
          "speaker_id": "2",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011842.1.14",
          "form": "센트럴파크 자유의 여신상 엠파이어 빌딩..",
          "original_form": "센트럴파크 자유의 여신상 엠파이어 빌딩..",
          "speaker_id": "2",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011842.1.15",
          "form": "여행지는 다 갔던 것 같아요",
          "original_form": "여행지는 다 갔던 것 같아요",
          "speaker_id": "2",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011842.1.16",
          "form": "나이아가라도 갔었구요",
          "original_form": "나이아가라도 갔었구요",
          "speaker_id": "2",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011842.1.17",
          "form": "와 뮤지컬 재밌을 것 같아요",
          "original_form": "와 뮤지컬 재밌을 것 같아요",
          "speaker_id": "1",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011842.1.18",
          "form": "영어로 하면 다 알아들으세요?",
          "original_form": "영어로 하면 다 알아들으세요?",
          "speaker_id": "1",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011842.1.19",
          "form": "그땐 진짜 옛날이라",
          "original_form": "그땐 진짜 옛날이라",
          "speaker_id": "2",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011842.1.20",
          "form": "못알아들었는데",
          "original_form": "못알아들었는데",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011842.1.21",
          "form": "대충 눈치로 이해할 수 있었어요!",
          "original_form": "대충 눈치로 이해할 수 있었어요!",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011842.1.22",
          "form": "그정도면 된거죠뭐!",
          "original_form": "그정도면 된거죠뭐!",
          "speaker_id": "1",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011842.1.23",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011842.1.24",
          "form": "저는 오페라의 유령을 꼭 브로드웨이에서 보고싶어요",
          "original_form": "저는 오페라의 유령을 꼭 브로드웨이에서 보고싶어요",
          "speaker_id": "1",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011842.1.25",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011842.1.26",
          "form": "저 그거 봤어요",
          "original_form": "저 그거 봤어요",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011842.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011842.1.28",
          "form": "엄청 멋졌어요",
          "original_form": "엄청 멋졌어요",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011842.1.29",
          "form": "아무것도 모르는 중학생이었는데 한번 보고 완전 뮤지컬 푹 빠져서",
          "original_form": "아무것도 모르는 중학생이었는데 한번 보고 완전 뮤지컬 푹 빠져서",
          "speaker_id": "2",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011842.1.30",
          "form": "지금도 좋아해요",
          "original_form": "지금도 좋아해요",
          "speaker_id": "2",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011842.1.31",
          "form": "저는 아직 영화로만 봤어요",
          "original_form": "저는 아직 영화로만 봤어요",
          "speaker_id": "1",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011842.1.32",
          "form": "뮤지컬은 정말 미국의 중요한 관람거리인 것 같아요",
          "original_form": "뮤지컬은 정말 미국의 중요한 관람거리인 것 같아요",
          "speaker_id": "1",
          "time": "20210824 16:38"
        }
      ]
    }
