      "utterance": [
        {
          "id": "MDRW2100025752.1.1",
          "form": "최근에 사신거 있으세요?",
          "original_form": "최근에 사신거 있으세요?",
          "speaker_id": "1",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100025752.1.2",
          "form": "갤럭시핏 오늘 사려구요!",
          "original_form": "갤럭시핏 오늘 사려구요!",
          "speaker_id": "2",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100025752.1.3",
          "form": "우와 실시간이네요",
          "original_form": "우와 실시간이네요",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025752.1.4",
          "form": "운동할때 쓰시려는건가요?",
          "original_form": "운동할때 쓰시려는건가요?",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025752.1.5",
          "form": "아뇨 ㅋㅋㄱ시계보려구요 ㅋㅋㄱ",
          "original_form": "아뇨 ㅋㅋㄱ시계보려구요 ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025752.1.6",
          "form": "앜ㅋㅋㅋㅋ그러시구나",
          "original_form": "앜ㅋㅋㅋㅋ그러시구나",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025752.1.7",
          "form": "오프라인에서 사시나요?",
          "original_form": "오프라인에서 사시나요?",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025752.1.8",
          "form": "딱히 새 거 필요없어서 당근마켓으로 구입했어요 ㅋㅋ",
          "original_form": "딱히 새 거 필요없어서 당근마켓으로 구입했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100025752.1.9",
          "form": "당근좋죠!",
          "original_form": "당근좋죠!",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025752.1.10",
          "form": "당근자주 쓰세요?",
          "original_form": "당근자주 쓰세요?",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025752.1.11",
          "form": "ㅋㅋㄱ제가 구매한건 이번이 3번째일꺼에요 ㅋㅋ",
          "original_form": "ㅋㅋㄱ제가 구매한건 이번이 3번째일꺼에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025752.1.12",
          "form": "어떤거 사셨었나요?",
          "original_form": "어떤거 사셨었나요?",
          "speaker_id": "1",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025752.1.13",
          "form": "갤럭시버즈도 샀었고 노트북도 샀었어요!",
          "original_form": "갤럭시버즈도 샀었고 노트북도 샀었어요!",
          "speaker_id": "2",
          "time": "20210901 17:50"
        },
        {
          "id": "MDRW2100025752.1.14",
          "form": "전자제품 위주로 파셨구나",
          "original_form": "전자제품 위주로 파셨구나",
          "speaker_id": "1",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100025752.1.15",
          "form": "아 사셨구나",
          "original_form": "아 사셨구나",
          "speaker_id": "1",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100025752.1.16",
          "form": "네 ㅋㅋ샀어요 ㅋㅋㄱ",
          "original_form": "네 ㅋㅋ샀어요 ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100025752.1.17",
          "form": "저는 전자제품은 밭솥사봤어요",
          "original_form": "저는 전자제품은 밭솥사봤어요",
          "speaker_id": "1",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100025752.1.18",
          "form": "오 ㅋㅋㄱ가끔 새상품도 싸게 올리시는 분 계셔서 좋은 것 같아묘",
          "original_form": "오 ㅋㅋㄱ가끔 새상품도 싸게 올리시는 분 계셔서 좋은 것 같아묘",
