      "utterance": [
        {
          "id": "MDRW2100016030.1.1",
          "form": "안녕하세요 어떤음식 좋아하시나요?",
          "original_form": "안녕하세요 어떤음식 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 08:19"
        },
        {
          "id": "MDRW2100016030.1.2",
          "form": "안녕하세요 비빔밥 볶음밥좋아해요",
          "original_form": "안녕하세요 비빔밥 볶음밥좋아해요",
          "speaker_id": "2",
          "time": "20210826 08:20"
        },
        {
          "id": "MDRW2100016030.1.3",
          "form": "어떤 음식좋아하시나요?",
          "original_form": "어떤 음식좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 08:20"
        },
        {
          "id": "MDRW2100016030.1.4",
          "form": "저는 떡볶이 좋아해요!",
          "original_form": "저는 떡볶이 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 08:21"
        },
        {
          "id": "MDRW2100016030.1.5",
          "form": "오옷 저도 떡볶이좋아해요! 떡볶이 어디꺼 좋아하서야ㅡ?",
          "original_form": "오옷 저도 떡볶이좋아해요! 떡볶이 어디꺼 좋아하서야ㅡ?",
          "speaker_id": "2",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016030.1.6",
          "form": "저는 엽기 떡볶이 좋아해요  매운걸 못먹어서 착한맛만 시켜먹네요 ㅎㅎ",
          "original_form": "저는 엽기 떡볶이 좋아해요  매운걸 못먹어서 착한맛만 시켜먹네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 16:50"
        },
        {
          "id": "MDRW2100016030.1.7",
          "form": "앗 저도 매운거못먹는데",
          "original_form": "앗 저도 매운거못먹는데",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100016030.1.8",
          "form": "아하 동지네요 ㅎㅎ",
          "original_form": "아하 동지네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100016030.1.9",
          "form": "그래도 엽떡 순한맛은 먹을수있습니다ㅎㅎ\r\n그것도 살짝매콤하던데",
          "original_form": "그래도 엽떡 순한맛은 먹을수있습니다ㅎㅎ\r\n그것도 살짝매콤하던데",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100016030.1.10",
          "form": "앗 그렇군요 배달음식 자주시켜드시나요?",
          "original_form": "앗 그렇군요 배달음식 자주시켜드시나요?",
          "speaker_id": "1",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100016030.1.11",
          "form": "아 커피자주시켜먹는거같아요",
          "original_form": "아 커피자주시켜먹는거같아요",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100016030.1.12",
          "form": "그렇군요 어떤 커피 드시나요?",
          "original_form": "그렇군요 어떤 커피 드시나요?",
          "speaker_id": "1",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100016030.1.13",
          "form": "아 사실 커피를 좋아하긴하는데 막 가리진않아요ㅎㅎ배달음식 자주 드세요?",
          "original_form": "아 사실 커피를 좋아하긴하는데 막 가리진않아요ㅎㅎ배달음식 자주 드세요?",
          "speaker_id": "2",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100016030.1.14",
          "form": "아뇨 가끔 시켜먹어요",
          "original_form": "아뇨 가끔 시켜먹어요",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100016030.1.15",
          "form": "보통 뭐드세요?",
          "original_form": "보통 뭐드세요?",
          "speaker_id": "2",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100016030.1.16",
          "form": "치킨이요!",
          "original_form": "치킨이요!",
          "speaker_id": "1",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100016030.1.17",
          "form": "아 배달이면 치킨이긴하죠",
          "original_form": "아 배달이면 치킨이긴하죠",
          "speaker_id": "2",
          "time": "20210827 18:38"
        }
      ]
