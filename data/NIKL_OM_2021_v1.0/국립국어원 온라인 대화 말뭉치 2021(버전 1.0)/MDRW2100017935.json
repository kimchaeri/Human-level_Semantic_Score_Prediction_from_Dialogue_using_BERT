      "utterance": [
        {
          "id": "MDRW2100017935.1.1",
          "form": "빵좋아하시나요?",
          "original_form": "빵좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017935.1.2",
          "form": "네 밀가루종류는 다 좋아해요",
          "original_form": "네 밀가루종류는 다 좋아해요",
          "speaker_id": "2",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100017935.1.3",
          "form": "저두요~",
          "original_form": "저두요~",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017935.1.4",
          "form": "탄수화물이 왜케 맛있는지..",
          "original_form": "탄수화물이 왜케 맛있는지..",
          "speaker_id": "1",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017935.1.5",
          "form": "네 라면은 정말 매일먹어도 안질릴거같아여",
          "original_form": "네 라면은 정말 매일먹어도 안질릴거같아여",
          "speaker_id": "2",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017935.1.6",
          "form": "무슨 라면 드세요?",
          "original_form": "무슨 라면 드세요?",
          "speaker_id": "1",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017935.1.7",
          "form": "저는 맵찔이라 짜파게티가 제일 좋아요",
          "original_form": "저는 맵찔이라 짜파게티가 제일 좋아요",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017935.1.8",
          "form": "전평소에 진라면 먹어여",
          "original_form": "전평소에 진라면 먹어여",
          "speaker_id": "2",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017935.1.9",
          "form": "다른 라면들도 자주끓여먹고여ㅋㅋ",
          "original_form": "다른 라면들도 자주끓여먹고여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100017935.1.10",
          "form": "특별히 맛있게 끓이는 팁이 있나요?",
          "original_form": "특별히 맛있게 끓이는 팁이 있나요?",
          "speaker_id": "1",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100017935.1.11",
          "form": "저는 끓이는법 딱 지켜서 끓여여 그게 제일맛잇더라고요",
          "original_form": "저는 끓이는법 딱 지켜서 끓여여 그게 제일맛잇더라고요",
          "speaker_id": "2",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100017935.1.12",
          "form": "저는 자꾸 뭘 더 넣게 되서 내일은 오랜만에 오리지널로 끓여먹어봐야겠어요",
          "original_form": "저는 자꾸 뭘 더 넣게 되서 내일은 오랜만에 오리지널로 끓여먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100017935.1.13",
          "form": "아니면 물없이 스프반이랑 게란만 넣어서도 자주 먹어여",
          "original_form": "아니면 물없이 스프반이랑 게란만 넣어서도 자주 먹어여",
          "speaker_id": "2",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100017935.1.14",
          "form": "그건 무슨 레시피죠?",
          "original_form": "그건 무슨 레시피죠?",
          "speaker_id": "1",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100017935.1.15",
          "form": "그렇게 하고 익히나요?",
          "original_form": "그렇게 하고 익히나요?",
          "speaker_id": "1",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100017935.1.16",
          "form": "네 전 그렇게 해도 맛있더라고요",
          "original_form": "네 전 그렇게 해도 맛있더라고요",
          "speaker_id": "2",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100017935.1.17",
          "form": "인터넷에 찾아봐야겠네여",
          "original_form": "인터넷에 찾아봐야겠네여",
          "speaker_id": "1",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100017935.1.18",
          "form": "물은 안넣으세요?",
          "original_form": "물은 안넣으세요?",
          "speaker_id": "1",
          "time": "20210826 22:16"
        }
      ]
    }
