        },
        {
          "id": "MDRW2100001838.1.16",
          "form": "그쵸!!!",
          "original_form": "그쵸!!!",
          "speaker_id": "2",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100001838.1.17",
