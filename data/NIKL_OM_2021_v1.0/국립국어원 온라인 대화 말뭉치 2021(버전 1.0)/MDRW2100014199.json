        },
        {
          "id": "MDRW2100014199.1.2",
          "form": "드루와드루와",
          "original_form": "드루와드루와",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014199.1.3",
          "form": "됬다",
          "original_form": "됬다",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014199.1.4",
          "form": "요즘 운동 좀 해야겠어 하하",
          "original_form": "요즘 운동 좀 해야겠어 하하",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014199.1.5",
          "form": "운동을 해야되",
          "original_form": "운동을 해야되",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014199.1.6",
          "form": "전점 둥둥이가 되고있어",
          "original_form": "전점 둥둥이가 되고있어",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014199.1.7",
          "form": "조금만 움직여도 피곤해",
          "original_form": "조금만 움직여도 피곤해",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014199.1.8",
          "form": "마자 근력이 부족해서 힘들어",
          "original_form": "마자 근력이 부족해서 힘들어",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014199.1.9",
          "form": "집에서 홈트라도 해야되는데 말이야",
          "original_form": "집에서 홈트라도 해야되는데 말이야",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014199.1.10",
          "form": "해봐야지 하고 절대 안하는 나",
          "original_form": "해봐야지 하고 절대 안하는 나",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014199.1.11",
          "form": "자 우리 시작을 해봅시다",
          "original_form": "자 우리 시작을 해봅시다",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014199.1.12",
          "form": "아주조금이라도 움직여야해",
          "original_form": "아주조금이라도 움직여야해",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014199.1.13",
          "form": "그리고 비타민도 먹어야해",
          "original_form": "그리고 비타민도 먹어야해",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014199.1.14",
          "form": "오키도키 코시국에 우리 운동해서 면역을 기르자",
          "original_form": "오키도키 코시국에 우리 운동해서 면역을 기르자",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014199.1.15",
          "form": "그래 아프면안되니깐",
          "original_form": "그래 아프면안되니깐",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014199.1.16",
          "form": "오래오래 살아야지",
          "original_form": "오래오래 살아야지",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014199.1.17",
          "form": "비타민 챙겨먹음?",
          "original_form": "비타민 챙겨먹음?",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014199.1.18",
          "form": "아니 안먹어",
          "original_form": "아니 안먹어",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014199.1.19",
          "form": "비티민C는 사탕으로 먹고 있어",
          "original_form": "비티민C는 사탕으로 먹고 있어",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014199.1.20",
          "form": "ㅋㅋㅋㅋ 나도 있어도 안먹어 귀찮아",
          "original_form": "ㅋㅋㅋㅋ 나도 있어도 안먹어 귀찮아",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014199.1.21",
          "form": "하나씩 챙겨먹자",
          "original_form": "하나씩 챙겨먹자",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014199.1.22",
          "form": "이제부터라도 챙겨먹어야지 ㅋㅋ",
          "original_form": "이제부터라도 챙겨먹어야지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014199.1.23",
          "form": "오메가3 중요하댕",
          "original_form": "오메가3 중요하댕",
          "speaker_id": "1",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014199.1.24",
          "form": "마자! 근데그거 비리다며 그래도 뭐 먹어야겠지만",
          "original_form": "마자! 근데그거 비리다며 그래도 뭐 먹어야겠지만",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014199.1.25",
          "form": "비타민D도 중요하대",
          "original_form": "비타민D도 중요하대",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014199.1.26",
          "form": "옥상가서 햇빛을 봐줘야되",
          "original_form": "옥상가서 햇빛을 봐줘야되",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014199.1.27",
          "form": "맞아 비타민디는 조그매서 먹기좋음ㅋㅋㅋㅋ",
          "original_form": "맞아 비타민디는 조그매서 먹기좋음ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014199.1.28",
          "form": "아 나생각났어 나그거먹고있어",
          "original_form": "아 나생각났어 나그거먹고있어",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014199.1.29",
          "form": "뭐요",
          "original_form": "뭐요",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014199.1.30",
          "form": "자궁유산균 질건강유산균인가 그거",
          "original_form": "자궁유산균 질건강유산균인가 그거",
          "speaker_id": "2",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014199.1.31",
          "form": "프리바이오틱스",
          "original_form": "프리바이오틱스",
          "speaker_id": "2",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014199.1.32",
          "form": "아하 나도 전에 먹었었는데 별 효과모르겠어서 안삼..",
          "original_form": "아하 나도 전에 먹었었는데 별 효과모르겠어서 안삼..",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014199.1.33",
          "form": "그래도 먹으면 효과가 어딘가에 나타나겠지뭐 ㅋㅋㅋ",
          "original_form": "그래도 먹으면 효과가 어딘가에 나타나겠지뭐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014199.1.34",
          "form": "꾸준한게 좋은거임",
          "original_form": "꾸준한게 좋은거임",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014199.1.35",
          "form": "마자 약은 계속먹어야 효과가 나타난다고 하더라고",
          "original_form": "마자 약은 계속먹어야 효과가 나타난다고 하더라고",
          "speaker_id": "2",
          "time": "20210825 18:01"
        }
      ]
