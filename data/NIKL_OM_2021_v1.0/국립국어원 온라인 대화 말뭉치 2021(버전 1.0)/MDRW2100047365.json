      "utterance": [
        {
          "id": "MDRW2100047365.1.1",
          "form": "챙겨먹는편이랍니다ㅋㅋㅋ",
          "original_form": "챙겨먹는편이랍니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:43"
        },
        {
          "id": "MDRW2100047365.1.2",
          "form": "아침 삼겹도 가능해요",
          "original_form": "아침 삼겹도 가능해요",
          "speaker_id": "1",
          "time": "20210826 13:43"
        },
        {
          "id": "MDRW2100047365.1.3",
          "form": "저도 아침 삼겹 가능한데\n먹는거보다 잠이 좋아서 자느라 먹는 걸 포기했어요",
          "original_form": "저도 아침 삼겹 가능한데\n먹는거보다 잠이 좋아서 자느라 먹는 걸 포기했어요",
          "speaker_id": "2",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100047365.1.4",
          "form": "아침을 든든하게 먹어야 하는데 아침 잠이 더 맛있어서 포기가 안되요",
          "original_form": "아침을 든든하게 먹어야 하는데 아침 잠이 더 맛있어서 포기가 안되요",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100047365.1.5",
          "form": "와우 저랑좀다르시네요ㅋㅋㅋ",
          "original_form": "와우 저랑좀다르시네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100047365.1.6",
          "form": "아침잠이 소중하긴하죠",
          "original_form": "아침잠이 소중하긴하죠",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100047365.1.7",
          "form": "저녁은 뭐 드실거에요?",
          "original_form": "저녁은 뭐 드실거에요?",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100047365.1.8",
          "form": "고민중이랍니다ㅋㅋㅋ",
          "original_form": "고민중이랍니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100047365.1.9",
          "form": "배민등급이 무엇인가요?",
          "original_form": "배민등급이 무엇인가요?",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100047365.1.10",
          "form": "고마운분이요",
          "original_form": "고마운분이요",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100047365.1.11",
          "form": "별로",
          "original_form": "별로",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100047365.1.12",
          "form": "안드시는군요ㅋㅋㅋ",
          "original_form": "안드시는군요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100047365.1.13",
          "form": "배달은 잘 안해요. 집이 시골이라 배달 되는 데가 많이 없어요",
          "original_form": "배달은 잘 안해요. 집이 시골이라 배달 되는 데가 많이 없어요",
          "speaker_id": "2",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100047365.1.14",
          "form": "그렇군요ㅠㅜㅠ",
          "original_form": "그렇군요ㅠㅜㅠ",
          "speaker_id": "1",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100047365.1.15",
          "form": "저녁은드셨나요?",
          "original_form": "저녁은드셨나요?",
          "speaker_id": "1",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100047365.1.16",
          "form": "아직이요ㅡ 이제 메기 매운탕 먹으러 가려구요",
          "original_form": "아직이요ㅡ 이제 메기 매운탕 먹으러 가려구요",
          "speaker_id": "2",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100047365.1.17",
          "form": "저녁 드셨나요",
          "original_form": "저녁 드셨나요",
          "speaker_id": "2",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100047365.1.18",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100047365.1.19",
          "form": "너무맛난거드시는거아닌가요",
          "original_form": "너무맛난거드시는거아닌가요",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100047365.1.20",
          "form": "아 근데 국물이 짰어요.. \n메기는 살도 많고 민물새우 들어가서 좋았는데",
          "original_form": "아 근데 국물이 짰어요.. \n메기는 살도 많고 민물새우 들어가서 좋았는데",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100047365.1.21",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100047365.1.22",
          "form": "물을 좀더넣었어야했나요",
          "original_form": "물을 좀더넣었어야했나요",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100047365.1.23",
          "form": "육수 넣어달라 할까 하다 그냥 메기랑 새우만 건져 먹었어요.\n주인이 바뀐건지 맛이 좀 변한거 같아요",
          "original_form": "육수 넣어달라 할까 하다 그냥 메기랑 새우만 건져 먹었어요.\n주인이 바뀐건지 맛이 좀 변한거 같아요",
          "speaker_id": "2",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100047365.1.24",
          "form": "아쉽네요ㅠ",
          "original_form": "아쉽네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100047365.1.25",
          "form": "메기매운탕으로",
          "original_form": "메기매운탕으로",
          "speaker_id": "1",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100047365.1.26",
          "form": "실패하기쉽지않은데ㅠ",
          "original_form": "실패하기쉽지않은데ㅠ",
          "speaker_id": "1",
          "time": "20210826 23:53"
        }
      ]
    }
