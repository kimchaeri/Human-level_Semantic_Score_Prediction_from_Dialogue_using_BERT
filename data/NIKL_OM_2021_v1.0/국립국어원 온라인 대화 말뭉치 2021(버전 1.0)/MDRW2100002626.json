      "utterance": [
        {
          "id": "MDRW2100002626.1.1",
          "form": "여행은 어디 가 보셨어요?",
          "original_form": "여행은 어디 가 보셨어요?",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002626.1.2",
          "form": "작년에는 여수에 갔어요",
          "original_form": "작년에는 여수에 갔어요",
          "speaker_id": "2",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002626.1.3",
          "form": "올해 휴가는 아무데도 못갔네요",
          "original_form": "올해 휴가는 아무데도 못갔네요",
          "speaker_id": "2",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002626.1.4",
          "form": "저도 코로나 이후로는 아무데도 못가봤네요.",
          "original_form": "저도 코로나 이후로는 아무데도 못가봤네요.",
          "speaker_id": "1",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002626.1.5",
          "form": "평소에 여행가고 싶은 곳 있으세요???",
          "original_form": "평소에 여행가고 싶은 곳 있으세요???",
          "speaker_id": "1",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002626.1.6",
          "form": "휴양지에 가보고 싶어요",
          "original_form": "휴양지에 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002626.1.7",
          "form": "하와이 괌 그런데요",
          "original_form": "하와이 괌 그런데요",
          "speaker_id": "2",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002626.1.8",
          "form": "한번도 안가봐서요",
          "original_form": "한번도 안가봐서요",
          "speaker_id": "2",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002626.1.9",
          "form": "크 저도 하와이 가보고 싶어요",
          "original_form": "크 저도 하와이 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002626.1.10",
          "form": "그렇게 좋다고 하더라고요...",
          "original_form": "그렇게 좋다고 하더라고요...",
          "speaker_id": "1",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002626.1.11",
          "form": "티비 화면으로만 봐도 너무 좋아보이잖아요",
          "original_form": "티비 화면으로만 봐도 너무 좋아보이잖아요",
          "speaker_id": "2",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002626.1.12",
          "form": "쇼핑이면 쇼핑... 레져면 레져..",
          "original_form": "쇼핑이면 쇼핑... 레져면 레져..",
          "speaker_id": "1",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002626.1.13",
          "form": "주변에 결혼하면서 신혼여행으로 다녀온 애들이",
          "original_form": "주변에 결혼하면서 신혼여행으로 다녀온 애들이",
          "speaker_id": "1",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002626.1.14",
          "form": "그렇게 좋다고 꼭 가보라고 추천하더라고요",
          "original_form": "그렇게 좋다고 꼭 가보라고 추천하더라고요",
          "speaker_id": "1",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002626.1.15",
          "form": "제친구는 근데 하와이로 갓는데 산불이 나서",
          "original_form": "제친구는 근데 하와이로 갓는데 산불이 나서",
          "speaker_id": "2",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002626.1.16",
          "form": "단점 하나는 유명관광지다 보니 호텔들이 배가 불러서 좀 컨디션이 안좋다? 이거 하나래요..",
          "original_form": "단점 하나는 유명관광지다 보니 호텔들이 배가 불러서 좀 컨디션이 안좋다? 이거 하나래요..",
          "speaker_id": "1",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002626.1.17",
          "form": "호텔도 못들어갈뻔햇대요",
          "original_form": "호텔도 못들어갈뻔햇대요",
          "speaker_id": "2",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002626.1.18",
          "form": "왁!!!산불이요??????",
          "original_form": "왁!!!산불이요??????",
          "speaker_id": "1",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002626.1.19",
          "form": "저는 신혼여행으로 몰디브 다녀왔거든요.... 아무것도 안하고 휴양하려고.....",
          "original_form": "저는 신혼여행으로 몰디브 다녀왔거든요.... 아무것도 안하고 휴양하려고.....",
          "speaker_id": "1",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002626.1.20",
          "form": "몰디브도 엄청 좋습니다.",
          "original_form": "몰디브도 엄청 좋습니다.",
          "speaker_id": "1",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002626.1.21",
          "form": "좋을것같아요",
          "original_form": "좋을것같아요",
          "speaker_id": "2",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002626.1.22",
          "form": "이름부터가 뭔가 휴양이네요",
          "original_form": "이름부터가 뭔가 휴양이네요",
          "speaker_id": "2",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002626.1.23",
          "form": "거긴 조그마한 섬하나가 리조트 하나라....",
          "original_form": "거긴 조그마한 섬하나가 리조트 하나라....",
          "speaker_id": "1",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002626.1.24",
          "form": "아 그렇게 작아요?",
          "original_form": "아 그렇게 작아요?",
          "speaker_id": "2",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002626.1.25",
          "form": "정말 할께 아무것도 없이 스노쿨링 낚시 뿐이에요...",
          "original_form": "정말 할께 아무것도 없이 스노쿨링 낚시 뿐이에요...",
          "speaker_id": "1",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002626.1.26",
          "form": "스노쿨링 하셨어요?",
          "original_form": "스노쿨링 하셨어요?",
          "speaker_id": "2",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002626.1.27",
          "form": "네네 큰섬도 있긴 한거 같은데.. 보통 섬하나에 리조트 하나더라고요..",
          "original_form": "네네 큰섬도 있긴 한거 같은데.. 보통 섬하나에 리조트 하나더라고요..",
          "speaker_id": "1",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002626.1.28",
          "form": "상어랑 거북이랑 같이 스노쿨링 했죠",
          "original_form": "상어랑 거북이랑 같이 스노쿨링 했죠{emoji:♡}",
          "speaker_id": "1",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002626.1.29",
          "form": "티비보면 막 엄청깨끗해서 물고기 다보이고 그런던데",
          "original_form": "티비보면 막 엄청깨끗해서 물고기 다보이고 그런던데",
          "speaker_id": "2",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002626.1.30",
          "form": "와 상어도 있나요",
          "original_form": "와 상어도 있나요",
          "speaker_id": "2",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002626.1.31",
          "form": "네네 그렇더라고요...",
          "original_form": "네네 그렇더라고요...",
          "speaker_id": "1",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002626.1.32",
          "form": "너무 좋으셨겠어요",
          "original_form": "너무 좋으셨겠어요",
          "speaker_id": "2",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002626.1.33",
          "form": "네 해초먹는 상어가 헤엄쳐 다니더라고요..",
          "original_form": "네 해초먹는 상어가 헤엄쳐 다니더라고요..",
          "speaker_id": "1",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002626.1.34",
          "form": "여행은 역시 그런곳으로 가야해요",
          "original_form": "여행은 역시 그런곳으로 가야해요",
          "speaker_id": "2",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002626.1.35",
          "form": "제인생 마지막 휴식이였나 봅니다..",
          "original_form": "제인생 마지막 휴식이였나 봅니다..",
          "speaker_id": "1",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002626.1.36",
          "form": "설마요ㅋㅋ",
          "original_form": "설마요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002626.1.37",
          "form": "애들 다클때 까지는 못갈곳으로 골라서 갔는데..",
          "original_form": "애들 다클때 까지는 못갈곳으로 골라서 갔는데..",
          "speaker_id": "1",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002626.1.38",
          "form": "정말 못갈꺼 같아요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "정말 못갈꺼 같아요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002626.1.39",
          "form": "애기들 데리고도 많이 가던데",
          "original_form": "애기들 데리고도 많이 가던데",
          "speaker_id": "2",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002626.1.40",
          "form": "음..... 애들이 너무 가만히 있지 못해서...",
          "original_form": "음..... 애들이 너무 가만히 있지 못해서...",
          "speaker_id": "1",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002626.1.41",
          "form": "비행시간 긴곳은 못가겠더라고요.",
          "original_form": "비행시간 긴곳은 못가겠더라고요.",
          "speaker_id": "1",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002626.1.42",
          "form": "아 하긴 비행기가 문제겠네요",
          "original_form": "아 하긴 비행기가 문제겠네요",
          "speaker_id": "2",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002626.1.43",
          "form": "괌이 아이들데리고도 많이가고... 태교여행으로 많이가는데...",
          "original_form": "괌이 아이들데리고도 많이가고... 태교여행으로 많이가는데...",
          "speaker_id": "1",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002626.1.44",
          "form": "신혼여행으로 가는 친구들이 너무 힘들었다고 하더라고요..",
          "original_form": "신혼여행으로 가는 친구들이 너무 힘들었다고 하더라고요..",
          "speaker_id": "1",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002626.1.45",
          "form": "비행내내 아이들 울고... 칭얼대서.ㅠㅠㅠ",
          "original_form": "비행내내 아이들 울고... 칭얼대서.ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002626.1.46",
          "form": "아아 그렇군여",
          "original_form": "아아 그렇군여",
          "speaker_id": "2",
          "time": "20210820 14:18"
        }
      ]
