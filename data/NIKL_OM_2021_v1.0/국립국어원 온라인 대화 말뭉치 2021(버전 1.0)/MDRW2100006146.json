      "utterance": [
        {
          "id": "MDRW2100006146.1.1",
          "form": "그런데 칠성주는 음식점은 잘 없지않나요??",
          "original_form": "그런데 칠성주는 음식점은 잘 없지않나요??",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100006146.1.2",
          "form": "어....맞아요",
          "original_form": "어....맞아요",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100006146.1.3",
          "form": "그래서 맨날 물어봐요",
          "original_form": "그래서 맨날 물어봐요",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100006146.1.4",
          "form": "아아ㅠㅜ없으시면 대체로 어떤거 드시나요",
          "original_form": "아아ㅠㅜ없으시면 대체로 어떤거 드시나요",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100006146.1.5",
          "form": "스프라이트 있는곳엔..코.카.콜.라가 있습니다 ㅎㅎㅎㅎㅎㅎ",
          "original_form": "스프라이트 있는곳엔..코.카.콜.라가 있습니다 ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100006146.1.6",
          "form": "코카콜라죠!",
          "original_form": "코카콜라죠!",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100006146.1.7",
          "form": "앜ㅋㅋㅋㅋ맞아요",
          "original_form": "앜ㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100006146.1.8",
          "form": "전 집에 코카콜라 박스로 두고 마셔서",
          "original_form": "전 집에 코카콜라 박스로 두고 마셔서",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100006146.1.9",
          "form": "펩시오면 체리콕 만들어먹어요",
          "original_form": "펩시오면 체리콕 만들어먹어요",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100006146.1.10",
          "form": "오오 그런것도 할줄아세요?",
          "original_form": "오오 그런것도 할줄아세요?",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006146.1.11",
          "form": "네 그런데 만들기 쉬워요",
          "original_form": "네 그런데 만들기 쉬워요",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006146.1.12",
          "form": "체리콕 가루만 준비하시면되요!",
          "original_form": "체리콕 가루만 준비하시면되요!",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006146.1.13",
          "form": "아하 가루를 따로 사기만 하면되는군요",
          "original_form": "아하 가루를 따로 사기만 하면되는군요",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006146.1.14",
          "form": "몰랐네요",
          "original_form": "몰랐네요",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006146.1.15",
          "form": "제가 카페 알바 경력이 있는데",
          "original_form": "제가 카페 알바 경력이 있는데",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006146.1.16",
          "form": "음료 만들기 은근 쉬워요",
          "original_form": "음료 만들기 은근 쉬워요",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006146.1.17",
          "form": "오오 그래요?",
          "original_form": "오오 그래요?",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100006146.1.18",
          "form": "가장 쉬운게 무엇인가요?",
          "original_form": "가장 쉬운게 무엇인가요?",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100006146.1.19",
          "form": "에이드류가 가장 쉬워요",
          "original_form": "에이드류가 가장 쉬워요",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100006146.1.20",
          "form": "알고있으셨나요?",
          "original_form": "알고있으셨나요?",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100006146.1.21",
          "form": "아 그건ㅋㅋㅋㅋ 정말 시럽넣고",
          "original_form": "아 그건ㅋㅋㅋㅋ 정말 시럽넣고",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100006146.1.22",
          "form": "탄산이면",
          "original_form": "탄산이면",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100006146.1.23",
          "form": "끝이더라구요",
          "original_form": "끝이더라구요",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100006146.1.24",
          "form": "맞아요 가장 쉬워요",
          "original_form": "맞아요 가장 쉬워요",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100006146.1.25",
          "form": "앜ㅋㅋㅋ알고있으시네요",
          "original_form": "앜ㅋㅋㅋ알고있으시네요",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006146.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ사먹기맨날아까운데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ사먹기맨날아까운데",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006146.1.27",
          "form": "먹습니다",
          "original_form": "먹습니다",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006146.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006146.1.29",
          "form": "ㅋㅋㅋㅋ카페 알바하신거에요?",
          "original_form": "ㅋㅋㅋㅋ카페 알바하신거에요?",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100006146.1.30",
          "form": "아녀아녀",
          "original_form": "아녀아녀",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100006146.1.31",
          "form": "전 궁금해서 열심히 봤어요",
          "original_form": "전 궁금해서 열심히 봤어요",
