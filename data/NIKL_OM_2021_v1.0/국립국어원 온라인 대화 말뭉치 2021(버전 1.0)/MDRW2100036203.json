      "utterance": [
        {
          "id": "MDRW2100036203.1.1",
          "form": "맞아요 ㅜㅜ 저는 아이유 예전 노래를 더 좋아하나봐요 !",
          "original_form": "맞아요 ㅜㅜ 저는 아이유 예전 노래를 더 좋아하나봐요 !",
          "speaker_id": "2",
          "time": "20210830 19:07"
        },
        {
          "id": "MDRW2100036203.1.2",
          "form": "지금 노래도 좋지만 ㅎㅎㅎ",
          "original_form": "지금 노래도 좋지만 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:08"
        },
        {
          "id": "MDRW2100036203.1.3",
          "form": "헛 저도요 ㅠ.ㅠ 약간 언제러고 해야지",
          "original_form": "헛 저도요 ㅠ.ㅠ 약간 언제러고 해야지",
          "speaker_id": "1",
          "time": "20210830 19:09"
        },
        {
          "id": "MDRW2100036203.1.4",
          "form": "그 두번째 리메이크 앨범 나왔을 때까지가",
          "original_form": "그 두번째 리메이크 앨범 나왔을 때까지가",
          "speaker_id": "1",
          "time": "20210830 19:09"
        },
        {
          "id": "MDRW2100036203.1.5",
          "form": "제 최애들이었어요 ㅠ.ㅠ",
          "original_form": "제 최애들이었어요 ㅠ.ㅠ",
          "speaker_id": "1",
          "time": "20210830 19:10"
        },
        {
          "id": "MDRW2100036203.1.6",
          "form": "그 다음부터는 저랑 스타일이 좀 다르더라고요",
          "original_form": "그 다음부터는 저랑 스타일이 좀 다르더라고요",
          "speaker_id": "1",
          "time": "20210830 19:10"
        },
        {
          "id": "MDRW2100036203.1.7",
          "form": "아 맞아요 !!",
          "original_form": "아 맞아요 !!",
          "speaker_id": "2",
          "time": "20210830 19:12"
        },
        {
          "id": "MDRW2100036203.1.8",
          "form": "저두요 ㅜㅜ",
          "original_form": "저두요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 19:12"
        },
        {
          "id": "MDRW2100036203.1.9",
          "form": "뭔가 그 노래 분위기? 라 해야될까요",
          "original_form": "뭔가 그 노래 분위기? 라 해야될까요",
          "speaker_id": "2",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100036203.1.10",
          "form": "그게 저랑 안맞더라고요 지금 노래들은 ㅜㅜ",
          "original_form": "그게 저랑 안맞더라고요 지금 노래들은 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 19:14"
        },
        {
          "id": "MDRW2100036203.1.11",
          "form": "맞아요 ㅠ 저 삐삐도 좀 낯설었는대",
          "original_form": "맞아요 ㅠ 저 삐삐도 좀 낯설었는대",
          "speaker_id": "1",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100036203.1.12",
          "form": "이번에 나왔던 라일락이랑 코인 다",
          "original_form": "이번에 나왔던 라일락이랑 코인 다",
          "speaker_id": "1",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100036203.1.13",
          "form": "넘 적응이 안 되더라고용 ㅠ.^ 아쉬워요",
          "original_form": "넘 적응이 안 되더라고용 ㅠ.^ 아쉬워요",
          "speaker_id": "1",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100036203.1.14",
          "form": "헐 저두요 ㅜㅜ",
          "original_form": "헐 저두요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100036203.1.15",
          "form": "이번 노래 그나마 라일락만 듣고 나머지는 안들었어요 ...",
          "original_form": "이번 노래 그나마 라일락만 듣고 나머지는 안들었어요 ...",
          "speaker_id": "2",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100036203.1.16",
          "form": "맞어요...... name2님은 그럼 좀 잔잔한 노래거 취향이신거예요!?",
          "original_form": "맞어요...... &name2&님은 그럼 좀 잔잔한 노래거 취향이신거예요!?",
          "speaker_id": "1",
          "time": "20210830 19:21"
        },
        {
          "id": "MDRW2100036203.1.17",
          "form": "잔잔한 노래를 더 좋아하지만",
          "original_form": "잔잔한 노래를 더 좋아하지만",
          "speaker_id": "2",
          "time": "20210830 19:23"
