      "utterance": [
        {
          "id": "MDRW2100038648.1.1",
          "form": "그럼 이마트편으점  가야겠당",
          "original_form": "그럼 이마트편으점  가야겠당",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100038648.1.2",
          "form": "필요하면가요",
          "original_form": "필요하면가요",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100038648.1.3",
          "form": "주로 뭐사세요?",
          "original_form": "주로 뭐사세요?",
          "speaker_id": "1",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100038648.1.4",
          "form": "필요한게 뭐지",
          "original_form": "필요한게 뭐지",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100038648.1.5",
          "form": "우유? 삼김? 막걸리?",
          "original_form": "우유? 삼김? 막걸리?",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100038648.1.6",
          "form": "물?",
          "original_form": "물?",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100038648.1.7",
          "form": "막걸리?",
          "original_form": "막걸리?",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100038648.1.8",
          "form": "자주드세요?",
          "original_form": "자주드세요?",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100038648.1.9",
          "form": "때가되믄 묵지요",
          "original_form": "때가되믄 묵지요",
          "speaker_id": "2",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100038648.1.10",
          "form": "ㅋㅋ오늘 비오는데 한잔,",
          "original_form": "ㅋㅋ오늘 비오는데 한잔,",
          "speaker_id": "1",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100038648.1.11",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100038648.1.12",
          "form": "ㅋㅋ2차 백신 맞은지아직 일주일안되엇소만",
          "original_form": "ㅋㅋ2차 백신 맞은지아직 일주일안되엇소만",
          "speaker_id": "2",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100038648.1.13",
          "form": "일주일뒤에 밤 막걸리드세요",
          "original_form": "일주일뒤에 밤 막걸리드세요",
          "speaker_id": "1",
          "time": "20210902 18:29"
        },
        {
