          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100038648.1.17",
          "form": "잘알겠슈ㅋㅋ",
          "original_form": "잘알겠슈ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100038648.1.18",
          "form": "반주로 알아서 먹거등요",
          "original_form": "반주로 알아서 먹거등요",
          "speaker_id": "2",
          "time": "20210902 18:58"
        },
