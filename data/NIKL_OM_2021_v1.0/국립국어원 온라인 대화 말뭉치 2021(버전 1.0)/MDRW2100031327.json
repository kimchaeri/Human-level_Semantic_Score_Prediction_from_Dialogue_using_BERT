      "utterance": [
        {
          "id": "MDRW2100031327.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100031327.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210904 20:28"
        },
        {
          "id": "MDRW2100031327.1.3",
          "form": "저녁식사 하셨나요?",
          "original_form": "저녁식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210904 20:28"
        },
        {
          "id": "MDRW2100031327.1.4",
          "form": "네 간단히 디저트류 먹었습니다. 식사 하셨어요?:)",
          "original_form": "네 간단히 디저트류 먹었습니다. 식사 하셨어요?:)",
          "speaker_id": "2",
          "time": "20210904 20:29"
        },
        {
          "id": "MDRW2100031327.1.5",
          "form": "저는 닭볶음탕 해서 먹었어요 ㅎㅎ",
          "original_form": "저는 닭볶음탕 해서 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 20:29"
        },
        {
          "id": "MDRW2100031327.1.6",
          "form": "디저트류 맛있었을 것 같네요 ㅠㅠ",
          "original_form": "디저트류 맛있었을 것 같네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 20:29"
        },
        {
          "id": "MDRW2100031327.1.7",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210904 20:29"
        },
        {
          "id": "MDRW2100031327.1.8",
          "form": "아 자주 요리하시나요?:)",
          "original_form": "아 자주 요리하시나요?:)",
          "speaker_id": "2",
          "time": "20210904 20:31"
        },
        {
          "id": "MDRW2100031327.1.9",
          "form": "당근케익이랑 커피 마셨습니담!",
          "original_form": "당근케익이랑 커피 마셨습니담!",
          "speaker_id": "2",
          "time": "20210904 20:31"
        },
        {
          "id": "MDRW2100031327.1.10",
          "form": "당근케익 ㅠㅠㅠ 케이크 안먹는지 오래 되서 너무 먹고 싶네요",
          "original_form": "당근케익 ㅠㅠㅠ 케이크 안먹는지 오래 되서 너무 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210904 20:31"
        },
        {
          "id": "MDRW2100031327.1.11",
          "form": "음 자주는 아닌데 좋아하는 편이에요!",
          "original_form": "음 자주는 아닌데 좋아하는 편이에요!",
          "speaker_id": "1",
          "time": "20210904 20:31"
        },
        {
          "id": "MDRW2100031327.1.12",
          "form": "name2님은 요리 좋아하시나요?",
          "original_form": "&name2&님은 요리 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100031327.1.13",
          "form": "아아ㅜㅜ그렇군요!  혹시 안먹은지 오래된 이유가 있으신가요?",
          "original_form": "아아ㅜㅜ그렇군요!  혹시 안먹은지 오래된 이유가 있으신가요?",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031327.1.14",
          "form": "아 ㅎㅎ 저는 요리를 잘 못하다 보니까 잘 안하게되는것같아요",
          "original_form": "아 ㅎㅎ 저는 요리를 잘 못하다 보니까 잘 안하게되는것같아요",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031327.1.15",
          "form": "괜찮은 당근케익집이 너무 멀어서 못갔거든요 ㅠㅠ",
          "original_form": "괜찮은 당근케익집이 너무 멀어서 못갔거든요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031327.1.16",
          "form": "아아ㅜㅜ 그럴수있죠!",
          "original_form": "아아ㅜㅜ 그럴수있죠!",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100031327.1.17",
          "form": "*그럴때",
          "original_form": "*그럴때",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100031327.1.18",
          "form": "당근 싫어하는데 당근케익은 참 맛있더라구요!",
          "original_form": "당근 싫어하는데 당근케익은 참 맛있더라구요!",
          "speaker_id": "1",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100031327.1.19",
          "form": "아 ㅎㅎ 저두요!",
          "original_form": "아 ㅎㅎ 저두요!",
          "speaker_id": "2",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100031327.1.20",
          "form": "혹시 계피가루도 좋아하시나요?",
          "original_form": "혹시 계피가루도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100031327.1.21",
          "form": "네 좋아하는 편이에요!",
          "original_form": "네 좋아하는 편이에요!",
          "speaker_id": "1",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100031327.1.22",
          "form": "시나몬롤도 너무 좋아하고",
          "original_form": "시나몬롤도 너무 좋아하고",
          "speaker_id": "1",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100031327.1.23",
          "form": "그냥 잘 먹게 되더라구요 ㅎㅎ",
          "original_form": "그냥 잘 먹게 되더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031327.1.24",
          "form": "아 ㅎㅎ그러시군요! 저도 좋아하는편이여서",
          "original_form": "아 ㅎㅎ그러시군요! 저도 좋아하는편이여서",
          "speaker_id": "2",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031327.1.25",
          "form": "시나몬번이나 츄러스도 좋아합니당!",
          "original_form": "시나몬번이나 츄러스도 좋아합니당!",
          "speaker_id": "2",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100031327.1.26",
          "form": "츄러스도 안먹은지 참 오래된것 같네요 ㅎㅎㅎ",
          "original_form": "츄러스도 안먹은지 참 오래된것 같네요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 20:39"
        }
      ]
