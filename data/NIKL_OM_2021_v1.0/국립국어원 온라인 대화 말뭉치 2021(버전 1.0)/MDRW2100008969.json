      "utterance": [
        {
          "id": "MDRW2100008969.1.1",
          "form": "사촌 언니 제주도 여행감ㅎ...",
          "original_form": "사촌 언니 제주도 여행감ㅎ...",
          "speaker_id": "1",
          "time": "20210822 20:01"
        },
        {
          "id": "MDRW2100008969.1.2",
          "form": "헐 부럽다...",
          "original_form": "헐 부럽다...",
          "speaker_id": "2",
          "time": "20210822 20:08"
        },
        {
          "id": "MDRW2100008969.1.3",
          "form": "나도 여행가고싶어...",
          "original_form": "나도 여행가고싶어...",
          "speaker_id": "2",
          "time": "20210822 20:08"
        },
        {
          "id": "MDRW2100008969.1.4",
          "form": "아니 지금 시국이 어떤 시국이냐고~~~",
          "original_form": "아니 지금 시국이 어떤 시국이냐고~~~",
          "speaker_id": "1",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100008969.1.5",
          "form": "출장도 아니고말이야",
          "original_form": "출장도 아니고말이야",
          "speaker_id": "1",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100008969.1.6",
          "form": "코세글자 끝나기만해봐라",
          "original_form": "코세글자 끝나기만해봐라",
          "speaker_id": "1",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100008969.1.7",
          "form": "돈모아서 해외간다",
          "original_form": "돈모아서 해외간다",
          "speaker_id": "1",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100008969.1.8",
          "form": "그치 시국이 시국인지라....",
          "original_form": "그치 시국이 시국인지라....",
          "speaker_id": "2",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100008969.1.9",
          "form": "나도 코로나끝나면 해외여행 가고싶다",
          "original_form": "나도 코로나끝나면 해외여행 가고싶다",
          "speaker_id": "2",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100008969.1.10",
          "form": "너는 해외 어디가고싶어?",
          "original_form": "너는 해외 어디가고싶어?",
          "speaker_id": "1",
          "time": "20210822 20:47"
        },
        {
          "id": "MDRW2100008969.1.11",
          "form": "나는 미국...?",
          "original_form": "나는 미국...?",
          "speaker_id": "2",
          "time": "20210822 21:06"
        },
        {
          "id": "MDRW2100008969.1.12",
          "form": "진짜 미국한번 가보고싶가",
          "original_form": "진짜 미국한번 가보고싶가",
          "speaker_id": "2",
          "time": "20210822 21:06"
        },
        {
          "id": "MDRW2100008969.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:06"
        },
        {
          "id": "MDRW2100008969.1.14",
          "form": "미국 하 뉴욕~~~~~",
          "original_form": "미국 하 뉴욕~~~~~",
          "speaker_id": "1",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100008969.1.15",
          "form": "나는 유럽...",
          "original_form": "나는 유럽...",
          "speaker_id": "1",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100008969.1.16",
          "form": "실물 그림 보고싶어",
          "original_form": "실물 그림 보고싶어",
          "speaker_id": "1",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100008969.1.17",
          "form": "전시회...ㅇ",
          "original_form": "전시회...ㅇ",
          "speaker_id": "1",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100008969.1.18",
          "form": "나도 유럽도 가보고싶다...",
          "original_form": "나도 유럽도 가보고싶다...",
          "speaker_id": "2",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100008969.1.19",
          "form": "아니면 스위스ㅠㅠ 풍경대박이더라",
          "original_form": "아니면 스위스ㅠㅠ 풍경대박이더라",
          "speaker_id": "1",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100008969.1.20",
          "form": "난 유럽가서 유럽축구도 보고싶음....",
          "original_form": "난 유럽가서 유럽축구도 보고싶음....",
          "speaker_id": "2",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100008969.1.21",
          "form": "스위스 크으알프스 크으",
          "original_form": "스위스 크으알프스 크으",
          "speaker_id": "2",
          "time": "20210822 21:59"
        },
        {
          "id": "MDRW2100008969.1.22",
          "form": "다 본인이 평소 좋아하던거ㅋㅋ",
          "original_form": "다 본인이 평소 좋아하던거ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100008969.1.23",
          "form": "너는 유럽가서 직관하면 좋겠다",
          "original_form": "너는 유럽가서 직관하면 좋겠다",
          "speaker_id": "1",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100008969.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ사실 응원하는팀 없지만",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ사실 응원하는팀 없지만",
          "speaker_id": "2",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100008969.1.25",
          "form": "보고는싶다",
          "original_form": "보고는싶다",
          "speaker_id": "2",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100008969.1.26",
          "form": "미국 이랑 유럽이랑..다가보고싶네",
          "original_form": "미국 이랑 유럽이랑..다가보고싶네",
          "speaker_id": "2",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100008969.1.27",
          "form": "진짜 열심히 돈벌어서 여행가자",
          "original_form": "진짜 열심히 돈벌어서 여행가자",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100008969.1.28",
          "form": "둘이 같이 가보자ㅋㅋ 너랑 어딜 간적이 별로없네",
          "original_form": "둘이 같이 가보자ㅋㅋ 너랑 어딜 간적이 별로없네",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100008969.1.29",
          "form": "아 코로나 끝나면 꼭 해외여행가야지",
          "original_form": "아 코로나 끝나면 꼭 해외여행가야지",
