          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006880.1.18",
          "form": "초장살짝 아님?",
          "original_form": "초장살짝 아님?",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006880.1.19",
          "form": "물에 불려서",
          "original_form": "물에 불려서",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006880.1.20",
          "form": "아예 비빔밥용으로 소스도 주는 게 있음",
          "original_form": "아예 비빔밥용으로 소스도 주는 게 있음",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006880.1.21",
          "form": "해초도 다양하고 소스도 맛있음",
          "original_form": "해초도 다양하고 소스도 맛있음",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006880.1.22",
          "form": "기장물산비빔밥용해초검색해보셈",
          "original_form": "기장물산비빔밥용해초검색해보셈",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006880.1.23",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006880.1.24",
