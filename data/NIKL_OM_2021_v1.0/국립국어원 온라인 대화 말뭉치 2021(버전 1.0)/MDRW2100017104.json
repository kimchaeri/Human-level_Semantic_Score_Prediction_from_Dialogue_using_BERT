      "utterance": [
        {
          "id": "MDRW2100017104.1.1",
          "form": "ㅋㅋㅋ어제 뭐드셨어요?",
          "original_form": "ㅋㅋㅋ어제 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017104.1.2",
          "form": "전 어제 초밥을 먹었습니다",
          "original_form": "전 어제 초밥을 먹었습니다",
          "speaker_id": "2",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017104.1.3",
          "form": "어제 냉동 파스타 먹었어요 ㅋㅋㅋ",
          "original_form": "어제 냉동 파스타 먹었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017104.1.4",
          "form": "어떤 초밥 좋아하세요?",
          "original_form": "어떤 초밥 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017104.1.5",
          "form": "저 연어랑 유부초밥이요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 연어랑 유부초밥이요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017104.1.6",
          "form": "저도 둘다 좋아해요 연어가 젤 맛있더라구요",
          "original_form": "저도 둘다 좋아해요 연어가 젤 맛있더라구요",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017104.1.7",
          "form": "유부초밥 만들어먹어도 맛있던데 귀찮아가지구 ㅋㅋㅋㅋ",
          "original_form": "유부초밥 만들어먹어도 맛있던데 귀찮아가지구 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017104.1.8",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017104.1.9",
          "form": "전 이상하게 다른 유부는 별론데 유부초밥은 맛있어요",
          "original_form": "전 이상하게 다른 유부는 별론데 유부초밥은 맛있어요",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017104.1.10",
          "form": "그쵸??!!!",
          "original_form": "그쵸??!!!",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017104.1.11",
          "form": "다똑같나봐요 ㅋㅋㅋ",
          "original_form": "다똑같나봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017104.1.12",
          "form": "ㅋㅋㅋㅋ그러게요 신기하네요",
          "original_form": "ㅋㅋㅋㅋ그러게요 신기하네요",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017104.1.13",
          "form": "파스타소스는 어떤걸로 드셨어요?",
          "original_form": "파스타소스는 어떤걸로 드셨어요?",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017104.1.14",
          "form": "토마토? 로제? 크림? 오일?",
          "original_form": "토마토? 로제? 크림? 오일?",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017104.1.15",
          "form": "라구소스 먹었는데 괜찮더라구요",
          "original_form": "라구소스 먹었는데 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017104.1.16",
          "form": "토마토베이스파스타 먹은 지 좀 됐는뎅... 맛있겠네용ㅋㅋㅋ",
          "original_form": "토마토베이스파스타 먹은 지 좀 됐는뎅... 맛있겠네용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017104.1.17",
          "form": "원래 크림류 좋아하는데 있길래 먹었어요 ㅋㅋㅋ",
          "original_form": "원래 크림류 좋아하는데 있길래 먹었어요 ㅋㅋㅋ",
