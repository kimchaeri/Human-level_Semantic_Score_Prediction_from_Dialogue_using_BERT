      "utterance": [
        {
          "id": "MDRW2100037706.1.1",
          "form": "커피좋아하세요?",
          "original_form": "커피좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100037706.1.2",
          "form": "프라푸치노나 바닐라라떼같이 달달한커피는 먹어요",
          "original_form": "프라푸치노나 바닐라라떼같이 달달한커피는 먹어요",
          "speaker_id": "2",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100037706.1.3",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100037706.1.4",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100037706.1.5",
          "form": "전 아메파에요! 가끔 라떼도 마시지만 !",
          "original_form": "전 아메파에요! 가끔 라떼도 마시지만 !",
          "speaker_id": "1",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100037706.1.6",
          "form": "어디브랜드꺼 즐겨 마시나요,",
          "original_form": "어디브랜드꺼 즐겨 마시나요,",
          "speaker_id": "2",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100037706.1.7",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100037706.1.8",
          "form": "동네에 다양하게잇어서 매일 다르지만",
          "original_form": "동네에 다양하게잇어서 매일 다르지만",
          "speaker_id": "1",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100037706.1.9",
          "form": "스벅도 가고빽다방도가고 ㅋㅋ 이디야도가고 ㅋㅋ개인커피숍도가고 컴포즈도 가고",
          "original_form": "스벅도 가고빽다방도가고 ㅋㅋ 이디야도가고 ㅋㅋ개인커피숍도가고 컴포즈도 가고",
          "speaker_id": "1",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100037706.1.10",
          "form": "매번 달라요!!",
          "original_form": "매번 달라요!!",
          "speaker_id": "1",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100037706.1.11",
          "form": "그럼 어디 아아를 가장 좋아하시나요?",
          "original_form": "그럼 어디 아아를 가장 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100037706.1.12",
          "form": "한군데만 꼽는다면 ㅠㅠㅍ",
          "original_form": "한군데만 꼽는다면 ㅠㅠㅍ",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100037706.1.13",
          "form": "전 빽다방이요!!",
          "original_form": "전 빽다방이요!!",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100037706.1.14",
          "form": "사장님도 친절하고 ㅋㅋ 부수적인 이유도 잇네요!",
          "original_form": "사장님도 친절하고 ㅋㅋ 부수적인 이유도 잇네요!",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100037706.1.15",
          "form": "그렇군요ㅋㅋㅋㅋ 빽다방 아아는 좀 단가요??",
          "original_form": "그렇군요ㅋㅋㅋㅋ 빽다방 아아는 좀 단가요??",
          "speaker_id": "2",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100037706.1.16",
          "form": "아뇨! 진해요 타브랜드에비해 제가 느끼기엔!!",
          "original_form": "아뇨! 진해요 타브랜드에비해 제가 느끼기엔!!",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100037706.1.17",
          "form": "쓴 거를 잘 드시나봐요 저는 쓴 걸 못먹어서 아메리카노를 못 먹거든요",
          "original_form": "쓴 거를 잘 드시나봐요 저는 쓴 걸 못먹어서 아메리카노를 못 먹거든요",
          "speaker_id": "2",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100037706.1.18",
          "form": "아ㅜㅠ! 전 달달구리도좋지만",
          "original_form": "아ㅜㅠ! 전 달달구리도좋지만",
          "speaker_id": "1",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100037706.1.19",
          "form": "커피마실때 아 커피들어오는구가 이느낌이좋아요 ㅋㅋ",
          "original_form": "커피마실때 아 커피들어오는구가 이느낌이좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:50"
        },
        {
          "id": "MDRW2100037706.1.20",
          "form": "아메리카노를 마실 때 포션을 마시는 기분인걸까요?",
          "original_form": "아메리카노를 마실 때 포션을 마시는 기분인걸까요?",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100037706.1.21",
          "form": "음? 포션??",
          "original_form": "음? 포션??",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100037706.1.22",
          "form": "게임에서 포션응 마시면 힘 올라가는 음료같은게 있거든요!!",
          "original_form": "게임에서 포션응 마시면 힘 올라가는 음료같은게 있거든요!!",
          "speaker_id": "2",
          "time": "20210901 17:55"
        }
      ]
