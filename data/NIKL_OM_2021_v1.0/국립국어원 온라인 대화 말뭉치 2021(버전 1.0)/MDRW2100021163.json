      "utterance": [
        {
          "id": "MDRW2100021163.1.1",
          "form": "아 ㄱㅊㄱㅊ",
          "original_form": "아 ㄱㅊㄱㅊ",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021163.1.2",
          "form": "그냥 더 조사하라는 뜻이었음",
          "original_form": "그냥 더 조사하라는 뜻이었음",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021163.1.3",
          "form": "뭔내용이었음?",
          "original_form": "뭔내용이었음?",
          "speaker_id": "1",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021163.1.4",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021163.1.5",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021163.1.6",
          "form": "그로신에도 그런 거 엄청 많잖아",
          "original_form": "그로신에도 그런 거 엄청 많잖아",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021163.1.7",
          "form": "맞음",
          "original_form": "맞음",
          "speaker_id": "1",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021163.1.8",
          "form": "그리스 로마 신화에",
          "original_form": "그리스 로마 신화에",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021163.1.9",
          "form": "ㅇㅇㅇ엉",
          "original_form": "ㅇㅇㅇ엉",
          "speaker_id": "1",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021163.1.10",
          "form": "막 이곳저곳 아주그냥",
          "original_form": "막 이곳저곳 아주그냥",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021163.1.11",
          "form": "아휴",
          "original_form": "아휴",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021163.1.12",
          "form": "그것도잘못해석한거있지않았음?",
          "original_form": "그것도잘못해석한거있지않았음?",
          "speaker_id": "1",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021163.1.13",
          "form": "봤던거같은데",
          "original_form": "봤던거같은데",
          "speaker_id": "1",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021163.1.14",
          "form": "아닌가?",
          "original_form": "아닌가?",
          "speaker_id": "1",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021163.1.15",
          "form": "어쨋든그런기록들도",
          "original_form": "어쨋든그런기록들도",
          "speaker_id": "1",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100021163.1.16",
          "form": "다",
          "original_form": "다",
          "speaker_id": "1",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100021163.1.17",
          "form": "남자입장에서정리하고",
          "original_form": "남자입장에서정리하고",
          "speaker_id": "1",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100021163.1.18",
          "form": "남자가해석한거라",
          "original_form": "남자가해석한거라",
          "speaker_id": "1",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100021163.1.19",
          "form": "걸러서봐야하는듯",
          "original_form": "걸러서봐야하는듯",
          "speaker_id": "1",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100021163.1.20",
          "form": "아 거의",
          "original_form": "아 거의",
          "speaker_id": "2",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100021163.1.21",
          "form": "이름을 잘못썼대",
          "original_form": "이름을 잘못썼대",
          "speaker_id": "2",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100021163.1.22",
          "form": "아니 그래도 그런 원형이 있어서 키르케 같은 작품도 나오고",
          "original_form": "아니 그래도 그런 원형이 있어서 키르케 같은 작품도 나오고",
          "speaker_id": "2",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100021163.1.23",
          "form": "눈물난다",
          "original_form": "눈물난다",
          "speaker_id": "2",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100021163.1.24",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100021163.1.25",
          "form": "나도 한챕터 밖에 못봤지만",
          "original_form": "나도 한챕터 밖에 못봤지만",
          "speaker_id": "2",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100021163.1.26",
          "form": "그런해석은개인의견이",
          "original_form": "그런해석은개인의견이",
          "speaker_id": "1",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100021163.1.27",
          "form": "들어갈수밖에없엇",
          "original_form": "들어갈수밖에없엇",
          "speaker_id": "1",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100021163.1.28",
          "form": "키르케는뭐임?",
          "original_form": "키르케는뭐임?",
          "speaker_id": "1",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100021163.1.29",
          "form": "책?",
          "original_form": "책?",
          "speaker_id": "1",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100021163.1.30",
          "form": "신화를 기반으로 나온 책인데",
          "original_form": "신화를 기반으로 나온 책인데",
          "speaker_id": "2",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100021163.1.31",
          "form": "엉ㅇㅇㅇ",
          "original_form": "엉ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100021163.1.32",
          "form": "신의 자식이지만 능력없다고 천대받던 여성 신이",
          "original_form": "신의 자식이지만 능력없다고 천대받던 여성 신이",
          "speaker_id": "2",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100021163.1.33",
          "form": "배신 당하고 쫓겨나게 되는데",
          "original_form": "배신 당하고 쫓겨나게 되는데",
          "speaker_id": "2",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100021163.1.34",
          "form": "옹ㅇㅇㅇ",
          "original_form": "옹ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100021163.1.35",
          "form": "그곳에서 자신의 능력을 깨닫고 강해지는 그런 이야기임",
          "original_form": "그곳에서 자신의 능력을 깨닫고 강해지는 그런 이야기임",
          "speaker_id": "2",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100021163.1.36",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100021163.1.37",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100021163.1.38",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "1",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100021163.1.39",
          "form": "자신이 뭘 잘하는지 깨닫고 그런가림",
          "original_form": "자신이 뭘 잘하는지 깨닫고 그런가림",
          "speaker_id": "2",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100021163.1.40",
          "form": "읽어봤어,?",
          "original_form": "읽어봤어,?",
          "speaker_id": "1",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100021163.1.41",
          "form": "벌만해?",
          "original_form": "벌만해?",
          "speaker_id": "1",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100021163.1.42",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100021163.1.43",
          "form": "문체가 술술 읽히는 문체임",
          "original_form": "문체가 술술 읽히는 문체임",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100021163.1.44",
          "form": "나온지도 오래되지 않았음",
          "original_form": "나온지도 오래되지 않았음",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100021163.1.45",
          "form": "근데 나도 아직 한 챕터 밖에 안읽음",
          "original_form": "근데 나도 아직 한 챕터 밖에 안읽음",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100021163.1.46",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100021163.1.47",
          "form": "한번읽어봐야할듯",
          "original_form": "한번읽어봐야할듯",
          "speaker_id": "1",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100021163.1.48",
          "form": "우선여자가주인공이면",
          "original_form": "우선여자가주인공이면",
          "speaker_id": "1",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100021163.1.49",
          "form": "기본적으로재밌음",
          "original_form": "기본적으로재밌음",
          "speaker_id": "1",
          "time": "20210828 16:36"
        }
      ]
    }
