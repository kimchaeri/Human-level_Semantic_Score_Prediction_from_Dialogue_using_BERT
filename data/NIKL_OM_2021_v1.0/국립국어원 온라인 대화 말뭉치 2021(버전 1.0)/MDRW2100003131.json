      "utterance": [
        {
          "id": "MDRW2100003131.1.1",
          "form": "요즘 날이 꽤 선선해져서 좋아요",
          "original_form": "요즘 날이 꽤 선선해져서 좋아요",
          "speaker_id": "1",
          "time": "20210820 19:08"
        },
        {
          "id": "MDRW2100003131.1.2",
          "form": "더위는 그나마 좀 사라진것같은데",
          "original_form": "더위는 그나마 좀 사라진것같은데",
          "speaker_id": "2",
          "time": "20210820 20:24"
        },
        {
          "id": "MDRW2100003131.1.3",
          "form": "곧 장마라네요ㅠㅠ",
          "original_form": "곧 장마라네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 20:24"
        },
        {
          "id": "MDRW2100003131.1.4",
          "form": "헐 이제요?",
          "original_form": "헐 이제요?",
          "speaker_id": "1",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003131.1.5",
          "form": "이번장마는 되게 늦네요",
          "original_form": "이번장마는 되게 늦네요",
          "speaker_id": "1",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003131.1.6",
          "form": "이제 가을인 것 같은데..",
          "original_form": "이제 가을인 것 같은데..",
          "speaker_id": "1",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003131.1.7",
          "form": "여기 지금 비 엄청 마이 오고있어요ㅜ",
          "original_form": "여기 지금 비 엄청 마이 오고있어요ㅜ",
          "speaker_id": "2",
          "time": "20210821 10:06"
        },
        {
          "id": "MDRW2100003131.1.8",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 10:06"
        },
        {
          "id": "MDRW2100003131.1.9",
          "form": "여기도요ㅠㅠ",
          "original_form": "여기도요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 10:50"
        },
        {
          "id": "MDRW2100003131.1.10",
          "form": "빗소리 듣고 깜짝 놀라서 얼른 창문 닫아버렸어요",
          "original_form": "빗소리 듣고 깜짝 놀라서 얼른 창문 닫아버렸어요",
          "speaker_id": "1",
          "time": "20210821 10:50"
        },
        {
          "id": "MDRW2100003131.1.11",
          "form": "지금 장마니깐 계속 비가 올꺼에요ㅠㅠ",
          "original_form": "지금 장마니깐 계속 비가 올꺼에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003131.1.12",
          "form": "작년에는태풍때문에 엄청 고생했었는데",
          "original_form": "작년에는태풍때문에 엄청 고생했었는데",
          "speaker_id": "2",
          "time": "20210821 11:53"
        },
        {
          "id": "MDRW2100003131.1.13",
          "form": "맞아요 진짜 정신 안 차리면 넘어질 것 같았어요",
          "original_form": "맞아요 진짜 정신 안 차리면 넘어질 것 같았어요",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100003131.1.14",
          "form": "작년엔 장마가 길었어서 별로 더운지 모르고 넘어갔는데",
          "original_form": "작년엔 장마가 길었어서 별로 더운지 모르고 넘어갔는데",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100003131.1.15",
          "form": "올해는 진짜 더웠어요",
          "original_form": "올해는 진짜 더웠어요",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100003131.1.16",
          "form": "맞아요ㅜㅜ빨리 더위 지나갔으면 좋겠어요",
          "original_form": "맞아요ㅜㅜ빨리 더위 지나갔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100003131.1.17",
          "form": "이제 9월달 다가오니깐",
          "original_form": "이제 9월달 다가오니깐",
          "speaker_id": "2",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100003131.1.18",
          "form": "장마끝나면",
          "original_form": "장마끝나면",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100003131.1.19",
          "form": "시원해질꺼에요",
          "original_form": "시원해질꺼에요",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100003131.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100003131.1.21",
          "form": "이제 외출할 때마다 겉옷 챙겨가야해요",
          "original_form": "이제 외출할 때마다 겉옷 챙겨가야해요",
          "speaker_id": "1",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100003131.1.22",
          "form": "어제는 비엄청 많이 왔었는데 오늘은 비가 안와서 좋았어요",
          "original_form": "어제는 비엄청 많이 왔었는데 오늘은 비가 안와서 좋았어요",
          "speaker_id": "2",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100003131.1.23",
          "form": "맞아요 이렇게 또 장마가 끝나는 건 아닐까 생각이 드네요",
          "original_form": "맞아요 이렇게 또 장마가 끝나는 건 아닐까 생각이 드네요",
          "speaker_id": "1",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100003131.1.24",
          "form": "오늘은 비가 엄청와서 바지랑 발 다 젖었어요ㅜㅜ",
          "original_form": "오늘은 비가 엄청와서 바지랑 발 다 젖었어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100003131.1.25",
          "form": "하늘에 구멍 뚫렸나봐요ㅠㅠ",
          "original_form": "하늘에 구멍 뚫렸나봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100003131.1.26",
          "form": "비가 막 쏟아져요",
          "original_form": "비가 막 쏟아져요",
          "speaker_id": "2",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100003131.1.27",
          "form": "헉ㅠㅠㅠㅠㅠ맞아요 오늘은 비가 그치지도 않고 계속 오네요ㅠㅜ",
          "original_form": "헉ㅠㅠㅠㅠㅠ맞아요 오늘은 비가 그치지도 않고 계속 오네요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210823 18:10"
        }
      ]
