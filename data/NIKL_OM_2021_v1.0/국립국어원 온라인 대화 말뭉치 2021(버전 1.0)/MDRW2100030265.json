      "utterance": [
        {
          "id": "MDRW2100030265.1.1",
          "form": "난 전민희작가님이 좋아 ㅎㅎ",
          "original_form": "난 전민희작가님이 좋아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030265.1.2",
          "form": "룬의아이들 태양의탑 세월의돌같은거 쓰셧어",
          "original_form": "룬의아이들 태양의탑 세월의돌같은거 쓰셧어",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030265.1.3",
          "form": "알어?",
          "original_form": "알어?",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030265.1.4",
          "form": "나두!! 난 정연작가님도 좋아 ㅎㅎ",
          "original_form": "나두!! 난 정연작가님도 좋아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030265.1.5",
          "form": "알지!!",
          "original_form": "알지!!",
          "speaker_id": "1",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030265.1.6",
          "form": "룬의아이들는 전말",
          "original_form": "룬의아이들는 전말",
          "speaker_id": "1",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030265.1.7",
          "form": "정말 명작이야",
          "original_form": "정말 명작이야",
          "speaker_id": "1",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030265.1.8",
          "form": "맞어 ㅋㅋ",
          "original_form": "맞어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030265.1.9",
          "form": "그책 완전 좋아",
          "original_form": "그책 완전 좋아",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030265.1.10",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030265.1.11",
          "form": "그리고 스테디셀러 아닌가 룬의아이들정도면?",
          "original_form": "그리고 스테디셀러 아닌가 룬의아이들정도면?",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030265.1.12",
          "form": "룬의아이들",
          "original_form": "룬의아이들",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030265.1.13",
          "form": "아하 ㅋㅋ",
          "original_form": "아하 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030265.1.14",
          "form": "사람들도 많이 봣지 ㅎㅎ",
          "original_form": "사람들도 많이 봣지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030265.1.15",
          "form": "맞아 완전 재밌구 좋아",
          "original_form": "맞아 완전 재밌구 좋아",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030265.1.16",
          "form": "그만큼 재밋잖어",
          "original_form": "그만큼 재밋잖어",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030265.1.17",
          "form": "요즘도 연재하시구",
          "original_form": "요즘도 연재하시구",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030265.1.18",
          "form": "출간된지 10년이 넘었는데도",
          "original_form": "출간된지 10년이 넘었는데도",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030265.1.19",
          "form": "인기가 많구",
          "original_form": "인기가 많구",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030265.1.20",
          "form": "맞아!",
          "original_form": "맞아!",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030265.1.21",
          "form": "너무 재밌어",
