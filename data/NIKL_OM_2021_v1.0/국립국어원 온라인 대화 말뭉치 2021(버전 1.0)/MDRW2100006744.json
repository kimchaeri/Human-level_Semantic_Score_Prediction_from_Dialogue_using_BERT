      "utterance": [
        {
          "id": "MDRW2100006744.1.1",
          "form": "아침 드셨나요",
          "original_form": "아침 드셨나요",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100006744.1.2",
          "form": "네 호박죽 먹었어요",
          "original_form": "네 호박죽 먹었어요",
          "speaker_id": "2",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100006744.1.3",
          "form": "아침식사 하셨어요",
          "original_form": "아침식사 하셨어요",
          "speaker_id": "2",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100006744.1.4",
          "form": "네 저는 밥으로요 ㅎㅎ",
          "original_form": "네 저는 밥으로요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100006744.1.5",
          "form": "아침은 꼭 먹어야 뇌에 좋다고 하죠",
          "original_form": "아침은 꼭 먹어야 뇌에 좋다고 하죠",
          "speaker_id": "2",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100006744.1.6",
          "form": "네 평소에도 죽 드시나요?",
          "original_form": "네 평소에도 죽 드시나요?",
          "speaker_id": "1",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100006744.1.7",
          "form": "아주 가끔 먹어요",
          "original_form": "아주 가끔 먹어요",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100006744.1.8",
          "form": "속이 편한것 같아요",
          "original_form": "속이 편한것 같아요",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100006744.1.9",
          "form": "죽은 끓여드시나여",
          "original_form": "죽은 끓여드시나여",
          "speaker_id": "1",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100006744.1.10",
          "form": "만들어 먹을때도 있고 기성품 먹을때도 있죠",
          "original_form": "만들어 먹을때도 있고 기성품 먹을때도 있죠",
          "speaker_id": "2",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100006744.1.11",
          "form": "호박죽은 아는 동생이 만들어 준거예요",
          "original_form": "호박죽은 아는 동생이 만들어 준거예요",
          "speaker_id": "2",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100006744.1.12",
          "form": "좋은 동생분을 두셨군요!!",
          "original_form": "좋은 동생분을 두셨군요!!",
          "speaker_id": "1",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100006744.1.13",
          "form": "아이들은 아침밥 먹나요",
          "original_form": "아이들은 아침밥 먹나요",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100006744.1.14",
          "form": "네 같이 챙겨먹입니다 ㅎㅎ",
          "original_form": "네 같이 챙겨먹입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100006744.1.15",
          "form": "아이들 밥 먹이기 힘들진 않나요",
          "original_form": "아이들 밥 먹이기 힘들진 않나요",
