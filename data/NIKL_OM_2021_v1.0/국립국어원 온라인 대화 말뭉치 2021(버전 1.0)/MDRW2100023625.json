      "utterance": [
        {
          "id": "MDRW2100023625.1.1",
          "form": "종교있나여!ㅋㅋㅋㅋ",
          "original_form": "종교있나여!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100023625.1.2",
          "form": "저는 무교에욬ㅋㅋㅋㅋ",
          "original_form": "저는 무교에욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100023625.1.3",
          "form": "아잌ㅋㅋㅋ",
          "original_form": "아잌ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023625.1.4",
          "form": "저두 무교거든요!",
          "original_form": "저두 무교거든요!",
          "speaker_id": "2",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023625.1.5",
          "form": "ㅋㅋㅋㅋㅋ 여기 여쭤보면 대부분 다 무교인거같아여 ㅌㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 여기 여쭤보면 대부분 다 무교인거같아여 ㅌㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023625.1.6",
          "form": "요새 종교인이 줄어들고잇은느낌 ㅜ",
          "original_form": "요새 종교인이 줄어들고잇은느낌 ㅜ",
          "speaker_id": "1",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023625.1.7",
          "form": "오옹ㅇ",
          "original_form": "오옹ㅇ",
          "speaker_id": "2",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023625.1.8",
          "form": "전 다른사람을 만나질않아스",
          "original_form": "전 다른사람을 만나질않아스",
          "speaker_id": "2",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023625.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023625.1.10",
          "form": "앜ㅋㅋㅋㅌㅋㅋ 암튼그렇더라구요~",
          "original_form": "앜ㅋㅋㅋㅌㅋㅋ 암튼그렇더라구요~",
          "speaker_id": "1",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100023625.1.11",
          "form": "그래도 어릴때 교회한번씩들 다 가보지않앗어여?",
          "original_form": "그래도 어릴때 교회한번씩들 다 가보지않앗어여?",
          "speaker_id": "1",
          "time": "20210830 13:36"
        },
        {
          "id": "MDRW2100023625.1.12",
          "form": "그거야그렇져!",
          "original_form": "그거야그렇져!",
          "speaker_id": "2",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100023625.1.13",
          "form": "달란트!",
          "original_form": "달란트!",
          "speaker_id": "2",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100023625.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ 마쟈옄ㅋㅋㅋㅋ 막 과자나 치킨피자 이런것도 시켜주셔서 재밋게간기억이나요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 마쟈옄ㅋㅋㅋㅋ 막 과자나 치킨피자 이런것도 시켜주셔서 재밋게간기억이나요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100023625.1.15",
          "form": "그죠그죠!ㅋㅋㄴㄴㄴㅋㅋㅋ",
          "original_form": "그죠그죠!ㅋㅋㄴㄴㄴㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:40"
        },
        {
          "id": "MDRW2100023625.1.16",
          "form": "엥 보낼수없는단어라며 안보내졌네요 ㅜㅋㅋ",
          "original_form": "엥 보낼수없는단어라며 안보내졌네요 ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:36"
        },
        {
          "id": "MDRW2100023625.1.17",
          "form": "아근데 종교는 딱히 할얘기가없긴해여 ㅋㅋ쿠ㅜ",
          "original_form": "아근데 종교는 딱히 할얘기가없긴해여 ㅋㅋ쿠ㅜ",
          "speaker_id": "1",
          "time": "20210831 14:36"
        },
        {
          "id": "MDRW2100023625.1.18",
          "form": "오잉",
          "original_form": "오잉",
          "speaker_id": "2",
          "time": "20210831 14:36"
        },
        {
          "id": "MDRW2100023625.1.19",
          "form": "둘다 무교다보니까",
          "original_form": "둘다 무교다보니까",
          "speaker_id": "2",
          "time": "20210831 14:36"
        },
        {
          "id": "MDRW2100023625.1.20",
          "form": "혹시 남자친구가 종교를 강요한다면",
          "original_form": "혹시 남자친구가 종교를 강요한다면",
          "speaker_id": "2",
          "time": "20210831 14:36"
        },
        {
          "id": "MDRW2100023625.1.21",
          "form": "어떻게할생각인가요!",
          "original_form": "어떻게할생각인가요!",
          "speaker_id": "2",
          "time": "20210831 14:36"
        },
        {
          "id": "MDRW2100023625.1.22",
          "form": "음.. 종교든뭐든 강요한다면 너무싫을것같아요 ㅋㅋㅋㅠ",
          "original_form": "음.. 종교든뭐든 강요한다면 너무싫을것같아요 ㅋㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100023625.1.23",
          "form": "그죠그죠",
          "original_form": "그죠그죠",
          "speaker_id": "2",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100023625.1.24",
          "form": "저두 그렇게생각해요!",
          "original_form": "저두 그렇게생각해요!",
