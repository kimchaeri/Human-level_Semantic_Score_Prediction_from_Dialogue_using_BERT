      "utterance": [
        {
          "id": "MDRW2100013944.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013944.1.2",
          "form": "온라인 커뮤니티 활동 하시나요?",
          "original_form": "온라인 커뮤니티 활동 하시나요?",
          "speaker_id": "1",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013944.1.3",
          "form": "네 저 더쿠해요",
          "original_form": "네 저 더쿠해요",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013944.1.4",
          "form": "헐 저도 해요 ㅎㅎ",
          "original_form": "헐 저도 해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013944.1.5",
          "form": "ㅎㅎ 반갑네요!",
          "original_form": "ㅎㅎ 반갑네요!",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013944.1.6",
          "form": "주로 어느게시판에서 활동하세요?",
          "original_form": "주로 어느게시판에서 활동하세요?",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013944.1.7",
          "form": "저 거의 케이돌토크랑 뷰티방에서 있는거같아요 ㅎㅎ",
          "original_form": "저 거의 케이돌토크랑 뷰티방에서 있는거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013944.1.8",
          "form": "비슷한가용 ?? 주로 어디계세요?",
          "original_form": "비슷한가용 ?? 주로 어디계세요?",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013944.1.9",
          "form": "저도 케이돌토크 뷰티방에 주로 있어요 가끔 연뮤나 주식방도 들어가고요",
          "original_form": "저도 케이돌토크 뷰티방에 주로 있어요 가끔 연뮤나 주식방도 들어가고요",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013944.1.10",
          "form": "핫게구경도 하고요!",
          "original_form": "핫게구경도 하고요!",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013944.1.11",
          "form": "저랑 비슷하시네요 !!",
          "original_form": "저랑 비슷하시네요 !!",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013944.1.12",
          "form": "저는 보통 케이돌토크에서 싸움나면",
          "original_form": "저는 보통 케이돌토크에서 싸움나면",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013944.1.13",
          "form": "다른 방으로 옮겨요 ㅎㅎ",
          "original_form": "다른 방으로 옮겨요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013944.1.14",
          "form": "저도요ㅋㅋㅋ뷰티방 그렇게 유입됐어요",
          "original_form": "저도요ㅋㅋㅋ뷰티방 그렇게 유입됐어요",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013944.1.15",
          "form": "뭐사고있다길랰ㅋㅋㅋㅋ",
          "original_form": "뭐사고있다길랰ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013944.1.16",
          "form": "맞아욬ㅋㅋ 들어가면 항상 뭐 사고있어요",
          "original_form": "맞아욬ㅋㅋ 들어가면 항상 뭐 사고있어요",
          "speaker_id": "1",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013944.1.17",
          "form": "저도 그래서 돈 엄청 쓴거같아요 ㅎㅎ",
          "original_form": "저도 그래서 돈 엄청 쓴거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013944.1.18",
          "form": "안사면 100프로 할인인데 맨날 싸다고 뭐사고 있고ㅋㅋㅋ",
          "original_form": "안사면 100프로 할인인데 맨날 싸다고 뭐사고 있고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013944.1.19",
          "form": "뷰티방하기 전엔 택배오면 무슨택배인지 짐작이 됐었는데",
          "original_form": "뷰티방하기 전엔 택배오면 무슨택배인지 짐작이 됐었는데",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013944.1.20",
          "form": "지금은 내가 뭐시켰더라?",
          "original_form": "지금은 내가 뭐시켰더라?",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013944.1.21",
          "form": "아 저두욬ㅋㅋㅋㅋ 맨날 뜯어보고 나서 알아요 ...",
          "original_form": "아 저두욬ㅋㅋㅋㅋ 맨날 뜯어보고 나서 알아요 ...",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013944.1.22",
          "form": "그리고 약간 물건 살때도 뷰티방에 항상 검색해보고 사요",
          "original_form": "그리고 약간 물건 살때도 뷰티방에 항상 검색해보고 사요",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013944.1.23",
          "form": "혹시나 싼거 있으려나 ㅋㅋㅋ",
          "original_form": "혹시나 싼거 있으려나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013944.1.24",
          "form": "정보 얻기 좋은것 같아요ㅋㅋ",
          "original_form": "정보 얻기 좋은것 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013944.1.25",
          "form": "케톡도 보고있으면 온갖아이돌 누가 컴백하고 누가 뭐하는지 다 알수 있고",
          "original_form": "케톡도 보고있으면 온갖아이돌 누가 컴백하고 누가 뭐하는지 다 알수 있고",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013944.1.26",
          "form": "아 맞아요 ㅎㅎ 스퀘에에서 항상 젤 빠르게 보는거 같아요 !",
          "original_form": "아 맞아요 ㅎㅎ 스퀘에에서 항상 젤 빠르게 보는거 같아요 !",
          "speaker_id": "1",
          "time": "20210825 16:54"
        }
      ]
    }
