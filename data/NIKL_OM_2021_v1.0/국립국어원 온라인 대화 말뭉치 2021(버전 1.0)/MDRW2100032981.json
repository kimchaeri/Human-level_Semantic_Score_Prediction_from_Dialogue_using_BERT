      "utterance": [
        {
          "id": "MDRW2100032981.1.1",
          "form": "오늘도 쇼핑하나요",
          "original_form": "오늘도 쇼핑하나요",
          "speaker_id": "1",
          "time": "20210906 11:49"
        },
        {
          "id": "MDRW2100032981.1.2",
          "form": "점심을 사려해요 맘스터치요",
          "original_form": "점심을 사려해요 맘스터치요",
          "speaker_id": "2",
          "time": "20210906 11:50"
        },
        {
          "id": "MDRW2100032981.1.3",
          "form": "도시락은 어때요",
          "original_form": "도시락은 어때요",
          "speaker_id": "1",
          "time": "20210906 11:50"
        },
        {
          "id": "MDRW2100032981.1.4",
          "form": "편의점 말인가요? 한솥은 잘 안보이네요",
          "original_form": "편의점 말인가요? 한솥은 잘 안보이네요",
          "speaker_id": "2",
          "time": "20210906 11:50"
        },
        {
          "id": "MDRW2100032981.1.5",
          "form": "햄버거 사시나요",
          "original_form": "햄버거 사시나요",
          "speaker_id": "1",
          "time": "20210906 11:50"
        },
        {
          "id": "MDRW2100032981.1.6",
          "form": "가족이랑 저녁을 치킨버거를 먹기로 했거든요",
          "original_form": "가족이랑 저녁을 치킨버거를 먹기로 했거든요",
          "speaker_id": "2",
          "time": "20210906 11:50"
        },
        {
          "id": "MDRW2100032981.1.7",
          "form": "좋네요 오붓하게",
          "original_form": "좋네요 오붓하게",
          "speaker_id": "1",
          "time": "20210906 11:50"
        },
        {
          "id": "MDRW2100032981.1.8",
          "form": "가족을 위해서 쇼핑하는 것은 보람있는 일이에요",
          "original_form": "가족을 위해서 쇼핑하는 것은 보람있는 일이에요",
          "speaker_id": "2",
          "time": "20210906 11:51"
        },
        {
          "id": "MDRW2100032981.1.9",
          "form": "혼자선 대충 사고말ㅈ：7",
          "original_form": "혼자선 대충 사고말ㅈ：7",
          "speaker_id": "1",
          "time": "20210906 11:51"
        },
        {
          "id": "MDRW2100032981.1.10",
          "form": "남을 위해 쇼핑하면 뿌듯하고 우울함이 사라져요",
          "original_form": "남을 위해 쇼핑하면 뿌듯하고 우울함이 사라져요",
          "speaker_id": "2",
          "time": "20210906 11:51"
        },
        {
          "id": "MDRW2100032981.1.11",
          "form": "도시락도 사시나요",
          "original_form": "도시락도 사시나요",
          "speaker_id": "1",
          "time": "20210906 11:51"
        },
        {
          "id": "MDRW2100032981.1.12",
          "form": "출출할땐 편의점 도시락이 최고에요",
          "original_form": "출출할땐 편의점 도시락이 최고에요",
          "speaker_id": "2",
          "time": "20210906 11:51"
        },
        {
          "id": "MDRW2100032981.1.13",
          "form": "저도 쇼핑할거어요",
          "original_form": "저도 쇼핑할거어요",
          "speaker_id": "1",
          "time": "20210906 11:51"
        },
        {
          "id": "MDRW2100032981.1.14",
          "form": "쇼핑은 저의 낙이에요",
          "original_form": "쇼핑은 저의 낙이에요",
          "speaker_id": "2",
          "time": "20210906 11:52"
        },
        {
          "id": "MDRW2100032981.1.15",
          "form": "가을에 어떤 옷 살거에요?",
          "original_form": "가을에 어떤 옷 살거에요?",
