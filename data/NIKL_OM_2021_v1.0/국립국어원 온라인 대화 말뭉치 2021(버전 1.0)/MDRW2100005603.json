      "utterance": [
        {
          "id": "MDRW2100005603.1.1",
          "form": "이번엔 여행 얘기 해볼까요 ㅎㅎ?",
          "original_form": "이번엔 여행 얘기 해볼까요 ㅎㅎ?",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005603.1.2",
          "form": "좋아요~",
          "original_form": "좋아요~",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005603.1.3",
          "form": "여행 다니는 거 좋아하세요?",
          "original_form": "여행 다니는 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005603.1.4",
          "form": "좋아해요!",
          "original_form": "좋아해요!",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005603.1.5",
          "form": "저는 새로운 풍경 보는것도 좋고",
          "original_form": "저는 새로운 풍경 보는것도 좋고",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005603.1.6",
          "form": "맛있는 거 먹는 것더 좋은것같아요!",
          "original_form": "맛있는 거 먹는 것더 좋은것같아요!",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005603.1.7",
          "form": "식도락 여행 가는 거 소원이에요",
          "original_form": "식도락 여행 가는 거 소원이에요",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005603.1.8",
          "form": "와!",
          "original_form": "와!",
          "speaker_id": "1",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005603.1.9",
          "form": "수험 생활 끝났는데 코로나 때문에 아직 제대로 된 여행을 못 가봤어요ㅠㅠ",
          "original_form": "수험 생활 끝났는데 코로나 때문에 아직 제대로 된 여행을 못 가봤어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005603.1.10",
          "form": "저는 코로나 끝나고 태국가서",
          "original_form": "저는 코로나 끝나고 태국가서",
          "speaker_id": "1",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005603.1.11",
          "form": "망고를 잔뜩먹고싶어요",
          "original_form": "망고를 잔뜩먹고싶어요",
          "speaker_id": "1",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005603.1.12",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005603.1.13",
          "form": "와 진짜 좋아요",
          "original_form": "와 진짜 좋아요",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005603.1.14",
          "form": "저도 동남아 한 번 가보고 싶어요",
          "original_form": "저도 동남아 한 번 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005603.1.15",
          "form": "과일 산더미처럼 쌓아서 먹어 보고 싶어요",
          "original_form": "과일 산더미처럼 쌓아서 먹어 보고 싶어요",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005603.1.16",
          "form": "진짜요 ㅋㅋ",
          "original_form": "진짜요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005603.1.17",
          "form": "동남아에서 먹방여행 하고싶어요",
          "original_form": "동남아에서 먹방여행 하고싶어요",
          "speaker_id": "1",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005603.1.18",
          "form": "저도 관광도 재밌지만 지금은 놀고 먹고 쉬고 싶어요. 맛있는 거 잔뜩먹고 마사지도 받고 물놀이도 하고...",
          "original_form": "저도 관광도 재밌지만 지금은 놀고 먹고 쉬고 싶어요. 맛있는 거 잔뜩먹고 마사지도 받고 물놀이도 하고...",
          "speaker_id": "2",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005603.1.19",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005603.1.20",
          "form": "정말 관광도 좋지만",
          "original_form": "정말 관광도 좋지만",
          "speaker_id": "1",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005603.1.21",
          "form": "그냥 떠나버리고싶은 ㅎㅎ",
          "original_form": "그냥 떠나버리고싶은 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005603.1.22",
          "form": "코로나 때문에 더 그런 거 같아요",
          "original_form": "코로나 때문에 더 그런 거 같아요",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005603.1.23",
          "form": "놀고 싶은 욕구를 계속 참고 있어서ㅋㅋㅋ",
          "original_form": "놀고 싶은 욕구를 계속 참고 있어서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005603.1.24",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005603.1.25",
          "form": "그럴수도있어요",
          "original_form": "그럴수도있어요",
          "speaker_id": "1",
          "time": "20210821 23:30"
        }
      ]
    }
  ]
