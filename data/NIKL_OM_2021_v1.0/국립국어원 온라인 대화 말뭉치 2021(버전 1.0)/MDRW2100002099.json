      "utterance": [
        {
          "id": "MDRW2100002099.1.1",
          "form": "안녕하세요! 요즘 재미있는 영화있나요?",
          "original_form": "안녕하세요! 요즘 재미있는 영화있나요?",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002099.1.2",
          "form": "싱크홀 보셨나요? 차승원나오는",
          "original_form": "싱크홀 보셨나요? 차승원나오는",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002099.1.3",
          "form": "아 그건 아직 못봤어요!",
          "original_form": "아 그건 아직 못봤어요!",
          "speaker_id": "1",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002099.1.4",
          "form": "최근에 티켓생겨서 보고왔는데 가볍게보기좋네요",
          "original_form": "최근에 티켓생겨서 보고왔는데 가볍게보기좋네요",
          "speaker_id": "2",
          "time": "20210819 23:49"
        },
        {
          "id": "MDRW2100002099.1.5",
          "form": "전 최근에본거라고는 마블영화에요",
          "original_form": "전 최근에본거라고는 마블영화에요",
          "speaker_id": "1",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002099.1.6",
          "form": "코시국이라 극장가기가 힘들죠ㅜ",
          "original_form": "코시국이라 극장가기가 힘들죠ㅜ",
          "speaker_id": "2",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002099.1.7",
          "form": "그처 언제쯤 풀리련지ㅠㅡ",
          "original_form": "그처 언제쯤 풀리련지ㅠㅡ",
          "speaker_id": "1",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002099.1.8",
          "form": "저도 진짜몇년만에 갔는데 사람 많아서 놀랐어요ㅠ 불편하실수있는데 죄송합니다ㅠ",
          "original_form": "저도 진짜몇년만에 갔는데 사람 많아서 놀랐어요ㅠ 불편하실수있는데 죄송합니다ㅠ",
          "speaker_id": "2",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002099.1.9",
          "form": "아니에요! 저도 시간맞으면 갓을꺼에요",
          "original_form": "아니에요! 저도 시간맞으면 갓을꺼에요",
          "speaker_id": "1",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100002099.1.10",
          "form": "웃긴게 팝콘은 파는데 상영관내에서는 금지에요",
          "original_form": "웃긴게 팝콘은 파는데 상영관내에서는 금지에요",
          "speaker_id": "2",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002099.1.11",
          "form": "바깥에서 먹고 들어간대요",
          "original_form": "바깥에서 먹고 들어간대요",
          "speaker_id": "2",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002099.1.12",
          "form": "맞아여ㅋㅋ안파는것도 아니고",
          "original_form": "맞아여ㅋㅋ안파는것도 아니고",
          "speaker_id": "1",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002099.1.13",
          "form": "근데 팝콘은 금지하고 음료는 된대요ㅋㅋ 이상한기준",
          "original_form": "근데 팝콘은 금지하고 음료는 된대요ㅋㅋ 이상한기준",
          "speaker_id": "2",
          "time": "20210820 00:12"
        },
        {
          "id": "MDRW2100002099.1.14",
          "form": "ㅋㅋ그니까요 아예못하게하던지",
          "original_form": "ㅋㅋ그니까요 아예못하게하던지",
          "speaker_id": "1",
          "time": "20210820 00:16"
        },
        {
          "id": "MDRW2100002099.1.15",
          "form": "네ㅋㅋ 근데 공연 업계도 힘들겠어요 다취소되고그러니..",
          "original_form": "네ㅋㅋ 근데 공연 업계도 힘들겠어요 다취소되고그러니..",
          "speaker_id": "2",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002099.1.16",
          "form": "그쳐 다들 힘든시기라",
          "original_form": "그쳐 다들 힘든시기라",
          "speaker_id": "1",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002099.1.17",
          "form": "끝이 안보여서 막막하네요ㅠ 콘서트가고싶어요",
          "original_form": "끝이 안보여서 막막하네요ㅠ 콘서트가고싶어요",
          "speaker_id": "2",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002099.1.18",
          "form": "저는 뮤지컬요",
          "original_form": "저는 뮤지컬요",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002099.1.19",
          "form": "오~뮤지컬도 넘그립네요",
          "original_form": "오~뮤지컬도 넘그립네요",
          "speaker_id": "2",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002099.1.20",
          "form": "가장최근에 뭐보셨어요?",
          "original_form": "가장최근에 뭐보셨어요?",
          "speaker_id": "2",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002099.1.21",
          "form": "전 오래됫어요ㅜㅜ 위키드요",
          "original_form": "전 오래됫어요ㅜㅜ 위키드요",
          "speaker_id": "1",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002099.1.22",
          "form": "위키드 유명한데 못봤어요",
          "original_form": "위키드 유명한데 못봤어요",
          "speaker_id": "2",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002099.1.23",
          "form": "저도 맨날 생각만하다 봣는데 재미잇엇어요",
          "original_form": "저도 맨날 생각만하다 봣는데 재미잇엇어요",
          "speaker_id": "1",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002099.1.24",
          "form": "그레이트코멧 봤는데 관객석이 무대안에 들어가있어서 신기하더라구요",
          "original_form": "그레이트코멧 봤는데 관객석이 무대안에 들어가있어서 신기하더라구요",
          "speaker_id": "2",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002099.1.25",
          "form": "오 그것도 특색잇네요?",
          "original_form": "오 그것도 특색잇네요?",
          "speaker_id": "1",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002099.1.26",
          "form": "네 눈앞에서 배우가 뛰어다니니 재밌는데 좌석따라 시야차이가 커서 티켓팅 잘해야해요ㅎ",
          "original_form": "네 눈앞에서 배우가 뛰어다니니 재밌는데 좌석따라 시야차이가 커서 티켓팅 잘해야해요ㅎ",
          "speaker_id": "2",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002099.1.27",
          "form": "비싼자리ㅋ",
          "original_form": "비싼자리ㅋ",
          "speaker_id": "1",
          "time": "20210820 00:31"
        }
      ]
    }
