      "utterance": [
        {
          "id": "MDRW2100044507.1.1",
          "form": "가을 옷 많이 안 입게 되지 않아",
          "original_form": "가을 옷 많이 안 입게 되지 않아",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044507.1.2",
          "form": "가을이 짧아서 아무래도",
          "original_form": "가을이 짧아서 아무래도",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044507.1.3",
          "form": "사고 싶은 옷 있어",
          "original_form": "사고 싶은 옷 있어",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044507.1.4",
          "form": "딱히 없는데",
          "original_form": "딱히 없는데",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044507.1.5",
          "form": "가을 원피스 하나 있으면 좋을 거 같애요",
          "original_form": "가을 원피스 하나 있으면 좋을 거 같애요",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044507.1.6",
          "form": "원피스 하나는 괜찮지",
          "original_form": "원피스 하나는 괜찮지",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044507.1.7",
          "form": "어디 갈 때 차로 입을게 필요해",
          "original_form": "어디 갈 때 차로 입을게 필요해",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044507.1.8",
          "form": "가을 원피스 하나 있지 않나",
          "original_form": "가을 원피스 하나 있지 않나",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044507.1.9",
          "form": "걘 있는데 그게 짧아서",
          "original_form": "걘 있는데 그게 짧아서",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044507.1.10",
          "form": "긴 거 살려고요",
          "original_form": "긴 거 살려고요",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044507.1.11",
          "form": "치마가 편하긴 하지",
          "original_form": "치마가 편하긴 하지",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044507.1.12",
          "form": "올라 논 거 있어",
          "original_form": "올라 논 거 있어",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044507.1.13",
          "form": "아니 이제부터 찾아보려고",
          "original_form": "아니 이제부터 찾아보려고",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044507.1.14",
          "form": "쿠팡에서 사게",
          "original_form": "쿠팡에서 사게",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044507.1.15",
          "form": "응  귀찮아서 응",
          "original_form": "응  귀찮아서 응",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044507.1.16",
          "form": "옷은 입어보고 사야되지 않아",
          "original_form": "옷은 입어보고 사야되지 않아",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044507.1.17",
          "form": "대충 보면 맞겠지",
          "original_form": "대충 보면 맞겠지",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044507.1.18",
          "form": "옷 입어보는 것도 귀찮아",
          "original_form": "옷 입어보는 것도 귀찮아",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044507.1.19",
          "form": "우리 딸 대단해",
          "original_form": "우리 딸 대단해",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044507.1.20",
          "form": "안 맞으면 다시 교환하고",
          "original_form": "안 맞으면 다시 교환하고",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044507.1.21",
          "form": "무난한 걸로 사야겠네",
          "original_form": "무난한 걸로 사야겠네",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044507.1.22",
          "form": "그럴려고요",
          "original_form": "그럴려고요",
          "speaker_id": "1",
          "time": "20210907 22:58"
        }
      ]
