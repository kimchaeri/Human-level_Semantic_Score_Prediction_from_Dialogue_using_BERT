        {
          "id": "MDRW2100046207.1.1",
          "form": "하면 뭐가 달라지나요?",
          "original_form": "하면 뭐가 달라지나요?",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046207.1.2",
          "form": "하면 뭐가 달라지나요",
          "original_form": "하면 뭐가 달라지나요",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046207.1.3",
          "form": "핸들링하기전엔 손을 안타요",
          "original_form": "핸들링하기전엔 손을 안타요",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046207.1.4",
          "form": "그러면 꼭 해야겠네요",
          "original_form": "그러면 꼭 해야겠네요",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046207.1.5",
          "form": "네 하는게 키우시편하죠",
          "original_form": "네 하는게 키우시편하죠",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046207.1.6",
          "form": "잡아야 할때가 많아서요?",
          "original_form": "잡아야 할때가 많아서요?",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046207.1.7",
          "form": "네 병원이나 이동할때 잡아야하니까",
          "original_form": "네 병원이나 이동할때 잡아야하니까",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046207.1.8",
          "form": "집청소할때도 그렇겠어요",
          "original_form": "집청소할때도 그렇겠어요",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046207.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046207.1.10",
          "form": "그럼 핸들링은 필수겠네요",
          "original_form": "그럼 핸들링은 필수겠네요",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046207.1.11",
          "form": "근데 성격따라서 달라뇨",
          "original_form": "근데 성격따라서 달라뇨",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046207.1.12",
          "form": "성격도 다 다른가요?",
          "original_form": "성격도 다 다른가요?",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046207.1.13",
          "form": "네 순한애도 있고 아닌애도 있어서",
          "original_form": "네 순한애도 있고 아닌애도 있어서",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046207.1.14",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046207.1.15",
          "form": "걔네도 사람이랑 다를바가 없군요",
          "original_form": "걔네도 사람이랑 다를바가 없군요",
