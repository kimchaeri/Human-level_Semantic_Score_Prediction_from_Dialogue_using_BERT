      "utterance": [
        {
          "id": "MDRW2100013845.1.1",
          "form": "저녁 무ㅓ 드실건가용",
          "original_form": "저녁 무ㅓ 드실건가용",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013845.1.2",
          "form": "라면을먹을것같아요",
          "original_form": "라면을먹을것같아요",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013845.1.3",
          "form": "배고프네요",
          "original_form": "배고프네요",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013845.1.4",
          "form": "슬슬 출출할 시간이죠ㅋㅋㅋ",
          "original_form": "슬슬 출출할 시간이죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013845.1.5",
          "form": "무슨 라면이요?",
          "original_form": "무슨 라면이요?",
          "speaker_id": "2",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013845.1.6",
          "form": "진라면이땡기넹ㆍ",
          "original_form": "진라면이땡기넹ㆍ",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013845.1.7",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013845.1.8",
          "form": "저듀 원래 국물라면은 진라면만 먹었었는데 요새는 신라면이 더 맛있는거 같아요",
          "original_form": "저듀 원래 국물라면은 진라면만 먹었었는데 요새는 신라면이 더 맛있는거 같아요",
          "speaker_id": "2",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013845.1.9",
          "form": "신라면도맛있죠!!",
          "original_form": "신라면도맛있죠!!",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013845.1.10",
          "form": "볶음신라면 목어보셨나용?",
          "original_form": "볶음신라면 목어보셨나용?",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013845.1.11",
          "form": "밥이랑같이 먹고싶네요",
          "original_form": "밥이랑같이 먹고싶네요",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013845.1.12",
          "form": "국물에 밥 말기는 국룰~~",
          "original_form": "국물에 밥 말기는 국룰~~",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013845.1.13",
          "form": "그거는아직못먹었어요",
          "original_form": "그거는아직못먹었어요",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013845.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013845.1.15",
          "form": "저도요ㅋㅋ새로 나와서 핫한거같던데",
          "original_form": "저도요ㅋㅋ새로 나와서 핫한거같던데",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013845.1.16",
          "form": "맛있죠 그렇게 먹으면요",
          "original_form": "맛있죠 그렇게 먹으면요",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013845.1.17",
          "form": "오옹!!나중에먹어봐야겠다",
          "original_form": "오옹!!나중에먹어봐야겠다",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
