      "utterance": [
        {
          "id": "MDRW2100003861.1.1",
          "form": "요즘 뭐 구매하셨어요??",
          "original_form": "요즘 뭐 구매하셨어요??",
          "speaker_id": "2",
          "time": "20210821 13:21"
        },
        {
          "id": "MDRW2100003861.1.2",
          "form": "저 최근에 식료품들구매하고 그랬네요",
          "original_form": "저 최근에 식료품들구매하고 그랬네요",
          "speaker_id": "1",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003861.1.3",
          "form": "코로나라 확실히 인터넷으로 더 많이사는것같아요",
          "original_form": "코로나라 확실히 인터넷으로 더 많이사는것같아요",
          "speaker_id": "1",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003861.1.4",
          "form": "name2님은여",
          "original_form": "&name2&님은여",
          "speaker_id": "1",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003861.1.5",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "2",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003861.1.6",
          "form": "맨날 인터넷만하게되죠 ㅠ",
          "original_form": "맨날 인터넷만하게되죠 ㅠ",
          "speaker_id": "2",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003861.1.7",
          "form": "네ㅜㅜ마트도 예전만큼 잘 안가게되더라구여",
          "original_form": "네ㅜㅜ마트도 예전만큼 잘 안가게되더라구여",
          "speaker_id": "1",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003861.1.8",
          "form": "거의 인터넷으로 다시키는것같아요",
          "original_form": "거의 인터넷으로 다시키는것같아요",
          "speaker_id": "1",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003861.1.9",
          "form": "또 배달잘되니까 안나갈라면 안나갈수있을듯여",
          "original_form": "또 배달잘되니까 안나갈라면 안나갈수있을듯여",
          "speaker_id": "1",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003861.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210821 13:30"
        },
        {
          "id": "MDRW2100003861.1.11",
          "form": "요즘은 뭐든 배달이 다 잘되어있어서",
          "original_form": "요즘은 뭐든 배달이 다 잘되어있어서",
          "speaker_id": "2",
          "time": "20210821 13:30"
        },
        {
          "id": "MDRW2100003861.1.12",
          "form": "배달로 다해요",
          "original_form": "배달로 다해요",
          "speaker_id": "2",
          "time": "20210821 13:30"
        },
        {
          "id": "MDRW2100003861.1.13",
          "form": "맞아요 요새 편의점에서도 배달을해서 넘편하긴한데",
          "original_form": "맞아요 요새 편의점에서도 배달을해서 넘편하긴한데",
          "speaker_id": "1",
          "time": "20210821 13:32"
        },
        {
          "id": "MDRW2100003861.1.14",
          "form": "진짜 움직이질않게되더라구여 ㅋㅋ",
          "original_form": "진짜 움직이질않게되더라구여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 13:32"
        },
        {
          "id": "MDRW2100003861.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210821 13:35"
        },
        {
          "id": "MDRW2100003861.1.16",
          "form": "전 방금 소비는 보쌈을 사먹은거에요",
          "original_form": "전 방금 소비는 보쌈을 사먹은거에요",
          "speaker_id": "2",
          "time": "20210821 13:36"
        },
        {
          "id": "MDRW2100003861.1.17",
          "form": "오 보쌈 맛있겠어요ㅜㅜ막국수랑같이 돌돌말아서",
          "original_form": "오 보쌈 맛있겠어요ㅜㅜ막국수랑같이 돌돌말아서",
          "speaker_id": "1",
          "time": "20210821 13:37"
        },
        {
          "id": "MDRW2100003861.1.18",
          "form": "네 너무 맛있었어요 ㅠㅠㅠㅠㅠ",
          "original_form": "네 너무 맛있었어요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 13:40"
        },
        {
          "id": "MDRW2100003861.1.19",
          "form": "크으 그러면진짜 돈쓸맛나요ㅜ",
          "original_form": "크으 그러면진짜 돈쓸맛나요ㅜ",
          "speaker_id": "1",
          "time": "20210821 13:41"
        },
        {
          "id": "MDRW2100003861.1.20",
          "form": "맛있는거사먹으려고 내가 돈을버는구나!",
          "original_form": "맛있는거사먹으려고 내가 돈을버는구나!",
          "speaker_id": "1",
          "time": "20210821 13:41"
        },
        {
          "id": "MDRW2100003861.1.21",
          "form": "정말요 아주 뿌듯했엉요!!!!!",
          "original_form": "정말요 아주 뿌듯했엉요!!!!!",
          "speaker_id": "2",
          "time": "20210821 13:41"
        },
        {
          "id": "MDRW2100003861.1.22",
          "form": "진짜 뿌듯 시간지나면 조금 현타오긴하지만 돈쓸때 넘행복해요ㅋㅋ",
          "original_form": "진짜 뿌듯 시간지나면 조금 현타오긴하지만 돈쓸때 넘행복해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 13:45"
        }
      ]
