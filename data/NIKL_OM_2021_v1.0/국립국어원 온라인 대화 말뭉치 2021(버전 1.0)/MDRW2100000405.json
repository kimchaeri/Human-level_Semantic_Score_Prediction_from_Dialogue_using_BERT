        },
        {
          "id": "MDRW2100000405.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 15:52"
        },
        {
          "id": "MDRW2100000405.1.3",
          "form": "요즘도 운동하고 계시나요",
          "original_form": "요즘도 운동하고 계시나요",
          "speaker_id": "2",
          "time": "20210819 15:52"
        },
        {
          "id": "MDRW2100000405.1.4",
          "form": "배드민턴 가끔씩 하고 있어요",
          "original_form": "배드민턴 가끔씩 하고 있어요",
          "speaker_id": "1",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000405.1.5",
          "form": "마스크 하고 운동하기가 쉽지 않아서",
          "original_form": "마스크 하고 운동하기가 쉽지 않아서",
          "speaker_id": "2",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000405.1.6",
          "form": "걱정이에요",
          "original_form": "걱정이에요",
          "speaker_id": "2",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000405.1.7",
          "form": "맞아요~그게 너무 어려워요",
          "original_form": "맞아요~그게 너무 어려워요",
          "speaker_id": "1",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000405.1.8",
          "form": "저도 산책이라도 하려면 마스크가 너무 힘들어요",
          "original_form": "저도 산책이라도 하려면 마스크가 너무 힘들어요",
          "speaker_id": "2",
          "time": "20210819 15:55"
        },
        {
          "id": "MDRW2100000405.1.9",
          "form": "살찌지 않으려고 겨우 기본만 하고 있어요",
          "original_form": "살찌지 않으려고 겨우 기본만 하고 있어요",
          "speaker_id": "2",
          "time": "20210819 15:55"
        },
        {
          "id": "MDRW2100000405.1.10",
          "form": "네~저는 담달부터 수영을 해보려고요",
          "original_form": "네~저는 담달부터 수영을 해보려고요",
          "speaker_id": "1",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000405.1.11",
          "form": "오 수영 가능한 곳이 있나 보네요.",
          "original_form": "오 수영 가능한 곳이 있나 보네요.",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000405.1.12",
          "form": "2년 전에 끊어놓은 수강권이 아직도ㅠㅠ",
          "original_form": "2년 전에 끊어놓은 수강권이 아직도ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000405.1.13",
          "form": "네~근처에 있더가구요~그래서 한번 해보려고요",
          "original_form": "네~근처에 있더가구요~그래서 한번 해보려고요",
          "speaker_id": "1",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000405.1.14",
          "form": "네 이럴때일수록 꾸준한 운동이 중요한 것 같아요",
          "original_form": "네 이럴때일수록 꾸준한 운동이 중요한 것 같아요",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000405.1.15",
          "form": "네 맞아요 살이 엄청 쪘어요ㅠ",
          "original_form": "네 맞아요 살이 엄청 쪘어요ㅠ",
          "speaker_id": "1",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000405.1.16",
          "form": "마스크 벗고 신나게 운동하고 싶네요 ㅠ",
          "original_form": "마스크 벗고 신나게 운동하고 싶네요 ㅠ",
          "speaker_id": "2",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000405.1.17",
          "form": "네 정말요ㅠㅠ",
          "original_form": "네 정말요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000405.1.18",
          "form": "찌긴 쉬워도 빼긴 어려우니까요 ㅠㅠ",
          "original_form": "찌긴 쉬워도 빼긴 어려우니까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000405.1.19",
          "form": "네 진짜 그말이 맞아요",
          "original_form": "네 진짜 그말이 맞아요",
          "speaker_id": "1",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000405.1.20",
          "form": "밖에 나가기가 힘들어서 실내 싸이클이라도 타야할까봐요",
          "original_form": "밖에 나가기가 힘들어서 실내 싸이클이라도 타야할까봐요",
          "speaker_id": "2",
          "time": "20210819 15:59"
        },
        {
          "id": "MDRW2100000405.1.21",
          "form": "그것도 좋겠어요!",
          "original_form": "그것도 좋겠어요!",
          "speaker_id": "1",
          "time": "20210819 15:59"
        },
        {
          "id": "MDRW2100000405.1.22",
          "form": "빨래 걸이가 될까봐 사려고 백만번 고민 중입니다 ㅎㅎ",
          "original_form": "빨래 걸이가 될까봐 사려고 백만번 고민 중입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000405.1.23",
          "form": "ㅋㅋ 저랑 같아요 ㅎㅎ",
          "original_form": "ㅋㅋ 저랑 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000405.1.24",
          "form": "집 공간도 좁은데 들여놨다 안하면 곤란하니까요 ^^;;;",
          "original_form": "집 공간도 좁은데 들여놨다 안하면 곤란하니까요 ^^;;;",
