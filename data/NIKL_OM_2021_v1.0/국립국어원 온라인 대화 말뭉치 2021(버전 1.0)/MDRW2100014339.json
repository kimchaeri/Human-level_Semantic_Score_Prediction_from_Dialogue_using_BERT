      "utterance": [
        {
          "id": "MDRW2100014339.1.1",
          "form": "코시국 때문에 못가지만 가고싶은 여행지 있으신가요?",
          "original_form": "코시국 때문에 못가지만 가고싶은 여행지 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014339.1.2",
          "form": "베네치아 가보고 싶어요",
          "original_form": "베네치아 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014339.1.3",
          "form": "오 베네치아 하면 그 곤돌라 타는게 생각나요",
          "original_form": "오 베네치아 하면 그 곤돌라 타는게 생각나요",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014339.1.4",
          "form": "해질녁에 물위에서 타면 멋있을거같아요",
          "original_form": "해질녁에 물위에서 타면 멋있을거같아요",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014339.1.5",
          "form": "맞아요! 석양을 곤돌라 위에서 보고 싶어요",
          "original_form": "맞아요! 석양을 곤돌라 위에서 보고 싶어요",
          "speaker_id": "2",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014339.1.6",
          "form": "배경도 멋있어서 진짜 예쁠것같아요",
          "original_form": "배경도 멋있어서 진짜 예쁠것같아요",
          "speaker_id": "1",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014339.1.7",
          "form": "전 날씨 좋은곳을 좋아해서",
          "original_form": "전 날씨 좋은곳을 좋아해서",
          "speaker_id": "1",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014339.1.8",
          "form": "la가보고 싶어요",
          "original_form": "la가보고 싶어요",
          "speaker_id": "1",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014339.1.9",
          "form": "la 좋죠ㅠㅠ 구경할 곳도 많구요",
          "original_form": "la 좋죠ㅠㅠ 구경할 곳도 많구요",
          "speaker_id": "2",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100014339.1.10",
          "form": "혹시 가보셨나요?",
          "original_form": "혹시 가보셨나요?",
          "speaker_id": "1",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014339.1.11",
          "form": "날씨도 좋고 디즈니랜드도있어서 꼭 가보고싶어요",
          "original_form": "날씨도 좋고 디즈니랜드도있어서 꼭 가보고싶어요",
          "speaker_id": "1",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100014339.1.12",
          "form": "예전에 한 번 가봤어요ㅋㅋ 구경할 곳이 많아 눈이 즐거웠어요",
          "original_form": "예전에 한 번 가봤어요ㅋㅋ 구경할 곳이 많아 눈이 즐거웠어요",
          "speaker_id": "2",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014339.1.13",
          "form": "와 진짜 부러워요",
          "original_form": "와 진짜 부러워요",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014339.1.14",
          "form": "혹시 추천해주실 곳 있으신가요?",
          "original_form": "혹시 추천해주실 곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014339.1.15",
          "form": "가신다면 할리우드 거리는 꼭 가세요! 해리포터 배우들 손도장도 있어서 사진 찍었었어요",
          "original_form": "가신다면 할리우드 거리는 꼭 가세요! 해리포터 배우들 손도장도 있어서 사진 찍었었어요",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014339.1.16",
          "form": "오 너무 부러운경험입니다",
          "original_form": "오 너무 부러운경험입니다",
          "speaker_id": "1",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014339.1.17",
          "form": "꼭 가볼께요",
          "original_form": "꼭 가볼께요",
          "speaker_id": "1",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014339.1.18",
          "form": "거기 마담투소 박물관이나",
          "original_form": "거기 마담투소 박물관이나",
          "speaker_id": "1",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014339.1.19",
          "form": "볼거리들도 많다고 들었어요",
          "original_form": "볼거리들도 많다고 들었어요",
          "speaker_id": "1",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014339.1.20",
          "form": "la만 돌아도 몇 박은 하셔야 할거에요ㅋㅋ",
          "original_form": "la만 돌아도 몇 박은 하셔야 할거에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014339.1.21",
          "form": "생각만 해도 좋은데요 마음 같아서는 한달있고싶어요",
          "original_form": "생각만 해도 좋은데요 마음 같아서는 한달있고싶어요",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014339.1.22",
          "form": "저도 다시 가서 몇 주 있고 싶어요ㅠ",
          "original_form": "저도 다시 가서 몇 주 있고 싶어요ㅠ",
