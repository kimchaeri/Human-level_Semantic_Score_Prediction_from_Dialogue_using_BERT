      "utterance": [
        {
          "id": "MDRW2100034357.1.1",
          "form": "와ㅋㅋ7번이면ㅋㅋ",
          "original_form": "와ㅋㅋ7번이면ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100034357.1.2",
          "form": "그래서 이것저것 많이봐요 ㅋㅋㅋ",
          "original_form": "그래서 이것저것 많이봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100034357.1.3",
          "form": "아하 약간 가볍게 볼수있는거군요",
          "original_form": "아하 약간 가볍게 볼수있는거군요",
          "speaker_id": "2",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100034357.1.4",
          "form": "또 없나요?ㅋㅋ",
          "original_form": "또 없나요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100034357.1.5",
          "form": "네 약간 미국 순풍산부인과? 느낌요",
          "original_form": "네 약간 미국 순풍산부인과? 느낌요",
          "speaker_id": "1",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100034357.1.6",
          "form": "추천하신거 다 봐야겠어요ㅋㅋ",
          "original_form": "추천하신거 다 봐야겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100034357.1.7",
          "form": "음.. 채울수없는 이라고 인생 드라마있어요",
          "original_form": "음.. 채울수없는 이라고 인생 드라마있어요",
          "speaker_id": "1",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100034357.1.8",
          "form": "아~그런거 좋죠!ㅋㅋ",
          "original_form": "아~그런거 좋죠!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100034357.1.9",
          "form": "순풍산부인과면 저랑 동년배시네요ㅋㅋ",
          "original_form": "순풍산부인과면 저랑 동년배시네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100034357.1.10",
          "form": "사실 그거 할때 보진 못햇는데 몇년전에 정주행했어요 ㅋㅋㅋ",
          "original_form": "사실 그거 할때 보진 못햇는데 몇년전에 정주행했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100034357.1.11",
          "form": "그것도 미드인가요?",
          "original_form": "그것도 미드인가요?",
          "speaker_id": "2",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100034357.1.12",
          "form": "네 하이틴인데 주로 학교내용보다는",
          "original_form": "네 하이틴인데 주로 학교내용보다는",
          "speaker_id": "1",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100034357.1.13",
          "form": "뚱뚱한 여자주인공한테 노숙자가 초코바 뺏어서 싸우다가",
          "original_form": "뚱뚱한 여자주인공한테 노숙자가 초코바 뺏어서 싸우다가",
          "speaker_id": "1",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100034357.1.14",
          "form": "네ㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100034357.1.15",
          "form": "노숙자한테 맞고 날씬해져서 미인대회 나가는 스토린데 생각보다 심오한 블랙코미디에요",
          "original_form": "노숙자한테 맞고 날씬해져서 미인대회 나가는 스토린데 생각보다 심오한 블랙코미디에요",
          "speaker_id": "1",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100034357.1.16",
          "form": "오~좋네요",
          "original_form": "오~좋네요",
          "speaker_id": "2",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100034357.1.17",
          "form": "그냥 웃긴건줄알았는데",
          "original_form": "그냥 웃긴건줄알았는데",
          "speaker_id": "2",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100034357.1.18",
          "form": "그쵸 ㅋㅋㅋ 재밌는데 다음시즌 없이 끝나버려서 아쉬워요 ㅜ",
          "original_form": "그쵸 ㅋㅋㅋ 재밌는데 다음시즌 없이 끝나버려서 아쉬워요 ㅜ",
