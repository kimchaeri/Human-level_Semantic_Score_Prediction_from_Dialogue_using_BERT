      "utterance": [
        {
          "id": "MDRW2100017931.1.1",
          "form": "나두..",
          "original_form": "나두..",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017931.1.2",
          "form": "세종문화회관같은 큰데",
          "original_form": "세종문화회관같은 큰데",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017931.1.3",
          "form": "가고싶다",
          "original_form": "가고싶다",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017931.1.4",
          "form": "국립극장",
          "original_form": "국립극장",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017931.1.5",
          "form": "대학로",
          "original_form": "대학로",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017931.1.6",
          "form": "홍대",
          "original_form": "홍대",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017931.1.7",
          "form": "가서공연보고싶다",
          "original_form": "가서공연보고싶다",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017931.1.8",
          "form": "와 그때 너랑 세종문화회관에서 보고",
          "original_form": "와 그때 너랑 세종문화회관에서 보고",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017931.1.9",
          "form": "다음엔 부산시립극장에서 봤는데",
          "original_form": "다음엔 부산시립극장에서 봤는데",
          "speaker_id": "1",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017931.1.10",
          "form": "나름 부산에서 젤 큰데도 서울이랑은 쨉도 안되더라 ㅠ",
          "original_form": "나름 부산에서 젤 큰데도 서울이랑은 쨉도 안되더라 ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017931.1.11",
          "form": "그니까 눈이",
          "original_form": "그니까 눈이",
          "speaker_id": "2",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017931.1.12",
          "form": "너무높아져쓰",
          "original_form": "너무높아져쓰",
          "speaker_id": "2",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017931.1.13",
          "form": "부산공연은 뭐보러갔는데?",
          "original_form": "부산공연은 뭐보러갔는데?",
          "speaker_id": "2",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017931.1.14",
          "form": "오페라의 유령",
          "original_form": "오페라의 유령",
          "speaker_id": "1",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017931.1.15",
          "form": "효신님 나와서 보러갔었쥐 ㅋㅋㅋㅋ",
          "original_form": "효신님 나와서 보러갔었쥐 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017931.1.16",
          "form": "오와",
          "original_form": "오와",
          "speaker_id": "2",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017931.1.17",
          "form": "ㅋㅋㅋㅋㅋ젯밥에 관심이었구만",
          "original_form": "ㅋㅋㅋㅋㅋ젯밥에 관심이었구만",
          "speaker_id": "2",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017931.1.18",
          "form": "근데 오페라의 유령에서 가장 유명한 넘버가 안나오더라 ㅠ",
          "original_form": "근데 오페라의 유령에서 가장 유명한 넘버가 안나오더라 ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017931.1.19",
          "form": "ㅋㅋㅋㅋ 글고 음향도...서울에 비함 너무 안좋아서 ㅠ 경험이 안좋았어",
          "original_form": "ㅋㅋㅋㅋ 글고 음향도...서울에 비함 너무 안좋아서 ㅠ 경험이 안좋았어",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017931.1.20",
          "form": "역시 문화랑 인프라는 서울인가봐..",
          "original_form": "역시 문화랑 인프라는 서울인가봐..",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017931.1.21",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017931.1.22",
          "form": "그런게진짜한번맛을보면",
          "original_form": "그런게진짜한번맛을보면",
          "speaker_id": "2",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017931.1.23",
          "form": "아쉽더라고ㅜ",
          "original_form": "아쉽더라고ㅜ",
          "speaker_id": "2",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017931.1.24",
          "form": "내려와서는 코로나탓도있고",
          "original_form": "내려와서는 코로나탓도있고",
          "speaker_id": "2",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017931.1.25",
          "form": "엄청 큰거 또 보러 가고싶다 ",
          "original_form": "엄청 큰거 또 보러 가고싶다 ",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017931.1.26",
          "form": "공연잘안보게됨",
          "original_form": "공연잘안보게됨",
          "speaker_id": "2",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017931.1.27",
          "form": "나도 안본지 엄청 오래됐다...",
          "original_form": "나도 안본지 엄청 오래됐다...",
          "speaker_id": "1",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017931.1.28",
          "form": "그래도 대학 다닐땐 소극장 공연이라도 드물게 보고",
          "original_form": "그래도 대학 다닐땐 소극장 공연이라도 드물게 보고",
          "speaker_id": "1",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017931.1.29",
          "form": "음악학부 연주회나 교양땜시라도 공연 보러 댕겼는데",
          "original_form": "음악학부 연주회나 교양땜시라도 공연 보러 댕겼는데",
          "speaker_id": "1",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017931.1.30",
          "form": "마지막으로 뭐 본게 언젠지 깜깜혀..",
          "original_form": "마지막으로 뭐 본게 언젠지 깜깜혀..",
          "speaker_id": "1",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017931.1.31",
          "form": "난 일주일2공연찍고그랬다구ㅋㅋㅋ",
          "original_form": "난 일주일2공연찍고그랬다구ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017931.1.32",
          "form": "대학동아리공연부터",
          "original_form": "대학동아리공연부터",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017931.1.33",
          "form": "세종문화회관까지..",
          "original_form": "세종문화회관까지..",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017931.1.34",
          "form": "지금은 시간도체력도없고",
          "original_form": "지금은 시간도체력도없고",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017931.1.35",
          "form": "눈이너무높아졌짘ㅋㅋㅋ",
          "original_form": "눈이너무높아졌짘ㅋㅋㅋ",
          "speaker_id": "2",
