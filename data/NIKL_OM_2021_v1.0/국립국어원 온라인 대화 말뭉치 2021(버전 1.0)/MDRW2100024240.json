      "utterance": [
        {
          "id": "MDRW2100024240.1.1",
          "form": "놀이방 있는데만 가시나요?ㅎㅎ",
          "original_form": "놀이방 있는데만 가시나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024240.1.2",
          "form": "일단은 놀이방 위주로요^^",
          "original_form": "일단은 놀이방 위주로요^^",
          "speaker_id": "1",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100024240.1.3",
          "form": "ㅎㅎ 그럼 메뉴선택에 제약이 많겟네요?",
          "original_form": "ㅎㅎ 그럼 메뉴선택에 제약이 많겟네요?",
          "speaker_id": "2",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100024240.1.4",
          "form": "일단은 그렇죠 그래도 고기집이 있어 다행이죠",
          "original_form": "일단은 그렇죠 그래도 고기집이 있어 다행이죠",
          "speaker_id": "1",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100024240.1.5",
          "form": "ㅎㅎ 고깃집은 좋아요ㅎㅎ",
          "original_form": "ㅎㅎ 고깃집은 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100024240.1.6",
          "form": "소주를 자주드시는편이애요? 맥주파에요?",
          "original_form": "소주를 자주드시는편이애요? 맥주파에요?",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100024240.1.7",
          "form": "진리죠 아니면 국밥포장후 집에서",
          "original_form": "진리죠 아니면 국밥포장후 집에서",
          "speaker_id": "1",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100024240.1.8",
          "form": "전 맥주파!!",
          "original_form": "전 맥주파!!",
          "speaker_id": "1",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100024240.1.9",
          "form": "ㅎㅎㅎ 맥주파ㅎㅎ",
          "original_form": "ㅎㅎㅎ 맥주파ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100024240.1.10",
          "form": "필굿파죠",
          "original_form": "필굿파죠",
          "speaker_id": "1",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100024240.1.11",
          "form": "배달은자주?ㅎ",
          "original_form": "배달은자주?ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100024240.1.12",
          "form": "2주에1번",
          "original_form": "2주에1번",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100024240.1.13",
          "form": "아니면 간단하게 드샤요?",
          "original_form": "아니면 간단하게 드샤요?",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100024240.1.14",
          "form": "우왐ㅎ 많이 안시켜드시네요ㅎ",
          "original_form": "우왐ㅎ 많이 안시켜드시네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100024240.1.15",
          "form": "애들때문에 기본 음식은해요",
          "original_form": "애들때문에 기본 음식은해요",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100024240.1.16",
          "form": "그럼 맥주안주는뭘로?",
          "original_form": "그럼 맥주안주는뭘로?",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100024240.1.17",
          "form": "아아ㅎㅎ",
