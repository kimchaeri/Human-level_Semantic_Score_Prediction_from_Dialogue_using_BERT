      "utterance": [
        {
          "id": "MDRW2100044354.1.1",
          "form": "이번에 나 휴가 때 어디 갈 거에요",
          "original_form": "이번에 나 휴가 때 어디 갈 거에요",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044354.1.2",
          "form": "저번에 갔던데",
          "original_form": "저번에 갔던데",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044354.1.3",
          "form": "어디???",
          "original_form": "어디???",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044354.1.4",
          "form": "영흥도??",
          "original_form": "영흥도??",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044354.1.5",
          "form": "응 거기가 좋아",
          "original_form": "응 거기가 좋아",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044354.1.6",
          "form": "이번에는 언제 갔다 언제 올 거에요",
          "original_form": "이번에는 언제 갔다 언제 올 거에요",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044354.1.7",
          "form": "주일에 갔다가 화요일에",
          "original_form": "주일에 갔다가 화요일에",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044354.1.8",
          "form": "수요일에 와도 되지 않아요",
          "original_form": "수요일에 와도 되지 않아요",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044354.1.9",
          "form": "수요일 오전에 오는 것도 괜찮지",
          "original_form": "수요일 오전에 오는 것도 괜찮지",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044354.1.10",
          "form": "그러면은 주일 날 오전에 갔다가 수요일날 오전에 오기",
          "original_form": "그러면은 주일 날 오전에 갔다가 수요일날 오전에 오기",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044354.1.11",
          "form": "그래 아빠한테 한번 얘기해볼게",
          "original_form": "그래 아빠한테 한번 얘기해볼게",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044354.1.12",
          "form": "아빠도 그게 괜찮을 거 같다고 할걸요",
          "original_form": "아빠도 그게 괜찮을 거 같다고 할걸요",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044354.1.13",
          "form": "그럴 것 같네",
          "original_form": "그럴 것 같네",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044354.1.14",
          "form": "그때 가서 뭐 먹을 거에요",
          "original_form": "그때 가서 뭐 먹을 거에요",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044354.1.15",
          "form": "글쎄 저번에랑 비슷하지 않을까",
          "original_form": "글쎄 저번에랑 비슷하지 않을까",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044354.1.16",
          "form": "무언처럼 반찬 다 싸가게",
          "original_form": "무언처럼 반찬 다 싸가게",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044354.1.17",
          "form": "응 그게 제일 좋은 것 같애",
          "original_form": "응 그게 제일 좋은 것 같애",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044354.1.18",
          "form": "이번에는 뭐 좀 사 먹을까",
          "original_form": "이번에는 뭐 좀 사 먹을까",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044354.1.19",
          "form": "그것도 괜찮지",
          "original_form": "그것도 괜찮지",
          "speaker_id": "1",
          "time": "20210907 22:25"
        }
      ]
