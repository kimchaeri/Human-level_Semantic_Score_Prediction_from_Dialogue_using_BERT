      "utterance": [
        {
          "id": "MDRW2100017095.1.1",
          "form": "일반쓰레기 많이 나오세요?",
          "original_form": "일반쓰레기 많이 나오세요?",
          "speaker_id": "1",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017095.1.2",
          "form": "네 많이 나와요",
          "original_form": "네 많이 나와요",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017095.1.3",
          "form": "아르바이트 어떠신가요",
          "original_form": "아르바이트 어떠신가요",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017095.1.4",
          "form": "완전 노잼",
          "original_form": "완전 노잼",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017095.1.5",
          "form": "님은여?",
          "original_form": "님은여?",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017095.1.6",
          "form": "저는 아르바이트 안해서요",
          "original_form": "저는 아르바이트 안해서요",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017095.1.7",
          "form": "재미없긴해요 아르바이트",
          "original_form": "재미없긴해요 아르바이트",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017095.1.8",
          "form": "혼자 일하는거 하는데 폰 엄청 해요",
          "original_form": "혼자 일하는거 하는데 폰 엄청 해요",
          "speaker_id": "1",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017095.1.9",
          "form": "아르바이트 팁 있으신가요",
          "original_form": "아르바이트 팁 있으신가요",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017095.1.10",
          "form": "아뇨 특별히 없는데",
          "original_form": "아뇨 특별히 없는데",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017095.1.11",
          "form": "그냥 아무생각없이 하는게 좋아요",
          "original_form": "그냥 아무생각없이 하는게 좋아요",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017095.1.12",
          "form": "맞아요 그게 정답이네요",
          "original_form": "맞아요 그게 정답이네요",
          "speaker_id": "2",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017095.1.13",
          "form": "업무스트레스는 역시 진상손님이겠죠?",
          "original_form": "업무스트레스는 역시 진상손님이겠죠?",
          "speaker_id": "2",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017095.1.14",
          "form": "yes...",
          "original_form": "yes...",
          "speaker_id": "1",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100017095.1.15",
          "form": "아주 스트레스받았었죠..",
          "original_form": "아주 스트레스받았었죠..",
          "speaker_id": "1",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100017095.1.16",
          "form": "저는 낙지집아르바이트 해봤어요",
          "original_form": "저는 낙지집아르바이트 해봤어요",
          "speaker_id": "2",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100017095.1.17",
          "form": "서빙은 옷이 너무 더러워져서 싫더라구여",
          "original_form": "서빙은 옷이 너무 더러워져서 싫더라구여",
          "speaker_id": "2",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100017095.1.18",
          "form": "음식이 너무 묻어요",
          "original_form": "음식이 너무 묻어요",
          "speaker_id": "2",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100017095.1.19",
          "form": "헐 정맖싫네요",
          "original_form": "헐 정맖싫네요",
          "speaker_id": "1",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100017095.1.20",
          "form": "거기는 진상 많이 없나요?",
          "original_form": "거기는 진상 많이 없나요?",
          "speaker_id": "1",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100017095.1.21",
          "form": "마감시간 알려드렸는데 좀 넘게 나가시는 분들?.. 말고는 잘 기억이 안나네요",
          "original_form": "마감시간 알려드렸는데 좀 넘게 나가시는 분들?.. 말고는 잘 기억이 안나네요",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100017095.1.22",
          "form": "단체손님이 항상 많았어서 바빴었네요",
          "original_form": "단체손님이 항상 많았어서 바빴었네요",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100017095.1.23",
          "form": "아르바이트해서 돈 모으면 하고 싶은거 있으신가요?",
          "original_form": "아르바이트해서 돈 모으면 하고 싶은거 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100017095.1.24",
          "form": "헐 퇴근시간 늦어지는게 젤싫은데",
          "original_form": "헐 퇴근시간 늦어지는게 젤싫은데",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100017095.1.25",
          "form": "돈 모아서 여행 가고싶어요",
          "original_form": "돈 모아서 여행 가고싶어요",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100017095.1.26",
          "form": "name2는요?",
          "original_form": "&name2&는요?",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100017095.1.27",
          "form": "와 여행 좋죠",
          "original_form": "와 여행 좋죠",
          "speaker_id": "2",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100017095.1.28",
          "form": "저는 우선 투자하고싶어요 돈 모으면",
          "original_form": "저는 우선 투자하고싶어요 돈 모으면",
          "speaker_id": "2",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100017095.1.29",
          "form": "저두!!",
          "original_form": "저두!!",
          "speaker_id": "1",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100017095.1.30",
          "form": "월급만큼 수익 얻는게 목표랍니다",
          "original_form": "월급만큼 수익 얻는게 목표랍니다",
          "speaker_id": "1",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100017095.1.31",
          "form": "주식 관심 많아요?",
          "original_form": "주식 관심 많아요?",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100017095.1.32",
          "form": "네 돈 많이 벌면 큰 단위로 장기투자 하려고 합니다",
          "original_form": "네 돈 많이 벌면 큰 단위로 장기투자 하려고 합니다",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100017095.1.33",
          "form": "오 좋다",
          "original_form": "오 좋다",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100017095.1.34",
          "form": "어떤 직종 선호하세요?",
          "original_form": "어떤 직종 선호하세요?",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100017095.1.35",
          "form": "저는 사무직이 맞는 것 같아요",
          "original_form": "저는 사무직이 맞는 것 같아요",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100017095.1.36",
          "form": "아직 직장생활을 하지 않아서 잘 모르겠네요",
          "original_form": "아직 직장생활을 하지 않아서 잘 모르겠네요",
