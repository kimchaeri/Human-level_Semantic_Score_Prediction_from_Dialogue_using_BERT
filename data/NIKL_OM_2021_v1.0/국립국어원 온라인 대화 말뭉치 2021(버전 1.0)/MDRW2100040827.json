      "utterance": [
        {
          "id": "MDRW2100040827.1.1",
          "form": "가까운 사람 중에 암 걸린 사람 보셨어요?",
          "original_form": "가까운 사람 중에 암 걸린 사람 보셨어요?",
          "speaker_id": "1",
          "time": "20210905 09:09"
        },
        {
          "id": "MDRW2100040827.1.2",
          "form": "아 요즘은 주변에 암 걸린 분 한두분씩은 다 있는 듯요",
          "original_form": "아 요즘은 주변에 암 걸린 분 한두분씩은 다 있는 듯요",
          "speaker_id": "2",
          "time": "20210905 09:09"
        },
        {
          "id": "MDRW2100040827.1.3",
          "form": "네 저도 친구가 20대 때 암에 걸렸어요",
          "original_form": "네 저도 친구가 20대 때 암에 걸렸어요",
          "speaker_id": "1",
          "time": "20210905 09:10"
        },
        {
          "id": "MDRW2100040827.1.4",
          "form": "에고",
          "original_form": "에고",
          "speaker_id": "2",
          "time": "20210905 09:10"
        },
        {
          "id": "MDRW2100040827.1.5",
          "form": "괜찮아지셨나요?",
          "original_form": "괜찮아지셨나요?",
          "speaker_id": "2",
          "time": "20210905 09:10"
        },
        {
          "id": "MDRW2100040827.1.6",
          "form": "그래도 안 죽고 살아 있어서 다행이에요",
          "original_form": "그래도 안 죽고 살아 있어서 다행이에요",
          "speaker_id": "1",
          "time": "20210905 09:10"
        },
        {
          "id": "MDRW2100040827.1.7",
          "form": "처음엔 죽는 줄 알았어요",
          "original_form": "처음엔 죽는 줄 알았어요",
          "speaker_id": "1",
          "time": "20210905 09:10"
        },
        {
          "id": "MDRW2100040827.1.8",
          "form": "정말 다행이네요",
          "original_form": "정말 다행이네요",
          "speaker_id": "2",
          "time": "20210905 09:10"
        },
        {
          "id": "MDRW2100040827.1.9",
          "form": "초기에 발견하셨나보다",
          "original_form": "초기에 발견하셨나보다",
          "speaker_id": "2",
          "time": "20210905 09:10"
        },
        {
          "id": "MDRW2100040827.1.10",
          "form": "내 혈액암이 두 번이나 재발했어요",
          "original_form": "내 혈액암이 두 번이나 재발했어요",
          "speaker_id": "1",
          "time": "20210905 09:10"
        },
        {
          "id": "MDRW2100040827.1.11",
          "form": "재발까지 했다니",
          "original_form": "재발까지 했다니",
          "speaker_id": "2",
          "time": "20210905 09:11"
        },
        {
          "id": "MDRW2100040827.1.12",
          "form": "고생 많으셨겠네요",
          "original_form": "고생 많으셨겠네요",
          "speaker_id": "2",
          "time": "20210905 09:11"
        },
        {
          "id": "MDRW2100040827.1.13",
          "form": "한참 약 먹을 때는",
          "original_form": "한참 약 먹을 때는",
          "speaker_id": "1",
          "time": "20210905 09:11"
        },
        {
          "id": "MDRW2100040827.1.14",
          "form": "약 기운 때문에 잔다고 연락이 안 됐어요",
          "original_form": "약 기운 때문에 잔다고 연락이 안 됐어요",
          "speaker_id": "1",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100040827.1.15",
          "form": "항암약이 독하니까요ㅜ",
          "original_form": "항암약이 독하니까요ㅜ",
          "speaker_id": "2",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100040827.1.16",
          "form": "그래도 건강해지셨다니 정말 다행이예요",
          "original_form": "그래도 건강해지셨다니 정말 다행이예요",
          "speaker_id": "2",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100040827.1.17",
          "form": "그러니까요 살아서 다행이지 죽으면 너무 우울한 거 같아요",
          "original_form": "그러니까요 살아서 다행이지 죽으면 너무 우울한 거 같아요",
          "speaker_id": "1",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100040827.1.18",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100040827.1.19",
          "form": "나이도 젊은데",
          "original_form": "나이도 젊은데",
          "speaker_id": "2",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100040827.1.20",
          "form": "젊으면 암세포도 빨리 퍼진다더라고요",
          "original_form": "젊으면 암세포도 빨리 퍼진다더라고요",
          "speaker_id": "2",
          "time": "20210905 09:13"
        },
        {
          "id": "MDRW2100040827.1.21",
          "form": "그런가 봐요 늙어서 안 걸리면",
          "original_form": "그런가 봐요 늙어서 안 걸리면",
          "speaker_id": "1",
          "time": "20210905 09:14"
        },
        {
          "id": "MDRW2100040827.1.22",
          "form": "암세포가 사람이랑 공생처럼 그러면서 잘 안 죽는다고",
          "original_form": "암세포가 사람이랑 공생처럼 그러면서 잘 안 죽는다고",
          "speaker_id": "1",
          "time": "20210905 09:14"
        }
      ]
