      "utterance": [
        {
          "id": "MDRW2100043760.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043760.1.2",
          "form": "저녁식사 하셨나요?",
          "original_form": "저녁식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043760.1.3",
          "form": "안녕하세용 넹 먹었어요 name1님은요?",
          "original_form": "안녕하세용 넹 먹었어요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100043760.1.4",
          "form": "저도 먹었어요",
          "original_form": "저도 먹었어요",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100043760.1.5",
          "form": "어떤거 드셨나요?",
          "original_form": "어떤거 드셨나요?",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100043760.1.6",
          "form": "저는 그뭐지 고기먹었어요",
          "original_form": "저는 그뭐지 고기먹었어요",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100043760.1.7",
          "form": "돼지고기찜이었나..?",
          "original_form": "돼지고기찜이었나..?",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100043760.1.8",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100043760.1.9",
          "form": "전 칼국수 먹었어요",
          "original_form": "전 칼국수 먹었어요",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100043760.1.10",
          "form": "으와 맛있었겠네요",
          "original_form": "으와 맛있었겠네요",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100043760.1.11",
          "form": "칼국수 안먹은지 디게 오랜된 거 같네요",
          "original_form": "칼국수 안먹은지 디게 오랜된 거 같네요",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100043760.1.12",
          "form": "저도 간만에 먹었어요",
          "original_form": "저도 간만에 먹었어요",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100043760.1.13",
          "form": "날씨가 쌀쌀하니까 국물이 땡기더라구요",
          "original_form": "날씨가 쌀쌀하니까 국물이 땡기더라구요",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100043760.1.14",
          "form": "확실히 그렇죠..",
          "original_form": "확실히 그렇죠..",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100043760.1.15",
          "form": "저도 언제 한 번 먹어야겠어요",
          "original_form": "저도 언제 한 번 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100043760.1.16",
          "form": "수제비도 좋은거같아요",
          "original_form": "수제비도 좋은거같아요",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100043760.1.17",
          "form": "아 맞아요 어릴 때 자주 먹었어요 특히 비올 때!",
          "original_form": "아 맞아요 어릴 때 자주 먹었어요 특히 비올 때!",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100043760.1.18",
          "form": "그쵸 ㅋㅋㅋ 저 바지락은 별로 안좋아하는데",
          "original_form": "그쵸 ㅋㅋㅋ 저 바지락은 별로 안좋아하는데",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100043760.1.19",
          "form": "바지락 국물은 좋아해요",
          "original_form": "바지락 국물은 좋아해요",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100043760.1.20",
          "form": "아 뭔지 알 거 같아요..ㅋㅋㅋ 저도 해산물 거의 싫어하는 편인데 국물은 맛있는 경우 있더라구요",
          "original_form": "아 뭔지 알 거 같아요..ㅋㅋㅋ 저도 해산물 거의 싫어하는 편인데 국물은 맛있는 경우 있더라구요",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100043760.1.21",
          "form": "네 뭔가 그 시원한 국물이 가끔 땡기더라구요 ㅋㅋㅋ",
          "original_form": "네 뭔가 그 시원한 국물이 가끔 땡기더라구요 ㅋㅋㅋ",
