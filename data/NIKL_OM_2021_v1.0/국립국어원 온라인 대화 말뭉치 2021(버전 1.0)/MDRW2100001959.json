        },
        {
          "id": "MDRW2100001959.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001959.1.3",
          "form": "혹시 좋아하는 책 있으세요?",
          "original_form": "혹시 좋아하는 책 있으세요?",
          "speaker_id": "1",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001959.1.4",
          "form": "ㅠ책과는 거리가 멀어서요 추리소설은 좋아했어요",
          "original_form": "ㅠ책과는 거리가 멀어서요 추리소설은 좋아했어요",
          "speaker_id": "2",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100001959.1.5",
          "form": "아가사 크리스티",
          "original_form": "아가사 크리스티",
          "speaker_id": "2",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100001959.1.6",
          "form": "사실 저도 그래요...ㅎㅎ",
          "original_form": "사실 저도 그래요...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001959.1.7",
          "form": "요새 책보다는 웹툰 웹소설로 읽어서요ㅋㅋ",
          "original_form": "요새 책보다는 웹툰 웹소설로 읽어서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001959.1.8",
          "form": "아가사 크리스티는 그리고아무도없었다 재밋게 읽었엇어요",
          "original_form": "아가사 크리스티는 그리고아무도없었다 재밋게 읽었엇어요",
          "speaker_id": "2",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001959.1.9",
          "form": "아 저 무한도전에서 특집으로 한게 기억이 나네요",
          "original_form": "아 저 무한도전에서 특집으로 한게 기억이 나네요",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001959.1.10",
          "form": "웹툰 뭐보세요?",
          "original_form": "웹툰 뭐보세요?",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001959.1.11",
          "form": "윌유메리미, 뽀짜툰 등 엄청 많이봐요",
          "original_form": "윌유메리미, 뽀짜툰 등 엄청 많이봐요",
          "speaker_id": "2",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001959.1.12",
          "form": "만화책은 안보세요?",
          "original_form": "만화책은 안보세요?",
          "speaker_id": "1",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001959.1.13",
          "form": "전 요즘 만화책 사서 보고있는데 재밌더라구요",
          "original_form": "전 요즘 만화책 사서 보고있는데 재밌더라구요",
          "speaker_id": "1",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001959.1.14",
          "form": "색다르고",
          "original_form": "색다르고",
          "speaker_id": "1",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001959.1.15",
          "form": "웹툰 보고나서는 만화책은 잘 안읽게되더라구요",
          "original_form": "웹툰 보고나서는 만화책은 잘 안읽게되더라구요",
          "speaker_id": "2",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001959.1.16",
          "form": "가끔 만화방가서 볼때도있는데 코로나라서 잘못가고있어요",
          "original_form": "가끔 만화방가서 볼때도있는데 코로나라서 잘못가고있어요",
          "speaker_id": "2",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001959.1.17",
          "form": "저도 만화방 가보고 싶은데",
          "original_form": "저도 만화방 가보고 싶은데",
          "speaker_id": "1",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001959.1.18",
          "form": "코로나도 그렇고 친구가 거기서 벌쏘였다고해서",
          "original_form": "코로나도 그렇고 친구가 거기서 벌쏘였다고해서",
          "speaker_id": "1",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001959.1.19",
          "form": "그냥 집에서 사서 봐요..ㅎㅎ",
          "original_form": "그냥 집에서 사서 봐요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001959.1.20",
          "form": "헐 벌;; 무섭네요 사서보는게 제일편하죠",
          "original_form": "헐 벌;; 무섭네요 사서보는게 제일편하죠",
          "speaker_id": "2",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100001959.1.21",
          "form": "네 ㅎㅎ 집에서 보는게 편한 것 같아요",
          "original_form": "네 ㅎㅎ 집에서 보는게 편한 것 같아요",
