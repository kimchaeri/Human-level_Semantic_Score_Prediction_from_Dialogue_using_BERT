      "utterance": [
        {
          "id": "MDRW2100047527.1.1",
          "form": "다이어트 해보셨나요 ?",
          "original_form": "다이어트 해보셨나요 ?",
          "speaker_id": "1",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100047527.1.2",
          "form": "한번도 없어요ㅋㅋ",
          "original_form": "한번도 없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100047527.1.3",
          "form": "헐 진짜요? 살이 안찌시나봐요 ㅠㅠ",
          "original_form": "헐 진짜요? 살이 안찌시나봐요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100047527.1.4",
          "form": "아니요 그냥 건강만 하자라는 주의라서요",
          "original_form": "아니요 그냥 건강만 하자라는 주의라서요",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100047527.1.5",
          "form": "아아 ㅎㅎ 맞아요",
          "original_form": "아아 ㅎㅎ 맞아요",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100047527.1.6",
          "form": "그냥 건강한 몸으로만 유지하면 되는거같아요 ㅋㅋ",
          "original_form": "그냥 건강한 몸으로만 유지하면 되는거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100047527.1.7",
          "form": "ㅋㅋ 그냥 제기능만 하면됩니다",
          "original_form": "ㅋㅋ 그냥 제기능만 하면됩니다",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100047527.1.8",
          "form": "그럼 운동도 평소에 안하시나요??",
          "original_form": "그럼 운동도 평소에 안하시나요??",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100047527.1.9",
          "form": "요가해요?",
          "original_form": "요가해요?",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100047527.1.10",
          "form": "아니요 ! 근데 해보고싶어요 ㅎㅎ",
          "original_form": "아니요 ! 근데 해보고싶어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100047527.1.11",
          "form": "어떤 운동하세요?",
          "original_form": "어떤 운동하세요?",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100047527.1.12",
          "form": "전 보통 자전거 타요 ㅎㅎ !",
          "original_form": "전 보통 자전거 타요 ㅎㅎ !",
          "speaker_id": "1",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100047527.1.13",
          "form": "저도 주로 자전거타요",
          "original_form": "저도 주로 자전거타요",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100047527.1.14",
          "form": "허벅지 튼튼해지더라고요",
          "original_form": "허벅지 튼튼해지더라고요",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100047527.1.15",
          "form": "맞아요 ㅋㅋ 다리 운동으로 최곤거같아요 !",
          "original_form": "맞아요 ㅋㅋ 다리 운동으로 최곤거같아요 !",
          "speaker_id": "1",
          "time": "20210826 19:33"
        }
