          "speaker_id": "2",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005556.1.4",
          "form": "나 전에 말한 그 남자",
          "original_form": "나 전에 말한 그 남자",
          "speaker_id": "2",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005556.1.5",
          "form": "요즘 가게로 종종 빵 사러 옴",
          "original_form": "요즘 가게로 종종 빵 사러 옴",
          "speaker_id": "2",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005556.1.6",
          "form": "헌",
          "original_form": "헌",
          "speaker_id": "1",
          "time": "20210821 23:04"
