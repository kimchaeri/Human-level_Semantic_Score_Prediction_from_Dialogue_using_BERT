      "utterance": [
        {
          "id": "MDRW2100036225.1.1",
          "form": "피티 다니신지 얼마나 되셨어요?",
          "original_form": "피티 다니신지 얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210830 19:27"
        },
        {
          "id": "MDRW2100036225.1.2",
          "form": "6달 다니고 지금은 코로나 때문에 쉬고 있어요",
          "original_form": "6달 다니고 지금은 코로나 때문에 쉬고 있어요",
          "speaker_id": "2",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100036225.1.3",
          "form": "대단하십니다",
          "original_form": "대단하십니다",
          "speaker_id": "1",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100036225.1.4",
          "form": "코로나가 문제네요 증말",
          "original_form": "코로나가 문제네요 증말",
          "speaker_id": "1",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100036225.1.5",
          "form": "1대1이신가요?",
          "original_form": "1대1이신가요?",
          "speaker_id": "1",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100036225.1.6",
          "form": "네 그렇게 했어요!",
          "original_form": "네 그렇게 했어요!",
          "speaker_id": "2",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100036225.1.7",
          "form": "식단도 체크해주나용?",
          "original_form": "식단도 체크해주나용?",
          "speaker_id": "1",
          "time": "20210830 19:37"
        },
        {
          "id": "MDRW2100036225.1.8",
          "form": "매끼 사진 찍어 보내고 그래야하는데 저는 식단 안하겠다고해서 안 그랬어요!",
          "original_form": "매끼 사진 찍어 보내고 그래야하는데 저는 식단 안하겠다고해서 안 그랬어요!",
          "speaker_id": "2",
          "time": "20210830 19:39"
        },
        {
          "id": "MDRW2100036225.1.9",
          "form": "아항! 안해도 되는거군요ㅡ!",
          "original_form": "아항! 안해도 되는거군요ㅡ!",
          "speaker_id": "1",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100036225.1.10",
          "form": "무조건인줄 알았어요ㅋㅋ",
          "original_form": "무조건인줄 알았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100036225.1.11",
          "form": "친구가 체중 늘린다고 식단 찍어서 보내라고 했는데",
          "original_form": "친구가 체중 늘린다고 식단 찍어서 보내라고 했는데",
          "speaker_id": "1",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100036225.1.12",
          "form": "스트레스 엄청 받아했거든요",
          "original_form": "스트레스 엄청 받아했거든요",
          "speaker_id": "1",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100036225.1.13",
          "form": "체중 늘리시는 분은 의무적으로 보내셔야 하는게 맞을거에요",
          "original_form": "체중 늘리시는 분은 의무적으로 보내셔야 하는게 맞을거에요",
          "speaker_id": "2",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100036225.1.14",
          "form": "체중 늘리는 건 운동으로 안되니까..",
          "original_form": "체중 늘리는 건 운동으로 안되니까..",
          "speaker_id": "2",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100036225.1.15",
          "form": "아ㅠㅜ 그래서 그런거군요..",
          "original_form": "아ㅠㅜ 그래서 그런거군요..",
          "speaker_id": "1",
          "time": "20210830 19:48"
        },
        {
          "id": "MDRW2100036225.1.16",
          "form": "보면서 다른 고충이 있구나 했던 기억이 있네요",
          "original_form": "보면서 다른 고충이 있구나 했던 기억이 있네요",
          "speaker_id": "1",
          "time": "20210830 19:48"
        },
        {
          "id": "MDRW2100036225.1.17",
          "form": "그 분도 벌크업 하시느라 고생하셨네요ㅠ",
          "original_form": "그 분도 벌크업 하시느라 고생하셨네요ㅠ",
          "speaker_id": "2",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100036225.1.18",
          "form": "네ㅜㅠ 그렇더라구요",
          "original_form": "네ㅜㅠ 그렇더라구요",
          "speaker_id": "1",
          "time": "20210830 19:58"
        },
        {
          "id": "MDRW2100036225.1.19",
          "form": "피티말고 다른거 하신거 있으세요?",
          "original_form": "피티말고 다른거 하신거 있으세요?",
          "speaker_id": "1",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100036225.1.20",
          "form": "수영했었어요",
          "original_form": "수영했었어요",
          "speaker_id": "2",
          "time": "20210830 20:02"
        }
      ]
