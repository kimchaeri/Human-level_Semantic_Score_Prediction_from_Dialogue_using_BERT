        },
        {
          "id": "MDRW2100037892.1.4",
          "form": "나 아이슬란드!",
          "original_form": "나 아이슬란드!",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037892.1.5",
          "form": "오로라 보고시퍼",
          "original_form": "오로라 보고시퍼",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037892.1.6",
          "form": "김밥으로 가자!",
          "original_form": "김밥으로 가자!",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037892.1.7",
          "form": "나도 오로라",
          "original_form": "나도 오로라",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037892.1.8",
          "form": "김밥으로 가서",
          "original_form": "김밥으로 가서",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037892.1.9",
          "form": "굴러다니자(?)",
          "original_form": "굴러다니자(?)",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037892.1.10",
          "form": "김밥이 뭐야?",
          "original_form": "김밥이 뭐야?",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037892.1.11",
          "form": "꽁꽁 싸맨다는 건가",
          "original_form": "꽁꽁 싸맨다는 건가",
          "speaker_id": "1",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100037892.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100037892.1.13",
          "form": "롱패딩 입으면",
          "original_form": "롱패딩 입으면",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100037892.1.14",
          "form": "모자까지 쓰면",
          "original_form": "모자까지 쓰면",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100037892.1.15",
          "form": "김밥이잖어",
          "original_form": "김밥이잖어",
          "speaker_id": "2",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100037892.1.16",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100037892.1.17",
          "form": "좋아좋아",
          "original_form": "좋아좋아",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100037892.1.18",
          "form": "오로라 보면서 코코아 마시기",
          "original_form": "오로라 보면서 코코아 마시기",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100037892.1.19",
          "form": "와 완전 좋을 듯",
          "original_form": "와 완전 좋을 듯",
          "speaker_id": "2",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100037892.1.20",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100037892.1.21",
          "form": "카메라 챙겨가서",
          "original_form": "카메라 챙겨가서",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100037892.1.22",
          "form": "별 움직이는 것도 찍을 수 있으면 좋겠다",
          "original_form": "별 움직이는 것도 찍을 수 있으면 좋겠다",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100037892.1.23",
          "form": "영어를 열심히 해야겠어...",
          "original_form": "영어를 열심히 해야겠어...",
          "speaker_id": "1",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100037892.1.24",
          "form": "영어 해야하는데",
          "original_form": "영어 해야하는데",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100037892.1.25",
          "form": "바디랭귀지도 뭐 상관 없지ㅋㅋ",
          "original_form": "바디랭귀지도 뭐 상관 없지ㅋㅋ",
