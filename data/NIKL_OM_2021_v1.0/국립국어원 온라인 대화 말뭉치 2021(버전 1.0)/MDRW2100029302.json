      "utterance": [
        {
          "id": "MDRW2100029302.1.1",
          "form": "ㅋㄱㅋㅋㄱㅋㅋㅋㅋㅋ인피니트알아요ㅠ",
          "original_form": "ㅋㄱㅋㅋㄱㅋㅋㅋㅋㅋ인피니트알아요ㅠ",
          "speaker_id": "2",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029302.1.2",
          "form": "왜 잊혀진그룹처럼 말하세요ㅠ",
          "original_form": "왜 잊혀진그룹처럼 말하세요ㅠ",
          "speaker_id": "2",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029302.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ 인피니트....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ 인피니트....",
          "speaker_id": "1",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029302.1.4",
          "form": "예전에 롯데리아에",
          "original_form": "예전에 롯데리아에",
          "speaker_id": "1",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029302.1.5",
          "form": "인피니트팩 팔고",
          "original_form": "인피니트팩 팔고",
          "speaker_id": "1",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029302.1.6",
          "form": "그랬었는데",
          "original_form": "그랬었는데",
          "speaker_id": "1",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029302.1.7",
          "form": "ㅋㄱㅋㅋㅋ롯데리아에서 본것같아요",
          "original_form": "ㅋㄱㅋㅋㅋ롯데리아에서 본것같아요",
          "speaker_id": "2",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029302.1.8",
          "form": "그때가 펩시 광고 때라",
          "original_form": "그때가 펩시 광고 때라",
          "speaker_id": "1",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100029302.1.9",
          "form": "근데 사실 저는 롯데리아 불고기 최애라",
          "original_form": "근데 사실 저는 롯데리아 불고기 최애라",
          "speaker_id": "1",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100029302.1.10",
          "form": "안 먹었었어요 ㅠㅠ",
          "original_form": "안 먹었었어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100029302.1.11",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100029302.1.12",
          "form": "저는 롯리 새우버거가",
          "original_form": "저는 롯리 새우버거가",
          "speaker_id": "2",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100029302.1.13",
          "form": "차애에요",
          "original_form": "차애에요",
          "speaker_id": "2",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100029302.1.14",
          "form": "오 최애는 불고기신가요?!",
          "original_form": "오 최애는 불고기신가요?!",
          "speaker_id": "1",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100029302.1.15",
          "form": "네 최애는 맥날불고기에요",
          "original_form": "네 최애는 맥날불고기에요",
          "speaker_id": "2",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100029302.1.16",
          "form": "오 맥날!",
          "original_form": "오 맥날!",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029302.1.17",
          "form": "맥날 차애는 무엇인가요",
          "original_form": "맥날 차애는 무엇인가요",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029302.1.18",
          "form": "맥날차에는 1955!",
          "original_form": "맥날차에는 1955!",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029302.1.19",
          "form": "1955~~~ 제 친구 최애네용",
          "original_form": "1955~~~ 제 친구 최애네용",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029302.1.20",
          "form": "저는 맥치킨 모짜렐라입니당",
          "original_form": "저는 맥치킨 모짜렐라입니당",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029302.1.21",
          "form": "아 그것도맛있죠!",
          "original_form": "아 그것도맛있죠!",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029302.1.22",
          "form": "맥날오늘갔는데",
          "original_form": "맥날오늘갔는데",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029302.1.23",
          "form": "아니어제",
          "original_form": "아니어제",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029302.1.24",
          "form": "또맥윙없어졌더라고요",
          "original_form": "또맥윙없어졌더라고요",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029302.1.25",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029302.1.26",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029302.1.27",
          "form": "맥윙",
          "original_form": "맥윙",
          "speaker_id": "1",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029302.1.28",
          "form": "왜그러는걸까요진짜...",
          "original_form": "왜그러는걸까요진짜...",
          "speaker_id": "1",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029302.1.29",
          "form": "돈벌줄모르는것같아요 진짜",
          "original_form": "돈벌줄모르는것같아요 진짜",
