      "utterance": [
        {
          "id": "MDRW2100032195.1.1",
          "form": "음식은 뭐 좋아하세요",
          "original_form": "음식은 뭐 좋아하세요",
          "speaker_id": "2",
          "time": "20210905 14:09"
        },
        {
          "id": "MDRW2100032195.1.2",
          "form": "고기요 ㅋㅋㅋㄱ",
          "original_form": "고기요 ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100032195.1.3",
          "form": "소불고기나 치킨 제육.. 다좋아해요 ㅎㅎ",
          "original_form": "소불고기나 치킨 제육.. 다좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100032195.1.4",
          "form": "어떤 요리를 좋아하시나요?",
          "original_form": "어떤 요리를 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100032195.1.5",
          "form": "저는 회요ㅋㅋ",
          "original_form": "저는 회요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032195.1.6",
          "form": "오 회도 종류가 여러가지 일건데 참치회 연어회 방어회 광어회 민어회... 어떤 류를 좋아하시나요?",
          "original_form": "오 회도 종류가 여러가지 일건데 참치회 연어회 방어회 광어회 민어회... 어떤 류를 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 14:38"
        },
        {
          "id": "MDRW2100032195.1.7",
          "form": "제 1픽은 연어인데",
          "original_form": "제 1픽은 연어인데",
          "speaker_id": "2",
          "time": "20210905 14:38"
        },
        {
          "id": "MDRW2100032195.1.8",
          "form": "연어\n참치 방어\n나머지",
          "original_form": "연어\n참치 방어\n나머지",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100032195.1.9",
          "form": "같아요 ㅎㅎ",
          "original_form": "같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100032195.1.10",
          "form": "연어회 좋아하는 분이 많은거 같아요~",
          "original_form": "연어회 좋아하는 분이 많은거 같아요~",
          "speaker_id": "1",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100032195.1.11",
          "form": "저도 가끔 먹지만 왠지 참치보단 연어가 좋고 방어가 맛있더라구요ㅋㅋㅋㅋ",
          "original_form": "저도 가끔 먹지만 왠지 참치보단 연어가 좋고 방어가 맛있더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100032195.1.12",
          "form": "연어만 주면",
          "original_form": "연어만 주면",
          "speaker_id": "2",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032195.1.13",
          "form": "올드보이 가능입니다",
          "original_form": "올드보이 가능입니다",
          "speaker_id": "2",
          "time": "20210905 14:42"
        },
        {
          "id": "MDRW2100032195.1.14",
          "form": "엌ㅋㅋㅋㅋㅋㅋㅋㅋ 만두보단 연어죠ㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋㅋㅋㅋㅋㅋ 만두보단 연어죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100032195.1.15",
          "form": "잘하시는 요리가 있으신가요?",
          "original_form": "잘하시는 요리가 있으신가요?",
          "speaker_id": "1",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100032195.1.16",
          "form": "저 계란찜 잘해요",
          "original_form": "저 계란찜 잘해요",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032195.1.17",
          "form": "그 말랑말랑이",
          "original_form": "그 말랑말랑이",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032195.1.18",
          "form": "오와 그 부드럽게 푸딩같이 만드는가요?",
          "original_form": "오와 그 부드럽게 푸딩같이 만드는가요?",
          "speaker_id": "1",
          "time": "20210905 14:54"
        },
        {
          "id": "MDRW2100032195.1.19",
          "form": "네 ㅋㅋ 그거로 와이프 꼬셨죠",
          "original_form": "네 ㅋㅋ 그거로 와이프 꼬셨죠",
          "speaker_id": "2",
          "time": "20210905 14:54"
        },
        {
          "id": "MDRW2100032195.1.20",
          "form": "오ㅋㅋㅋㅋㅋㅋ 그럼 자주 해주시나요?",
          "original_form": "오ㅋㅋㅋㅋㅋㅋ 그럼 자주 해주시나요?",
          "speaker_id": "1",
