      "utterance": [
        {
          "id": "MDRW2100007332.1.1",
          "form": "조별과제 했고...",
          "original_form": "조별과제 했고...",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007332.1.2",
          "form": "이제 그럼 동아리?",
          "original_form": "이제 그럼 동아리?",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007332.1.3",
          "form": "동아리 해보까유?",
          "original_form": "동아리 해보까유?",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007332.1.4",
          "form": "동아리 좋아유",
          "original_form": "동아리 좋아유",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007332.1.5",
          "form": "나는 일년반정도",
          "original_form": "나는 일년반정도",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007332.1.6",
          "form": "대학에서 봉사동아리를 햇엇어",
          "original_form": "대학에서 봉사동아리를 햇엇어",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007332.1.7",
          "form": "아 진짜? 와",
          "original_form": "아 진짜? 와",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007332.1.8",
          "form": "아 그래서 affiliation 이런데",
          "original_form": "아 그래서 &affiliation& 이런데",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007332.1.9",
          "form": "다닌거야??",
          "original_form": "다닌거야??",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007332.1.10",
          "form": "좋은 동아리했다",
          "original_form": "좋은 동아리했다",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007332.1.11",
          "form": "멋지다",
          "original_form": "멋지다",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007332.1.12",
          "form": "affiliation 말고 affiliation라고",
          "original_form": "&affiliation& 말고 &affiliation&라고",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007332.1.13",
          "form": "보육원에서 애들 가르쳐주는 교육봉사햇당!",
          "original_form": "보육원에서 애들 가르쳐주는 교육봉사햇당!",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007332.1.14",
          "form": "너는 어떤 동아리를 햇엇니?",
          "original_form": "너는 어떤 동아리를 햇엇니?",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007332.1.15",
          "form": "알면서 형식상 물어본다^^ㅋㅋㅋㅋㅋ",
          "original_form": "알면서 형식상 물어본다^^ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007332.1.16",
          "form": "나아는... 대학때는 걍 교지편집부 했었지 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나아는... 대학때는 걍 교지편집부 했었지 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007332.1.17",
          "form": "고딩때도 했었으니까 뭐 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "고딩때도 했었으니까 뭐 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007332.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그러고보면",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그러고보면",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007332.1.19",
          "form": "대학에서는 근데 안에 사람들이랑 마음 잘맞고 친하고 그랬어서",
          "original_form": "대학에서는 근데 안에 사람들이랑 마음 잘맞고 친하고 그랬어서",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007332.1.20",
          "form": "너가 기사쓰고 이런거랑",
          "original_form": "너가 기사쓰고 이런거랑",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007332.1.21",
          "form": "재밌었어",
          "original_form": "재밌었어",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007332.1.22",
          "form": "연이 깊네",
          "original_form": "연이 깊네",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007332.1.23",
          "form": "그렇지...? 아마??ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그렇지...? 아마??ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007332.1.24",
          "form": "맞아 너",
          "original_form": "맞아 너",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007332.1.25",
          "form": "동아리 회장도 했으니까 ㅋㅋㅋㅋ",
          "original_form": "동아리 회장도 했으니까 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007332.1.26",
          "form": "선배들이랑 동아리 수련회가서",
          "original_form": "선배들이랑 동아리 수련회가서",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007332.1.27",
          "form": "사우나...찜질방... 그리고 온천은 아니고 그 비슷한거 있잖아 그거...",
          "original_form": "사우나...찜질방... 그리고 온천은 아니고 그 비슷한거 있잖아 그거...",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007332.1.28",
          "form": "그거를 같이 들어갔었지",
          "original_form": "그거를 같이 들어갔었지",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007332.1.29",
          "form": "냉탕 온탕 오가면서",
          "original_form": "냉탕 온탕 오가면서",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007332.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ친밀감 왕",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ친밀감 왕",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007332.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ아니",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아니",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007332.1.32",
          "form": "온천같은데도 같이 갔다고...??",
          "original_form": "온천같은데도 같이 갔다고...??",
