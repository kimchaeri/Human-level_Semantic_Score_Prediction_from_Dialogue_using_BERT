      "utterance": [
        {
          "id": "MDRW2100018432.1.1",
          "form": "무슨버거를 먹었을까요?!!!",
          "original_form": "무슨버거를 먹었을까요?!!!",
          "speaker_id": "1",
          "time": "20210827 01:43"
        },
        {
          "id": "MDRW2100018432.1.2",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210827 01:43"
        },
        {
          "id": "MDRW2100018432.1.3",
          "form": "버거킹 드셨다면서요",
          "original_form": "버거킹 드셨다면서요",
          "speaker_id": "2",
          "time": "20210827 01:43"
        },
        {
          "id": "MDRW2100018432.1.4",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018432.1.5",
          "form": "버거킹중에서요",
          "original_form": "버거킹중에서요",
          "speaker_id": "1",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018432.1.6",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018432.1.7",
          "form": "버거가많잖아요",
          "original_form": "버거가많잖아요",
          "speaker_id": "1",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018432.1.8",
          "form": "종류가",
          "original_form": "종류가",
          "speaker_id": "1",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018432.1.9",
          "form": "2번째 퀴즈랍니다",
          "original_form": "2번째 퀴즈랍니다",
          "speaker_id": "1",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018432.1.10",
          "form": "그런 디테일까지라니",
          "original_form": "그런 디테일까지라니",
          "speaker_id": "2",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018432.1.11",
          "form": "어렵군요",
          "original_form": "어렵군요",
          "speaker_id": "2",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018432.1.12",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:45"
        },
        {
          "id": "MDRW2100018432.1.13",
          "form": "자자 가보죠",
          "original_form": "자자 가보죠",
          "speaker_id": "1",
          "time": "20210827 01:45"
        },
        {
          "id": "MDRW2100018432.1.14",
          "form": "3번기회입니다",
          "original_form": "3번기회입니다",
          "speaker_id": "1",
          "time": "20210827 01:45"
        },
        {
