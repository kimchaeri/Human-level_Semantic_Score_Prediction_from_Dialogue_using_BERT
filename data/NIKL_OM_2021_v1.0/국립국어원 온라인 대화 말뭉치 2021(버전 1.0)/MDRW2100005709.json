      "utterance": [
        {
          "id": "MDRW2100005709.1.1",
          "form": "스포츠에 대한 지식이 없는 걸?",
          "original_form": "스포츠에 대한 지식이 없는 걸?",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005709.1.2",
          "form": "이번 올림픽 경기에도 진심이 아닌 사람 중 하나였단 말이야",
          "original_form": "이번 올림픽 경기에도 진심이 아닌 사람 중 하나였단 말이야",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005709.1.3",
          "form": "여자 배구도 그렇구",
          "original_form": "여자 배구도 그렇구",
          "speaker_id": "1",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005709.1.4",
          "form": "펜싱도 그렇고",
          "original_form": "펜싱도 그렇고",
          "speaker_id": "1",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005709.1.5",
          "form": "그래도 평창 올림픽 할때는 스키점프 직관했다",
          "original_form": "그래도 평창 올림픽 할때는 스키점프 직관했다",
          "speaker_id": "1",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005709.1.6",
          "form": "역시 강원피플",
          "original_form": "역시 강원피플",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005709.1.7",
          "form": "우리나라 선수는 없기는 했지만",
          "original_form": "우리나라 선수는 없기는 했지만",
          "speaker_id": "1",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005709.1.8",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005709.1.9",
          "form": "난 하는 걸 좋아해",
          "original_form": "난 하는 걸 좋아해",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005709.1.10",
          "form": "강원 피플이라 꽁자로 감",
          "original_form": "강원 피플이라 꽁자로 감",
          "speaker_id": "1",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005709.1.11",
          "form": "정확히는 내가 이기는 걸 좋아해",
          "original_form": "정확히는 내가 이기는 걸 좋아해",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005709.1.12",
          "form": "지면재미없어",
          "original_form": "지면재미없어",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005709.1.13",
          "form": "스포츠?",
          "original_form": "스포츠?",
          "speaker_id": "1",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005709.1.14",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005709.1.15",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005709.1.16",
          "form": "지는 스포츠는 재미없지",
          "original_form": "지는 스포츠는 재미없지",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005709.1.17",
          "form": "경기피플도 보고싶었는데",
          "original_form": "경기피플도 보고싶었는데",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005709.1.18",
          "form": "지는 걸 보는 것도 재미없어",
          "original_form": "지는 걸 보는 것도 재미없어",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005709.1.19",
          "form": "사실 우리 학교도 봉사지원 신청받았었어",
          "original_form": "사실 우리 학교도 봉사지원 신청받았었어",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005709.1.20",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005709.1.21",
          "form": "학교 째도 된대서 가려고했는데",
          "original_form": "학교 째도 된대서 가려고했는데",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005709.1.22",
          "form": "근데 왜 안갔어?",
          "original_form": "근데 왜 안갔어?",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005709.1.23",
          "form": "알아서 가야한다더라고",
          "original_form": "알아서 가야한다더라고",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005709.1.24",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005709.1.25",
          "form": "강원까지 어떻게 가겠어",
          "original_form": "강원까지 어떻게 가겠어",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005709.1.26",
          "form": "수원에서 평창까지?",
          "original_form": "수원에서 평창까지?",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005709.1.27",
          "form": "우리는 버스태워 줬다",
          "original_form": "우리는 버스태워 줬다",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005709.1.28",
          "form": "부럽지?",
          "original_form": "부럽지?",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005709.1.29",
          "form": "부럽냐구~!",
          "original_form": "부럽냐구~!",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005709.1.30",
          "form": "부럽..다",
          "original_form": "부럽..다",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005709.1.31",
          "form": "나도 외국인 친구를 만나고 싶었어",
          "original_form": "나도 외국인 친구를 만나고 싶었어",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005709.1.32",
          "form": "올림픽에 가서",
          "original_form": "올림픽에 가서",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005709.1.33",
          "form": "거기 가면 강원 피플과",
          "original_form": "거기 가면 강원 피플과",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005709.1.34",
          "form": "외국인과",
          "original_form": "외국인과",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005709.1.35",
          "form": "선수들 밖에 없었엉",
          "original_form": "선수들 밖에 없었엉",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005709.1.36",
          "form": "근데 체력이 나쁜 미지는",
          "original_form": "근데 체력이 나쁜 미지는",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005709.1.37",
          "form": "힘들지",
          "original_form": "힘들지",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005709.1.38",
          "form": "평창이면",
          "original_form": "평창이면",
          "speaker_id": "2",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005709.1.39",
          "form": "공주도 힘들었을걸?",
          "original_form": "공주도 힘들었을걸?",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005709.1.40",
          "form": "피겨했던 애하고",
          "original_form": "피겨했던 애하고",
          "speaker_id": "2",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100005709.1.41",
          "form": "통가남",
          "original_form": "통가남",
          "speaker_id": "2",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100005709.1.42",
          "form": "잘생긴 사람",
          "original_form": "잘생긴 사람",
          "speaker_id": "2",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100005709.1.43",
          "form": "피겨하는 남자 좋아",
          "original_form": "피겨하는 남자 좋아",
          "speaker_id": "2",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100005709.1.44",
          "form": "옹?",
          "original_form": "옹?",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100005709.1.45",
          "form": "피겨하는 남자는 못봐서",
          "original_form": "피겨하는 남자는 못봐서",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100005709.1.46",
          "form": "피겨하는 남자는 어떤데?",
          "original_form": "피겨하는 남자는 어떤데?",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100005709.1.47",
          "form": "피겨하는거 뭔가 본지 오래되었다",
          "original_form": "피겨하는거 뭔가 본지 오래되었다",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100005709.1.48",
          "form": "나는 발레하는 남자",
          "original_form": "나는 발레하는 남자",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005709.1.49",
          "form": "피겨하는 남자 좋아해",
          "original_form": "피겨하는 남자 좋아해",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005709.1.50",
          "form": "뭔가..... 탄탄한데 마른 느낌",
          "original_form": "뭔가..... 탄탄한데 마른 느낌",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005709.1.51",
          "form": "몸선도 이쁘고",
          "original_form": "몸선도 이쁘고",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005709.1.52",
          "form": "뭔가 선이 이쁜 느낌",
          "original_form": "뭔가 선이 이쁜 느낌",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005709.1.53",
          "form": "뭔지 알겠어",
          "original_form": "뭔지 알겠어",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005709.1.54",
          "form": "흠 근데 피겨는",
          "original_form": "흠 근데 피겨는",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005709.1.55",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100005709.1.56",
          "form": "점수 채점기준을 모르겠어",
          "original_form": "점수 채점기준을 모르겠어",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100005709.1.57",
          "form": "무지 대단하다",
          "original_form": "무지 대단하다",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100005709.1.58",
          "form": "까지밖에",
          "original_form": "까지밖에",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100005709.1.59",
          "form": "나는 그것도 그래",
          "original_form": "나는 그것도 그래",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100005709.1.60",
          "form": "그 뛰어서  착지하는",
          "original_form": "그 뛰어서  착지하는",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100005709.1.61",
          "form": "기계체조!",
          "original_form": "기계체조!",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005709.1.62",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005709.1.63",
          "form": "그것도 체점기준 모르겠어",
          "original_form": "그것도 체점기준 모르겠어",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005709.1.64",
          "form": "잘 착지만 하면 되는 건가 싶어",
          "original_form": "잘 착지만 하면 되는 건가 싶어",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
