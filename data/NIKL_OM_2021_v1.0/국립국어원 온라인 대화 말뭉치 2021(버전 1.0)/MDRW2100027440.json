      "utterance": [
        {
          "id": "MDRW2100027440.1.1",
          "form": "나는 머스타드소스",
          "original_form": "나는 머스타드소스",
          "speaker_id": "2",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100027440.1.2",
          "form": "에기저기 다 어울려",
          "original_form": "에기저기 다 어울려",
          "speaker_id": "2",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100027440.1.3",
          "form": "오리나 샌드위치",
          "original_form": "오리나 샌드위치",
          "speaker_id": "1",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100027440.1.4",
          "form": "와사비는 삼겹살에 어울려",
          "original_form": "와사비는 삼겹살에 어울려",
          "speaker_id": "1",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100027440.1.5",
          "form": "와사비는 코를 찔러",
          "original_form": "와사비는 코를 찔러",
          "speaker_id": "2",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100027440.1.6",
          "form": "그 매운맛이 매력이야",
          "original_form": "그 매운맛이 매력이야",
          "speaker_id": "2",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100027440.1.7",
          "form": "와사비가 있어야 회가 맛있지",
          "original_form": "와사비가 있어야 회가 맛있지",
          "speaker_id": "1",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100027440.1.8",
          "form": "초장에 비벼도 좋아",
          "original_form": "초장에 비벼도 좋아",
          "speaker_id": "2",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100027440.1.9",
          "form": "초장은 만능이야",
          "original_form": "초장은 만능이야",
          "speaker_id": "1",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100027440.1.10",
          "form": "비빔국수먹고싶다",
          "original_form": "비빔국수먹고싶다",
          "speaker_id": "2",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100027440.1.11",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100027440.1.12",
          "form": "새콤달콤해",
          "original_form": "새콤달콤해",
          "speaker_id": "1",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100027440.1.13",
          "form": "냉면도 좋고",
          "original_form": "냉면도 좋고",
          "speaker_id": "2",
          "time": "20210902 12:38"
        },
        {
          "id": "MDRW2100027440.1.14",
          "form": "냉면은 고기랑 먹지",
          "original_form": "냉면은 고기랑 먹지",
          "speaker_id": "1",
          "time": "20210902 12:38"
        },
        {
          "id": "MDRW2100027440.1.15",
          "form": "고쌈냉면",
          "original_form": "고쌈냉면",
          "speaker_id": "2",
          "time": "20210902 12:38"
        },
        {
          "id": "MDRW2100027440.1.16",
          "form": "비빔이 좋니",
          "original_form": "비빔이 좋니",
          "speaker_id": "1",
          "time": "20210902 12:38"
        },
        {
          "id": "MDRW2100027440.1.17",
          "form": "물도 좋고 비빔도 좋아",
          "original_form": "물도 좋고 비빔도 좋아",
          "speaker_id": "2",
          "time": "20210902 12:38"
        },
        {
          "id": "MDRW2100027440.1.18",
          "form": "뭐든 준다면 어케이",
          "original_form": "뭐든 준다면 어케이",
          "speaker_id": "1",
          "time": "20210902 12:39"
        },
        {
          "id": "MDRW2100027440.1.19",
          "form": "케이크먹고싶어",
          "original_form": "케이크먹고싶어",
          "speaker_id": "1",
