      "utterance": [
        {
          "id": "MDRW2100012656.1.1",
          "form": "버스타고 어떤 일터로 가시나여?",
          "original_form": "버스타고 어떤 일터로 가시나여?",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100012656.1.2",
          "form": "전 우체국이요!",
          "original_form": "전 우체국이요!",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012656.1.3",
          "form": "방학동안 아르바이트로 가요",
          "original_form": "방학동안 아르바이트로 가요",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012656.1.4",
          "form": "취직하셨나요?",
          "original_form": "취직하셨나요?",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012656.1.5",
          "form": "전 어른이라.. 취직은 옛날에.. ㅎㅎㅎ",
          "original_form": "전 어른이라.. 취직은 옛날에.. ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100012656.1.6",
          "form": "아 그거 정부에서하는 알자 그거여?",
          "original_form": "아 그거 정부에서하는 알자 그거여?",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100012656.1.7",
          "form": "알바",
          "original_form": "알바",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100012656.1.8",
          "form": "대충 그런거예요",
          "original_form": "대충 그런거예요",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100012656.1.9",
          "form": "어떤 직종에서 일하세요?",
          "original_form": "어떤 직종에서 일하세요?",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100012656.1.10",
          "form": "저도 학생때 우체국에서 그거 했었어요..ㅋㅋㅋ \n전 행사관련업종이엇는데.. 코로나로 다 파산햇어요",
          "original_form": "저도 학생때 우체국에서 그거 했었어요..ㅋㅋㅋ \n전 행사관련업종이엇는데.. 코로나로 다 파산햇어요",
          "speaker_id": "1",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012656.1.11",
          "form": "네에?! 그럼 회사가 없어진건가요?",
          "original_form": "네에?! 그럼 회사가 없어진건가요?",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012656.1.12",
          "form": "네..ㅎㅎ 그래서 백수에요..",
          "original_form": "네..ㅎㅎ 그래서 백수에요..",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100012656.1.13",
          "form": "장래 어떤일 하고싶은세여?",
          "original_form": "장래 어떤일 하고싶은세여?",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100012656.1.14",
          "form": "전 코딩 배워서 개발자하고싶어요",
          "original_form": "전 코딩 배워서 개발자하고싶어요",
          "speaker_id": "2",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100012656.1.15",
          "form": "전공은 다르지만요",
          "original_form": "전공은 다르지만요",
          "speaker_id": "2",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100012656.1.16",
          "form": "프로그래머는 영어만 잘라시면 될듯.. ㅎㅎ",
          "original_form": "프로그래머는 영어만 잘라시면 될듯.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012656.1.17",
          "form": "잘하",
          "original_form": "잘하",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012656.1.18",
          "form": "그럴까요 영어는 어느정도 할줄 아는데 코딩이 처음 배우려니 겁나고 어려워요",
          "original_form": "그럴까요 영어는 어느정도 할줄 아는데 코딩이 처음 배우려니 겁나고 어려워요",
          "speaker_id": "2",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012656.1.19",
          "form": "요즘은 모듈화가 잘되있어서..\n막히면 외국 커뮤니티에 물어보면 답 나온다고..",
          "original_form": "요즘은 모듈화가 잘되있어서..\n막히면 외국 커뮤니티에 물어보면 답 나온다고..",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100012656.1.20",
          "form": "그래서 영어를 잘해야한다고.. ㅋㅋ",
          "original_form": "그래서 영어를 잘해야한다고.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100012656.1.21",
          "form": "그건 그래요",
          "original_form": "그건 그래요",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012656.1.22",
          "form": "요즘 재택 알바 정부지원 일자리 많던데 해보신적 있나요?",
          "original_form": "요즘 재택 알바 정부지원 일자리 많던데 해보신적 있나요?",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100012656.1.23",
          "form": "빅데이터 4차산업혁명 뭐다 해서요",
          "original_form": "빅데이터 4차산업혁명 뭐다 해서요",
          "speaker_id": "2",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100012656.1.24",
          "form": "재택알바도 있어요? 첨들어봐요",
          "original_form": "재택알바도 있어요? 첨들어봐요",
          "speaker_id": "1",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100012656.1.25",
          "form": "뭐하는거에요?",
          "original_form": "뭐하는거에요?",
          "speaker_id": "1",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100012656.1.26",
          "form": "데이터 라벨링하거나 검수하거나 그런것들이 많아요",
          "original_form": "데이터 라벨링하거나 검수하거나 그런것들이 많아요",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100012656.1.27",
          "form": "정부에서 지원금 주는거라서 최저시급보다 많이 받을수 있는게 많던데요",
          "original_form": "정부에서 지원금 주는거라서 최저시급보다 많이 받을수 있는게 많던데요",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100012656.1.28",
          "form": "오.. 알아봐야겟네요.. 첨들었어요",
          "original_form": "오.. 알아봐야겟네요.. 첨들었어요",
          "speaker_id": "1",
          "time": "20210824 23:56"
        }
      ]
