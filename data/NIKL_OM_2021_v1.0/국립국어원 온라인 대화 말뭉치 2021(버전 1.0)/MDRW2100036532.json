      "utterance": [
        {
          "id": "MDRW2100036532.1.1",
          "form": "벼룩시장 말고",
          "original_form": "벼룩시장 말고",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036532.1.2",
          "form": "응 식재료를 공급하는 시장인데",
          "original_form": "응 식재료를 공급하는 시장인데",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036532.1.3",
          "form": "먹을거리 많겠다 그럼",
          "original_form": "먹을거리 많겠다 그럼",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036532.1.4",
          "form": "엉안그래도 관광객들이 많이 찾아온데",
          "original_form": "엉안그래도 관광객들이 많이 찾아온데",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036532.1.5",
          "form": "그렁 또 소매치기 조심",
          "original_form": "그렁 또 소매치기 조심",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036532.1.6",
          "form": "엉 전통시장이었는데 여기도 화재가 났었다네",
          "original_form": "엉 전통시장이었는데 여기도 화재가 났었다네",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036532.1.7",
          "form": "에궁 여기저기 화재네",
          "original_form": "에궁 여기저기 화재네",
          "speaker_id": "2",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036532.1.8",
          "form": "그러게 리모델링을 했다",
          "original_form": "그러게 리모델링을 했다",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036532.1.9",
          "form": "그럼 깔끔하겠다",
          "original_form": "그럼 깔끔하겠다",
          "speaker_id": "2",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036532.1.10",
          "form": "응 리모델링을 햇으니 깔금할듯",
          "original_form": "응 리모델링을 햇으니 깔금할듯",
          "speaker_id": "1",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100036532.1.11",
          "form": "여러가지  시장 응식들 먹을수 있겠다",
          "original_form": "여러가지  시장 응식들 먹을수 있겠다",
          "speaker_id": "2",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100036532.1.12",
          "form": "응 가격도 부담없겠고",
          "original_form": "응 가격도 부담없겠고",
          "speaker_id": "1",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100036532.1.13",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100036532.1.14",
          "form": "알무데나 대성당이라고 있데",
          "original_form": "알무데나 대성당이라고 있데",
