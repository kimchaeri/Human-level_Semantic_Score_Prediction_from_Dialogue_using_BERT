      "utterance": [
        {
          "id": "MDRW2100019724.1.1",
          "form": "종교가 있으십니까?",
          "original_form": "종교가 있으십니까?",
          "speaker_id": "1",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100019724.1.2",
          "form": "천주교에요",
          "original_form": "천주교에요",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100019724.1.3",
          "form": "저는 불교를 조금 믿어요",
          "original_form": "저는 불교를 조금 믿어요",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100019724.1.4",
          "form": "불교는 내용이 너무 좋은거같아요",
          "original_form": "불교는 내용이 너무 좋은거같아요",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100019724.1.5",
          "form": "목탁두드리는 소리 가끔 들으면 평온해지더라고요",
          "original_form": "목탁두드리는 소리 가끔 들으면 평온해지더라고요",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100019724.1.6",
          "form": "맞아요 마음이 안정되는소리에요",
          "original_form": "맞아요 마음이 안정되는소리에요",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100019724.1.7",
          "form": "옛날에 교회다녔었는데 너무 안맞아서 마음이 안가더라고요",
          "original_form": "옛날에 교회다녔었는데 너무 안맞아서 마음이 안가더라고요",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100019724.1.8",
          "form": "그러나 교황님은 멋있으신것 같아요",
          "original_form": "그러나 교황님은 멋있으신것 같아요",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100019724.1.9",
          "form": "맞아요 교회는 친구따라 선물받으러 몇번 갔었어요",
          "original_form": "맞아요 교회는 친구따라 선물받으러 몇번 갔었어요",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100019724.1.10",
          "form": "천주교는 교회랑 많이 다르죠?",
          "original_form": "천주교는 교회랑 많이 다르죠?",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100019724.1.11",
          "form": "다르다고 알고있어요. 분위기자체도 다르고..",
          "original_form": "다르다고 알고있어요. 분위기자체도 다르고..",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100019724.1.12",
          "form": "앗 믿는다고 하셔서 성당다니시는줄 알았어요",
          "original_form": "앗 믿는다고 하셔서 성당다니시는줄 알았어요",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100019724.1.13",
          "form": "냉담자에요 ㅎㅎ",
          "original_form": "냉담자에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100019724.1.14",
          "form": "성당은 안가지만 기도의힘은 믿고있어요",
          "original_form": "성당은 안가지만 기도의힘은 믿고있어요",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100019724.1.15",
          "form": "로또살때 절실해지는 기도",
          "original_form": "로또살때 절실해지는 기도",
          "speaker_id": "1",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100019724.1.16",
          "form": "절에는 자주 가시나요?",
          "original_form": "절에는 자주 가시나요?",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100019724.1.17",
          "form": "ㅋㅋ로또 내일이네요",
          "original_form": "ㅋㅋ로또 내일이네요",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100019724.1.18",
          "form": "앗 이제오늘이구나",
          "original_form": "앗 이제오늘이구나",
          "speaker_id": "2",
          "time": "20210828 00:00"
        }
      ]
