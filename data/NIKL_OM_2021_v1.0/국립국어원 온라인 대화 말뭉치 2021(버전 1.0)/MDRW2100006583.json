      "utterance": [
        {
          "id": "MDRW2100006583.1.1",
          "form": "뭐 온라인 커뮤 하는거 있으세요?",
          "original_form": "뭐 온라인 커뮤 하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006583.1.2",
          "form": "앙해요",
          "original_form": "앙해요",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006583.1.3",
          "form": "안해요",
          "original_form": "안해요",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006583.1.4",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006583.1.5",
          "form": "그것말고도",
          "original_form": "그것말고도",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006583.1.6",
          "form": "재미있늗게 많아서",
          "original_form": "재미있늗게 많아서",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006583.1.7",
          "form": "그건 맞아요",
          "original_form": "그건 맞아요",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006583.1.8",
          "form": "굳이 그걸 할 필요를 못 느껴요",
          "original_form": "굳이 그걸 할 필요를 못 느껴요",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006583.1.9",
          "form": "친구들이 계속 하자는데",
          "original_form": "친구들이 계속 하자는데",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006583.1.10",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006583.1.11",
          "form": "친구버려요",
          "original_form": "친구버려요",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006583.1.12",
          "form": "해봤자 뭘 안 올릴 것 같거든요 저는",
          "original_form": "해봤자 뭘 안 올릴 것 같거든요 저는",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006583.1.13",
          "form": "제친구는 인티해요",
          "original_form": "제친구는 인티해요",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006583.1.14",
          "form": "와중에 친구들이 인스타그램보면서",
          "original_form": "와중에 친구들이 인스타그램보면서",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100006583.1.15",
          "form": "남돌덕질하기 좋죠",
          "original_form": "남돌덕질하기 좋죠",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100006583.1.16",
          "form": "뭐 어디가자 뭐 먹자 하는데",
          "original_form": "뭐 어디가자 뭐 먹자 하는데",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100006583.1.17",
          "form": "네이트판 구경도 가끔흐고요 저는",
          "original_form": "네이트판 구경도 가끔흐고요 저는",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100006583.1.18",
          "form": "커뮤 하면",
          "original_form": "커뮤 하면",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100006583.1.19",
          "form": "완전 돈 줄줄 세던데",
          "original_form": "완전 돈 줄줄 세던데",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100006583.1.20",
          "form": "인스타도 좀 애매하죠",
          "original_form": "인스타도 좀 애매하죠",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100006583.1.21",
          "form": "아 가끔 친구가 재미있다면서",
          "original_form": "아 가끔 친구가 재미있다면서",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100006583.1.22",
          "form": "네이트판 얘기 던져줘요",
          "original_form": "네이트판 얘기 던져줘요",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100006583.1.23",
          "form": "장사꾼많잖아요",
          "original_form": "장사꾼많잖아요",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100006583.1.24",
          "form": "그것민 봐도 커뮤 다했죠",
          "original_form": "그것민 봐도 커뮤 다했죠",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100006583.1.25",
          "form": "자극적이고 재밌어요",
          "original_form": "자극적이고 재밌어요",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100006583.1.26",
          "form": "요즘은 다 짤오 나도니까",
          "original_form": "요즘은 다 짤오 나도니까",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100006583.1.27",
          "form": "굳이 할 필요 없죠",
          "original_form": "굳이 할 필요 없죠",
          "speaker_id": "1",
          "time": "20210908 02:50"
        }
      ]
    }
