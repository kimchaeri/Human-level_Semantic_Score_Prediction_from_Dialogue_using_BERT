      "utterance": [
        {
          "id": "MDRW2100003364.1.1",
          "form": "요즘 건강관리 하시고 계세요?",
          "original_form": "요즘 건강관리 하시고 계세요?",
          "speaker_id": "1",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003364.1.2",
          "form": "아니요ㅠㅠ해야 할것 같아요",
          "original_form": "아니요ㅠㅠ해야 할것 같아요",
          "speaker_id": "2",
          "time": "20210821 07:49"
        },
        {
          "id": "MDRW2100003364.1.3",
          "form": "치아도 임플란트 한후 좀 부실해서 관리 해야할것 같아요",
          "original_form": "치아도 임플란트 한후 좀 부실해서 관리 해야할것 같아요",
          "speaker_id": "2",
          "time": "20210821 09:19"
        },
        {
          "id": "MDRW2100003364.1.4",
          "form": "임플란트할때 안아프셨어요?",
          "original_form": "임플란트할때 안아프셨어요?",
          "speaker_id": "1",
          "time": "20210821 11:50"
        },
        {
          "id": "MDRW2100003364.1.5",
          "form": "아프죠 근데 의사쌤이 정말 잘하셔서",
          "original_form": "아프죠 근데 의사쌤이 정말 잘하셔서",
          "speaker_id": "2",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003364.1.6",
          "form": "잘넘어갔어요",
          "original_form": "잘넘어갔어요",
          "speaker_id": "2",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003364.1.7",
          "form": "근데 다들 고생하셨더라고요",
          "original_form": "근데 다들 고생하셨더라고요",
          "speaker_id": "2",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003364.1.8",
          "form": "엄청떨었는데 의사쌤을 잘만났어요",
          "original_form": "엄청떨었는데 의사쌤을 잘만났어요",
          "speaker_id": "2",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003364.1.9",
          "form": "마취주사 놓을땐 어떠셨어요?",
          "original_form": "마취주사 놓을땐 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210821 11:54"
        },
        {
          "id": "MDRW2100003364.1.10",
          "form": "헉 아파요",
          "original_form": "헉 아파요",
          "speaker_id": "2",
          "time": "20210821 11:55"
        },
        {
          "id": "MDRW2100003364.1.11",
          "form": "그게 젤아파요 저 마취안되서 엄청 많이 해요 금방풀려요",
          "original_form": "그게 젤아파요 저 마취안되서 엄청 많이 해요 금방풀려요",
          "speaker_id": "2",
          "time": "20210821 11:55"
        },
        {
          "id": "MDRW2100003364.1.12",
          "form": "어디 놓을때가 제일 아파요?",
          "original_form": "어디 놓을때가 제일 아파요?",
          "speaker_id": "1",
          "time": "20210821 12:02"
        },
        {
          "id": "MDRW2100003364.1.13",
          "form": "전 송곳니 사이가 약해요",
          "original_form": "전 송곳니 사이가 약해요",
          "speaker_id": "2",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003364.1.14",
          "form": "눈물나요 ㅜㅜ",
          "original_form": "눈물나요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003364.1.15",
          "form": "거기가 ㅜㅜ마취도 안돼요",
          "original_form": "거기가 ㅜㅜ마취도 안돼요",
          "speaker_id": "2",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003364.1.16",
          "form": "눈물날 정도에요? 다음주에 사랑니 빼는데 무섭네요...",
          "original_form": "눈물날 정도에요? 다음주에 사랑니 빼는데 무섭네요...",
          "speaker_id": "1",
          "time": "20210821 12:06"
        },
        {
          "id": "MDRW2100003364.1.17",
          "form": "사랑니 저 진짜",
          "original_form": "사랑니 저 진짜",
          "speaker_id": "2",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003364.1.18",
          "form": "저세상을 봤어요",
          "original_form": "저세상을 봤어요",
          "speaker_id": "2",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003364.1.19",
          "form": "울면서 전화해서ㅠ죽겠다고 진상짓했어요 뽑고 너무 아파요",
          "original_form": "울면서 전화해서ㅠ죽겠다고 진상짓했어요 뽑고 너무 아파요",
          "speaker_id": "2",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003364.1.20",
          "form": "근데 병원 옮기니 몇달전 뽑았는데 별로 안아팠어요",
          "original_form": "근데 병원 옮기니 몇달전 뽑았는데 별로 안아팠어요",
          "speaker_id": "2",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003364.1.21",
          "form": "저 매몰인데도 역시 선생님 따라 달라요",
          "original_form": "저 매몰인데도 역시 선생님 따라 달라요",
          "speaker_id": "2",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003364.1.22",
          "form": "넘 걱정마세요 약을 잘 먹음 안아파요 아프기정에 약을 언능 먹어야해요",
          "original_form": "넘 걱정마세요 약을 잘 먹음 안아파요 아프기정에 약을 언능 먹어야해요",
          "speaker_id": "2",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003364.1.23",
          "form": "뽑을때가 아파요 그 이후가 아파요?",
          "original_form": "뽑을때가 아파요 그 이후가 아파요?",
          "speaker_id": "1",
          "time": "20210821 12:16"
        },
        {
          "id": "MDRW2100003364.1.24",
          "form": "전 둘다 아프더라구요 ㅋㅋ근데 얼마전 한건 약 바로 먹으니 안아팠어요",
          "original_form": "전 둘다 아프더라구요 ㅋㅋ근데 얼마전 한건 약 바로 먹으니 안아팠어요",
          "speaker_id": "2",
          "time": "20210821 12:25"
        },
        {
          "id": "MDRW2100003364.1.25",
          "form": "저는 수평매복이라던데",
          "original_form": "저는 수평매복이라던데",
          "speaker_id": "1",
          "time": "20210821 12:29"
        },
        {
          "id": "MDRW2100003364.1.26",
          "form": "대학병원 가서 빼야한다더라구요",
          "original_form": "대학병원 가서 빼야한다더라구요",
          "speaker_id": "1",
          "time": "20210821 12:29"
        },
        {
          "id": "MDRW2100003364.1.27",
          "form": "네 아무나 못빼요 제가 그거예요 그러고 20년을 누워있는데 이제뺐네요",
          "original_form": "네 아무나 못빼요 제가 그거예요 그러고 20년을 누워있는데 이제뺐네요",
          "speaker_id": "2",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003364.1.28",
          "form": "옆으로 이가 나서 나오는데 20년 걸렸어요",
          "original_form": "옆으로 이가 나서 나오는데 20년 걸렸어요",
          "speaker_id": "2",
          "time": "20210821 12:58"
        }
      ]
