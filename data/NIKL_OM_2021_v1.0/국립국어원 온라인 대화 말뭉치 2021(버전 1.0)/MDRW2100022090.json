      "utterance": [
        {
          "id": "MDRW2100022090.1.1",
          "form": "가사 가족이군요ㅎㅎ",
          "original_form": "가사 가족이군요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100022090.1.2",
          "form": "그낭 하고 싶은 말 하시라구요 ㅎㅎ",
          "original_form": "그낭 하고 싶은 말 하시라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100022090.1.3",
          "form": "그래야 마음에 병도 안생겨요",
          "original_form": "그래야 마음에 병도 안생겨요",
          "speaker_id": "1",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100022090.1.4",
          "form": "아아ㅎㅎㅎㅎ",
          "original_form": "아아ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100022090.1.5",
          "form": "이젠뭐 병도 없어요ㅎㅎ",
          "original_form": "이젠뭐 병도 없어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100022090.1.6",
          "form": "포기를해서ㅎㅎ",
          "original_form": "포기를해서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100022090.1.7",
          "form": "이야기하면 길어져요ㅎ 너무너무ㅎㅎ",
          "original_form": "이야기하면 길어져요ㅎ 너무너무ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100022090.1.8",
          "form": "3년동안 살아왔던이야기라ㅎㅎ",
          "original_form": "3년동안 살아왔던이야기라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100022090.1.9",
          "form": "아고",
          "original_form": "아고",
          "speaker_id": "1",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100022090.1.10",
          "form": "ㅎㅎㅎ 너무 걱정마요ㅎ",
          "original_form": "ㅎㅎㅎ 너무 걱정마요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100022090.1.11",
          "form": "오히려 행복해요ㅎ",
          "original_form": "오히려 행복해요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100022090.1.12",
          "form": "그럼 다행이구요",
          "original_form": "그럼 다행이구요",
          "speaker_id": "1",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100022090.1.13",
          "form": "다 그런거 아니겟어요?ㅎ 인생이라는게 내마음대로 가면 인생이 아니잖아요ㅎ",
          "original_form": "다 그런거 아니겟어요?ㅎ 인생이라는게 내마음대로 가면 인생이 아니잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:44"
        },
        {
          "id": "MDRW2100022090.1.14",
          "form": "앞르로 꽃길만 걸음 되죠 ㅎㅎ",
          "original_form": "앞르로 꽃길만 걸음 되죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 02:44"
        },
        {
          "id": "MDRW2100022090.1.15",
          "form": "처음에는 절때 이혼은 안된다 생각햇는데ㅎ 지금와서 생각해보면",
          "original_form": "처음에는 절때 이혼은 안된다 생각햇는데ㅎ 지금와서 생각해보면",
          "speaker_id": "2",
          "time": "20210829 02:44"
        },
        {
          "id": "MDRW2100022090.1.16",
          "form": "왜그랬을까 생각하게되더라구요ㄹ",
          "original_form": "왜그랬을까 생각하게되더라구요ㄹ",
          "speaker_id": "2",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100022090.1.17",
          "form": "가족이니까 사랑하던 사람이니까",
          "original_form": "가족이니까 사랑하던 사람이니까",
          "speaker_id": "1",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100022090.1.18",
          "form": "감사해요ㅎ 그래도 위로해줄 사람이 한명은 있으니 다행이네요ㅎ",
          "original_form": "감사해요ㅎ 그래도 위로해줄 사람이 한명은 있으니 다행이네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100022090.1.19",
          "form": "아이 엄마니까  더 잡고 싶으셨을거에요",
          "original_form": "아이 엄마니까  더 잡고 싶으셨을거에요",
          "speaker_id": "1",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100022090.1.20",
          "form": "그렇죠ㅎ",
          "original_form": "그렇죠ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100022090.1.21",
          "form": "사랑했던사람인데 그랬는데ㅎ",
          "original_form": "사랑했던사람인데 그랬는데ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100022090.1.22",
          "form": "본인도 스트레스였을꺼에요ㅎ",
          "original_form": "본인도 스트레스였을꺼에요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100022090.1.23",
          "form": "육아도 힘든데 나한테도 스트레스받으니",
          "original_form": "육아도 힘든데 나한테도 스트레스받으니",
          "speaker_id": "2",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100022090.1.24",
          "form": "음 이유는 과소비 인거에요?",
          "original_form": "음 이유는 과소비 인거에요?",
          "speaker_id": "1",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100022090.1.25",
          "form": "딱 꼽으라면",
          "original_form": "딱 꼽으라면",
          "speaker_id": "1",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100022090.1.26",
          "form": "서로을 위해 헤어지는게 맞아요ㅎ 아이한테는 미안하지만ㅎ 아이는 엄마가 잘 돌봐주니깐 걱정없어요ㄹ",
          "original_form": "서로을 위해 헤어지는게 맞아요ㅎ 아이한테는 미안하지만ㅎ 아이는 엄마가 잘 돌봐주니깐 걱정없어요ㄹ",
          "speaker_id": "2",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100022090.1.27",
          "form": "아니요ㅎ",
          "original_form": "아니요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100022090.1.28",
          "form": "음.. 다말해드릴까요?",
          "original_form": "음.. 다말해드릴까요?",
          "speaker_id": "2",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100022090.1.29",
          "form": "name1님 주무셔야하는뎅ㅎㅎㅎ",
          "original_form": "&name1&님 주무셔야하는뎅ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100022090.1.30",
          "form": "제 의견보단 ㅎㅎ",
          "original_form": "제 의견보단 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100022090.1.31",
          "form": "name2님 마음이지요~~",
          "original_form": "&name2&님 마음이지요~~",
          "speaker_id": "1",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100022090.1.32",
          "form": "음 간략하게 말하자면ㅎ",
          "original_form": "음 간략하게 말하자면ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100022090.1.33",
          "form": "일단 제가 돈돈거리는게 싫어했고",
          "original_form": "일단 제가 돈돈거리는게 싫어했고",
          "speaker_id": "2",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100022090.1.34",
          "form": "저늠 성격이 빨리빨리하는 성격이고 아내는 신중한 성격ㅇ이에요",
          "original_form": "저늠 성격이 빨리빨리하는 성격이고 아내는 신중한 성격ㅇ이에요",
          "speaker_id": "2",
          "time": "20210829 02:49"
        },
        {
          "id": "MDRW2100022090.1.35",
          "form": "네넴",
          "original_form": "네넴",
          "speaker_id": "1",
          "time": "20210829 02:49"
        },
        {
          "id": "MDRW2100022090.1.36",
          "form": "그러다보니 의사결정할때 자꾸 틀어졌어요ㅎ",
          "original_form": "그러다보니 의사결정할때 자꾸 틀어졌어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:49"
        },
        {
          "id": "MDRW2100022090.1.37",
          "form": "그리고 육아에대해서도 아내말은 곧 법이어서",
          "original_form": "그리고 육아에대해서도 아내말은 곧 법이어서",
          "speaker_id": "2",
          "time": "20210829 02:49"
        },
        {
          "id": "MDRW2100022090.1.38",
          "form": "제가 끼어들면 곧싸움으로 일어났구요",
          "original_form": "제가 끼어들면 곧싸움으로 일어났구요",
          "speaker_id": "2",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100022090.1.39",
          "form": "아내는 또 결벽증이있어요ㅎ 손씻는거 청소하는거",
          "original_form": "아내는 또 결벽증이있어요ㅎ 손씻는거 청소하는거",
          "speaker_id": "2",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100022090.1.40",
          "form": "복합적이네요",
          "original_form": "복합적이네요",
          "speaker_id": "1",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100022090.1.41",
          "form": "엄청 예민해서 제가 다 치우다보니깐 거의 매일 청소했던것같애요 지금도 3일?일주일?",
          "original_form": "엄청 예민해서 제가 다 치우다보니깐 거의 매일 청소했던것같애요 지금도 3일?일주일?",
          "speaker_id": "2",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100022090.1.42",
          "form": "뭐이런식으로 하다보니 저도 스트레스고 아내도 스트레스고",
          "original_form": "뭐이런식으로 하다보니 저도 스트레스고 아내도 스트레스고",
          "speaker_id": "2",
          "time": "20210829 02:51"
        },
        {
          "id": "MDRW2100022090.1.43",
          "form": "그리고 식사문제도 달라요ㅎ 저한테 밥 차려준게 10번 안되요ㅎ 다 시켜먹거나 아니면 라면끓여주고",
          "original_form": "그리고 식사문제도 달라요ㅎ 저한테 밥 차려준게 10번 안되요ㅎ 다 시켜먹거나 아니면 라면끓여주고",
          "speaker_id": "2",
          "time": "20210829 02:52"
        },
        {
          "id": "MDRW2100022090.1.44",
          "form": "에공  ㅠㅠ",
          "original_form": "에공  ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 02:52"
        },
        {
          "id": "MDRW2100022090.1.45",
          "form": "아이에게도 돌지나서 이유식 안하고 배달시켰어서 저는 아빠입장에서 조금 화가 많이나요",
          "original_form": "아이에게도 돌지나서 이유식 안하고 배달시켰어서 저는 아빠입장에서 조금 화가 많이나요",
          "speaker_id": "2",
          "time": "20210829 02:52"
        },
        {
          "id": "MDRW2100022090.1.46",
          "form": "저한테안해줘도 되는데 아이한테까지는 안그래야하는데",
          "original_form": "저한테안해줘도 되는데 아이한테까지는 안그래야하는데",
          "speaker_id": "2",
          "time": "20210829 02:53"
        },
        {
          "id": "MDRW2100022090.1.47",
          "form": "이건 제 주관적인 거라ㅎㅎ",
          "original_form": "이건 제 주관적인 거라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:53"
        },
        {
          "id": "MDRW2100022090.1.48",
          "form": "또 사댁에 대한 관점이 다른것같애요",
          "original_form": "또 사댁에 대한 관점이 다른것같애요",
          "speaker_id": "2",
          "time": "20210829 02:53"
        },
        {
          "id": "MDRW2100022090.1.49",
          "form": "시댁ㅎ",
          "original_form": "시댁ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:53"
        },
        {
          "id": "MDRW2100022090.1.50",
          "form": "아버지가 너무 좋아해서 아내에게 장난을 치는데 그게 불편하대요",
          "original_form": "아버지가 너무 좋아해서 아내에게 장난을 치는데 그게 불편하대요",
          "speaker_id": "2",
          "time": "20210829 02:54"
        },
        {
          "id": "MDRW2100022090.1.51",
          "form": "음 이게 누가 잘못이다",
          "original_form": "음 이게 누가 잘못이다",
