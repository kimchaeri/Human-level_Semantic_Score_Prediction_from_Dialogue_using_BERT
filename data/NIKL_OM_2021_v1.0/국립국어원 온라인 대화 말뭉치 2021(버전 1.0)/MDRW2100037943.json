      "utterance": [
        {
          "id": "MDRW2100037943.1.1",
          "form": "네 비밀이 풀렸지 뭐에요ㅜ",
          "original_form": "네 비밀이 풀렸지 뭐에요ㅜ",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100037943.1.2",
          "form": "그이후로 랜선집사가 되었어요ㅜㅜ",
          "original_form": "그이후로 랜선집사가 되었어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100037943.1.3",
          "form": "ㅠㅠㅠㅠ 유투브 보니 약먹고 키우는 집사 봤는데",
          "original_form": "ㅠㅠㅠㅠ 유투브 보니 약먹고 키우는 집사 봤는데",
          "speaker_id": "2",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100037943.1.4",
          "form": "여간 힘든게 아닐것같네요",
          "original_form": "여간 힘든게 아닐것같네요",
          "speaker_id": "2",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100037943.1.5",
          "form": "저는 또 같이사는 가족들이 싫어해서요 ㅋㅋ",
          "original_form": "저는 또 같이사는 가족들이 싫어해서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100037943.1.6",
          "form": "이제 방법은 하나뿐이에요 ㅋㅋ",
          "original_form": "이제 방법은 하나뿐이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100037943.1.7",
          "form": "알레르기도 있으시니 ㅡㅜ 근데 키우면 진짜 반하실텐데 ㅜㅜㅋㅋ",
          "original_form": "알레르기도 있으시니 ㅡㅜ 근데 키우면 진짜 반하실텐데 ㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100037943.1.8",
          "form": "랜선집사 뿐ㅋㅋ",
          "original_form": "랜선집사 뿐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100037943.1.9",
          "form": "그래서 저는 누가 반려동물 자랑함 그거듣는거좋아해요ㅋㅋ",
          "original_form": "그래서 저는 누가 반려동물 자랑함 그거듣는거좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100037943.1.10",
          "form": "호곡 저였다면 정말 슬플것같아요 ㅜㅜ",
          "original_form": "호곡 저였다면 정말 슬플것같아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100037943.1.11",
          "form": "근데 키우는 저도 유투브 방송 맨날 본다는 ㅋㅋㅋ",
          "original_form": "근데 키우는 저도 유투브 방송 맨날 본다는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100037943.1.12",
          "form": "다른냥이도 왜이렇게 귀여운지",
          "original_form": "다른냥이도 왜이렇게 귀여운지",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100037943.1.13",
          "form": "내냥도 귀엽고 다른냥도 귀엽지 않나요 ㅋㅋ",
          "original_form": "내냥도 귀엽고 다른냥도 귀엽지 않나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037943.1.14",
          "form": "그니까요 ㅠㅠ",
          "original_form": "그니까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037943.1.15",
          "form": "전 고양이데리고 산책다니고싶었어요ㅜㅜ",
          "original_form": "전 고양이데리고 산책다니고싶었어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037943.1.16",
          "form": "근데 친구는",
          "original_form": "근데 친구는",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037943.1.17",
          "form": "자기네 강아지만 귀엽데요 ㅋㅋㅋㅋㅋ",
          "original_form": "자기네 강아지만 귀엽데요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037943.1.18",
          "form": "그 아비나 물좋아하는 애들은 산책냥도된다는",
          "original_form": "그 아비나 물좋아하는 애들은 산책냥도된다는",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037943.1.19",
          "form": "그런걸 또 어디서보고 ㅋㅋ",
          "original_form": "그런걸 또 어디서보고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037943.1.20",
          "form": "박수홍네 고양이는 대단하던데요",
          "original_form": "박수홍네 고양이는 대단하던데요",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037943.1.21",
          "form": "그정도는 되야 산책 다닐듯요 ㅠ",
          "original_form": "그정도는 되야 산책 다닐듯요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037943.1.22",
          "form": "저는 까만애들이 그렇게 이쁘더라고요",
          "original_form": "저는 까만애들이 그렇게 이쁘더라고요",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100037943.1.23",
          "form": "올블랙냥이나",
          "original_form": "올블랙냥이나",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100037943.1.24",
          "form": "턱시도입은애들",
          "original_form": "턱시도입은애들",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100037943.1.25",
          "form": "진짜 멋있지용 ㅎㅎ",
          "original_form": "진짜 멋있지용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100037943.1.26",
          "form": "작은이모가",
          "original_form": "작은이모가",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100037943.1.27",
          "form": "애기 고양이 주웠는데",
          "original_form": "애기 고양이 주웠는데",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100037943.1.28",
          "form": "턱시도냥이더라구요",
          "original_form": "턱시도냥이더라구요",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100037943.1.29",
          "form": "흰양말을 신고있어요 ㅋㅋㅋ",
          "original_form": "흰양말을 신고있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100037943.1.30",
          "form": "턱시도가 진짜 짱귀여운거같아요",
          "original_form": "턱시도가 진짜 짱귀여운거같아요",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100037943.1.31",
          "form": "한때는 뱅갈이니",
          "original_form": "한때는 뱅갈이니",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100037943.1.32",
          "form": "아비한테 정신 못차렸는데 ㅋㅋ",
          "original_form": "아비한테 정신 못차렸는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100037943.1.33",
          "form": "우와 저도 뱅갈이요",
          "original_form": "우와 저도 뱅갈이요",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100037943.1.34",
          "form": "고등어를 좋아해서 뱅갈도 멋있더라구요",
          "original_form": "고등어를 좋아해서 뱅갈도 멋있더라구요",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100037943.1.35",
          "form": "근데 에너지가 넘친데요",
          "original_form": "근데 에너지가 넘친데요",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100037943.1.36",
          "form": "턱시도냥이 어디서 만나보셨나봐요?",
          "original_form": "턱시도냥이 어디서 만나보셨나봐요?",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100037943.1.37",
          "form": "뱅갈이 물좋아한대요",
          "original_form": "뱅갈이 물좋아한대요",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100037943.1.38",
          "form": "그래서 막 ㅇㅅㅇ욕조에서 헤엄도치더라고요",
          "original_form": "그래서 막 ㅇㅅㅇ욕조에서 헤엄도치더라고요",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100037943.1.39",
          "form": "저 동네 산책할때 저를 바람처럼 스쳐지나가여 ㅋㅋ",
          "original_form": "저 동네 산책할때 저를 바람처럼 스쳐지나가여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100037943.1.40",
          "form": "물속성이 있군요 ㅋㅋㅋㅋㅋ",
          "original_form": "물속성이 있군요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100037943.1.41",
          "form": "턱시도가 지나가나용?!",
          "original_form": "턱시도가 지나가나용?!",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100037943.1.42",
          "form": "제가 동네산책하는 쪼꼬만 그냥운동기구놓여있고 이런곳이있는데",
          "original_form": "제가 동네산책하는 쪼꼬만 그냥운동기구놓여있고 이런곳이있는데",
          "speaker_id": "1",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100037943.1.43",
          "form": "고기 주변 언저리가 활동구역같아요 ㅋㅋ",
          "original_form": "고기 주변 언저리가 활동구역같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:07"
        }
      ]
    }
