          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100047512.1.14",
          "form": "그래서 또 겁나 그집에 돈보태줌",
          "original_form": "그래서 또 겁나 그집에 돈보태줌",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
