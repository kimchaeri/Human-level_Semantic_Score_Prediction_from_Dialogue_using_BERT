      "utterance": [
        {
          "id": "MDRW2100032457.1.1",
          "form": "잘 상하긴 하지",
          "original_form": "잘 상하긴 하지",
          "speaker_id": "2",
          "time": "20210905 18:18"
        },
        {
          "id": "MDRW2100032457.1.2",
          "form": "멸균우유로 사면 날짜 걱정 없어서 좋아 보통 우유보다 저렴하고",
          "original_form": "멸균우유로 사면 날짜 걱정 없어서 좋아 보통 우유보다 저렴하고",
          "speaker_id": "1",
          "time": "20210905 18:18"
        },
        {
          "id": "MDRW2100032457.1.3",
          "form": "얼마 전에 우유 상해서 2병이나 버렸어",
          "original_form": "얼마 전에 우유 상해서 2병이나 버렸어",
          "speaker_id": "2",
          "time": "20210905 18:19"
        },
        {
          "id": "MDRW2100032457.1.4",
          "form": "응응 맛은 좀 떨어져ㅗ",
          "original_form": "응응 맛은 좀 떨어져ㅗ",
          "speaker_id": "1",
          "time": "20210905 18:19"
        },
        {
          "id": "MDRW2100032457.1.5",
          "form": "ㅕ떨어져도",
          "original_form": "ㅕ떨어져도",
          "speaker_id": "1",
          "time": "20210905 18:19"
        },
        {
          "id": "MDRW2100032457.1.6",
          "form": "떨어져도",
          "original_form": "떨어져도",
          "speaker_id": "1",
          "time": "20210905 18:19"
        },
        {
          "id": "MDRW2100032457.1.7",
          "form": "멸균우유는 잘 안상해서 좋아",
          "original_form": "멸균우유는 잘 안상해서 좋아",
          "speaker_id": "1",
          "time": "20210905 18:19"
        },
        {
          "id": "MDRW2100032457.1.8",
          "form": "멸균우유는 우유를 멸균한 거지?",
          "original_form": "멸균우유는 우유를 멸균한 거지?",
          "speaker_id": "2",
          "time": "20210905 18:20"
        },
        {
          "id": "MDRW2100032457.1.9",
          "form": "응응 살균 방법 차이인 것 같더라고",
          "original_form": "응응 살균 방법 차이인 것 같더라고",
          "speaker_id": "1",
          "time": "20210905 18:20"
        },
        {
          "id": "MDRW2100032457.1.10",
          "form": "찾아봐야겠다",
          "original_form": "찾아봐야겠다",
          "speaker_id": "2",
          "time": "20210905 18:20"
        },
        {
          "id": "MDRW2100032457.1.11",
          "form": "그리고 명란젓이나 베이컨 파지로 사면 저렴하게 살 수 있어",
          "original_form": "그리고 명란젓이나 베이컨 파지로 사면 저렴하게 살 수 있어",
          "speaker_id": "1",
          "time": "20210905 18:21"
        },
        {
          "id": "MDRW2100032457.1.12",
          "form": "우와 좋네",
          "original_form": "우와 좋네",
          "speaker_id": "2",
          "time": "20210905 18:21"
        },
        {
          "id": "MDRW2100032457.1.13",
          "form": "인터넷으로 사나?",
          "original_form": "인터넷으로 사나?",
          "speaker_id": "2",
          "time": "20210905 18:21"
        },
        {
          "id": "MDRW2100032457.1.14",
          "form": "응 나는 인터넷이 오프라인보다 저렴한 경우가 많더라구",
          "original_form": "응 나는 인터넷이 오프라인보다 저렴한 경우가 많더라구",
          "speaker_id": "1",
          "time": "20210905 18:22"
        },
        {
          "id": "MDRW2100032457.1.15",
          "form": "맞아맞아 저렴하지",
          "original_form": "맞아맞아 저렴하지",
          "speaker_id": "2",
          "time": "20210905 18:22"
        },
        {
          "id": "MDRW2100032457.1.16",
          "form": "쿠폰 할인 같은 게 많아서",
          "original_form": "쿠폰 할인 같은 게 많아서",
          "speaker_id": "1",
          "time": "20210905 18:22"
        },
        {
          "id": "MDRW2100032457.1.17",
          "form": "나도 인터넷으로 먹을 거 주문 많이 해",
          "original_form": "나도 인터넷으로 먹을 거 주문 많이 해",
          "speaker_id": "2",
          "time": "20210905 18:23"
        },
        {
          "id": "MDRW2100032457.1.18",
          "form": "달걀국 쉽고 간단하고 좋아!",
          "original_form": "달걀국 쉽고 간단하고 좋아!",
          "speaker_id": "2",
          "time": "20210905 18:24"
        }
      ]
