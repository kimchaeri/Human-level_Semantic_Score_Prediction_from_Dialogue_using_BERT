      "utterance": [
        {
          "id": "MDRW2100038334.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100038334.1.2",
          "form": "키우는 반려동물이 있으신가요 ㅎㅎ",
          "original_form": "키우는 반려동물이 있으신가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100038334.1.3",
          "form": "아니요. 키우시나요?",
          "original_form": "아니요. 키우시나요?",
          "speaker_id": "2",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100038334.1.4",
          "form": "저도 키우지는않고요 음 훌륭한 랜선집사네요 ㅋㅋ",
          "original_form": "저도 키우지는않고요 음 훌륭한 랜선집사네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100038334.1.5",
          "form": "동물 계정 보시나봐요",
          "original_form": "동물 계정 보시나봐요",
          "speaker_id": "2",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100038334.1.6",
          "form": "유투브도보는데 네이버웹툰많이봐요!",
          "original_form": "유투브도보는데 네이버웹툰많이봐요!",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038334.1.7",
          "form": "웹툰 어떤거요?",
          "original_form": "웹툰 어떤거요?",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100038334.1.8",
          "form": "그냥 되게 유명한건데 평소 네이버 웹툰 보시나요?",
          "original_form": "그냥 되게 유명한건데 평소 네이버 웹툰 보시나요?",
          "speaker_id": "1",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100038334.1.9",
          "form": "몇개 안봐요ㅎㅎ",
          "original_form": "몇개 안봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100038334.1.10",
          "form": "그중에서 노곤하개랑 개를 낳았다  열심히 보고있어요",
          "original_form": "그중에서 노곤하개랑 개를 낳았다  열심히 보고있어요",
          "speaker_id": "1",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100038334.1.11",
          "form": "반려견 일상툰 인가봐요?",
          "original_form": "반려견 일상툰 인가봐요?",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100038334.1.12",
          "form": "넵넵 지금 시즌2하는데",
          "original_form": "넵넵 지금 시즌2하는데",
          "speaker_id": "1",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100038334.1.13",
          "form": "정주행 추천이요 ㅎㅎ",
          "original_form": "정주행 추천이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100038334.1.14",
          "form": "ㅋㅋㅋ한번 봐야겠네요",
          "original_form": "ㅋㅋㅋ한번 봐야겠네요",
          "speaker_id": "2",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100038334.1.15",
          "form": "실제 동물 키우는 분들 그림은 좀 리얼하더라구요",
          "original_form": "실제 동물 키우는 분들 그림은 좀 리얼하더라구요",
          "speaker_id": "2",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100038334.1.16",
          "form": "스토리가 나름 감동적이에요 개를 낳았다는",
          "original_form": "스토리가 나름 감동적이에요 개를 낳았다는",
          "speaker_id": "1",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100038334.1.17",
          "form": "강아지 육아일기인가 보네요ㅋㅋ",
          "original_form": "강아지 육아일기인가 보네요ㅋㅋ",
