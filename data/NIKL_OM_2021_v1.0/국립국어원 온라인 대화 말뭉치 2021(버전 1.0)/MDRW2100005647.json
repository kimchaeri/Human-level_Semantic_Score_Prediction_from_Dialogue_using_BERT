      "utterance": [
        {
          "id": "MDRW2100005647.1.1",
          "form": "혹시 게임 좋아하세요?",
          "original_form": "혹시 게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005647.1.2",
          "form": "혹시 어떤 게임 좋아하세요?",
          "original_form": "혹시 어떤 게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005647.1.3",
          "form": "저는 피시방가면  pc로 하는 피파온라인4  이것만 합니다~",
          "original_form": "저는 피시방가면  pc로 하는 피파온라인4  이것만 합니다~",
          "speaker_id": "2",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005647.1.4",
          "form": "다른게임은 쳐다도 안보고요",
          "original_form": "다른게임은 쳐다도 안보고요",
          "speaker_id": "2",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005647.1.5",
          "form": "모바일게임은 안하세요?",
          "original_form": "모바일게임은 안하세요?",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005647.1.6",
          "form": "요즘에 모바일게임 재밌는걸 못찾아서 잘 안하게 되더라고요",
          "original_form": "요즘에 모바일게임 재밌는걸 못찾아서 잘 안하게 되더라고요",
          "speaker_id": "2",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005647.1.7",
          "form": "모마일게임 쉽고 재밌는거 있나요?",
          "original_form": "모마일게임 쉽고 재밌는거 있나요?",
          "speaker_id": "2",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005647.1.8",
          "form": "저는 요즘 하스스톤이 재밌더라구요",
          "original_form": "저는 요즘 하스스톤이 재밌더라구요",
          "speaker_id": "1",
          "time": "20210822 09:35"
        },
        {
          "id": "MDRW2100005647.1.9",
          "form": "하스스톤 아세요?",
          "original_form": "하스스톤 아세요?",
          "speaker_id": "1",
          "time": "20210822 09:36"
        },
        {
          "id": "MDRW2100005647.1.10",
          "form": "제가 직접 해보진 않았고 친구가 하는걸 봤어요",
          "original_form": "제가 직접 해보진 않았고 친구가 하는걸 봤어요",
          "speaker_id": "2",
          "time": "20210822 09:37"
        },
        {
          "id": "MDRW2100005647.1.11",
          "form": "이스포츠 뭐 보는거 있으세요?",
          "original_form": "이스포츠 뭐 보는거 있으세요?",
          "speaker_id": "1",
          "time": "20210822 09:38"
        },
        {
          "id": "MDRW2100005647.1.12",
          "form": "저는 피파온라인 보는데 지금도 축구게임을 하고싶더라고요",
          "original_form": "저는 피파온라인 보는데 지금도 축구게임을 하고싶더라고요",
          "speaker_id": "2",
          "time": "20210822 09:39"
        },
        {
          "id": "MDRW2100005647.1.13",
          "form": "이스포츠 보는걸 좋아하시나요?",
          "original_form": "이스포츠 보는걸 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 09:39"
        },
        {
          "id": "MDRW2100005647.1.14",
          "form": "저는 롤이나 오버워치대회같은게 재밌더라구요",
          "original_form": "저는 롤이나 오버워치대회같은게 재밌더라구요",
          "speaker_id": "1",
          "time": "20210822 09:40"
        },
        {
          "id": "MDRW2100005647.1.15",
          "form": "오버워치 직접 하시는건 아니고요?  저랑 해보는거 어때요?",
          "original_form": "오버워치 직접 하시는건 아니고요?  저랑 해보는거 어때요?",
          "speaker_id": "2",
          "time": "20210822 09:41"
        },
        {
          "id": "MDRW2100005647.1.16",
          "form": "저도 오버워치 빠져들었던 시기가 있어거든요",
          "original_form": "저도 오버워치 빠져들었던 시기가 있어거든요",
          "speaker_id": "2",
          "time": "20210822 09:41"
        },
        {
          "id": "MDRW2100005647.1.17",
          "form": "친구들과 내기하면서 밥사는거 자주 했었는데",
          "original_form": "친구들과 내기하면서 밥사는거 자주 했었는데",
          "speaker_id": "2",
          "time": "20210822 09:41"
        },
        {
          "id": "MDRW2100005647.1.18",
          "form": "저 되게 잘하는데",
          "original_form": "저 되게 잘하는데",
          "speaker_id": "1",
          "time": "20210822 09:42"
        },
        {
          "id": "MDRW2100005647.1.19",
          "form": "잘하는 편이에요?",
          "original_form": "잘하는 편이에요?",
          "speaker_id": "1",
          "time": "20210822 09:42"
        },
        {
          "id": "MDRW2100005647.1.20",
          "form": "게임 했었으니까 잘할거같는데 거의 운빨이죠",
          "original_form": "게임 했었으니까 잘할거같는데 거의 운빨이죠",
          "speaker_id": "2",
          "time": "20210822 09:44"
        },
        {
          "id": "MDRW2100005647.1.21",
          "form": "게임은 운빨로 승부나는거잖아요",
          "original_form": "게임은 운빨로 승부나는거잖아요",
          "speaker_id": "2",
          "time": "20210822 09:44"
        },
        {
          "id": "MDRW2100005647.1.22",
          "form": "실력도 중요하지만",
          "original_form": "실력도 중요하지만",
          "speaker_id": "2",
          "time": "20210822 09:44"
        },
        {
          "id": "MDRW2100005647.1.23",
          "form": "아 그렇죠",
          "original_form": "아 그렇죠",
          "speaker_id": "1",
          "time": "20210822 09:44"
        }
      ]
    }
