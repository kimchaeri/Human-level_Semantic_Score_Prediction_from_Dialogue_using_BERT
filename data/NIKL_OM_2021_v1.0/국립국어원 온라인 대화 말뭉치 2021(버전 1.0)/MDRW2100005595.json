      "utterance": [
        {
          "id": "MDRW2100005595.1.1",
          "form": "머리는 단발 선호해 긴머리 선호해?",
          "original_form": "머리는 단발 선호해 긴머리 선호해?",
          "speaker_id": "2",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005595.1.2",
          "form": "나는 고등학교 내내 단발이었는데",
          "original_form": "나는 고등학교 내내 단발이었는데",
          "speaker_id": "1",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005595.1.3",
          "form": "대학 와서는 계속 긴머리야",
          "original_form": "대학 와서는 계속 긴머리야",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005595.1.4",
          "form": "요즘에 다시 단발병왔어ㅠㅠ 너는?",
          "original_form": "요즘에 다시 단발병왔어ㅠㅠ 너는?",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005595.1.5",
          "form": "난 단발은 못하구 긴머리 선호하는데ㅋㅋㅋㅋ",
          "original_form": "난 단발은 못하구 긴머리 선호하는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005595.1.6",
          "form": "그렇구나 긴머리 관리하기 힘들어ㅠㅠㅠ",
          "original_form": "그렇구나 긴머리 관리하기 힘들어ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005595.1.7",
          "form": "미용실 가는거를 귀찮아해서ㅋㅋㅋ한번 자를때 좀 자르긴해 어깨이상으로",
          "original_form": "미용실 가는거를 귀찮아해서ㅋㅋㅋ한번 자를때 좀 자르긴해 어깨이상으로",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005595.1.8",
          "form": "단발 해보면 얼마나 편한지 알텐데",
          "original_form": "단발 해보면 얼마나 편한지 알텐데",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005595.1.9",
          "form": "아 그렇구나 그러면 중단발 긴머리 정도네 ㅎㅎㅎ",
          "original_form": "아 그렇구나 그러면 중단발 긴머리 정도네 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005595.1.10",
          "form": "그래야 쭉 기르다가 또 년에 한번씩 갈수있어서",
          "original_form": "그래야 쭉 기르다가 또 년에 한번씩 갈수있어서",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005595.1.11",
          "form": "어어ㅋㅋㅋㅋㅋ",
          "original_form": "어어ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005595.1.12",
          "form": "단발하면",
          "original_form": "단발하면",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005595.1.13",
          "form": "내 얼굴이 너무 동그래보일까봐",
          "original_form": "내 얼굴이 너무 동그래보일까봐",
          "speaker_id": "2",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005595.1.14",
          "form": "앜ㅋㅋㅋㅋㅋ 나도 동그란데",
          "original_form": "앜ㅋㅋㅋㅋㅋ 나도 동그란데",
          "speaker_id": "1",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005595.1.15",
          "form": "난 그냥 동그란 채로 살았어~",
          "original_form": "난 그냥 동그란 채로 살았어~",
          "speaker_id": "1",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005595.1.16",
          "form": "난 남친이 그렇게 긴 생머리 좋아하더라",
          "original_form": "난 남친이 그렇게 긴 생머리 좋아하더라",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005595.1.17",
          "form": "진짜? 우리 남친이랑 똑같네 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜? 우리 남친이랑 똑같네 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005595.1.18",
          "form": "ㅋㅋㅋㅋㅋ그래서 긴생머리 말고 날 좋아하라고 교육 시켰는데",
          "original_form": "ㅋㅋㅋㅋㅋ그래서 긴생머리 말고 날 좋아하라고 교육 시켰는데",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005595.1.19",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ 교육 시켰단 ㅣ웃겨",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ 교육 시켰단 ㅣ웃겨",
          "speaker_id": "1",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005595.1.20",
          "form": "ㅋㅋㅋㅋㅋ어느날 뽀글머리하고 나타나면 잠깐 집에서 바쁘게 지내자고ㅋㅋㅋ파마에 식겁해",
          "original_form": "ㅋㅋㅋㅋㅋ어느날 뽀글머리하고 나타나면 잠깐 집에서 바쁘게 지내자고ㅋㅋㅋ파마에 식겁해",
