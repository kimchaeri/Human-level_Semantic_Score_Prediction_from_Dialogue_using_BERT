      "utterance": [
        {
          "id": "MDRW2100031424.1.1",
          "form": "와 엄청 덥더니 대박",
          "original_form": "와 엄청 덥더니 대박",
          "speaker_id": "1",
          "time": "20210907 00:31"
        },
        {
          "id": "MDRW2100031424.1.2",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "2",
          "time": "20210907 00:31"
        },
        {
          "id": "MDRW2100031424.1.3",
          "form": "쌀쌀하냐",
          "original_form": "쌀쌀하냐",
          "speaker_id": "2",
          "time": "20210907 00:31"
        },
        {
          "id": "MDRW2100031424.1.4",
          "form": "ㅇㅇ썰렁하다 진짜",
          "original_form": "ㅇㅇ썰렁하다 진짜",
          "speaker_id": "1",
          "time": "20210907 00:32"
        },
        {
          "id": "MDRW2100031424.1.5",
          "form": "9월 되자마자 바로 썰렁해졌다",
          "original_form": "9월 되자마자 바로 썰렁해졌다",
          "speaker_id": "2",
          "time": "20210907 00:38"
        },
        {
          "id": "MDRW2100031424.1.6",
          "form": "맞음 갑자기 확 쌀쌀해져서 잘 때",
          "original_form": "맞음 갑자기 확 쌀쌀해져서 잘 때",
          "speaker_id": "1",
          "time": "20210907 00:41"
        },
        {
          "id": "MDRW2100031424.1.7",
          "form": "이불덮고잔다ㅋㅋ",
          "original_form": "이불덮고잔다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:41"
        },
        {
          "id": "MDRW2100031424.1.8",
          "form": "방문도 닫고 자 밤에 춥다",
          "original_form": "방문도 닫고 자 밤에 춥다",
          "speaker_id": "2",
          "time": "20210907 00:45"
        },
        {
          "id": "MDRW2100031424.1.9",
          "form": "아직 더운데 창문만 최대한 닫고 자면 안되?",
          "original_form": "아직 더운데 창문만 최대한 닫고 자면 안되?",
          "speaker_id": "1",
          "time": "20210907 00:47"
        },
        {
          "id": "MDRW2100031424.1.10",
          "form": "문도",
          "original_form": "문도",
          "speaker_id": "2",
          "time": "20210907 00:49"
        },
        {
          "id": "MDRW2100031424.1.11",
          "form": "문도",
          "original_form": "문도",
          "speaker_id": "2",
          "time": "20210907 00:49"
        },
        {
          "id": "MDRW2100031424.1.12",
          "form": "닫",
          "original_form": "닫",
          "speaker_id": "2",
          "time": "20210907 00:50"
        },
        {
          "id": "MDRW2100031424.1.13",
          "form": "고",
          "original_form": "고",
          "speaker_id": "2",
          "time": "20210907 00:50"
        },
        {
          "id": "MDRW2100031424.1.14",
          "form": "자",
          "original_form": "자",
          "speaker_id": "2",
          "time": "20210907 00:50"
        },
        {
          "id": "MDRW2100031424.1.15",
          "form": "문열고 이불 꼭 닫고 잘게",
          "original_form": "문열고 이불 꼭 닫고 잘게",
          "speaker_id": "1",
          "time": "20210907 00:52"
        },
        {
          "id": "MDRW2100031424.1.16",
          "form": "아직 좀 더운데ㅠㅜ",
          "original_form": "아직 좀 더운데ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 00:52"
        },
        {
          "id": "MDRW2100031424.1.17",
          "form": "말도 안듣는다",
          "original_form": "말도 안듣는다",
          "speaker_id": "2",
          "time": "20210907 00:54"
        },
        {
          "id": "MDRW2100031424.1.18",
          "form": "날씨가 애매해서 그럼ㅠㅜ",
          "original_form": "날씨가 애매해서 그럼ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 00:56"
        },
        {
          "id": "MDRW2100031424.1.19",
          "form": "덥다춥다하긴한다",
          "original_form": "덥다춥다하긴한다",
          "speaker_id": "2",
          "time": "20210907 00:58"
        },
        {
          "id": "MDRW2100031424.1.20",
          "form": "오늘은 좀 덥더라",
          "original_form": "오늘은 좀 덥더라",
          "speaker_id": "1",
          "time": "20210907 00:58"
        },
        {
          "id": "MDRW2100031424.1.21",
          "form": "어젠 하루종일 쌀쌀했는데",
          "original_form": "어젠 하루종일 쌀쌀했는데",
          "speaker_id": "1",
          "time": "20210907 00:59"
        },
        {
          "id": "MDRW2100031424.1.22",
          "form": "잘때 문단속하고 자라고",
          "original_form": "잘때 문단속하고 자라고",
          "speaker_id": "2",
          "time": "20210907 01:00"
        },
        {
          "id": "MDRW2100031424.1.23",
          "form": "엄마도 문닫고 자슈",
          "original_form": "엄마도 문닫고 자슈",
          "speaker_id": "1",
          "time": "20210907 01:01"
        },
        {
          "id": "MDRW2100031424.1.24",
          "form": "거실이 더 썰렁해",
          "original_form": "거실이 더 썰렁해",
          "speaker_id": "1",
          "time": "20210907 01:01"
        },
        {
          "id": "MDRW2100031424.1.25",
