      "utterance": [
        {
          "id": "MDRW2100001954.1.1",
          "form": "동물키우시나요?",
          "original_form": "동물키우시나요?",
          "speaker_id": "2",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001954.1.2",
          "form": "저를 키우고있습니다",
          "original_form": "저를 키우고있습니다",
          "speaker_id": "1",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001954.1.3",
          "form": "안 키우고있어요",
          "original_form": "안 키우고있어요",
          "speaker_id": "1",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001954.1.4",
          "form": "저는 캣맘 ㅎ",
          "original_form": "저는 캣맘 ㅎ",
          "speaker_id": "2",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001954.1.5",
          "form": "너무 부럽다",
          "original_form": "너무 부럽다",
          "speaker_id": "1",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001954.1.6",
          "form": "고양이 최고",
          "original_form": "고양이 최고",
          "speaker_id": "1",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001954.1.7",
          "form": "사람따르는고양이 있어서 궁딩팡팡해줘요",
          "original_form": "사람따르는고양이 있어서 궁딩팡팡해줘요",
          "speaker_id": "2",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001954.1.8",
          "form": "고양이 카페에서는 일해봤죠",
          "original_form": "고양이 카페에서는 일해봤죠",
          "speaker_id": "1",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001954.1.9",
          "form": "마스크 안쓰면 기침하다 퇴근했습니다",
          "original_form": "마스크 안쓰면 기침하다 퇴근했습니다",
          "speaker_id": "1",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001954.1.10",
          "form": "오오 저는 안해봤어요",
          "original_form": "오오 저는 안해봤어요",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001954.1.11",
          "form": "고양이 카폐에서 사료 무료로 받았거든요",
          "original_form": "고양이 카폐에서 사료 무료로 받았거든요",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001954.1.12",
          "form": "우와 신기하네요",
          "original_form": "우와 신기하네요",
          "speaker_id": "1",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001954.1.13",
          "form": "그 때 살찐 고양이의 매력을 알았습니다",
          "original_form": "그 때 살찐 고양이의 매력을 알았습니다",
          "speaker_id": "1",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001954.1.14",
          "form": "얘들이 살찌면 개냥이가 되던데요",
          "original_form": "얘들이 살찌면 개냥이가 되던데요",
          "speaker_id": "1",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001954.1.15",
          "form": "그렇죠 ㅎㅎ",
          "original_form": "그렇죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001954.1.16",
          "form": "페럿 키우고싶습니다",
          "original_form": "페럿 키우고싶습니다",
          "speaker_id": "1",
          "time": "20210819 22:36"
        },
