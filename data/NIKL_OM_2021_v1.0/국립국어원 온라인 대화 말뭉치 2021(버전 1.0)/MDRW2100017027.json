      "utterance": [
        {
          "id": "MDRW2100017027.1.1",
          "form": "혼자 사세요??",
          "original_form": "혼자 사세요??",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100017027.1.2",
          "form": "네 혼자사세요?",
          "original_form": "네 혼자사세요?",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100017027.1.3",
          "form": "전 부모님이랑 살아요 ㅋㅋㅋ",
          "original_form": "전 부모님이랑 살아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100017027.1.4",
          "form": "집밥 부럽다",
          "original_form": "집밥 부럽다",
          "speaker_id": "2",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100017027.1.5",
          "form": "ㅋㅋㅋㅋ 근데 사실 외식을 넘 좋아해서..",
          "original_form": "ㅋㅋㅋㅋ 근데 사실 외식을 넘 좋아해서..",
          "speaker_id": "1",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100017027.1.6",
          "form": "앗 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100017027.1.7",
          "form": "그런데 외식도 외식만 하다보면 질려요 ㅋㅋㅋㅋ",
          "original_form": "그런데 외식도 외식만 하다보면 질려요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100017027.1.8",
          "form": "그래요? 좀 질려보고 싶네요.. ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래요? 좀 질려보고 싶네요.. ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100017027.1.9",
          "form": "ㅋㅋㅋㅋㅋ그리고 건강도 은근히 상해요",
          "original_form": "ㅋㅋㅋㅋㅋ그리고 건강도 은근히 상해요",
          "speaker_id": "2",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100017027.1.10",
          "form": "위장이 안좋아지더라구요",
          "original_form": "위장이 안좋아지더라구요",
          "speaker_id": "2",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100017027.1.11",
          "form": "아 그렇겠네요 외식은 아무래도 ㅠㅠ",
          "original_form": "아 그렇겠네요 외식은 아무래도 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017027.1.12",
          "form": "네 집밥이 최고예요 ㅋㅋㅋㅋ",
          "original_form": "네 집밥이 최고예요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017027.1.13",
          "form": "형제자매는 없으세요?",
          "original_form": "형제자매는 없으세요?",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017027.1.14",
          "form": "네 외동이에요 ㅋㅋㅋ",
          "original_form": "네 외동이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017027.1.15",
          "form": "전 남동생이 잇어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 남동생이 잇어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017027.1.16",
          "form": "뭔가 재밌을 것 같아요 외동은 외로워서 ㅠㅠ",
          "original_form": "뭔가 재밌을 것 같아요 외동은 외로워서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017027.1.17",
          "form": "같이 재밋게 놀때도 잇엇지만 보통은 귀찮아햇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "같이 재밋게 놀때도 잇엇지만 보통은 귀찮아햇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:06"
        }
      ]
