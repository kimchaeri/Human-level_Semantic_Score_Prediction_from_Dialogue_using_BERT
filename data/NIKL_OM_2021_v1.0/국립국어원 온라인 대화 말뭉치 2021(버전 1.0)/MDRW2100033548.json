      "utterance": [
        {
          "id": "MDRW2100033548.1.1",
          "form": "네 물도 산따라 내려와서 아이들이 잘놀아요",
          "original_form": "네 물도 산따라 내려와서 아이들이 잘놀아요",
          "speaker_id": "2",
          "time": "20210824 13:17"
        },
        {
          "id": "MDRW2100033548.1.2",
          "form": "조카들이 작년엔 빙수에 놀다갔는데",
          "original_form": "조카들이 작년엔 빙수에 놀다갔는데",
          "speaker_id": "2",
          "time": "20210824 13:17"
        },
        {
          "id": "MDRW2100033548.1.3",
          "form": "요번엔 인원제한으로 못놀았네요",
          "original_form": "요번엔 인원제한으로 못놀았네요",
          "speaker_id": "2",
          "time": "20210824 13:17"
        },
        {
          "id": "MDRW2100033548.1.4",
          "form": "빙수가 특히 망고빙수가",
          "original_form": "빙수가 특히 망고빙수가",
          "speaker_id": "2",
          "time": "20210824 13:18"
        },
        {
          "id": "MDRW2100033548.1.5",
          "form": "양이 많고 맛났어요",
          "original_form": "양이 많고 맛났어요",
          "speaker_id": "2",
          "time": "20210824 13:18"
        },
        {
          "id": "MDRW2100033548.1.6",
          "form": "와 요즘 이런카페들이 많더라구요",
          "original_form": "와 요즘 이런카페들이 많더라구요",
          "speaker_id": "1",
          "time": "20210824 13:18"
        },
        {
          "id": "MDRW2100033548.1.7",
          "form": "네 일석이조죠 놀거리 먹을거리",
          "original_form": "네 일석이조죠 놀거리 먹을거리",
          "speaker_id": "2",
          "time": "20210824 13:19"
        },
        {
          "id": "MDRW2100033548.1.8",
          "form": "저희도 청도에 이런 카페갔었는데\r\n거긴 떡볶이 립 나오는 밥메뉴도 있어서",
          "original_form": "저희도 청도에 이런 카페갔었는데\r\n거긴 떡볶이 립 나오는 밥메뉴도 있어서",
          "speaker_id": "1",
          "time": "20210824 13:19"
        },
        {
          "id": "MDRW2100033548.1.9",
          "form": "내년엔 다시 맛난거 같이 먹고 놀수있음좋겠어요",
          "original_form": "내년엔 다시 맛난거 같이 먹고 놀수있음좋겠어요",
          "speaker_id": "2",
          "time": "20210824 13:19"
        },
        {
          "id": "MDRW2100033548.1.10",
          "form": "세상에 애들과 가기 딱좋네요",
          "original_form": "세상에 애들과 가기 딱좋네요",
          "speaker_id": "2",
          "time": "20210824 13:19"
        },
        {
          "id": "MDRW2100033548.1.11",
          "form": "근데 방학 시즌이라 초딩이들이 물총들고 날뛰니 우리 꼬마들 겁먹고 나오더라구요 ㅎ",
          "original_form": "근데 방학 시즌이라 초딩이들이 물총들고 날뛰니 우리 꼬마들 겁먹고 나오더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 13:19"
        },
        {
          "id": "MDRW2100033548.1.12",
          "form": "아 그럼 넘 어리면 힘들어요",
          "original_form": "아 그럼 넘 어리면 힘들어요",
          "speaker_id": "2",
          "time": "20210824 13:20"
        },
        {
          "id": "MDRW2100033548.1.13",
          "form": "아이들이 겁내서",
          "original_form": "아이들이 겁내서",
          "speaker_id": "2",
          "time": "20210824 13:20"
        },
        {
          "id": "MDRW2100033548.1.14",
          "form": "좀크거나 사람 없을때 가야겠네요 ㅜㅜ",
          "original_form": "좀크거나 사람 없을때 가야겠네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 13:20"
        },
        {
          "id": "MDRW2100033548.1.15",
          "form": "엄마는 밥값 5마넌어치 뽑고싶은데 30분놀고 밥먹고 나왔어요 ㅎ",
          "original_form": "엄마는 밥값 5마넌어치 뽑고싶은데 30분놀고 밥먹고 나왔어요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 13:20"
        },
        {
          "id": "MDRW2100033548.1.16",
          "form": "그래야할듯요 방학아닐때...ㅎ",
          "original_form": "그래야할듯요 방학아닐때...ㅎ",
          "speaker_id": "1",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100033548.1.17",
          "form": "오만원이면 당연히 뽑아야죠 ㅜㅜ",
          "original_form": "오만원이면 당연히 뽑아야죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100033548.1.18",
          "form": "네 그럴때 가야해요",
          "original_form": "네 그럴때 가야해요",
          "speaker_id": "2",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100033548.1.19",
          "form": "좀 후회했어요",
          "original_form": "좀 후회했어요",
          "speaker_id": "1",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100033548.1.20",
          "form": "한상 아이가 어리면 식당가기 힘들어요",
          "original_form": "한상 아이가 어리면 식당가기 힘들어요",
          "speaker_id": "2",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100033548.1.21",
          "form": "담엔 디저트 먹을때 가야겠더라구요",
          "original_form": "담엔 디저트 먹을때 가야겠더라구요",
          "speaker_id": "1",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100033548.1.22",
          "form": "네 그러세요 밥은 다 먹이려면 힘들어요",
          "original_form": "네 그러세요 밥은 다 먹이려면 힘들어요",
          "speaker_id": "2",
          "time": "20210824 13:22"
        },
        {
          "id": "MDRW2100033548.1.23",
          "form": "근데 거기 유명카페일것 같아요",
          "original_form": "근데 거기 유명카페일것 같아요",
          "speaker_id": "2",
          "time": "20210824 13:22"
        },
        {
          "id": "MDRW2100033548.1.24",
          "form": "엄마들이 좋아하는",
          "original_form": "엄마들이 좋아하는",
          "speaker_id": "2",
          "time": "20210824 13:22"
        },
        {
          "id": "MDRW2100033548.1.25",
          "form": "그럼 사람많죠",
          "original_form": "그럼 사람많죠",
          "speaker_id": "2",
          "time": "20210824 13:22"
        },
        {
          "id": "MDRW2100033548.1.26",
          "form": "네 청도에서 좀 유명하더라구요",
          "original_form": "네 청도에서 좀 유명하더라구요",
          "speaker_id": "1",
          "time": "20210824 13:22"
        },
        {
          "id": "MDRW2100033548.1.27",
          "form": "물놀이도 가능하니...",
          "original_form": "물놀이도 가능하니...",
          "speaker_id": "1",
          "time": "20210824 13:22"
        },
        {
          "id": "MDRW2100033548.1.28",
          "form": "그럼 더 아이들 많죠",
          "original_form": "그럼 더 아이들 많죠",
          "speaker_id": "2",
          "time": "20210824 13:22"
        },
        {
          "id": "MDRW2100033548.1.29",
          "form": "저 가는데도 사람이 엄청 많아요",
          "original_form": "저 가는데도 사람이 엄청 많아요",
          "speaker_id": "2",
          "time": "20210824 13:23"
        },
        {
          "id": "MDRW2100033548.1.30",
          "form": "카페가면 거리두기 되는건지싶을정도로 사람 많은거같아요",
          "original_form": "카페가면 거리두기 되는건지싶을정도로 사람 많은거같아요",
          "speaker_id": "1",
          "time": "20210824 13:24"
        }
      ]
