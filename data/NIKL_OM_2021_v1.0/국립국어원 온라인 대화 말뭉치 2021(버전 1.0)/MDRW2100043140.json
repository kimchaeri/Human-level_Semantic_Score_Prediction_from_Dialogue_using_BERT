      "utterance": [
        {
          "id": "MDRW2100043140.1.1",
          "form": "오늘 학교가셨나요?,",
          "original_form": "오늘 학교가셨나요?,",
          "speaker_id": "2",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100043140.1.2",
          "form": "갔어요ㅎㅎ..",
          "original_form": "갔어요ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043140.1.3",
          "form": "집에가고싶네용",
          "original_form": "집에가고싶네용",
          "speaker_id": "1",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043140.1.4",
          "form": "그러게요 집이 최고야",
          "original_form": "그러게요 집이 최고야",
          "speaker_id": "2",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043140.1.5",
          "form": "집이세요?",
          "original_form": "집이세요?",
          "speaker_id": "1",
          "time": "20210907 14:59"
        },
        {
          "id": "MDRW2100043140.1.6",
          "form": "아니요 아직 아니죠ㅠㅠ",
          "original_form": "아니요 아직 아니죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 15:00"
        },
        {
          "id": "MDRW2100043140.1.7",
          "form": "가고싶어서요ㅠㅋㅋ강한 귀소 본능",
          "original_form": "가고싶어서요ㅠㅋㅋ강한 귀소 본능",
          "speaker_id": "2",
          "time": "20210907 15:00"
        },
        {
          "id": "MDRW2100043140.1.8",
          "form": "대학원 무슨 전공이세용??",
          "original_form": "대학원 무슨 전공이세용??",
          "speaker_id": "2",
          "time": "20210907 15:01"
        },
        {
          "id": "MDRW2100043140.1.9",
          "form": "ㅋㄱㅋㅇㄱㄱㅋ",
          "original_form": "ㅋㄱㅋㅇㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100043140.1.10",
          "form": "알죠..",
          "original_form": "알죠..",
          "speaker_id": "1",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100043140.1.11",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100043140.1.12",
          "form": "한방내과학이요ㅎㅎ",
          "original_form": "한방내과학이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100043140.1.13",
          "form": "우와 한의대인가요?",
          "original_form": "우와 한의대인가요?",
          "speaker_id": "2",
          "time": "20210907 15:04"
        },
        {
          "id": "MDRW2100043140.1.14",
          "form": "한의사 너무 멋져요",
          "original_form": "한의사 너무 멋져요",
          "speaker_id": "2",
          "time": "20210907 15:05"
        },
        {
          "id": "MDRW2100043140.1.15",
          "form": "ㅎㅎ아직 막내라",
          "original_form": "ㅎㅎ아직 막내라",
          "speaker_id": "1",
          "time": "20210907 15:06"
        },
        {
          "id": "MDRW2100043140.1.16",
          "form": "노예죠..",
          "original_form": "노예죠..",
          "speaker_id": "1",
          "time": "20210907 15:06"
        },
        {
          "id": "MDRW2100043140.1.17",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100043140.1.18",
          "form": "산청에 본디올 한의원 가보니 거기서도 수련하시던데",
          "original_form": "산청에 본디올 한의원 가보니 거기서도 수련하시던데",
          "speaker_id": "2",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100043140.1.19",
          "form": "ㅎㅎㅎ그렇군요저는대학병원인데",
          "original_form": "ㅎㅎㅎ그렇군요저는대학병원인데",
          "speaker_id": "1",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043140.1.20",
          "form": "그냥 노예죵..",
          "original_form": "그냥 노예죵..",
          "speaker_id": "1",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043140.1.21",
          "form": "어느분야세요?",
          "original_form": "어느분야세요?",
          "speaker_id": "1",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043140.1.22",
          "form": "저는 공기업 + 교육대학원 다니고 있어요",
          "original_form": "저는 공기업 + 교육대학원 다니고 있어요",
          "speaker_id": "2",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100043140.1.23",
          "form": "회사 들어오기전에 대학원 마무리를 못해서",
          "original_form": "회사 들어오기전에 대학원 마무리를 못해서",
          "speaker_id": "2",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100043140.1.24",
          "form": "다시 진학했습니다ㅠㅠ",
          "original_form": "다시 진학했습니다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100043140.1.25",
          "form": "그냥 미련남아서요 ㅋㅋ",
          "original_form": "그냥 미련남아서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100043140.1.26",
          "form": "이야",
          "original_form": "이야",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100043140.1.27",
          "form": "빡세네요ㅜㅜ",
          "original_form": "빡세네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 15:12"
        }
      ]
