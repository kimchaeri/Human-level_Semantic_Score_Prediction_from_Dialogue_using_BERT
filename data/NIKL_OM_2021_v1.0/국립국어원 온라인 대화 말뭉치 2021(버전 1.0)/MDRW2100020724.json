      "utterance": [
        {
          "id": "MDRW2100020724.1.1",
          "form": "저도 횟집 지나갈 땐 숨참아요ㅋㅋㅋ",
          "original_form": "저도 횟집 지나갈 땐 숨참아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:40"
        },
        {
          "id": "MDRW2100020724.1.2",
          "form": "맞아요.. 듣기만 했는데 음....",
          "original_form": "맞아요.. 듣기만 했는데 음....",
          "speaker_id": "1",
          "time": "20210828 11:40"
        },
        {
          "id": "MDRW2100020724.1.3",
          "form": "ㅋㅋㅋㅋㅋ사람 사는거 다 같나봐요",
          "original_form": "ㅋㅋㅋㅋㅋ사람 사는거 다 같나봐요",
          "speaker_id": "1",
          "time": "20210828 11:40"
        },
        {
          "id": "MDRW2100020724.1.4",
          "form": "샤브샤브는 프랜차이즈 가시나요",
          "original_form": "샤브샤브는 프랜차이즈 가시나요",
          "speaker_id": "2",
          "time": "20210828 11:40"
        },
        {
          "id": "MDRW2100020724.1.5",
          "form": "집에서 해먹는경우가 많은거같아오",
          "original_form": "집에서 해먹는경우가 많은거같아오",
          "speaker_id": "1",
          "time": "20210828 11:41"
        },
        {
          "id": "MDRW2100020724.1.6",
          "form": "프차가시나요",
          "original_form": "프차가시나요",
          "speaker_id": "1",
          "time": "20210828 11:41"
        },
        {
          "id": "MDRW2100020724.1.7",
          "form": "오호 다이어트하는 친구들이랑 외식할 때",
          "original_form": "오호 다이어트하는 친구들이랑 외식할 때",
          "speaker_id": "2",
          "time": "20210828 11:41"
        },
        {
          "id": "MDRW2100020724.1.8",
          "form": "자주가요",
          "original_form": "자주가요",
          "speaker_id": "2",
          "time": "20210828 11:41"
        },
        {
          "id": "MDRW2100020724.1.9",
          "form": "어디로 가시나요",
          "original_form": "어디로 가시나요",
          "speaker_id": "1",
          "time": "20210828 11:41"
        },
        {
          "id": "MDRW2100020724.1.10",
          "form": "채선당?",
          "original_form": "채선당?",
          "speaker_id": "1",
          "time": "20210828 11:41"
        },
        {
          "id": "MDRW2100020724.1.11",
          "form": "샤브향도 자주가고",
          "original_form": "샤브향도 자주가고",
          "speaker_id": "2",
          "time": "20210828 11:42"
        },
        {
          "id": "MDRW2100020724.1.12",
          "form": "편백찜 같이하는 곳도 자주가고",
          "original_form": "편백찜 같이하는 곳도 자주가고",
          "speaker_id": "2",
          "time": "20210828 11:42"
        },
        {
          "id": "MDRW2100020724.1.13",
          "form": "음 맛있겠다",
          "original_form": "음 맛있겠다",
          "speaker_id": "1",
          "time": "20210828 11:42"
        },
        {
          "id": "MDRW2100020724.1.14",
          "form": "다 좋은데 이름이 다 비슷해서 기억이 안나네요",
          "original_form": "다 좋은데 이름이 다 비슷해서 기억이 안나네요",
          "speaker_id": "2",
          "time": "20210828 11:42"
        },
        {
          "id": "MDRW2100020724.1.15",
          "form": "그 이랜드거기도 있었던거같은데",
          "original_form": "그 이랜드거기도 있었던거같은데",
          "speaker_id": "1",
          "time": "20210828 11:42"
        },
        {
          "id": "MDRW2100020724.1.16",
          "form": "야들한 소고기가 맛있죠",
          "original_form": "야들한 소고기가 맛있죠",
          "speaker_id": "2",
          "time": "20210828 11:42"
        },
        {
          "id": "MDRW2100020724.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 11:42"
        },
        {
          "id": "MDRW2100020724.1.18",
          "form": "살짝데쳐서",
          "original_form": "살짝데쳐서",
          "speaker_id": "1",
          "time": "20210828 11:42"
        },
        {
          "id": "MDRW2100020724.1.19",
          "form": "소스찍어먹는거 좋아요",
          "original_form": "소스찍어먹는거 좋아요",
          "speaker_id": "1",
          "time": "20210828 11:43"
        },
        {
          "id": "MDRW2100020724.1.20",
          "form": "맞아요 땅콩소스 좋아해요 전",
          "original_form": "맞아요 땅콩소스 좋아해요 전",
          "speaker_id": "2",
          "time": "20210828 11:43"
        },
        {
          "id": "MDRW2100020724.1.21",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210828 11:43"
        },
        {
          "id": "MDRW2100020724.1.22",
          "form": "그 칠리도좋긴한데",
          "original_form": "그 칠리도좋긴한데",
          "speaker_id": "1",
          "time": "20210828 11:43"
        },
        {
          "id": "MDRW2100020724.1.23",
          "form": "땅콩이 더 ㅎ",
          "original_form": "땅콩이 더 ㅎ",
          "speaker_id": "1",
          "time": "20210828 11:43"
        }
      ]
