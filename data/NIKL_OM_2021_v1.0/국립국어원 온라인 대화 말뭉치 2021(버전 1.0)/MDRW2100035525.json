      "utterance": [
        {
          "id": "MDRW2100035525.1.1",
          "form": "버거킹 주문하려구요!",
          "original_form": "버거킹 주문하려구요!",
          "speaker_id": "1",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100035525.1.2",
          "form": "버거킹 좋죠 ㅋㅋㅋ",
          "original_form": "버거킹 좋죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100035525.1.3",
          "form": "상어가족 맛있어요",
          "original_form": "상어가족 맛있어요",
          "speaker_id": "2",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100035525.1.4",
          "form": "오늘은 느끼한게 당겨서 콰트로치즈 먹으려구요ㅎㅎ",
          "original_form": "오늘은 느끼한게 당겨서 콰트로치즈 먹으려구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100035525.1.5",
          "form": "오 맛있죠 ㅎㅎ",
          "original_form": "오 맛있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100035525.1.6",
          "form": "전 스콘 기다리면서 낼 아침에 먹을거 마켓컬리 보고있어요 ㅋㅋ",
          "original_form": "전 스콘 기다리면서 낼 아침에 먹을거 마켓컬리 보고있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100035525.1.7",
          "form": "마켓컬리 자주 이용하시나요?",
          "original_form": "마켓컬리 자주 이용하시나요?",
          "speaker_id": "1",
          "time": "20210829 20:46"
        },
        {
          "id": "MDRW2100035525.1.8",
          "form": "네 먹을거없을때 가끔 이용해요 ㅎㅎ",
          "original_form": "네 먹을거없을때 가끔 이용해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:47"
        },
        {
          "id": "MDRW2100035525.1.9",
          "form": "정말 새벽배송을 시간 맞춰 잘 오나요?",
          "original_form": "정말 새벽배송을 시간 맞춰 잘 오나요?",
          "speaker_id": "1",
          "time": "20210829 20:50"
        },
        {
          "id": "MDRW2100035525.1.10",
          "form": "네네 아침 7시에 문열면 도착해있더라고요",
          "original_form": "네네 아침 7시에 문열면 도착해있더라고요",
          "speaker_id": "2",
          "time": "20210829 20:51"
        },
        {
          "id": "MDRW2100035525.1.11",
          "form": "안 해봤는데 나중에 필요하면 해봐야겠네요ㅎㅎ",
          "original_form": "안 해봤는데 나중에 필요하면 해봐야겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 20:51"
        },
        {
          "id": "MDRW2100035525.1.12",
          "form": "네 필요할때만 하는걸 추천드려요",
          "original_form": "네 필요할때만 하는걸 추천드려요",
          "speaker_id": "2",
          "time": "20210829 20:52"
        },
        {
          "id": "MDRW2100035525.1.13",
          "form": "색다른 먹거리가 많아요",
          "original_form": "색다른 먹거리가 많아요",
          "speaker_id": "2",
          "time": "20210829 20:52"
        },
        {
          "id": "MDRW2100035525.1.14",
          "form": "배달 비용은 다른 곳과 비슷한가요?",
          "original_form": "배달 비용은 다른 곳과 비슷한가요?",
          "speaker_id": "1",
          "time": "20210829 20:52"
        },
        {
          "id": "MDRW2100035525.1.15",
          "form": "얼마이상이면 무료배송이요",
          "original_form": "얼마이상이면 무료배송이요",
          "speaker_id": "2",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100035525.1.16",
          "form": "오 좋네요ㅎㅎ",
          "original_form": "오 좋네요ㅎㅎ",
          "speaker_id": "1",
