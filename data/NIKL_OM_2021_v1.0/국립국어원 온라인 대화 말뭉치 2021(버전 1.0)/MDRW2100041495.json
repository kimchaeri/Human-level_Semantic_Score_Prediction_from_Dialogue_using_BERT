      "utterance": [
        {
          "id": "MDRW2100041495.1.1",
          "form": "너 어깨 아픈 건 괜찮아졌어?",
          "original_form": "너 어깨 아픈 건 괜찮아졌어?",
          "speaker_id": "2",
          "time": "20210906 05:12"
        },
        {
          "id": "MDRW2100041495.1.2",
          "form": "아 그거 아직도 아퍼",
          "original_form": "아 그거 아직도 아퍼",
          "speaker_id": "1",
          "time": "20210906 05:12"
        },
        {
          "id": "MDRW2100041495.1.3",
          "form": "진짜 병원을 가야되나",
          "original_form": "진짜 병원을 가야되나",
          "speaker_id": "1",
          "time": "20210906 05:12"
        },
        {
          "id": "MDRW2100041495.1.4",
          "form": "정확이 어디가 어떻게 아퍼",
          "original_form": "정확이 어디가 어떻게 아퍼",
          "speaker_id": "2",
          "time": "20210906 05:12"
        },
        {
          "id": "MDRW2100041495.1.5",
          "form": "오른쪽 어깻죽지가 시려",
          "original_form": "오른쪽 어깻죽지가 시려",
          "speaker_id": "1",
          "time": "20210906 05:13"
        },
        {
          "id": "MDRW2100041495.1.6",
          "form": "아파서 팔을 들기가 힘들어",
          "original_form": "아파서 팔을 들기가 힘들어",
          "speaker_id": "1",
          "time": "20210906 05:13"
        },
        {
          "id": "MDRW2100041495.1.7",
          "form": "정형외과를 한번 가보는 게 좋을 것 같긴 해",
          "original_form": "정형외과를 한번 가보는 게 좋을 것 같긴 해",
          "speaker_id": "2",
          "time": "20210906 05:13"
        },
        {
          "id": "MDRW2100041495.1.8",
          "form": "서치해보니까 자세 때문이라고",
          "original_form": "서치해보니까 자세 때문이라고",
          "speaker_id": "1",
          "time": "20210906 05:14"
        },
        {
          "id": "MDRW2100041495.1.9",
          "form": "자세 교정하거나 물리치료를 꾸준히 받아야 된다는데",
          "original_form": "자세 교정하거나 물리치료를 꾸준히 받아야 된다는데",
          "speaker_id": "1",
          "time": "20210906 05:14"
        },
        {
          "id": "MDRW2100041495.1.10",
          "form": "아니 어깻죽지 통증은 오십견 같은 건데",
          "original_form": "아니 어깻죽지 통증은 오십견 같은 건데",
          "speaker_id": "2",
          "time": "20210906 05:15"
        },
        {
          "id": "MDRW2100041495.1.11",
          "form": "넌 어린 애가 벌써 그래서 어떻게 해",
          "original_form": "넌 어린 애가 벌써 그래서 어떻게 해",
          "speaker_id": "2",
          "time": "20210906 05:15"
        },
        {
          "id": "MDRW2100041495.1.12",
          "form": "ㅋㅋㅋ큐ㅠㅠ 요새는 젊은 사람들도 많이 이런대",
          "original_form": "ㅋㅋㅋ큐ㅠㅠ 요새는 젊은 사람들도 많이 이런대",
          "speaker_id": "1",
          "time": "20210906 05:16"
        },
        {
          "id": "MDRW2100041495.1.13",
          "form": "목디스크일수도 있다는데 그건 아닌 것 같고",
          "original_form": "목디스크일수도 있다는데 그건 아닌 것 같고",
          "speaker_id": "1",
          "time": "20210906 05:16"
        },
        {
          "id": "MDRW2100041495.1.14",
          "form": "너 발목도 아프잖아",
          "original_form": "너 발목도 아프잖아",
          "speaker_id": "2",
          "time": "20210906 05:17"
        },
        {
          "id": "MDRW2100041495.1.15",
          "form": "아 그건 예전에 인대 늘어나서 그런 거고",
          "original_form": "아 그건 예전에 인대 늘어나서 그런 거고",
          "speaker_id": "1",
          "time": "20210906 05:17"
        },
        {
          "id": "MDRW2100041495.1.16",
          "form": "인대는 한번 늘어나면 계속 그런대",
          "original_form": "인대는 한번 늘어나면 계속 그런대",
          "speaker_id": "1",
          "time": "20210906 05:17"
        },
        {
          "id": "MDRW2100041495.1.17",
          "form": "그니까 너도 관절이 안좋은 거야",
          "original_form": "그니까 너도 관절이 안좋은 거야",
          "speaker_id": "2",
          "time": "20210906 05:18"
        },
        {
          "id": "MDRW2100041495.1.18",
          "form": "운동할 때도 조심하고",
          "original_form": "운동할 때도 조심하고",
          "speaker_id": "2",
          "time": "20210906 05:18"
        },
        {
          "id": "MDRW2100041495.1.19",
          "form": "평소에 자세도 신경써야돼 흐트러지지 않게",
          "original_form": "평소에 자세도 신경써야돼 흐트러지지 않게",
          "speaker_id": "2",
          "time": "20210906 05:18"
        },
        {
          "id": "MDRW2100041495.1.20",
          "form": "알지 알지",
          "original_form": "알지 알지",
          "speaker_id": "1",
          "time": "20210906 05:18"
        },
        {
          "id": "MDRW2100041495.1.21",
          "form": "요새는 스트레칭도 꾸준히 하는데 별 효과가 없네",
          "original_form": "요새는 스트레칭도 꾸준히 하는데 별 효과가 없네",
          "speaker_id": "1",
          "time": "20210906 05:19"
        },
        {
          "id": "MDRW2100041495.1.22",
          "form": "칼슘 같은 것도 챙겨먹는게 좋을 것 같아",
          "original_form": "칼슘 같은 것도 챙겨먹는게 좋을 것 같아",
          "speaker_id": "2",
          "time": "20210906 05:19"
        },
        {
          "id": "MDRW2100041495.1.23",
          "form": "ㅋㅋㅋ지금도 영양제 충분히 많이 먹고 있어",
          "original_form": "ㅋㅋㅋ지금도 영양제 충분히 많이 먹고 있어",
          "speaker_id": "1",
          "time": "20210906 05:19"
        }
      ]
    }
  ]
