      "utterance": [
        {
          "id": "MDRW2100030160.1.1",
          "form": "집안일 많이하시나요",
          "original_form": "집안일 많이하시나요",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030160.1.2",
          "form": "저는 생활비 드리는 대신 집안일로 퉁치고 있어요 ㅋㅋㅋ",
          "original_form": "저는 생활비 드리는 대신 집안일로 퉁치고 있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030160.1.3",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030160.1.4",
          "form": "저는 생활비 안드리곸ㅋㅋㅋ제가 하네요",
          "original_form": "저는 생활비 안드리곸ㅋㅋㅋ제가 하네요",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030160.1.5",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030160.1.6",
          "form": "제가 많이해요 ㅋㅋ부모님 바쁘셔서",
          "original_form": "제가 많이해요 ㅋㅋ부모님 바쁘셔서",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030160.1.7",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030160.1.8",
          "form": "아 그러시구나!!",
          "original_form": "아 그러시구나!!",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030160.1.9",
          "form": "음식도 하세요?",
          "original_form": "음식도 하세요?",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030160.1.10",
          "form": "네네 요리 많이해요",
          "original_form": "네네 요리 많이해요",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030160.1.11",
          "form": "여름엔 힘들었는데 선선해지니 좋네요",
          "original_form": "여름엔 힘들었는데 선선해지니 좋네요",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030160.1.12",
          "form": "마자요 지금 날씨가 정말 좋죠",
          "original_form": "마자요 지금 날씨가 정말 좋죠",
          "speaker_id": "1",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100030160.1.13",
          "form": "우와 어떤 요리 가장 자신있으세요?",
          "original_form": "우와 어떤 요리 가장 자신있으세요?",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030160.1.14",
          "form": "전 미역국이 반응이좋더라고요",
          "original_form": "전 미역국이 반응이좋더라고요",
          "speaker_id": "2",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030160.1.15",
          "form": "우와 멋져요!!",
          "original_form": "우와 멋져요!!",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100030160.1.16",
          "form": "시원하니 집안일도 괜찬ㄹ은데",
          "original_form": "시원하니 집안일도 괜찬ㄹ은데",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100030160.1.17",
          "form": "더울땐 짜증나서 동생한테 화내고 그랬어요",
          "original_form": "더울땐 짜증나서 동생한테 화내고 그랬어요",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100030160.1.18",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100030160.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100030160.1.20",
          "form": "막 밥먹고 쏠랑 들어가버려서",
          "original_form": "막 밥먹고 쏠랑 들어가버려서",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030160.1.21",
          "form": "어찌나 열받던지",
          "original_form": "어찌나 열받던지",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030160.1.22",
          "form": "지금은 미안하네요",
          "original_form": "지금은 미안하네요",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030160.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아이고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아이고",
          "speaker_id": "1",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030160.1.24",
          "form": "동생분이 잘못했네요",
          "original_form": "동생분이 잘못했네요",
          "speaker_id": "1",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030160.1.25",
          "form": "설거지라도 하고 다지",
          "original_form": "설거지라도 하고 다지",
          "speaker_id": "1",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030160.1.26",
