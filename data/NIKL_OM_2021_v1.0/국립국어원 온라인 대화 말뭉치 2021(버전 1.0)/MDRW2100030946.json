      "utterance": [
        {
          "id": "MDRW2100030946.1.1",
          "form": "영화 좋아하시나요",
          "original_form": "영화 좋아하시나요",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100030946.1.2",
          "form": "네 자주보러가요",
          "original_form": "네 자주보러가요",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100030946.1.3",
          "form": "가장 기억에 남는 영화는?",
          "original_form": "가장 기억에 남는 영화는?",
          "speaker_id": "2",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100030946.1.4",
          "form": "크레센도 괜찮았어요",
          "original_form": "크레센도 괜찮았어요",
          "speaker_id": "1",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100030946.1.5",
          "form": "오 뭐죠",
          "original_form": "오 뭐죠",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100030946.1.6",
          "form": "독일인 유명지휘자가 이스라엘과 팔레스타인의 연주자들을 뽑아 평화오케스트라를 만드는 내용이요",
          "original_form": "독일인 유명지휘자가 이스라엘과 팔레스타인의 연주자들을 뽑아 평화오케스트라를 만드는 내용이요",
          "speaker_id": "1",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100030946.1.7",
          "form": "오.....",
          "original_form": "오.....",
          "speaker_id": "2",
          "time": "20210904 21:37"
        },
        {
          "id": "MDRW2100030946.1.8",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100030946.1.9",
          "form": "저는 신세계가...ㅋㅋ",
          "original_form": "저는 신세계가...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100030946.1.10",
          "form": "드루와드루와 고거요 ?ㅋㅋ",
          "original_form": "드루와드루와 고거요 ?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100030946.1.11",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100030946.1.12",
          "form": "이정재 연기가 ㅎㅎ",
          "original_form": "이정재 연기가 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100030946.1.13",
          "form": "제가 어릴땐 이정재가 왜 인기있는지 몰랐는데",
          "original_form": "제가 어릴땐 이정재가 왜 인기있는지 몰랐는데",
          "speaker_id": "1",
          "time": "20210906 19:23"
        },
        {
          "id": "MDRW2100030946.1.14",
          "form": "신세계보고 으른 섹시라는거에 눈을 떴습니다",
          "original_form": "신세계보고 으른 섹시라는거에 눈을 떴습니다",
          "speaker_id": "1",
          "time": "20210906 19:23"
        },
        {
          "id": "MDRW2100030946.1.15",
          "form": "정점은 악에서 구하소서",
          "original_form": "정점은 악에서 구하소서",
          "speaker_id": "1",
          "time": "20210906 19:23"
        },
        {
          "id": "MDRW2100030946.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ악에서 구하소서는 너무 못돼게 나와서",
          "original_form": "ㅋㅋㅋㅋㅋㅋ악에서 구하소서는 너무 못돼게 나와서",
          "speaker_id": "2",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100030946.1.17",
          "form": "악역인데 멋있었다능",
          "original_form": "악역인데 멋있었다능",
          "speaker_id": "1",
          "time": "20210906 20:08"
        },
        {
          "id": "MDRW2100030946.1.18",
          "form": "멋있긴했어요ㅋㅋㅋ",
          "original_form": "멋있긴했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:58"
        },
        {
          "id": "MDRW2100030946.1.19",
          "form": "아역. 소이너무 귀엽지않아요?",
          "original_form": "아역. 소이너무 귀엽지않아요?",
