      "utterance": [
        {
          "id": "MDRW2100023051.1.1",
          "form": "안됩니다ㅜㅜ",
          "original_form": "안됩니다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 21:09"
        },
        {
          "id": "MDRW2100023051.1.2",
          "form": "아토피에 밀가루가 진짜 안좋다그래서",
          "original_form": "아토피에 밀가루가 진짜 안좋다그래서",
          "speaker_id": "1",
          "time": "20210829 21:09"
        },
        {
          "id": "MDRW2100023051.1.3",
          "form": "참고있다면서요",
          "original_form": "참고있다면서요",
          "speaker_id": "2",
          "time": "20210829 21:09"
        },
        {
          "id": "MDRW2100023051.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210829 21:09"
        },
        {
          "id": "MDRW2100023051.1.5",
          "form": "나을때까지만 정크푸드 참아봐요",
          "original_form": "나을때까지만 정크푸드 참아봐요",
          "speaker_id": "2",
          "time": "20210829 21:09"
        },
        {
          "id": "MDRW2100023051.1.6",
          "form": "그래야겠죠ㅜㅜ",
          "original_form": "그래야겠죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 21:09"
        },
        {
          "id": "MDRW2100023051.1.7",
          "form": "병원비 아깝잖아요",
          "original_form": "병원비 아깝잖아요",
          "speaker_id": "2",
          "time": "20210829 21:10"
        },
        {
          "id": "MDRW2100023051.1.8",
          "form": "그래도 일주일에 한 번은 먹어도 되지않을까요",
          "original_form": "그래도 일주일에 한 번은 먹어도 되지않을까요",
          "speaker_id": "1",
          "time": "20210829 21:11"
        },
        {
          "id": "MDRW2100023051.1.9",
          "form": "솔직히 좋아지고 있을 때 확 잡는 게 중요하죠",
          "original_form": "솔직히 좋아지고 있을 때 확 잡는 게 중요하죠",
          "speaker_id": "2",
          "time": "20210829 21:11"
        },
        {
          "id": "MDRW2100023051.1.10",
          "form": "그러겠죠...",
          "original_form": "그러겠죠...",
          "speaker_id": "1",
          "time": "20210829 21:12"
        },
        {
          "id": "MDRW2100023051.1.11",
          "form": "넹! 다낫고 맛있는 거 챙겨드세요",
          "original_form": "넹! 다낫고 맛있는 거 챙겨드세요",
          "speaker_id": "2",
          "time": "20210829 21:12"
        },
        {
          "id": "MDRW2100023051.1.12",
          "form": "건강한 걸로다가 챙겨먹어볼게요",
          "original_form": "건강한 걸로다가 챙겨먹어볼게요",
          "speaker_id": "1",
          "time": "20210829 21:12"
        },
        {
          "id": "MDRW2100023051.1.13",
          "form": "어떤거드세요?",
          "original_form": "어떤거드세요?",
          "speaker_id": "2",
          "time": "20210829 21:12"
        },
        {
          "id": "MDRW2100023051.1.14",
          "form": "과일류 많이 챙겨먹고 있어요",
          "original_form": "과일류 많이 챙겨먹고 있어요",
          "speaker_id": "1",
          "time": "20210829 21:13"
        },
        {
          "id": "MDRW2100023051.1.15",
          "form": "좋네요ㅋㅋ",
          "original_form": "좋네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:13"
        },
        {
          "id": "MDRW2100023051.1.16",
          "form": "회사에서 일하다보면 너무 배고플 때 어떻게하세요?",
          "original_form": "회사에서 일하다보면 너무 배고플 때 어떻게하세요?",
          "speaker_id": "2",
