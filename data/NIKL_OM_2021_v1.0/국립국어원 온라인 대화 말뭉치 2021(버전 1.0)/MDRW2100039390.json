      "utterance": [
        {
          "id": "MDRW2100039390.1.1",
          "form": "언니 저 그냥 삼성 노트북 사랴고요! 연동이 되네요??",
          "original_form": "언니 저 그냥 삼성 노트북 사랴고요! 연동이 되네요??",
          "speaker_id": "2",
          "time": "20210903 17:05"
        },
        {
          "id": "MDRW2100039390.1.2",
          "form": "조교 채점하기 편할거같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "조교 채점하기 편할거같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:05"
        },
        {
          "id": "MDRW2100039390.1.3",
          "form": "오?!?! 대박대박",
          "original_form": "오?!?! 대박대박",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039390.1.4",
          "form": "그 사이",
          "original_form": "그 사이",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039390.1.5",
          "form": "삼성 노트북도 많이 발전했네요!!!",
          "original_form": "삼성 노트북도 많이 발전했네요!!!",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039390.1.6",
          "form": "저렴한가요???",
          "original_form": "저렴한가요???",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039390.1.7",
          "form": "아뇨ㅜㅜ 스펙 앵간한거는 그냥 다 비싼 것 같아요ㅜㅜ 제가 어떤 걸 사게될지는 모르겟지만...흑흑",
          "original_form": "아뇨ㅜㅜ 스펙 앵간한거는 그냥 다 비싼 것 같아요ㅜㅜ 제가 어떤 걸 사게될지는 모르겟지만...흑흑",
          "speaker_id": "2",
          "time": "20210903 18:33"
        },
        {
          "id": "MDRW2100039390.1.8",
          "form": "삼성 노트 같은거를 보낼 수가 있더라고요?? 앱도 노트북으로 킬 수 있고요!!",
          "original_form": "삼성 노트 같은거를 보낼 수가 있더라고요?? 앱도 노트북으로 킬 수 있고요!!",
          "speaker_id": "2",
          "time": "20210903 18:33"
        },
        {
          "id": "MDRW2100039390.1.9",
          "form": "헐헐 너무 신기해요",
          "original_form": "헐헐 너무 신기해요",
          "speaker_id": "1",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100039390.1.10",
          "form": "근데진짜 장비가",
          "original_form": "근데진짜 장비가",
          "speaker_id": "1",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100039390.1.11",
          "form": "제가 채점 노트로 할거니까 바로 보내면 좋을거같아서요 ㅋㅋㅋㅋㅋ",
          "original_form": "제가 채점 노트로 할거니까 바로 보내면 좋을거같아서요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100039390.1.12",
          "form": "어느정도... 서포트 되어야",
          "original_form": "어느정도... 서포트 되어야",
          "speaker_id": "1",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100039390.1.13",
          "form": "공부할 맛이 나는데!!",
          "original_form": "공부할 맛이 나는데!!",
          "speaker_id": "1",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100039390.1.14",
          "form": "와 저는 그 생각 못하고",
          "original_form": "와 저는 그 생각 못하고",
          "speaker_id": "1",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100039390.1.15",
          "form": "그냥 다 메일로 보냈었어요!!!",
          "original_form": "그냥 다 메일로 보냈었어요!!!",
          "speaker_id": "1",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100039390.1.16",
          "form": "그쵸ㅜㅜ",
          "original_form": "그쵸ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100039390.1.17",
          "form": "저도 삼성꺼 못사면 그냥 메일로 보낼거에요 ㅋㅋㅋㅋ",
          "original_form": "저도 삼성꺼 못사면 그냥 메일로 보낼거에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100039390.1.18",
          "form": "네네네!",
          "original_form": "네네네!",
          "speaker_id": "1",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100039390.1.19",
          "form": "그램이 더 싼가...ㅜㅜ",
          "original_form": "그램이 더 싼가...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039390.1.20",
          "form": "삼성 노트북이",
          "original_form": "삼성 노트북이",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039390.1.21",
          "form": "그람보다 더 비싼가요",
          "original_form": "그람보다 더 비싼가요",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039390.1.22",
          "form": "네넨",
          "original_form": "네넨",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039390.1.23",
          "form": "아 그램ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 그램ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039390.1.24",
          "form": "그람이라니까 웃기메욬ㅋㅋㅋㅋ",
          "original_form": "그람이라니까 웃기메욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039390.1.25",
          "form": "음 비슷한거 같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "음 비슷한거 같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039390.1.26",
          "form": "그 그램이 가성비 좋은거같긴 해요,,,",
          "original_form": "그 그램이 가성비 좋은거같긴 해요,,,",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039390.1.27",
          "form": "1000그람...",
          "original_form": "1000그람...",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039390.1.28",
          "form": "만그람...",
          "original_form": "만그람...",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039390.1.29",
          "form": "아하 가격차이 많이나면 그램살수도 있겠네요ㅠㅠ",
          "original_form": "아하 가격차이 많이나면 그램살수도 있겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039390.1.30",
          "form": "8ㅅ8",
          "original_form": "8ㅅ8",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039390.1.31",
          "form": "와... 저 그럼 프로그램 다 다시깔아야해요...",
          "original_form": "와... 저 그럼 프로그램 다 다시깔아야해요...",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039390.1.32",
          "form": "아쉽겠어요 ..",
          "original_form": "아쉽겠어요 ..",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039390.1.33",
          "form": "되려나....",
          "original_form": "되려나....",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039390.1.34",
          "form": "앗 켐드로우요?",
          "original_form": "앗 켐드로우요?",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039390.1.35",
          "form": "그거",
          "original_form": "그거",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039390.1.36",
          "form": "한솔 가서",
          "original_form": "한솔 가서",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039390.1.37",
          "form": "학문관 한솔 가서",
          "original_form": "학문관 한솔 가서",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039390.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ 그거랑 한글이랑 오피스랑 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ 그거랑 한글이랑 오피스랑 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039390.1.39",
          "form": "설치하면 돼요!",
          "original_form": "설치하면 돼요!",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039390.1.40",
          "form": "네네 그거요!!",
          "original_form": "네네 그거요!!",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039390.1.41",
          "form": "저도 학문관에서 깔았어요",
          "original_form": "저도 학문관에서 깔았어요",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039390.1.42",
          "form": "학문관에 한솔이 이ㅛ어요??",
          "original_form": "학문관에 한솔이 이ㅛ어요??",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039390.1.43",
          "form": "네 그..한솔 맞나",
          "original_form": "네 그..한솔 맞나",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039390.1.44",
          "form": "컴퓨터 봐주는 데요!",
          "original_form": "컴퓨터 봐주는 데요!",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039390.1.45",
          "form": "와아아아",
          "original_form": "와아아아",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039390.1.46",
          "form": "처음들어요",
          "original_form": "처음들어요",
          "speaker_id": "2",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100039390.1.47",
          "form": "그럴리가 없는데",
          "original_form": "그럴리가 없는데",
          "speaker_id": "2",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100039390.1.48",
          "form": "왜처음듣지?!?!",
          "original_form": "왜처음듣지?!?!",
          "speaker_id": "2",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100039390.1.49",
          "form": "아!!",
          "original_form": "아!!",
          "speaker_id": "2",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100039390.1.50",
          "form": "알거같아요 그 봉사부서 있는쪽??",
          "original_form": "알거같아요 그 봉사부서 있는쪽??",
          "speaker_id": "2",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100039390.1.51",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100039390.1.52",
          "form": "거기서 싸게 주고 했어요!!!",
          "original_form": "거기서 싸게 주고 했어요!!!",
          "speaker_id": "1",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100039390.1.53",
          "form": "아 돈 주고 까셨어요??",
          "original_form": "아 돈 주고 까셨어요??",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039390.1.54",
          "form": "대학원생은 학교에서 안주나ㅠㅠㅠ",
          "original_form": "대학원생은 학교에서 안주나ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039390.1.55",
          "form": "네.... ㅠㅠ",
          "original_form": "네.... ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100039390.1.56",
          "form": "아마 학교 계정으로 오피스 사용할 수는 있는데",
          "original_form": "아마 학교 계정으로 오피스 사용할 수는 있는데",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100039390.1.57",
          "form": "항상 온라인이어야하고",
          "original_form": "항상 온라인이어야하고",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100039390.1.58",
          "form": "뭔가... 그냥 없느니만 못하던걸로 기억해요",
          "original_form": "뭔가... 그냥 없느니만 못하던걸로 기억해요",
