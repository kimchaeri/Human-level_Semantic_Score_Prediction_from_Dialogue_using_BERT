      "utterance": [
        {
          "id": "MDRW2100046419.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211020 15:27"
        },
        {
          "id": "MDRW2100046419.1.2",
          "form": "안녕하세요~! ",
          "original_form": "안녕하세요~! ",
          "speaker_id": "2",
          "time": "20211020 15:29"
        },
        {
          "id": "MDRW2100046419.1.3",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "2",
          "time": "20211020 15:29"
        },
        {
          "id": "MDRW2100046419.1.4",
          "form": "아니요 직장인요",
          "original_form": "아니요 직장인요",
          "speaker_id": "1",
          "time": "20211020 15:30"
        },
        {
          "id": "MDRW2100046419.1.5",
          "form": "오호 퇴근 시간이 다가오고 있어요!",
          "original_form": "오호 퇴근 시간이 다가오고 있어요!",
          "speaker_id": "2",
          "time": "20211020 15:31"
        },
        {
          "id": "MDRW2100046419.1.6",
          "form": "정말 지루한 수요일이네요 ㅠㅠ",
          "original_form": "정말 지루한 수요일이네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211020 15:31"
        },
        {
          "id": "MDRW2100046419.1.7",
          "form": "그니까요 ㅋ",
          "original_form": "그니까요 ㅋ",
          "speaker_id": "1",
          "time": "20211020 15:31"
        },
        {
          "id": "MDRW2100046419.1.8",
          "form": "퇴근 후에 하는 취미 있으세요?",
          "original_form": "퇴근 후에 하는 취미 있으세요?",
          "speaker_id": "2",
          "time": "20211020 15:32"
        },
        {
          "id": "MDRW2100046419.1.9",
          "form": "헬스하고 있어요",
          "original_form": "헬스하고 있어요",
          "speaker_id": "1",
          "time": "20211020 15:32"
        },
        {
          "id": "MDRW2100046419.1.10",
          "form": "운동하시는구나",
          "original_form": "운동하시는구나",
          "speaker_id": "2",
          "time": "20211020 15:32"
        },
        {
          "id": "MDRW2100046419.1.11",
          "form": "또 다른 운동 하는 거 있으세요?",
          "original_form": "또 다른 운동 하는 거 있으세요?",
          "speaker_id": "2",
          "time": "20211020 15:32"
        },
        {
          "id": "MDRW2100046419.1.12",
          "form": "다른건 안해요 ㅋㅋ",
          "original_form": "다른건 안해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 15:32"
        },
        {
          "id": "MDRW2100046419.1.13",
          "form": "헬스하신지는 얼마나 되셨어요?",
          "original_form": "헬스하신지는 얼마나 되셨어요?",
          "speaker_id": "2",
          "time": "20211020 16:16"
        },
        {
          "id": "MDRW2100046419.1.14",
          "form": "한 5년했어요",
          "original_form": "한 5년했어요",
          "speaker_id": "1",
          "time": "20211020 16:19"
        },
        {
          "id": "MDRW2100046419.1.15",
          "form": "슬슬 헬스도 질린다는",
          "original_form": "슬슬 헬스도 질린다는",
          "speaker_id": "1",
          "time": "20211020 16:22"
        },
        {
          "id": "MDRW2100046419.1.16",
          "form": "헐 엄청 오래하셨네요",
          "original_form": "헐 엄청 오래하셨네요",
          "speaker_id": "2",
          "time": "20211020 16:31"
        },
        {
          "id": "MDRW2100046419.1.17",
          "form": "저도 헬스했었는데 저는 의지가 박약해서 안가게 되더라고요",
          "original_form": "저도 헬스했었는데 저는 의지가 박약해서 안가게 되더라고요",
          "speaker_id": "2",
          "time": "20211020 16:31"
        },
        {
          "id": "MDRW2100046419.1.18",
          "form": "매일 갈 수 있는 꿀팁 있나요",
          "original_form": "매일 갈 수 있는 꿀팁 있나요",
          "speaker_id": "2",
          "time": "20211020 16:32"
        },
        {
          "id": "MDRW2100046419.1.19",
          "form": "저도 매일은 안가요 일주일에 두세번요 ㅋㅋ",
          "original_form": "저도 매일은 안가요 일주일에 두세번요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 16:33"
        },
        {
          "id": "MDRW2100046419.1.20",
          "form": "채팅은 언제까지 해야하나용?",
          "original_form": "채팅은 언제까지 해야하나용?",
          "speaker_id": "1",
          "time": "20211020 16:44"
        }
      ]
