      "utterance": [
        {
          "id": "MDRW2100014756.1.1",
          "form": "엄마 나 설거지 귀찮아",
          "original_form": "엄마 나 설거지 귀찮아",
          "speaker_id": "1",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014756.1.2",
          "form": "그동안 애썼다",
          "original_form": "그동안 애썼다",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014756.1.3",
          "form": "하지만 엄마를 사랑해",
          "original_form": "하지만 엄마를 사랑해",
          "speaker_id": "1",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014756.1.4",
          "form": "손목도 안좋은데",
          "original_form": "손목도 안좋은데",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014756.1.5",
          "form": "너밖에 없다.",
          "original_form": "너밖에 없다.",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014756.1.6",
          "form": "엄마가 그동안 가사 혼자 다 했잖아",
          "original_form": "엄마가 그동안 가사 혼자 다 했잖아",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014756.1.7",
          "form": "당연한거지",
          "original_form": "당연한거지",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014756.1.8",
          "form": "아니야 고생했어 손가락도 안좋은데 많이 힘들었을텐데 가족을 위해서 수고했어",
          "original_form": "아니야 고생했어 손가락도 안좋은데 많이 힘들었을텐데 가족을 위해서 수고했어",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014756.1.9",
          "form": "주부 사표 많이 내고 싶었다",
          "original_form": "주부 사표 많이 내고 싶었다",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014756.1.10",
          "form": "이제 가족들이 엄마 많이 도와줄게",
          "original_form": "이제 가족들이 엄마 많이 도와줄게",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014756.1.11",
          "form": "고맙다",
          "original_form": "고맙다",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014756.1.12",
          "form": "집안일은 매일 새로 생기니까 넘 힘든것같아",
          "original_form": "집안일은 매일 새로 생기니까 넘 힘든것같아",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014756.1.13",
          "form": "맞아 끝이 안보여",
          "original_form": "맞아 끝이 안보여",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014756.1.14",
          "form": "오늘도 쓰레기통 안비워서 내일 버려야해",
          "original_form": "오늘도 쓰레기통 안비워서 내일 버려야해",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014756.1.15",
          "form": "같이 버리러 가자",
          "original_form": "같이 버리러 가자",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014756.1.16",
          "form": "그래 비오기전에 가자",
          "original_form": "그래 비오기전에 가자",
          "speaker_id": "1",
          "time": "20210825 20:57"
        }
