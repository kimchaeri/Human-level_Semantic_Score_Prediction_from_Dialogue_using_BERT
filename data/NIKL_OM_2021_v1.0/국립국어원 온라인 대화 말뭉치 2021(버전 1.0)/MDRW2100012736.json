      "utterance": [
        {
          "id": "MDRW2100012736.1.1",
          "form": "스포츠좋아하세요?",
          "original_form": "스포츠좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 00:28"
        },
        {
          "id": "MDRW2100012736.1.2",
          "form": "스포츠 경기 보는걸 좋아해요 그중에 축구요~",
          "original_form": "스포츠 경기 보는걸 좋아해요 그중에 축구요~",
          "speaker_id": "2",
          "time": "20210825 00:29"
        },
        {
          "id": "MDRW2100012736.1.3",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100012736.1.4",
          "form": "epl보세요?",
          "original_form": "epl보세요?",
          "speaker_id": "1",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100012736.1.5",
          "form": "당연하죠 ㅋㅋ 프리미어리그 경기영상 하이라이트만 봐요 ㅋㅋ",
          "original_form": "당연하죠 ㅋㅋ 프리미어리그 경기영상 하이라이트만 봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100012736.1.6",
          "form": "근데 요즘 파리 생제르맹 축구경기만 보는거같아요",
          "original_form": "근데 요즘 파리 생제르맹 축구경기만 보는거같아요",
          "speaker_id": "2",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100012736.1.7",
          "form": "프리미어리그 축구 경기 보나요?",
          "original_form": "프리미어리그 축구 경기 보나요?",
          "speaker_id": "2",
          "time": "20210825 00:44"
        },
        {
          "id": "MDRW2100012736.1.8",
          "form": "ㅋㅋㅋ 네네 저는 epl봐요",
          "original_form": "ㅋㅋㅋ 네네 저는 epl봐요",
          "speaker_id": "1",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100012736.1.9",
          "form": "메시가 생제르망 갔던가요? 메시팬이예요?",
          "original_form": "메시가 생제르망 갔던가요? 메시팬이예요?",
          "speaker_id": "1",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100012736.1.10",
          "form": "팬까지는 아니고 생제르망 간다고 해서 팀 선수들 보니",
          "original_form": "팬까지는 아니고 생제르망 간다고 해서 팀 선수들 보니",
          "speaker_id": "2",
          "time": "20210825 00:54"
        },
        {
          "id": "MDRW2100012736.1.11",
          "form": "다들어본 선수들이라 관심이 가게됬어요",
          "original_form": "다들어본 선수들이라 관심이 가게됬어요",
          "speaker_id": "2",
          "time": "20210825 00:54"
        },
        {
          "id": "MDRW2100012736.1.12",
          "form": "이번에 기대되는 팀입니다.",
          "original_form": "이번에 기대되는 팀입니다.",
          "speaker_id": "2",
          "time": "20210825 01:00"
        },
        {
          "id": "MDRW2100012736.1.13",
          "form": "그쪽은 어느 팀이 잘할거같아요?",
          "original_form": "그쪽은 어느 팀이 잘할거같아요?",
          "speaker_id": "2",
          "time": "20210825 01:00"
        },
        {
          "id": "MDRW2100012736.1.14",
          "form": "메시가있으니 생제르망이 잘하겠네요",
          "original_form": "메시가있으니 생제르망이 잘하겠네요",
          "speaker_id": "1",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012736.1.15",
          "form": "저는 라리가는 잘안봐서요ㅋㅋㅋ",
          "original_form": "저는 라리가는 잘안봐서요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012736.1.16",
          "form": "epl좋아해요 토트넘",
          "original_form": "epl좋아해요 토트넘",
          "speaker_id": "1",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012736.1.17",
          "form": "손흥민있는 팀이군요 축구경기도 자주 보시겠네요",
          "original_form": "손흥민있는 팀이군요 축구경기도 자주 보시겠네요",
          "speaker_id": "2",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100012736.1.18",
          "form": "유튜만 틀어도 손흥민 경기영상 떠서  몇번 보긴했는데",
          "original_form": "유튜만 틀어도 손흥민 경기영상 떠서  몇번 보긴했는데",
          "speaker_id": "2",
          "time": "20210825 01:07"
        },
        {
          "id": "MDRW2100012736.1.19",
          "form": "축구경기영상은 얼마나 보세요?",
          "original_form": "축구경기영상은 얼마나 보세요?",
          "speaker_id": "2",
          "time": "20210825 01:08"
        },
        {
          "id": "MDRW2100012736.1.20",
          "form": "최근에 리그개막해서",
          "original_form": "최근에 리그개막해서",
          "speaker_id": "1",
          "time": "20210825 01:15"
        },
        {
          "id": "MDRW2100012736.1.21",
          "form": "이번 epl 손흥민경기는",
          "original_form": "이번 epl 손흥민경기는",
          "speaker_id": "1",
          "time": "20210825 01:16"
        },
        {
          "id": "MDRW2100012736.1.22",
          "form": "다보려고요ㅋㅋㅋㅋ",
          "original_form": "다보려고요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:16"
        },
        {
          "id": "MDRW2100012736.1.23",
          "form": "이번에 2승해서 출발은 괜찮고 게속 좋아야할텐데",
          "original_form": "이번에 2승해서 출발은 괜찮고 게속 좋아야할텐데",
          "speaker_id": "2",
          "time": "20210825 01:22"
        },
        {
          "id": "MDRW2100012736.1.24",
          "form": "근데 공격수가",
          "original_form": "근데 공격수가",
          "speaker_id": "1",
          "time": "20210825 01:27"
        },
        {
          "id": "MDRW2100012736.1.25",
          "form": "좀 있어야겠더라고요",
          "original_form": "좀 있어야겠더라고요",
          "speaker_id": "1",
          "time": "20210825 01:27"
        },
        {
          "id": "MDRW2100012736.1.26",
          "form": "토트넘 이번에 기대해봐도 된다고 생각하시나요? ㅋㅋ",
          "original_form": "토트넘 이번에 기대해봐도 된다고 생각하시나요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:28"
        },
        {
          "id": "MDRW2100012736.1.27",
          "form": "ㅋㅋㅋ 근데",
          "original_form": "ㅋㅋㅋ 근데",
          "speaker_id": "1",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100012736.1.28",
          "form": "ㅋㅋㅋ 음",
          "original_form": "ㅋㅋㅋ 음",
          "speaker_id": "1",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100012736.1.29",
          "form": "뭔가 영입이 있어야된다고봅니다",
          "original_form": "뭔가 영입이 있어야된다고봅니다",
          "speaker_id": "1",
          "time": "20210825 01:35"
        },
        {
          "id": "MDRW2100012736.1.30",
          "form": "영입이 있어야한다고봐요ㅎㅎㅎ",
          "original_form": "영입이 있어야한다고봐요ㅎㅎㅎ",
