      "utterance": [
        {
          "id": "MDRW2100038699.1.1",
          "form": "세상에 멋진분ㅜ",
          "original_form": "세상에 멋진분ㅜ",
          "speaker_id": "2",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100038699.1.2",
          "form": "복받으실 사장님",
          "original_form": "복받으실 사장님",
          "speaker_id": "2",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100038699.1.3",
          "form": "요새는 얼마에요",
          "original_form": "요새는 얼마에요",
          "speaker_id": "2",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100038699.1.4",
          "form": "대학땐 그래도 좀 싼편이었거든요",
          "original_form": "대학땐 그래도 좀 싼편이었거든요",
          "speaker_id": "2",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100038699.1.5",
          "form": "3번세트 19900원이래요  오르긴 쫌 올랐어요",
          "original_form": "3번세트 19900원이래요  오르긴 쫌 올랐어요",
          "speaker_id": "1",
          "time": "20210902 19:40"
        },
        {
          "id": "MDRW2100038699.1.6",
          "form": "요새 진짜 치킨값 금값이네요 ㄷㄷ",
          "original_form": "요새 진짜 치킨값 금값이네요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100038699.1.7",
          "form": "저는 그거 혼자서도 다먹겠던데 솔직히 ㅋㅋ",
          "original_form": "저는 그거 혼자서도 다먹겠던데 솔직히 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100038699.1.8",
          "form": "떡볶이도 넘 비싸고...",
          "original_form": "떡볶이도 넘 비싸고...",
          "speaker_id": "1",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100038699.1.9",
          "form": "헉 저는 조금 느끼해서 쉬고이써요ㅋㅋ",
          "original_form": "헉 저는 조금 느끼해서 쉬고이써요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100038699.1.10",
          "form": "저는 좀 많이 대식가라서",
          "original_form": "저는 좀 많이 대식가라서",
          "speaker_id": "2",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100038699.1.11",
          "form": "기본 햄버거 2세트이상",
          "original_form": "기본 햄버거 2세트이상",
          "speaker_id": "2",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100038699.1.12",
          "form": "피자 배달피자는 혼자서 한판도 먹어요 ㅋㅋ",
          "original_form": "피자 배달피자는 혼자서 한판도 먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100038699.1.13",
          "form": "와 저는 한셋 최대인데",
          "original_form": "와 저는 한셋 최대인데",
          "speaker_id": "1",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100038699.1.14",
          "form": "저를 기준으로 잡으심 안돼요ㅜㅜ",
          "original_form": "저를 기준으로 잡으심 안돼요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100038699.1.15",
          "form": "머싯어요 ㄷㄷ",
          "original_form": "머싯어요 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100038699.1.16",
          "form": "피자는 3~4조각 ㅋㅋ",
          "original_form": "피자는 3~4조각 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100038699.1.17",
          "form": "혼자시켜 먹오서 맨날 담날에 맛없는거 먹어야하는 ㅜㅜ",
          "original_form": "혼자시켜 먹오서 맨날 담날에 맛없는거 먹어야하는 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100038699.1.18",
          "form": "아뇨 덕분에 세달만에 15키로 쪘어요ㅜ",
          "original_form": "아뇨 덕분에 세달만에 15키로 쪘어요ㅜ",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100038699.1.19",
          "form": "그래서 요새 집에서 구박 받아요ㅜㅜ",
          "original_form": "그래서 요새 집에서 구박 받아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100038699.1.20",
          "form": "헉 세달사이 무슨일이?!",
          "original_form": "헉 세달사이 무슨일이?!",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100038699.1.21",
          "form": "갑자기 식욕 폭발하신걸까요",
          "original_form": "갑자기 식욕 폭발하신걸까요",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100038699.1.22",
          "form": "제가 몇년동안 1일1식 유니시티 각종 다이어트쉐이크 등등으로",
          "original_form": "제가 몇년동안 1일1식 유니시티 각종 다이어트쉐이크 등등으로",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100038699.1.23",
          "form": "15키로뺐거든요",
          "original_form": "15키로뺐거든요",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100038699.1.24",
          "form": "몇년에걸쳐서",
          "original_form": "몇년에걸쳐서",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100038699.1.25",
          "form": "그때도 집에서 구박이란 구박다받고",
          "original_form": "그때도 집에서 구박이란 구박다받고",
          "speaker_id": "2",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100038699.1.26",
          "form": "돈이란 돈은다쓰고",
          "original_form": "돈이란 돈은다쓰고",
          "speaker_id": "2",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100038699.1.27",
          "form": "잔소리듣고 뺐죠",
          "original_form": "잔소리듣고 뺐죠",
          "speaker_id": "2",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100038699.1.28",
          "form": "요요가 왔을까요 ㅠㅠ",
          "original_form": "요요가 왔을까요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100038699.1.29",
          "form": "너무해요 살쪘다고 구박하시다니",
          "original_form": "너무해요 살쪘다고 구박하시다니",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100038699.1.30",
          "form": "그러다가 폭발해서",
          "original_form": "그러다가 폭발해서",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100038699.1.31",
          "form": "내맘대로할거다",
          "original_form": "내맘대로할거다",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100038699.1.32",
          "form": "나한테신경꺼라하고",
          "original_form": "나한테신경꺼라하고",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100038699.1.33",
          "form": "지맘대로살고 세달만에 다시 찌웠습니다 ㄷㄷㄷ",
          "original_form": "지맘대로살고 세달만에 다시 찌웠습니다 ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100038699.1.34",
          "form": "왠지 속시원한데요",
          "original_form": "왠지 속시원한데요",
          "speaker_id": "1",
          "time": "20210902 19:55"
        }
      ]
    }
