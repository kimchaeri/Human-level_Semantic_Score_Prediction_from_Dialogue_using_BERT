      "utterance": [
        {
          "id": "MDRW2100015944.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 03:28"
        },
        {
          "id": "MDRW2100015944.1.2",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210826 03:28"
        },
        {
          "id": "MDRW2100015944.1.3",
          "form": "모임 나가시는거 있으신가용?",
          "original_form": "모임 나가시는거 있으신가용?",
          "speaker_id": "1",
          "time": "20210826 03:29"
        },
        {
          "id": "MDRW2100015944.1.4",
          "form": "아니요 코로나때문에",
          "original_form": "아니요 코로나때문에",
          "speaker_id": "2",
          "time": "20210826 03:29"
        },
        {
          "id": "MDRW2100015944.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210826 03:29"
        },
        {
          "id": "MDRW2100015944.1.6",
          "form": "아앗 저도 없어요ㅜㅜㅋㅋ",
          "original_form": "아앗 저도 없어요ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 03:30"
        },
        {
          "id": "MDRW2100015944.1.7",
          "form": "근데 코로나 전에도 딱히 없었던것같아요",
          "original_form": "근데 코로나 전에도 딱히 없었던것같아요",
          "speaker_id": "1",
          "time": "20210826 03:30"
        },
        {
          "id": "MDRW2100015944.1.8",
          "form": "대신 인터넷으로 동아리? 카페? 같은건 가입했어요!",
          "original_form": "대신 인터넷으로 동아리? 카페? 같은건 가입했어요!",
          "speaker_id": "1",
          "time": "20210826 03:30"
        },
        {
          "id": "MDRW2100015944.1.9",
          "form": "저는 배구 동호회 나가보고싶네요",
          "original_form": "저는 배구 동호회 나가보고싶네요",
          "speaker_id": "2",
          "time": "20210826 03:31"
        },
        {
          "id": "MDRW2100015944.1.10",
          "form": "배구 좋아하시는구나!",
          "original_form": "배구 좋아하시는구나!",
          "speaker_id": "1",
          "time": "20210826 03:32"
        },
        {
          "id": "MDRW2100015944.1.11",
          "form": "사람들 어울려서 좋죠ㅎㅎ",
          "original_form": "사람들 어울려서 좋죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 03:32"
        },
        {
          "id": "MDRW2100015944.1.12",
          "form": "올림픽보고 좋아졌어요",
          "original_form": "올림픽보고 좋아졌어요",
          "speaker_id": "2",
          "time": "20210826 03:33"
        },
        {
          "id": "MDRW2100015944.1.13",
          "form": "아! 저도 봤어요ㅎㅎ",
          "original_form": "아! 저도 봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 03:34"
        },
        {
          "id": "MDRW2100015944.1.14",
          "form": "배구 동호회면 회원들끼리 합이 중요한것같아요",
          "original_form": "배구 동호회면 회원들끼리 합이 중요한것같아요",
          "speaker_id": "1",
          "time": "20210826 03:34"
        },
        {
          "id": "MDRW2100015944.1.15",
          "form": "봉사 해보셨나요?",
          "original_form": "봉사 해보셨나요?",
          "speaker_id": "2",
          "time": "20210826 03:34"
        },
        {
          "id": "MDRW2100015944.1.16",
          "form": "학생때 말고는 안했네요",
          "original_form": "학생때 말고는 안했네요",
          "speaker_id": "1",
          "time": "20210826 03:34"
        },
        {
          "id": "MDRW2100015944.1.17",
          "form": "반성합니더ㅠ",
          "original_form": "반성합니더ㅠ",
          "speaker_id": "1",
          "time": "20210826 03:34"
        },
        {
          "id": "MDRW2100015944.1.18",
          "form": "하고싶으면 하는거죵",
          "original_form": "하고싶으면 하는거죵",
          "speaker_id": "2",
          "time": "20210826 03:35"
        },
        {
          "id": "MDRW2100015944.1.19",
          "form": "봉사하면서 살아야할것같아요...",
          "original_form": "봉사하면서 살아야할것같아요...",
          "speaker_id": "1",
          "time": "20210826 03:35"
        },
        {
          "id": "MDRW2100015944.1.20",
          "form": "너무 안했네요ㅋㅋ",
          "original_form": "너무 안했네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 03:35"
        },
        {
          "id": "MDRW2100015944.1.21",
          "form": "그럴수도있죠",
          "original_form": "그럴수도있죠",
          "speaker_id": "2",
          "time": "20210826 03:36"
        }
      ]
    }
