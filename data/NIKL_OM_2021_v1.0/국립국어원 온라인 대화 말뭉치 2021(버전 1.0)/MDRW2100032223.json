      "utterance": [
        {
          "id": "MDRW2100032223.1.1",
          "form": "저희 지역은 가을이라고 좀 확 추워졌는데",
          "original_form": "저희 지역은 가을이라고 좀 확 추워졌는데",
          "speaker_id": "1",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100032223.1.2",
          "form": "name2님 쪽은 괜찮으신가요??",
          "original_form": "&name2&님 쪽은 괜찮으신가요??",
          "speaker_id": "1",
          "time": "20210905 14:12"
        },
        {
          "id": "MDRW2100032223.1.3",
          "form": "여기도 추워용",
          "original_form": "여기도 추워용",
          "speaker_id": "2",
          "time": "20210905 14:12"
        },
        {
          "id": "MDRW2100032223.1.4",
          "form": "근데 어제 백신 맞아서",
          "original_form": "근데 어제 백신 맞아서",
          "speaker_id": "2",
          "time": "20210905 14:12"
        },
        {
          "id": "MDRW2100032223.1.5",
          "form": "따땃한 이불속에",
          "original_form": "따땃한 이불속에",
          "speaker_id": "2",
          "time": "20210905 14:12"
        },
        {
          "id": "MDRW2100032223.1.6",
          "form": "계속 있답니당",
          "original_form": "계속 있답니당",
          "speaker_id": "2",
          "time": "20210905 14:12"
        },
        {
          "id": "MDRW2100032223.1.7",
          "form": "아 어제 맞고 오신거예요??",
          "original_form": "아 어제 맞고 오신거예요??",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032223.1.8",
          "form": "몸 상태가 좀 안좋아진다고 들었어요",
          "original_form": "몸 상태가 좀 안좋아진다고 들었어요",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032223.1.9",
          "form": "넹 2차 맞았는데",
          "original_form": "넹 2차 맞았는데",
          "speaker_id": "2",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032223.1.10",
          "form": "몸이 좀 안좋아서",
          "original_form": "몸이 좀 안좋아서",
          "speaker_id": "2",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032223.1.11",
          "form": "어제 하루종일 잤어요ㅠ",
          "original_form": "어제 하루종일 잤어요ㅠ",
          "speaker_id": "2",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032223.1.12",
          "form": "헐 ㅠㅠ 지금은 괜찮으신가요??",
          "original_form": "헐 ㅠㅠ 지금은 괜찮으신가요??",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032223.1.13",
          "form": "어제 날씨 진짜 좋았다던데 아쉬우시겠어요 ㅠㅠ",
          "original_form": "어제 날씨 진짜 좋았다던데 아쉬우시겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032223.1.14",
          "form": "지금도 그닥 컨디션이 좋지는 않아서",
          "original_form": "지금도 그닥 컨디션이 좋지는 않아서",
          "speaker_id": "2",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032223.1.15",
          "form": "낼부터는",
          "original_form": "낼부터는",
          "speaker_id": "2",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032223.1.16",
          "form": "긴팔입고",
          "original_form": "긴팔입고",
          "speaker_id": "2",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032223.1.17",
          "form": "출근하려구용",
          "original_form": "출근하려구용",
          "speaker_id": "2",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032223.1.18",
          "form": "아 출근도 하셔야하죠 ㅠㅠ",
          "original_form": "아 출근도 하셔야하죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 14:16"
        },
        {
          "id": "MDRW2100032223.1.19",
          "form": "환절기라서 더 걱정돼요",
          "original_form": "환절기라서 더 걱정돼요",
          "speaker_id": "1",
          "time": "20210905 14:16"
        },
        {
          "id": "MDRW2100032223.1.20",
          "form": "아침에 출근할때는 진짜 춥잖아요",
          "original_form": "아침에 출근할때는 진짜 춥잖아요",
          "speaker_id": "1",
          "time": "20210905 14:16"
        },
        {
          "id": "MDRW2100032223.1.21",
          "form": "생각보다는 더워용",
          "original_form": "생각보다는 더워용",
          "speaker_id": "2",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100032223.1.22",
          "form": "근데 겨울은 진짜...엄청 차이가 심해요",
          "original_form": "근데 겨울은 진짜...엄청 차이가 심해요",
          "speaker_id": "2",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100032223.1.23",
          "form": "아 겨울",
          "original_form": "아 겨울",
          "speaker_id": "1",
          "time": "20210905 14:19"
        },
        {
          "id": "MDRW2100032223.1.24",
          "form": "이번 여름이 엄청 더웠잖아요",
          "original_form": "이번 여름이 엄청 더웠잖아요",
          "speaker_id": "1",
          "time": "20210905 14:19"
        },
        {
          "id": "MDRW2100032223.1.25",
          "form": "그래서 이번 겨울은 또 얼마나 추울지.. 어휴...",
          "original_form": "그래서 이번 겨울은 또 얼마나 추울지.. 어휴...",
          "speaker_id": "1",
          "time": "20210905 14:19"
        },
        {
          "id": "MDRW2100032223.1.26",
          "form": "코트를 슬슬",
          "original_form": "코트를 슬슬",
          "speaker_id": "2",
          "time": "20210905 16:10"
        },
        {
          "id": "MDRW2100032223.1.27",
          "form": "살때가 되었어요",
          "original_form": "살때가 되었어요",
          "speaker_id": "2",
          "time": "20210905 16:10"
        },
        {
          "id": "MDRW2100032223.1.28",
          "form": "설마 얼죽코..? 신가요?",
          "original_form": "설마 얼죽코..? 신가요?",
          "speaker_id": "1",
          "time": "20210905 16:11"
        },
        {
          "id": "MDRW2100032223.1.29",
          "form": "아뇨 가을엔 코트죠ㅋㅋㅋㅋ",
          "original_form": "아뇨 가을엔 코트죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100032223.1.30",
          "form": "겨울은",
          "original_form": "겨울은",
          "speaker_id": "2",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100032223.1.31",
          "form": "무적권 롱패딩",
          "original_form": "무적권 롱패딩",
          "speaker_id": "2",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100032223.1.32",
          "form": "진짜 겨울에는 롱패딩 아니면 힘들어요 ㅠㅠ",
          "original_form": "진짜 겨울에는 롱패딩 아니면 힘들어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100032223.1.33",
          "form": "이번 겨울도 롱패팅으로 살아남아봅시다 !",
          "original_form": "이번 겨울도 롱패팅으로 살아남아봅시다 !",
          "speaker_id": "1",
          "time": "20210905 20:54"
        }
      ]
