      "utterance": [
        {
          "id": "MDRW2100046102.1.1",
          "form": "네 꼬들 파 입니다",
          "original_form": "네 꼬들 파 입니다",
          "speaker_id": "2",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046102.1.2",
          "form": "우아 저도욬ㅋㅋㅋㅋ",
          "original_form": "우아 저도욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046102.1.3",
          "form": "예전에는 퍼진 걸 좋아했는데",
          "original_form": "예전에는 퍼진 걸 좋아했는데",
          "speaker_id": "2",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046102.1.4",
          "form": "제가 끓이면 안되드라구요 ㅠ",
          "original_form": "제가 끓이면 안되드라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046102.1.5",
          "form": "입맛이 바뀌더라구요",
          "original_form": "입맛이 바뀌더라구요",
          "speaker_id": "2",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046102.1.6",
          "form": "퍼진것도 좋긴해여 ㅋㅋㅋ",
          "original_form": "퍼진것도 좋긴해여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046102.1.7",
          "form": "ㅎㅎㅎㅎㅎㅎㅎ 시간이 중요하죠",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎ 시간이 중요하죠",
          "speaker_id": "2",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046102.1.8",
          "form": "물 맞추는게 어려워요 ㅠㅠ",
          "original_form": "물 맞추는게 어려워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046102.1.9",
          "form": "라면은 뭐 다 맛있으니 ㅎㅎ",
          "original_form": "라면은 뭐 다 맛있으니 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046102.1.10",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046102.1.11",
          "form": "예전엔 공기를 넣어서 만들라구 ㅋㅋㅋ",
          "original_form": "예전엔 공기를 넣어서 만들라구 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046102.1.12",
          "form": "커피 좋아하세요??",
          "original_form": "커피 좋아하세요??",
          "speaker_id": "2",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046102.1.13",
          "form": "저는 달달한것만 좋아해요",
          "original_form": "저는 달달한것만 좋아해요",
          "speaker_id": "1",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046102.1.14",
          "form": "아메리카노 드세요?",
          "original_form": "아메리카노 드세요?",
          "speaker_id": "1",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046102.1.15",
          "form": "우와 저두요",
          "original_form": "우와 저두요",
          "speaker_id": "2",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046102.1.16",
          "form": "아니요 커피는 좋아하는데",
          "original_form": "아니요 커피는 좋아하는데",
          "speaker_id": "2",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046102.1.17",
          "form": "바닐라 라떼가 젤 좋아요 ㅋㅋ",
          "original_form": "바닐라 라떼가 젤 좋아요 ㅋㅋ",
