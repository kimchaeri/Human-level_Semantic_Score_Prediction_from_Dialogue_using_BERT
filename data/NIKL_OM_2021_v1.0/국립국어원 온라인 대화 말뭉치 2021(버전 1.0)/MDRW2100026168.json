      "utterance": [
        {
          "id": "MDRW2100026168.1.1",
          "form": "코스개발을 좀 해야겠어요",
          "original_form": "코스개발을 좀 해야겠어요",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026168.1.2",
          "form": "^^",
          "original_form": "^^",
          "speaker_id": "2",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026168.1.3",
          "form": "최근에 책 보신거 있으신가요?",
          "original_form": "최근에 책 보신거 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100026168.1.4",
          "form": "책이라.. 학생문화원 도서관서 빌려봐요",
          "original_form": "책이라.. 학생문화원 도서관서 빌려봐요",
          "speaker_id": "2",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100026168.1.5",
          "form": "곰탕. 여름 어디선가 시체가.",
          "original_form": "곰탕. 여름 어디선가 시체가.",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026168.1.6",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "1",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026168.1.7",
          "form": "소설인가요 무서운데요",
          "original_form": "소설인가요 무서운데요",
          "speaker_id": "1",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026168.1.8",
          "form": "콩가루수사단은 어제 반납했어요ㅋ",
          "original_form": "콩가루수사단은 어제 반납했어요ㅋ",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026168.1.9",
          "form": "다 재밌어요",
          "original_form": "다 재밌어요",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026168.1.10",
          "form": "저도 셜록홈즈같은 추리물 좋아해여",
          "original_form": "저도 셜록홈즈같은 추리물 좋아해여",
          "speaker_id": "1",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026168.1.11",
          "form": "추리입니다",
          "original_form": "추리입니다",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026168.1.12",
          "form": "콩가루 한번 빌려보세요",
          "original_form": "콩가루 한번 빌려보세요",
          "speaker_id": "2",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100026168.1.13",
          "form": "제가 어제 반납했어요",
          "original_form": "제가 어제 반납했어요",
          "speaker_id": "2",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100026168.1.14",
          "form": "일본 히가시노게이노 작가님은 어떠신가요",
          "original_form": "일본 히가시노게이노 작가님은 어떠신가요",
          "speaker_id": "1",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100026168.1.15",
          "form": "어 콩가루 체크하겠습니다",
          "original_form": "어 콩가루 체크하겠습니다",
          "speaker_id": "1",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100026168.1.16",
          "form": "아. 나미야잡화점 좋아해요",
          "original_form": "아. 나미야잡화점 좋아해요",
          "speaker_id": "2",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100026168.1.17",
          "form": "예전책이지만",
          "original_form": "예전책이지만",
          "speaker_id": "2",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100026168.1.18",
          "form": "너무 자주 책내서좀 보다말았죠ㅎ",
          "original_form": "너무 자주 책내서좀 보다말았죠ㅎ",
          "speaker_id": "2",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100026168.1.19",
          "form": "콩가루 검색해봤는데 추천이 많네요",
          "original_form": "콩가루 검색해봤는데 추천이 많네요",
          "speaker_id": "1",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100026168.1.20",
          "form": "불편한편의점도 보세요. 꿈백화점이랑 비슷해요",
          "original_form": "불편한편의점도 보세요. 꿈백화점이랑 비슷해요",
          "speaker_id": "2",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100026168.1.21",
          "form": "네 재미져요ㅎ",
          "original_form": "네 재미져요ㅎ",
          "speaker_id": "2",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100026168.1.22",
          "form": "여름 어디선가 시체가ㅡ 이것도 강추요",
          "original_form": "여름 어디선가 시체가ㅡ 이것도 강추요",
          "speaker_id": "2",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100026168.1.23",
          "form": "알겠슴니다 오늘 전입신고해서 우당도서관 가입해야겠어요",
          "original_form": "알겠슴니다 오늘 전입신고해서 우당도서관 가입해야겠어요",
          "speaker_id": "1",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100026168.1.24",
          "form": "어디서 책 빌려보시나여",
          "original_form": "어디서 책 빌려보시나여",
          "speaker_id": "1",
          "time": "20210901 20:33"
        }
      ]
    }
