      "utterance": [
        {
          "id": "MDRW2100028727.1.1",
          "form": "가족이랑 같이 살고 계시나요?",
          "original_form": "가족이랑 같이 살고 계시나요?",
          "speaker_id": "1",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028727.1.2",
          "form": "집안일은 해도해도 티가나지 않네요",
          "original_form": "집안일은 해도해도 티가나지 않네요",
          "speaker_id": "2",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100028727.1.3",
          "form": "그러니까요 ㅎㅎ 열심히 해도 한 티가 안나는 ㅋㅋ",
          "original_form": "그러니까요 ㅎㅎ 열심히 해도 한 티가 안나는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:04"
        },
        {
          "id": "MDRW2100028727.1.4",
          "form": "화장실청소는 조금 티가 나긴하는데",
          "original_form": "화장실청소는 조금 티가 나긴하는데",
          "speaker_id": "2",
          "time": "20210905 21:07"
        },
        {
          "id": "MDRW2100028727.1.5",
          "form": "청소하기가 귀찮ㅈㅛ",
          "original_form": "청소하기가 귀찮ㅈㅛ",
          "speaker_id": "2",
          "time": "20210905 21:07"
        },
        {
          "id": "MDRW2100028727.1.6",
          "form": "네 바닥 이런 데는 진짜 안 하자니 그렇고 해도 티도 안나는 ㅋㅋ",
          "original_form": "네 바닥 이런 데는 진짜 안 하자니 그렇고 해도 티도 안나는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100028727.1.7",
          "form": "맞아요ㅋㅋㅋ맘같아선 미니멀리스트 하고싶은데 ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ맘같아선 미니멀리스트 하고싶은데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100028727.1.8",
          "form": "진짜 미너멀리스트 젤 신기해요.. 전 그런거 못해서 ㅋㅋㅋ",
          "original_form": "진짜 미너멀리스트 젤 신기해요.. 전 그런거 못해서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100028727.1.9",
          "form": "다 이고 살아요 ㅋㅋㅋ",
          "original_form": "다 이고 살아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100028727.1.10",
          "form": "짐 정리하는것도 몇년째 못하고 있어여ㅋㅋ",
          "original_form": "짐 정리하는것도 몇년째 못하고 있어여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100028727.1.11",
          "form": "그쵸ㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100028727.1.12",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100028727.1.13",
          "form": "맥시멀리스트임ㅋㅋ",
          "original_form": "맥시멀리스트임ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100028727.1.14",
          "form": "저는 이사오면서 거의 반을 버린거 같아요 큰맘먹고..",
          "original_form": "저는 이사오면서 거의 반을 버린거 같아요 큰맘먹고..",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100028727.1.15",
          "form": "그래도 많아요 ㅋㅋㅋ",
          "original_form": "그래도 많아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100028727.1.16",
          "form": "ㅋㅋ 그쵸",
          "original_form": "ㅋㅋ 그쵸",
          "speaker_id": "2",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100028727.1.17",
          "form": "저는 이사는아니고 리모델링 하면서 버렸는데도",
          "original_form": "저는 이사는아니고 리모델링 하면서 버렸는데도",
          "speaker_id": "2",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100028727.1.18",
          "form": "넘쳐흘러여",
          "original_form": "넘쳐흘러여",
          "speaker_id": "2",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100028727.1.19",
          "form": "진짜 짐정리는 한번씩 해야겠더라고요",
          "original_form": "진짜 짐정리는 한번씩 해야겠더라고요",
          "speaker_id": "1",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100028727.1.20",
          "form": "다 꺼내서 뒤집어보고..",
          "original_form": "다 꺼내서 뒤집어보고..",
          "speaker_id": "1",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100028727.1.21",
          "form": "진짜2년에 한번은 해줘야해요 특히 옷정리",
          "original_form": "진짜2년에 한번은 해줘야해요 특히 옷정리",
          "speaker_id": "2",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100028727.1.22",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100028727.1.23",
          "form": "옷은 정말.. 안 입는 옷이 너무 많으니까",
          "original_form": "옷은 정말.. 안 입는 옷이 너무 많으니까",
          "speaker_id": "1",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100028727.1.24",
          "form": "일단 수납부터 너무 힘들어요 ㅋㅋ",
          "original_form": "일단 수납부터 너무 힘들어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100028727.1.25",
          "form": "",
          "original_form": "",
          "speaker_id": "2",
          "time": "20210905 21:22"
        },
        {
          "id": "MDRW2100028727.1.26",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210905 21:22"
        },
        {
          "id": "MDRW2100028727.1.27",
          "form": "저 봄에 안입는거 버렸어요 ㅎ",
          "original_form": "저 봄에 안입는거 버렸어요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100028727.1.28",
          "form": "붙박이장 안에 다 넣는걸 목표로",
          "original_form": "붙박이장 안에 다 넣는걸 목표로",
          "speaker_id": "2",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100028727.1.29",
          "form": "지금 보니깐 더 버려야겠어요",
          "original_form": "지금 보니깐 더 버려야겠어요",
          "speaker_id": "2",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100028727.1.30",
          "form": "ㅎㅎ 저는 당근에 팔고 아름다운가게 기부하고 그랬네요 ㅋㅋ",
          "original_form": "ㅎㅎ 저는 당근에 팔고 아름다운가게 기부하고 그랬네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100028727.1.31",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100028727.1.32",
          "form": "저는 옷 한번도 안팔아봤어요",
          "original_form": "저는 옷 한번도 안팔아봤어요",
          "speaker_id": "2",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100028727.1.33",
          "form": "잘팔리나요?",
          "original_form": "잘팔리나요?",
          "speaker_id": "2",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100028727.1.34",
          "form": "막 가격을 엄청 낮게 해서 ㅋㅋㅋ",
          "original_form": "막 가격을 엄청 낮게 해서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100028727.1.35",
          "form": "아하! 그런방법이",
          "original_form": "아하! 그런방법이",
          "speaker_id": "2",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100028727.1.36",
          "form": "저도 나중에 코트에 미련을 버리면 팔아봐야겠어요 거의 새거라서",
          "original_form": "저도 나중에 코트에 미련을 버리면 팔아봐야겠어요 거의 새거라서",
          "speaker_id": "2",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100028727.1.37",
          "form": "네 어차피 버릴거면 그렇게 파는 것도 방법이더라고요",
          "original_form": "네 어차피 버릴거면 그렇게 파는 것도 방법이더라고요",
          "speaker_id": "1",
          "time": "20210905 21:55"
        }
      ]
    }
