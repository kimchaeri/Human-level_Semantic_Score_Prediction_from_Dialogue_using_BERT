      "utterance": [
        {
          "id": "MDRW2100032310.1.1",
          "form": "머리아프겠다",
          "original_form": "머리아프겠다",
          "speaker_id": "2",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100032310.1.2",
          "form": "속터지지않나??",
          "original_form": "속터지지않나??",
          "speaker_id": "1",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100032310.1.3",
          "form": "팀장??",
          "original_form": "팀장??",
          "speaker_id": "1",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100032310.1.4",
          "form": "니는 사람을 평가를 해야한다고 생각하나 평을 해야한다고 생각하나?",
          "original_form": "니는 사람을 평가를 해야한다고 생각하나 평을 해야한다고 생각하나?",
          "speaker_id": "1",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100032310.1.5",
          "form": "니가 누군가를 이야기할때",
          "original_form": "니가 누군가를 이야기할때",
          "speaker_id": "1",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100032310.1.6",
          "form": "당연히 평해야지",
          "original_form": "당연히 평해야지",
          "speaker_id": "2",
          "time": "20210905 15:18"
        },
        {
          "id": "MDRW2100032310.1.7",
          "form": "근데 저런사람많다..",
          "original_form": "근데 저런사람많다..",
          "speaker_id": "2",
          "time": "20210905 15:18"
        },
        {
          "id": "MDRW2100032310.1.8",
          "form": "근데 기안에다가 평가라고 안 썼다고 난리났단다",
          "original_form": "근데 기안에다가 평가라고 안 썼다고 난리났단다",
          "speaker_id": "1",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100032310.1.9",
          "form": "평이라는 단어가 어디있나고...평가지 이렇게",
          "original_form": "평이라는 단어가 어디있나고...평가지 이렇게",
          "speaker_id": "1",
          "time": "20210905 15:29"
        },
        {
          "id": "MDRW2100032310.1.10",
          "form": "이런식으로 이상한 사람이다ㅜ",
          "original_form": "이런식으로 이상한 사람이다ㅜ",
          "speaker_id": "1",
          "time": "20210905 15:29"
        },
        {
          "id": "MDRW2100032310.1.11",
          "form": "맞나 ㅜ 힘드시겠네",
          "original_form": "맞나 ㅜ 힘드시겠네",
          "speaker_id": "2",
          "time": "20210905 15:37"
        },
        {
          "id": "MDRW2100032310.1.12",
          "form": "말이 안통하고 지가 다 옳다다ㅜ",
          "original_form": "말이 안통하고 지가 다 옳다다ㅜ",
          "speaker_id": "1",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032310.1.13",
          "form": "그래 옛날사람들이  다그랬다",
          "original_form": "그래 옛날사람들이  다그랬다",
          "speaker_id": "2",
          "time": "20210905 15:57"
        },
        {
          "id": "MDRW2100032310.1.14",
          "form": "몇살 먹지도 않았다 50대초반이다",
          "original_form": "몇살 먹지도 않았다 50대초반이다",
          "speaker_id": "1",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100032310.1.15",
          "form": "70년인가 그럴걸",
          "original_form": "70년인가 그럴걸",
          "speaker_id": "1",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100032310.1.16",
          "form": "그래 ㅜ 회사다니면 그런인간들많다",
          "original_form": "그래 ㅜ 회사다니면 그런인간들많다",
          "speaker_id": "2",
          "time": "20210905 16:25"
        },
        {
          "id": "MDRW2100032310.1.17",
          "form": "나ㅜ아직까지 회사악몽꾼다",
          "original_form": "나ㅜ아직까지 회사악몽꾼다",
          "speaker_id": "2",
          "time": "20210905 16:25"
        },
        {
          "id": "MDRW2100032310.1.18",
          "form": "맞나 누구??",
          "original_form": "맞나 누구??",
          "speaker_id": "1",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100032310.1.19",
          "form": "나는 명륜진사갈비 왔다",
          "original_form": "나는 명륜진사갈비 왔다",
          "speaker_id": "1",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100032310.1.20",
          "form": "화해했다",
          "original_form": "화해했다",
          "speaker_id": "1",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100032310.1.21",
          "form": "ㅋㅋ잘했다",
          "original_form": "ㅋㅋ잘했다",
          "speaker_id": "2",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100032310.1.22",
          "form": "우린 금산삼계탕가서  신랑은 삼계탕\r\n우리는 고디탕 각각먹었다",
          "original_form": "우린 금산삼계탕가서  신랑은 삼계탕\r\n우리는 고디탕 각각먹었다",
          "speaker_id": "2",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100032310.1.23",
          "form": "갈비 먹는데도 팀장 욕이다~ㅋㅋ",
          "original_form": "갈비 먹는데도 팀장 욕이다~ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100032310.1.24",
          "form": "ㅋㅋㅋ스트레스가 심하네",
          "original_form": "ㅋㅋㅋ스트레스가 심하네",
          "speaker_id": "2",
          "time": "20210905 18:01"
        },
        {
          "id": "MDRW2100032310.1.25",
          "form": "회사스트레스로 컨디션이 안좋으시네",
          "original_form": "회사스트레스로 컨디션이 안좋으시네",
          "speaker_id": "2",
