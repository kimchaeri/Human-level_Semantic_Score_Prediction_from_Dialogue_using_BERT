      "utterance": [
        {
          "id": "MDRW2100035126.1.1",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "1",
          "time": "20210829 03:29"
        },
        {
          "id": "MDRW2100035126.1.2",
          "form": "서복 보셨나용?",
          "original_form": "서복 보셨나용?",
          "speaker_id": "1",
          "time": "20210829 03:29"
        },
        {
          "id": "MDRW2100035126.1.3",
          "form": "박보검이랑 공유나오는!",
          "original_form": "박보검이랑 공유나오는!",
          "speaker_id": "1",
          "time": "20210829 03:29"
        },
        {
          "id": "MDRW2100035126.1.4",
          "form": "봣어요!!",
          "original_form": "봣어요!!",
          "speaker_id": "2",
          "time": "20210829 03:29"
        },
        {
          "id": "MDRW2100035126.1.5",
          "form": "영화 보는 내내 얼굴 보느라",
          "original_form": "영화 보는 내내 얼굴 보느라",
          "speaker_id": "1",
          "time": "20210829 03:30"
        },
        {
          "id": "MDRW2100035126.1.6",
          "form": "집중을 못했어요ㅋㅋㅋㅋ",
          "original_form": "집중을 못했어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:30"
        },
        {
          "id": "MDRW2100035126.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ다들그럴꺼에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ다들그럴꺼에요",
          "speaker_id": "2",
          "time": "20210829 03:30"
        },
        {
          "id": "MDRW2100035126.1.8",
          "form": "나중에 무슨 얘기지?",
          "original_form": "나중에 무슨 얘기지?",
          "speaker_id": "2",
          "time": "20210829 03:30"
        },
        {
          "id": "MDRW2100035126.1.9",
          "form": "ㅋㅋㅋ이런",
          "original_form": "ㅋㅋㅋ이런",
          "speaker_id": "2",
          "time": "20210829 03:30"
        },
        {
          "id": "MDRW2100035126.1.10",
          "form": "맞아요ㅠㅠ 근데 박보검 배우 역할이",
          "original_form": "맞아요ㅠㅠ 근데 박보검 배우 역할이",
          "speaker_id": "1",
          "time": "20210829 03:30"
        },
        {
          "id": "MDRW2100035126.1.11",
          "form": "너무 불쌍하더라구요",
          "original_form": "너무 불쌍하더라구요",
          "speaker_id": "1",
          "time": "20210829 03:30"
        },
        {
          "id": "MDRW2100035126.1.12",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 03:31"
        },
        {
          "id": "MDRW2100035126.1.13",
          "form": "그 캐릭터가 뭔가 자기 의지로 해본것도 없고...",
          "original_form": "그 캐릭터가 뭔가 자기 의지로 해본것도 없고...",
          "speaker_id": "1",
          "time": "20210829 03:31"
        },
        {
          "id": "MDRW2100035126.1.14",
          "form": "다 이용하려하고 무기로 삼고..",
          "original_form": "다 이용하려하고 무기로 삼고..",
          "speaker_id": "1",
          "time": "20210829 03:31"
        },
        {
          "id": "MDRW2100035126.1.15",
          "form": "진짜 그냥 복제인간 ㅠㅠ",
          "original_form": "진짜 그냥 복제인간 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:31"
        },
        {
          "id": "MDRW2100035126.1.16",
          "form": "네네ㅠㅠ 내용은 잔잔하고 어두웠는데",
          "original_form": "네네ㅠㅠ 내용은 잔잔하고 어두웠는데",
          "speaker_id": "1",
          "time": "20210829 03:31"
        },
        {
          "id": "MDRW2100035126.1.17",
          "form": "작품 메세지는 좋았어요",
          "original_form": "작품 메세지는 좋았어요",
          "speaker_id": "1",
          "time": "20210829 03:31"
        },
        {
          "id": "MDRW2100035126.1.18",
          "form": "맞아요 !",
          "original_form": "맞아요 !",
          "speaker_id": "2",
          "time": "20210829 03:31"
        },
        {
          "id": "MDRW2100035126.1.19",
          "form": "티빙도 넷플릭스처럼 이런 컨텐츠 많이 만들면 좋겠어용",
          "original_form": "티빙도 넷플릭스처럼 이런 컨텐츠 많이 만들면 좋겠어용",
          "speaker_id": "1",
          "time": "20210829 03:32"
        },
        {
          "id": "MDRW2100035126.1.20",
          "form": "디즈니랑 쿠팡도 ott 사업 하던데",
          "original_form": "디즈니랑 쿠팡도 ott 사업 하던데",
          "speaker_id": "1",
          "time": "20210829 03:32"
        },
        {
          "id": "MDRW2100035126.1.21",
          "form": "기대되요!",
          "original_form": "기대되요!",
          "speaker_id": "1",
          "time": "20210829 03:32"
        },
        {
          "id": "MDRW2100035126.1.22",
          "form": "와 볼거리가 더더 넘쳐나는 세상이되겠네요 ㅋㅋㅋ",
          "original_form": "와 볼거리가 더더 넘쳐나는 세상이되겠네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:32"
        },
        {
          "id": "MDRW2100035126.1.23",
          "form": "넹넹ㅋㅋㅋ 집순이라 이런거 많아질수록 넘 좋아요",
          "original_form": "넹넹ㅋㅋㅋ 집순이라 이런거 많아질수록 넘 좋아요",
          "speaker_id": "1",
          "time": "20210829 03:32"
        },
        {
          "id": "MDRW2100035126.1.24",
          "form": "이젠 티비 드라마도 경쟁력을 갖춰야할것같아용",
          "original_form": "이젠 티비 드라마도 경쟁력을 갖춰야할것같아용",
          "speaker_id": "1",
          "time": "20210829 03:32"
        },
        {
          "id": "MDRW2100035126.1.25",
          "form": "저도 집순이에요 ㅋㅋㅋ",
          "original_form": "저도 집순이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:33"
        },
        {
          "id": "MDRW2100035126.1.26",
          "form": "요새 뭐보세요?",
          "original_form": "요새 뭐보세요?",
          "speaker_id": "2",
          "time": "20210829 03:33"
        },
        {
          "id": "MDRW2100035126.1.27",
          "form": "저 드라마 예전꺼 좀 보는데",
          "original_form": "저 드라마 예전꺼 좀 보는데",
          "speaker_id": "1",
          "time": "20210829 03:33"
        },
        {
          "id": "MDRW2100035126.1.28",
          "form": "라이프 온 마스 봤구",
          "original_form": "라이프 온 마스 봤구",
          "speaker_id": "1",
          "time": "20210829 03:33"
        },
        {
          "id": "MDRW2100035126.1.29",
          "form": "그 장나라 나오는 고백부부도 봤어용",
          "original_form": "그 장나라 나오는 고백부부도 봤어용",
          "speaker_id": "1",
          "time": "20210829 03:33"
        },
        {
          "id": "MDRW2100035126.1.30",
          "form": "모르는거다 ㅠㅠ",
          "original_form": "모르는거다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:33"
        },
        {
          "id": "MDRW2100035126.1.31",
          "form": "전 주로 완결작품 몰아봐용",
          "original_form": "전 주로 완결작품 몰아봐용",
          "speaker_id": "1",
          "time": "20210829 03:33"
        }
      ]
    }
