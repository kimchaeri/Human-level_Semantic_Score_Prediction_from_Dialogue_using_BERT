      "utterance": [
        {
          "id": "MDRW2100042485.1.1",
          "form": "혹시 그 비경책",
          "original_form": "혹시 그 비경책",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042485.1.2",
          "form": "뭐 써?",
          "original_form": "뭐 써?",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042485.1.3",
          "form": "풍아의 인도?엿나",
          "original_form": "풍아의 인도?엿나",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042485.1.4",
          "form": "근데 종류가 바뀌더라구",
          "original_form": "근데 종류가 바뀌더라구",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042485.1.5",
          "form": "아 주인공은",
          "original_form": "아 주인공은",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042485.1.6",
          "form": "평타 스킬 궁",
          "original_form": "평타 스킬 궁",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042485.1.7",
          "form": "다 다르게 쓰더라",
          "original_form": "다 다르게 쓰더라",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042485.1.8",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042485.1.9",
          "form": "그래서 걍",
          "original_form": "그래서 걍",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042485.1.10",
          "form": "이나즈마 비경이나 돌란 소린듯",
          "original_form": "이나즈마 비경이나 돌란 소린듯",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042485.1.11",
          "form": "ㅇㅏ 진짜",
          "original_form": "ㅇㅏ 진짜",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042485.1.12",
          "form": "손도 안댈ᆢ고했는데",
          "original_form": "손도 안댈ᆢ고했는데",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042485.1.13",
          "form": "팔자에 안맞는",
          "original_form": "팔자에 안맞는",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042485.1.14",
          "form": "라이덴을 뽑아가지고",
          "original_form": "라이덴을 뽑아가지고",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042485.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042485.1.16",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042485.1.17",
          "form": "근데 라이덴 평타 계수 만렙 기준 3위라고 하네",
          "original_form": "근데 라이덴 평타 계수 만렙 기준 3위라고 하네",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042485.1.18",
          "form": "5성 잇으니까 좀 낫지 않아?",
          "original_form": "5성 잇으니까 좀 낫지 않아?",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042485.1.19",
          "form": "1돌기준?",
          "original_form": "1돌기준?",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042485.1.20",
          "form": "스킬 다 찍으면 평타도 키워도 나쁘지 않을듯",
          "original_form": "스킬 다 찍으면 평타도 키워도 나쁘지 않을듯",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042485.1.21",
          "form": "아니 아직",
          "original_form": "아니 아직",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042485.1.22",
          "form": "쓸수가 없잖아",
          "original_form": "쓸수가 없잖아",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042485.1.23",
          "form": "그건 모르겠어",
          "original_form": "그건 모르겠어",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042485.1.24",
          "form": "아 그리고",
          "original_form": "아 그리고",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042485.1.25",
          "form": "놀라운 사실을 봤어",
          "original_form": "놀라운 사실을 봤어",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042485.1.26",
          "form": "캐릭터별 특성 요리 잇잔아",
          "original_form": "캐릭터별 특성 요리 잇잔아",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042485.1.27",
          "form": "그걸 보려고 했는데",
          "original_form": "그걸 보려고 했는데",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042485.1.28",
          "form": "라이덴은 그 칸에 그 톱밥위키 특유의 가로선? 이 있는거야",
          "original_form": "라이덴은 그 칸에 그 톱밥위키 특유의 가로선? 이 있는거야",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042485.1.29",
          "form": "뭔가 싶었더니",
          "original_form": "뭔가 싶었더니",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042485.1.30",
          "form": "라이덴은 요리불가래",
          "original_form": "라이덴은 요리불가래",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042485.1.31",
          "form": "어떤 요리도 못한대",
          "original_form": "어떤 요리도 못한대",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042485.1.32",
          "form": "에엥",
          "original_form": "에엥",
          "speaker_id": "1",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042485.1.33",
          "form": "신기하네 ㅋㅋㅋㅋㅋ",
          "original_form": "신기하네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042485.1.34",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042485.1.35",
          "form": "아 그리고 그 죽은 쌍둥이는 메이래",
          "original_form": "아 그리고 그 죽은 쌍둥이는 메이래",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042485.1.36",
          "form": "그럼 그림자 영 써서 에이인가봐",
          "original_form": "그럼 그림자 영 써서 에이인가봐",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042485.1.37",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042485.1.38",
          "form": "아닐걸?",
          "original_form": "아닐걸?",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042485.1.39",
          "form": "라이덴 메이는 미호요 다른겜인",
          "original_form": "라이덴 메이는 미호요 다른겜인",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042485.1.40",
          "form": "붕괴에 나오는 캐야",
          "original_form": "붕괴에 나오는 캐야",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042485.1.41",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042485.1.42",
          "form": "근데 그 캐랑 캐디도 비슷하고 속성도 같아서",
          "original_form": "근데 그 캐랑 캐디도 비슷하고 속성도 같아서",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042485.1.43",
          "form": "걍 드립으로 쌍둥이 소리 나오는거 아냐?",
          "original_form": "걍 드립으로 쌍둥이 소리 나오는거 아냐?",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042485.1.44",
          "form": "근데 메이면 밝을 명 자라",
          "original_form": "근데 메이면 밝을 명 자라",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042485.1.45",
          "form": "미호요쪽에서 노린거 아닐까?",
          "original_form": "미호요쪽에서 노린거 아닐까?",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042485.1.46",
          "form": "얘네가 전에 누구더라",
          "original_form": "얘네가 전에 누구더라",
          "speaker_id": "1",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042485.1.47",
          "form": "콜라보?",
          "original_form": "콜라보?",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042485.1.48",
          "form": "암튼 다른 캐도 붕괴에서 나왔던 애랑 되게 비슷하게 짰다고 하더라고",
          "original_form": "암튼 다른 캐도 붕괴에서 나왔던 애랑 되게 비슷하게 짰다고 하더라고",
          "speaker_id": "1",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042485.1.49",
          "form": "콜라보는 피슬",
          "original_form": "콜라보는 피슬",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042485.1.50",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042485.1.51",
          "form": "이름만 한글자씩 바꿔서 넣는다고",
          "original_form": "이름만 한글자씩 바꿔서 넣는다고",
          "speaker_id": "1",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042485.1.52",
          "form": "아니 꺼라위키자식들은",
          "original_form": "아니 꺼라위키자식들은",
          "speaker_id": "2",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042485.1.53",
          "form": "왜 공설처럼",
          "original_form": "왜 공설처럼",
          "speaker_id": "2",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042485.1.54",
          "form": "써놓은거야",
          "original_form": "써놓은거야",
          "speaker_id": "2",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042485.1.55",
          "form": "이래서 위키는",
          "original_form": "이래서 위키는",
          "speaker_id": "2",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042485.1.56",
          "form": "앗시",
          "original_form": "앗시",
          "speaker_id": "2",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042485.1.57",
          "form": "라이덴 원소 충전 효율에 따라 딜 올라간대서",
          "original_form": "라이덴 원소 충전 효율에 따라 딜 올라간대서",
          "speaker_id": "2",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042485.1.58",
          "form": "??ㄴ내꺼엔 그런 설명 없던데",
          "original_form": "??ㄴ내꺼엔 그런 설명 없던데",
          "speaker_id": "2",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042485.1.59",
          "form": "했는데",
          "original_form": "했는데",
          "speaker_id": "2",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042485.1.60",
          "form": "4돌해야 열리는거잖아ㅡㅡ",
          "original_form": "4돌해야 열리는거잖아ㅡㅡ",
          "speaker_id": "2",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042485.1.61",
          "form": "이걸 왜 보통 있는것처럼",
          "original_form": "이걸 왜 보통 있는것처럼",
