      "utterance": [
        {
          "id": "MDRW2100010190.1.1",
          "form": "날씨어때",
          "original_form": "날씨어때",
          "speaker_id": "1",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010190.1.2",
          "form": "여긴 비와 아주많이 와",
          "original_form": "여긴 비와 아주많이 와",
          "speaker_id": "1",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010190.1.3",
          "form": "나 커튼에 에어컨 키고 살아서",
          "original_form": "나 커튼에 에어컨 키고 살아서",
          "speaker_id": "2",
          "time": "20210823 15:24"
        },
        {
          "id": "MDRW2100010190.1.4",
          "form": "비 온다는 소식은 들었지 아직 안 봤네",
          "original_form": "비 온다는 소식은 들었지 아직 안 봤네",
          "speaker_id": "2",
          "time": "20210823 15:24"
        },
        {
          "id": "MDRW2100010190.1.5",
          "form": "잠깐만",
          "original_form": "잠깐만",
          "speaker_id": "2",
          "time": "20210823 15:24"
        },
        {
          "id": "MDRW2100010190.1.6",
          "form": "집에있음 빗소리들리지않아?",
          "original_form": "집에있음 빗소리들리지않아?",
          "speaker_id": "1",
          "time": "20210823 15:24"
        },
        {
          "id": "MDRW2100010190.1.7",
          "form": "비는 그친 거 같아",
          "original_form": "비는 그친 거 같아",
          "speaker_id": "2",
          "time": "20210823 15:24"
        },
        {
          "id": "MDRW2100010190.1.8",
          "form": "우리집이 방음이 안되는건가",
          "original_form": "우리집이 방음이 안되는건가",
          "speaker_id": "1",
          "time": "20210823 15:24"
        },
        {
          "id": "MDRW2100010190.1.9",
          "form": "비가 그친거구나",
          "original_form": "비가 그친거구나",
          "speaker_id": "1",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010190.1.10",
          "form": "비가 그렇게 많이 안 온 걸 수도",
          "original_form": "비가 그렇게 많이 안 온 걸 수도",
          "speaker_id": "2",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010190.1.11",
          "form": "비오는 날 어때",
          "original_form": "비오는 날 어때",
          "speaker_id": "1",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010190.1.12",
          "form": "심하게 오면",
          "original_form": "심하게 오면",
          "speaker_id": "2",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010190.1.13",
          "form": "타닥타닥 소리 들리긴 해",
          "original_form": "타닥타닥 소리 들리긴 해",
          "speaker_id": "2",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010190.1.14",
          "form": "비오는 날에 딱히 우울하거나 하는 심경변화는 없는데",
          "original_form": "비오는 날에 딱히 우울하거나 하는 심경변화는 없는데",
          "speaker_id": "2",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010190.1.15",
          "form": "외출해야 될 때 불편해서 싫어",
          "original_form": "외출해야 될 때 불편해서 싫어",
          "speaker_id": "2",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010190.1.16",
          "form": "너는 비오는 날 어때",
          "original_form": "너는 비오는 날 어때",
          "speaker_id": "2",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010190.1.17",
          "form": "그건 나도 공감해",
          "original_form": "그건 나도 공감해",
          "speaker_id": "1",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010190.1.18",
          "form": "특히 비에 젖는게 너무 싫어",
          "original_form": "특히 비에 젖는게 너무 싫어",
          "speaker_id": "1",
          "time": "20210823 15:26"
        },
        {
          "id": "MDRW2100010190.1.19",
          "form": "우산 쓰는데도 다 젖어서 불쾌해",
          "original_form": "우산 쓰는데도 다 젖어서 불쾌해",
          "speaker_id": "1",
          "time": "20210823 15:26"
        },
        {
          "id": "MDRW2100010190.1.20",
          "form": "뭔 느낌인지 알지",
          "original_form": "뭔 느낌인지 알지",
          "speaker_id": "1",
          "time": "20210823 15:26"
        },
        {
          "id": "MDRW2100010190.1.21",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210823 15:26"
        },
        {
          "id": "MDRW2100010190.1.22",
          "form": "그래서 이왕 젖는 거 짧은 바지 입고 나가",
          "original_form": "그래서 이왕 젖는 거 짧은 바지 입고 나가",
          "speaker_id": "2",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100010190.1.23",
          "form": "분명 우산쓰고있는데  우산의 이미가 없어지는 기분",
          "original_form": "분명 우산쓰고있는데  우산의 이미가 없어지는 기분",
          "speaker_id": "1",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100010190.1.24",
          "form": "짧은바지 입어야 찝찝함이 덜하긴해",
          "original_form": "짧은바지 입어야 찝찝함이 덜하긴해",
          "speaker_id": "1",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100010190.1.25",
          "form": "옷은 잘 안마르니까",
          "original_form": "옷은 잘 안마르니까",
          "speaker_id": "1",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100010190.1.26",
          "form": "장화 신어본 적 있어?",
          "original_form": "장화 신어본 적 있어?",
          "speaker_id": "2",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100010190.1.27",
          "form": "현명하구나",
          "original_form": "현명하구나",
          "speaker_id": "1",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100010190.1.28",
          "form": "아니 장화도 불편해서 못신어",
          "original_form": "아니 장화도 불편해서 못신어",
          "speaker_id": "1",
          "time": "20210823 15:27"
        }
      ]
    }
  ]
