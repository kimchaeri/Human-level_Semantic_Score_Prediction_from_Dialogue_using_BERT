      "utterance": [
        {
          "id": "MDRW2100019487.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019487.1.2",
          "form": "여튼 이제",
          "original_form": "여튼 이제",
          "speaker_id": "1",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019487.1.3",
          "form": "원신하러",
          "original_form": "원신하러",
          "speaker_id": "1",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019487.1.4",
          "form": "가겠어",
          "original_form": "가겠어",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019487.1.5",
          "form": "이따보자",
          "original_form": "이따보자",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019487.1.6",
          "form": "그랭",
          "original_form": "그랭",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019487.1.7",
          "form": "나도 일일퀘스트 해야겠다",
          "original_form": "나도 일일퀘스트 해야겠다",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019487.1.8",
          "form": "하다가 궁금한거 잇으면 연락해 ㅋㅋㅋ",
          "original_form": "하다가 궁금한거 잇으면 연락해 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019487.1.9",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019487.1.10",
          "form": "오늘자 타타라스나 다 햇고",
          "original_form": "오늘자 타타라스나 다 햇고",
          "speaker_id": "1",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019487.1.11",
          "form": "별거 없네?",
          "original_form": "별거 없네?",
          "speaker_id": "1",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019487.1.12",
          "form": "걍 대포 아무데나 조준해서",
          "original_form": "걍 대포 아무데나 조준해서",
          "speaker_id": "1",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019487.1.13",
          "form": "쏴보면 되네",
          "original_form": "쏴보면 되네",
          "speaker_id": "1",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019487.1.14",
          "form": "아 그 대포 다 쏨?",
          "original_form": "아 그 대포 다 쏨?",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019487.1.15",
          "form": "그 대포 쏴서 열 수 있는 던전 있어",
          "original_form": "그 대포 쏴서 열 수 있는 던전 있어",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019487.1.16",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019487.1.17",
          "form": "나 그걸로",
          "original_form": "나 그걸로",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019487.1.18",
          "form": "몹이나 쏴보면서",
          "original_form": "몹이나 쏴보면서",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019487.1.19",
          "form": "놀고있었는데",
          "original_form": "놀고있었는데",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019487.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019487.1.21",
          "form": "그",
          "original_form": "그",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019487.1.22",
          "form": "절벽 향해서 쏠 수 있는 대포야",
          "original_form": "절벽 향해서 쏠 수 있는 대포야",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019487.1.23",
          "form": "아마 이쯤되면 안열린 비경은 딱 하나일걸?",
          "original_form": "아마 이쯤되면 안열린 비경은 딱 하나일걸?",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019487.1.24",
          "form": "ㅇㅎ.......",
          "original_form": "ㅇㅎ.......",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019487.1.25",
          "form": "내일 함",
          "original_form": "내일 함",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019487.1.26",
          "form": "해볼게",
          "original_form": "해볼게",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019487.1.27",
          "form": "ㅇㅇ어쩐지",
          "original_form": "ㅇㅇ어쩐지",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019487.1.28",
          "form": "거기있다는데",
          "original_form": "거기있다는데",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019487.1.29",
          "form": "ㅇ아무리 찾앋ᆢ",
          "original_form": "ㅇ아무리 찾앋ᆢ",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019487.1.30",
          "form": "안나오더라니",
          "original_form": "안나오더라니",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019487.1.31",
          "form": "ㅇㅇ 그거야 ㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 그거야 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019487.1.32",
          "form": "던전주제에 꽁꽁 숨겨져있음",
          "original_form": "던전주제에 꽁꽁 숨겨져있음",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019487.1.33",
          "form": "ㅇㅇ.......",
          "original_form": "ㅇㅇ.......",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019487.1.34",
          "form": "비경 열었어?",
          "original_form": "비경 열었어?",
          "speaker_id": "2",
          "time": "20210828 11:50"
        },
        {
          "id": "MDRW2100019487.1.35",
          "form": "나 방금 신규 캐릭터 특징 봤는데",
          "original_form": "나 방금 신규 캐릭터 특징 봤는데",
          "speaker_id": "2",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100019487.1.36",
          "form": "에일로이 기본 특성이",
          "original_form": "에일로이 기본 특성이",
          "speaker_id": "2",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100019487.1.37",
          "form": "사냥감이 도망 안가는거래",
          "original_form": "사냥감이 도망 안가는거래",
          "speaker_id": "2",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100019487.1.38",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100019487.1.39",
          "form": "뭔소리지?",
          "original_form": "뭔소리지?",
          "speaker_id": "1",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100019487.1.40",
          "form": "원작에서 설산 사냥꾼이라",
          "original_form": "원작에서 설산 사냥꾼이라",
          "speaker_id": "2",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100019487.1.41",
          "form": "원신에서도",
          "original_form": "원신에서도",
          "speaker_id": "2",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100019487.1.42",
          "form": "새, 멧돼지 이런애들이",
          "original_form": "새, 멧돼지 이런애들이",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100019487.1.43",
          "form": "근접거리에서 도망 안간대",
          "original_form": "근접거리에서 도망 안간대",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100019487.1.44",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100019487.1.45",
          "form": "그럼 걔만있으면",
          "original_form": "그럼 걔만있으면",
          "speaker_id": "1",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100019487.1.46",
          "form": "라즈베리 안줘도",
          "original_form": "라즈베리 안줘도",
          "speaker_id": "1",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100019487.1.47",
          "form": "스샷 가능?",
          "original_form": "스샷 가능?",
          "speaker_id": "1",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100019487.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100019487.1.49",
          "form": "아 그럴지도?",
          "original_form": "아 그럴지도?",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100019487.1.50",
          "form": "일단 사냥은 굳이 멀리서 조준할 필요없이",
          "original_form": "일단 사냥은 굳이 멀리서 조준할 필요없이",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100019487.1.51",
          "form": "맘대로 가능",
          "original_form": "맘대로 가능",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100019487.1.52",
          "form": "오 근데 디게 특이한 특성이다",
          "original_form": "오 근데 디게 특이한 특성이다",
          "speaker_id": "1",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100019487.1.53",
          "form": "ㅇㅇ 나도 전혀 생각 못한 특성이야",
          "original_form": "ㅇㅇ 나도 전혀 생각 못한 특성이야",
          "speaker_id": "2",
          "time": "20210828 12:32"
        }
      ]
