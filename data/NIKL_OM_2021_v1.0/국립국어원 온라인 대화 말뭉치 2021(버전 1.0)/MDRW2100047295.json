      "utterance": [
        {
          "id": "MDRW2100047295.1.1",
          "form": "안녕하세요! 식사하셨나요?",
          "original_form": "안녕하세요! 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210826 09:43"
        },
        {
          "id": "MDRW2100047295.1.2",
          "form": "반가워요~",
          "original_form": "반가워요~",
          "speaker_id": "2",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100047295.1.3",
          "form": "점심으로 샌드위치 먹었어요!",
          "original_form": "점심으로 샌드위치 먹었어요!",
          "speaker_id": "2",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100047295.1.4",
          "form": "오 샌드위치 좋네요 만들어서 드신건가요?",
          "original_form": "오 샌드위치 좋네요 만들어서 드신건가요?",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100047295.1.5",
          "form": "서브웨이에서 먹었어요ㅎㅎ",
          "original_form": "서브웨이에서 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047295.1.6",
          "form": "저녁은 뭐 드셨어요?",
          "original_form": "저녁은 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047295.1.7",
          "form": "로제라볶이 먹었어요",
          "original_form": "로제라볶이 먹었어요",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100047295.1.8",
          "form": "서브웨이에서 뭐 드셨어요??",
          "original_form": "서브웨이에서 뭐 드셨어요??",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100047295.1.9",
          "form": "저는 이탈리안비엠티 먹었어요!!",
          "original_form": "저는 이탈리안비엠티 먹었어요!!",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100047295.1.10",
          "form": "제가 제일 좋아하는 메뉴예요ㅎㅎ",
          "original_form": "제가 제일 좋아하는 메뉴예요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100047295.1.11",
          "form": "오오 그렇군요ㅎㅎ 쿠키도 드셨나요?!",
          "original_form": "오오 그렇군요ㅎㅎ 쿠키도 드셨나요?!",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100047295.1.12",
          "form": "쿠키는 안먹어용 탕비실에서 과자를 많이 먹어서ㅋㅋ",
          "original_form": "쿠키는 안먹어용 탕비실에서 과자를 많이 먹어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100047295.1.13",
          "form": "어떤 쿠키 좋아하세요??",
          "original_form": "어떤 쿠키 좋아하세요??",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100047295.1.14",
          "form": "아 저는 화이트마카다미아 좋아해요!ㅋㅋㅋㅋ",
          "original_form": "아 저는 화이트마카다미아 좋아해요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100047295.1.15",
          "form": "ㅋㅋㅋ화이트 맛있죠!!!",
          "original_form": "ㅋㅋㅋ화이트 맛있죠!!!",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100047295.1.16",
          "form": "이번에 파인애플이었나?",
          "original_form": "이번에 파인애플이었나?",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100047295.1.17",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100047295.1.18",
          "form": "아 아니요ㅋㅋㅋㅋㅋㅋ 뭔가 상상이 안가서 거부감이 들어갖고 안 먹어봤어요",
          "original_form": "아 아니요ㅋㅋㅋㅋㅋㅋ 뭔가 상상이 안가서 거부감이 들어갖고 안 먹어봤어요",
          "speaker_id": "1",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100047295.1.19",
          "form": "name2 님은 파인애플 구운 거 좋아하시나요?",
          "original_form": "&name2& 님은 파인애플 구운 거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100047295.1.20",
          "form": "저는 좋아해요!",
          "original_form": "저는 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100047295.1.21",
          "form": "근데 호불호 왜 갈리는지 모르겠어요 맛있는뎅ㅠ",
          "original_form": "근데 호불호 왜 갈리는지 모르겠어요 맛있는뎅ㅠ",
          "speaker_id": "2",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100047295.1.22",
          "form": "아 그럼 파인애플 피자도 좋아하시겠네요! 전 안 먹어봐서 그냥 거부감이 드나봐요ㅋㅋㅋ",
          "original_form": "아 그럼 파인애플 피자도 좋아하시겠네요! 전 안 먹어봐서 그냥 거부감이 드나봐요ㅋㅋㅋ",
          "speaker_id": "1",
