        },
        {
          "id": "MDRW2100019175.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100019175.1.3",
          "form": "점심뭐드셨어요?",
          "original_form": "점심뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019175.1.4",
          "form": "점심 빵이랑 시리얼먹었어요",
          "original_form": "점심 빵이랑 시리얼먹었어요",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019175.1.5",
          "form": "아하! 양식 좋아하시나봐요?",
          "original_form": "아하! 양식 좋아하시나봐요?",
          "speaker_id": "1",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019175.1.6",
          "form": "사실 점심먹기 귀찮아서",
          "original_form": "사실 점심먹기 귀찮아서",
          "speaker_id": "2",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019175.1.7",
          "form": "그냥 떼운느낌이긴해요",
          "original_form": "그냥 떼운느낌이긴해요",
          "speaker_id": "2",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019175.1.8",
          "form": "아하 그렇구나",
          "original_form": "아하 그렇구나",
          "speaker_id": "1",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019175.1.9",
          "form": "점심 뭐드셨어요",
          "original_form": "점심 뭐드셨어요",
          "speaker_id": "2",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100019175.1.10",
          "form": "저는 오늘 브리또먹었어요!",
          "original_form": "저는 오늘 브리또먹었어요!",
          "speaker_id": "1",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100019175.1.11",
          "form": "브리또가 뭐에요?",
          "original_form": "브리또가 뭐에요?",
          "speaker_id": "2",
          "time": "20210827 16:04"
        },
        {
          "id": "MDRW2100019175.1.12",
          "form": "음 타코랑 비슷한음식인데 쉽게말해서 내용물이 알찬 랩이라고 보면될거같아요",
          "original_form": "음 타코랑 비슷한음식인데 쉽게말해서 내용물이 알찬 랩이라고 보면될거같아요",
          "speaker_id": "1",
          "time": "20210827 16:07"
        },
        {
          "id": "MDRW2100019175.1.13",
          "form": "우와 맛있을거같아요",
          "original_form": "우와 맛있을거같아요",
          "speaker_id": "2",
          "time": "20210827 16:08"
        },
        {
          "id": "MDRW2100019175.1.14",
          "form": "맛있어요! 찾아가서먹을만큼",
          "original_form": "맛있어요! 찾아가서먹을만큼",
          "speaker_id": "1",
          "time": "20210827 16:09"
        },
        {
          "id": "MDRW2100019175.1.15",
          "form": "저도 한번 찾아서 먹어보고싶어요",
          "original_form": "저도 한번 찾아서 먹어보고싶어요",
          "speaker_id": "2",
          "time": "20210827 16:09"
        },
        {
          "id": "MDRW2100019175.1.16",
          "form": "브리또는 종류가 하나인가요",
          "original_form": "브리또는 종류가 하나인가요",
          "speaker_id": "2",
          "time": "20210827 16:10"
        },
        {
          "id": "MDRW2100019175.1.17",
          "form": "아뇨 여러개에요~ 치킨브리또 목살브리또 연어브리또 등등이요",
          "original_form": "아뇨 여러개에요~ 치킨브리또 목살브리또 연어브리또 등등이요",
          "speaker_id": "1",
          "time": "20210827 16:11"
        },
        {
          "id": "MDRW2100019175.1.18",
          "form": "어떤 브리또가 가장 맛있나요?",
          "original_form": "어떤 브리또가 가장 맛있나요?",
          "speaker_id": "2",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100019175.1.19",
          "form": "저는 개인적으로 치킨브리또 좋아해요 ㅋㅋ",
          "original_form": "저는 개인적으로 치킨브리또 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100019175.1.20",
          "form": "치킨너겟같은 느낌인가요?",
          "original_form": "치킨너겟같은 느낌인가요?",
          "speaker_id": "2",
          "time": "20210827 16:13"
        },
        {
          "id": "MDRW2100019175.1.21",
          "form": "음 그것보단 치킨텐더랑 더 가까운느낌인거같아요",
          "original_form": "음 그것보단 치킨텐더랑 더 가까운느낌인거같아요",
          "speaker_id": "1",
          "time": "20210827 16:14"
        }
      ]
