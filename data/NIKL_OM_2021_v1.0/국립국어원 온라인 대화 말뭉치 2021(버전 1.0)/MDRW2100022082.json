      "utterance": [
        {
          "id": "MDRW2100022082.1.1",
          "form": "음식 얘기 해야겠네요 ㅎㅎ",
          "original_form": "음식 얘기 해야겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 02:27"
        },
        {
          "id": "MDRW2100022082.1.2",
          "form": "매워서 못먹는거 흑흑.. 아 안매운 닭발 좀 팔면 좋겠어요",
          "original_form": "매워서 못먹는거 흑흑.. 아 안매운 닭발 좀 팔면 좋겠어요",
          "speaker_id": "2",
          "time": "20210829 02:27"
        },
        {
          "id": "MDRW2100022082.1.3",
          "form": "닭발 너무 맵지 않나요?",
          "original_form": "닭발 너무 맵지 않나요?",
          "speaker_id": "1",
          "time": "20210829 02:27"
        },
        {
          "id": "MDRW2100022082.1.4",
          "form": "먹는거 얘긴 밤새도록 할 수 있는데 크..",
          "original_form": "먹는거 얘긴 밤새도록 할 수 있는데 크..",
          "speaker_id": "2",
          "time": "20210829 02:27"
        },
        {
          "id": "MDRW2100022082.1.5",
          "form": "매워요! 매워서 슬퍼요 ㅠㅠ 안맵다고 하는데가 없어요 이제는 크흑",
          "original_form": "매워요! 매워서 슬퍼요 ㅠㅠ 안맵다고 하는데가 없어요 이제는 크흑",
          "speaker_id": "2",
          "time": "20210829 02:27"
        },
        {
          "id": "MDRW2100022082.1.6",
          "form": "저는 닭발 너무 매워서 안먹어요",
          "original_form": "저는 닭발 너무 매워서 안먹어요",
          "speaker_id": "1",
          "time": "20210829 02:27"
        },
        {
          "id": "MDRW2100022082.1.7",
          "form": "저두요 ;ㅅ; 못먹는거에 가깝지만..",
          "original_form": "저두요 ;ㅅ; 못먹는거에 가깝지만..",
          "speaker_id": "2",
          "time": "20210829 02:28"
        },
        {
          "id": "MDRW2100022082.1.8",
          "form": "적당히 매운게 아니라서",
          "original_form": "적당히 매운게 아니라서",
          "speaker_id": "1",
          "time": "20210829 02:28"
        },
        {
          "id": "MDRW2100022082.1.9",
          "form": "어릴땐 엄마가 사다주면 적당히 맵달달해서 끌어안고 먹었는데",
          "original_form": "어릴땐 엄마가 사다주면 적당히 맵달달해서 끌어안고 먹었는데",
          "speaker_id": "2",
          "time": "20210829 02:28"
        },
        {
          "id": "MDRW2100022082.1.10",
          "form": "요샌 그런거 아예 안팔더라구여",
          "original_form": "요샌 그런거 아예 안팔더라구여",
          "speaker_id": "2",
          "time": "20210829 02:28"
        },
        {
          "id": "MDRW2100022082.1.11",
          "form": "슬프고 통곡이에여 으악",
          "original_form": "슬프고 통곡이에여 으악",
          "speaker_id": "2",
          "time": "20210829 02:28"
        },
        {
          "id": "MDRW2100022082.1.12",
          "form": "회 닭발",
          "original_form": "회 닭발",
          "speaker_id": "1",
          "time": "20210829 02:28"
        },
        {
          "id": "MDRW2100022082.1.13",
          "form": "걍 맵다고~!해야 팔리니 원",
          "original_form": "걍 맵다고~!해야 팔리니 원",
          "speaker_id": "2",
          "time": "20210829 02:28"
        },
        {
          "id": "MDRW2100022082.1.14",
          "form": "또 좋아하는 음식은?",
          "original_form": "또 좋아하는 음식은?",
          "speaker_id": "1",
          "time": "20210829 02:28"
        },
        {
          "id": "MDRW2100022082.1.15",
          "form": "어 안좋아하는 음식을 찾는게 빠를텐데..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "어 안좋아하는 음식을 찾는게 빠를텐데..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:29"
        },
        {
          "id": "MDRW2100022082.1.16",
          "form": "피자 파스타도 좋아하구여",
          "original_form": "피자 파스타도 좋아하구여",
          "speaker_id": "2",
          "time": "20210829 02:29"
        },
        {
          "id": "MDRW2100022082.1.17",
          "form": "스테이크도 좋아하구 육회도 잘먹고",
          "original_form": "스테이크도 좋아하구 육회도 잘먹고",
          "speaker_id": "2",
          "time": "20210829 02:29"
        },
        {
          "id": "MDRW2100022082.1.18",
          "form": "순대고 좋아하구 오뎅탕도 좋구 아 배고픈데..ㅋㅋㅋㅋㅋ",
          "original_form": "순대고 좋아하구 오뎅탕도 좋구 아 배고픈데..ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:29"
        },
        {
          "id": "MDRW2100022082.1.19",
          "form": "저녁 먹었는데 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저녁 먹었는데 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:29"
        },
        {
          "id": "MDRW2100022082.1.20",
          "form": "그...그만...",
          "original_form": "그...그만...",
          "speaker_id": "1",
          "time": "20210829 02:29"
        },
        {
          "id": "MDRW2100022082.1.21",
          "form": "그럼 안좋아하는 음식은?",
          "original_form": "그럼 안좋아하는 음식은?",
          "speaker_id": "1",
          "time": "20210829 02:29"
        },
        {
          "id": "MDRW2100022082.1.22",
          "form": "그쳐?괴롭겠다 응응..",
          "original_form": "그쳐?괴롭겠다 응응..",
          "speaker_id": "2",
          "time": "20210829 02:29"
        },
        {
          "id": "MDRW2100022082.1.23",
          "form": "아까 선지 말했고",
          "original_form": "아까 선지 말했고",
          "speaker_id": "1",
          "time": "20210829 02:29"
        },
        {
          "id": "MDRW2100022082.1.24",
          "form": "아 안좋아하는건 아까 말한거처럼 선지",
          "original_form": "아 안좋아하는건 아까 말한거처럼 선지",
          "speaker_id": "2",
          "time": "20210829 02:29"
        },
        {
          "id": "MDRW2100022082.1.25",
          "form": "선지는 빼고 먹어요 선지국에선",
          "original_form": "선지는 빼고 먹어요 선지국에선",
          "speaker_id": "2",
          "time": "20210829 02:29"
        },
        {
          "id": "MDRW2100022082.1.26",
          "form": "그 허브.. 이름 뭐지.. 로션냄새 나는거..",
          "original_form": "그 허브.. 이름 뭐지.. 로션냄새 나는거..",
          "speaker_id": "2",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100022082.1.27",
          "form": "ㅠㅁㅠ ㄱ..고수!ㅇ",
          "original_form": "ㅠㅁㅠ ㄱ..고수!ㅇ",
          "speaker_id": "2",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100022082.1.28",
          "form": "로션냄새나는 허브?",
          "original_form": "로션냄새나는 허브?",
          "speaker_id": "1",
