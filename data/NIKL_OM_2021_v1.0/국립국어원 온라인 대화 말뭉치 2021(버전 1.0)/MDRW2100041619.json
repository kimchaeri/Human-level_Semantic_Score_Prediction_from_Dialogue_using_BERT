      "utterance": [
        {
          "id": "MDRW2100041619.1.1",
          "form": "커피는 못마시고",
          "original_form": "커피는 못마시고",
          "speaker_id": "2",
          "time": "20210906 13:14"
        },
        {
          "id": "MDRW2100041619.1.2",
          "form": "저는 아아랑 디카페인 한잔 해요",
          "original_form": "저는 아아랑 디카페인 한잔 해요",
          "speaker_id": "1",
          "time": "20210906 13:14"
        },
        {
          "id": "MDRW2100041619.1.3",
          "form": "음료수 마시고이ㅛ어요 ㅎㅎ",
          "original_form": "음료수 마시고이ㅛ어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 13:14"
        },
        {
          "id": "MDRW2100041619.1.4",
          "form": "오 어떤거 드세여",
          "original_form": "오 어떤거 드세여",
          "speaker_id": "1",
          "time": "20210906 13:14"
        },
        {
          "id": "MDRW2100041619.1.5",
          "form": "아침햇살이용!!",
          "original_form": "아침햇살이용!!",
          "speaker_id": "2",
          "time": "20210906 13:15"
        },
        {
          "id": "MDRW2100041619.1.6",
          "form": "피곤하면 박카스를 먹어요 ㅋㅋㅋ",
          "original_form": "피곤하면 박카스를 먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:15"
        },
        {
          "id": "MDRW2100041619.1.7",
          "form": "와 아침햇살과 박카스",
          "original_form": "와 아침햇살과 박카스",
          "speaker_id": "1",
          "time": "20210906 13:15"
        },
        {
          "id": "MDRW2100041619.1.8",
          "form": "커피 잘 안드세요?",
          "original_form": "커피 잘 안드세요?",
          "speaker_id": "1",
          "time": "20210906 13:15"
        },
        {
          "id": "MDRW2100041619.1.9",
          "form": "커피는 바닐라라떼같은? 달달한거만 마셔용",
          "original_form": "커피는 바닐라라떼같은? 달달한거만 마셔용",
          "speaker_id": "2",
          "time": "20210906 13:18"
        },
        {
          "id": "MDRW2100041619.1.10",
          "form": "아메리카노 드세요?",
          "original_form": "아메리카노 드세요?",
          "speaker_id": "2",
          "time": "20210906 13:18"
        },
        {
          "id": "MDRW2100041619.1.11",
          "form": "네네 예전엔 라떼 요즘은 아메리카노 ㅋㅋ",
          "original_form": "네네 예전엔 라떼 요즘은 아메리카노 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:47"
        },
        {
          "id": "MDRW2100041619.1.12",
          "form": "아메리카노는 아이스로 드시나용?",
          "original_form": "아메리카노는 아이스로 드시나용?",
          "speaker_id": "2",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041619.1.13",
          "form": "네네 저는 아이스 커피 좋아해요",
          "original_form": "네네 저는 아이스 커피 좋아해요",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041619.1.14",
          "form": "따뜩한거 드세요?",
          "original_form": "따뜩한거 드세요?",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041619.1.15",
          "form": "저는 커피는 무조건 아이스로 먹어용 ㅋㅋㅋ",
          "original_form": "저는 커피는 무조건 아이스로 먹어용 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041619.1.16",
          "form": "겨울에도 아이스 !",
          "original_form": "겨울에도 아이스 !",
          "speaker_id": "2",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041619.1.17",
          "form": "저도그래요",
          "original_form": "저도그래요",
          "speaker_id": "1",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100041619.1.18",
          "form": "너무추운날은 따신거",
