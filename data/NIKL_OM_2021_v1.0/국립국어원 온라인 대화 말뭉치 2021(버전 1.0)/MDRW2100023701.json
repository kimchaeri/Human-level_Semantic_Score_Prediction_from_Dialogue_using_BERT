      "utterance": [
        {
          "id": "MDRW2100023701.1.1",
          "form": "안녕하세요 \n재미밌었던 여행지는 어디셨어요?",
          "original_form": "안녕하세요 \n재미밌었던 여행지는 어디셨어요?",
          "speaker_id": "1",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023701.1.2",
          "form": "올여름휴가 울릉도 독도 다녀왓는데 너무 뜻깊엇어요",
          "original_form": "올여름휴가 울릉도 독도 다녀왓는데 너무 뜻깊엇어요",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100023701.1.3",
          "form": "와 정말 좋으셨겠어요\n전 암데도 안갔거든요ㅠ",
          "original_form": "와 정말 좋으셨겠어요\n전 암데도 안갔거든요ㅠ",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100023701.1.4",
          "form": "접종 완료하고 마스크잘끼고 다녀왓는데도",
          "original_form": "접종 완료하고 마스크잘끼고 다녀왓는데도",
          "speaker_id": "2",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100023701.1.5",
          "form": "혹시나싶어서 걱정됏어요ㅜㅜ",
          "original_form": "혹시나싶어서 걱정됏어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100023701.1.6",
          "form": "기억에남는 여행지 잇으신가욤",
          "original_form": "기억에남는 여행지 잇으신가욤",
          "speaker_id": "2",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100023701.1.7",
          "form": "전 일본에서 온천간거요\n엄청 옛날이네요",
          "original_form": "전 일본에서 온천간거요\n엄청 옛날이네요",
          "speaker_id": "1",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100023701.1.8",
          "form": "ㅎㅎ저는 후쿠오카한번갓엇는데",
          "original_form": "ㅎㅎ저는 후쿠오카한번갓엇는데",
          "speaker_id": "2",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100023701.1.9",
          "form": "처음이자마지막이될듯요 ㅎㅎ",
          "original_form": "처음이자마지막이될듯요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100023701.1.10",
          "form": "네 저도요 안갈거같긴 해요ㅋㅋ",
          "original_form": "네 저도요 안갈거같긴 해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100023701.1.11",
          "form": "ㅋㅋ그쳐",
          "original_form": "ㅋㅋ그쳐",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100023701.1.12",
          "form": "이제굳이일본갈일은없을듯싶어요",
          "original_form": "이제굳이일본갈일은없을듯싶어요",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100023701.1.13",
          "form": "전 코로나 없어지면 휴양지로 가고싶어요",
          "original_form": "전 코로나 없어지면 휴양지로 가고싶어요",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100023701.1.14",
          "form": "저는 방콕이요ㅜㅜ",
          "original_form": "저는 방콕이요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100023701.1.15",
          "form": "마사지 무한 받고싶어요",
          "original_form": "마사지 무한 받고싶어요",
          "speaker_id": "2",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100023701.1.16",
          "form": "휴양지 간절합니다ㅜㅜ",
          "original_form": "휴양지 간절합니다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100023701.1.17",
          "form": "아 마사지요ㅋ 전한번도 안받아봐서",
          "original_form": "아 마사지요ㅋ 전한번도 안받아봐서",
          "speaker_id": "1",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100023701.1.18",
          "form": "신혼여행 멀리갈꺼라서 미루고잇어요ㅋㅋ",
          "original_form": "신혼여행 멀리갈꺼라서 미루고잇어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:23"
        },
        {
          "id": "MDRW2100023701.1.19",
          "form": "멀리 휴양지 가는 그날까지!!",
          "original_form": "멀리 휴양지 가는 그날까지!!",
          "speaker_id": "2",
          "time": "20210901 17:23"
        },
        {
          "id": "MDRW2100023701.1.20",
          "form": "아 전 그때는 하와이를 갈거에요ㅋㅋㅋ",
          "original_form": "아 전 그때는 하와이를 갈거에요ㅋㅋㅋ",
