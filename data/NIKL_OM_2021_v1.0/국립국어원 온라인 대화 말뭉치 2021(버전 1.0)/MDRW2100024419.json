      "utterance": [
        {
          "id": "MDRW2100024419.1.1",
          "form": "오 엄청 일찍 일어나시네요!!",
          "original_form": "오 엄청 일찍 일어나시네요!!",
          "speaker_id": "2",
          "time": "20210831 07:43"
        },
        {
          "id": "MDRW2100024419.1.2",
          "form": "오늘은 쉬는 날이라 7시에 일어났어요",
          "original_form": "오늘은 쉬는 날이라 7시에 일어났어요",
          "speaker_id": "1",
          "time": "20210831 07:43"
        },
        {
          "id": "MDRW2100024419.1.3",
          "form": "부지런하신 것 같아요",
          "original_form": "부지런하신 것 같아요",
          "speaker_id": "2",
          "time": "20210831 07:43"
        },
        {
          "id": "MDRW2100024419.1.4",
          "form": "출근준비하고 출근해야하니까요",
          "original_form": "출근준비하고 출근해야하니까요",
          "speaker_id": "1",
          "time": "20210831 07:43"
        },
        {
          "id": "MDRW2100024419.1.5",
          "form": "name2님은 출근 안하시는날 뭐하세요?",
          "original_form": "&name2&님은 출근 안하시는날 뭐하세요?",
          "speaker_id": "1",
          "time": "20210831 07:44"
        },
        {
          "id": "MDRW2100024419.1.6",
          "form": "전 그냥 뒹굴뒹굴하다가",
          "original_form": "전 그냥 뒹굴뒹굴하다가",
          "speaker_id": "2",
          "time": "20210831 07:45"
        },
        {
          "id": "MDRW2100024419.1.7",
          "form": "그동안은 학교를 안가서 놀기만 했어요ㅎㅎ",
          "original_form": "그동안은 학교를 안가서 놀기만 했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 07:45"
        },
        {
          "id": "MDRW2100024419.1.8",
          "form": "이제 내일 개강입니다ㅠㅜ",
          "original_form": "이제 내일 개강입니다ㅠㅜ",
          "speaker_id": "2",
          "time": "20210831 07:45"
        },
        {
          "id": "MDRW2100024419.1.9",
          "form": "name1님은 지금 직장 이전에",
          "original_form": "&name1&님은 지금 직장 이전에",
          "speaker_id": "2",
          "time": "20210831 07:46"
        },
        {
          "id": "MDRW2100024419.1.10",
          "form": "다른 곳에서 일해보신 적 있으신가요?",
          "original_form": "다른 곳에서 일해보신 적 있으신가요?",
          "speaker_id": "2",
          "time": "20210831 07:46"
        },
        {
          "id": "MDRW2100024419.1.11",
          "form": "네 전에는 사기업에서 일했었어요",
          "original_form": "네 전에는 사기업에서 일했었어요",
          "speaker_id": "1",
          "time": "20210831 07:47"
        },
        {
          "id": "MDRW2100024419.1.12",
          "form": "오 거긴 어떠셨어요?",
          "original_form": "오 거긴 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210831 07:47"
        },
        {
          "id": "MDRW2100024419.1.13",
          "form": "지금 직장은 이직한거에요",
          "original_form": "지금 직장은 이직한거에요",
          "speaker_id": "1",
          "time": "20210831 07:47"
        },
        {
          "id": "MDRW2100024419.1.14",
          "form": "비슷한 일을 하신거예요?",
          "original_form": "비슷한 일을 하신거예요?",
          "speaker_id": "2",
          "time": "20210831 07:47"
        },
        {
          "id": "MDRW2100024419.1.15",
          "form": "거기도 사람들은 좋았는데 업무량이 더 많았어요",
          "original_form": "거기도 사람들은 좋았는데 업무량이 더 많았어요",
          "speaker_id": "1",
          "time": "20210831 07:47"
        },
        {
          "id": "MDRW2100024419.1.16",
          "form": "네 비슷한 일이었어요",
          "original_form": "네 비슷한 일이었어요",
          "speaker_id": "1",
          "time": "20210831 07:48"
        },
        {
          "id": "MDRW2100024419.1.17",
          "form": "지금 더 만족하시는 것 같아서 좋아보여요ㅎㅎ",
          "original_form": "지금 더 만족하시는 것 같아서 좋아보여요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 07:48"
        },
        {
          "id": "MDRW2100024419.1.18",
          "form": "name2님은 어느분야 일을 하고싶으세요?",
          "original_form": "&name2&님은 어느분야 일을 하고싶으세요?",
          "speaker_id": "1",
          "time": "20210831 07:48"
        },
        {
          "id": "MDRW2100024419.1.19",
          "form": "전 요즘 엄청 고민중인데, 공무원 시험 준비할까 싶어요ㅎ",
          "original_form": "전 요즘 엄청 고민중인데, 공무원 시험 준비할까 싶어요ㅎ",
