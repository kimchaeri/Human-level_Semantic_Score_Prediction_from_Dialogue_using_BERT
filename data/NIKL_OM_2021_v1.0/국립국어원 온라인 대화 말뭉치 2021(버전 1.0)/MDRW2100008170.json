      "utterance": [
        {
          "id": "MDRW2100008170.1.1",
          "form": "하시는 게임 있으세요?",
          "original_form": "하시는 게임 있으세요?",
          "speaker_id": "2",
          "time": "20210822 13:48"
        },
        {
          "id": "MDRW2100008170.1.2",
          "form": "저는 쿠킹덤하다 접었는데..하시는 겜있으세요?",
          "original_form": "저는 쿠킹덤하다 접었는데..하시는 겜있으세요?",
          "speaker_id": "1",
          "time": "20210822 13:48"
        },
        {
          "id": "MDRW2100008170.1.3",
          "form": "저는 캐시퍼랑 치킨각이요",
          "original_form": "저는 캐시퍼랑 치킨각이요",
          "speaker_id": "2",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008170.1.4",
          "form": "요새 인기 게임은 뭐가있지...",
          "original_form": "요새 인기 게임은 뭐가있지...",
          "speaker_id": "2",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008170.1.5",
          "form": "제2의 나라도 재밌다는데 막상 해보진 않았네요",
          "original_form": "제2의 나라도 재밌다는데 막상 해보진 않았네요",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008170.1.6",
          "form": "저는 좀 고전게임이 좋은가봐요",
          "original_form": "저는 좀 고전게임이 좋은가봐요",
          "speaker_id": "2",
          "time": "20210822 13:53"
        },
        {
          "id": "MDRW2100008170.1.7",
          "form": "어렵지 않은거 ㅋㅋ",
          "original_form": "어렵지 않은거 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:53"
        },
        {
          "id": "MDRW2100008170.1.8",
          "form": "캐시퍼는 진짜 암생각없이 화면만 터치터치 하면되서",
          "original_form": "캐시퍼는 진짜 암생각없이 화면만 터치터치 하면되서",
          "speaker_id": "2",
          "time": "20210822 13:53"
        },
        {
          "id": "MDRW2100008170.1.9",
          "form": "저도 타일깨고 그런거 좋아해요ㅎㅎ시간 잘가서ㅎㅎ",
          "original_form": "저도 타일깨고 그런거 좋아해요ㅎㅎ시간 잘가서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100008170.1.10",
          "form": "캐시퍼는 진짜 깨알같이 포인트 주거든요",
          "original_form": "캐시퍼는 진짜 깨알같이 포인트 주거든요",
          "speaker_id": "2",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100008170.1.11",
          "form": "1포인트 2포인트모아서 만포인트되면 환급 ㅋㅋ",
          "original_form": "1포인트 2포인트모아서 만포인트되면 환급 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100008170.1.12",
          "form": "오 신기하네요ㅎㅎ 게임에 포인트까지",
          "original_form": "오 신기하네요ㅎㅎ 게임에 포인트까지",
          "speaker_id": "1",
          "time": "20210822 14:10"
        },
        {
          "id": "MDRW2100008170.1.13",
          "form": "덕분에 그만두지 못하고있어요 ㅋㅋㅋ",
          "original_form": "덕분에 그만두지 못하고있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008170.1.14",
          "form": "재미도 있고ㅎㅎ 좋을거같은데요ㅎㅎ",
          "original_form": "재미도 있고ㅎㅎ 좋을거같은데요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008170.1.15",
          "form": "어제도 영드보면서 캐시퍼로 땅팠는데 세시간이 날아갔어요",
          "original_form": "어제도 영드보면서 캐시퍼로 땅팠는데 세시간이 날아갔어요",
          "speaker_id": "2",
          "time": "20210822 14:20"
        },
        {
          "id": "MDRW2100008170.1.16",
          "form": "왜 티탄철석이 안나오닛 ㅋㅋ",
          "original_form": "왜 티탄철석이 안나오닛 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:20"
        },
        {
          "id": "MDRW2100008170.1.17",
          "form": "겜하면 시간이 참빠르죠ㅎㅎ",
          "original_form": "겜하면 시간이 참빠르죠ㅎㅎ",
