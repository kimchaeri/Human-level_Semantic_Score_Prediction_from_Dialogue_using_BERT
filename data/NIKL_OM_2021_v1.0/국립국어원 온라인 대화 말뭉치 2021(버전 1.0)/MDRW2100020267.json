      "utterance": [
        {
          "id": "MDRW2100020267.1.1",
          "form": "박효신이 제 초중학교때  막 나왓던거 같아요 ㅋㅋ 연배나오나요.. ㅎㅎ",
          "original_form": "박효신이 제 초중학교때  막 나왓던거 같아요 ㅋㅋ 연배나오나요.. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020267.1.2",
          "form": "ㅋㅋㅋ같은세대일지도.. \n저는 요즘 재택근무해요",
          "original_form": "ㅋㅋㅋ같은세대일지도.. \n저는 요즘 재택근무해요",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020267.1.3",
          "form": "집에서 쉬다가 아르바이트식으로 6개월 단기프로젝트 소개받아서",
          "original_form": "집에서 쉬다가 아르바이트식으로 6개월 단기프로젝트 소개받아서",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020267.1.4",
          "form": "아 무슨 직종이세요?",
          "original_form": "아 무슨 직종이세요?",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020267.1.5",
          "form": "ai쪽이에요 ㅎㅎ",
          "original_form": "ai쪽이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020267.1.6",
          "form": "와 멋있어요!! 전파워문과라 ㅋㅋㅋㅋ",
          "original_form": "와 멋있어요!! 전파워문과라 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020267.1.7",
          "form": "진짜신기하네요!",
          "original_form": "진짜신기하네요!",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020267.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ저도문과에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도문과에요",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020267.1.9",
          "form": "알바로하는거라 저는 데이터수집하는업무하고있어요",
          "original_form": "알바로하는거라 저는 데이터수집하는업무하고있어요",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020267.1.10",
          "form": "오잉 프로그래밍이런거 생각했어요!",
          "original_form": "오잉 프로그래밍이런거 생각했어요!",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020267.1.11",
          "form": "ai 1도 몰라요",
          "original_form": "ai 1도 몰라요",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020267.1.12",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020267.1.13",
          "form": "데이터수집은 어떻게하는건가요?",
          "original_form": "데이터수집은 어떻게하는건가요?",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020267.1.14",
          "form": "업체에서 준 프로그램 다운받아서",
          "original_form": "업체에서 준 프로그램 다운받아서",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020267.1.15",
          "form": "거기서 메뉴얼대로 하기만하면되서",
          "original_form": "거기서 메뉴얼대로 하기만하면되서",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020267.1.16",
          "form": "말이 데이터수집이지",
          "original_form": "말이 데이터수집이지",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020267.1.17",
          "form": "아 그렇구나~",
          "original_form": "아 그렇구나~",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020267.1.18",
          "form": "컴퓨터로 인형눈알붙히기 하는거같아요",
          "original_form": "컴퓨터로 인형눈알붙히기 하는거같아요",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020267.1.19",
          "form": "ㅋㅋㅋㅋㅋ지루해요ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ지루해요ㅠ",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020267.1.20",
          "form": "집에서도 충분히 할수있는 일이군요!",
          "original_form": "집에서도 충분히 할수있는 일이군요!",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020267.1.21",
          "form": "저는 재택이 어려워요 ㅜ",
          "original_form": "저는 재택이 어려워요 ㅜ",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020267.1.22",
          "form": "네네 ㅋㅋ 크게 힘든거아니고 시간도 적게 쓸 수 있어서",
          "original_form": "네네 ㅋㅋ 크게 힘든거아니고 시간도 적게 쓸 수 있어서",
          "speaker_id": "1",
