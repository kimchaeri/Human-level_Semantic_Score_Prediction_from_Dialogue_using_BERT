      "utterance": [
        {
          "id": "MDRW2100013645.1.1",
          "form": "나 어제 친구집놀러가서 고양이랑 실컷놀다왔음 ㅋㅋㅋ 넘나 귀여웡",
          "original_form": "나 어제 친구집놀러가서 고양이랑 실컷놀다왔음 ㅋㅋㅋ 넘나 귀여웡",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013645.1.2",
          "form": "우와.. 부럽다.. ㅋㅋㅋ",
          "original_form": "우와.. 부럽다.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013645.1.3",
          "form": "나도 고양이 짱 좋아하는디...",
          "original_form": "나도 고양이 짱 좋아하는디...",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013645.1.4",
          "form": "몇마리키워?",
          "original_form": "몇마리키워?",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013645.1.5",
          "form": "난 안키우고 친구는 한마리!",
          "original_form": "난 안키우고 친구는 한마리!",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013645.1.6",
          "form": "이름이 name3이야 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이름이 &name3&이야 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013645.1.7",
          "form": "ㅋㅋ name3이면? 하양색?",
          "original_form": "ㅋㅋ &name3&이면? 하양색?",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013645.1.8",
          "form": "웅 하얀색인데 약간 노란색얼룩",
          "original_form": "웅 하얀색인데 약간 노란색얼룩",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100013645.1.9",
          "form": "까칠한데 놀아주면 엄청 좋아해ㅋㅋㅋ 너는 키우는동물 있어?",
          "original_form": "까칠한데 놀아주면 엄청 좋아해ㅋㅋㅋ 너는 키우는동물 있어?",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100013645.1.10",
          "form": "난 나라나키우기도 힘들어..",
          "original_form": "난 나라나키우기도 힘들어..",
          "speaker_id": "1",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100013645.1.11",
          "form": "너는 뭐 키우고싶은거잇어ㅡ",
          "original_form": "너는 뭐 키우고싶은거잇어ㅡ",
          "speaker_id": "1",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100013645.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 나도 누굴 키울수있는처지는 아니라서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 나도 누굴 키울수있는처지는 아니라서",
          "speaker_id": "2",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100013645.1.13",
          "form": "근데 예전에 앵무새 키운적있거든",
          "original_form": "근데 예전에 앵무새 키운적있거든",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100013645.1.14",
          "form": "또 키우고싶긴해",
          "original_form": "또 키우고싶긴해",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100013645.1.15",
          "form": "앵무새 엄청 앙칼지지?",
          "original_form": "앵무새 엄청 앙칼지지?",
          "speaker_id": "1",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100013645.1.16",
          "form": "웅 처음엔 좀 물리고 그랬는데",
          "original_form": "웅 처음엔 좀 물리고 그랬는데",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100013645.1.17",
          "form": "그래서 역시 새는 멍청한가 했거든",
          "original_form": "그래서 역시 새는 멍청한가 했거든",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100013645.1.18",
          "form": "그래두 좀 지나니까 밥주는사람은 아는거같더라 ㅋㅋ",
          "original_form": "그래두 좀 지나니까 밥주는사람은 아는거같더라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100013645.1.19",
          "form": "엄청 똑똑하지 않아?",
          "original_form": "엄청 똑똑하지 않아?",
          "speaker_id": "1",
          "time": "20210825 19:01"
        },
        {
          "id": "MDRW2100013645.1.20",
