      "utterance": [
        {
          "id": "MDRW2100007708.1.1",
          "form": "요즘 날씨가 우중충하네요",
          "original_form": "요즘 날씨가 우중충하네요",
          "speaker_id": "2",
          "time": "20210822 10:37"
        },
        {
          "id": "MDRW2100007708.1.2",
          "form": "가을장마라 그런가봐요",
          "original_form": "가을장마라 그런가봐요",
          "speaker_id": "1",
          "time": "20210822 10:43"
        },
        {
          "id": "MDRW2100007708.1.3",
          "form": "비오는거 안좋아하세요?",
          "original_form": "비오는거 안좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 10:43"
        },
        {
          "id": "MDRW2100007708.1.4",
          "form": "네 습하고 우산들고다니기도 귀찮아서요 ㅠ",
          "original_form": "네 습하고 우산들고다니기도 귀찮아서요 ㅠ",
          "speaker_id": "2",
          "time": "20210822 10:46"
        },
        {
          "id": "MDRW2100007708.1.5",
          "form": "가을장마 지나면 이제 비도 그치겠네요",
          "original_form": "가을장마 지나면 이제 비도 그치겠네요",
          "speaker_id": "1",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007708.1.6",
          "form": "ㅋㅋㅋ이번장마는 짧았다더니 뜬금 가을장마래요",
          "original_form": "ㅋㅋㅋ이번장마는 짧았다더니 뜬금 가을장마래요",
          "speaker_id": "2",
          "time": "20210822 10:49"
        },
        {
          "id": "MDRW2100007708.1.7",
          "form": "네 이제곧 겨울인네",
          "original_form": "네 이제곧 겨울인네",
          "speaker_id": "1",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007708.1.8",
          "form": "눈 좋아하세요?",
          "original_form": "눈 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007708.1.9",
          "form": "내리는거보는건좋은데 녹아서 질퍽질퍽에지는건 싫어요 ㅋㅋ",
          "original_form": "내리는거보는건좋은데 녹아서 질퍽질퍽에지는건 싫어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:55"
        },
        {
          "id": "MDRW2100007708.1.10",
          "form": "그쪽엔 눈 많이내리나요?",
          "original_form": "그쪽엔 눈 많이내리나요?",
          "speaker_id": "1",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007708.1.11",
          "form": "많이 안 내리는거같은데..? 여긴 서울이에요",
          "original_form": "많이 안 내리는거같은데..? 여긴 서울이에요",
          "speaker_id": "2",
          "time": "20210822 11:01"
        },
        {
          "id": "MDRW2100007708.1.12",
          "form": "저희도 경상도라 잘 안내리는편인거같아요",
          "original_form": "저희도 경상도라 잘 안내리는편인거같아요",
          "speaker_id": "1",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100007708.1.13",
          "form": "약간 강원도쪽이 많이 내리는 느낌이에요 ㅋㅋ",
          "original_form": "약간 강원도쪽이 많이 내리는 느낌이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007708.1.14",
          "form": "겨울에 눈 쌓일정도로 내리는걸 못본거같아요",
          "original_form": "겨울에 눈 쌓일정도로 내리는걸 못본거같아요",
          "speaker_id": "1",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007708.1.15",
          "form": "그래도 작년엔 눈 많이내리지않았나요 ㅋㅋ",
          "original_form": "그래도 작년엔 눈 많이내리지않았나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:09"
        },
        {
          "id": "MDRW2100007708.1.16",
          "form": "저희쪽은 1년에 1~2번만 내리더라고요..",
          "original_form": "저희쪽은 1년에 1~2번만 내리더라고요..",
          "speaker_id": "1",
          "time": "20210822 11:18"
        }
      ]
