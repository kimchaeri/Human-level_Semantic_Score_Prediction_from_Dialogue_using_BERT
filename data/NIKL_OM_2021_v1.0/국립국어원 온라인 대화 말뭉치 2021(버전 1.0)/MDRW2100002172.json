      "utterance": [
        {
          "id": "MDRW2100002172.1.1",
          "form": "아 저도 그랬어요",
          "original_form": "아 저도 그랬어요",
          "speaker_id": "1",
          "time": "20210820 01:10"
        },
        {
          "id": "MDRW2100002172.1.2",
          "form": "둘째라 첫째 때문에",
          "original_form": "둘째라 첫째 때문에",
          "speaker_id": "1",
          "time": "20210820 01:10"
        },
        {
          "id": "MDRW2100002172.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210820 01:10"
        },
        {
          "id": "MDRW2100002172.1.4",
          "form": "요새 비싸죠?",
          "original_form": "요새 비싸죠?",
          "speaker_id": "1",
          "time": "20210820 01:10"
        },
        {
          "id": "MDRW2100002172.1.5",
          "form": "2주에200정도요",
          "original_form": "2주에200정도요",
          "speaker_id": "2",
          "time": "20210820 01:11"
        },
        {
          "id": "MDRW2100002172.1.6",
          "form": "비싸네요",
          "original_form": "비싸네요",
          "speaker_id": "1",
          "time": "20210820 01:11"
        },
        {
          "id": "MDRW2100002172.1.7",
          "form": "저때도 비쌌던거 같긴 하네요",
          "original_form": "저때도 비쌌던거 같긴 하네요",
          "speaker_id": "1",
          "time": "20210820 01:11"
        },
        {
          "id": "MDRW2100002172.1.8",
          "form": "백 얼마였는데",
          "original_form": "백 얼마였는데",
          "speaker_id": "1",
          "time": "20210820 01:11"
        },
        {
          "id": "MDRW2100002172.1.9",
          "form": "네 그나마 싼게 저거에요",
          "original_form": "네 그나마 싼게 저거에요",
          "speaker_id": "2",
          "time": "20210820 01:11"
        },
        {
          "id": "MDRW2100002172.1.10",
          "form": "휴 애도 돈으로 키우고 낳는단말이 맞아요",
          "original_form": "휴 애도 돈으로 키우고 낳는단말이 맞아요",
          "speaker_id": "1",
          "time": "20210820 01:12"
        },
        {
          "id": "MDRW2100002172.1.11",
          "form": "없으면 못키우겟어요ㅠ",
          "original_form": "없으면 못키우겟어요ㅠ",
          "speaker_id": "2",
          "time": "20210820 01:12"
        },
        {
          "id": "MDRW2100002172.1.12",
          "form": "출산 장려금은 나오나요?",
          "original_form": "출산 장려금은 나오나요?",
          "speaker_id": "1",
          "time": "20210820 01:12"
        },
        {
          "id": "MDRW2100002172.1.13",
          "form": "맞아요 못키우죠",
          "original_form": "맞아요 못키우죠",
          "speaker_id": "1",
          "time": "20210820 01:12"
        },
        {
          "id": "MDRW2100002172.1.14",
          "form": "전 그런것도 못받았네요",
          "original_form": "전 그런것도 못받았네요",
          "speaker_id": "1",
          "time": "20210820 01:12"
        },
        {
          "id": "MDRW2100002172.1.15",
          "form": "내년에는 더 확대됫다는데",
          "original_form": "내년에는 더 확대됫다는데",
          "speaker_id": "2",
          "time": "20210820 01:13"
        },
        {
          "id": "MDRW2100002172.1.16",
          "form": "그럼 좋겠네요",
          "original_form": "그럼 좋겠네요",
          "speaker_id": "1",
          "time": "20210820 01:13"
        },
        {
          "id": "MDRW2100002172.1.17",
          "form": "저는 9월 출산이라 혜택이 적은거같아요ㅎ",
          "original_form": "저는 9월 출산이라 혜택이 적은거같아요ㅎ",
          "speaker_id": "2",
          "time": "20210820 01:13"
        },
        {
          "id": "MDRW2100002172.1.18",
          "form": "아 그렇죠 ㅜㅜ아쉽네요",
          "original_form": "아 그렇죠 ㅜㅜ아쉽네요",
          "speaker_id": "1",
          "time": "20210820 01:13"
        },
        {
          "id": "MDRW2100002172.1.19",
          "form": "아이를 낳으라면서 너무 돈을 안줘요",
          "original_form": "아이를 낳으라면서 너무 돈을 안줘요",
          "speaker_id": "1",
          "time": "20210820 01:13"
        },
        {
          "id": "MDRW2100002172.1.20",
          "form": "지원이 필요해요",
          "original_form": "지원이 필요해요",
          "speaker_id": "1",
          "time": "20210820 01:14"
        },
        {
          "id": "MDRW2100002172.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 01:14"
        },
        {
          "id": "MDRW2100002172.1.22",
          "form": "애키우느라 영화관도 못가고",
          "original_form": "애키우느라 영화관도 못가고",
          "speaker_id": "2",
          "time": "20210820 01:14"
        },
        {
          "id": "MDRW2100002172.1.23",
          "form": "공연도 못보고",
          "original_form": "공연도 못보고",
          "speaker_id": "2",
          "time": "20210820 01:14"
        },
        {
          "id": "MDRW2100002172.1.24",
          "form": "애낳고 조리정도는 해줘야죠",
          "original_form": "애낳고 조리정도는 해줘야죠",
          "speaker_id": "1",
          "time": "20210820 01:14"
        },
        {
          "id": "MDRW2100002172.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210820 01:14"
        },
        {
          "id": "MDRW2100002172.1.26",
          "form": "코로나로못가 아이로못가",
          "original_form": "코로나로못가 아이로못가",
          "speaker_id": "1",
          "time": "20210820 01:14"
        }
      ]
