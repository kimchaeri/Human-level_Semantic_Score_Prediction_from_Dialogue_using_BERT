      "utterance": [
        {
          "id": "MDRW2100038530.1.1",
          "form": "그거 지금도 그렇다고 들었어요!!",
          "original_form": "그거 지금도 그렇다고 들었어요!!",
          "speaker_id": "2",
          "time": "20210902 15:26"
        },
        {
          "id": "MDRW2100038530.1.2",
          "form": "유기실만 들어본 저에겐 새로운 충격이었어요",
          "original_form": "유기실만 들어본 저에겐 새로운 충격이었어요",
          "speaker_id": "2",
          "time": "20210902 15:26"
        },
        {
          "id": "MDRW2100038530.1.3",
          "form": "아 지금도 그래요?",
          "original_form": "아 지금도 그래요?",
          "speaker_id": "1",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100038530.1.4",
          "form": "그쵸 저도 충격이었어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쵸 저도 충격이었어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100038530.1.5",
          "form": "그래서 조원이랑 친해지기 힘들고....",
          "original_form": "그래서 조원이랑 친해지기 힘들고....",
          "speaker_id": "1",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100038530.1.6",
          "form": "그리고 조원에 따라서 진짜 협조가 되냐 안되냐 컸어요",
          "original_form": "그리고 조원에 따라서 진짜 협조가 되냐 안되냐 컸어요",
          "speaker_id": "1",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100038530.1.7",
          "form": "사실 저는 화실기할 때 만났던 조원들한테 인류애를 잃었었어요 ㅋㅋㅋㅋ",
          "original_form": "사실 저는 화실기할 때 만났던 조원들한테 인류애를 잃었었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100038530.1.8",
          "form": "name3 교수님 실험졸업요건 바꾼거부터시작해서",
          "original_form": "&name3& 교수님 실험졸업요건 바꾼거부터시작해서",
          "speaker_id": "2",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100038530.1.9",
          "form": "많은것들을 이루셨네요....",
          "original_form": "많은것들을 이루셨네요....",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100038530.1.10",
          "form": "하 화실기...벌써 무서워요ㅋㅋ",
          "original_form": "하 화실기...벌써 무서워요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100038530.1.11",
          "form": "아니그니까요",
          "original_form": "아니그니까요",
          "speaker_id": "1",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100038530.1.12",
          "form": "제가 이유없이 싫어하는",
          "original_form": "제가 이유없이 싫어하는",
          "speaker_id": "1",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100038530.1.13",
          "form": "랩 중 하나,,,",
          "original_form": "랩 중 하나,,,",
          "speaker_id": "1",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100038530.1.14",
          "form": "그냥 비호감입니다...",
          "original_form": "그냥 비호감입니다...",
          "speaker_id": "1",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100038530.1.15",
          "form": "이유가 없다뇨 너무 명확한걸욬ㅋㅋ",
          "original_form": "이유가 없다뇨 너무 명확한걸욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100038530.1.16",
          "form": "당황스러운 학생들....",
          "original_form": "당황스러운 학생들....",
          "speaker_id": "2",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100038530.1.17",
          "form": "ㅋㅋㅋㅋㅋ 그쵸...",
          "original_form": "ㅋㅋㅋㅋㅋ 그쵸...",
          "speaker_id": "1",
          "time": "20210902 15:31"
        },
        {
          "id": "MDRW2100038530.1.18",
          "form": "정말 비호감이에요...",
          "original_form": "정말 비호감이에요...",
          "speaker_id": "1",
          "time": "20210902 15:31"
        },
        {
          "id": "MDRW2100038530.1.19",
          "form": "학생들 고생시키기 위해",
          "original_form": "학생들 고생시키기 위해",
          "speaker_id": "1",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100038530.1.20",
          "form": "머리 싸매는 이상한 교수님들...",
          "original_form": "머리 싸매는 이상한 교수님들...",
          "speaker_id": "1",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100038530.1.21",
          "form": "진짜요ㅋㅋㅋㄱ",
          "original_form": "진짜요ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100038530.1.22",
          "form": "언니 오늘 반가웠어요",
          "original_form": "언니 오늘 반가웠어요",
          "speaker_id": "2",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100038530.1.23",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100038530.1.24",
          "form": "저두요 저두요",
          "original_form": "저두요 저두요",
          "speaker_id": "1",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100038530.1.25",
          "form": "민초볼도 너무 고마워요!!!",
          "original_form": "민초볼도 너무 고마워요!!!",
          "speaker_id": "1",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100038530.1.26",
          "form": "저 얼떨결에",
          "original_form": "저 얼떨결에",
          "speaker_id": "1",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100038530.1.27",
          "form": "쟝교수님 방 사람들이랑 내일",
          "original_form": "쟝교수님 방 사람들이랑 내일",
          "speaker_id": "1",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100038530.1.28",
          "form": "밥 먹기로 했어요!!!!",
          "original_form": "밥 먹기로 했어요!!!!",
          "speaker_id": "1",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100038530.1.29",
          "form": "헉 신기하다",
          "original_form": "헉 신기하다",
          "speaker_id": "2",
