      "utterance": [
        {
          "id": "MDRW2100018802.1.1",
          "form": "건강 관심 많으세요?",
          "original_form": "건강 관심 많으세요?",
          "speaker_id": "1",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100018802.1.2",
          "form": "네! 당류 줄이기 하고있어요!",
          "original_form": "네! 당류 줄이기 하고있어요!",
          "speaker_id": "2",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100018802.1.3",
          "form": "그럼 탄수화물도 줄이시나여?",
          "original_form": "그럼 탄수화물도 줄이시나여?",
          "speaker_id": "1",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100018802.1.4",
          "form": "아뇨 음료나 아이스크림만 좀 줄이려고해요! 운동하시나요?",
          "original_form": "아뇨 음료나 아이스크림만 좀 줄이려고해요! 운동하시나요?",
          "speaker_id": "2",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100018802.1.5",
          "form": "맨몸운동 틈틈히 하고있어요..",
          "original_form": "맨몸운동 틈틈히 하고있어요..",
          "speaker_id": "1",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018802.1.6",
          "form": "근데 과자를 좋아해서 살이 안빤지네요",
          "original_form": "근데 과자를 좋아해서 살이 안빤지네요",
          "speaker_id": "1",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018802.1.7",
          "form": "유튜브보고 따라하시나요?",
          "original_form": "유튜브보고 따라하시나요?",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018802.1.8",
          "form": "오래해서 그냥하고있어요..\r\n가끔 새로운거 해볼려고할때 검색하고요",
          "original_form": "오래해서 그냥하고있어요..\r\n가끔 새로운거 해볼려고할때 검색하고요",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100018802.1.9",
          "form": "좋아하는 패션브랜드 있으세요?",
          "original_form": "좋아하는 패션브랜드 있으세요?",
          "speaker_id": "2",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018802.1.10",
          "form": "어디꺼 자주입으세여?",
          "original_form": "어디꺼 자주입으세여?",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018802.1.11",
          "form": "가성비의 지오다노요... ㅋㅋㅋ",
          "original_form": "가성비의 지오다노요... ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018802.1.12",
          "form": "타미힐피거 좋아해요ㅋㅋ아시나요?",
          "original_form": "타미힐피거 좋아해요ㅋㅋ아시나요?",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018802.1.13",
          "form": "로고 너무 이쁘져...",
          "original_form": "로고 너무 이쁘져...",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018802.1.14",
          "form": "가슴팍에 딱 박힌거 티 두개있어요.ㅋㅋㅋ",
          "original_form": "가슴팍에 딱 박힌거 티 두개있어요.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018802.1.15",
          "form": "반팔인가요?",
          "original_form": "반팔인가요?",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018802.1.16",
          "form": "네.. 옛날에 유행한 빨강하양 사각형 크게 박힌거라..",
          "original_form": "네.. 옛날에 유행한 빨강하양 사각형 크게 박힌거라..",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018802.1.17",
          "form": "올해도 심플한거 한개삿어요..",
          "original_form": "올해도 심플한거 한개삿어요..",
          "speaker_id": "1",
          "time": "20210827 12:34"
        }
      ]
