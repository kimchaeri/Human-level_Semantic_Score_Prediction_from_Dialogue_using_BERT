        {
          "id": "MDRW2100022898.1.1",
          "form": "잘은 못하지만 좋아합니다 ...",
          "original_form": "잘은 못하지만 좋아합니다 ...{emoji:♡}",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022898.1.2",
          "form": "하트뭐예요",
          "original_form": "하트뭐예요",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022898.1.3",
          "form": "뭔 술 좋아하세요",
          "original_form": "뭔 술 좋아하세요",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022898.1.4",
          "form": "전 쏘주요",
          "original_form": "전 쏘주요",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022898.1.5",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022898.1.6",
          "form": "전 흑맥이랑 와인이요",
          "original_form": "전 흑맥이랑 와인이요",
          "speaker_id": "1",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022898.1.7",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "1",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022898.1.8",
          "form": "오 흑맥 어떤거 드세요",
          "original_form": "오 흑맥 어떤거 드세요",
          "speaker_id": "2",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022898.1.9",
          "form": "전 맥주 잘 못 마셔요",
          "original_form": "전 맥주 잘 못 마셔요",
          "speaker_id": "2",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022898.1.10",
          "form": "칭따오거 맛잇어요",
          "original_form": "칭따오거 맛잇어요",
          "speaker_id": "1",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100022898.1.11",
          "form": "노맛인데요",
          "original_form": "노맛인데요",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100022898.1.12",
          "form": "6ㅏ실 안먹어봤어요",
          "original_form": "6ㅏ실 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100022898.1.13",
          "form": "왜 맛있나요 ?",
          "original_form": "왜 맛있나요 ?",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100022898.1.14",
          "form": "안 먹어봣난데 무슨 노맛이에요",
          "original_form": "안 먹어봣난데 무슨 노맛이에요",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100022898.1.15",
          "form": "부드러워요",
          "original_form": "부드러워요",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100022898.1.16",
          "form": "카스테라도 아니도",
          "original_form": "카스테라도 아니도",
          "speaker_id": "2",
          "time": "20210831 13:00"
        },
        {
          "id": "MDRW2100022898.1.17",
          "form": "맥주가 어떨게 부드러워요",
          "original_form": "맥주가 어떨게 부드러워요",
          "speaker_id": "2",
          "time": "20210831 13:00"
        },
        {
          "id": "MDRW2100022898.1.18",
          "form": "님이 안 마셔봐서 그래요",
          "original_form": "님이 안 마셔봐서 그래요",
          "speaker_id": "1",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100022898.1.19",
          "form": "췽 똬~오",
          "original_form": "췽 똬~오",
          "speaker_id": "2",
          "time": "20210831 15:31"
        },
        {
          "id": "MDRW2100022898.1.20",
          "form": "무슨뜻이에요",
          "original_form": "무슨뜻이에요",
          "speaker_id": "2",
          "time": "20210831 15:31"
        },
        {
          "id": "MDRW2100022898.1.21",
          "form": "맥주요",
          "original_form": "맥주요",
          "speaker_id": "1",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100022898.1.22",
          "form": "이름진짜 대충지었네요",
          "original_form": "이름진짜 대충지었네요",
          "speaker_id": "2",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100022898.1.23",
          "form": "그래도 외우기 쉽잖아요",
          "original_form": "그래도 외우기 쉽잖아요",
          "speaker_id": "1",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100022898.1.24",
          "form": "까르보나라 사주세요",
          "original_form": "까르보나라 사주세요",
