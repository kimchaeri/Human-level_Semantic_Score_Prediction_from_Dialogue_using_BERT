      "utterance": [
        {
          "id": "MDRW2100029639.1.1",
          "form": "박물관 가고싶어요",
          "original_form": "박물관 가고싶어요",
          "speaker_id": "2",
          "time": "20210904 12:29"
        },
        {
          "id": "MDRW2100029639.1.2",
          "form": "다음에 같이 갈까?",
          "original_form": "다음에 같이 갈까?",
          "speaker_id": "1",
          "time": "20210904 12:30"
        },
        {
          "id": "MDRW2100029639.1.3",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210904 12:31"
        },
        {
          "id": "MDRW2100029639.1.4",
          "form": "박물관 시원하고 재밌어서",
          "original_form": "박물관 시원하고 재밌어서",
          "speaker_id": "2",
          "time": "20210904 12:31"
        },
        {
          "id": "MDRW2100029639.1.5",
          "form": "어렸을때 많이 데려가줬잖아",
          "original_form": "어렸을때 많이 데려가줬잖아",
          "speaker_id": "1",
          "time": "20210904 12:32"
        },
        {
          "id": "MDRW2100029639.1.6",
          "form": "그래서 좋았어요 ㅋㅋㅋ",
          "original_form": "그래서 좋았어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:32"
        },
        {
          "id": "MDRW2100029639.1.7",
          "form": "넌 여행보다 그런거 좋아했으니까",
          "original_form": "넌 여행보다 그런거 좋아했으니까",
          "speaker_id": "1",
          "time": "20210904 12:32"
        },
        {
          "id": "MDRW2100029639.1.8",
          "form": "실내에서 돌아다니는것도 재밌고",
          "original_form": "실내에서 돌아다니는것도 재밌고",
          "speaker_id": "2",
          "time": "20210904 12:32"
        },
        {
          "id": "MDRW2100029639.1.9",
          "form": "보는것도 재밌어요",
          "original_form": "보는것도 재밌어요",
          "speaker_id": "2",
          "time": "20210904 12:32"
        },
        {
          "id": "MDRW2100029639.1.10",
          "form": "신기한거 많긴 하더라",
          "original_form": "신기한거 많긴 하더라",
          "speaker_id": "1",
          "time": "20210904 12:32"
        },
        {
          "id": "MDRW2100029639.1.11",
          "form": "역사 교과서에 나올만한 거랑",
          "original_form": "역사 교과서에 나올만한 거랑",
          "speaker_id": "1",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100029639.1.12",
          "form": "전시회같은건 주변에 없어서 ㅠㅠ",
          "original_form": "전시회같은건 주변에 없어서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100029639.1.13",
          "form": "지방이니까 어쩔수없지",
          "original_form": "지방이니까 어쩔수없지",
          "speaker_id": "1",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100029639.1.14",
          "form": "서울 갔을때 그런거 많더라구요",
          "original_form": "서울 갔을때 그런거 많더라구요",
          "speaker_id": "2",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100029639.1.15",
          "form": "재밌었니?",
          "original_form": "재밌었니?",
          "speaker_id": "1",
