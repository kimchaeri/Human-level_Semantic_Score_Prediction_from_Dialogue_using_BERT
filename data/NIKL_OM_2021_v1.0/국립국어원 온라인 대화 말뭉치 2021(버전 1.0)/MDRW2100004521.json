      "utterance": [
        {
          "id": "MDRW2100004521.1.1",
          "form": "너 저녁 뭐 먹을거야?",
          "original_form": "너 저녁 뭐 먹을거야?",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004521.1.2",
          "form": "나 엄마가 떡볶이 해주기로 약속했어",
          "original_form": "나 엄마가 떡볶이 해주기로 약속했어",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004521.1.3",
          "form": "그말 들으니 나더 매운더 먹고싶어",
          "original_form": "그말 들으니 나더 매운더 먹고싶어",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004521.1.4",
          "form": "애슐리 로제 떡볶이가 너무 그리워",
          "original_form": "애슐리 로제 떡볶이가 너무 그리워",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004521.1.5",
          "form": "아ㅋㅋㅋ 울엄마도 로제를 제일 좋아해",
          "original_form": "아ㅋㅋㅋ 울엄마도 로제를 제일 좋아해",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004521.1.6",
          "form": "하지만 오늘 먹을거는",
          "original_form": "하지만 오늘 먹을거는",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004521.1.7",
          "form": "빨간 오리지널 떡볶이다!",
          "original_form": "빨간 오리지널 떡볶이다!",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004521.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004521.1.9",
          "form": "나 지금 입에 침 고였는데",
          "original_form": "나 지금 입에 침 고였는데",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004521.1.10",
          "form": "나는 라면먹으려고",
          "original_form": "나는 라면먹으려고",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004521.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004521.1.12",
          "form": "라면도 좋지",
          "original_form": "라면도 좋지",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004521.1.13",
          "form": "어제 백종원의 세계음식 봤는데",
          "original_form": "어제 백종원의 세계음식 봤는데",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004521.1.14",
          "form": "라면이 나오더라",
          "original_form": "라면이 나오더라",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004521.1.15",
          "form": "그것도 새벽에...",
          "original_form": "그것도 새벽에...",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004521.1.16",
          "form": "전에 피자 나와서 시카고 피자먹었어",
          "original_form": "전에 피자 나와서 시카고 피자먹었어",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004521.1.17",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004521.1.18",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004521.1.19",
          "form": "시카고 피자",
          "original_form": "시카고 피자",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004521.1.20",
          "form": "이마트에서 파는거",
          "original_form": "이마트에서 파는거",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004521.1.21",
          "form": "어디서 시켜먹었어?",
          "original_form": "어디서 시켜먹었어?",
          "speaker_id": "2",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004521.1.22",
          "form": "괜찮더라",
          "original_form": "괜찮더라",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004521.1.23",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004521.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004521.1.25",
          "form": "시켜먹으려고 했는데",
          "original_form": "시켜먹으려고 했는데",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004521.1.26",
          "form": "배달이 없어...",
          "original_form": "배달이 없어...",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004521.1.27",
          "form": "아...아...",
          "original_form": "아...아...",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004521.1.28",
          "form": "우리집은 은근 배달이 잘 안오더라",
          "original_form": "우리집은 은근 배달이 잘 안오더라",
          "speaker_id": "1",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004521.1.29",
          "form": "그래서 이마트에서 맨날 사다먹어",
          "original_form": "그래서 이마트에서 맨날 사다먹어",
          "speaker_id": "1",
          "time": "20210821 17:09"
        }
      ]
    }
