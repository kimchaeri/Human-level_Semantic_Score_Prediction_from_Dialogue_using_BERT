      "utterance": [
        {
          "id": "MDRW2100016339.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100016339.1.2",
          "form": "요즘 날씨가 서늘해졌어요",
          "original_form": "요즘 날씨가 서늘해졌어요",
          "speaker_id": "1",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100016339.1.3",
          "form": "이제 가을인것같아요",
          "original_form": "이제 가을인것같아요",
          "speaker_id": "1",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100016339.1.4",
          "form": "네~! 이곳은 아직 덥지만",
          "original_form": "네~! 이곳은 아직 덥지만",
          "speaker_id": "2",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100016339.1.5",
          "form": "그래도 여름의 끝인거같아요",
          "original_form": "그래도 여름의 끝인거같아요",
          "speaker_id": "2",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100016339.1.6",
          "form": "남부쪽이세요?",
          "original_form": "남부쪽이세요?",
          "speaker_id": "1",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100016339.1.7",
          "form": "대프리카임돠 ㅎㅎ",
          "original_form": "대프리카임돠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100016339.1.8",
          "form": "ㅋㅋㅋ 덥겠어요",
          "original_form": "ㅋㅋㅋ 덥겠어요",
          "speaker_id": "1",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100016339.1.9",
          "form": "몇일전에 비 많이왔죠?",
          "original_form": "몇일전에 비 많이왔죠?",
          "speaker_id": "1",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016339.1.10",
          "form": "번개가장난안던데",
          "original_form": "번개가장난안던데",
          "speaker_id": "1",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016339.1.11",
          "form": "그나마 올여름은 살만햇어요ㅜ",
          "original_form": "그나마 올여름은 살만햇어요ㅜ",
          "speaker_id": "2",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016339.1.12",
          "form": "대구는 동남아처럼 스콜 왓어요ㅜ",
          "original_form": "대구는 동남아처럼 스콜 왓어요ㅜ",
          "speaker_id": "2",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016339.1.13",
          "form": "왠지 올여름더위는짧았던것같아요",
          "original_form": "왠지 올여름더위는짧았던것같아요",
          "speaker_id": "1",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016339.1.14",
          "form": "대구 한번 가봤어요",
          "original_form": "대구 한번 가봤어요",
          "speaker_id": "1",
          "time": "20210826 12:08"
        },
        {
          "id": "MDRW2100016339.1.15",
          "form": "여기는경기도예요!",
          "original_form": "여기는경기도예요!",
          "speaker_id": "1",
          "time": "20210826 12:08"
        },
        {
          "id": "MDRW2100016339.1.16",
          "form": "가을이 좀더 길어주길 바라고있어요 ㅎㅎ",
          "original_form": "가을이 좀더 길어주길 바라고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:08"
        },
        {
          "id": "MDRW2100016339.1.17",
          "form": "그랬으면좋겠어요!",
          "original_form": "그랬으면좋겠어요!",
          "speaker_id": "1",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100016339.1.18",
          "form": "가을은또 금방 지나가잖아요.",
          "original_form": "가을은또 금방 지나가잖아요.",
          "speaker_id": "1",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100016339.1.19",
          "form": "지구가  아파서ㅠ 기상이변이 진짜 심각해지는것같아요",
          "original_form": "지구가  아파서ㅠ 기상이변이 진짜 심각해지는것같아요",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016339.1.20",
          "form": "ㅠㅠ짧아서 아쉬워요ㅠㅠ",
          "original_form": "ㅠㅠ짧아서 아쉬워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016339.1.21",
          "form": "맞아요ㅜㅜㅜ사람이 제일 나쁨",
          "original_form": "맞아요ㅜㅜㅜ사람이 제일 나쁨",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016339.1.22",
          "form": "맞아요ㅠ 그래두 나라별로 대책을세우고 있긴하니깐. .",
          "original_form": "맞아요ㅠ 그래두 나라별로 대책을세우고 있긴하니깐. .",
          "speaker_id": "1",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016339.1.23",
          "form": "요즘은 미세먼지는 좀 괜찮은것같아요",
          "original_form": "요즘은 미세먼지는 좀 괜찮은것같아요",
          "speaker_id": "1",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016339.1.24",
          "form": "좋아하는 계절 잇으세요?",
          "original_form": "좋아하는 계절 잇으세요?",
          "speaker_id": "2",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016339.1.25",
          "form": "봄이요!",
          "original_form": "봄이요!",
          "speaker_id": "1",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016339.1.26",
          "form": "시작하는 느낌이좋아요",
          "original_form": "시작하는 느낌이좋아요",
          "speaker_id": "1",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016339.1.27",
          "form": "저는 여름 빼고 싹 다 좋아요",
          "original_form": "저는 여름 빼고 싹 다 좋아요",
          "speaker_id": "2",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016339.1.28",
          "form": "봄 좋죵",
          "original_form": "봄 좋죵",
          "speaker_id": "2",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016339.1.29",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016339.1.30",
          "form": "여름이 너무덥죠ㅋㅋ",
          "original_form": "여름이 너무덥죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016339.1.31",
          "form": "네ㅠㅠ힘드럿지만",
          "original_form": "네ㅠㅠ힘드럿지만",
          "speaker_id": "2",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016339.1.32",
          "form": "이제 가을이니까",
          "original_form": "이제 가을이니까",
          "speaker_id": "2",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016339.1.33",
          "form": "추석지나면 시원할꺼같아욤",
          "original_form": "추석지나면 시원할꺼같아욤",
          "speaker_id": "2",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016339.1.34",
          "form": "작년에는추석까지 더웠던것같은데 올해는좀 나은것같아요",
          "original_form": "작년에는추석까지 더웠던것같은데 올해는좀 나은것같아요",
          "speaker_id": "1",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100016339.1.35",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100016339.1.36",
          "form": "그래두",
          "original_form": "그래두",
          "speaker_id": "2",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100016339.1.37",
          "form": "아직 비가 왓다갓다하니",
          "original_form": "아직 비가 왓다갓다하니",
          "speaker_id": "2",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100016339.1.38",
          "form": "우산 꼭 챙겨다니세용",
          "original_form": "우산 꼭 챙겨다니세용",
          "speaker_id": "2",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100016339.1.39",
          "form": "ㅎㅎ감사합니당",
          "original_form": "ㅎㅎ감사합니당",
          "speaker_id": "1",
          "time": "20210826 12:20"
        }
      ]
    }
