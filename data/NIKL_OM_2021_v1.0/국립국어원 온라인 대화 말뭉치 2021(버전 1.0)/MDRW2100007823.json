      "utterance": [
        {
          "id": "MDRW2100007823.1.1",
          "form": "요즘 먹곰싶은 음료있니",
          "original_form": "요즘 먹곰싶은 음료있니",
          "speaker_id": "1",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100007823.1.2",
          "form": "나지금 먹고있지",
          "original_form": "나지금 먹고있지",
          "speaker_id": "2",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007823.1.3",
          "form": "핫식스",
          "original_form": "핫식스",
          "speaker_id": "2",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007823.1.4",
          "form": "그건 카페인 넘 많은것 아니니",
          "original_form": "그건 카페인 넘 많은것 아니니",
          "speaker_id": "1",
          "time": "20210822 11:07"
        },
        {
          "id": "MDRW2100007823.1.5",
          "form": "커피랑 같은거야",
          "original_form": "커피랑 같은거야",
          "speaker_id": "2",
          "time": "20210822 11:07"
        },
        {
          "id": "MDRW2100007823.1.6",
          "form": "그래도 몸에 안 좋을것 같은데",
          "original_form": "그래도 몸에 안 좋을것 같은데",
          "speaker_id": "1",
          "time": "20210822 11:08"
        },
        {
          "id": "MDRW2100007823.1.7",
          "form": "공부하는데 넘 졸려서 먹는거야",
          "original_form": "공부하는데 넘 졸려서 먹는거야",
          "speaker_id": "2",
          "time": "20210822 11:08"
        },
        {
          "id": "MDRW2100007823.1.8",
          "form": "졸음을 참는데 다른 음료는 없을까?",
          "original_form": "졸음을 참는데 다른 음료는 없을까?",
          "speaker_id": "1",
          "time": "20210822 11:09"
        },
        {
          "id": "MDRW2100007823.1.9",
          "form": "난 모르는데 ?\r\n커피 몇잔 보다 낫지",
          "original_form": "난 모르는데 ?\r\n커피 몇잔 보다 낫지",
          "speaker_id": "2",
          "time": "20210822 11:10"
        },
        {
          "id": "MDRW2100007823.1.10",
          "form": "나두 커피 양을 좀 줄여야 하는데 어렵네 ?",
          "original_form": "나두 커피 양을 좀 줄여야 하는데 어렵네 ?",
          "speaker_id": "1",
          "time": "20210822 11:11"
        },
        {
          "id": "MDRW2100007823.1.11",
          "form": "조금만 줄이면 좋을것 같아",
          "original_form": "조금만 줄이면 좋을것 같아",
          "speaker_id": "2",
          "time": "20210822 11:12"
        },
        {
          "id": "MDRW2100007823.1.12",
          "form": "그 조금이 엄청 어럽다",
          "original_form": "그 조금이 엄청 어럽다",
          "speaker_id": "1",
          "time": "20210822 11:12"
        },
        {
          "id": "MDRW2100007823.1.13",
          "form": "그럼 커피 한잔을 두번 나누어서 먹는건 어떻까 .",
          "original_form": "그럼 커피 한잔을 두번 나누어서 먹는건 어떻까 .",
          "speaker_id": "2",
          "time": "20210822 11:13"
        },
        {
          "id": "MDRW2100007823.1.14",
          "form": "좋은 방법 이긴한데 할수 있은까 ?",
          "original_form": "좋은 방법 이긴한데 할수 있은까 ?",
          "speaker_id": "1",
          "time": "20210822 11:13"
        },
        {
          "id": "MDRW2100007823.1.15",
          "form": "할수 있도록 노력을 해보세요",
          "original_form": "할수 있도록 노력을 해보세요",
          "speaker_id": "2",
          "time": "20210822 11:14"
        },
        {
          "id": "MDRW2100007823.1.16",
          "form": "그래 노력과 노력을 해볼께",
          "original_form": "그래 노력과 노력을 해볼께",
