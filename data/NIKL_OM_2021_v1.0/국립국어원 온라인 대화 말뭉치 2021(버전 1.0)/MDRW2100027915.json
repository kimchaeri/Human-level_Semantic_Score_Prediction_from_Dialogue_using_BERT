      "utterance": [
        {
          "id": "MDRW2100027915.1.1",
          "form": "이건... 소아온인가?",
          "original_form": "이건... 소아온인가?",
          "speaker_id": "1",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027915.1.2",
          "form": "아 그런거야?",
          "original_form": "아 그런거야?",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027915.1.3",
          "form": "오메가버스 하하버스 이런건줄",
          "original_form": "오메가버스 하하버스 이런건줄",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027915.1.4",
          "form": "가상현실 이계통 아냐?",
          "original_form": "가상현실 이계통 아냐?",
          "speaker_id": "1",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027915.1.5",
          "form": "그런 거 언제 생기려나몰라",
          "original_form": "그런 거 언제 생기려나몰라",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027915.1.6",
          "form": "내가 죽기 전에 그런 겜을 할 수는 있을까ㅠ",
          "original_form": "내가 죽기 전에 그런 겜을 할 수는 있을까ㅠ",
          "speaker_id": "2",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027915.1.7",
          "form": "가상현실은 다들 아니까ㅋㅋㅋㅋㅋ 그 예시가 소아온인거지!!!",
          "original_form": "가상현실은 다들 아니까ㅋㅋㅋㅋㅋ 그 예시가 소아온인거지!!!",
          "speaker_id": "1",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027915.1.8",
          "form": "할 수 있을 거 같음 ㅇ0ㅇ",
          "original_form": "할 수 있을 거 같음 ㅇ0ㅇ",
          "speaker_id": "1",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027915.1.9",
          "form": "가상현실 겜처럼 몰입이 가능한지는 모르겠는데",
          "original_form": "가상현실 겜처럼 몰입이 가능한지는 모르겠는데",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027915.1.10",
          "form": "요즘도 눈에 끼고 하는 거는 실감나서 허우적거리지 않나",
          "original_form": "요즘도 눈에 끼고 하는 거는 실감나서 허우적거리지 않나",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027915.1.11",
          "form": "근데 허우적거리지않는게",
          "original_form": "근데 허우적거리지않는게",
          "speaker_id": "2",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027915.1.12",
          "form": "중요한 거 아니야?",
          "original_form": "중요한 거 아니야?",
          "speaker_id": "2",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027915.1.13",
          "form": "허허벌판을 뛰어도",
          "original_form": "허허벌판을 뛰어도",
          "speaker_id": "2",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027915.1.14",
          "form": "내 몸은 잠들어잇어야",
          "original_form": "내 몸은 잠들어잇어야",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027915.1.15",
          "form": "집안에사 난리 부르수 안치지",
          "original_form": "집안에사 난리 부르수 안치지",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027915.1.16",
          "form": "가상현실 겜도 사실 날 재우는 게 아니잖아",
          "original_form": "가상현실 겜도 사실 날 재우는 게 아니잖아",
          "speaker_id": "1",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027915.1.17",
          "form": "정확히는 그 부분을 뭉갠 것에 가깝지만ㅋㅋㅋㅋㅋ",
          "original_form": "정확히는 그 부분을 뭉갠 것에 가깝지만ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027915.1.18",
          "form": "뇌속에서만 움직어야해",
          "original_form": "뇌속에서만 움직어야해",
          "speaker_id": "2",
          "time": "20210902 16:15"
        },
        {
          "id": "MDRW2100027915.1.19",
          "form": "그건 좀 오래걸릴 거 같은데",
          "original_form": "그건 좀 오래걸릴 거 같은데",
          "speaker_id": "1",
          "time": "20210902 16:15"
        },
        {
          "id": "MDRW2100027915.1.20",
          "form": "위험해서 안될거같기도 라고",
          "original_form": "위험해서 안될거같기도 라고",
          "speaker_id": "1",
          "time": "20210902 16:15"
        },
        {
          "id": "MDRW2100027915.1.21",
          "form": "서버가 끊기면 다들 뇌사상태가 되는거지",
          "original_form": "서버가 끊기면 다들 뇌사상태가 되는거지",
          "speaker_id": "1",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100027915.1.22",
          "form": "근데 유레카처럼 환자 치료 재활에",
          "original_form": "근데 유레카처럼 환자 치료 재활에",
          "speaker_id": "1",
          "time": "20210902 16:19"
        },
        {
          "id": "MDRW2100027915.1.23",
          "form": "쓰이면 좋겠다",
          "original_form": "쓰이면 좋겠다",
          "speaker_id": "1",
          "time": "20210902 16:19"
        },
        {
          "id": "MDRW2100027915.1.24",
          "form": "사실 엔터보다는 이부분을 우선으로 하고 있겠지만...",
          "original_form": "사실 엔터보다는 이부분을 우선으로 하고 있겠지만...",
          "speaker_id": "1",
          "time": "20210902 16:19"
        },
        {
          "id": "MDRW2100027915.1.25",
          "form": "그렇간하지... 난 브이알은 어지러울거같아",
          "original_form": "그렇간하지... 난 브이알은 어지러울거같아",
          "speaker_id": "2",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027915.1.26",
          "form": "아직은 좀 정신 사납더라ㅋㅋㅋㅋ 기기도 무겁구",
          "original_form": "아직은 좀 정신 사납더라ㅋㅋㅋㅋ 기기도 무겁구",
          "speaker_id": "1",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027915.1.27",
          "form": "근데 그 브이알 꽂고 리겜 하는건 넘 재미있어 보였어",
          "original_form": "근데 그 브이알 꽂고 리겜 하는건 넘 재미있어 보였어",
          "speaker_id": "1",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027915.1.28",
          "form": "칼로 베는거?",
          "original_form": "칼로 베는거?",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100027915.1.29",
          "form": "어 그거ㅋㅋㅋ",
          "original_form": "어 그거ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100027915.1.30",
          "form": "나도 그건 재밌어보이더라 ㅋㅋㅋ",
          "original_form": "나도 그건 재밌어보이더라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100027915.1.31",
          "form": "말나온김에ㅡ찾아보는중",
          "original_form": "말나온김에ㅡ찾아보는중",
          "speaker_id": "1",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027915.1.32",
          "form": "안비싸네",
          "original_form": "안비싸네",
          "speaker_id": "1",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027915.1.33",
          "form": "근데 눈에 쓰는게 비싸다",
          "original_form": "근데 눈에 쓰는게 비싸다",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027915.1.34",
          "form": "곧 더 좋은 게 나오겠지",
          "original_form": "곧 더 좋은 게 나오겠지",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027915.1.35",
          "form": "나도 가상세계 해보고 싶다아",
          "original_form": "나도 가상세계 해보고 싶다아",
          "speaker_id": "1",
          "time": "20210902 16:32"
        }
      ]
