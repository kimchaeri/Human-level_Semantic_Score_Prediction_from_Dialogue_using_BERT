      "utterance": [
        {
          "id": "MDRW2100006593.1.1",
          "form": "접종 시 부작용은 없으셨나요",
          "original_form": "접종 시 부작용은 없으셨나요",
          "speaker_id": "1",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100006593.1.2",
          "form": "저 1차는 근육통만 있었고",
          "original_form": "저 1차는 근육통만 있었고",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100006593.1.3",
          "form": "2차는 고열때문에 고생했네요ㅠㅠ",
          "original_form": "2차는 고열때문에 고생했네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100006593.1.4",
          "form": "화이자였나요?",
          "original_form": "화이자였나요?",
          "speaker_id": "1",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100006593.1.5",
          "form": "넵 화이자 접종했네요",
          "original_form": "넵 화이자 접종했네요",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100006593.1.6",
          "form": "고열나셔서 약도 드셨나요",
          "original_form": "고열나셔서 약도 드셨나요",
          "speaker_id": "1",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100006593.1.7",
          "form": "네 새벽내내 앓다가 타이레놀 먹었어요",
          "original_form": "네 새벽내내 앓다가 타이레놀 먹었어요",
          "speaker_id": "2",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100006593.1.8",
          "form": "부모님이 확실히 약 먹으니까 열 내렸다고 하더라구요",
          "original_form": "부모님이 확실히 약 먹으니까 열 내렸다고 하더라구요",
          "speaker_id": "2",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100006593.1.9",
          "form": "몇 번이나 드신건가요",
          "original_form": "몇 번이나 드신건가요",
          "speaker_id": "1",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100006593.1.10",
          "form": "5알은 먹었던 것 같아요",
          "original_form": "5알은 먹었던 것 같아요",
          "speaker_id": "2",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100006593.1.11",
          "form": "고열은 내렸는데 미열이랑 근육통이 있어서~",
          "original_form": "고열은 내렸는데 미열이랑 근육통이 있어서~",
          "speaker_id": "2",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100006593.1.12",
          "form": "많이 드셨네요 저도 먹었었어요",
          "original_form": "많이 드셨네요 저도 먹었었어요",
          "speaker_id": "1",
          "time": "20210908 03:02"
        },
        {
          "id": "MDRW2100006593.1.13",
          "form": "어우ㅜㅜ 좀 괜찮으셨어요?",
          "original_form": "어우ㅜㅜ 좀 괜찮으셨어요?",
          "speaker_id": "2",
          "time": "20210908 03:02"
        },
        {
          "id": "MDRW2100006593.1.14",
          "form": "저도 3.4.번 먹고 나아지더라구요 컨디션이",
          "original_form": "저도 3.4.번 먹고 나아지더라구요 컨디션이",
          "speaker_id": "1",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100006593.1.15",
          "form": "전 부모님이 아직 2차 접종 안하셔서 걱정이에요~",
          "original_form": "전 부모님이 아직 2차 접종 안하셔서 걱정이에요~",
          "speaker_id": "2",
          "time": "20210908 03:04"
        },
        {
          "id": "MDRW2100006593.1.16",
          "form": "연세가 연세인지라.. 아프실까봐요ㅠㅠ",
          "original_form": "연세가 연세인지라.. 아프실까봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 03:04"
        },
        {
          "id": "MDRW2100006593.1.17",
          "form": "그러시군요 무탈히 잘 지나가시길요",
          "original_form": "그러시군요 무탈히 잘 지나가시길요",
