      "utterance": [
        {
          "id": "MDRW2100018832.1.1",
          "form": "똑똑해서 이름 부르면 온데요 ㅋㅋㅋ",
          "original_form": "똑똑해서 이름 부르면 온데요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018832.1.2",
          "form": "그런데 주인을 물어요",
          "original_form": "그런데 주인을 물어요",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018832.1.3",
          "form": "해봤나 한번 해봐라 이름 부르면 오는지",
          "original_form": "해봤나 한번 해봐라 이름 부르면 오는지",
          "speaker_id": "1",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018832.1.4",
          "form": "와서 무는 거 아이가",
          "original_form": "와서 무는 거 아이가",
          "speaker_id": "1",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018832.1.5",
          "form": "가깝고도 멀고",
          "original_form": "가깝고도 멀고",
          "speaker_id": "1",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018832.1.6",
          "form": "짐승은 짐승이에요",
          "original_form": "짐승은 짐승이에요",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018832.1.7",
          "form": "똑똑하면서도 등*같네",
          "original_form": "똑똑하면서도 등*같네",
          "speaker_id": "1",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018832.1.8",
          "form": "맞아요 언니",
          "original_form": "맞아요 언니",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018832.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018832.1.10",
          "form": "근데 저거 별표 자체 처리된다",
          "original_form": "근데 저거 별표 자체 처리된다",
          "speaker_id": "1",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018832.1.11",
          "form": "조만간 탈출할 듯 요",
          "original_form": "조만간 탈출할 듯 요",
          "speaker_id": "2",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018832.1.12",
          "form": "내가 안 냈거든",
          "original_form": "내가 안 냈거든",
          "speaker_id": "1",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018832.1.13",
          "form": "멋지네요 언니",
          "original_form": "멋지네요 언니",
          "speaker_id": "2",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018832.1.14",
          "form": "몸 부수고 탈출하는가 아이가",
          "original_form": "몸 부수고 탈출하는가 아이가",
          "speaker_id": "1",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018832.1.15",
          "form": "다 삐 처리하는 거 아니에요",
          "original_form": "다 삐 처리하는 거 아니에요",
          "speaker_id": "2",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018832.1.16",
          "form": "예 나름 똑똑해졌음",
          "original_form": "예 나름 똑똑해졌음",
          "speaker_id": "1",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018832.1.17",
          "form": "내가 구멍이라 했는데 나쁜 얘기 래요",
          "original_form": "내가 구멍이라 했는데 나쁜 얘기 래요",
          "speaker_id": "2",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018832.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018832.1.19",
          "form": "똑똑한 척 하는데 아님",
          "original_form": "똑똑한 척 하는데 아님",
          "speaker_id": "2",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018832.1.20",
          "form": "아기 가진것도 안됨",
          "original_form": "아기 가진것도 안됨",
          "speaker_id": "1",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018832.1.21",
          "form": "임*",
          "original_form": "임*",
          "speaker_id": "1",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018832.1.22",
          "form": "그러니까요 아",
          "original_form": "그러니까요 아",
          "speaker_id": "2",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018832.1.23",
          "form": "진짜 사상 이상함",
          "original_form": "진짜 사상 이상함",
          "speaker_id": "2",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018832.1.24",
          "form": "어제 모르는 사람이랑 대화하는데",
          "original_form": "어제 모르는 사람이랑 대화하는데",
          "speaker_id": "1",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018832.1.25",
          "form": "저거를 계속 나쁜 말이라고 안 된다는 거야",
          "original_form": "저거를 계속 나쁜 말이라고 안 된다는 거야",
          "speaker_id": "1",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018832.1.26",
          "form": "고양이가 아기를 가졌어",
          "original_form": "고양이가 아기를 가졌어",
          "speaker_id": "1",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018832.1.27",
          "form": "저거를 아기를 가졌다고 얘기하니까 너무 웃긴거야",
          "original_form": "저거를 아기를 가졌다고 얘기하니까 너무 웃긴거야",
          "speaker_id": "1",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018832.1.28",
          "form": "근데 임*은 안 돼",
          "original_form": "근데 임*은 안 돼",
          "speaker_id": "1",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018832.1.29",
          "form": "답답했다 정말",
          "original_form": "답답했다 정말",
          "speaker_id": "1",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018832.1.30",
          "form": "진짜 대화 흐름이 뚝뚝 끊김",
          "original_form": "진짜 대화 흐름이 뚝뚝 끊김",
          "speaker_id": "2",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018832.1.31",
          "form": "모르는 사람은 뭐라고 생각하겠노",
          "original_form": "모르는 사람은 뭐라고 생각하겠노",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018832.1.32",
          "form": "저 사람 뭐지라고 생각했을 거예요",
          "original_form": "저 사람 뭐지라고 생각했을 거예요",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018832.1.33",
          "form": "주제가 가족인데 왜 임신이 안 되는 거죠",
          "original_form": "주제가 가족인데 왜 임신이 안 되는 거죠",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018832.1.34",
          "form": "어 또 이거는 되네요",
          "original_form": "어 또 이거는 되네요",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018832.1.35",
          "form": "반려동물을 정말 가족 같이 생각하는 사람",
          "original_form": "반려동물을 정말 가족 같이 생각하는 사람",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018832.1.36",
          "form": "반려 동물의 아기도 내 아기",
          "original_form": "반려 동물의 아기도 내 아기",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018832.1.37",
          "form": "언니 미라 언니 나 가능하지",
          "original_form": "언니 미라 언니 나 가능하지",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018832.1.38",
          "form": "어 뭐지??",
          "original_form": "어 뭐지??",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018832.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018832.1.40",
          "form": "난 정말 반려동물 별로 에요",
          "original_form": "난 정말 반려동물 별로 에요",
          "speaker_id": "2",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018832.1.41",
          "form": "왜 되는거고",
          "original_form": "왜 되는거고",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018832.1.42",
          "form": "그래 나도 사실 반려동물 안 맞다",
          "original_form": "그래 나도 사실 반려동물 안 맞다",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018832.1.43",
          "form": "흐름을 살피나 봐요",
          "original_form": "흐름을 살피나 봐요",
          "speaker_id": "2",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018832.1.44",
          "form": "우리 집에 오면 다 죽음",
          "original_form": "우리 집에 오면 다 죽음",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018832.1.45",
          "form": "지금 번데기로 돼 있는데 왠지 말라비 틀어서 죽을 것 같은",
          "original_form": "지금 번데기로 돼 있는데 왠지 말라비 틀어서 죽을 것 같은",
          "speaker_id": "1",
          "time": "20210827 12:40"
        }
      ]
    }
