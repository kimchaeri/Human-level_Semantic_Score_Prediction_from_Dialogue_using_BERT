      "utterance": [
        {
          "id": "MDRW2100044670.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋ 그치만 백년 전에 뭐했어요 하면",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그치만 백년 전에 뭐했어요 하면",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044670.1.2",
          "form": "일제강점기라구",
          "original_form": "일제강점기라구",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044670.1.3",
          "form": "근현대사가 급 튀어나오네 기계의 역사..",
          "original_form": "근현대사가 급 튀어나오네 기계의 역사..",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044670.1.4",
          "form": "ㅋㅋㅋㅋ 글킨 하지만...",
          "original_form": "ㅋㅋㅋㅋ 글킨 하지만...",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044670.1.5",
          "form": "요즘 코로나라고",
          "original_form": "요즘 코로나라고",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044670.1.6",
          "form": "갑자기 사람이 키오스크로 대체되는거 보면",
          "original_form": "갑자기 사람이 키오스크로 대체되는거 보면",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044670.1.7",
          "form": "기분이 묘함",
          "original_form": "기분이 묘함",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044670.1.8",
          "form": "아 ㅋㅋㅋ 그러게 생각해보면",
          "original_form": "아 ㅋㅋㅋ 그러게 생각해보면",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044670.1.9",
          "form": "슬슬 번지는 추세였는데",
          "original_form": "슬슬 번지는 추세였는데",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044670.1.10",
          "form": "코로나로 더 급증한 모양새",
          "original_form": "코로나로 더 급증한 모양새",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044670.1.11",
          "form": "ㅇㅇ 그전까지는 키오스크보다 사람이 쌌는데",
          "original_form": "ㅇㅇ 그전까지는 키오스크보다 사람이 쌌는데",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044670.1.12",
          "form": "위생이니 뭐니 말 많아지면서",
          "original_form": "위생이니 뭐니 말 많아지면서",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044670.1.13",
          "form": "확 바꿔버린거같더라",
          "original_form": "확 바꿔버린거같더라",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044670.1.14",
          "form": "그렇군..",
          "original_form": "그렇군..",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044670.1.15",
          "form": "근데 또 화면 터치하다보면 더러운 건 마차가지 같긴 한데",
          "original_form": "근데 또 화면 터치하다보면 더러운 건 마차가지 같긴 한데",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044670.1.16",
          "form": "나이드신 분들은 사용 못한다 어쩐다 말 많아서",
          "original_form": "나이드신 분들은 사용 못한다 어쩐다 말 많아서",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044670.1.17",
          "form": "우체국도 키오스크해죠",
          "original_form": "우체국도 키오스크해죠",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044670.1.18",
          "form": "우편 등기 소포 나눠서 아..\n이미 무인기가 있구나",
          "original_form": "우편 등기 소포 나눠서 아..\n이미 무인기가 있구나",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044670.1.19",
          "form": "(이마팍팍",
          "original_form": "(이마팍팍",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044670.1.20",
          "form": "ㅇㅇ 우체국도 요즘은 있더라 ㅋㅋㅋ",
          "original_form": "ㅇㅇ 우체국도 요즘은 있더라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044670.1.21",
          "form": "ㅇㅇ... 근데 무인기마다 달라..",
          "original_form": "ㅇㅇ... 근데 무인기마다 달라..",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044670.1.22",
          "form": "미리 주문하고 오는 사람은 주소 입력 안해도 되는데",
          "original_form": "미리 주문하고 오는 사람은 주소 입력 안해도 되는데",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044670.1.23",
          "form": "현장에서 보내는 무인기는 주소 입력 인간이 해",
          "original_form": "현장에서 보내는 무인기는 주소 입력 인간이 해",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044670.1.24",
          "form": "아 무인기도 사전 접수가 돼?",
          "original_form": "아 무인기도 사전 접수가 돼?",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044670.1.25",
          "form": "ㅋㅋ큐ㅠ",
          "original_form": "ㅋㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044670.1.26",
          "form": "사전접수하는 그게 있더라고",
          "original_form": "사전접수하는 그게 있더라고",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044670.1.27",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044670.1.28",
          "form": "그 창구가 따로 있더라 고객참여형 어쩌구해서",
          "original_form": "그 창구가 따로 있더라 고객참여형 어쩌구해서",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044670.1.29",
          "form": "물론 인간한테 가서 사전접수했다고 하고 할 수도 있지만",
          "original_form": "물론 인간한테 가서 사전접수했다고 하고 할 수도 있지만",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044670.1.30",
          "form": "ㅇㅇ 나 예전엔 사전접수도 창구에서 했거든",
          "original_form": "ㅇㅇ 나 예전엔 사전접수도 창구에서 했거든",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044670.1.31",
          "form": "ㅇㅇ 있는 곳이 있고 없는 곳도 있고 하나봐",
          "original_form": "ㅇㅇ 있는 곳이 있고 없는 곳도 있고 하나봐",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044670.1.32",
          "form": "여기가 그나마 중앙으로 모이는 곳애 가까워서 좀 크거든..",
          "original_form": "여기가 그나마 중앙으로 모이는 곳애 가까워서 좀 크거든..",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044670.1.33",
          "form": "하기야 그런 무인기는 큰데 위주로 설치하더라",
          "original_form": "하기야 그런 무인기는 큰데 위주로 설치하더라",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044670.1.34",
          "form": "ㅇㅇ 그런가봐 동네엔 우리도 없으니까",
          "original_form": "ㅇㅇ 그런가봐 동네엔 우리도 없으니까",
          "speaker_id": "2",
          "time": "20210907 23:34"
        }
      ]
