      "utterance": [
        {
          "id": "MDRW2100033018.1.1",
          "form": "시간많은데 일로 삶고 하셨네요",
          "original_form": "시간많은데 일로 삶고 하셨네요",
          "speaker_id": "2",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100033018.1.2",
          "form": "저는 디자인 쪽이라 프리랜서 거예요",
          "original_form": "저는 디자인 쪽이라 프리랜서 거예요",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100033018.1.3",
          "form": "재택한지는 3년 됐어요",
          "original_form": "재택한지는 3년 됐어요",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100033018.1.4",
          "form": "드자이너~",
          "original_form": "드자이너~",
          "speaker_id": "2",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100033018.1.5",
          "form": "전 재택싫어요",
          "original_form": "전 재택싫어요",
          "speaker_id": "2",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100033018.1.6",
          "form": "잡지사에서 일했는데 마감 때문에 너무 힘들었어요",
          "original_form": "잡지사에서 일했는데 마감 때문에 너무 힘들었어요",
          "speaker_id": "1",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100033018.1.7",
          "form": "그러셨겠다 피마르죠",
          "original_form": "그러셨겠다 피마르죠",
          "speaker_id": "2",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100033018.1.8",
          "form": "정답이 없는 일이다 보니까요",
          "original_form": "정답이 없는 일이다 보니까요",
          "speaker_id": "1",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100033018.1.9",
          "form": "그래서 그냥 몇 개 회사랑 계약해서 팜플렛이나 이모티콘 같은 거 만들어요",
          "original_form": "그래서 그냥 몇 개 회사랑 계약해서 팜플렛이나 이모티콘 같은 거 만들어요",
          "speaker_id": "1",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100033018.1.10",
          "form": "디자인은 유행도있어서",
          "original_form": "디자인은 유행도있어서",
          "speaker_id": "2",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100033018.1.11",
          "form": "결혼하면 감이 많이 떨어지더라구요",
          "original_form": "결혼하면 감이 많이 떨어지더라구요",
          "speaker_id": "2",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100033018.1.12",
          "form": "요새는 배치 표시 같은 거 일러스트 많이 들어가더라고요",
          "original_form": "요새는 배치 표시 같은 거 일러스트 많이 들어가더라고요",
          "speaker_id": "1",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100033018.1.13",
          "form": "책표지",
          "original_form": "책표지",
          "speaker_id": "1",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100033018.1.14",
          "form": "생각보다는 이래저래 쓸모가 많아요",
          "original_form": "생각보다는 이래저래 쓸모가 많아요",
          "speaker_id": "1",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100033018.1.15",
          "form": "계속 계속 발전해야 되는 직업이라 머리가 아프긴 하죠",
          "original_form": "계속 계속 발전해야 되는 직업이라 머리가 아프긴 하죠",
          "speaker_id": "1",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100033018.1.16",
          "form": "그럼 디자인일 계속 하실거예여",
          "original_form": "그럼 디자인일 계속 하실거예여",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033018.1.17",
          "form": "이제 생활에 전부이니까 익숙한 일이니까요",
          "original_form": "이제 생활에 전부이니까 익숙한 일이니까요",
          "speaker_id": "1",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033018.1.18",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033018.1.19",
          "form": "취미로 주식도 하고 있어요",
          "original_form": "취미로 주식도 하고 있어요",
          "speaker_id": "1",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033018.1.20",
          "form": "사람 스트레스는 좀 없겠어요",
          "original_form": "사람 스트레스는 좀 없겠어요",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033018.1.21",
          "form": "어떤 일이 하고 싶으세요",
          "original_form": "어떤 일이 하고 싶으세요",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033018.1.22",
          "form": "오 주식",
          "original_form": "오 주식",
          "speaker_id": "2",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033018.1.23",
          "form": "글쎄여",
          "original_form": "글쎄여",
          "speaker_id": "2",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033018.1.24",
          "form": "미팅 할 때는 스트레스 받아요ㅜ",
          "original_form": "미팅 할 때는 스트레스 받아요ㅜ",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033018.1.25",
          "form": "ㅋㅋ 네 디자인도 커뮤니케이션이 좀 있죠",
          "original_form": "ㅋㅋ 네 디자인도 커뮤니케이션이 좀 있죠",
          "speaker_id": "2",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033018.1.26",
          "form": "가장 하고 싶은 건 놀고 먹는 거에요ㅎ",
          "original_form": "가장 하고 싶은 건 놀고 먹는 거에요ㅎ",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033018.1.27",
          "form": "아무것도 안 하는 거",
          "original_form": "아무것도 안 하는 거",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033018.1.28",
          "form": "작년 8월까진 인도에 있었는데",
          "original_form": "작년 8월까진 인도에 있었는데",
          "speaker_id": "2",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033018.1.29",
          "form": "코로나 끝나면 다시 해외로 가고 싶어요",
          "original_form": "코로나 끝나면 다시 해외로 가고 싶어요",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100033018.1.30",
          "form": "보람 있고 재밌는 삶을 사셨군요",
          "original_form": "보람 있고 재밌는 삶을 사셨군요",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033018.1.31",
          "form": "활동적인 분인 거 같아요",
          "original_form": "활동적인 분인 거 같아요",
          "speaker_id": "1",
          "time": "20210906 14:07"
        }
      ]
    }
