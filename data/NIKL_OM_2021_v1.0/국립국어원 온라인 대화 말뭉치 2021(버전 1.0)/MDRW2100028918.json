        {
          "id": "MDRW2100028918.1.13",
          "form": "아 저 affiliation이요ㅋㅋㅋ",
          "original_form": "아 저 &affiliation&이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:36"
