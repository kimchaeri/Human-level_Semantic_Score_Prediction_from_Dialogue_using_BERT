      "utterance": [
        {
          "id": "MDRW2100042004.1.1",
          "form": "나는 떡볶이랑 순대시켜서 먹고 name3이 이제 밥 먹기 시작",
          "original_form": "나는 떡볶이랑 순대시켜서 먹고 &name3&이 이제 밥 먹기 시작",
          "speaker_id": "2",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100042004.1.2",
          "form": "안서방 일찍 왔나?",
          "original_form": "안서방 일찍 왔나?",
          "speaker_id": "2",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100042004.1.3",
          "form": "아니 우리끼리",
          "original_form": "아니 우리끼리",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100042004.1.4",
          "form": "10분전에 수학학원갔거든 배고프다고",
          "original_form": "10분전에 수학학원갔거든 배고프다고",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100042004.1.5",
          "form": "저녁 뭐 먹였는데?",
          "original_form": "저녁 뭐 먹였는데?",
          "speaker_id": "2",
          "time": "20210906 18:36"
        },
        {
          "id": "MDRW2100042004.1.6",
          "form": "가쓰오우동에 돈가스먹고싶데서",
          "original_form": "가쓰오우동에 돈가스먹고싶데서",
          "speaker_id": "1",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100042004.1.7",
          "form": "아까사와서 해줬어",
          "original_form": "아까사와서 해줬어",
          "speaker_id": "1",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100042004.1.8",
          "form": "시켜주지않고?",
          "original_form": "시켜주지않고?",
          "speaker_id": "2",
          "time": "20210906 18:47"
        },
        {
          "id": "MDRW2100042004.1.9",
          "form": "난 시켜줬다~ㅋㅋㅋ",
          "original_form": "난 시켜줬다~ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:48"
        },
        {
          "id": "MDRW2100042004.1.10",
          "form": "ㅇㅇ난 떡국끓여먹고",
          "original_form": "ㅇㅇ난 떡국끓여먹고",
          "speaker_id": "1",
          "time": "20210906 19:02"
        },
        {
          "id": "MDRW2100042004.1.11",
          "form": "따로 먹었네~",
          "original_form": "따로 먹었네~",
          "speaker_id": "2",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042004.1.12",
          "form": "어 일찍 저녁먹고 정리하니 좋네",
          "original_form": "어 일찍 저녁먹고 정리하니 좋네",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042004.1.13",
          "form": "그렇네ㅜname4이 한글 가르치다 뒷골",
          "original_form": "그렇네ㅜ&name4&이 한글 가르치다 뒷골",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042004.1.14",
          "form": "잘 못하나?",
          "original_form": "잘 못하나?",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042004.1.15",
          "form": "아예 집중을 안한다 싫어하는거라고ㅜ",
          "original_form": "아예 집중을 안한다 싫어하는거라고ㅜ",
          "speaker_id": "2",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042004.1.16",
          "form": "맞나 name4이 의외네",
          "original_form": "맞나 &name4&이 의외네",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042004.1.17",
          "form": "백신맞고.. 몸괜찮나",
          "original_form": "백신맞고.. 몸괜찮나",
          "speaker_id": "1",
          "time": "20210906 23:17"
        },
        {
          "id": "MDRW2100042004.1.18",
          "form": "팔만 아프네ㅜ우리집 얘들 둘다 왜 이렇겠노싶다ㅜ",
          "original_form": "팔만 아프네ㅜ우리집 얘들 둘다 왜 이렇겠노싶다ㅜ",
          "speaker_id": "2",
          "time": "20210907 07:07"
        },
        {
          "id": "MDRW2100042004.1.19",
          "form": "둘다 한글 가르치기가 어렵네ㅜ바로 머리에 안 떠오르면 포기해버린다ㅜname4",
          "original_form": "둘다 한글 가르치기가 어렵네ㅜ바로 머리에 안 떠오르면 포기해버린다ㅜ&name4&",
          "speaker_id": "2",
          "time": "20210907 07:08"
        },
        {
          "id": "MDRW2100042004.1.20",
          "form": "맞나 ㅜ 몸안피곤하나",
          "original_form": "맞나 ㅜ 몸안피곤하나",
          "speaker_id": "1",
          "time": "20210907 09:57"
        },
