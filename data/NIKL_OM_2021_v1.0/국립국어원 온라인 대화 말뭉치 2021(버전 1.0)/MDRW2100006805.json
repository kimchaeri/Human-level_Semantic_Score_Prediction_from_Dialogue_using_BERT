      "utterance": [
        {
          "id": "MDRW2100006805.1.1",
          "form": "다른 식재료는 어디서 사시나요?",
          "original_form": "다른 식재료는 어디서 사시나요?",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100006805.1.2",
          "form": "아내와 함께 시장이나 마트를 갑니다",
          "original_form": "아내와 함께 시장이나 마트를 갑니다",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100006805.1.3",
          "form": "저희는 아직 온라인 쇼핑을 잘 못해요",
          "original_form": "저희는 아직 온라인 쇼핑을 잘 못해요",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100006805.1.4",
          "form": "전통시장이요?",
          "original_form": "전통시장이요?",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100006805.1.5",
          "form": "네 동네에 시장이 있습니다",
          "original_form": "네 동네에 시장이 있습니다",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100006805.1.6",
          "form": "아내가 시장에서 장보는 것을 좋아해요",
          "original_form": "아내가 시장에서 장보는 것을 좋아해요",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100006805.1.7",
          "form": "전 짐꾼으로 같이 다니는 거지요 ㅎㅎ",
          "original_form": "전 짐꾼으로 같이 다니는 거지요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100006805.1.8",
          "form": "짐꾼해주시니 사모님이 좋으시겠어요",
          "original_form": "짐꾼해주시니 사모님이 좋으시겠어요",
          "speaker_id": "1",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100006805.1.9",
          "form": "아내가 손이 커서 한번에 많이씩 사요",
          "original_form": "아내가 손이 커서 한번에 많이씩 사요",
          "speaker_id": "2",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100006805.1.10",
          "form": "혼자 못들고 오지요",
          "original_form": "혼자 못들고 오지요",
          "speaker_id": "2",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100006805.1.11",
          "form": "제가 같이 가줘야 한답니다",
          "original_form": "제가 같이 가줘야 한답니다",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100006805.1.12",
          "form": "보통 무엇을 사시나요ㅡ",
          "original_form": "보통 무엇을 사시나요ㅡ",
          "speaker_id": "1",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100006805.1.13",
          "form": "일반 식재료 산답니다 집에서 다듬지요",
          "original_form": "일반 식재료 산답니다 집에서 다듬지요",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100006805.1.14",
          "form": "요즘 구매하시고 싶은 물건 있으신가요",
          "original_form": "요즘 구매하시고 싶은 물건 있으신가요",
          "speaker_id": "1",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100006805.1.15",
          "form": "저는 쇼핑을 안좋아합니다. 물건도 사는 걸 안좋아해요",
          "original_form": "저는 쇼핑을 안좋아합니다. 물건도 사는 걸 안좋아해요",
          "speaker_id": "2",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100006805.1.16",
          "form": "알뜰하신가요?",
          "original_form": "알뜰하신가요?",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100006805.1.17",
          "form": "있는 걸 활용하자는 주의입니다",
          "original_form": "있는 걸 활용하자는 주의입니다",
          "speaker_id": "2",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100006805.1.18",
          "form": "그러시군요 중고거래해보셨나요",
          "original_form": "그러시군요 중고거래해보셨나요",
          "speaker_id": "1",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100006805.1.19",
          "form": "제 아내가 요즘 하고 있습니다",
          "original_form": "제 아내가 요즘 하고 있습니다",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100006805.1.20",
          "form": "당근마켓이라고 하더군요",
          "original_form": "당근마켓이라고 하더군요",
          "speaker_id": "2",
          "time": "20210908 10:20"
        }
