      "utterance": [
        {
          "id": "MDRW2100025428.1.1",
          "form": "오늘은 비가 안오겠죠?",
          "original_form": "오늘은 비가 안오겠죠?",
          "speaker_id": "2",
          "time": "20210902 07:41"
        },
        {
          "id": "MDRW2100025428.1.2",
          "form": "선선하니 이런날씨 좋아요",
          "original_form": "선선하니 이런날씨 좋아요",
          "speaker_id": "2",
          "time": "20210902 07:41"
        },
        {
          "id": "MDRW2100025428.1.3",
          "form": "아침 출근길에 햇빛 비추더라고요!!!!",
          "original_form": "아침 출근길에 햇빛 비추더라고요!!!!",
          "speaker_id": "1",
          "time": "20210902 08:21"
        },
        {
          "id": "MDRW2100025428.1.4",
          "form": "기분 좋습니다",
          "original_form": "기분 좋습니다",
          "speaker_id": "1",
          "time": "20210902 08:21"
        },
        {
          "id": "MDRW2100025428.1.5",
          "form": "그쵸?! 진짜 회사에 앉아있기 싫은 날씨에요",
          "original_form": "그쵸?! 진짜 회사에 앉아있기 싫은 날씨에요",
          "speaker_id": "2",
          "time": "20210902 08:26"
        },
        {
          "id": "MDRW2100025428.1.6",
          "form": "ㅋㅋㅋㅋ사노비 일하러 가는 길.. ㅋㅋ 화창하네요ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ사노비 일하러 가는 길.. ㅋㅋ 화창하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 08:27"
        },
        {
          "id": "MDRW2100025428.1.7",
          "form": "이제 사무실에 앉으면 비가오든 눈이 오든 모르죠",
          "original_form": "이제 사무실에 앉으면 비가오든 눈이 오든 모르죠",
          "speaker_id": "2",
          "time": "20210902 09:40"
        },
        {
          "id": "MDRW2100025428.1.8",
          "form": "여름에는 회사가 짱이긴해여",
          "original_form": "여름에는 회사가 짱이긴해여",
          "speaker_id": "2",
          "time": "20210902 09:40"
        },
        {
          "id": "MDRW2100025428.1.9",
          "form": "천둥번개가 쳐도ㅋㅋ 폭염이어도요!!",
          "original_form": "천둥번개가 쳐도ㅋㅋ 폭염이어도요!!",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100025428.1.10",
          "form": "지금도 에어컨. 켜져있어요ㅋㅋ",
          "original_form": "지금도 에어컨. 켜져있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100025428.1.11",
          "form": "맞아여ㅋㅋ사업용전력비라 쌉니다",
          "original_form": "맞아여ㅋㅋ사업용전력비라 쌉니다",
          "speaker_id": "2",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100025428.1.12",
          "form": "ㅋㅋ가디건 필수예요!!! ㅋㅋ",
          "original_form": "ㅋㅋ가디건 필수예요!!! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:52"
        },
        {
          "id": "MDRW2100025428.1.13",
          "form": "맞어요ㅎㅎ지금 날씨가 엄청 선선해요",
          "original_form": "맞어요ㅎㅎ지금 날씨가 엄청 선선해요",
          "speaker_id": "2",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100025428.1.14",
          "form": "이불필수네요",
          "original_form": "이불필수네요",
          "speaker_id": "2",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100025428.1.15",
          "form": "보일러넣고 이불속 입니다ㅋㅋ",
          "original_form": "보일러넣고 이불속 입니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100025428.1.16",
          "form": "와 벌써 보일러 트셨어요?",
          "original_form": "와 벌써 보일러 트셨어요?",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100025428.1.17",
          "form": "저는 공기는추운데 이불속 따뜻한거 좋아요ㅋㅋ",
          "original_form": "저는 공기는추운데 이불속 따뜻한거 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100025428.1.18",
          "form": "네ㅋㅋ 저희 집 주택이라ㅋㅋ",
          "original_form": "네ㅋㅋ 저희 집 주택이라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100025428.1.19",
          "form": "아하 저는 산근처? 에서 살아서",
          "original_form": "아하 저는 산근처? 에서 살아서",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100025428.1.20",
          "form": "지금 귀뚜라미소리들려요",
          "original_form": "지금 귀뚜라미소리들려요",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100025428.1.21",
          "form": "ㆍ완전 가을이에여",
