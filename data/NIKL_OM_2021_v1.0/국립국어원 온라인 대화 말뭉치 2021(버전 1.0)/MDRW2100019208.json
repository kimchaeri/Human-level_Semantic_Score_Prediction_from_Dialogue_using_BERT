      "utterance": [
        {
          "id": "MDRW2100019208.1.1",
          "form": "피자좋아하세요?",
          "original_form": "피자좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100019208.1.2",
          "form": "네 피자 좋아합니다",
          "original_form": "네 피자 좋아합니다",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019208.1.3",
          "form": "어느브랜드 좋아하세요?",
          "original_form": "어느브랜드 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019208.1.4",
          "form": "저는 브랜드피자 말고",
          "original_form": "저는 브랜드피자 말고",
          "speaker_id": "1",
          "time": "20210827 16:23"
        },
        {
          "id": "MDRW2100019208.1.5",
          "form": "동네 자주 시키는 피자집이 있어서",
          "original_form": "동네 자주 시키는 피자집이 있어서",
          "speaker_id": "1",
          "time": "20210827 16:23"
        },
        {
          "id": "MDRW2100019208.1.6",
          "form": "거기 피자만 먹어요",
          "original_form": "거기 피자만 먹어요",
          "speaker_id": "1",
          "time": "20210827 16:23"
        },
        {
          "id": "MDRW2100019208.1.7",
          "form": "오 궁금한데요?? 어느지역에사시나요?",
          "original_form": "오 궁금한데요?? 어느지역에사시나요?",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019208.1.8",
          "form": "저는 대구에 살아요",
          "original_form": "저는 대구에 살아요",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019208.1.9",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "2",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019208.1.10",
          "form": "배라3픽 여쭤봐도되요?",
          "original_form": "배라3픽 여쭤봐도되요?",
          "speaker_id": "2",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019208.1.11",
          "form": "뉴욕치즈케잌 엄마는외계인",
          "original_form": "뉴욕치즈케잌 엄마는외계인",
          "speaker_id": "1",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019208.1.12",
          "form": "그리고 잠깐 나왔던 메뉴인데",
          "original_form": "그리고 잠깐 나왔던 메뉴인데",
          "speaker_id": "1",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019208.1.13",
          "form": "쫀떡궁합 아직도 그립네요",
          "original_form": "쫀떡궁합 아직도 그립네요",
          "speaker_id": "1",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019208.1.14",
          "form": "아하! 그거 인기많더라구요",
          "original_form": "아하! 그거 인기많더라구요",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019208.1.15",
          "form": "다시나왔으면 좋겠어요",
          "original_form": "다시나왔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019208.1.16",
          "form": "웽디님은 좋아하시는 음식이 뭔가요",
          "original_form": "웽디님은 좋아하시는 음식이 뭔가요",
          "speaker_id": "1",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019208.1.17",
          "form": "맞아요 그런거너무많아요 ㅠ",
          "original_form": "맞아요 그런거너무많아요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019208.1.18",
          "form": "음식이요? 음 너무많는데 양식 좋아해요!",
          "original_form": "음식이요? 음 너무많는데 양식 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019208.1.19",
          "form": "양식중에서도 어떤 메뉴가 좋으신가요",
          "original_form": "양식중에서도 어떤 메뉴가 좋으신가요",
          "speaker_id": "1",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019208.1.20",
          "form": "저는 피자 파스타 이런 느끼한것들 좋아해요",
          "original_form": "저는 피자 파스타 이런 느끼한것들 좋아해요",
          "speaker_id": "2",
          "time": "20210827 16:35"
        }
      ]
    }
