      "utterance": [
        {
          "id": "MDRW2100002268.1.1",
          "form": "알바나 일하시나요~?",
          "original_form": "알바나 일하시나요~?",
          "speaker_id": "1",
          "time": "20210820 02:24"
        },
        {
          "id": "MDRW2100002268.1.2",
          "form": "둘 다 해요",
          "original_form": "둘 다 해요",
          "speaker_id": "2",
          "time": "20210820 02:25"
        },
        {
          "id": "MDRW2100002268.1.3",
          "form": "일은 재택 근무중이고 아르바이트는 데이터 관련으로 소소하게 해요",
          "original_form": "일은 재택 근무중이고 아르바이트는 데이터 관련으로 소소하게 해요",
          "speaker_id": "2",
          "time": "20210820 02:25"
        },
        {
          "id": "MDRW2100002268.1.4",
          "form": "오~ 저도 데이터 관련일했었는데",
          "original_form": "오~ 저도 데이터 관련일했었는데",
          "speaker_id": "1",
          "time": "20210820 09:37"
        },
        {
          "id": "MDRW2100002268.1.5",
          "form": "이미지 데이터 라벨링했었어요~",
          "original_form": "이미지 데이터 라벨링했었어요~",
          "speaker_id": "1",
          "time": "20210820 09:37"
        },
        {
          "id": "MDRW2100002268.1.6",
          "form": "이미지 라벨링요?",
          "original_form": "이미지 라벨링요?",
          "speaker_id": "2",
          "time": "20210820 09:47"
        },
        {
          "id": "MDRW2100002268.1.7",
          "form": "주로 어떻게 해요? 전 텍스트만 했었거든요",
          "original_form": "주로 어떻게 해요? 전 텍스트만 했었거든요",
          "speaker_id": "2",
          "time": "20210820 09:47"
        },
        {
          "id": "MDRW2100002268.1.8",
          "form": "은근 노가다 일이예요",
          "original_form": "은근 노가다 일이예요",
          "speaker_id": "2",
          "time": "20210820 09:48"
        },
        {
          "id": "MDRW2100002268.1.9",
          "form": "저는 차 박스쳐서한것도있고",
          "original_form": "저는 차 박스쳐서한것도있고",
          "speaker_id": "1",
          "time": "20210820 09:48"
        },
        {
          "id": "MDRW2100002268.1.10",
          "form": "텍스트 라벨링도 해봤어요~#",
          "original_form": "텍스트 라벨링도 해봤어요~#",
          "speaker_id": "1",
          "time": "20210820 09:48"
        },
        {
          "id": "MDRW2100002268.1.11",
          "form": "이미지 관련 일은 잘 없더라구요",
          "original_form": "이미지 관련 일은 잘 없더라구요",
          "speaker_id": "2",
          "time": "20210820 09:50"
        },
        {
          "id": "MDRW2100002268.1.12",
          "form": "전 올해 초까지 했어요",
          "original_form": "전 올해 초까지 했어요",
          "speaker_id": "2",
          "time": "20210820 09:50"
        },
        {
          "id": "MDRW2100002268.1.13",
          "form": "아하~~ 요즘 라벨링 일도 많이없긴하더라구여ㅠㅠ",
          "original_form": "아하~~ 요즘 라벨링 일도 많이없긴하더라구여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 11:01"
        },
        {
          "id": "MDRW2100002268.1.14",
          "form": "그래도 있을때 신청하는편이에용!",
          "original_form": "그래도 있을때 신청하는편이에용!",
          "speaker_id": "1",
          "time": "20210820 11:01"
        },
        {
          "id": "MDRW2100002268.1.15",
          "form": "요즘 소소하게 아르바이트도 많이 줄었어요",
          "original_form": "요즘 소소하게 아르바이트도 많이 줄었어요",
          "speaker_id": "2",
          "time": "20210820 11:08"
        },
        {
          "id": "MDRW2100002268.1.16",
          "form": "그렇다고 외부 밖에 나가서 하는것도 힘들구요",
          "original_form": "그렇다고 외부 밖에 나가서 하는것도 힘들구요",
          "speaker_id": "2",
          "time": "20210820 11:08"
        },
        {
          "id": "MDRW2100002268.1.17",
          "form": "맞아요ㅠㅠ 코로나때문에 전체적으로 일거리가 많이 줄거라구요ㅠ",
          "original_form": "맞아요ㅠㅠ 코로나때문에 전체적으로 일거리가 많이 줄거라구요ㅠ",
          "speaker_id": "1",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002268.1.18",
          "form": "데이터알바도 초반보다는 많이 줄은것같아요",
          "original_form": "데이터알바도 초반보다는 많이 줄은것같아요",
          "speaker_id": "1",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002268.1.19",
          "form": "인원이나 기간이 대폭 축소된 것 같아요",
          "original_form": "인원이나 기간이 대폭 축소된 것 같아요",
          "speaker_id": "2",
          "time": "20210820 16:42"
        },
        {
          "id": "MDRW2100002268.1.20",
          "form": "맞아요 찾기 힘들어요",
          "original_form": "맞아요 찾기 힘들어요",
          "speaker_id": "2",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100002268.1.21",
          "form": "그래서 조금 조금 하는데 못 버는 것보단 나아요",
          "original_form": "그래서 조금 조금 하는데 못 버는 것보단 나아요",
          "speaker_id": "2",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100002268.1.22",
          "form": "소소하게라도 일단 할수있어서 그나마 다행인것같네요ㅠ~",
          "original_form": "소소하게라도 일단 할수있어서 그나마 다행인것같네요ㅠ~",
          "speaker_id": "1",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002268.1.23",
          "form": "재택근무는 스트레스는 많이 없나요~?",
          "original_form": "재택근무는 스트레스는 많이 없나요~?",
          "speaker_id": "1",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002268.1.24",
          "form": "앞으로 소소한 아르바이트 T.O가 나왔으면 좋겠어요",
          "original_form": "앞으로 소소한 아르바이트 T.O가 나왔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210820 17:36"
        },
        {
          "id": "MDRW2100002268.1.25",
          "form": "재택 근무는 해당 일만 잘 처리하면 장소나 눈치 볼 일은 없어요",
          "original_form": "재택 근무는 해당 일만 잘 처리하면 장소나 눈치 볼 일은 없어요",
          "speaker_id": "2",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100002268.1.26",
          "form": "오옹~~ 좋은것같아요!!!",
          "original_form": "오옹~~ 좋은것같아요!!!",
          "speaker_id": "1",
          "time": "20210820 18:29"
        }
      ]
    }
