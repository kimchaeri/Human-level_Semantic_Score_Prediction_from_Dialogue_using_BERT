      "utterance": [
        {
          "id": "MDRW2100043693.1.1",
          "form": "아 알겠다",
          "original_form": "아 알겠다",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043693.1.2",
          "form": "야채 곁들여서 많이 먹어",
          "original_form": "야채 곁들여서 많이 먹어",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043693.1.3",
          "form": "정답 보쌈!",
          "original_form": "정답 보쌈!",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043693.1.4",
          "form": "보쌈 맞아!!!!",
          "original_form": "보쌈 맞아!!!!",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043693.1.5",
          "form": "아싸 아싸 아싸",
          "original_form": "아싸 아싸 아싸",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043693.1.6",
          "form": "오늘은 김치도 완성됐거든",
          "original_form": "오늘은 김치도 완성됐거든",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043693.1.7",
          "form": "헉 겉절이파티",
          "original_form": "헉 겉절이파티",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043693.1.8",
          "form": "열겠네",
          "original_form": "열겠네",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043693.1.9",
          "form": "응 푹삭으면 김치찌개도 해먹게",
          "original_form": "응 푹삭으면 김치찌개도 해먹게",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043693.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ맛있겠다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맛있겠다",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043693.1.11",
          "form": "넌 김치없이 여행가능?",
          "original_form": "넌 김치없이 여행가능?",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043693.1.12",
          "form": "아주 가능",
          "original_form": "아주 가능",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043693.1.13",
          "form": "넌 김치 필수로 먹어야해?",
          "original_form": "넌 김치 필수로 먹어야해?",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043693.1.14",
          "form": "헐 난 불가능",
          "original_form": "헐 난 불가능",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043693.1.15",
          "form": "헐 진짜? 김치 안 먹으면 어떤데?",
          "original_form": "헐 진짜? 김치 안 먹으면 어떤데?",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043693.1.16",
          "form": "응 필수요소야 내 공기",
          "original_form": "응 필수요소야 내 공기",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043693.1.17",
          "form": "느끼해서 못살짇ᆢ",
          "original_form": "느끼해서 못살짇ᆢ",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043693.1.18",
          "form": "단기여행은 버텨도......",
          "original_form": "단기여행은 버텨도......",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043693.1.19",
          "form": "헐.... 대박",
          "original_form": "헐.... 대박",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043693.1.20",
          "form": "난 김치를 아주 가끔 땡길 때 먹어",
          "original_form": "난 김치를 아주 가끔 땡길 때 먹어",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043693.1.21",
          "form": "캐나다가 음식반입이 힘들대",
          "original_form": "캐나다가 음식반입이 힘들대",
          "speaker_id": "1",
          "time": "20210907 20:03"
        }
      ]
