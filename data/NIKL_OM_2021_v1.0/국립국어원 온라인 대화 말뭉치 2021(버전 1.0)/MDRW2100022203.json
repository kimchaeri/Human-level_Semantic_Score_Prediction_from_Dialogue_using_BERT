        },
        {
          "id": "MDRW2100022203.1.6",
          "form": "카카오게임 ㅎㅎㅎ",
          "original_form": "카카오게임 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022203.1.7",
          "form": "전 온라인 게임 좋아해요~",
          "original_form": "전 온라인 게임 좋아해요~",
          "speaker_id": "1",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022203.1.8",
          "form": "어떤 게임 하세요?",
          "original_form": "어떤 게임 하세요?",
          "speaker_id": "1",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022203.1.9",
          "form": "저는 그냥 간단한거요. ㅋㅋ",
          "original_form": "저는 그냥 간단한거요. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100022203.1.10",
          "form": "어떤게임하세요?",
          "original_form": "어떤게임하세요?",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100022203.1.11",
          "form": "저 지금은 게임 잘 안해요",
          "original_form": "저 지금은 게임 잘 안해요",
          "speaker_id": "1",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100022203.1.12",
          "form": "한 때 메이플 한창 했어요",
          "original_form": "한 때 메이플 한창 했어요",
          "speaker_id": "1",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100022203.1.13",
          "form": "저도 사실 게임은 자주안해요",
          "original_form": "저도 사실 게임은 자주안해요",
          "speaker_id": "2",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100022203.1.14",
          "form": "딸이랑 같이하려고 숨은그림찾기, 애니팡이런거 다운받았어요ㅋ",
          "original_form": "딸이랑 같이하려고 숨은그림찾기, 애니팡이런거 다운받았어요ㅋ",
          "speaker_id": "2",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100022203.1.15",
          "form": "ㅋㅋㅋ애니팡 한때 한참 유행했지않아요?",
          "original_form": "ㅋㅋㅋ애니팡 한때 한참 유행했지않아요?",
          "speaker_id": "1",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100022203.1.16",
          "form": "네네 맞아요ㅋㄱㅋㅋ",
          "original_form": "네네 맞아요ㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:31"
        },
        {
          "id": "MDRW2100022203.1.17",
          "form": "저도 사실 게임을 잘 몰라요ㅋㅋ",
          "original_form": "저도 사실 게임을 잘 몰라요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:31"
        },
        {
          "id": "MDRW2100022203.1.18",
          "form": "ㅎㅎ그럼 요즘 젊은이들이 많이 하는 게임 뭔지 아세요?",
          "original_form": "ㅎㅎ그럼 요즘 젊은이들이 많이 하는 게임 뭔지 아세요?",
          "speaker_id": "1",
          "time": "20210829 14:46"
        },
        {
          "id": "MDRW2100022203.1.19",
          "form": "아니요 잘몰라요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니요 잘몰라요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:59"
        },
        {
          "id": "MDRW2100022203.1.20",
          "form": "뭐를 많이하나요?ㅋ",
          "original_form": "뭐를 많이하나요?ㅋ",
          "speaker_id": "2",
          "time": "20210829 15:00"
        },
        {
          "id": "MDRW2100022203.1.21",
          "form": "요즘 배틀그라운드라고 총게임 많이 하구요",
          "original_form": "요즘 배틀그라운드라고 총게임 많이 하구요",
          "speaker_id": "1",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100022203.1.22",
          "form": "리그오브레전드라고 전략게임도 많이 하네요 ㅎ",
          "original_form": "리그오브레전드라고 전략게임도 많이 하네요 ㅎ",
          "speaker_id": "1",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100022203.1.23",
          "form": "배틀그라운드는 많이 들어봤어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "배틀그라운드는 많이 들어봤어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
