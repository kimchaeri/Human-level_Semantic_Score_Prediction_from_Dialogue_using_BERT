      "utterance": [
        {
          "id": "MDRW2100036153.1.1",
          "form": "블랙 알리오 좋아해요",
          "original_form": "블랙 알리오 좋아해요",
          "speaker_id": "2",
          "time": "20210830 17:35"
        },
        {
          "id": "MDRW2100036153.1.2",
          "form": "저도 블랙알리오 제일 좋아해요!",
          "original_form": "저도 블랙알리오 제일 좋아해요!",
          "speaker_id": "1",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100036153.1.3",
          "form": "한국 사람이라 마늘 들어간게 좋은가봐욬ㅋㅋㅋㅋ",
          "original_form": "한국 사람이라 마늘 들어간게 좋은가봐욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100036153.1.4",
          "form": "마늘 조각 튀긴게 잘어울려요ㅋㅋㅋ 맛있고",
          "original_form": "마늘 조각 튀긴게 잘어울려요ㅋㅋㅋ 맛있고",
          "speaker_id": "1",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100036153.1.5",
          "form": "순살이랑 뼈 중에서는 어떤 걸 더 좋아하세요?",
          "original_form": "순살이랑 뼈 중에서는 어떤 걸 더 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100036153.1.6",
          "form": "저는 뼈가 더 좋아요! 좀 더 신선한 느낌?",
          "original_form": "저는 뼈가 더 좋아요! 좀 더 신선한 느낌?",
          "speaker_id": "2",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100036153.1.7",
          "form": "아 뭔지 알 거 같아요ㅎㅎ",
          "original_form": "아 뭔지 알 거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 17:40"
        },
        {
          "id": "MDRW2100036153.1.8",
          "form": "아 근데 지코바 먹을 때는 순살이 좋아요",
          "original_form": "아 근데 지코바 먹을 때는 순살이 좋아요",
          "speaker_id": "2",
          "time": "20210830 17:41"
        },
        {
          "id": "MDRW2100036153.1.9",
          "form": "조리법이 달라서 그런가..",
          "original_form": "조리법이 달라서 그런가..",
          "speaker_id": "2",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100036153.1.10",
          "form": "지코바는 어디 치킨인가요?",
          "original_form": "지코바는 어디 치킨인가요?",
          "speaker_id": "1",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100036153.1.11",
          "form": "치킨집 이름이 지코바에요 ㅋㅋㅋ 거기는 구워주는? 치킨 팔아요",
          "original_form": "치킨집 이름이 지코바에요 ㅋㅋㅋ 거기는 구워주는? 치킨 팔아요",
          "speaker_id": "2",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100036153.1.12",
          "form": "아 그렇군요ㅋㅋㅋㅋㅋ",
          "original_form": "아 그렇군요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100036153.1.13",
          "form": "구운 치킨이라니 특이하네요",
          "original_form": "구운 치킨이라니 특이하네요",
          "speaker_id": "1",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100036153.1.14",
          "form": "오븐에 굽는 느낌인가요?",
          "original_form": "오븐에 굽는 느낌인가요?",
          "speaker_id": "1",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100036153.1.15",
          "form": "숯불에 굽는? 것 같아요",
          "original_form": "숯불에 굽는? 것 같아요",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100036153.1.16",
          "form": "소스도 맛있어서 밥에 비벼먹우면 좋아요",
          "original_form": "소스도 맛있어서 밥에 비벼먹우면 좋아요",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100036153.1.17",
          "form": "헉 너무 맛있을 거 같아요",
          "original_form": "헉 너무 맛있을 거 같아요",
          "speaker_id": "1",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100036153.1.18",
          "form": "닭꼬치도 진짜 좋아하거든요",
          "original_form": "닭꼬치도 진짜 좋아하거든요",
          "speaker_id": "1",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100036153.1.19",
          "form": "진짜요? 저는 닭강정도 좋아해요",
          "original_form": "진짜요? 저는 닭강정도 좋아해요",
          "speaker_id": "2",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100036153.1.20",
          "form": "시장에서 파는 걸로요",
          "original_form": "시장에서 파는 걸로요",
          "speaker_id": "2",
          "time": "20210830 18:01"
        }
      ]
