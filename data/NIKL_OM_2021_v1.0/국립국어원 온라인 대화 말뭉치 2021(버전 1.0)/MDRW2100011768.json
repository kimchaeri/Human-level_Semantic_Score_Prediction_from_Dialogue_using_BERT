      "utterance": [
        {
          "id": "MDRW2100011768.1.1",
          "form": "가사..",
          "original_form": "가사..",
          "speaker_id": "2",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011768.1.2",
          "form": "정말 싫습니다",
          "original_form": "정말 싫습니다",
          "speaker_id": "2",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011768.1.3",
          "form": "저동ㅅ",
          "original_form": "저동ㅅ",
          "speaker_id": "1",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011768.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011768.1.5",
          "form": "솔직히 아직도 가사일은 여자 일이라는",
          "original_form": "솔직히 아직도 가사일은 여자 일이라는",
          "speaker_id": "2",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011768.1.6",
          "form": "난진자 돈많이벌어서 비싸고 좋은 가전기계 다살거임",
          "original_form": "난진자 돈많이벌어서 비싸고 좋은 가전기계 다살거임",
          "speaker_id": "1",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011768.1.7",
          "form": "멋 같은 인식이 많아서",
          "original_form": "멋 같은 인식이 많아서",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011768.1.8",
          "form": "ex 박종선",
          "original_form": "ex 박종선",
          "speaker_id": "1",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011768.1.9",
          "form": "더 하기가 싫다",
          "original_form": "더 하기가 싫다",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011768.1.10",
          "form": "너 그것도 못해?",
          "original_form": "너 그것도 못해?",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011768.1.11",
          "form": "이러면",
          "original_form": "이러면",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011768.1.12",
          "form": "응 못해~",
          "original_form": "응 못해~",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011768.1.13",
          "form": "이래야지",
          "original_form": "이래야지",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011768.1.14",
          "form": "제일 싫은 가사 일이 뭐야?",
          "original_form": "제일 싫은 가사 일이 뭐야?",
          "speaker_id": "2",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011768.1.15",
          "form": "난",
          "original_form": "난",
          "speaker_id": "1",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011768.1.16",
          "form": "다싫은데 음",
          "original_form": "다싫은데 음",
          "speaker_id": "1",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011768.1.17",
          "form": "청소?",
          "original_form": "청소?",
          "speaker_id": "1",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011768.1.18",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "2",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011768.1.19",
          "form": "난 음쓰 버리고",
          "original_form": "난 음쓰 버리고",
          "speaker_id": "2",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011768.1.20",
          "form": "설거지가 싫더라",
          "original_form": "설거지가 싫더라",
          "speaker_id": "2",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011768.1.21",
          "form": "분리수거 정말싫어!",
          "original_form": "분리수거 정말싫어!",
          "speaker_id": "1",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011768.1.22",
          "form": "분리수거도",
          "original_form": "분리수거도",
          "speaker_id": "2",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011768.1.23",
          "form": "애매한것들이 많아서",
          "original_form": "애매한것들이 많아서",
          "speaker_id": "2",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011768.1.24",
          "form": "좀 하기가 싫더라",
          "original_form": "좀 하기가 싫더라",
          "speaker_id": "2",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011768.1.25",
          "form": "맞아 진짜귀찮음",
          "original_form": "맞아 진짜귀찮음",
          "speaker_id": "1",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011768.1.26",
          "form": "분리수거 해야할것 들고 고민함",
          "original_form": "분리수거 해야할것 들고 고민함",
          "speaker_id": "2",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011768.1.27",
          "form": "얘는 어디로 가야하나",
          "original_form": "얘는 어디로 가야하나",
          "speaker_id": "2",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011768.1.28",
          "form": "나도ㅋㅋㅋㅋㅋ",
          "original_form": "나도ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:59"
        }
      ]
    }
