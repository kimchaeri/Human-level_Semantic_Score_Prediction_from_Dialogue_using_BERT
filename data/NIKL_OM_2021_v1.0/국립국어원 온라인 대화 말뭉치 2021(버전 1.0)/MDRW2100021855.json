      "utterance": [
        {
          "id": "MDRW2100021855.1.1",
          "form": "육회도 좋아하시나요",
          "original_form": "육회도 좋아하시나요",
          "speaker_id": "2",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021855.1.2",
          "form": "없어서 못 먹죠ㅜㅜㅜ",
          "original_form": "없어서 못 먹죠ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021855.1.3",
          "form": "이것도 주변에 좋아하는 사람은 잘 없지만",
          "original_form": "이것도 주변에 좋아하는 사람은 잘 없지만",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021855.1.4",
          "form": "육회까지는 그래도 대중적인데 육사시미는 진짜 좋아하는 사람 별로 없지 않나요",
          "original_form": "육회까지는 그래도 대중적인데 육사시미는 진짜 좋아하는 사람 별로 없지 않나요",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021855.1.5",
          "form": "ㅜㅇㅜ 왜 이 귀한걸",
          "original_form": "ㅜㅇㅜ 왜 이 귀한걸",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021855.1.6",
          "form": "사시미는 심지어 부위도 좋은데",
          "original_form": "사시미는 심지어 부위도 좋은데",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021855.1.7",
          "form": "혹시 말고기 드셔 보셨나요",
          "original_form": "혹시 말고기 드셔 보셨나요",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021855.1.8",
          "form": "아 저는 제주도 몇 번 가봤는데도 말고기는 한 번도 못 먹어봤어요...",
          "original_form": "아 저는 제주도 몇 번 가봤는데도 말고기는 한 번도 못 먹어봤어요...",
          "speaker_id": "2",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100021855.1.9",
          "form": "아 전 말고기 코스를 먹었었거든요",
          "original_form": "아 전 말고기 코스를 먹었었거든요",
          "speaker_id": "1",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100021855.1.10",
          "form": "거기서 육회와 육사시미가 나왔었죠ㅎ",
          "original_form": "거기서 육회와 육사시미가 나왔었죠ㅎ",
          "speaker_id": "1",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100021855.1.11",
          "form": "말고기는 어떤 느낌인가요",
          "original_form": "말고기는 어떤 느낌인가요",
          "speaker_id": "2",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021855.1.12",
          "form": "생각보다 담백해요",
          "original_form": "생각보다 담백해요",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021855.1.13",
          "form": "모르고 먹으면 그냥 소같아요",
          "original_form": "모르고 먹으면 그냥 소같아요",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021855.1.14",
          "form": "담백하기로는 고래 말 소 이렇게",
          "original_form": "담백하기로는 고래 말 소 이렇게",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021855.1.15",
          "form": "뒤로 갈수록 진해요ㅎ 정말 고래와 소의 중간 느낌",
          "original_form": "뒤로 갈수록 진해요ㅎ 정말 고래와 소의 중간 느낌",
          "speaker_id": "1",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100021855.1.16",
          "form": "암튼 그 코스를 먹는데 같이간 친구 둘 다 날거 못 먹는다는 거예요ㅜ",
          "original_form": "암튼 그 코스를 먹는데 같이간 친구 둘 다 날거 못 먹는다는 거예요ㅜ",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100021855.1.17",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100021855.1.18",
          "form": "그래서 사시미를 구워먹더라고요ㅜㅜ",
          "original_form": "그래서 사시미를 구워먹더라고요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100021855.1.19",
          "form": "헐..아까워ㅜㅜ",
          "original_form": "헐..아까워ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100021855.1.20",
          "form": "사시미가 찐 신선한 고기인데!!",
          "original_form": "사시미가 찐 신선한 고기인데!!",
          "speaker_id": "2",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100021855.1.21",
          "form": "진짜 속으로 피눈물 났어요. 아까워서ㅜㅜㅜ",
          "original_form": "진짜 속으로 피눈물 났어요. 아까워서ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100021855.1.22",
          "form": "그럴만두요ㅜㅜ",
          "original_form": "그럴만두요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100021855.1.23",
          "form": "말고기 코스요리면 또 뭐가 나오나요",
          "original_form": "말고기 코스요리면 또 뭐가 나오나요",
          "speaker_id": "2",
          "time": "20210828 23:41"
        }
      ]
    }
