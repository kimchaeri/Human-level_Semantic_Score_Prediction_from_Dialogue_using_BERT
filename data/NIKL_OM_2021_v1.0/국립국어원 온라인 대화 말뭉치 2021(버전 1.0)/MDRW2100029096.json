        },
        {
          "id": "MDRW2100029096.1.3",
          "form": "다이어트하세요?",
          "original_form": "다이어트하세요?",
          "speaker_id": "1",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029096.1.4",
          "form": "넵 ! 지금도 현재진행중요 님은 ?",
          "original_form": "넵 ! 지금도 현재진행중요 님은 ?",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029096.1.5",
          "form": "전 입다이어터예요 ㄱㅋㅋ",
          "original_form": "전 입다이어터예요 ㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029096.1.6",
          "form": "식이조절 하시는거예요?",
          "original_form": "식이조절 하시는거예요?",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029096.1.7",
          "form": "운동은 홈트하구용 ㅎㅎ",
          "original_form": "운동은 홈트하구용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029096.1.8",
          "form": "식단은 딱히아니구 저녁소식이욤",
          "original_form": "식단은 딱히아니구 저녁소식이욤",
          "speaker_id": "2",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029096.1.9",
          "form": "홈트 좋죠",
          "original_form": "홈트 좋죠",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029096.1.10",
          "form": "어떤 채널보세요?",
          "original_form": "어떤 채널보세요?",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029096.1.11",
          "form": "땅끄랑 에이핏요",
          "original_form": "땅끄랑 에이핏요",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029096.1.12",
          "form": "보시는 채널있어요?",
          "original_form": "보시는 채널있어요?",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029096.1.13",
          "form": "땅끄 유명하죠",
          "original_form": "땅끄 유명하죠",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029096.1.14",
          "form": "저는 빅씨스요",
          "original_form": "저는 빅씨스요",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029096.1.15",
          "form": "오 첨들어용 어떤류에요?",
          "original_form": "오 첨들어용 어떤류에요?",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029096.1.16",
          "form": "땅끄랑 비슷한거같아요",
          "original_form": "땅끄랑 비슷한거같아요",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029096.1.17",
          "form": "타바타같은거요",
          "original_form": "타바타같은거요",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029096.1.18",
          "form": "함 봐야겠네용 ㅎㅎ",
          "original_form": "함 봐야겠네용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029096.1.19",
          "form": "네 반복없어서 좀 덜지루해요",
          "original_form": "네 반복없어서 좀 덜지루해요",
