        {
          "id": "MDRW2100038155.1.2",
          "form": "브람스를 좋아하세요 아니",
          "original_form": "브람스를 좋아하세요 아니",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100038155.1.3",
          "form": "너도 그 드라마 아는구나",
          "original_form": "너도 그 드라마 아는구나",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038155.1.4",
          "form": "드라마 우리 학교에 활용했어",
          "original_form": "드라마 우리 학교에 활용했어",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038155.1.5",
          "form": "그 촬영지가 너네 학교라니",
          "original_form": "그 촬영지가 너네 학교라니",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038155.1.6",
          "form": "자세히 보면 우리 학교인 거 알 텐데",
          "original_form": "자세히 보면 우리 학교인 거 알 텐데",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038155.1.7",
          "form": "대학교 한번 가봐야겠다 촬영지 탐험",
          "original_form": "대학교 한번 가봐야겠다 촬영지 탐험",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038155.1.8",
          "form": "심지어 내가 맨날 가는 건물",
          "original_form": "심지어 내가 맨날 가는 건물",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038155.1.9",
          "form": "그 드라마 클래식 내용이라 재밌어",
          "original_form": "그 드라마 클래식 내용이라 재밌어",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038155.1.10",
          "form": "맞아 남자 주인공이 피아노 천재",
          "original_form": "맞아 남자 주인공이 피아노 천재",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038155.1.11",
          "form": "여자 주인공은 그냥 공부 잘하는 사람",
          "original_form": "여자 주인공은 그냥 공부 잘하는 사람",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100038155.1.12",
          "form": "주인공 조금 이상해",
          "original_form": "주인공 조금 이상해",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100038155.1.13",
          "form": "남자 캐릭터가 워낙 괜찮더라",
          "original_form": "남자 캐릭터가 워낙 괜찮더라",
