      "utterance": [
        {
          "id": "MDRW2100030496.1.1",
          "form": "문희준이 거의 다만듦",
          "original_form": "문희준이 거의 다만듦",
          "speaker_id": "2",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030496.1.2",
          "form": "화장도진하고",
          "original_form": "화장도진하고",
          "speaker_id": "1",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030496.1.3",
          "form": "세기말감성이좀",
          "original_form": "세기말감성이좀",
          "speaker_id": "1",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030496.1.4",
          "form": "과했지",
          "original_form": "과했지",
          "speaker_id": "1",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030496.1.5",
          "form": "하지만세기초도 만만치않지",
          "original_form": "하지만세기초도 만만치않지",
          "speaker_id": "1",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030496.1.6",
          "form": "이미오고있던데",
          "original_form": "이미오고있던데",
          "speaker_id": "1",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030496.1.7",
          "form": "태연서타일이쁘더라",
          "original_form": "태연서타일이쁘더라",
          "speaker_id": "1",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030496.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ사이버라면이라고 있더라구",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ사이버라면이라고 있더라구",
          "speaker_id": "2",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030496.1.9",
          "form": "나도사고싶어",
          "original_form": "나도사고싶어",
          "speaker_id": "1",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030496.1.10",
          "form": "난 핑크알러지있서",
          "original_form": "난 핑크알러지있서",
          "speaker_id": "2",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030496.1.11",
          "form": "그런 티네이저하이틴서타일",
          "original_form": "그런 티네이저하이틴서타일",
          "speaker_id": "1",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030496.1.12",
          "form": "옷",
          "original_form": "옷",
          "speaker_id": "1",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030496.1.13",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030496.1.14",
          "form": "남자는",
          "original_form": "남자는",
          "speaker_id": "1",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030496.1.15",
          "form": "핑크지",
          "original_form": "핑크지",
          "speaker_id": "1",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030496.1.16",
          "form": "갤럭시놋북살때 핑크로삼",
          "original_form": "갤럭시놋북살때 핑크로삼",
          "speaker_id": "1",
          "time": "20210904 02:11"
        },
        {
          "id": "MDRW2100030496.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:11"
        },
        {
          "id": "MDRW2100030496.1.18",
          "form": "근데 사실 칼라보단 무채색이",
          "original_form": "근데 사실 칼라보단 무채색이",
          "speaker_id": "2",
          "time": "20210904 02:12"
        },
        {
          "id": "MDRW2100030496.1.19",
          "form": "조와",
          "original_form": "조와",
          "speaker_id": "2",
          "time": "20210904 02:12"
        },
        {
          "id": "MDRW2100030496.1.20",
          "form": "난퍼스널칼라가",
          "original_form": "난퍼스널칼라가",
          "speaker_id": "1",
          "time": "20210904 02:12"
        },
        {
          "id": "MDRW2100030496.1.21",
          "form": "회끼들어가지만",
          "original_form": "회끼들어가지만",
          "speaker_id": "1",
          "time": "20210904 02:12"
        },
        {
          "id": "MDRW2100030496.1.22",
          "form": "채도가있어야한대서",
          "original_form": "채도가있어야한대서",
          "speaker_id": "1",
          "time": "20210904 02:12"
        },
        {
          "id": "MDRW2100030496.1.23",
          "form": "쨍한 컬러는 악세사리로 족해",
          "original_form": "쨍한 컬러는 악세사리로 족해",
          "speaker_id": "2",
          "time": "20210904 02:12"
        },
        {
          "id": "MDRW2100030496.1.24",
          "form": "무채식회색은 안색이죽는대",
          "original_form": "무채식회색은 안색이죽는대",
          "speaker_id": "1",
          "time": "20210904 02:12"
        },
        {
          "id": "MDRW2100030496.1.25",
          "form": "나도받아보까",
          "original_form": "나도받아보까",
          "speaker_id": "2",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030496.1.26",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030496.1.27",
          "form": "옷은 다 채도있는거 사야함",
          "original_form": "옷은 다 채도있는거 사야함",
          "speaker_id": "1",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030496.1.28",
          "form": "그래서 네이비삼",
          "original_form": "그래서 네이비삼",
          "speaker_id": "1",
          "time": "20210904 02:13"
        },
