      "utterance": [
        {
          "id": "MDRW2100043401.1.1",
          "form": "월병? 옛날과자?",
          "original_form": "월병? 옛날과자?",
          "speaker_id": "2",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043401.1.2",
          "form": "중국에서 파는 거",
          "original_form": "중국에서 파는 거",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043401.1.3",
          "form": "왠지 엄청 달거 같애",
          "original_form": "왠지 엄청 달거 같애",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043401.1.4",
          "form": "어디에는 고기도 들어간다는데",
          "original_form": "어디에는 고기도 들어간다는데",
          "speaker_id": "1",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043401.1.5",
          "form": "오... 나도 먹어볼래",
          "original_form": "오... 나도 먹어볼래",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043401.1.6",
          "form": "언제 사올거야",
          "original_form": "언제 사올거야",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043401.1.7",
          "form": "당연히 내가 사올거라는 전제구낰ㅋㅋㅋㅋㅋ",
          "original_form": "당연히 내가 사올거라는 전제구낰ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043401.1.8",
          "form": "궁금하다며! 그럼 사오겄지",
          "original_form": "궁금하다며! 그럼 사오겄지",
          "speaker_id": "2",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043401.1.9",
          "form": "근데 곧 살 것 같긴 해...",
          "original_form": "근데 곧 살 것 같긴 해...",
          "speaker_id": "1",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043401.1.10",
          "form": "수미니도 궁금해하더라고",
          "original_form": "수미니도 궁금해하더라고",
          "speaker_id": "1",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043401.1.11",
          "form": "좋아좋아",
          "original_form": "좋아좋아",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043401.1.12",
          "form": "또 전통주 사먹고싶다",
          "original_form": "또 전통주 사먹고싶다",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043401.1.13",
          "form": "아 문경에서 파는 사과주 마셔봄?",
          "original_form": "아 문경에서 파는 사과주 마셔봄?",
          "speaker_id": "1",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100043401.1.14",
          "form": "아니. 맛있엉?",
          "original_form": "아니. 맛있엉?",
          "speaker_id": "2",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100043401.1.15",
          "form": "아니 그냥 생각나서 물어봤어",
          "original_form": "아니 그냥 생각나서 물어봤어",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043401.1.16",
          "form": "거기 찐 증류주 같은 건 살 엄두가 안 나서",
          "original_form": "거기 찐 증류주 같은 건 살 엄두가 안 나서",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043401.1.17",
          "form": "그랭? 한번 찾아서 마셔봐야지",
          "original_form": "그랭? 한번 찾아서 마셔봐야지",
          "speaker_id": "2",
          "time": "20210907 19:06"
        }
      ]
