      "utterance": [
        {
          "id": "MDRW2100042054.1.1",
          "form": "샤인머스캣은 정말 비싸요ㅠㅠ",
          "original_form": "샤인머스캣은 정말 비싸요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100042054.1.2",
          "form": "비싸게 샀는데 맛없을 때 화나요",
          "original_form": "비싸게 샀는데 맛없을 때 화나요",
          "speaker_id": "2",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100042054.1.3",
          "form": "아그럼배신이져",
          "original_form": "아그럼배신이져",
          "speaker_id": "1",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100042054.1.4",
          "form": "맞아요 넘나 슬퍼요",
          "original_form": "맞아요 넘나 슬퍼요",
          "speaker_id": "2",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100042054.1.5",
          "form": "배신감들거같아요!!!!\r\n체리도먹고싶어요",
          "original_form": "배신감들거같아요!!!!\r\n체리도먹고싶어요",
          "speaker_id": "1",
          "time": "20210906 19:17"
        },
        {
          "id": "MDRW2100042054.1.6",
          "form": "체리좋죠",
          "original_form": "체리좋죠",
          "speaker_id": "2",
          "time": "20210906 19:17"
        },
        {
          "id": "MDRW2100042054.1.7",
          "form": "과일이 젤 조아요",
          "original_form": "과일이 젤 조아요",
          "speaker_id": "2",
          "time": "20210906 19:17"
        },
        {
