      "utterance": [
        {
          "id": "MDRW2100043432.1.1",
          "form": "저녁 뭐 드시나용~?",
          "original_form": "저녁 뭐 드시나용~?",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043432.1.2",
          "form": "김치볶음밥 오뎅탕 먹엇어요",
          "original_form": "김치볶음밥 오뎅탕 먹엇어요",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043432.1.3",
          "form": "오! 맛있는 조합이네요",
          "original_form": "오! 맛있는 조합이네요",
          "speaker_id": "2",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043432.1.4",
          "form": "비도오니깐 오뎅국물 땡기네요",
          "original_form": "비도오니깐 오뎅국물 땡기네요",
          "speaker_id": "2",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043432.1.5",
          "form": "맛나게 먹엇어요",
          "original_form": "맛나게 먹엇어요",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043432.1.6",
          "form": "식사하셧나여",
          "original_form": "식사하셧나여",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043432.1.7",
          "form": "저는 닭꼬치 조금 남은거랑 만두구워먹으려고용",
          "original_form": "저는 닭꼬치 조금 남은거랑 만두구워먹으려고용",
          "speaker_id": "2",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043432.1.8",
          "form": "ㅜㅜ맛나것어요",
          "original_form": "ㅜㅜ맛나것어요",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043432.1.9",
          "form": "언능 드세요!!",
          "original_form": "언능 드세요!!",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043432.1.10",
          "form": "만두먹으려다가 탕수육 먹으려고 에프로 굽고있어요",
          "original_form": "만두먹으려다가 탕수육 먹으려고 에프로 굽고있어요",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043432.1.11",
          "form": "크~~~",
          "original_form": "크~~~",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043432.1.12",
          "form": "저는 에프 귀찮아서 잘 안써지더라구요",
          "original_form": "저는 에프 귀찮아서 잘 안써지더라구요",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043432.1.13",
          "form": "한번 쓰고 쳐박템 됏어여",
          "original_form": "한번 쓰고 쳐박템 됏어여",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043432.1.14",
          "form": "ㅋㅋㅋ저도 그렇긴한데 최근에 만두랑 탕수육을 샀더니 좀 쓰게되네요",
          "original_form": "ㅋㅋㅋ저도 그렇긴한데 최근에 만두랑 탕수육을 샀더니 좀 쓰게되네요",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043432.1.15",
          "form": "에프에 삼겹살 해보셧나용",
          "original_form": "에프에 삼겹살 해보셧나용",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043432.1.16",
          "form": "궁금해요",
          "original_form": "궁금해요",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043432.1.17",
          "form": "주말에 시도해볼까해요",
          "original_form": "주말에 시도해볼까해요",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043432.1.18",
          "form": "아뇨 아직 삼겹살은 안해봤어요!!!",
          "original_form": "아뇨 아직 삼겹살은 안해봤어요!!!",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043432.1.19",
          "form": "해보신다면 후기 알려주세요! ",
          "original_form": "해보신다면 후기 알려주세요! ",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043432.1.20",
          "form": "네ㅎㅎ 유튜브 보고",
          "original_form": "네ㅎㅎ 유튜브 보고",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043432.1.21",
          "form": "따라해바야겟어여",
          "original_form": "따라해바야겟어여",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043432.1.22",
          "form": "좋아요!!!",
          "original_form": "좋아요!!!",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043432.1.23",
          "form": "유튜브가 있어서 편리한거같아요",
          "original_form": "유튜브가 있어서 편리한거같아요",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043432.1.24",
          "form": "요리 레시피도 금방찾고",
          "original_form": "요리 레시피도 금방찾고",
          "speaker_id": "2",
          "time": "20210907 20:30"
        }
      ]
