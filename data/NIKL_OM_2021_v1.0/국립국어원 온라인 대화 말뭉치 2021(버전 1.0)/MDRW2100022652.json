      "utterance": [
        {
          "id": "MDRW2100022652.1.1",
          "form": "name2은 어떤 날씨 좋아하세요?!",
          "original_form": "&name2&은 어떤 날씨 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022652.1.2",
          "form": "전 가을날씨 좋아해요!!",
          "original_form": "전 가을날씨 좋아해요!!",
          "speaker_id": "2",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022652.1.3",
          "form": "살짝 서늘한 날씨요~",
          "original_form": "살짝 서늘한 날씨요~",
          "speaker_id": "2",
          "time": "20210829 18:43"
        },
        {
          "id": "MDRW2100022652.1.4",
          "form": "헛 가을이 확실히 여유롭게 만들어주는 거 같아요 ㅎ.ㅎ",
          "original_form": "헛 가을이 확실히 여유롭게 만들어주는 거 같아요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210829 18:43"
        },
        {
          "id": "MDRW2100022652.1.5",
          "form": "싫어하는 날씨는 있으신가요?",
          "original_form": "싫어하는 날씨는 있으신가요?",
          "speaker_id": "1",
          "time": "20210829 18:43"
        },
        {
          "id": "MDRW2100022652.1.6",
          "form": "음 전 여름이요ㅠㅠ",
          "original_form": "음 전 여름이요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 18:43"
        },
        {
          "id": "MDRW2100022652.1.7",
          "form": "너무 더운 날 너무 힘들어요",
          "original_form": "너무 더운 날 너무 힘들어요",
          "speaker_id": "2",
          "time": "20210829 18:43"
        },
        {
          "id": "MDRW2100022652.1.8",
          "form": "으으 맞아요 땀나면 짜증나잖아요 -.-",
          "original_form": "으으 맞아요 땀나면 짜증나잖아요 -.-",
          "speaker_id": "1",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100022652.1.9",
          "form": "일기예보 잘 챙겨보시나요?",
          "original_form": "일기예보 잘 챙겨보시나요?",
          "speaker_id": "1",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100022652.1.10",
          "form": "어 원래는 잘 안챙겨봤는데",
          "original_form": "어 원래는 잘 안챙겨봤는데",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022652.1.11",
          "form": "아침에 출근 전에 비오는진 확인해요",
          "original_form": "아침에 출근 전에 비오는진 확인해요",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022652.1.12",
          "form": "헛 저도요!",
          "original_form": "헛 저도요!",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022652.1.13",
          "form": "저번에 한 번 쫄딱 젖었다가",
          "original_form": "저번에 한 번 쫄딱 젖었다가",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022652.1.14",
          "form": "일기예보의 중요성을 알게 됐거든요... ㅎ.ㅎ",
          "original_form": "일기예보의 중요성을 알게 됐거든요... ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022652.1.15",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100022652.1.16",
          "form": "근데 우산 가져가도 제대로 쓴 날은 몇번 없는거같아요ㅋㅋㅋ",
          "original_form": "근데 우산 가져가도 제대로 쓴 날은 몇번 없는거같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100022652.1.17",
          "form": "맞아요 -.- 예상외로 자주 틀리더라고요 예보가???",
          "original_form": "맞아요 -.- 예상외로 자주 틀리더라고요 예보가???",
          "speaker_id": "1",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100022652.1.18",
          "form": "name2은 장우산 좋아하세요?",
          "original_form": "&name2&은 장우산 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100022652.1.19",
          "form": "네 근데 휴대하기가 어려워서ㅠㅠ",
          "original_form": "네 근데 휴대하기가 어려워서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 23:57"
        },
        {
          "id": "MDRW2100022652.1.20",
          "form": "비 많이 안오면 작은거 가지고 다녀요",
          "original_form": "비 많이 안오면 작은거 가지고 다녀요",
          "speaker_id": "2",
          "time": "20210829 23:57"
        },
        {
          "id": "MDRW2100022652.1.21",
          "form": "헛 저는 죽어도 장우산 파에요",
          "original_form": "헛 저는 죽어도 장우산 파에요",
          "speaker_id": "1",
          "time": "20210830 00:02"
        },
        {
          "id": "MDRW2100022652.1.22",
          "form": "절대 비 맞기 싫어서 ㅎㅎ",
          "original_form": "절대 비 맞기 싫어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:02"
        },
        {
          "id": "MDRW2100022652.1.23",
          "form": "오 근데 들고다니기 무겁지 않나요?",
          "original_form": "오 근데 들고다니기 무겁지 않나요?",
          "speaker_id": "2",
          "time": "20210831 18:00"
        },
        {
          "id": "MDRW2100022652.1.24",
          "form": "전 키가 작은 편이라 장우산 질질 끌고 다녀요ㅎㅎ..",
          "original_form": "전 키가 작은 편이라 장우산 질질 끌고 다녀요ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210831 18:00"
        },
        {
          "id": "MDRW2100022652.1.25",
          "form": "무겁긴한데 ^.^ 젖는 것보단 낫더라고요 ㅎㅎ",
          "original_form": "무겁긴한데 ^.^ 젖는 것보단 낫더라고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 19:22"
        },
        {
          "id": "MDRW2100022652.1.26",
          "form": "오늘 저희 동네는 비 많이 왓는데 밥님네는요?!",
          "original_form": "오늘 저희 동네는 비 많이 왓는데 밥님네는요?!",
          "speaker_id": "1",
          "time": "20210831 19:22"
        },
        {
          "id": "MDRW2100022652.1.27",
          "form": "저희도 꽤 왔어요",
          "original_form": "저희도 꽤 왔어요",
          "speaker_id": "2",
          "time": "20210831 20:44"
        },
        {
          "id": "MDRW2100022652.1.28",
          "form": "오다 안오다 하더니 지금은 또 많이 오네요",
          "original_form": "오다 안오다 하더니 지금은 또 많이 오네요",
