      "utterance": [
        {
          "id": "MDRW2100041129.1.1",
          "form": "오이까지 함께 넣는거 좋은데요?",
          "original_form": "오이까지 함께 넣는거 좋은데요?",
          "speaker_id": "1",
          "time": "20210905 17:36"
        },
        {
          "id": "MDRW2100041129.1.2",
          "form": "같은 방법으로 만들꺼라면 한번에 하는거 좋네요!",
          "original_form": "같은 방법으로 만들꺼라면 한번에 하는거 좋네요!",
          "speaker_id": "1",
          "time": "20210905 17:37"
        },
        {
          "id": "MDRW2100041129.1.3",
          "form": "저 워낙 피클을 좋아해요ㅠ",
          "original_form": "저 워낙 피클을 좋아해요ㅠ",
          "speaker_id": "2",
          "time": "20210905 17:37"
        },
        {
          "id": "MDRW2100041129.1.4",
          "form": "맥주마실때 피클만있으면 돼요",
          "original_form": "맥주마실때 피클만있으면 돼요",
          "speaker_id": "2",
          "time": "20210905 17:37"
        },
        {
          "id": "MDRW2100041129.1.5",
          "form": "오 피클 진짜 좋아하시나봐요!",
          "original_form": "오 피클 진짜 좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210905 17:43"
        },
        {
          "id": "MDRW2100041129.1.6",
          "form": "저는 오이는 싫어하는데 피클은 잘 먹어요ㅋㅋㅋ",
          "original_form": "저는 오이는 싫어하는데 피클은 잘 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:44"
        },
        {
          "id": "MDRW2100041129.1.7",
          "form": "아 오이안좋아하시는구나!",
          "original_form": "아 오이안좋아하시는구나!",
          "speaker_id": "2",
          "time": "20210905 17:44"
        },
        {
          "id": "MDRW2100041129.1.8",
          "form": "냉면에있는 오이도 안좋아하세요?!",
          "original_form": "냉면에있는 오이도 안좋아하세요?!",
          "speaker_id": "2",
          "time": "20210905 17:44"
        },
        {
          "id": "MDRW2100041129.1.9",
          "form": "네ㅜㅜ냉면에 오이도 별로 안 좋아해요",
          "original_form": "네ㅜㅜ냉면에 오이도 별로 안 좋아해요",
          "speaker_id": "1",
          "time": "20210905 17:46"
        },
        {
          "id": "MDRW2100041129.1.10",
          "form": "오이 안좋아하는 분들 많더라고요!",
          "original_form": "오이 안좋아하는 분들 많더라고요!",
          "speaker_id": "2",
          "time": "20210905 17:46"
        },
        {
          "id": "MDRW2100041129.1.11",
          "form": "맞아요! 저는 피망이랑 파프리카도 안 좋아해요",
          "original_form": "맞아요! 저는 피망이랑 파프리카도 안 좋아해요",
          "speaker_id": "1",
          "time": "20210905 17:48"
        },
        {
          "id": "MDRW2100041129.1.12",
          "form": "오 저는 노랑파프리카는 좋아해요",
          "original_form": "오 저는 노랑파프리카는 좋아해요",
          "speaker_id": "2",
          "time": "20210905 17:51"
        },
        {
          "id": "MDRW2100041129.1.13",
          "form": "파프리카가 색마다 맛이 조금 다르다고는 하더라구요!",
          "original_form": "파프리카가 색마다 맛이 조금 다르다고는 하더라구요!",
          "speaker_id": "1",
          "time": "20210905 18:03"
        },
        {
          "id": "MDRW2100041129.1.14",
          "form": "그쵸?!! 전 그런것같아요ㅠ",
          "original_form": "그쵸?!! 전 그런것같아요ㅠ",
          "speaker_id": "2",
          "time": "20210905 18:27"
        },
        {
          "id": "MDRW2100041129.1.15",
          "form": "노랑은좀달달한것같아요",
          "original_form": "노랑은좀달달한것같아요",
          "speaker_id": "2",
          "time": "20210905 18:27"
        },
        {
          "id": "MDRW2100041129.1.16",
          "form": "달달함이 느껴져서 노랑색을 좋아시는거군요!",
          "original_form": "달달함이 느껴져서 노랑색을 좋아시는거군요!",
          "speaker_id": "1",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100041129.1.17",
          "form": "새콤달달한걸 좋아해요!",
          "original_form": "새콤달달한걸 좋아해요!",
          "speaker_id": "2",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100041129.1.18",
          "form": "새콤달달하니 갑자기 비빔면이 떠오르네요ㅋㅋㅋ",
          "original_form": "새콤달달하니 갑자기 비빔면이 떠오르네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:34"
        },
        {
          "id": "MDRW2100041129.1.19",
          "form": "비빔면도 너무 좋아해요ㅠ",
          "original_form": "비빔면도 너무 좋아해요ㅠ",
          "speaker_id": "2",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100041129.1.20",
          "form": "비빔면도 맛있죠ㅎㅎ\r\n비빔면은 특히 여름에 자주 생각나는 음식 같아요",
          "original_form": "비빔면도 맛있죠ㅎㅎ\r\n비빔면은 특히 여름에 자주 생각나는 음식 같아요",
