      "utterance": [
        {
          "id": "MDRW2100029433.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 07:54"
        },
        {
          "id": "MDRW2100029433.1.2",
          "form": "혹시 사랑하고 있으신가요?",
          "original_form": "혹시 사랑하고 있으신가요?",
          "speaker_id": "1",
          "time": "20210903 07:54"
        },
        {
          "id": "MDRW2100029433.1.3",
          "form": "하핫 아니요",
          "original_form": "하핫 아니요",
          "speaker_id": "2",
          "time": "20210903 07:54"
        },
        {
          "id": "MDRW2100029433.1.4",
          "form": "빛이나는 솔로",
          "original_form": "빛이나는 솔로",
          "speaker_id": "2",
          "time": "20210903 07:54"
        },
        {
          "id": "MDRW2100029433.1.5",
          "form": "생활중ㅋㄱㅋㅋ",
          "original_form": "생활중ㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 07:54"
        },
        {
          "id": "MDRW2100029433.1.6",
          "form": "사랑하는 중이세요",
          "original_form": "사랑하는 중이세요",
          "speaker_id": "2",
          "time": "20210903 07:55"
        },
        {
          "id": "MDRW2100029433.1.7",
          "form": "ㅎㅎ솔로가 좋죠",
          "original_form": "ㅎㅎ솔로가 좋죠",
          "speaker_id": "1",
          "time": "20210903 07:55"
        },
        {
          "id": "MDRW2100029433.1.8",
          "form": "저도 빛나는 솔로입니다",
          "original_form": "저도 빛나는 솔로입니다",
          "speaker_id": "1",
          "time": "20210903 07:55"
        },
        {
          "id": "MDRW2100029433.1.9",
          "form": "이게 편해요",
          "original_form": "이게 편해요",
          "speaker_id": "2",
          "time": "20210903 07:55"
        },
        {
          "id": "MDRW2100029433.1.10",
          "form": "세상 좋아요ㅋㅋ",
          "original_form": "세상 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 07:55"
        },
        {
          "id": "MDRW2100029433.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210903 07:55"
        },
        {
          "id": "MDRW2100029433.1.12",
          "form": "다만 해보지 못한 사랑이 어떤지 궁금하긴 하네요",
          "original_form": "다만 해보지 못한 사랑이 어떤지 궁금하긴 하네요",
          "speaker_id": "1",
          "time": "20210903 07:55"
        },
        {
          "id": "MDRW2100029433.1.13",
          "form": "혹시 이상형이 따로 있으세요?",
          "original_form": "혹시 이상형이 따로 있으세요?",
          "speaker_id": "2",
          "time": "20210903 07:56"
        },
        {
          "id": "MDRW2100029433.1.14",
          "form": "생각이 비슷해서 대화가 잘되는 사람이요ㅎ",
          "original_form": "생각이 비슷해서 대화가 잘되는 사람이요ㅎ",
          "speaker_id": "1",
          "time": "20210903 07:56"
        },
        {
          "id": "MDRW2100029433.1.15",
          "form": "외모도 매력적이라면 좋겠지만",
          "original_form": "외모도 매력적이라면 좋겠지만",
          "speaker_id": "1",
          "time": "20210903 07:57"
        },
        {
          "id": "MDRW2100029433.1.16",
          "form": "금상첨화죠",
          "original_form": "금상첨화죠",
          "speaker_id": "2",
          "time": "20210903 07:57"
        },
        {
          "id": "MDRW2100029433.1.17",
          "form": "혹시 어떤 연애를 해보셨나요",
          "original_form": "혹시 어떤 연애를 해보셨나요",
          "speaker_id": "1",
          "time": "20210903 07:58"
        },
        {
          "id": "MDRW2100029433.1.18",
          "form": "그치만 아무리 정우성 강동원 얼굴이어도 요상망측한 말만 한다면 정뚝떨일듯",
          "original_form": "그치만 아무리 정우성 강동원 얼굴이어도 요상망측한 말만 한다면 정뚝떨일듯",
          "speaker_id": "2",
          "time": "20210903 07:58"
        },
        {
          "id": "MDRW2100029433.1.19",
          "form": "맞습니다ㅎㅎ",
          "original_form": "맞습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 07:59"
        },
        {
          "id": "MDRW2100029433.1.20",
          "form": "일단 말부터 통해야 연애를 시작하죠",
          "original_form": "일단 말부터 통해야 연애를 시작하죠",
          "speaker_id": "1",
          "time": "20210903 07:59"
        },
        {
          "id": "MDRW2100029433.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 07:59"
        },
        {
          "id": "MDRW2100029433.1.22",
          "form": "지난 연애들은 그냥 그저그랬어요",
          "original_form": "지난 연애들은 그냥 그저그랬어요",
          "speaker_id": "2",
          "time": "20210903 07:59"
        },
        {
          "id": "MDRW2100029433.1.23",
          "form": "참 좋았다하는 기억이 없네요ㅋㅋ",
          "original_form": "참 좋았다하는 기억이 없네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:00"
        },
        {
          "id": "MDRW2100029433.1.24",
          "form": "ㅜㅜㅜㅡ",
          "original_form": "ㅜㅜㅜㅡ",
          "speaker_id": "2",
          "time": "20210903 08:00"
        },
        {
          "id": "MDRW2100029433.1.25",
          "form": "아쉽네요 한번쯤 딱맞는 사람을 만났더라면 좋았을텐데",
          "original_form": "아쉽네요 한번쯤 딱맞는 사람을 만났더라면 좋았을텐데",
          "speaker_id": "1",
          "time": "20210903 08:00"
        },
        {
          "id": "MDRW2100029433.1.26",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210903 08:01"
        },
        {
          "id": "MDRW2100029433.1.27",
          "form": "영혼이 충만해지는 사건은 영화에만 있나 봐요",
          "original_form": "영혼이 충만해지는 사건은 영화에만 있나 봐요",
