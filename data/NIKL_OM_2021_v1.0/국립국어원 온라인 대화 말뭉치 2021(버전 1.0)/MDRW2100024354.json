      "utterance": [
        {
          "id": "MDRW2100024354.1.1",
          "form": "버블티 좋아하세요?",
          "original_form": "버블티 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 01:35"
        },
        {
          "id": "MDRW2100024354.1.2",
          "form": "네 좋아하는데 동네에서 맛있는 곳 찾기가 힘들더라구요",
          "original_form": "네 좋아하는데 동네에서 맛있는 곳 찾기가 힘들더라구요",
          "speaker_id": "2",
          "time": "20210831 01:36"
        },
        {
          "id": "MDRW2100024354.1.3",
          "form": "앗ㅋㅋㅋ 저는 그냥",
          "original_form": "앗ㅋㅋㅋ 저는 그냥",
          "speaker_id": "1",
          "time": "20210831 01:36"
        },
        {
          "id": "MDRW2100024354.1.4",
          "form": "공차 가요",
          "original_form": "공차 가요",
          "speaker_id": "1",
          "time": "20210831 01:36"
        },
        {
          "id": "MDRW2100024354.1.5",
          "form": "공차가시면 커스텀 어떻게 해서 드세요?",
          "original_form": "공차가시면 커스텀 어떻게 해서 드세요?",
          "speaker_id": "1",
          "time": "20210831 01:37"
        },
        {
          "id": "MDRW2100024354.1.6",
          "form": "흡 제가 공차를 잘 안가서요ㅋㅋㅋ멀어서 귀찮더라구요...",
          "original_form": "흡 제가 공차를 잘 안가서요ㅋㅋㅋ멀어서 귀찮더라구요...",
          "speaker_id": "2",
          "time": "20210831 01:37"
        },
        {
          "id": "MDRW2100024354.1.7",
          "form": "추천해주세요",
          "original_form": "추천해주세요",
          "speaker_id": "2",
          "time": "20210831 01:37"
        },
        {
          "id": "MDRW2100024354.1.8",
          "form": "저는 좀 심플하게 먹는데",
          "original_form": "저는 좀 심플하게 먹는데",
          "speaker_id": "1",
          "time": "20210831 01:38"
        },
        {
          "id": "MDRW2100024354.1.9",
          "form": "타로버블티에 펄 2번 추가하구요",
          "original_form": "타로버블티에 펄 2번 추가하구요",
          "speaker_id": "1",
          "time": "20210831 01:38"
        },
        {
          "id": "MDRW2100024354.1.10",
          "form": "당도는 70",
          "original_form": "당도는 70",
          "speaker_id": "1",
          "time": "20210831 01:38"
        },
        {
          "id": "MDRW2100024354.1.11",
          "form": "얼음은 적게 해서 먹고있어요ㅋㅋ",
          "original_form": "얼음은 적게 해서 먹고있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:38"
        },
        {
          "id": "MDRW2100024354.1.12",
          "form": "타로버블티는 무슨 맛이에요??",
          "original_form": "타로버블티는 무슨 맛이에요??",
          "speaker_id": "2",
          "time": "20210831 01:40"
        },
        {
          "id": "MDRW2100024354.1.13",
          "form": "타로 파우더 맛인데",
          "original_form": "타로 파우더 맛인데",
          "speaker_id": "1",
          "time": "20210831 01:41"
        },
        {
          "id": "MDRW2100024354.1.14",
          "form": "그 타로를 뭐가 설명드려야할지 모르겠네요ㅋㅋㅋ",
          "original_form": "그 타로를 뭐가 설명드려야할지 모르겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:41"
        },
        {
          "id": "MDRW2100024354.1.15",
          "form": "약간 고구마같은 맛인가요??!",
          "original_form": "약간 고구마같은 맛인가요??!",
          "speaker_id": "2",
          "time": "20210831 01:41"
        },
        {
          "id": "MDRW2100024354.1.16",
          "form": "음 아주 조금만 비슷해요ㅋㅋㅋ",
          "original_form": "음 아주 조금만 비슷해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:42"
        },
        {
          "id": "MDRW2100024354.1.17",
          "form": "궁금하시면",
          "original_form": "궁금하시면",
          "speaker_id": "1",
          "time": "20210831 01:42"
        },
        {
          "id": "MDRW2100024354.1.18",
          "form": "드셔보세요!",
          "original_form": "드셔보세요!",
          "speaker_id": "1",
          "time": "20210831 01:42"
        },
        {
          "id": "MDRW2100024354.1.19",
          "form": "쥬씨에서도 타로버블티 팔아요",
          "original_form": "쥬씨에서도 타로버블티 팔아요",
          "speaker_id": "1",
          "time": "20210831 01:43"
        },
        {
          "id": "MDRW2100024354.1.20",
          "form": "오!! 나중에 꼭 먹어볼게요",
          "original_form": "오!! 나중에 꼭 먹어볼게요",
          "speaker_id": "2",
          "time": "20210831 01:45"
        },
        {
          "id": "MDRW2100024354.1.21",
          "form": "저는 찬걸 좋아해서 프라프치노 같은걸 좋아하거든요",
          "original_form": "저는 찬걸 좋아해서 프라프치노 같은걸 좋아하거든요",
          "speaker_id": "2",
          "time": "20210831 01:46"
        },
        {
          "id": "MDRW2100024354.1.22",
          "form": "프라프치노 좋아하세요??",
          "original_form": "프라프치노 좋아하세요??",
          "speaker_id": "2",
          "time": "20210831 01:46"
        },
        {
          "id": "MDRW2100024354.1.23",
          "form": "네네! 자바칩 프라푸치노 즐겨먹어용ㅎㅎ",
          "original_form": "네네! 자바칩 프라푸치노 즐겨먹어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 01:46"
        },
        {
          "id": "MDRW2100024354.1.24",
          "form": "역시 자바칩이 맛있져",
          "original_form": "역시 자바칩이 맛있져",
          "speaker_id": "2",
          "time": "20210831 01:47"
        },
        {
          "id": "MDRW2100024354.1.25",
          "form": "커스텀도 해드세요??",
          "original_form": "커스텀도 해드세요??",
          "speaker_id": "2",
          "time": "20210831 01:47"
        },
        {
          "id": "MDRW2100024354.1.26",
          "form": "자바칩 9번 추가해서",
          "original_form": "자바칩 9번 추가해서",
          "speaker_id": "1",
          "time": "20210831 01:48"
        },
        {
          "id": "MDRW2100024354.1.27",
          "form": "반은 갈고 반은 위에 통으로 뿌려먹어용",
          "original_form": "반은 갈고 반은 위에 통으로 뿌려먹어용",
          "speaker_id": "1",
          "time": "20210831 01:48"
        }
      ]
    }
