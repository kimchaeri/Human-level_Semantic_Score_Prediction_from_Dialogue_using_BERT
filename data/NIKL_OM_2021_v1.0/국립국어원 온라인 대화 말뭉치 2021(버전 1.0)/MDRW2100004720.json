      "utterance": [
        {
          "id": "MDRW2100004720.1.1",
          "form": "name2님 오늘 뭐 드셨어요?",
          "original_form": "&name2&님 오늘 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004720.1.2",
          "form": "저는 오늘 칼국수 먹었어요 전이랑!",
          "original_form": "저는 오늘 칼국수 먹었어요 전이랑!",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004720.1.3",
          "form": "비오는날이라 잘어울리더라구요",
          "original_form": "비오는날이라 잘어울리더라구요",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004720.1.4",
          "form": "name1님은 뭐드셨어요?",
          "original_form": "&name1&님은 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004720.1.5",
          "form": "우와 비오는 날 너무 잘어울려요!",
          "original_form": "우와 비오는 날 너무 잘어울려요!",
          "speaker_id": "1",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004720.1.6",
          "form": "전 복숭아랑 샌드위치 먹었어요",
          "original_form": "전 복숭아랑 샌드위치 먹었어요",
          "speaker_id": "1",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004720.1.7",
          "form": "우와 무슨 샌드위치 드셨어요??!",
          "original_form": "우와 무슨 샌드위치 드셨어요??!",
          "speaker_id": "2",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004720.1.8",
          "form": "복숭아는 딱복인가요 물복인가요..?",
          "original_form": "복숭아는 딱복인가요 물복인가요..?",
          "speaker_id": "2",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004720.1.9",
          "form": "퀴즈노스라구 아세요? 근처에 생겨서 처음 먹어봤어요!",
          "original_form": "퀴즈노스라구 아세요? 근처에 생겨서 처음 먹어봤어요!",
          "speaker_id": "1",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004720.1.10",
          "form": "복숭아는 물복이었어요",
          "original_form": "복숭아는 물복이었어요",
          "speaker_id": "1",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004720.1.11",
          "form": "헐 ㅠㅠㅠㅠ 저 퀴즈노스 제 최애예요..",
          "original_form": "헐 ㅠㅠㅠㅠ 저 퀴즈노스 제 최애예요..",
          "speaker_id": "2",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004720.1.12",
          "form": "퀴즈노스 먹고 난 후로 서브웨이 안가요ㅠㅠ",
          "original_form": "퀴즈노스 먹고 난 후로 서브웨이 안가요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004720.1.13",
          "form": "저는...딱복파예요",
          "original_form": "저는...딱복파예요",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004720.1.14",
          "form": "헉 아시는구나 저 처음 먹어봤는데 맛있더라구요!!",
          "original_form": "헉 아시는구나 저 처음 먹어봤는데 맛있더라구요!!",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004720.1.15",
          "form": "퀴즈노스 먹고싶다.. 부러워요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "퀴즈노스 먹고싶다.. 부러워요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004720.1.16",
          "form": "전 물복 딱복 다 잘 먹어요ㅎㅎ근데 요새는 물복이 더 많이 나오나봐요",
          "original_form": "전 물복 딱복 다 잘 먹어요ㅎㅎ근데 요새는 물복이 더 많이 나오나봐요",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004720.1.17",
          "form": "저녁은 뭐드세요??",
          "original_form": "저녁은 뭐드세요??",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004720.1.18",
          "form": "음 저녁메뉴는 아직인데 뭔가 라면이 땡겨요",
          "original_form": "음 저녁메뉴는 아직인데 뭔가 라면이 땡겨요",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004720.1.19",
          "form": "헐 그렇구나 저희집은 딱복 한가득인데 처치불가예요ㅠ",
          "original_form": "헐 그렇구나 저희집은 딱복 한가득인데 처치불가예요ㅠ",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004720.1.20",
          "form": "라면은 뭐 좋아하세요?",
          "original_form": "라면은 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004720.1.21",
          "form": "저는 불닭볶음면 좋아해요 아니면 너구리!",
          "original_form": "저는 불닭볶음면 좋아해요 아니면 너구리!",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004720.1.22",
          "form": "헉 저도 불닭이요! 국물 있는 건 신라면 좋아합니당ㅎㅎ",
          "original_form": "헉 저도 불닭이요! 국물 있는 건 신라면 좋아합니당ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004720.1.23",
          "form": "불닭에 치즈까지 꼭..",
          "original_form": "불닭에 치즈까지 꼭..",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004720.1.24",
          "form": "와 맛있겠다...",
          "original_form": "와 맛있겠다...",
          "speaker_id": "2",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004720.1.25",
          "form": "불닭 좀 촉촉하게 먹는 것도 맛있더라구요.",
          "original_form": "불닭 좀 촉촉하게 먹는 것도 맛있더라구요.",
          "speaker_id": "2",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004720.1.26",
          "form": "와 촉촉하게는 안 먹어봤는데",
          "original_form": "와 촉촉하게는 안 먹어봤는데",
          "speaker_id": "1",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004720.1.27",
          "form": "물을 좀 남겨서 그렇게 먹는거죠? 벌써 맛있다",
          "original_form": "물을 좀 남겨서 그렇게 먹는거죠? 벌써 맛있다",
          "speaker_id": "1",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004720.1.28",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210821 18:20"
        }
      ]
