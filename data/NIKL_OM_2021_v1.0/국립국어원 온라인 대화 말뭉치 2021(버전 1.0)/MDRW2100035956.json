      "utterance": [
        {
          "id": "MDRW2100035956.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100035956.1.2",
          "form": "식사는 하셨어용?",
          "original_form": "식사는 하셨어용?",
          "speaker_id": "1",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100035956.1.3",
          "form": "안녕하세요! 방금 점심 먹었습니다~ㅎ.ㅎ",
          "original_form": "안녕하세요! 방금 점심 먹었습니다~ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210830 12:39"
        },
        {
          "id": "MDRW2100035956.1.4",
          "form": "우옹 전 아직인데",
          "original_form": "우옹 전 아직인데",
          "speaker_id": "1",
          "time": "20210830 12:39"
        },
        {
          "id": "MDRW2100035956.1.5",
          "form": "뭐 드셨어요??",
          "original_form": "뭐 드셨어요??",
          "speaker_id": "1",
          "time": "20210830 12:39"
        },
        {
          "id": "MDRW2100035956.1.6",
          "form": "두부면에 도전해봤어요! 두부면으로 굴소스볶음면 만들었어요~!",
          "original_form": "두부면에 도전해봤어요! 두부면으로 굴소스볶음면 만들었어요~!",
          "speaker_id": "2",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100035956.1.7",
          "form": "헐 저 두부면 완전 좋아하는데!",
          "original_form": "헐 저 두부면 완전 좋아하는데!",
          "speaker_id": "1",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100035956.1.8",
          "form": "어디꺼 쓰세요?",
          "original_form": "어디꺼 쓰세요?",
          "speaker_id": "1",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100035956.1.9",
          "form": "풀무원??",
          "original_form": "풀무원??",
          "speaker_id": "1",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100035956.1.10",
          "form": "아 자주드셨었구나!! 저 이번에 처음 먹어보는거라 ㅋㄱㄱㅋ 풀무원꺼 얇은거 먹어봤어요!",
          "original_form": "아 자주드셨었구나!! 저 이번에 처음 먹어보는거라 ㅋㄱㄱㅋ 풀무원꺼 얇은거 먹어봤어요!",
          "speaker_id": "2",
          "time": "20210830 13:24"
        },
        {
          "id": "MDRW2100035956.1.11",
          "form": "이거 맛 괜찮지않아용? ㅎㅎㅎ",
          "original_form": "이거 맛 괜찮지않아용? ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100035956.1.12",
          "form": "고소하고 담백하고 두부로 이런 요리를 할 수 있다는게!",
          "original_form": "고소하고 담백하고 두부로 이런 요리를 할 수 있다는게!",
          "speaker_id": "1",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100035956.1.13",
          "form": "네 생각보다 괜찮더라구요! 밀가루면처럼 먹고나서 배에 얹히는 느낌도 없고 ㅎ.ㅎ 자주 먹게될거 같아요",
          "original_form": "네 생각보다 괜찮더라구요! 밀가루면처럼 먹고나서 배에 얹히는 느낌도 없고 ㅎ.ㅎ 자주 먹게될거 같아요",
          "speaker_id": "2",
          "time": "20210830 13:39"
        },
        {
          "id": "MDRW2100035956.1.14",
          "form": "name1님은 어디꺼 면두부 드시고 계셨나요? 추천 좀 해주세요!!",
          "original_form": "&name1&님은 어디꺼 면두부 드시고 계셨나요? 추천 좀 해주세요!!",
          "speaker_id": "2",
          "time": "20210830 13:39"
        },
        {
          "id": "MDRW2100035956.1.15",
          "form": "아 맞아요 소화가 잘되더라구요",
          "original_form": "아 맞아요 소화가 잘되더라구요",
          "speaker_id": "1",
          "time": "20210830 13:39"
        },
        {
          "id": "MDRW2100035956.1.16",
          "form": "저 면두부 그 대형마트 말고",
          "original_form": "저 면두부 그 대형마트 말고",
          "speaker_id": "1",
          "time": "20210830 13:39"
        },
        {
          "id": "MDRW2100035956.1.17",
          "form": "동네에 있는 마트에서 중소기업에서 만든",
          "original_form": "동네에 있는 마트에서 중소기업에서 만든",
          "speaker_id": "1",
          "time": "20210830 13:39"
        },
        {
          "id": "MDRW2100035956.1.18",
          "form": "ㅇㅇ푸드 이런곳에서 만든 두부면 사먹는데",
          "original_form": "ㅇㅇ푸드 이런곳에서 만든 두부면 사먹는데",
          "speaker_id": "1",
          "time": "20210830 13:40"
        },
        {
          "id": "MDRW2100035956.1.19",
          "form": "풀무원께 제일 맛있는것같아용",
          "original_form": "풀무원께 제일 맛있는것같아용",
          "speaker_id": "1",
          "time": "20210830 13:40"
        },
        {
          "id": "MDRW2100035956.1.20",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "2",
          "time": "20210830 14:09"
        },
        {
          "id": "MDRW2100035956.1.21",
          "form": "그렇지만 풀무원거 가격이 조금 있는거 같아요,.ㅠ 지금 포두부 잘라서 해먹는걸로 대체할까 생각 중이에요!!",
          "original_form": "그렇지만 풀무원거 가격이 조금 있는거 같아요,.ㅠ 지금 포두부 잘라서 해먹는걸로 대체할까 생각 중이에요!!",
          "speaker_id": "2",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100035956.1.22",
          "form": "포두부 자르는것도 방법이겠네용",
          "original_form": "포두부 자르는것도 방법이겠네용",
          "speaker_id": "1",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100035956.1.23",
          "form": "원하는 굵기로!ㅎㅎ",
          "original_form": "원하는 굵기로!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100035956.1.24",
          "form": "풀무원께 양에 비해 비싸긴해요ㅠ",
          "original_form": "풀무원께 양에 비해 비싸긴해요ㅠ",
          "speaker_id": "1",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100035956.1.25",
          "form": "역시 조금 비싸냐 아니면 맛을 살짝 포기하느냐의 문제네요 ㅠㅜㅠㅜㅠ",
          "original_form": "역시 조금 비싸냐 아니면 맛을 살짝 포기하느냐의 문제네요 ㅠㅜㅠㅜㅠ",
          "speaker_id": "2",
          "time": "20210830 14:12"
        },
        {
          "id": "MDRW2100035956.1.26",
          "form": "비싼게 맛있는건 진리인것같아요ㅋㅋㅋ",
