      "utterance": [
        {
          "id": "MDRW2100046870.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211108 09:33"
        },
        {
          "id": "MDRW2100046870.1.2",
          "form": "안녕하세요~ 비가 많이오네요",
          "original_form": "안녕하세요~ 비가 많이오네요",
          "speaker_id": "2",
          "time": "20211108 09:34"
        },
        {
          "id": "MDRW2100046870.1.3",
          "form": "네 월요일 아침부터 비가 많이 내리네요",
          "original_form": "네 월요일 아침부터 비가 많이 내리네요",
          "speaker_id": "1",
          "time": "20211108 09:36"
        },
        {
          "id": "MDRW2100046870.1.4",
          "form": "월요일부터 비오면 출근하기가 더 힘들어지는거같아요",
          "original_form": "월요일부터 비오면 출근하기가 더 힘들어지는거같아요",
          "speaker_id": "1",
          "time": "20211108 09:36"
        },
        {
          "id": "MDRW2100046870.1.5",
          "form": "맞아요.. 저는 지하철에서 사람 붐비는게 싫어서",
          "original_form": "맞아요.. 저는 지하철에서 사람 붐비는게 싫어서",
          "speaker_id": "2",
          "time": "20211108 09:37"
        },
        {
          "id": "MDRW2100046870.1.6",
          "form": "일부러 더 일찍 나왔어요 ",
          "original_form": "일부러 더 일찍 나왔어요 ",
          "speaker_id": "2",
          "time": "20211108 09:37"
        },
        {
          "id": "MDRW2100046870.1.7",
          "form": "비오는 지하철 계단내려갈때 너무 미끄러워서",
          "original_form": "비오는 지하철 계단내려갈때 너무 미끄러워서",
          "speaker_id": "1",
          "time": "20211108 09:54"
        },
        {
          "id": "MDRW2100046870.1.8",
          "form": "조심해야해요",
          "original_form": "조심해야해요",
          "speaker_id": "1",
          "time": "20211108 09:54"
        },
        {
          "id": "MDRW2100046870.1.9",
          "form": "퇴근길도 걱정이네요.. 물난리만 안났으면 좋겠어요 ㅋㅋ",
          "original_form": "퇴근길도 걱정이네요.. 물난리만 안났으면 좋겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 09:56"
        },
        {
          "id": "MDRW2100046870.1.10",
          "form": "그리고 우산 쓰는게 너무 귀찮아요",
          "original_form": "그리고 우산 쓰는게 너무 귀찮아요",
          "speaker_id": "1",
          "time": "20211108 09:56"
        },
        {
          "id": "MDRW2100046870.1.11",
          "form": "설마 태풍오나요?",
          "original_form": "설마 태풍오나요?",
          "speaker_id": "1",
          "time": "20211108 09:56"
        },
        {
          "id": "MDRW2100046870.1.12",
          "form": "에이 설마요 태풍 소식은 못들었어요",
          "original_form": "에이 설마요 태풍 소식은 못들었어요",
          "speaker_id": "2",
          "time": "20211108 09:56"
        },
        {
          "id": "MDRW2100046870.1.13",
          "form": "우산은 정말 쓰고나서 들고다니는 것도 힘들고 귀찮죠 ㅠ",
          "original_form": "우산은 정말 쓰고나서 들고다니는 것도 힘들고 귀찮죠 ㅠ",
          "speaker_id": "2",
          "time": "20211108 09:56"
        },
        {
          "id": "MDRW2100046870.1.14",
          "form": "와 지금 비 엄청많이 오네요",
          "original_form": "와 지금 비 엄청많이 오네요",
          "speaker_id": "1",
          "time": "20211108 09:58"
        },
        {
          "id": "MDRW2100046870.1.15",
          "form": "제가나올땐 비가 별로 안왔는데",
          "original_form": "제가나올땐 비가 별로 안왔는데",
          "speaker_id": "1",
          "time": "20211108 09:58"
        },
        {
          "id": "MDRW2100046870.1.16",
          "form": "여긴 서울인데 어제 밤부터 계속 왔어요 ",
          "original_form": "여긴 서울인데 어제 밤부터 계속 왔어요 ",
          "speaker_id": "2",
          "time": "20211108 09:58"
        },
        {
          "id": "MDRW2100046870.1.17",
          "form": "뉴스보니 오늘오후에 그칠예정이래요",
          "original_form": "뉴스보니 오늘오후에 그칠예정이래요",
          "speaker_id": "1",
          "time": "20211108 09:59"
        },
        {
          "id": "MDRW2100046870.1.18",
          "form": "후.. 부디 퇴근 때는 비가 그치면 좋겠네요",
          "original_form": "후.. 부디 퇴근 때는 비가 그치면 좋겠네요",
          "speaker_id": "2",
          "time": "20211108 10:00"
        },
        {
          "id": "MDRW2100046870.1.19",
          "form": "네  저도 퇴근할때 비가 그쳤어면좋겠어요",
          "original_form": "네  저도 퇴근할때 비가 그쳤어면좋겠어요",
          "speaker_id": "1",
          "time": "20211108 10:00"
        },
        {
          "id": "MDRW2100046870.1.20",
          "form": "이따 퇴근 조심히 하시길!",
          "original_form": "이따 퇴근 조심히 하시길!",
          "speaker_id": "2",
          "time": "20211108 10:00"
        }
      ]
