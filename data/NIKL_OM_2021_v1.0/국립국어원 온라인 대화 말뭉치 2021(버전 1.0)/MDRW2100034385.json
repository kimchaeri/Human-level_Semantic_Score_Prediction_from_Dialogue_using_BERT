        },
        {
          "id": "MDRW2100034385.1.2",
          "form": "투피엠이 재결합했어",
          "original_form": "투피엠이 재결합했어",
          "speaker_id": "1",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100034385.1.3",
          "form": "컴백했지 오랜만에",
          "original_form": "컴백했지 오랜만에",
          "speaker_id": "2",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100034385.1.4",
          "form": "나 투피엠 진짜 좋아했었는데",
          "original_form": "나 투피엠 진짜 좋아했었는데",
          "speaker_id": "1",
          "time": "20210828 02:39"
        },
        {
          "id": "MDRW2100034385.1.5",
          "form": "투피엠 우리 집은 레전드",
          "original_form": "투피엠 우리 집은 레전드",
          "speaker_id": "2",
          "time": "20210828 02:39"
        },
        {
          "id": "MDRW2100034385.1.6",
          "form": "니콘 얼굴도 레전드",
          "original_form": "니콘 얼굴도 레전드",
          "speaker_id": "1",
          "time": "20210828 02:39"
        },
        {
          "id": "MDRW2100034385.1.7",
          "form": "닉쿤",
          "original_form": "닉쿤",
          "speaker_id": "1",
          "time": "20210828 02:39"
        },
        {
          "id": "MDRW2100034385.1.8",
          "form": "우리 집은 준호인 거 모르니",
          "original_form": "우리 집은 준호인 거 모르니",
          "speaker_id": "2",
          "time": "20210828 02:39"
        },
        {
          "id": "MDRW2100034385.1.9",
          "form": "이번 노래도 좋더라",
          "original_form": "이번 노래도 좋더라",
          "speaker_id": "1",
          "time": "20210828 02:39"
        },
        {
          "id": "MDRW2100034385.1.10",
          "form": "이번 노래 제목 해야 해 너무 좋아",
          "original_form": "이번 노래 제목 해야 해 너무 좋아",
          "speaker_id": "2",
          "time": "20210828 02:39"
        },
        {
          "id": "MDRW2100034385.1.11",
          "form": "그거 멤버가 자꾸 간 거야",
          "original_form": "그거 멤버가 자꾸 간 거야",
          "speaker_id": "1",
          "time": "20210828 02:39"
        },
        {
          "id": "MDRW2100034385.1.12",
          "form": "그거 멤버가 작곡한 거야",
          "original_form": "그거 멤버가 작곡한 거야",
          "speaker_id": "1",
          "time": "20210828 02:39"
        },
        {
          "id": "MDRW2100034385.1.13",
          "form": "와 어떤 멤버가 작곡거래",
          "original_form": "와 어떤 멤버가 작곡거래",
          "speaker_id": "2",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100034385.1.14",
          "form": "우영이라고 노래를 잘하는 사람이야",
          "original_form": "우영이라고 노래를 잘하는 사람이야",
          "speaker_id": "1",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100034385.1.15",
          "form": "투피엠은 키가 커서 멋있는 거 같아",
          "original_form": "투피엠은 키가 커서 멋있는 거 같아",
