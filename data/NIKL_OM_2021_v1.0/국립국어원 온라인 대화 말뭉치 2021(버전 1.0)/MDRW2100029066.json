      "utterance": [
        {
          "id": "MDRW2100029066.1.1",
          "form": "아침 드시나요?",
          "original_form": "아침 드시나요?",
          "speaker_id": "2",
          "time": "20210903 08:34"
        },
        {
          "id": "MDRW2100029066.1.2",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:35"
        },
        {
          "id": "MDRW2100029066.1.3",
          "form": "챙겨먹을때는 있지만 일정있을땐 꼭먹어요",
          "original_form": "챙겨먹을때는 있지만 일정있을땐 꼭먹어요",
          "speaker_id": "1",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029066.1.4",
          "form": "꼬르륵 소리가 엄청 잘나가지구요 ㅎㅎ",
          "original_form": "꼬르륵 소리가 엄청 잘나가지구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029066.1.5",
          "form": "요즘은 쇼핑도 인터넷으로만 하는것같아요",
          "original_form": "요즘은 쇼핑도 인터넷으로만 하는것같아요",
          "speaker_id": "1",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029066.1.6",
          "form": "직접가시나요 인터넷을 이용하시나요?,",
          "original_form": "직접가시나요 인터넷을 이용하시나요?,",
          "speaker_id": "1",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029066.1.7",
          "form": "원래는 퇴근길에 환승역에 있는 마트에서 필요한 거 후닥 사곤 했는데요",
          "original_form": "원래는 퇴근길에 환승역에 있는 마트에서 필요한 거 후닥 사곤 했는데요",
          "speaker_id": "2",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100029066.1.8",
          "form": "코로나땜에 마트가 9시에 닫으니까 그러질 못해서",
          "original_form": "코로나땜에 마트가 9시에 닫으니까 그러질 못해서",
          "speaker_id": "2",
          "time": "20210903 10:38"
        },
        {
          "id": "MDRW2100029066.1.9",
          "form": "이젠 거의 다 인터넷이에요",
          "original_form": "이젠 거의 다 인터넷이에요",
          "speaker_id": "2",
          "time": "20210903 10:38"
        },
        {
          "id": "MDRW2100029066.1.10",
          "form": "그렇죠 빨리 닫기도 하가",
          "original_form": "그렇죠 빨리 닫기도 하가",
          "speaker_id": "1",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100029066.1.11",
          "form": "저도 거의 인터넷인것같아요 ㅎㅎ",
          "original_form": "저도 거의 인터넷인것같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100029066.1.12",
          "form": "뭔가 신선식품은 직접 보고 사고 싶긴한데",
          "original_form": "뭔가 신선식품은 직접 보고 사고 싶긴한데",
          "speaker_id": "2",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029066.1.13",
          "form": "아직까지는 제법 상태 괜찮은 애들이 배송되서 일단 만족하고 있습니다 ㅎㅎ",
          "original_form": "아직까지는 제법 상태 괜찮은 애들이 배송되서 일단 만족하고 있습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100029066.1.14",
          "form": "맞아요 음식같은거는 그렇죠",
          "original_form": "맞아요 음식같은거는 그렇죠",
          "speaker_id": "1",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100029066.1.15",
          "form": "다행이네요!!",
          "original_form": "다행이네요!!",
          "speaker_id": "1",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100029066.1.16",
          "form": "좀 시들한거 오면 너무 속삼하죠",
          "original_form": "좀 시들한거 오면 너무 속삼하죠",
          "speaker_id": "1",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100029066.1.17",
          "form": "주로 뭐사세요??",
          "original_form": "주로 뭐사세요??",
          "speaker_id": "1",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100029066.1.18",
          "form": "주로 생필품이죠",
          "original_form": "주로 생필품이죠",
          "speaker_id": "2",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100029066.1.19",
          "form": "최근에는 계란이랑 양배추, 양파, 파, 다진마늘, 콩나물 두부",
          "original_form": "최근에는 계란이랑 양배추, 양파, 파, 다진마늘, 콩나물 두부",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100029066.1.20",
          "form": "뭐 그런거 샀네요",
          "original_form": "뭐 그런거 샀네요",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100029066.1.21",
          "form": "계란은 처음 배달시켰는데",
          "original_form": "계란은 처음 배달시켰는데",
          "speaker_id": "2",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100029066.1.22",
          "form": "하나도 안깨지고 와서 놀람요",
          "original_form": "하나도 안깨지고 와서 놀람요",
          "speaker_id": "2",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100029066.1.23",
          "form": "그러시군요 계란 배달은 해본적없는데 좋네요!!",
          "original_form": "그러시군요 계란 배달은 해본적없는데 좋네요!!",
          "speaker_id": "1",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100029066.1.24",
          "form": "요즘 올리브영도 할인하던데 화장품은 주로 어디서 사시나요?",
          "original_form": "요즘 올리브영도 할인하던데 화장품은 주로 어디서 사시나요?",
          "speaker_id": "1",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029066.1.25",
          "form": "솔직히 시키면서도 다 깨져서 오면 어떡하지? 했거든요",
          "original_form": "솔직히 시키면서도 다 깨져서 오면 어떡하지? 했거든요",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029066.1.26",
          "form": "올영이요",
          "original_form": "올영이요",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029066.1.27",
          "form": "이번에 세일해서",
          "original_form": "이번에 세일해서",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029066.1.28",
          "form": "스킨이랑 퍼프 살거에요",
          "original_form": "스킨이랑 퍼프 살거에요",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029066.1.29",
          "form": "전 그 온천수미스트를 스킨으로 써요",
          "original_form": "전 그 온천수미스트를 스킨으로 써요",
          "speaker_id": "2",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100029066.1.30",
          "form": "색조는 거의 안하고요",
          "original_form": "색조는 거의 안하고요",
          "speaker_id": "2",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100029066.1.31",
          "form": "저두그럴것같아요 ㅎㅎ 계란 다 깨지면 너무 속상하잖아요 ㅠㅠ",
          "original_form": "저두그럴것같아요 ㅎㅎ 계란 다 깨지면 너무 속상하잖아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029066.1.32",
          "form": "그렇군요 저도 색조안해요 ㅎㅎ",
          "original_form": "그렇군요 저도 색조안해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029066.1.33",
          "form": "온천수미스트는 처음들어봤어요!",
          "original_form": "온천수미스트는 처음들어봤어요!",
          "speaker_id": "1",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029066.1.34",
          "form": "어떤가요?,",
          "original_form": "어떤가요?,",
          "speaker_id": "1",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029066.1.35",
          "form": "그냥 물같은거긴 한데",
          "original_form": "그냥 물같은거긴 한데",
          "speaker_id": "2",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100029066.1.36",
          "form": "로션은 따로 바르니까요",
          "original_form": "로션은 따로 바르니까요",
          "speaker_id": "2",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100029066.1.37",
          "form": "얼굴에 있는 찌꺼기? 같은건 잘 닦아줘요",
          "original_form": "얼굴에 있는 찌꺼기? 같은건 잘 닦아줘요",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029066.1.38",
          "form": "근데 보습 영양 그런 기능은 없죠",
          "original_form": "근데 보습 영양 그런 기능은 없죠",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029066.1.39",
          "form": "아하 그렇군요 좋네요!!",
          "original_form": "아하 그렇군요 좋네요!!",
          "speaker_id": "1",
          "time": "20210903 12:20"
        },
        {
          "id": "MDRW2100029066.1.40",
          "form": "전 할인 할때 다 사놓고 거의 안쓰는것같아요",
          "original_form": "전 할인 할때 다 사놓고 거의 안쓰는것같아요",
          "speaker_id": "1",
          "time": "20210903 12:20"
        },
        {
          "id": "MDRW2100029066.1.41",
          "form": "몇달전 패드를 샀는데 그것도 좋더라구요",
          "original_form": "몇달전 패드를 샀는데 그것도 좋더라구요",
          "speaker_id": "1",
          "time": "20210903 12:20"
        },
        {
          "id": "MDRW2100029066.1.42",
          "form": "닦토패드요?",
          "original_form": "닦토패드요?",
          "speaker_id": "2",
          "time": "20210903 12:28"
        },
        {
          "id": "MDRW2100029066.1.43",
          "form": "그거 편하긴 해요",
          "original_form": "그거 편하긴 해요",
          "speaker_id": "2",
          "time": "20210903 12:29"
        },
        {
          "id": "MDRW2100029066.1.44",
          "form": "어머니가 병원에 입원하셨을때 아주 유용했어요",
          "original_form": "어머니가 병원에 입원하셨을때 아주 유용했어요",
          "speaker_id": "2",
          "time": "20210903 12:29"
        }
      ]
