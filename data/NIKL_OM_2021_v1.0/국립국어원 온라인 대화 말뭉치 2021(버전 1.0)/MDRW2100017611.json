      "utterance": [
        {
          "id": "MDRW2100017611.1.1",
          "form": "나도 그래요",
          "original_form": "나도 그래요",
          "speaker_id": "2",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100017611.1.2",
          "form": "원래도 운동 안 했는데",
          "original_form": "원래도 운동 안 했는데",
          "speaker_id": "1",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100017611.1.3",
          "form": "헬스장 가기 어려우니 그 핑계로 더 안 가게 돼요",
          "original_form": "헬스장 가기 어려우니 그 핑계로 더 안 가게 돼요",
          "speaker_id": "1",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100017611.1.4",
          "form": "나도 원래 산에 한번씩 갔었는데\r\n요즘은 그냥 마스크쓰고 집 앞 산책만 하고있어요",
          "original_form": "나도 원래 산에 한번씩 갔었는데\r\n요즘은 그냥 마스크쓰고 집 앞 산책만 하고있어요",
          "speaker_id": "2",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100017611.1.5",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017611.1.6",
          "form": "참 답답한 시국이에요",
          "original_form": "참 답답한 시국이에요",
          "speaker_id": "2",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017611.1.7",
          "form": "저는 집에 매트랑 폼롤러 사놓았는데",
          "original_form": "저는 집에 매트랑 폼롤러 사놓았는데",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017611.1.8",
          "form": "요가매트",
          "original_form": "요가매트",
          "speaker_id": "2",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017611.1.9",
          "form": "네 요가 매트",
          "original_form": "네 요가 매트",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017611.1.10",
          "form": "막상 별로 안 쓰고 있어요",
          "original_form": "막상 별로 안 쓰고 있어요",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017611.1.11",
          "form": "폼롤러로 마사지 하는게 건강에 좋다고 들었어요",
          "original_form": "폼롤러로 마사지 하는게 건강에 좋다고 들었어요",
          "speaker_id": "2",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017611.1.12",
          "form": "저도 글케 들었는데",
          "original_form": "저도 글케 들었는데",
          "speaker_id": "1",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017611.1.13",
          "form": "귀찮아서 안하고 모셔두고 잇어요ㅜㅜ",
          "original_form": "귀찮아서 안하고 모셔두고 잇어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017611.1.14",
          "form": "조금씩 꾸준히 해봐요",
          "original_form": "조금씩 꾸준히 해봐요",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017611.1.15",
          "form": "말 나온김에 해야겟네요ㅛ",
          "original_form": "말 나온김에 해야겟네요ㅛ",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017611.1.16",
          "form": "티비볼때 하고 자기전에하고",
          "original_form": "티비볼때 하고 자기전에하고",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017611.1.17",
          "form": "이렇게 시간을 정해두면 좋다고 해요",
          "original_form": "이렇게 시간을 정해두면 좋다고 해요",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017611.1.18",
          "form": "조언 감사합니다",
          "original_form": "조언 감사합니다",
