      "utterance": [
        {
          "id": "MDRW2100012231.1.1",
          "form": "게임 좋아하세요??",
          "original_form": "게임 좋아하세요??",
          "speaker_id": "1",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100012231.1.2",
          "form": "가끔 해요!",
          "original_form": "가끔 해요!",
          "speaker_id": "2",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100012231.1.3",
          "form": "보드게임 같은 것도 좋아하고",
          "original_form": "보드게임 같은 것도 좋아하고",
          "speaker_id": "2",
          "time": "20210824 20:43"
        },
        {
          "id": "MDRW2100012231.1.4",
          "form": "게임 좋아하시나요?",
          "original_form": "게임 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210824 20:43"
        },
        {
          "id": "MDRW2100012231.1.5",
          "form": "전 게임 좋아해요ㅎㅎ",
          "original_form": "전 게임 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:43"
        },
        {
          "id": "MDRW2100012231.1.6",
          "form": "요즘은 핸드폰 게임ㅋㅋㅋ",
          "original_form": "요즘은 핸드폰 게임ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:43"
        },
        {
          "id": "MDRW2100012231.1.7",
          "form": "옛날에 심즈에 심취해서 밤새서 했었어요ㅋㅋㅋ",
          "original_form": "옛날에 심즈에 심취해서 밤새서 했었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:44"
        },
        {
          "id": "MDRW2100012231.1.8",
          "form": "심즈 재밌죠! 건축은 영 소질이 없어서 캐릭터만 많이 만들다가 말았지만 꽤 재밌게 했어요!",
          "original_form": "심즈 재밌죠! 건축은 영 소질이 없어서 캐릭터만 많이 만들다가 말았지만 꽤 재밌게 했어요!",
          "speaker_id": "2",
          "time": "20210824 20:45"
        },
        {
          "id": "MDRW2100012231.1.9",
          "form": "핸드폰 게임 어떤 거 하세요?",
          "original_form": "핸드폰 게임 어떤 거 하세요?",
          "speaker_id": "2",
          "time": "20210824 20:45"
        },
        {
          "id": "MDRW2100012231.1.10",
          "form": "요즘엔 머지게임해요. 합병게임?",
          "original_form": "요즘엔 머지게임해요. 합병게임?",
          "speaker_id": "1",
          "time": "20210824 20:46"
        },
        {
          "id": "MDRW2100012231.1.11",
          "form": "그리고 쿠키덤!",
          "original_form": "그리고 쿠키덤!",
          "speaker_id": "1",
          "time": "20210824 20:47"
        },
        {
          "id": "MDRW2100012231.1.12",
          "form": "쿠킹덤! 컨텐츠도 많고 재밌더라고요!",
          "original_form": "쿠킹덤! 컨텐츠도 많고 재밌더라고요!",
          "speaker_id": "2",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100012231.1.13",
          "form": "해보셨어요??ㅎㅎ",
          "original_form": "해보셨어요??ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100012231.1.14",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100012231.1.15",
          "form": "보상을 많이 줘서",
          "original_form": "보상을 많이 줘서",
          "speaker_id": "2",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100012231.1.16",
          "form": "가챠 돌리는 재미가 있었어요ㅋㅋㅋㅋ",
          "original_form": "가챠 돌리는 재미가 있었어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100012231.1.17",
          "form": "쿠키들도 귀엽고",
          "original_form": "쿠키들도 귀엽고",
          "speaker_id": "2",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100012231.1.18",
          "form": "처음엔 친구 추천으로 했는데 지금 친구는 안하고 저만해요ㅎㅎ",
          "original_form": "처음엔 친구 추천으로 했는데 지금 친구는 안하고 저만해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100012231.1.19",
          "form": "가챠ㅋㅋㅋㅋ",
          "original_form": "가챠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100012231.1.20",
          "form": "쿠키들 귀엽죠~:)",
          "original_form": "쿠키들 귀엽죠~:)",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100012231.1.21",
          "form": "어떤 게임 종류 좋아하세요??",
          "original_form": "어떤 게임 종류 좋아하세요??",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100012231.1.22",
          "form": "PC는 생존 게임 좋아해요~ 모바일은 추리나 퍼즐 게임도 즐겨했던 거 같아요~",
          "original_form": "PC는 생존 게임 좋아해요~ 모바일은 추리나 퍼즐 게임도 즐겨했던 거 같아요~",
          "speaker_id": "2",
          "time": "20210824 20:51"
        },
        {
          "id": "MDRW2100012231.1.23",
          "form": "생존게임!! 추리나 퍼즐류도 재밌죠~",
          "original_form": "생존게임!! 추리나 퍼즐류도 재밌죠~",
          "speaker_id": "1",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100012231.1.24",
          "form": "전 좀비물은 너무 싫어서 그런거 빼곤 왠만한건 해보는 것 같아요ㅎㅎ",
          "original_form": "전 좀비물은 너무 싫어서 그런거 빼곤 왠만한건 해보는 것 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100012231.1.25",
          "form": "좀비물은 저도 너무 무서워서 못 하겠더라고요ㅋㅋㅋ",
          "original_form": "좀비물은 저도 너무 무서워서 못 하겠더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100012231.1.26",
          "form": "3d 게임도 해보셨나요? 저는 멀미나서 잘 못하겠더라고요",
          "original_form": "3d 게임도 해보셨나요? 저는 멀미나서 잘 못하겠더라고요",
          "speaker_id": "2",
          "time": "20210824 20:55"
        },
        {
          "id": "MDRW2100012231.1.27",
          "form": "어..하는 곳을 가보긴 했는데 그 땐 임신 중이라 직접 하지는 못하고 보기만 했었어요.",
          "original_form": "어..하는 곳을 가보긴 했는데 그 땐 임신 중이라 직접 하지는 못하고 보기만 했었어요.",
          "speaker_id": "1",
          "time": "20210824 20:55"
        },
        {
          "id": "MDRW2100012231.1.28",
          "form": "재밌을것 같더라구요!",
          "original_form": "재밌을것 같더라구요!",
          "speaker_id": "1",
          "time": "20210824 20:55"
        }
      ]
    }
