      "utterance": [
        {
          "id": "MDRW2100041952.1.1",
          "form": "name2님오늘 옷차림은 어땠나요?",
          "original_form": "&name2&님오늘 옷차림은 어땠나요?",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041952.1.2",
          "form": "반팔에 자켓 하나 걸쳤어요",
          "original_form": "반팔에 자켓 하나 걸쳤어요",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041952.1.3",
          "form": "비오니까 좀 춥네요ㅎ",
          "original_form": "비오니까 좀 춥네요ㅎ",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041952.1.4",
          "form": "저는 긴팔 긴바지 입었어요ㅋㅋㅋㅋ",
          "original_form": "저는 긴팔 긴바지 입었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041952.1.5",
          "form": "약간 오바같긴한데 안입으면 감기걸릴 것 같아서 후딱 입었어요",
          "original_form": "약간 오바같긴한데 안입으면 감기걸릴 것 같아서 후딱 입었어요",
          "speaker_id": "1",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041952.1.6",
          "form": "감기걸리면 안되져. 요샌 기침도 눈치보여서ㅋㅋ",
          "original_form": "감기걸리면 안되져. 요샌 기침도 눈치보여서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041952.1.7",
          "form": "하 맞아요 재채기했는데 다 쳐다보더라구요 근데 그럴만해요",
          "original_form": "하 맞아요 재채기했는데 다 쳐다보더라구요 근데 그럴만해요",
          "speaker_id": "1",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100041952.1.8",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100041952.1.9",
          "form": "ㅋㅋㅋ내일도 비온다니까 잘 챙겨입어야겠어요",
          "original_form": "ㅋㅋㅋ내일도 비온다니까 잘 챙겨입어야겠어요",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100041952.1.10",
          "form": "헐 진짜요?몰랐는데 알려주셔서 감사해요ㅋㅋㅋ",
          "original_form": "헐 진짜요?몰랐는데 알려주셔서 감사해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100041952.1.11",
          "form": "내일도 반팔에 자켓인가요?",
          "original_form": "내일도 반팔에 자켓인가요?",
          "speaker_id": "1",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100041952.1.12",
          "form": "그럴거 같아요.",
          "original_form": "그럴거 같아요.",
          "speaker_id": "2",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100041952.1.13",
          "form": "낮엔 좀 덥더라구요",
          "original_form": "낮엔 좀 덥더라구요",
          "speaker_id": "2",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100041952.1.14",
          "form": "아 진짜요? 낮엔 밖에 잘 안나가서 몰랐어요 그럼 반팔 입어야죠 맞죠 맞죠",
          "original_form": "아 진짜요? 낮엔 밖에 잘 안나가서 몰랐어요 그럼 반팔 입어야죠 맞죠 맞죠",
          "speaker_id": "1",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100041952.1.15",
          "form": "올해 가을에 비가 자주 오는 것 같아요",
          "original_form": "올해 가을에 비가 자주 오는 것 같아요",
          "speaker_id": "1",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100041952.1.16",
          "form": "가을 오자마자 계속 비내려서 그런거 같네요",
          "original_form": "가을 오자마자 계속 비내려서 그런거 같네요",
          "speaker_id": "2",
          "time": "20210906 19:15"
        },
        {
          "id": "MDRW2100041952.1.17",
          "form": "그쳐도 가을날씨 답진 않고요",
          "original_form": "그쳐도 가을날씨 답진 않고요",
          "speaker_id": "2",
          "time": "20210906 19:15"
        },
        {
          "id": "MDRW2100041952.1.18",
          "form": "맞아요 가을도 금방 지나갈텐데 계속 비가 오니까 속상해요",
          "original_form": "맞아요 가을도 금방 지나갈텐데 계속 비가 오니까 속상해요",
          "speaker_id": "1",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100041952.1.19",
          "form": "이번주엔 좀 덜 더웠으면 좋겠네요",
          "original_form": "이번주엔 좀 덜 더웠으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210906 19:30"
        },
        {
          "id": "MDRW2100041952.1.20",
          "form": "비 그치면 하늘은 이쁘더라구요",
          "original_form": "비 그치면 하늘은 이쁘더라구요",
          "speaker_id": "2",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100041952.1.21",
          "form": "맞아요 여느때처럼 가을하늘이죠",
          "original_form": "맞아요 여느때처럼 가을하늘이죠",
          "speaker_id": "1",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100041952.1.22",
