      "utterance": [
        {
          "id": "MDRW2100038537.1.1",
          "form": "점심드셨나요",
          "original_form": "점심드셨나요",
          "speaker_id": "1",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100038537.1.2",
          "form": "네 먹었어요ㅎㅎ",
          "original_form": "네 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100038537.1.3",
          "form": "머드셧어요",
          "original_form": "머드셧어요",
          "speaker_id": "1",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100038537.1.4",
          "form": "김치볶음밥에 반숙계란 올려진거 먹었어요",
          "original_form": "김치볶음밥에 반숙계란 올려진거 먹었어요",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100038537.1.5",
          "form": "반숙계란 ! 직접 삶아서용 ?",
          "original_form": "반숙계란 ! 직접 삶아서용 ?",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100038537.1.6",
          "form": "아 반숙 계란 후라이요ㅋㅋ",
          "original_form": "아 반숙 계란 후라이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100038537.1.7",
          "form": "아하! 맛있었겟네용 볶음밥 직접 ?",
          "original_form": "아하! 맛있었겟네용 볶음밥 직접 ?",
          "speaker_id": "1",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100038537.1.8",
          "form": "냉동 ?",
          "original_form": "냉동 ?",
          "speaker_id": "1",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100038537.1.9",
          "form": "식당꺼 포장했어요",
          "original_form": "식당꺼 포장했어요",
          "speaker_id": "2",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100038537.1.10",
          "form": "계란후라이 반숙 잘드세요?",
          "original_form": "계란후라이 반숙 잘드세요?",
          "speaker_id": "2",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100038537.1.11",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "1",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100038537.1.12",
          "form": "네내 그럼요",
          "original_form": "네내 그럼요",
          "speaker_id": "1",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100038537.1.13",
          "form": "조아합니다..",
          "original_form": "조아합니다..",
          "speaker_id": "1",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100038537.1.14",
          "form": "간식음 안드셨나요",
          "original_form": "간식음 안드셨나요",
          "speaker_id": "1",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100038537.1.15",
          "form": "간식은 원래 잘 안먹어요ㅎ",
          "original_form": "간식은 원래 잘 안먹어요ㅎ",
          "speaker_id": "2",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100038537.1.16",
          "form": "name1님은 뭐 드셨나요?",
          "original_form": "&name1&님은 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100038537.1.17",
          "form": "저요 쭈꾸미용",
          "original_form": "저요 쭈꾸미용",
          "speaker_id": "1",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100038537.1.18",
          "form": "냉동 ㅎㅎ",
          "original_form": "냉동 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100038537.1.19",
          "form": "하남쭈꾸미 맛잇",
          "original_form": "하남쭈꾸미 맛잇",
          "speaker_id": "1",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100038537.1.20",
          "form": "하남 쭈꾸미 많이 들어봤어요",
          "original_form": "하남 쭈꾸미 많이 들어봤어요",
          "speaker_id": "2",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100038537.1.21",
          "form": "많이들 사먹던데",
          "original_form": "많이들 사먹던데",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038537.1.22",
          "form": "넹 저 짱조아함..",
          "original_form": "넹 저 짱조아함..",
          "speaker_id": "1",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038537.1.23",
          "form": "매운거드세요?",
          "original_form": "매운거드세요?",
          "speaker_id": "1",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038537.1.24",
          "form": "하남 쭈꾸미가 다른 쭈꾸미에 비해 특별한게 있나요?",
          "original_form": "하남 쭈꾸미가 다른 쭈꾸미에 비해 특별한게 있나요?",
