      "utterance": [
        {
          "id": "MDRW2100023585.1.1",
          "form": "난 짬뽕 먹으려고 ㅎㅎ",
          "original_form": "난 짬뽕 먹으려고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 11:55"
        },
        {
          "id": "MDRW2100023585.1.2",
          "form": "밀가루 말고 밥먹지",
          "original_form": "밀가루 말고 밥먹지",
          "speaker_id": "1",
          "time": "20210830 11:55"
        },
        {
          "id": "MDRW2100023585.1.3",
          "form": "날씨가 우중충하니 매콤한 게 먹고 싶네",
          "original_form": "날씨가 우중충하니 매콤한 게 먹고 싶네",
          "speaker_id": "2",
          "time": "20210830 11:55"
        },
        {
          "id": "MDRW2100023585.1.4",
          "form": "회사 근처에 맛집있어?",
          "original_form": "회사 근처에 맛집있어?",
          "speaker_id": "1",
          "time": "20210830 11:56"
        },
        {
          "id": "MDRW2100023585.1.5",
          "form": "맛집은 아닌데 그냥저냥 먹을만해",
          "original_form": "맛집은 아닌데 그냥저냥 먹을만해",
          "speaker_id": "2",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100023585.1.6",
          "form": "ㅎㅎ 맛있겠네",
          "original_form": "ㅎㅎ 맛있겠네",
          "speaker_id": "1",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100023585.1.7",
          "form": "여기 나름 해물도 많이 들어있고 맛이 괜찮아",
          "original_form": "여기 나름 해물도 많이 들어있고 맛이 괜찮아",
          "speaker_id": "2",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100023585.1.8",
          "form": "오래된 곳이야?",
          "original_form": "오래된 곳이야?",
          "speaker_id": "1",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100023585.1.9",
          "form": "ㅇㅇ 10년도 넘은 것 같은데",
          "original_form": "ㅇㅇ 10년도 넘은 것 같은데",
          "speaker_id": "2",
          "time": "20210830 12:01"
        },
        {
          "id": "MDRW2100023585.1.10",
          "form": "와.. 오래됐네",
          "original_form": "와.. 오래됐네",
          "speaker_id": "1",
          "time": "20210830 12:01"
        },
        {
          "id": "MDRW2100023585.1.11",
          "form": "손님도 꽤 많아~ 점심 시간에 가면 자리 별로 없더라고",
          "original_form": "손님도 꽤 많아~ 점심 시간에 가면 자리 별로 없더라고",
          "speaker_id": "2",
          "time": "20210830 12:01"
        },
        {
          "id": "MDRW2100023585.1.12",
          "form": "ㅇㅇ 얼른 가서 맛있게 먹어 ㅎㅎ",
          "original_form": "ㅇㅇ 얼른 가서 맛있게 먹어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 12:01"
        },
        {
          "id": "MDRW2100023585.1.13",
          "form": "알겠어~ 색시도 즐점해",
          "original_form": "알겠어~ 색시도 즐점해",
          "speaker_id": "2",
          "time": "20210830 12:01"
        },
        {
          "id": "MDRW2100023585.1.14",
          "form": "점심은 먹었어?",
          "original_form": "점심은 먹었어?",
          "speaker_id": "1",
          "time": "20210906 12:17"
        }
