          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100039807.1.19",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100039807.1.20",
          "form": "저도2주",
          "original_form": "저도2주",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100039807.1.21",
