      "utterance": [
        {
          "id": "MDRW2100039807.1.1",
          "form": "ㅋㅋㅋㄱㄱㅋㅋ 아 추파춥스넘잘샀어",
          "original_form": "ㅋㅋㅋㄱㄱㅋㅋ 아 추파춥스넘잘샀어",
          "speaker_id": "1",
