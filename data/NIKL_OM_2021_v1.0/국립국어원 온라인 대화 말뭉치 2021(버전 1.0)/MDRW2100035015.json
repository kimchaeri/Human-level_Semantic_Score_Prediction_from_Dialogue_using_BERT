      "utterance": [
        {
          "id": "MDRW2100035015.1.1",
          "form": "여행은 언제쯤 갈 수 있을까요?ㅠ",
          "original_form": "여행은 언제쯤 갈 수 있을까요?ㅠ",
          "speaker_id": "2",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100035015.1.2",
          "form": "올해는 안될것같고 내년쯤 갈까요?",
          "original_form": "올해는 안될것같고 내년쯤 갈까요?",
          "speaker_id": "1",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100035015.1.3",
          "form": "가능하면 좋겠어요",
          "original_form": "가능하면 좋겠어요",
          "speaker_id": "2",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100035015.1.4",
          "form": "여행 자주 다니셨어요?",
          "original_form": "여행 자주 다니셨어요?",
          "speaker_id": "2",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100035015.1.5",
          "form": "처녀땐 자주 다녔는데",
          "original_form": "처녀땐 자주 다녔는데",
          "speaker_id": "1",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100035015.1.6",
          "form": "지금은 잘못다니네요ㅠ",
          "original_form": "지금은 잘못다니네요ㅠ",
          "speaker_id": "1",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100035015.1.7",
          "form": "아이있으면 자주 못간다하시더라구요ㅠ",
          "original_form": "아이있으면 자주 못간다하시더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100035015.1.8",
          "form": "가고싶은곳 있으세요?",
          "original_form": "가고싶은곳 있으세요?",
          "speaker_id": "2",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100035015.1.9",
          "form": "전 싱가포르요",
          "original_form": "전 싱가포르요",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100035015.1.10",
          "form": "좋다고해서ㅎ",
          "original_form": "좋다고해서ㅎ",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100035015.1.11",
          "form": "우와ㅎㅎ 어떤게 좋대요??",
          "original_form": "우와ㅎㅎ 어떤게 좋대요??",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100035015.1.12",
          "form": "네 깨끗하고 안전하고",
          "original_form": "네 깨끗하고 안전하고",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100035015.1.13",
          "form": "쇼핑도좋고ㅎ",
          "original_form": "쇼핑도좋고ㅎ",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100035015.1.14",
          "form": "오오.. 깨끗 안전 쇼핑!!!! 진짜 좋네요",
          "original_form": "오오.. 깨끗 안전 쇼핑!!!! 진짜 좋네요",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100035015.1.15",
          "form": "저도 하나 적어둬야겠어요ㅎㅎ",
          "original_form": "저도 하나 적어둬야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100035015.1.16",
          "form": "네 저도요ㅋ",
          "original_form": "네 저도요ㅋ",
          "speaker_id": "1",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100035015.1.17",
          "form": "저는 해외는 일본밖에 못가봤어요ㅜ",
          "original_form": "저는 해외는 일본밖에 못가봤어요ㅜ",
          "speaker_id": "2",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100035015.1.18",
          "form": "앗 일본도 좋더라고요",
          "original_form": "앗 일본도 좋더라고요",
          "speaker_id": "1",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100035015.1.19",
          "form": "좋긴 좋았어요!",
          "original_form": "좋긴 좋았어요!",
          "speaker_id": "2",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100035015.1.20",
          "form": "봄에 가보고싶었는데.. 코로나에 봄이 늦춰졌어요ㅜ",
          "original_form": "봄에 가보고싶었는데.. 코로나에 봄이 늦춰졌어요ㅜ",
          "speaker_id": "2",
          "time": "20210829 12:50"
        }
