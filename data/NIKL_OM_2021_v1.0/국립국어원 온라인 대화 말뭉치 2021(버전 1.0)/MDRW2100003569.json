      "utterance": [
        {
          "id": "MDRW2100003569.1.1",
          "form": "요즘 게임 어떤거 하세요?",
          "original_form": "요즘 게임 어떤거 하세요?",
          "speaker_id": "1",
          "time": "20210821 02:13"
        },
        {
          "id": "MDRW2100003569.1.2",
          "form": "게임은 잘 못해서 그냥 퍼즐 맞추기 이런거 심심할때해요",
          "original_form": "게임은 잘 못해서 그냥 퍼즐 맞추기 이런거 심심할때해요",
          "speaker_id": "2",
          "time": "20210821 02:20"
        },
        {
          "id": "MDRW2100003569.1.3",
          "form": "좋아하는  게임 있으신가요?",
          "original_form": "좋아하는  게임 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 02:20"
        },
        {
          "id": "MDRW2100003569.1.4",
          "form": "저두 게임은 잘 못해서",
          "original_form": "저두 게임은 잘 못해서",
          "speaker_id": "1",
          "time": "20210821 02:20"
        },
        {
          "id": "MDRW2100003569.1.5",
          "form": "요즘은 쿠키런 킹덤 하고있어요",
          "original_form": "요즘은 쿠키런 킹덤 하고있어요",
          "speaker_id": "1",
          "time": "20210821 02:20"
        },
        {
          "id": "MDRW2100003569.1.6",
          "form": "쿠키런 예전에 해보긴 했는데 아직도 있군요ㅎ",
          "original_form": "쿠키런 예전에 해보긴 했는데 아직도 있군요ㅎ",
          "speaker_id": "2",
          "time": "20210821 02:21"
        },
        {
          "id": "MDRW2100003569.1.7",
          "form": "예전에는 달리기 게임이었는데",
          "original_form": "예전에는 달리기 게임이었는데",
          "speaker_id": "1",
          "time": "20210821 02:21"
        },
        {
          "id": "MDRW2100003569.1.8",
          "form": "요즘은 집꾸미기 게임으로 변했더라구요 ㅋㅋ",
          "original_form": "요즘은 집꾸미기 게임으로 변했더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 02:21"
        },
        {
          "id": "MDRW2100003569.1.9",
          "form": "재밌어요!",
          "original_form": "재밌어요!",
          "speaker_id": "1",
          "time": "20210821 02:22"
        },
        {
          "id": "MDRW2100003569.1.10",
          "form": "아 달리기가 아니예요?ㅋ",
          "original_form": "아 달리기가 아니예요?ㅋ",
          "speaker_id": "2",
          "time": "20210821 02:22"
        },
        {
          "id": "MDRW2100003569.1.11",
          "form": "네네 ㅋㅋㅋ 킹덤이라구 쿠키런 오븐브레이크말고",
          "original_form": "네네 ㅋㅋㅋ 킹덤이라구 쿠키런 오븐브레이크말고",
          "speaker_id": "1",
          "time": "20210821 02:22"
        },
        {
          "id": "MDRW2100003569.1.12",
          "form": "새로운 게임이 나왔어요!",
          "original_form": "새로운 게임이 나왔어요!",
          "speaker_id": "1",
          "time": "20210821 02:22"
        },
        {
          "id": "MDRW2100003569.1.13",
          "form": "킹덤은 어떤종류게임인가요?",
          "original_form": "킹덤은 어떤종류게임인가요?",
          "speaker_id": "2",
          "time": "20210821 02:23"
        },
        {
          "id": "MDRW2100003569.1.14",
          "form": "킹덤은 집꾸미기..? 그런게임이에요 ㅋㅋ",
          "original_form": "킹덤은 집꾸미기..? 그런게임이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 02:23"
        },
        {
          "id": "MDRW2100003569.1.15",
          "form": "왕국 꾸미기 게임이 더 알맞는 말이겠네요",
          "original_form": "왕국 꾸미기 게임이 더 알맞는 말이겠네요",
          "speaker_id": "1",
          "time": "20210821 02:23"
        },
        {
          "id": "MDRW2100003569.1.16",
          "form": "아기자기하고 재밌겠어요ㅎ",
          "original_form": "아기자기하고 재밌겠어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 02:24"
        },
        {
          "id": "MDRW2100003569.1.17",
          "form": "네 근데 쿠키런 달리기때가 더 재미있었어요",
          "original_form": "네 근데 쿠키런 달리기때가 더 재미있었어요",
          "speaker_id": "1",
          "time": "20210821 02:24"
        },
        {
          "id": "MDRW2100003569.1.18",
          "form": "퍼즐맞추기도 심심할때 좋을거같아요",
          "original_form": "퍼즐맞추기도 심심할때 좋을거같아요",
          "speaker_id": "1",
          "time": "20210821 02:25"
        },
        {
          "id": "MDRW2100003569.1.19",
          "form": "퍼즐맞추면서 시간도 때우고 돈도벌고 ㅎ 일석이조예요",
          "original_form": "퍼즐맞추면서 시간도 때우고 돈도벌고 ㅎ 일석이조예요",
          "speaker_id": "2",
          "time": "20210821 02:25"
        },
        {
          "id": "MDRW2100003569.1.20",
          "form": "우와 대박 그런게임도 있어요?",
          "original_form": "우와 대박 그런게임도 있어요?",
          "speaker_id": "1",
          "time": "20210821 02:26"
        },
        {
          "id": "MDRW2100003569.1.21",
          "form": "신기하네여 ㅋㅋㅋ!!",
          "original_form": "신기하네여 ㅋㅋㅋ!!",
          "speaker_id": "1",
          "time": "20210821 02:26"
        },
        {
          "id": "MDRW2100003569.1.22",
          "form": "네 퍼즐맞추고 그걸 다른사람한테 넘겨서 돌고돌아 나한테오면 포인트를줘요",
          "original_form": "네 퍼즐맞추고 그걸 다른사람한테 넘겨서 돌고돌아 나한테오면 포인트를줘요",
          "speaker_id": "2",
          "time": "20210821 02:27"
        },
        {
          "id": "MDRW2100003569.1.23",
          "form": "우와 진짜 신기해요",
          "original_form": "우와 진짜 신기해요",
          "speaker_id": "1",
          "time": "20210821 02:28"
        },
        {
          "id": "MDRW2100003569.1.24",
          "form": "게임 이름이 뭔가요?",
          "original_form": "게임 이름이 뭔가요?",
          "speaker_id": "1",
          "time": "20210821 02:28"
        },
        {
          "id": "MDRW2100003569.1.25",
          "form": "플레이랑 이라고 검색해보세요",
          "original_form": "플레이랑 이라고 검색해보세요",
          "speaker_id": "2",
          "time": "20210821 02:28"
        },
        {
          "id": "MDRW2100003569.1.26",
          "form": "감사합니다 ! 조금있다 검색해볼게요 ㅋㅋ!!",
          "original_form": "감사합니다 ! 조금있다 검색해볼게요 ㅋㅋ!!",
          "speaker_id": "1",
          "time": "20210821 02:29"
        },
        {
          "id": "MDRW2100003569.1.27",
          "form": "첨시작할땐 좀복잡해보이고 시간대비 돈이 안돼요",
          "original_form": "첨시작할땐 좀복잡해보이고 시간대비 돈이 안돼요",
          "speaker_id": "2",
          "time": "20210821 02:30"
        },
        {
          "id": "MDRW2100003569.1.28",
          "form": "가성비는 좀떨어지는데 열심히하는만큼 그래도 돈쌓이더라구요 한때 미쳐서할땐 40일정도하고 5만원 출금했는데 요즘 지쳐서 하루몇백원ㅋ",
          "original_form": "가성비는 좀떨어지는데 열심히하는만큼 그래도 돈쌓이더라구요 한때 미쳐서할땐 40일정도하고 5만원 출금했는데 요즘 지쳐서 하루몇백원ㅋ",
          "speaker_id": "2",
          "time": "20210821 02:31"
        },
        {
          "id": "MDRW2100003569.1.29",
          "form": "우와 진짜 5만원일때 열심히 하셨나봐요!!",
          "original_form": "우와 진짜 5만원일때 열심히 하셨나봐요!!",
