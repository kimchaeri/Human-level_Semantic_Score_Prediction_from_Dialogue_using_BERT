      "utterance": [
        {
          "id": "MDRW2100031400.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031400.1.2",
          "form": "안녕하세요! 진짜 제주 사세요? 좋으시겠다",
          "original_form": "안녕하세요! 진짜 제주 사세요? 좋으시겠다",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031400.1.3",
          "form": "네에 3주전에 왔어요",
          "original_form": "네에 3주전에 왔어요",
          "speaker_id": "1",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031400.1.4",
          "form": "주부세요?",
          "original_form": "주부세요?",
          "speaker_id": "1",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031400.1.5",
          "form": "저는 아직 결혼을 안했어요 주부신가요?",
          "original_form": "저는 아직 결혼을 안했어요 주부신가요?",
          "speaker_id": "2",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100031400.1.6",
          "form": "저도 아직이요 지금은 혼자 사는데 이전에는 쉐어하우스 생활했어요",
          "original_form": "저도 아직이요 지금은 혼자 사는데 이전에는 쉐어하우스 생활했어요",
          "speaker_id": "1",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100031400.1.7",
          "form": "캬 대단하시네여",
          "original_form": "캬 대단하시네여",
          "speaker_id": "2",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100031400.1.8",
          "form": "쉐어하우스라니!!",
          "original_form": "쉐어하우스라니!!",
          "speaker_id": "2",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100031400.1.9",
          "form": "저 쉐어하우스 사는분이랑 처음 대화해요",
          "original_form": "저 쉐어하우스 사는분이랑 처음 대화해요",
          "speaker_id": "2",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100031400.1.10",
          "form": "지금은 제주와서 혼자 살고요",
          "original_form": "지금은 제주와서 혼자 살고요",
          "speaker_id": "1",
          "time": "20210904 21:27"
        },
        {
          "id": "MDRW2100031400.1.11",
          "form": "룸메이트 있음 재미있고 좋아요",
          "original_form": "룸메이트 있음 재미있고 좋아요",
          "speaker_id": "1",
          "time": "20210904 21:27"
        },
        {
          "id": "MDRW2100031400.1.12",
          "form": "캬 기숙사랑은 또 다르잖아요",
          "original_form": "캬 기숙사랑은 또 다르잖아요",
          "speaker_id": "2",
          "time": "20210904 21:28"
        },
        {
          "id": "MDRW2100031400.1.13",
          "form": "제주도민은 아니셨을거 같은데 어떻게 가셨어요?",
          "original_form": "제주도민은 아니셨을거 같은데 어떻게 가셨어요?",
          "speaker_id": "2",
          "time": "20210904 21:29"
        },
        {
          "id": "MDRW2100031400.1.14",
          "form": "아 여행다니니다 좋아서 얼마전에 내려왔어요",
          "original_form": "아 여행다니니다 좋아서 얼마전에 내려왔어요",
          "speaker_id": "1",
          "time": "20210905 11:14"
        },
        {
          "id": "MDRW2100031400.1.15",
          "form": "진짜 좋으시겠어요 편협한 생각이겠지만 바다 보이는 곳인가요?ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 좋으시겠어요 편협한 생각이겠지만 바다 보이는 곳인가요?ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:15"
        },
        {
          "id": "MDRW2100031400.1.16",
          "form": "저 멀리 보이고요",
          "original_form": "저 멀리 보이고요",
          "speaker_id": "1",
          "time": "20210905 15:18"
        },
        {
          "id": "MDRW2100031400.1.17",
          "form": "집 앞에 사라봉 보이네요",
          "original_form": "집 앞에 사라봉 보이네요",
          "speaker_id": "1",
          "time": "20210905 15:18"
        },
        {
          "id": "MDRW2100031400.1.18",
          "form": "바다 바로 앞은 넘 습해서요",
          "original_form": "바다 바로 앞은 넘 습해서요",
          "speaker_id": "1",
          "time": "20210905 15:18"
        },
        {
          "id": "MDRW2100031400.1.19",
          "form": "지금도 습해서 제습기 구입할까 하다가 가을넘어가면 괜찮다고 해서 참고 있어요",
          "original_form": "지금도 습해서 제습기 구입할까 하다가 가을넘어가면 괜찮다고 해서 참고 있어요",
          "speaker_id": "1",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100031400.1.20",
          "form": "캬 진짜 보이는 곳에서 사시네요",
          "original_form": "캬 진짜 보이는 곳에서 사시네요",
          "speaker_id": "2",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100031400.1.21",
          "form": "바다 안보이는 곳이 더 잘사는 사람 사는 곳이래요",
          "original_form": "바다 안보이는 곳이 더 잘사는 사람 사는 곳이래요",
          "speaker_id": "1",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100031400.1.22",
          "form": "이제 신선한 귤도 많이 드시겠어요",
          "original_form": "이제 신선한 귤도 많이 드시겠어요",
          "speaker_id": "2",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100031400.1.23",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100031400.1.24",
          "form": "ㅋㅋㅋㅋ 아 그렇군여",
          "original_form": "ㅋㅋㅋㅋ 아 그렇군여",
          "speaker_id": "2",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100031400.1.25",
          "form": "아 얼마전에 유기농 청귤사서 첨담았어요",
          "original_form": "아 얼마전에 유기농 청귤사서 첨담았어요",
          "speaker_id": "1",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100031400.1.26",
          "form": "심심해서 ㅋㅋㅋ",
          "original_form": "심심해서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100031400.1.27",
          "form": "역시 제주도는 다르구나!!",
          "original_form": "역시 제주도는 다르구나!!",
          "speaker_id": "2",
          "time": "20210905 15:20"
        }
      ]
    }
  ]
