      "utterance": [
        {
          "id": "MDRW2100025779.1.1",
          "form": "좋아하는 스포츠",
          "original_form": "좋아하는 스포츠",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100025779.1.2",
          "form": "있더요?",
          "original_form": "있더요?",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100025779.1.3",
          "form": "저는 딱히..즐겨보는건 없는데 올림픽은 재밌게 봤어요ㅎㅎ",
          "original_form": "저는 딱히..즐겨보는건 없는데 올림픽은 재밌게 봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100025779.1.4",
          "form": "어떤 종목이",
          "original_form": "어떤 종목이",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100025779.1.5",
          "form": "가장 재밌었나여ㅋㅋ",
          "original_form": "가장 재밌었나여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100025779.1.6",
          "form": "펜싱이 쫄깃하고 재밌더라구요!",
          "original_form": "펜싱이 쫄깃하고 재밌더라구요!",
          "speaker_id": "2",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100025779.1.7",
          "form": "펜싱 재밌었죠!",
          "original_form": "펜싱 재밌었죠!",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100025779.1.8",
          "form": "선수들도 넘 멋있었죠...!!",
          "original_form": "선수들도 넘 멋있었죠...!!",
          "speaker_id": "2",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100025779.1.9",
          "form": "그렇죠 ! 어벤져스",
          "original_form": "그렇죠 ! 어벤져스",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100025779.1.10",
          "form": "다들 훤칠하고 잘생기셨더라구요",
          "original_form": "다들 훤칠하고 잘생기셨더라구요",
          "speaker_id": "2",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100025779.1.11",
          "form": "ㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100025779.1.12",
          "form": "양궁도 보셨나요",
          "original_form": "양궁도 보셨나요",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100025779.1.13",
          "form": "아 네네 봤죠 제덕쿵야ㅋㅋ",
          "original_form": "아 네네 봤죠 제덕쿵야ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100025779.1.14",
          "form": "너무 귀여워요ㅜㅜ",
          "original_form": "너무 귀여워요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100025779.1.15",
          "form": "ㅠㅠ 맞아요",
          "original_form": "ㅠㅠ 맞아요",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100025779.1.16",
          "form": "사니사니안사니....",
          "original_form": "사니사니안사니....",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100025779.1.17",
          "form": "안산선수도 너무 멋져요...걸크러쉬",
          "original_form": "안산선수도 너무 멋져요...걸크러쉬",
          "speaker_id": "2",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100025779.1.18",
          "form": "다음 올림픽이 기대되는ㅜㅜ",
          "original_form": "다음 올림픽이 기대되는ㅜㅜ",
