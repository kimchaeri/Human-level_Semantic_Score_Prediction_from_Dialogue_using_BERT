      "utterance": [
        {
          "id": "MDRW2100047361.1.1",
          "form": "사진 잘 나오는데요?ㅋㅋ",
          "original_form": "사진 잘 나오는데요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100047361.1.2",
          "form": "ㅋㅋㅋㅋ 그쵸",
          "original_form": "ㅋㅋㅋㅋ 그쵸",
          "speaker_id": "1",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100047361.1.3",
          "form": "음식점이 이쁘거나",
          "original_form": "음식점이 이쁘거나",
          "speaker_id": "1",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100047361.1.4",
          "form": "음식이 이쁘거나",
          "original_form": "음식이 이쁘거나",
          "speaker_id": "1",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100047361.1.5",
          "form": "둘중하나!",
          "original_form": "둘중하나!",
          "speaker_id": "1",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100047361.1.6",
          "form": "맞아요. 인스타 맛집들 많잖아요",
          "original_form": "맞아요. 인스타 맛집들 많잖아요",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100047361.1.7",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100047361.1.8",
          "form": "사실 저는 음식맛이 못먹을정도만아니면",
          "original_form": "사실 저는 음식맛이 못먹을정도만아니면",
          "speaker_id": "1",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100047361.1.9",
          "form": "그런 맛집도 좋아해요",
          "original_form": "그런 맛집도 좋아해요",
          "speaker_id": "1",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100047361.1.10",
          "form": "그쵸. 친구들이랑 가기도 좋더라구요.",
          "original_form": "그쵸. 친구들이랑 가기도 좋더라구요.",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100047361.1.11",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100047361.1.12",
          "form": "눈으로도 먹는거죠~~일석이조!",
          "original_form": "눈으로도 먹는거죠~~일석이조!",
          "speaker_id": "1",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100047361.1.13",
          "form": "요샌 파인다이닝도 많대서 한번 가보고 싶어요",
          "original_form": "요샌 파인다이닝도 많대서 한번 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100047361.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100047361.1.15",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100047361.1.16",
          "form": "근데 겁나비싸더라고요",
          "original_form": "근데 겁나비싸더라고요",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100047361.1.17",
          "form": "맞아요. 좀 마음먹고 가야될거 같아요ㅋㅋ",
          "original_form": "맞아요. 좀 마음먹고 가야될거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100047361.1.18",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100047361.1.19",
          "form": "저그런데 좋아해서",
          "original_form": "저그런데 좋아해서",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100047361.1.20",
          "form": "사실돈만있음 다가보고싶어요",
          "original_form": "사실돈만있음 다가보고싶어요",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100047361.1.21",
          "form": "아 맞아요ㅋㅋㅋㅋ 돈만 있다면...",
          "original_form": "아 맞아요ㅋㅋㅋㅋ 돈만 있다면...",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100047361.1.22",
          "form": "호텔 레스토랑두 좋구요",
          "original_form": "호텔 레스토랑두 좋구요",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100047361.1.23",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100047361.1.24",
          "form": "저랑비슷하시네요",
          "original_form": "저랑비슷하시네요",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100047361.1.25",
          "form": "이런거싫어하는?관심없는?",
          "original_form": "이런거싫어하는?관심없는?",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100047361.1.26",
          "form": "애들도 많더라고요",
          "original_form": "애들도 많더라고요",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100047361.1.27",
          "form": "그래요? 전 심심하면 찾아보고 하거든요",
          "original_form": "그래요? 전 심심하면 찾아보고 하거든요",
          "speaker_id": "2",
          "time": "20210826 17:28"
        }
      ]
    }
  ]
