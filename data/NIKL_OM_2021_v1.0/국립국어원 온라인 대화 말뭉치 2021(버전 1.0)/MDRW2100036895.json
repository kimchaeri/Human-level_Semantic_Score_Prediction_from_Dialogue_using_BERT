      "utterance": [
        {
          "id": "MDRW2100036895.1.1",
          "form": "어 일반적이지는 않지",
          "original_form": "어 일반적이지는 않지",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036895.1.2",
          "form": "그래서 주말에 놀아도는 되는데",
          "original_form": "그래서 주말에 놀아도는 되는데",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036895.1.3",
          "form": "오 절교 할 일이 있을 수도 있으니",
          "original_form": "오 절교 할 일이 있을 수도 있으니",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036895.1.4",
          "form": "또",
          "original_form": "또",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036895.1.5",
          "form": "너무 마음 줄지는 말기",
          "original_form": "너무 마음 줄지는 말기",
          "speaker_id": "1",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036895.1.6",
          "form": "그때 상처받지 말라고 미리 얘기해두려고",
          "original_form": "그때 상처받지 말라고 미리 얘기해두려고",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036895.1.7",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036895.1.8",
          "form": "우리 name3이 고집은 센데",
          "original_form": "우리 &name3&이 고집은 센데",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036895.1.9",
          "form": "마음이 또 여려서",
          "original_form": "마음이 또 여려서",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036895.1.10",
          "form": "그냥 학교에서 노는 정도가 딱 맞을 듯 요;;",
          "original_form": "그냥 학교에서 노는 정도가 딱 맞을 듯 요;;",
          "speaker_id": "1",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036895.1.11",
          "form": "상처 받을 듯",
          "original_form": "상처 받을 듯",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036895.1.12",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036895.1.13",
          "form": "따로 안 놀았으면 좋겠는데",
          "original_form": "따로 안 놀았으면 좋겠는데",
          "speaker_id": "2",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036895.1.14",
          "form": "애들은 또 그래도 놀고 싶으니까ㅠ",
          "original_form": "애들은 또 그래도 놀고 싶으니까ㅠ",
          "speaker_id": "1",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036895.1.15",
          "form": "또 사과하자마자 약속을 잡았네",
          "original_form": "또 사과하자마자 약속을 잡았네",
          "speaker_id": "2",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036895.1.16",
          "form": "이번 주 하루 놀고 나서",
          "original_form": "이번 주 하루 놀고 나서",
          "speaker_id": "2",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036895.1.17",
          "form": "또 절교 하는 거 아닌가 몰라",
          "original_form": "또 절교 하는 거 아닌가 몰라",
          "speaker_id": "2",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036895.1.18",
          "form": "쉽다 쉬워",
          "original_form": "쉽다 쉬워",
          "speaker_id": "2",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036895.1.19",
          "form": "그럴 수도 있을 듯 요;;",
          "original_form": "그럴 수도 있을 듯 요;;",
          "speaker_id": "1",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036895.1.20",
          "form": "자기 기분 안 좋으면 또 절교할 듯",
          "original_form": "자기 기분 안 좋으면 또 절교할 듯",
          "speaker_id": "1",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036895.1.21",
          "form": "일학년이 절교라..",
          "original_form": "일학년이 절교라..",
          "speaker_id": "1",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036895.1.22",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036895.1.23",
          "form": "우리 name3이 계속 맞춰줘야 될 거 같아서",
          "original_form": "우리 &name3&이 계속 맞춰줘야 될 거 같아서",
          "speaker_id": "2",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036895.1.24",
          "form": "안 놀았으면 좋겠다",
          "original_form": "안 놀았으면 좋겠다",
          "speaker_id": "2",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036895.1.25",
          "form": "그럼 같이 안 노는 게 맞죠ㅠ",
          "original_form": "그럼 같이 안 노는 게 맞죠ㅠ",
          "speaker_id": "1",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036895.1.26",
          "form": "딱히 누구한테 맞춰주는 스타일 아닌데",
          "original_form": "딱히 누구한테 맞춰주는 스타일 아닌데",
          "speaker_id": "2",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036895.1.27",
          "form": "나도 그런 건 너무 싫음",
          "original_form": "나도 그런 건 너무 싫음",
          "speaker_id": "1",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036895.1.28",
          "form": "그러이",
          "original_form": "그러이",
          "speaker_id": "2",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036895.1.29",
          "form": "너가 첫째도 많이 당했잖아",
          "original_form": "너가 첫째도 많이 당했잖아",
          "speaker_id": "2",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036895.1.30",
          "form": "눈에 뻔히 보이는데",
          "original_form": "눈에 뻔히 보이는데",
          "speaker_id": "1",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036895.1.31",
          "form": "난 그래서 name4랑 못 놀게 하잖아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "난 그래서 &name4&랑 못 놀게 하잖아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036895.1.32",
          "form": "이런 거 보면 어 어른이 더 뒤끝 김ㅋㅋㅋㅋ",
          "original_form": "이런 거 보면 어 어른이 더 뒤끝 김ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036895.1.33",
          "form": "그래 name4는 앙칼지더라",
          "original_form": "그래 &name4&는 앙칼지더라",
          "speaker_id": "2",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036895.1.34",
          "form": "애가 애가 아님",
          "original_form": "애가 애가 아님",
          "speaker_id": "2",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036895.1.35",
          "form": "오죽하면 name5이도 끌려 다닐까",
          "original_form": "오죽하면 &name5&이도 끌려 다닐까",
          "speaker_id": "2",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036895.1.36",
          "form": "거의 둘이 같이 노니",
          "original_form": "거의 둘이 같이 노니",
          "speaker_id": "1",
          "time": "20210831 14:43"
        },
        {
          "id": "MDRW2100036895.1.37",
          "form": "name5이랑 도 못놈 ㅋㅋㅋㅋ",
          "original_form": "&name5&이랑 도 못놈 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:43"
        },
        {
          "id": "MDRW2100036895.1.38",
          "form": "그때 놀이터에서 놀다가  우리 집 보고 싶다고 했다 하대요",
          "original_form": "그때 놀이터에서 놀다가  우리 집 보고 싶다고 했다 하대요",
          "speaker_id": "1",
          "time": "20210831 14:45"
        },
        {
          "id": "MDRW2100036895.1.39",
          "form": "오고",
          "original_form": "오고",
          "speaker_id": "1",
          "time": "20210831 14:45"
        },
        {
          "id": "MDRW2100036895.1.40",
          "form": "전화 왔길래 ㅋㅋㅋㅋ",
          "original_form": "전화 왔길래 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:45"
        },
        {
          "id": "MDRW2100036895.1.41",
          "form": "안돼!!",
          "original_form": "안돼!!",
