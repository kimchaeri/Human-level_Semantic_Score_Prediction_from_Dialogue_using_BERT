      "utterance": [
        {
          "id": "MDRW2100019925.1.1",
          "form": "이제 가을이다 친구야...ㅠ",
          "original_form": "이제 가을이다 친구야...ㅠ",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019925.1.2",
          "form": "나 긴바지입음",
          "original_form": "나 긴바지입음",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019925.1.3",
          "form": "오우긴바지입어야지",
          "original_form": "오우긴바지입어야지",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019925.1.4",
          "form": "춥다추워",
          "original_form": "춥다추워",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019925.1.5",
          "form": "곧 긴팔도 꺼냐야함",
          "original_form": "곧 긴팔도 꺼냐야함",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019925.1.6",
          "form": "창문도 닫았다구",
          "original_form": "창문도 닫았다구",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019925.1.7",
          "form": "으슬으슬해요",
          "original_form": "으슬으슬해요",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019925.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019925.1.9",
          "form": "나도용",
          "original_form": "나도용",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019925.1.10",
          "form": "창문아주조금",
          "original_form": "창문아주조금",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019925.1.11",
          "form": "열어놧당",
          "original_form": "열어놧당",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019925.1.12",
          "form": "핵조금",
          "original_form": "핵조금",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019925.1.13",
          "form": "미투",
          "original_form": "미투",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019925.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019925.1.15",
          "form": "다닫으면",
          "original_form": "다닫으면",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019925.1.16",
          "form": "조금더울거같아서",
          "original_form": "조금더울거같아서",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019925.1.17",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019925.1.18",
          "form": "환기를 위해",
          "original_form": "환기를 위해",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019925.1.19",
          "form": "약간은 열어줘야지",
          "original_form": "약간은 열어줘야지",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019925.1.20",
          "form": "이제 조만간 또 옷장정리해야하네",
          "original_form": "이제 조만간 또 옷장정리해야하네",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019925.1.21",
          "form": "개귀찮다",
          "original_form": "개귀찮다",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019925.1.22",
          "form": "옷장정리는왜합니까??",
          "original_form": "옷장정리는왜합니까??",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100019925.1.23",
          "form": "가을옷 꺼내야지 바부야",
          "original_form": "가을옷 꺼내야지 바부야",
          "speaker_id": "1",
          "time": "20210828 06:05"
        },
        {
          "id": "MDRW2100019925.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 06:05"
        },
        {
          "id": "MDRW2100019925.1.25",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100019925.1.26",
          "form": "나는항상똑같아서",
          "original_form": "나는항상똑같아서",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100019925.1.27",
          "form": "정리할필요가없어욬ㅋㅋ",
          "original_form": "정리할필요가없어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100019925.1.28",
          "form": "아그래?ㅋㅋㅋㅋ아니 모든계절 옷을",
          "original_form": "아그래?ㅋㅋㅋㅋ아니 모든계절 옷을",
          "speaker_id": "1",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100019925.1.29",
          "form": "다 넣어놓고 사냐",
          "original_form": "다 넣어놓고 사냐",
          "speaker_id": "1",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100019925.1.30",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100019925.1.31",
          "form": "옷장에",
          "original_form": "옷장에",
          "speaker_id": "2",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100019925.1.32",
          "form": "다들어잇징",
          "original_form": "다들어잇징",
          "speaker_id": "2",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100019925.1.33",
          "form": "어떻게 그러지ㅋㅋㅋ",
          "original_form": "어떻게 그러지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100019925.1.34",
          "form": "매우 큰 옷장..",
          "original_form": "매우 큰 옷장..",
          "speaker_id": "1",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100019925.1.35",
          "form": "나도 언젠가 장만해야겠군",
          "original_form": "나도 언젠가 장만해야겠군",
          "speaker_id": "1",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100019925.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋ내방에잇는",
          "original_form": "ㅋㅋㅋㅋㅋㅋ내방에잇는",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100019925.1.37",
          "form": "옷장못봣어??",
          "original_form": "옷장못봣어??",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100019925.1.38",
          "form": "ㅋㅋㅋㅋㅋㅌㅋ좀 크긴 하지",
          "original_form": "ㅋㅋㅋㅋㅋㅌㅋ좀 크긴 하지",
          "speaker_id": "1",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100019925.1.39",
          "form": "생각해보니 넌 행거가 없네 아닌가",
          "original_form": "생각해보니 넌 행거가 없네 아닌가",
          "speaker_id": "1",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100019925.1.40",
          "form": "입던건 어따놔",
          "original_form": "입던건 어따놔",
          "speaker_id": "1",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100019925.1.41",
          "form": "입던거??",
          "original_form": "입던거??",
