      "utterance": [
        {
          "id": "MDRW2100026480.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026480.1.2",
          "form": "안녕하세요 ~",
          "original_form": "안녕하세요 ~",
          "speaker_id": "2",
          "time": "20210902 07:52"
        },
        {
          "id": "MDRW2100026480.1.3",
          "form": "안녕하세요? 도쿄 올림픽을 보셨나요?",
          "original_form": "안녕하세요? 도쿄 올림픽을 보셨나요?",
          "speaker_id": "1",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100026480.1.4",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "2",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100026480.1.5",
          "form": "양궁하고 배구 봤어요 ㅎㅎ",
          "original_form": "양궁하고 배구 봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100026480.1.6",
          "form": "재밌드라구요",
          "original_form": "재밌드라구요",
          "speaker_id": "2",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100026480.1.7",
          "form": "저도요 저도 배구랑 양궁 봤어요",
          "original_form": "저도요 저도 배구랑 양궁 봤어요",
          "speaker_id": "1",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100026480.1.8",
          "form": "김연경 선수 너무 잘하더라고요",
          "original_form": "김연경 선수 너무 잘하더라고요",
          "speaker_id": "1",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100026480.1.9",
          "form": "정말요 ~ 너무 대단하더라구요",
          "original_form": "정말요 ~ 너무 대단하더라구요",
          "speaker_id": "2",
          "time": "20210902 17:03"
        },
        {
          "id": "MDRW2100026480.1.10",
          "form": "멘탈이 진짜 대단하지 않나요",
          "original_form": "멘탈이 진짜 대단하지 않나요",
          "speaker_id": "1",
          "time": "20210902 17:03"
        },
        {
          "id": "MDRW2100026480.1.11",
          "form": "괜히 유명한게 아닌거같아요 ㅋㅋ",
          "original_form": "괜히 유명한게 아닌거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:03"
        },
        {
          "id": "MDRW2100026480.1.12",
          "form": "전 모 자매 때문에 이번 올림픽 배구는 사실 좀 져도 상관없다고 생각하고 봤는데",
          "original_form": "전 모 자매 때문에 이번 올림픽 배구는 사실 좀 져도 상관없다고 생각하고 봤는데",
          "speaker_id": "1",
          "time": "20210902 17:03"
        },
        {
          "id": "MDRW2100026480.1.13",
          "form": "그 상태서 성과를 내다니 대단해요",
          "original_form": "그 상태서 성과를 내다니 대단해요",
          "speaker_id": "1",
          "time": "20210902 17:03"
        },
        {
          "id": "MDRW2100026480.1.14",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210902 17:04"
        },
        {
          "id": "MDRW2100026480.1.15",
          "form": "자매둘이 다해쳐먹던데",
          "original_form": "자매둘이 다해쳐먹던데",
          "speaker_id": "2",
          "time": "20210902 17:04"
        },
        {
          "id": "MDRW2100026480.1.16",
          "form": "그고생이 있았어도",
          "original_form": "그고생이 있았어도",
          "speaker_id": "2",
          "time": "20210902 17:04"
        },
        {
          "id": "MDRW2100026480.1.17",
          "form": "실력은 어디안가나봐욤 ㅋㅋ",
          "original_form": "실력은 어디안가나봐욤 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:04"
        },
        {
          "id": "MDRW2100026480.1.18",
          "form": "김연경의 국대를 더 보고 싶지만",
          "original_form": "김연경의 국대를 더 보고 싶지만",
          "speaker_id": "1",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100026480.1.19",
          "form": "몸이 힘들다면 더 잡고 싶진 않아요 ㅠ",
          "original_form": "몸이 힘들다면 더 잡고 싶진 않아요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100026480.1.20",
          "form": "몇십년을 해가지고 ㅠㅠ",
          "original_form": "몇십년을 해가지고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100026480.1.21",
          "form": "몸이.많이상했다는걸 아니까..",
          "original_form": "몸이.많이상했다는걸 아니까..",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100026480.1.22",
          "form": "아쉽긴해도 ㅠㅠ",
          "original_form": "아쉽긴해도 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100026480.1.23",
          "form": "네 그렇죠 그리고 이제 예능에 많이 나오니깐요 하하하",
          "original_form": "네 그렇죠 그리고 이제 예능에 많이 나오니깐요 하하하",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100026480.1.24",
          "form": "배구경기직관을 해보고 싶었는데 코로나라 못가고있어요",
          "original_form": "배구경기직관을 해보고 싶었는데 코로나라 못가고있어요",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100026480.1.25",
          "form": "진짜...코로나때무네 ㅜㅠ",
          "original_form": "진짜...코로나때무네 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 20:33"
        },
        {
          "id": "MDRW2100026480.1.26",
          "form": "그런건 직관해봐야되는데 ㅠ",
          "original_form": "그런건 직관해봐야되는데 ㅠ",
          "speaker_id": "2",
          "time": "20210902 20:33"
        },
        {
          "id": "MDRW2100026480.1.27",
          "form": "배구는 관람비도 저렴한데",
          "original_form": "배구는 관람비도 저렴한데",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100026480.1.28",
          "form": "코로나가~~~ 배구도 막는다니",
          "original_form": "코로나가~~~ 배구도 막는다니",
          "speaker_id": "1",
          "time": "20210902 20:35"
        }
      ]
