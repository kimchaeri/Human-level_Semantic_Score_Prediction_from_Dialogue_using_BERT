      "utterance": [
        {
          "id": "MDRW2100045238.1.1",
          "form": "전 사실 이번학기 휴학했어요 ㅎㅎ",
          "original_form": "전 사실 이번학기 휴학했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045238.1.2",
          "form": "앗 쉬시려고 휴학 하신건가요?",
          "original_form": "앗 쉬시려고 휴학 하신건가요?",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045238.1.3",
          "form": "네ㅜㅜ 지난학기가 너무 힘들었어서",
          "original_form": "네ㅜㅜ 지난학기가 너무 힘들었어서",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045238.1.4",
          "form": "자격증 공부겸 휴학했는데.. 사실은 놀고있어요",
          "original_form": "자격증 공부겸 휴학했는데.. 사실은 놀고있어요",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045238.1.5",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045238.1.6",
          "form": "그럴땐 쉬는 것도 좋은거같아요",
          "original_form": "그럴땐 쉬는 것도 좋은거같아요",
          "speaker_id": "2",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045238.1.7",
          "form": "저는 인턴하느라 쉰거 말고는",
          "original_form": "저는 인턴하느라 쉰거 말고는",
          "speaker_id": "2",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045238.1.8",
          "form": "스트레이트로 졸업햇는대",
          "original_form": "스트레이트로 졸업햇는대",
          "speaker_id": "2",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045238.1.9",
          "form": "너무 후회돼요....ㅠ",
          "original_form": "너무 후회돼요....ㅠ",
          "speaker_id": "2",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045238.1.10",
          "form": "헉 ㅠㅠ 스트레이트 졸업이면 빡세셨을텐데 대단해요",
          "original_form": "헉 ㅠㅠ 스트레이트 졸업이면 빡세셨을텐데 대단해요",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045238.1.11",
          "form": "전 학년 올라가는게 무서워서 휴학하는 것도 있어서..",
          "original_form": "전 학년 올라가는게 무서워서 휴학하는 것도 있어서..",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045238.1.12",
          "form": "학년 올라가면 크게 바뀌나요?",
          "original_form": "학년 올라가면 크게 바뀌나요?",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045238.1.13",
          "form": "과가 실습이 많은과라서 아무래도 실습 비중이 늘어나는데",
          "original_form": "과가 실습이 많은과라서 아무래도 실습 비중이 늘어나는데",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045238.1.14",
          "form": "코로나 때문에 불확실한게 좀 많아서요ㅠㅠ",
          "original_form": "코로나 때문에 불확실한게 좀 많아서요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045238.1.15",
          "form": "아하....ㅠ 그럼 좀 꺼려지긴 하겠네요",
          "original_form": "아하....ㅠ 그럼 좀 꺼려지긴 하겠네요",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045238.1.16",
          "form": "동기들도 많이 휴학하더라구요ㅠㅠ",
          "original_form": "동기들도 많이 휴학하더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045238.1.17",
          "form": "동아리 활동도 잠잠한게 아쉬워요",
          "original_form": "동아리 활동도 잠잠한게 아쉬워요",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045238.1.18",
          "form": "그쵸 요즘은 학교가 학교 분위기가 아니에요",
          "original_form": "그쵸 요즘은 학교가 학교 분위기가 아니에요",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045238.1.19",
          "form": "요즘 초중고는 등교를 하는지 안하는지도 모르겠어요ㅠㅠ",
          "original_form": "요즘 초중고는 등교를 하는지 안하는지도 모르겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045238.1.20",
          "form": "그쵸 ㅋㅋㅋ 애들이 같은 반 친구 얼굴도 잘 모른다더라구요",
