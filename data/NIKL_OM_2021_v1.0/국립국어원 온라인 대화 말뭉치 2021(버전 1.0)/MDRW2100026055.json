      "utterance": [
        {
          "id": "MDRW2100026055.1.1",
          "form": "상그라 맛있더라",
          "original_form": "상그라 맛있더라",
          "speaker_id": "2",
          "time": "20210905 09:26"
        },
        {
          "id": "MDRW2100026055.1.2",
          "form": "상그라가 뭐야",
          "original_form": "상그라가 뭐야",
          "speaker_id": "1",
          "time": "20210905 09:27"
        },
        {
          "id": "MDRW2100026055.1.3",
          "form": "ㅋ 중식집 이름\n코스먹었는데 맛있었어",
          "original_form": "ㅋ 중식집 이름\n코스먹었는데 맛있었어",
          "speaker_id": "2",
          "time": "20210905 09:27"
        },
        {
          "id": "MDRW2100026055.1.4",
          "form": "유래등보다 맛있었어?",
          "original_form": "유래등보다 맛있었어?",
          "speaker_id": "1",
          "time": "20210905 09:27"
        },
        {
          "id": "MDRW2100026055.1.5",
          "form": "거기보다 가격은 비싼데 먓있는듯",
          "original_form": "거기보다 가격은 비싼데 먓있는듯",
          "speaker_id": "2",
          "time": "20210905 09:28"
        },
        {
          "id": "MDRW2100026055.1.6",
          "form": "비싼값을 하는군 음식이 몇개 나와?",
          "original_form": "비싼값을 하는군 음식이 몇개 나와?",
          "speaker_id": "1",
          "time": "20210905 09:28"
        },
        {
          "id": "MDRW2100026055.1.7",
          "form": "한 열개정도\n근데 짬뽕에 마약넣었나 왜그리 맛나 했다는데 그정돈아님",
          "original_form": "한 열개정도\n근데 짬뽕에 마약넣었나 왜그리 맛나 했다는데 그정돈아님",
          "speaker_id": "2",
          "time": "20210905 09:29"
        },
        {
          "id": "MDRW2100026055.1.8",
          "form": "ㅋㅋㅋ 게살수프도 있어?",
          "original_form": "ㅋㅋㅋ 게살수프도 있어?",
          "speaker_id": "1",
          "time": "20210905 09:29"
        },
        {
          "id": "MDRW2100026055.1.9",
          "form": "그코슨엔 없구 런치코스에 있대",
          "original_form": "그코슨엔 없구 런치코스에 있대",
          "speaker_id": "2",
          "time": "20210905 09:30"
        },
        {
          "id": "MDRW2100026055.1.10",
          "form": "그럼 디너엔 무슨 수프나오는데?",
          "original_form": "그럼 디너엔 무슨 수프나오는데?",
          "speaker_id": "1",
          "time": "20210905 09:30"
        },
        {
          "id": "MDRW2100026055.1.11",
          "form": "스픈 기억이 앉나네 단호박스프였나 ㅠ",
          "original_form": "스픈 기억이 앉나네 단호박스프였나 ㅠ",
          "speaker_id": "2",
          "time": "20210905 09:31"
        },
        {
          "id": "MDRW2100026055.1.12",
          "form": "뭐야 중국집에서 왜 뜬금없이 단호박스프?",
          "original_form": "뭐야 중국집에서 왜 뜬금없이 단호박스프?",
          "speaker_id": "1",
          "time": "20210905 09:32"
        },
        {
          "id": "MDRW2100026055.1.13",
          "form": "ㅋ 그치  담에가서 봐야겠다\n칠리새우가 탱글탱글 맛났어",
          "original_form": "ㅋ 그치  담에가서 봐야겠다\n칠리새우가 탱글탱글 맛났어",
          "speaker_id": "2",
          "time": "20210905 09:33"
        }
      ]
