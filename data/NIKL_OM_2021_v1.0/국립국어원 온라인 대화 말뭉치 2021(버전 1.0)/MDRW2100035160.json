      "utterance": [
        {
          "id": "MDRW2100035160.1.1",
          "form": "음 그래도 없진않은것같아요ㅋㅋㅋ",
          "original_form": "음 그래도 없진않은것같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 09:05"
        },
        {
          "id": "MDRW2100035160.1.2",
          "form": "매점가서 과자 맨날 사먹고",
          "original_form": "매점가서 과자 맨날 사먹고",
          "speaker_id": "2",
          "time": "20210829 09:05"
        },
        {
          "id": "MDRW2100035160.1.3",
          "form": "친구들이랑 도서관가고ㅋㅋ",
          "original_form": "친구들이랑 도서관가고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 09:05"
        },
        {
          "id": "MDRW2100035160.1.4",
          "form": "매점에선 뭘 애용하셨나요,",
          "original_form": "매점에선 뭘 애용하셨나요,",
          "speaker_id": "1",
          "time": "20210829 09:09"
        },
        {
          "id": "MDRW2100035160.1.5",
          "form": "나나콘 아세요?",
          "original_form": "나나콘 아세요?",
          "speaker_id": "2",
          "time": "20210829 09:10"
        },
        {
          "id": "MDRW2100035160.1.6",
          "form": "브이콘인가?",
          "original_form": "브이콘인가?",
          "speaker_id": "2",
          "time": "20210829 09:10"
        },
        {
          "id": "MDRW2100035160.1.7",
          "form": "지역마다 이름이 좀 다른데 ㅋㅋ",
          "original_form": "지역마다 이름이 좀 다른데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 09:10"
        },
        {
          "id": "MDRW2100035160.1.8",
          "form": "오 브이콘 알아요 ㅋㅋ 매점의 추억..ㅋㅋ",
          "original_form": "오 브이콘 알아요 ㅋㅋ 매점의 추억..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 09:10"
        },
        {
          "id": "MDRW2100035160.1.9",
          "form": "그쵸ㅋㅋ 그거 한 5백원이었나 사먹구",
          "original_form": "그쵸ㅋㅋ 그거 한 5백원이었나 사먹구",
          "speaker_id": "2",
          "time": "20210829 09:11"
        },
        {
          "id": "MDRW2100035160.1.10",
          "form": "초코우유 사먹고",
          "original_form": "초코우유 사먹고",
          "speaker_id": "2",
          "time": "20210829 09:11"
        },
        {
          "id": "MDRW2100035160.1.11",
          "form": "그랬어요ㅋㅋ",
          "original_form": "그랬어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 09:11"
        },
        {
          "id": "MDRW2100035160.1.12",
          "form": "매점 최애 과자 있으신가요?",
          "original_form": "매점 최애 과자 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 09:11"
        },
        {
          "id": "MDRW2100035160.1.13",
          "form": "음 저도 주로 우유와 빵이요 ㅋ",
          "original_form": "음 저도 주로 우유와 빵이요 ㅋ",
          "speaker_id": "1",
          "time": "20210829 09:11"
        },
        {
          "id": "MDRW2100035160.1.14",
          "form": "급식으로 점심 맛없으면",
          "original_form": "급식으로 점심 맛없으면",
          "speaker_id": "2",
          "time": "20210829 09:12"
        },
        {
          "id": "MDRW2100035160.1.15",
          "form": "안먹고 매점가서 사먹고 그랬어용ㅋㅋㅋ",
          "original_form": "안먹고 매점가서 사먹고 그랬어용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 09:12"
        },
        {
          "id": "MDRW2100035160.1.16",
          "form": "그랬죠 그 매점빵 있잖아요 ㅋ 제과점과는 다른..ㅋ 아쉬운데 생각나는 맛 ㅋ",
          "original_form": "그랬죠 그 매점빵 있잖아요 ㅋ 제과점과는 다른..ㅋ 아쉬운데 생각나는 맛 ㅋ",
          "speaker_id": "1",
          "time": "20210829 09:14"
        },
        {
          "id": "MDRW2100035160.1.17",
          "form": "넹ㅋㅋㅋ 졸업하고나서는 더 맛있는거 많이 먹으니까 굳이 돈주고 안사먹는데",
          "original_form": "넹ㅋㅋㅋ 졸업하고나서는 더 맛있는거 많이 먹으니까 굳이 돈주고 안사먹는데",
          "speaker_id": "2",
          "time": "20210829 09:15"
        },
        {
          "id": "MDRW2100035160.1.18",
          "form": "학생땐 그게 그렇게 맛있더라구요",
          "original_form": "학생땐 그게 그렇게 맛있더라구요",
          "speaker_id": "2",
          "time": "20210829 09:15"
        },
        {
          "id": "MDRW2100035160.1.19",
          "form": "맞아요 그 시절 그 맛들이 기억나죠^^",
          "original_form": "맞아요 그 시절 그 맛들이 기억나죠^^",
          "speaker_id": "1",
          "time": "20210829 09:15"
        },
        {
          "id": "MDRW2100035160.1.20",
          "form": "교복은 졸업하시고",
          "original_form": "교복은 졸업하시고",
          "speaker_id": "2",
          "time": "20210829 09:16"
        },
        {
          "id": "MDRW2100035160.1.21",
          "form": "버리셨나용?",
          "original_form": "버리셨나용?",
          "speaker_id": "2",
          "time": "20210829 09:16"
        },
        {
          "id": "MDRW2100035160.1.22",
          "form": "네 한동안 갖고있다가 버렸던듯해요",
          "original_form": "네 한동안 갖고있다가 버렸던듯해요",
          "speaker_id": "1",
          "time": "20210829 09:17"
        },
        {
          "id": "MDRW2100035160.1.23",
          "form": "전 아직도 가지고 있어용ㅋㅋ",
          "original_form": "전 아직도 가지고 있어용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 09:17"
        },
        {
          "id": "MDRW2100035160.1.24",
          "form": "못버리겠더라구요",
          "original_form": "못버리겠더라구요",
          "speaker_id": "2",
          "time": "20210829 09:17"
        }
      ]
    }
