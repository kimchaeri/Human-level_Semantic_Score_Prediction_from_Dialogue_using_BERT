        },
        {
          "id": "MDRW2100035816.1.2",
          "form": "안 안녕할것같은데",
          "original_form": "안 안녕할것같은데",
          "speaker_id": "1",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100035816.1.3",
          "form": "그쵸 마음이 좀 그렇네요",
          "original_form": "그쵸 마음이 좀 그렇네요",
          "speaker_id": "2",
          "time": "20210830 03:26"
        },
        {
          "id": "MDRW2100035816.1.4",
          "form": "패션에 관심 많으세여?",
          "original_form": "패션에 관심 많으세여?",
          "speaker_id": "1",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100035816.1.5",
          "form": "넹 많아여",
          "original_form": "넹 많아여",
          "speaker_id": "2",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100035816.1.6",
          "form": "오.. 뭐 사고싶은거 잇르세여?",
          "original_form": "오.. 뭐 사고싶은거 잇르세여?",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
