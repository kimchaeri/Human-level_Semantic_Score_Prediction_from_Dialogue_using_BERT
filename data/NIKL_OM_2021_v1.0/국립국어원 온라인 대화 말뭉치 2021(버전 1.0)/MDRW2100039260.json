      "utterance": [
        {
          "id": "MDRW2100039260.1.1",
          "form": "어제 머리자르고왔는데 대실패했어요",
          "original_form": "어제 머리자르고왔는데 대실패했어요",
          "speaker_id": "2",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100039260.1.2",
          "form": "너무짧아져서 순박해졌어요ㅠㅠ흑ㅎ.ㄱ",
          "original_form": "너무짧아져서 순박해졌어요ㅠㅠ흑ㅎ.ㄱ",
          "speaker_id": "2",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100039260.1.3",
          "form": "순박해질 정도로 자른건 무슨 스타일일까요...",
          "original_form": "순박해질 정도로 자른건 무슨 스타일일까요...",
          "speaker_id": "1",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100039260.1.4",
          "form": "그래도 언젠간 자랄거예요",
          "original_form": "그래도 언젠간 자랄거예요",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039260.1.5",
          "form": "약간 몽실이 됐어요 ㅋㅋ",
          "original_form": "약간 몽실이 됐어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100039260.1.6",
          "form": "네ㅠㅠ자라겠죠",
          "original_form": "네ㅠㅠ자라겠죠",
          "speaker_id": "2",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100039260.1.7",
          "form": "날씨 추워질때 깎으면 춥지 않나요",
          "original_form": "날씨 추워질때 깎으면 춥지 않나요",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100039260.1.8",
          "form": "그러게요 그저께 너무 더워서 잘라버렸는데",
          "original_form": "그러게요 그저께 너무 더워서 잘라버렸는데",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100039260.1.9",
          "form": "다음날 선선해졌어요 ㅋㅋ",
          "original_form": "다음날 선선해졌어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100039260.1.10",
          "form": "앜ㅋㅋ 가을이 돼버렸죠",
          "original_form": "앜ㅋㅋ 가을이 돼버렸죠",
          "speaker_id": "1",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100039260.1.11",
          "form": "맞아요 하루아침에",
          "original_form": "맞아요 하루아침에",
          "speaker_id": "2",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100039260.1.12",
          "form": "그래도 햇볕은 좀 덥네요",
          "original_form": "그래도 햇볕은 좀 덥네요",
          "speaker_id": "1",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100039260.1.13",
          "form": "맞아요 밤은 이제 춥네요",
          "original_form": "맞아요 밤은 이제 춥네요",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039260.1.14",
          "form": "긴팔 입었어요 첨으로",
          "original_form": "긴팔 입었어요 첨으로",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039260.1.15",
          "form": "날씨가 장마 기점으로 확 바뀌니",
          "original_form": "날씨가 장마 기점으로 확 바뀌니",
          "speaker_id": "1",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100039260.1.16",
          "form": "정신이 없어요",
          "original_form": "정신이 없어요",
          "speaker_id": "1",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100039260.1.17",
          "form": "맞아요 ㅋㅋㅋ 그래도 선선해서좋네여",
          "original_form": "맞아요 ㅋㅋㅋ 그래도 선선해서좋네여",
          "speaker_id": "2",
          "time": "20210903 22:01"
        },
        {
          "id": "MDRW2100039260.1.18",
          "form": "돌아다니기 좋은 날씨가 됐지요",
          "original_form": "돌아다니기 좋은 날씨가 됐지요",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
