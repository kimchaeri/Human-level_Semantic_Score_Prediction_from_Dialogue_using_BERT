      "utterance": [
        {
          "id": "MDRW2100045459.1.1",
          "form": "야채 도전~!",
          "original_form": "야채 도전~!",
          "speaker_id": "2",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045459.1.2",
          "form": "뿌리식물인가요?",
          "original_form": "뿌리식물인가요?",
          "speaker_id": "2",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045459.1.3",
          "form": "음..... 그렇...죠...?",
          "original_form": "음..... 그렇...죠...?",
          "speaker_id": "1",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045459.1.4",
          "form": "ㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ되게",
          "original_form": "ㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ되게",
          "speaker_id": "2",
          "time": "20210908 02:43"
        },
        {
          "id": "MDRW2100045459.1.5",
          "form": "가물가물하게 대답하시는데",
          "original_form": "가물가물하게 대답하시는데",
          "speaker_id": "2",
          "time": "20210908 02:43"
        },
        {
          "id": "MDRW2100045459.1.6",
          "form": "뿌리식물인지 긴가민가한 야채인가요?",
          "original_form": "뿌리식물인지 긴가민가한 야채인가요?",
          "speaker_id": "2",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045459.1.7",
          "form": "뿌리식물이 당근 같은거 말하시는거죠?",
          "original_form": "뿌리식물이 당근 같은거 말하시는거죠?",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045459.1.8",
          "form": "네! 고구마 감자 무 당근처럼",
          "original_form": "네! 고구마 감자 무 당근처럼",
          "speaker_id": "2",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045459.1.9",
          "form": "파묻혀있는 거요!",
          "original_form": "파묻혀있는 거요!",
          "speaker_id": "2",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045459.1.10",
          "form": "그런 종류는 아니에요!",
          "original_form": "그런 종류는 아니에요!",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045459.1.11",
          "form": "반만 파묻힌 이상한 친구인가요?",
          "original_form": "반만 파묻힌 이상한 친구인가요?",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045459.1.12",
          "form": "아뇨 파묻힌 친구는 아녜요!",
          "original_form": "아뇨 파묻힌 친구는 아녜요!",
          "speaker_id": "1",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045459.1.13",
          "form": "아앗.. 그럼 덩굴식물인가요?",
          "original_form": "아앗.. 그럼 덩굴식물인가요?",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100045459.1.14",
          "form": "아뇨 덩굴은 아닙니다!",
          "original_form": "아뇨 덩굴은 아닙니다!",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100045459.1.15",
          "form": "초록색인가요? 이번 건 좀 어려울 것 같은 느낌이 드는걸요ㅋㅌㅋ",
          "original_form": "초록색인가요? 이번 건 좀 어려울 것 같은 느낌이 드는걸요ㅋㅌㅋ",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045459.1.16",
          "form": "아뇨 초록색은 아니에요!",
          "original_form": "아뇨 초록색은 아니에요!",
          "speaker_id": "1",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045459.1.17",
          "form": "으으으음 그럼 하얀 색인가요?",
          "original_form": "으으으음 그럼 하얀 색인가요?",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045459.1.18",
          "form": "음... 네 하얀색이 많아요!",
          "original_form": "음... 네 하얀색이 많아요!",
          "speaker_id": "1",
          "time": "20210908 02:51"
        }
