      "utterance": [
        {
          "id": "MDRW2100026445.1.1",
          "form": "어 알아요~ㅋㅋ 너무 신기해요",
          "original_form": "어 알아요~ㅋㅋ 너무 신기해요",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026445.1.2",
          "form": "ㅋㅋ 지금도신기해요",
          "original_form": "ㅋㅋ 지금도신기해요",
          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026445.1.3",
          "form": "어떻게하는건지",
          "original_form": "어떻게하는건지",
          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026445.1.4",
          "form": "저 완전 의심의 눈초리로 아래도 쳐다보고 그랬어요 ㅋㅋㅋ",
          "original_form": "저 완전 의심의 눈초리로 아래도 쳐다보고 그랬어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026445.1.5",
          "form": "ㅋㅋㅋ 다시보고싶네요",
          "original_form": "ㅋㅋㅋ 다시보고싶네요",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026445.1.6",
          "form": "저도요ㅠㅜ 코로나 끝나면 반드시 갈 거예요",
          "original_form": "저도요ㅠㅜ 코로나 끝나면 반드시 갈 거예요",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026445.1.7",
          "form": "또 다녀오신곳 좋았던곳있어요?",
          "original_form": "또 다녀오신곳 좋았던곳있어요?",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026445.1.8",
          "form": "어디어디 가보셨어요??",
          "original_form": "어디어디 가보셨어요??",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026445.1.9",
          "form": "저 일본도 가보고~ 헝가리도 가보고~",
          "original_form": "저 일본도 가보고~ 헝가리도 가보고~",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026445.1.10",
          "form": "일본은 친구가 거기 살아서 가봤었어요!",
          "original_form": "일본은 친구가 거기 살아서 가봤었어요!",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026445.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026445.1.12",
          "form": "일본은 어느지역이예요?",
          "original_form": "일본은 어느지역이예요?",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026445.1.13",
          "form": "헝가리도궁금해요!",
          "original_form": "헝가리도궁금해요!",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026445.1.14",
          "form": "일본은 오사카요!",
          "original_form": "일본은 오사카요!",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026445.1.15",
          "form": "저는오사카 가봤어요!",
          "original_form": "저는오사카 가봤어요!",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026445.1.16",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026445.1.17",
          "form": "어머! ㅋㅋㅋ",
          "original_form": "어머! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026445.1.18",
          "form": "저는 유니버셜 갔어요",
          "original_form": "저는 유니버셜 갔어요",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026445.1.19",
          "form": "사실 딱 오사카는 아니고 오사카 근처 어디랬는데 기억이 안나요 ㅋㅋㅋ",
          "original_form": "사실 딱 오사카는 아니고 오사카 근처 어디랬는데 기억이 안나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026445.1.20",
          "form": "우와아 어때요 거긴?",
          "original_form": "우와아 어때요 거긴?",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026445.1.21",
          "form": "ㅋㅋㅋ 저 1월1일에갔거든요.",
          "original_form": "ㅋㅋㅋ 저 1월1일에갔거든요.",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026445.1.22",
          "form": "근데 12월 31일날입장하면 다음날까지 이용할수있대요",
          "original_form": "근데 12월 31일날입장하면 다음날까지 이용할수있대요",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026445.1.23",
          "form": "유니버셜도 신세계였어요.  한국에는없으니깐.",
          "original_form": "유니버셜도 신세계였어요.  한국에는없으니깐.",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026445.1.24",
          "form": "막 무서운 기구보다 좀신기한기구?  체험하는게 많아서 좋아요.",
          "original_form": "막 무서운 기구보다 좀신기한기구?  체험하는게 많아서 좋아요.",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026445.1.25",
          "form": "사실저는 무서운거 잘 못타거든요",
          "original_form": "사실저는 무서운거 잘 못타거든요",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026445.1.26",
          "form": "캐릭터들도 많이 돌아다니구",
          "original_form": "캐릭터들도 많이 돌아다니구",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026445.1.27",
          "form": "우와 귀여울 것 같아요~",
          "original_form": "우와 귀여울 것 같아요~",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026445.1.28",
          "form": "캐릭터 돌아다니는거 넘 취향저격 ㅋㅋㅋ",
          "original_form": "캐릭터 돌아다니는거 넘 취향저격 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026445.1.29",
          "form": "저 캐릭터 귀여운거에 환장하거든요",
          "original_form": "저 캐릭터 귀여운거에 환장하거든요",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026445.1.30",
          "form": "캐릭터는 뭐뭐 있어요?",
          "original_form": "캐릭터는 뭐뭐 있어요?",
          "speaker_id": "2",
          "time": "20210901 22:32"
        }
      ]
    }
  ]
