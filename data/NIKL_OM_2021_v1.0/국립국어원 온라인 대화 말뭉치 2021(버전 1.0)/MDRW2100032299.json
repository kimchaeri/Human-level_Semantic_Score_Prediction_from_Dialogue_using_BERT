      "utterance": [
        {
          "id": "MDRW2100032299.1.1",
          "form": "머리 자르고 싶어요",
          "original_form": "머리 자르고 싶어요",
          "speaker_id": "1",
          "time": "20210905 15:02"
        },
        {
          "id": "MDRW2100032299.1.2",
          "form": "전 매직하고싶어요",
          "original_form": "전 매직하고싶어요",
          "speaker_id": "2",
          "time": "20210905 15:02"
        },
        {
          "id": "MDRW2100032299.1.3",
          "form": "전 파마요",
          "original_form": "전 파마요",
          "speaker_id": "1",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100032299.1.4",
          "form": "재난지원금 나오면 할 거에요",
          "original_form": "재난지원금 나오면 할 거에요",
          "speaker_id": "1",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100032299.1.5",
          "form": "매직하고 가르마도 바꾸고싶어요",
          "original_form": "매직하고 가르마도 바꾸고싶어요",
          "speaker_id": "2",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100032299.1.6",
          "form": "저도 재난지원금 나오면 머리하려구요",
          "original_form": "저도 재난지원금 나오면 머리하려구요",
          "speaker_id": "2",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100032299.1.7",
          "form": "어디로 가세요?",
          "original_form": "어디로 가세요?",
          "speaker_id": "1",
          "time": "20210905 15:30"
        },
        {
          "id": "MDRW2100032299.1.8",
          "form": "매직만 할거라서 고민중이랍니다",
          "original_form": "매직만 할거라서 고민중이랍니다",
          "speaker_id": "2",
          "time": "20210905 15:36"
        },
        {
          "id": "MDRW2100032299.1.9",
          "form": "앞머리는 자르지 마세요",
          "original_form": "앞머리는 자르지 마세요",
          "speaker_id": "1",
          "time": "20210905 16:07"
        },
        {
          "id": "MDRW2100032299.1.10",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100032299.1.11",
          "form": "관리가 넘 힘들어요",
          "original_form": "관리가 넘 힘들어요",
          "speaker_id": "1",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100032299.1.12",
          "form": "이마만 이쁘면 넘기고 싶어요",
          "original_form": "이마만 이쁘면 넘기고 싶어요",
          "speaker_id": "1",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100032299.1.13",
          "form": "공부 하기도 짱나요",
          "original_form": "공부 하기도 짱나요",
          "speaker_id": "1",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100032299.1.14",
          "form": "자꾸 앞머리가 휘어서...",
          "original_form": "자꾸 앞머리가 휘어서...",
          "speaker_id": "2",
          "time": "20210905 16:22"
        },
        {
          "id": "MDRW2100032299.1.15",
          "form": "자르고 싶어요.",
          "original_form": "자르고 싶어요.",
          "speaker_id": "2",
          "time": "20210905 16:22"
        },
        {
          "id": "MDRW2100032299.1.16",
          "form": "안돼요",
          "original_form": "안돼요",
          "speaker_id": "1",
          "time": "20210905 16:25"
        },
        {
          "id": "MDRW2100032299.1.17",
          "form": "매직으로 넘기세요",
          "original_form": "매직으로 넘기세요",
          "speaker_id": "1",
          "time": "20210905 16:25"
        },
        {
          "id": "MDRW2100032299.1.18",
          "form": "매직하면 넘겨지나요?",
          "original_form": "매직하면 넘겨지나요?",
          "speaker_id": "2",
          "time": "20210905 16:25"
        },
        {
          "id": "MDRW2100032299.1.19",
          "form": "아니면 앞머리만 컬 넣어서 넘겨달라 하세요",
          "original_form": "아니면 앞머리만 컬 넣어서 넘겨달라 하세요",
          "speaker_id": "1",
          "time": "20210905 16:25"
        }
      ]
