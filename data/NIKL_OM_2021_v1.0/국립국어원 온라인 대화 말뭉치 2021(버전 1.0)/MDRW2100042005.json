      "utterance": [
        {
          "id": "MDRW2100042005.1.1",
          "form": "끝난줄 알앗는데 또 비네요",
          "original_form": "끝난줄 알앗는데 또 비네요",
          "speaker_id": "2",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100042005.1.2",
          "form": "낼도 비가 많이 온다고 하네요",
          "original_form": "낼도 비가 많이 온다고 하네요",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100042005.1.3",
          "form": "이번주 계속 오려나봐여",
          "original_form": "이번주 계속 오려나봐여",
          "speaker_id": "2",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100042005.1.4",
          "form": "넘 자주 비가 오네요",
          "original_form": "넘 자주 비가 오네요",
          "speaker_id": "1",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100042005.1.5",
          "form": "채소값 비싸던데요",
          "original_form": "채소값 비싸던데요",
          "speaker_id": "1",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100042005.1.6",
          "form": "네 추석이 다가오니 비싸지네요",
          "original_form": "네 추석이 다가오니 비싸지네요",
          "speaker_id": "2",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100042005.1.7",
          "form": "비가 엄청 계속 오네요",
          "original_form": "비가 엄청 계속 오네요",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042005.1.8",
          "form": "오늘 계속오더니 슬슬 그치려나요",
          "original_form": "오늘 계속오더니 슬슬 그치려나요",
          "speaker_id": "2",
          "time": "20210907 17:06"
        },
        {
          "id": "MDRW2100042005.1.9",
          "form": "여긴 하루 종일 오고 있네요",
          "original_form": "여긴 하루 종일 오고 있네요",
          "speaker_id": "1",
          "time": "20210907 17:18"
        },
        {
          "id": "MDRW2100042005.1.10",
          "form": "날이 추워지네요",
          "original_form": "날이 추워지네요",
          "speaker_id": "1",
          "time": "20210907 17:18"
        },
        {
          "id": "MDRW2100042005.1.11",
          "form": "오늘 날이 좋아요!!",
          "original_form": "오늘 날이 좋아요!!",
          "speaker_id": "2",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100042005.1.12",
          "form": "여긴 비는 그쳤지만 흐린 날씨네요",
          "original_form": "여긴 비는 그쳤지만 흐린 날씨네요",
          "speaker_id": "1",
          "time": "20210908 07:27"
        },
        {
          "id": "MDRW2100042005.1.13",
          "form": "이제 비는 끝낫으려나요",
          "original_form": "이제 비는 끝낫으려나요",
          "speaker_id": "2",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100042005.1.14",
          "form": "일기예보에 오후에 그친다고는하네요",
          "original_form": "일기예보에 오후에 그친다고는하네요",
          "speaker_id": "1",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100042005.1.15",
          "form": "출근할때 비오면 너무 가기싫어요ㅠ",
          "original_form": "출근할때 비오면 너무 가기싫어요ㅠ",
          "speaker_id": "2",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100042005.1.16",
          "form": "옷도 신발도 젖고 정말싫어요",
          "original_form": "옷도 신발도 젖고 정말싫어요",
