      "utterance": [
        {
          "id": "MDRW2100032394.1.1",
          "form": "훈련뿐아니라 동기랑재미있게",
          "original_form": "훈련뿐아니라 동기랑재미있게",
          "speaker_id": "1",
          "time": "20210905 18:03"
        },
        {
          "id": "MDRW2100032394.1.2",
          "form": "아..그렇군요",
          "original_form": "아..그렇군요",
          "speaker_id": "2",
          "time": "20210905 18:03"
        },
        {
          "id": "MDRW2100032394.1.3",
          "form": "무슨일요?",
          "original_form": "무슨일요?",
          "speaker_id": "2",
          "time": "20210905 18:03"
        },
        {
          "id": "MDRW2100032394.1.4",
          "form": "그냥 군생활요",
          "original_form": "그냥 군생활요",
          "speaker_id": "1",
          "time": "20210905 18:03"
        },
        {
          "id": "MDRW2100032394.1.5",
          "form": "훈련빼고 뭐요?",
          "original_form": "훈련빼고 뭐요?",
          "speaker_id": "2",
          "time": "20210905 18:03"
        },
        {
          "id": "MDRW2100032394.1.6",
          "form": "동기랑퇴근하고 술마시기도하고 놀기도하고",
          "original_form": "동기랑퇴근하고 술마시기도하고 놀기도하고",
          "speaker_id": "1",
          "time": "20210905 18:04"
        },
        {
          "id": "MDRW2100032394.1.7",
          "form": "그게가능해요?",
          "original_form": "그게가능해요?",
          "speaker_id": "2",
          "time": "20210905 18:04"
        },
        {
          "id": "MDRW2100032394.1.8",
          "form": "출퇴근을해서 가능해요",
          "original_form": "출퇴근을해서 가능해요",
          "speaker_id": "1",
          "time": "20210905 18:04"
        },
        {
          "id": "MDRW2100032394.1.9",
          "form": "아.그렇군요",
          "original_form": "아.그렇군요",
          "speaker_id": "2",
          "time": "20210905 18:04"
        },
        {
          "id": "MDRW2100032394.1.10",
          "form": "하사관이라.일반병이아니고",
          "original_form": "하사관이라.일반병이아니고",
          "speaker_id": "1",
          "time": "20210905 18:04"
        },
        {
          "id": "MDRW2100032394.1.11",
          "form": "그런게있군요.",
          "original_form": "그런게있군요.",
          "speaker_id": "2",
          "time": "20210905 18:04"
        },
        {
          "id": "MDRW2100032394.1.12",
          "form": "여자분이시죠?",
          "original_form": "여자분이시죠?",
          "speaker_id": "1",
          "time": "20210905 18:05"
        },
        {
          "id": "MDRW2100032394.1.13",
          "form": "네..그래서잘몰라요",
          "original_form": "네..그래서잘몰라요",
          "speaker_id": "2",
          "time": "20210905 18:05"
        },
        {
          "id": "MDRW2100032394.1.14",
          "form": "군대도재미있어요",
          "original_form": "군대도재미있어요",
          "speaker_id": "1",
          "time": "20210905 18:05"
        },
        {
          "id": "MDRW2100032394.1.15",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210905 18:05"
        },
        {
          "id": "MDRW2100032394.1.16",
          "form": "군대갔다온사람들얘기많이하죠?",
          "original_form": "군대갔다온사람들얘기많이하죠?",
          "speaker_id": "1",
          "time": "20210905 18:05"
        },
        {
          "id": "MDRW2100032394.1.17",
          "form": "네..엄청들해요",
          "original_form": "네..엄청들해요",
          "speaker_id": "2",
          "time": "20210905 18:05"
        },
        {
          "id": "MDRW2100032394.1.18",
          "form": "끝도없이하죠",
          "original_form": "끝도없이하죠",
          "speaker_id": "1",
          "time": "20210905 18:06"
        },
        {
          "id": "MDRW2100032394.1.19",
          "form": "네.어쩔땐지겨워요~ㅎ",
          "original_form": "네.어쩔땐지겨워요~ㅎ",
          "speaker_id": "2",
          "time": "20210905 18:06"
        },
        {
          "id": "MDRW2100032394.1.20",
          "form": "군대에서도 운동많이하죠~~",
          "original_form": "군대에서도 운동많이하죠~~",
          "speaker_id": "1",
          "time": "20210905 18:06"
        },
        {
          "id": "MDRW2100032394.1.21",
          "form": "아.운동 많이하셨어요?",
          "original_form": "아.운동 많이하셨어요?",
          "speaker_id": "2",
          "time": "20210905 18:06"
        },
        {
          "id": "MDRW2100032394.1.22",
          "form": "네..계속하는중이예요",
          "original_form": "네..계속하는중이예요",
          "speaker_id": "1",
          "time": "20210905 18:07"
        },
        {
          "id": "MDRW2100032394.1.23",
          "form": "어떤거하시나요?",
          "original_form": "어떤거하시나요?",
          "speaker_id": "2",
          "time": "20210905 18:07"
        },
        {
          "id": "MDRW2100032394.1.24",
          "form": "런닝많이해요",
          "original_form": "런닝많이해요",
          "speaker_id": "1",
          "time": "20210905 18:07"
        },
        {
          "id": "MDRW2100032394.1.25",
          "form": "유산소하시는군요",
          "original_form": "유산소하시는군요",
          "speaker_id": "2",
          "time": "20210905 18:07"
        },
        {
          "id": "MDRW2100032394.1.26",
          "form": "네..다이어트효과 만점",
          "original_form": "네..다이어트효과 만점",
          "speaker_id": "1",
          "time": "20210905 18:07"
        },
        {
          "id": "MDRW2100032394.1.27",
          "form": "저도운동해야겠네요",
          "original_form": "저도운동해야겠네요",
          "speaker_id": "2",
          "time": "20210905 18:07"
        }
      ]
