      "utterance": [
        {
          "id": "MDRW2100030882.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100030882.1.2",
          "form": "어제 오늘 날씨가 참 좋네요ㅎㅎ",
          "original_form": "어제 오늘 날씨가 참 좋네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100030882.1.3",
          "form": "안녕하세요~  수업듣느라  답이 늦었네요.\r\n오늘 날씨진짜좋지요",
          "original_form": "안녕하세요~  수업듣느라  답이 늦었네요.\r\n오늘 날씨진짜좋지요",
          "speaker_id": "2",
          "time": "20210904 16:30"
        },
        {
          "id": "MDRW2100030882.1.4",
          "form": "파란하늘 뭉게구름이 넘 이쁘네요",
          "original_form": "파란하늘 뭉게구름이 넘 이쁘네요",
          "speaker_id": "2",
          "time": "20210904 17:31"
        },
        {
          "id": "MDRW2100030882.1.5",
          "form": "네 날이 진짜 좋아요",
          "original_form": "네 날이 진짜 좋아요",
          "speaker_id": "1",
          "time": "20210904 17:32"
        },
        {
          "id": "MDRW2100030882.1.6",
          "form": "하늘도 파랗고 구름도 뭉게뭉게 선명해요",
          "original_form": "하늘도 파랗고 구름도 뭉게뭉게 선명해요",
          "speaker_id": "1",
          "time": "20210904 17:32"
        },
        {
          "id": "MDRW2100030882.1.7",
          "form": "하늘보면서 멍때리기 좋은날이네요",
          "original_form": "하늘보면서 멍때리기 좋은날이네요",
          "speaker_id": "2",
          "time": "20210904 17:33"
        },
        {
          "id": "MDRW2100030882.1.8",
          "form": "네네 바람도 선선하고",
          "original_form": "네네 바람도 선선하고",
          "speaker_id": "1",
          "time": "20210904 17:34"
        },
        {
          "id": "MDRW2100030882.1.9",
          "form": "내일도 오늘같으면 좋겠네요~~",
          "original_form": "내일도 오늘같으면 좋겠네요~~",
          "speaker_id": "2",
          "time": "20210904 17:37"
        },
        {
          "id": "MDRW2100030882.1.10",
          "form": "네네 계속 날씨가 좋았으면!!",
          "original_form": "네네 계속 날씨가 좋았으면!!",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100030882.1.11",
          "form": "한동안 습해서 힘들었는데 빨래도 잘마르고 좋네요ㅎ",
          "original_form": "한동안 습해서 힘들었는데 빨래도 잘마르고 좋네요ㅎ",
          "speaker_id": "2",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100030882.1.12",
          "form": "네 모처럼 뽀송뽀송한 날이예요",
          "original_form": "네 모처럼 뽀송뽀송한 날이예요",
          "speaker_id": "1",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100030882.1.13",
          "form": "황사 미세먼지 없이 이렇게 맑은하늘이 진짜 소중하네요ㅠ",
          "original_form": "황사 미세먼지 없이 이렇게 맑은하늘이 진짜 소중하네요ㅠ",
          "speaker_id": "2",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100030882.1.14",
          "form": "네 요즘 기온도 딱 좋고요",
          "original_form": "네 요즘 기온도 딱 좋고요",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100030882.1.15",
          "form": "이렇게 날씨좋은날 보면 우리나라도 참 이쁜데",
          "original_form": "이렇게 날씨좋은날 보면 우리나라도 참 이쁜데",
          "speaker_id": "2",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100030882.1.16",
          "form": "네네 살기 좋은 날씨인데",
          "original_form": "네네 살기 좋은 날씨인데",
          "speaker_id": "1",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100030882.1.17",
          "form": "요즘 여름과 겨울은 너무 극단적이라ㅠ",
          "original_form": "요즘 여름과 겨울은 너무 극단적이라ㅠ",
          "speaker_id": "1",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100030882.1.18",
          "form": "봄이 제일 별로인거 같아요..황사의 계절",
          "original_form": "봄이 제일 별로인거 같아요..황사의 계절",
          "speaker_id": "2",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100030882.1.19",
          "form": "저는 봄 좋아하는데",
          "original_form": "저는 봄 좋아하는데",
          "speaker_id": "1",
          "time": "20210904 18:34"
        },
        {
          "id": "MDRW2100030882.1.20",
          "form": "요즘은 황사랑 미세먼지가 너무 심해요ㅜ",
          "original_form": "요즘은 황사랑 미세먼지가 너무 심해요ㅜ",
