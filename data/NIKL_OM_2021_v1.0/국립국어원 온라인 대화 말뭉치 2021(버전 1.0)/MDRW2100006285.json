        {
          "id": "MDRW2100006285.1.1",
          "form": "허재 아들도 많이 나와요",
          "original_form": "허재 아들도 많이 나와요",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006285.1.2",
          "form": "세 명이 같이 많이 나오더라",
          "original_form": "세 명이 같이 많이 나오더라",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006285.1.3",
          "form": "가족끼리 다 하네요",
          "original_form": "가족끼리 다 하네요",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006285.1.4",
          "form": "농구 많이 보라고 광고하나봐",
          "original_form": "농구 많이 보라고 광고하나봐",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006285.1.5",
          "form": "농구 광고때문일까여",
          "original_form": "농구 광고때문일까여",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006285.1.6",
          "form": "지금 해방 타운 에도 나오네",
          "original_form": "지금 해방 타운 에도 나오네",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006285.1.7",
          "form": "말로는 그렇다고 하더라고요 ㅎㅎ",
          "original_form": "말로는 그렇다고 하더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006285.1.8",
          "form": "근데 또 저 사람은 누구니",
          "original_form": "근데 또 저 사람은 누구니",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006285.1.9",
          "form": "특전사래요",
          "original_form": "특전사래요",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006285.1.10",
          "form": "쟤는 연예인이니",
          "original_form": "쟤는 연예인이니",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006285.1.11",
          "form": "저도 처음봐요",
          "original_form": "저도 처음봐요",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006285.1.12",
          "form": "요즘은 모르는 사람이 티비에 많이 나와",
          "original_form": "요즘은 모르는 사람이 티비에 많이 나와",
