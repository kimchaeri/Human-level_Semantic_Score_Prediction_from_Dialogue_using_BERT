      "utterance": [
        {
          "id": "MDRW2100022812.1.1",
          "form": "반려동물 있어요?",
          "original_form": "반려동물 있어요?",
          "speaker_id": "1",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100022812.1.2",
          "form": "아니요 ㅜㅜ 있으신가요?",
          "original_form": "아니요 ㅜㅜ 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 18:08"
        },
        {
          "id": "MDRW2100022812.1.3",
          "form": "네 저희집에 강아지 한마리 키워요",
          "original_form": "네 저희집에 강아지 한마리 키워요",
          "speaker_id": "1",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022812.1.4",
          "form": "오 어떤 종이에요?",
          "original_form": "오 어떤 종이에요?",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022812.1.5",
          "form": "미니핀입니다",
          "original_form": "미니핀입니다",
          "speaker_id": "1",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022812.1.6",
          "form": "헐 귀엽겠어요 ㅜㅜ 몇살이에요?",
          "original_form": "헐 귀엽겠어요 ㅜㅜ 몇살이에요?",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022812.1.7",
          "form": "12살요\r\n님도 강아지 좋아하세요?",
          "original_form": "12살요\r\n님도 강아지 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022812.1.8",
          "form": "네 좋아해요 ㅎㅎ 오래키우셨군요??",
          "original_form": "네 좋아해요 ㅎㅎ 오래키우셨군요??",
          "speaker_id": "2",
          "time": "20210829 18:21"
        },
        {
          "id": "MDRW2100022812.1.9",
          "form": "네.. 아이들이 좋아해서 어쩔수없이요 ㅎ",
          "original_form": "네.. 아이들이 좋아해서 어쩔수없이요 ㅎ",
          "speaker_id": "1",
          "time": "20210829 18:22"
        },
        {
          "id": "MDRW2100022812.1.10",
          "form": "아 ! 혹시 아이들이랑 같이 자랐나요 ㅎㅎ ?",
          "original_form": "아 ! 혹시 아이들이랑 같이 자랐나요 ㅎㅎ ?",
          "speaker_id": "2",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022812.1.11",
          "form": "네 애들이 초등학생일때부터 같이요\r\n지금은 아이들도 성인이 됏구요 ㅎ",
          "original_form": "네 애들이 초등학생일때부터 같이요\r\n지금은 아이들도 성인이 됏구요 ㅎ",
          "speaker_id": "1",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022812.1.12",
          "form": "헐 ㅎㅎ 같이 키운거나 다름 없군요 ㅎㅎ !",
          "original_form": "헐 ㅎㅎ 같이 키운거나 다름 없군요 ㅎㅎ !",
          "speaker_id": "2",
          "time": "20210829 18:29"
        },
        {
          "id": "MDRW2100022812.1.13",
          "form": "네 ㅎㅎ 우리집 막둥이예요",
          "original_form": "네 ㅎㅎ 우리집 막둥이예요",
          "speaker_id": "1",
          "time": "20210829 18:30"
        },
        {
          "id": "MDRW2100022812.1.14",
          "form": "엄청 손 많이 가셨겠어요 어린 아이들에 어린 강아지까지 ㅎㅎ",
          "original_form": "엄청 손 많이 가셨겠어요 어린 아이들에 어린 강아지까지 ㅎㅎ",
          "speaker_id": "2",
