      "utterance": [
        {
          "id": "MDRW2100018906.1.1",
          "form": "안녕하세요 울산 여행 해보셨어요?",
          "original_form": "안녕하세요 울산 여행 해보셨어요?",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018906.1.2",
          "form": "아니요 울산 좋나요?",
          "original_form": "아니요 울산 좋나요?",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018906.1.3",
          "form": "울산 좋은 도시에요",
          "original_form": "울산 좋은 도시에요",
          "speaker_id": "1",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018906.1.4",
          "form": "뭐가 있나요?",
          "original_form": "뭐가 있나요?",
          "speaker_id": "2",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018906.1.5",
          "form": "태화강변의 십리대밭길은 국가정원으로 선정되었고",
          "original_form": "태화강변의 십리대밭길은 국가정원으로 선정되었고",
          "speaker_id": "1",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018906.1.6",
          "form": "신불산 억새평원도 아름답습니다",
          "original_form": "신불산 억새평원도 아름답습니다",
          "speaker_id": "1",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018906.1.7",
          "form": "어떤 스타일의 여행을 선호 하시나요",
          "original_form": "어떤 스타일의 여행을 선호 하시나요",
          "speaker_id": "1",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018906.1.8",
          "form": "울산사세요?멀어서 ㅜ",
          "original_form": "울산사세요?멀어서 ㅜ",
          "speaker_id": "2",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018906.1.9",
          "form": "네 울산 살고 있어요",
          "original_form": "네 울산 살고 있어요",
          "speaker_id": "1",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018906.1.10",
          "form": "직장때문에 정착했는데 살기 좋은 도시더라고요",
          "original_form": "직장때문에 정착했는데 살기 좋은 도시더라고요",
          "speaker_id": "1",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018906.1.11",
          "form": "여행은 더 좋고",
          "original_form": "여행은 더 좋고",
          "speaker_id": "1",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018906.1.12",
          "form": "아지역사랑이 대단하시네요 ㅎㅎ",
          "original_form": "아지역사랑이 대단하시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018906.1.13",
          "form": "사실 억지로 정붙이고 살고있어요",
          "original_form": "사실 억지로 정붙이고 살고있어요",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018906.1.14",
          "form": "이직하려다 실패해서",
          "original_form": "이직하려다 실패해서",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018906.1.15",
          "form": "아 ㅜㅜ 저런 안타깝네요",
          "original_form": "아 ㅜㅜ 저런 안타깝네요",
          "speaker_id": "2",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018906.1.16",
          "form": "여행지 추천해주세요",
          "original_form": "여행지 추천해주세요",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018906.1.17",
          "form": "글쎄요 컨셉이 ㅋ 너무멀어서 ㅋ",
          "original_form": "글쎄요 컨셉이 ㅋ 너무멀어서 ㅋ",
          "speaker_id": "2",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018906.1.18",
          "form": "어디인데요?",
          "original_form": "어디인데요?",
          "speaker_id": "1",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018906.1.19",
          "form": "인천요",
          "original_form": "인천요",
          "speaker_id": "2",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100018906.1.20",
          "form": "아 인천 가본지 정말 오래 되었네요",
          "original_form": "아 인천 가본지 정말 오래 되었네요",
          "speaker_id": "1",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100018906.1.21",
          "form": "십몇년전 다녀왔었는데",
          "original_form": "십몇년전 다녀왔었는데",
          "speaker_id": "1",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100018906.1.22",
          "form": "ㅎㅎ공항?ㅋ",
          "original_form": "ㅎㅎ공항?ㅋ",
          "speaker_id": "2",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100018906.1.23",
          "form": "아 생각해보니까 공항 포함하면 그렇게 안된다",
          "original_form": "아 생각해보니까 공항 포함하면 그렇게 안된다",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100018906.1.24",
          "form": "공항 제외하고 놀러간게 십몇년전",
          "original_form": "공항 제외하고 놀러간게 십몇년전",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100018906.1.25",
          "form": "아 ㅎㅎㅎ 인천 까지 놀러오긴 쉽지 않으실 듯요",
          "original_form": "아 ㅎㅎㅎ 인천 까지 놀러오긴 쉽지 않으실 듯요",
          "speaker_id": "2",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100018906.1.26",
          "form": "대학시절에 갔었죠...",
          "original_form": "대학시절에 갔었죠...",
          "speaker_id": "1",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100018906.1.27",
          "form": "그게 언제인지",
          "original_form": "그게 언제인지",
          "speaker_id": "1",
          "time": "20210827 16:25"
        }
      ]
    }
