        {
          "id": "MDRW2100016212.1.1",
          "form": "점심 뭐드세요?",
          "original_form": "점심 뭐드세요?",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016212.1.2",
          "form": "집밥 먹어야죠",
          "original_form": "집밥 먹어야죠",
          "speaker_id": "2",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016212.1.3",
          "form": "맛있는 거 먹고싶어요",
          "original_form": "맛있는 거 먹고싶어요",
          "speaker_id": "1",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016212.1.4",
          "form": "맛난 거 시켜드세요",
          "original_form": "맛난 거 시켜드세요",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016212.1.5",
          "form": "생활비가...",
          "original_form": "생활비가...",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016212.1.6",
          "form": "아ㅠㅠ",
          "original_form": "아ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016212.1.7",
          "form": "유튜브채널에 음식조리법 쉽게 나와있어요",
          "original_form": "유튜브채널에 음식조리법 쉽게 나와있어요",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016212.1.8",
          "form": "네ㅜㅜ 감사합니다",
          "original_form": "네ㅜㅜ 감사합니다",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016212.1.9",
          "form": "아침드셨어요?",
          "original_form": "아침드셨어요?",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016212.1.10",
          "form": "아니용! 늦잠잤어요",
          "original_form": "아니용! 늦잠잤어요",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016212.1.11",
          "form": "전 아아랑 과일 먹었어요",
          "original_form": "전 아아랑 과일 먹었어요",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016212.1.12",
          "form": "어떤 과일 드셨어요?",
          "original_form": "어떤 과일 드셨어요?",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016212.1.13",
          "form": "초록사과랑 복숭아요",
          "original_form": "초록사과랑 복숭아요",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016212.1.14",
          "form": "우와 사과 요즘 맛있나요?",
          "original_form": "우와 사과 요즘 맛있나요?",
          "speaker_id": "1",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016212.1.15",
          "form": "사과진짜 제철이에요",
          "original_form": "사과진짜 제철이에요",
          "speaker_id": "2",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016212.1.16",
          "form": "꼭드세요",
          "original_form": "꼭드세요",
          "speaker_id": "2",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016212.1.17",
          "form": "꼭 먹어볼게요ㅋㅋ",
          "original_form": "꼭 먹어볼게요ㅋㅋ",
