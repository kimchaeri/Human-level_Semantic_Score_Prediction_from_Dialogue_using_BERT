      "utterance": [
        {
          "id": "MDRW2100006801.1.1",
          "form": "반려동물 키워보신 적 있으신지요",
          "original_form": "반려동물 키워보신 적 있으신지요",
          "speaker_id": "1",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100006801.1.2",
          "form": "아주 예전 어릴때요",
          "original_form": "아주 예전 어릴때요",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100006801.1.3",
          "form": "강아지 키웠나요?",
          "original_form": "강아지 키웠나요?",
          "speaker_id": "1",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100006801.1.4",
          "form": "네 강아지 키웠어요",
          "original_form": "네 강아지 키웠어요",
          "speaker_id": "2",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100006801.1.5",
          "form": "참 귀여웠겠습니다",
          "original_form": "참 귀여웠겠습니다",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100006801.1.6",
          "form": "이름이 뭐였나요?",
          "original_form": "이름이 뭐였나요?",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100006801.1.7",
          "form": "동생 강아지인데 이름이 name3ㅋ",
          "original_form": "동생 강아지인데 이름이 &name3&ㅋ",
          "speaker_id": "2",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100006801.1.8",
          "form": "귀여웠어요",
          "original_form": "귀여웠어요",
          "speaker_id": "2",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100006801.1.9",
          "form": "name3",
          "original_form": "&name3&",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100006801.1.10",
          "form": "귀엽군요",
          "original_form": "귀엽군요",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100006801.1.11",
          "form": "지금은 하늘나라에 ㅠ",
          "original_form": "지금은 하늘나라에 ㅠ",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100006801.1.12",
          "form": "무지개다리를 건넜군요",
          "original_form": "무지개다리를 건넜군요",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100006801.1.13",
          "form": "반려동물은 식구 같아서 참 헤어짐이 힘들죠",
          "original_form": "반려동물은 식구 같아서 참 헤어짐이 힘들죠",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100006801.1.14",
          "form": "저는 그래서 물고기 키웁니다",
          "original_form": "저는 그래서 물고기 키웁니다",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100006801.1.15",
          "form": "물고기 키우는건 편한가요?",
          "original_form": "물고기 키우는건 편한가요?",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100006801.1.16",
          "form": "일단 어항에만 있으니 집을 어지르지 않아요",
          "original_form": "일단 어항에만 있으니 집을 어지르지 않아요",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100006801.1.17",
          "form": "그리고 조용해요",
          "original_form": "그리고 조용해요",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100006801.1.18",
          "form": "밥 주고 나가면 자기들끼리 헤엄치며 잘 놀아요",
          "original_form": "밥 주고 나가면 자기들끼리 헤엄치며 잘 놀아요",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100006801.1.19",
          "form": "거의 관상용이네요",
          "original_form": "거의 관상용이네요",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100006801.1.20",
          "form": "관상용인데 또 은근히 정이 갑니다",
          "original_form": "관상용인데 또 은근히 정이 갑니다",
          "speaker_id": "1",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100006801.1.21",
          "form": "밥주려고 다가 가면 알고 막 헤엄쳐오는데 꼭 강아들들 달려오는 것과 비슷한 느낌이 들어요",
          "original_form": "밥주려고 다가 가면 알고 막 헤엄쳐오는데 꼭 강아들들 달려오는 것과 비슷한 느낌이 들어요",
          "speaker_id": "1",
          "time": "20210908 09:58"
        }
      ]
