      "utterance": [
        {
          "id": "MDRW2100040974.1.1",
          "form": "좋아하기는 하는데 흰살생선을 더 좋아해요!",
          "original_form": "좋아하기는 하는데 흰살생선을 더 좋아해요!",
          "speaker_id": "2",
          "time": "20210905 14:16"
        },
        {
          "id": "MDRW2100040974.1.2",
          "form": "연어 케이퍼랑 홀스래디시랑 먹으면 너무 맛있죠 ㅋㅋ",
          "original_form": "연어 케이퍼랑 홀스래디시랑 먹으면 너무 맛있죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:16"
        },
        {
          "id": "MDRW2100040974.1.3",
          "form": "회 너무 좋아요ㅠㅠ",
          "original_form": "회 너무 좋아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100040974.1.4",
          "form": "근데 회 못먹는 사람이 은근히 많더라구요",
          "original_form": "근데 회 못먹는 사람이 은근히 많더라구요",
          "speaker_id": "1",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100040974.1.5",
          "form": "전 바닷가쪽에 살아서 ㅋㅋㅋ 주변인대부분은 회를 먹는거같아요 ㅋㅋ",
          "original_form": "전 바닷가쪽에 살아서 ㅋㅋㅋ 주변인대부분은 회를 먹는거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:25"
        },
        {
          "id": "MDRW2100040974.1.6",
          "form": "바닷가 근처에 사신다니 부러워요ㅠㅠㅠ",
          "original_form": "바닷가 근처에 사신다니 부러워요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 19:17"
        },
        {
          "id": "MDRW2100040974.1.7",
          "form": "수산물 많이 드시겠어요!",
          "original_form": "수산물 많이 드시겠어요!",
          "speaker_id": "1",
          "time": "20210905 19:17"
        },
        {
          "id": "MDRW2100040974.1.8",
          "form": "근데 수산물은 또 생선만먹어욬ㅋㅋㅋㅋ",
          "original_form": "근데 수산물은 또 생선만먹어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:18"
        },
        {
          "id": "MDRW2100040974.1.9",
          "form": "조개류 갑각류 오징어와친구들 다 안먹습니다....",
          "original_form": "조개류 갑각류 오징어와친구들 다 안먹습니다....",
          "speaker_id": "2",
          "time": "20210905 19:19"
        },
        {
          "id": "MDRW2100040974.1.10",
          "form": "아 그러시구나!!",
          "original_form": "아 그러시구나!!",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100040974.1.11",
          "form": "저는 다 좋아해요ㅎㅎ",
          "original_form": "저는 다 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100040974.1.12",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100040974.1.13",
          "form": "저녁 컵라면 먹었어요 ㅎㅎㅎ",
          "original_form": "저녁 컵라면 먹었어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 23:43"
        },
        {
          "id": "MDRW2100040974.1.14",
          "form": "오 어디 라면 드셨나요?!",
          "original_form": "오 어디 라면 드셨나요?!",
          "speaker_id": "1",
          "time": "20210906 12:30"
        },
        {
          "id": "MDRW2100040974.1.15",
          "form": "농심인가에서 나온 쌀국수요 ㅋㅋㅋ",
          "original_form": "농심인가에서 나온 쌀국수요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:32"
        },
        {
          "id": "MDRW2100040974.1.16",
          "form": "어떤 맛일지 궁금하네요!!",
          "original_form": "어떤 맛일지 궁금하네요!!",
          "speaker_id": "1",
          "time": "20210906 12:32"
        },
        {
          "id": "MDRW2100040974.1.17",
          "form": "쌀국수 좋아하시나요?",
          "original_form": "쌀국수 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210906 12:32"
        },
        {
          "id": "MDRW2100040974.1.18",
          "form": "네네!! 쌀국수좋아해요 ㅋㅋㅋㅋ",
          "original_form": "네네!! 쌀국수좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:33"
        },
        {
          "id": "MDRW2100040974.1.19",
          "form": "저두요ㅎㅎ",
          "original_form": "저두요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100040974.1.20",
          "form": "분짜도 좋아하시나요??!",
          "original_form": "분짜도 좋아하시나요??!",
          "speaker_id": "1",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100040974.1.21",
          "form": "헐 진짜 좋아해요 ㅜㅜ 님이 말해서 급 땡기네요 ㅜㅜ 분짜 밀키트이런거 괜찮으려나요",
          "original_form": "헐 진짜 좋아해요 ㅜㅜ 님이 말해서 급 땡기네요 ㅜㅜ 분짜 밀키트이런거 괜찮으려나요",
          "speaker_id": "2",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100040974.1.22",
          "form": "오 분짜 밀키트도 판매하나요??",
          "original_form": "오 분짜 밀키트도 판매하나요??",
