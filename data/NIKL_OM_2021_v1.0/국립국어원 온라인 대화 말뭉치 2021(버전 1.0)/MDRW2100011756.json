      "utterance": [
        {
          "id": "MDRW2100011756.1.1",
          "form": "이렇게보니 현 사회가 엉망진창이긴하네요",
          "original_form": "이렇게보니 현 사회가 엉망진창이긴하네요",
          "speaker_id": "2",
          "time": "20210824 15:44"
        },
        {
          "id": "MDRW2100011756.1.2",
          "form": "맞아요 정말..",
          "original_form": "맞아요 정말..",
          "speaker_id": "1",
          "time": "20210824 15:45"
        },
        {
          "id": "MDRW2100011756.1.3",
          "form": "사실 전 여성우월주의여도 임신이 싫어서",
          "original_form": "사실 전 여성우월주의여도 임신이 싫어서",
          "speaker_id": "1",
          "time": "20210824 15:45"
        },
        {
          "id": "MDRW2100011756.1.4",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "2",
          "time": "20210824 15:45"
        },
        {
          "id": "MDRW2100011756.1.5",
          "form": "결혼 피할거같은데 현 사회는 충공깽..",
          "original_form": "결혼 피할거같은데 현 사회는 충공깽..",
          "speaker_id": "1",
          "time": "20210824 15:45"
        },
        {
          "id": "MDRW2100011756.1.6",
          "form": "솔직히 성병 걱정도 되고요",
          "original_form": "솔직히 성병 걱정도 되고요",
          "speaker_id": "2",
          "time": "20210824 15:46"
        },
        {
          "id": "MDRW2100011756.1.7",
          "form": "자궁경부암",
          "original_form": "자궁경부암",
          "speaker_id": "2",
          "time": "20210824 15:46"
        },
        {
          "id": "MDRW2100011756.1.8",
          "form": "저도요! 신체적으로 너무 손해에요",
          "original_form": "저도요! 신체적으로 너무 손해에요",
          "speaker_id": "1",
          "time": "20210824 15:46"
        },
        {
          "id": "MDRW2100011756.1.9",
          "form": "가다실 맞으셨나요",
          "original_form": "가다실 맞으셨나요",
          "speaker_id": "1",
          "time": "20210824 15:46"
        },
        {
          "id": "MDRW2100011756.1.10",
          "form": "아뇨 아직 안맞았어요",
          "original_form": "아뇨 아직 안맞았어요",
          "speaker_id": "2",
          "time": "20210824 15:46"
        },
        {
          "id": "MDRW2100011756.1.11",
          "form": "가다실 안그래도 가격 오르는데",
          "original_form": "가다실 안그래도 가격 오르는데",
          "speaker_id": "2",
          "time": "20210824 15:47"
        },
        {
          "id": "MDRW2100011756.1.12",
          "form": "그쵸 가다실 너무비싸요 ㅜㅜ 저도 못맞앗어요",
          "original_form": "그쵸 가다실 너무비싸요 ㅜㅜ 저도 못맞앗어요",
          "speaker_id": "1",
          "time": "20210824 15:47"
