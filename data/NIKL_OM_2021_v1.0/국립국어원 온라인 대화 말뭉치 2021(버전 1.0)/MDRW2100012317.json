      "utterance": [
        {
          "id": "MDRW2100012317.1.1",
          "form": "안좋은데...",
          "original_form": "안좋은데...",
          "speaker_id": "2",
          "time": "20210824 21:09"
        },
        {
          "id": "MDRW2100012317.1.2",
          "form": "무슨일이야?",
          "original_form": "무슨일이야?",
          "speaker_id": "1",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100012317.1.3",
          "form": "그 오빠가 나 좋아하는 줄 알았는데 아니였나봐ㅠ",
          "original_form": "그 오빠가 나 좋아하는 줄 알았는데 아니였나봐ㅠ",
          "speaker_id": "2",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100012317.1.4",
          "form": "헐 설마 고백한건 아니지?",
          "original_form": "헐 설마 고백한건 아니지?",
          "speaker_id": "1",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100012317.1.5",
          "form": "으응 고백은 안해찌..",
          "original_form": "으응 고백은 안해찌..",
          "speaker_id": "2",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100012317.1.6",
          "form": "그럼 잘 될 수 있는거 아니야?",
          "original_form": "그럼 잘 될 수 있는거 아니야?",
          "speaker_id": "1",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012317.1.7",
          "form": "그 오빠가 나 헷갈리게해ㅜ",
          "original_form": "그 오빠가 나 헷갈리게해ㅜ",
          "speaker_id": "2",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012317.1.8",
          "form": "예를 들면?",
          "original_form": "예를 들면?",
          "speaker_id": "1",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012317.1.9",
          "form": "아 구럼 내가 여러 얘기해볼테니까 보고 알려줄래?",
          "original_form": "아 구럼 내가 여러 얘기해볼테니까 보고 알려줄래?",
          "speaker_id": "2",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100012317.1.10",
          "form": "웅 말해보ㅓ",
          "original_form": "웅 말해보ㅓ",
          "speaker_id": "1",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100012317.1.11",
          "form": "말해봐",
          "original_form": "말해봐",
          "speaker_id": "1",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100012317.1.12",
          "form": "나랑 얘기하다가 우리 근데 무슨 사이야 라고 물어봤어!",
          "original_form": "나랑 얘기하다가 우리 근데 무슨 사이야 라고 물어봤어!",
          "speaker_id": "2",
          "time": "20210824 21:13"
        },
        {
          "id": "MDRW2100012317.1.13",
          "form": "그랬더니 뭐래?",
          "original_form": "그랬더니 뭐래?",
          "speaker_id": "1",
          "time": "20210824 21:13"
        },
        {
          "id": "MDRW2100012317.1.14",
          "form": "내가 당황해서 친한 친구 사이죠 라고해썽",
          "original_form": "내가 당황해서 친한 친구 사이죠 라고해썽",
          "speaker_id": "2",
          "time": "20210824 21:14"
        },
        {
          "id": "MDRW2100012317.1.15",
          "form": "웅 맞잖아",
          "original_form": "웅 맞잖아",
          "speaker_id": "1",
          "time": "20210824 21:14"
        },
        {
          "id": "MDRW2100012317.1.16",
          "form": "근데 이거 떠본 거 아냐?",
          "original_form": "근데 이거 떠본 거 아냐?",
          "speaker_id": "2",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012317.1.17",
          "form": "맞아 그런거같은데 그래서 뭐라햇는데?",
          "original_form": "맞아 그런거같은데 그래서 뭐라햇는데?",
          "speaker_id": "1",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012317.1.18",
          "form": "글쿤 이래쎀ㅋㅋㅋㅋ",
          "original_form": "글쿤 이래쎀ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012317.1.19",
          "form": "아 그리고 내가 오빠한테  옷 얘기했거든?",
          "original_form": "아 그리고 내가 오빠한테  옷 얘기했거든?",
          "speaker_id": "2",
          "time": "20210824 21:16"
        },
        {
          "id": "MDRW2100012317.1.20",
          "form": "무슨 옷?",
          "original_form": "무슨 옷?",
          "speaker_id": "1",
          "time": "20210824 21:16"
        },
        {
          "id": "MDRW2100012317.1.21",
          "form": "오빠가 맨날 똑같은 셔츠만 입어서 내가 왜 그것만 입어요라고 해쓰",
          "original_form": "오빠가 맨날 똑같은 셔츠만 입어서 내가 왜 그것만 입어요라고 해쓰",
          "speaker_id": "2",
          "time": "20210824 21:16"
        },
        {
          "id": "MDRW2100012317.1.22",
          "form": "그랬더니 설마 옷 바꿔입었어?",
          "original_form": "그랬더니 설마 옷 바꿔입었어?",
          "speaker_id": "1",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100012317.1.23",
          "form": "오빠가 멋있는 셔츠 입었어",
          "original_form": "오빠가 멋있는 셔츠 입었어",
          "speaker_id": "2",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100012317.1.24",
          "form": "오 뭐야 너 좋아하는거아냐?",
          "original_form": "오 뭐야 너 좋아하는거아냐?",
          "speaker_id": "1",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100012317.1.25",
          "form": "ㅎㅎ그런가?",
          "original_form": "ㅎㅎ그런가?",
          "speaker_id": "2",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100012317.1.26",
          "form": "응!!",
          "original_form": "응!!",
          "speaker_id": "1",
          "time": "20210824 21:21"
        }
      ]
    }
  ]
