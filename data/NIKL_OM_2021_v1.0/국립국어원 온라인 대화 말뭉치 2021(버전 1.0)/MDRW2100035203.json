      "utterance": [
        {
          "id": "MDRW2100035203.1.1",
          "form": "5위 레드벨벳 슬기란다",
          "original_form": "5위 레드벨벳 슬기란다",
          "speaker_id": "2",
          "time": "20210829 11:44"
        },
        {
          "id": "MDRW2100035203.1.2",
          "form": "의외의 인물이네",
          "original_form": "의외의 인물이네",
          "speaker_id": "1",
          "time": "20210829 11:44"
        },
        {
          "id": "MDRW2100035203.1.3",
          "form": "성난 복근이란다 ㅋㅋ",
          "original_form": "성난 복근이란다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:44"
        },
        {
          "id": "MDRW2100035203.1.4",
          "form": "무슨 또 성난 복근이야",
          "original_form": "무슨 또 성난 복근이야",
          "speaker_id": "1",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100035203.1.5",
          "form": "슬기 검색하면 자동으로 복근이 연관검색어 나온데",
          "original_form": "슬기 검색하면 자동으로 복근이 연관검색어 나온데",
          "speaker_id": "2",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100035203.1.6",
          "form": "아 글쿤ㅋㅋ 처음 알았네",
          "original_form": "아 글쿤ㅋㅋ 처음 알았네",
          "speaker_id": "1",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100035203.1.7",
          "form": "안무로 복근을 만들었다는데",
          "original_form": "안무로 복근을 만들었다는데",
          "speaker_id": "2",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100035203.1.8",
          "form": "모든 걸그룹이 복근이 있어야 되는거 아님? ㅋ",
          "original_form": "모든 걸그룹이 복근이 있어야 되는거 아님? ㅋ",
          "speaker_id": "1",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100035203.1.9",
          "form": "격한 안무로 잔근육이 생겼단다",
          "original_form": "격한 안무로 잔근육이 생겼단다",
          "speaker_id": "2",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100035203.1.10",
          "form": "아닌거 같은데ㅋㅋ",
          "original_form": "아닌거 같은데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100035203.1.11",
          "form": "게스트로 나온 남자 아이돌도 안무로 생긴다고 하네 ㅋㅋ",
          "original_form": "게스트로 나온 남자 아이돌도 안무로 생긴다고 하네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100035203.1.12",
          "form": "안 생기는 애들은 뭐고?",
          "original_form": "안 생기는 애들은 뭐고?",
          "speaker_id": "1",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100035203.1.13",
          "form": "3위발표 옷찟 퍼포먼스 한 마마무 솔라",
          "original_form": "3위발표 옷찟 퍼포먼스 한 마마무 솔라",
