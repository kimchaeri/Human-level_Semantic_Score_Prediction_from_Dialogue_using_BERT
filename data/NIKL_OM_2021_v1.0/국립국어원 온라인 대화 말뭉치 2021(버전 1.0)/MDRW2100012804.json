      "utterance": [
        {
          "id": "MDRW2100012804.1.1",
          "form": "졸리면 주무셔요~^^ 낼또하묜되니깐요ㄹ",
          "original_form": "졸리면 주무셔요~^^ 낼또하묜되니깐요ㄹ",
          "speaker_id": "2",
          "time": "20210825 01:36"
        },
        {
          "id": "MDRW2100012804.1.2",
          "form": "술드셔서~^^",
          "original_form": "술드셔서~^^",
          "speaker_id": "2",
          "time": "20210825 01:36"
        },
        {
          "id": "MDRW2100012804.1.3",
          "form": "아니ㅎ과음 안했다니까요ㅎ",
          "original_form": "아니ㅎ과음 안했다니까요ㅎ",
          "speaker_id": "1",
          "time": "20210825 01:37"
        },
        {
          "id": "MDRW2100012804.1.4",
          "form": "아ㅎㅎ 전 피곤하실까봐ㅎㅎ",
          "original_form": "아ㅎㅎ 전 피곤하실까봐ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:39"
        },
        {
          "id": "MDRW2100012804.1.5",
          "form": "그럼 술이야기말고ㅎ 좋아하는 음식은요?",
          "original_form": "그럼 술이야기말고ㅎ 좋아하는 음식은요?",
          "speaker_id": "2",
          "time": "20210825 01:39"
        },
        {
          "id": "MDRW2100012804.1.6",
          "form": "회와 삼겹살을 제외하면...",
          "original_form": "회와 삼겹살을 제외하면...",
          "speaker_id": "1",
          "time": "20210825 01:40"
        },
        {
          "id": "MDRW2100012804.1.7",
          "form": "매운 거요. 마라탕 같은.",
          "original_form": "매운 거요. 마라탕 같은.",
          "speaker_id": "1",
          "time": "20210825 01:40"
        },
        {
          "id": "MDRW2100012804.1.8",
          "form": "마라탕 좋아하시는군요ㅎㅎ",
          "original_form": "마라탕 좋아하시는군요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:41"
        },
        {
          "id": "MDRW2100012804.1.9",
          "form": "호불호가 강해서ㄹㄹ",
          "original_form": "호불호가 강해서ㄹㄹ",
          "speaker_id": "2",
          "time": "20210825 01:41"
        },
        {
          "id": "MDRW2100012804.1.10",
          "form": "저같은경우는 조금 연하면 좋겟더라구요ㅎ 너무쎄면 못먹겟어요ㅎㅎ",
          "original_form": "저같은경우는 조금 연하면 좋겟더라구요ㅎ 너무쎄면 못먹겟어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:42"
        },
        {
          "id": "MDRW2100012804.1.11",
          "form": "네 대부분 호불호가  심한 음식은 제게는 호예요",
          "original_form": "네 대부분 호불호가  심한 음식은 제게는 호예요",
          "speaker_id": "1",
          "time": "20210825 01:42"
        },
        {
          "id": "MDRW2100012804.1.12",
          "form": "아 그럼 백탕같은게 낫겠어요",
          "original_form": "아 그럼 백탕같은게 낫겠어요",
          "speaker_id": "1",
          "time": "20210825 01:42"
        },
        {
          "id": "MDRW2100012804.1.13",
          "form": "막 그러시는건 아니죠? 따로 소스를 가지고 다니시거나?",
          "original_form": "막 그러시는건 아니죠? 따로 소스를 가지고 다니시거나?",
          "speaker_id": "2",
          "time": "20210825 01:45"
        },
        {
          "id": "MDRW2100012804.1.14",
          "form": "아니ㅋ 그 정도는 아녜요",
          "original_form": "아니ㅋ 그 정도는 아녜요",
          "speaker_id": "1",
          "time": "20210825 01:45"
        },
        {
          "id": "MDRW2100012804.1.15",
          "form": "마라샹궈 소스 사두고 귀찮아서 한번도 요리 안해봤네요",
          "original_form": "마라샹궈 소스 사두고 귀찮아서 한번도 요리 안해봤네요",
          "speaker_id": "1",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100012804.1.16",
          "form": "ㅎㅎ 매운거는 저도 잘먹는데",
          "original_form": "ㅎㅎ 매운거는 저도 잘먹는데",
          "speaker_id": "2",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100012804.1.17",
          "form": "그보다 더한사람이 있어서 같이 밥먹는데",
          "original_form": "그보다 더한사람이 있어서 같이 밥먹는데",
          "speaker_id": "2",
          "time": "20210825 01:47"
        },
        {
          "id": "MDRW2100012804.1.18",
          "form": "핵붉닭소쓰를 듬뿍 뿌려먹더라구요ㅎ 그래서 제가 그럴바엔 캡사이신을 뿌리고먹어라 했었어요ㅎㅎ",
          "original_form": "핵붉닭소쓰를 듬뿍 뿌려먹더라구요ㅎ 그래서 제가 그럴바엔 캡사이신을 뿌리고먹어라 했었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:47"
        },
        {
          "id": "MDRW2100012804.1.19",
          "form": "근데... 가방에 있다고 나중에 안매우면 뿌리겟다고 했던 기억이있네요ㅎㅎㅎ",
          "original_form": "근데... 가방에 있다고 나중에 안매우면 뿌리겟다고 했던 기억이있네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:48"
        },
        {
          "id": "MDRW2100012804.1.20",
          "form": "ㄷㄷㄷ 그래서 그런 질문을 하셨군요",
          "original_form": "ㄷㄷㄷ 그래서 그런 질문을 하셨군요",
          "speaker_id": "1",
          "time": "20210825 01:49"
        },
        {
          "id": "MDRW2100012804.1.21",
          "form": "아니 매워야 맛있는게 있고 담백해야 맛있는 게 있는데ㅜ",
          "original_form": "아니 매워야 맛있는게 있고 담백해야 맛있는 게 있는데ㅜ",
          "speaker_id": "1",
          "time": "20210825 01:49"
        },
        {
          "id": "MDRW2100012804.1.22",
          "form": "맞아요ㅎㅎㅎ",
          "original_form": "맞아요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:49"
        },
        {
          "id": "MDRW2100012804.1.23",
          "form": "잘 아시네요ㅎ음식에는 조리법이 다있는거잖아요ㅎㅎ",
          "original_form": "잘 아시네요ㅎ음식에는 조리법이 다있는거잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:50"
        },
        {
          "id": "MDRW2100012804.1.24",
          "form": "그럼 거의 배달만 시켜드셔요? 나만의 조리법은 없으세요ㅡ",
          "original_form": "그럼 거의 배달만 시켜드셔요? 나만의 조리법은 없으세요ㅡ",
          "speaker_id": "2",
          "time": "20210825 01:50"
        },
        {
          "id": "MDRW2100012804.1.25",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210825 01:50"
        },
        {
          "id": "MDRW2100012804.1.26",
          "form": "나만의 조리법은 대부분 라면이죠ㅎ",
          "original_form": "나만의 조리법은 대부분 라면이죠ㅎ",
          "speaker_id": "1",
          "time": "20210825 01:51"
        },
        {
          "id": "MDRW2100012804.1.27",
          "form": "기본에 충실해야해여",
          "original_form": "기본에 충실해야해여",
          "speaker_id": "2",
          "time": "20210825 01:51"
        },
        {
          "id": "MDRW2100012804.1.28",
          "form": "요리는",
          "original_form": "요리는",
          "speaker_id": "2",
          "time": "20210825 01:51"
        },
        {
          "id": "MDRW2100012804.1.29",
          "form": "라면이 요리의 기본 중에 기본ㅎ",
          "original_form": "라면이 요리의 기본 중에 기본ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:52"
        },
        {
          "id": "MDRW2100012804.1.30",
          "form": "파기름과 고추기름을 내요",
          "original_form": "파기름과 고추기름을 내요",
          "speaker_id": "1",
          "time": "20210825 01:52"
        },
        {
          "id": "MDRW2100012804.1.31",
          "form": "제대로 드시네여ㅎㅎ",
          "original_form": "제대로 드시네여ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:52"
        },
        {
          "id": "MDRW2100012804.1.32",
          "form": "그리고요?ㅎ",
          "original_form": "그리고요?ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:52"
        },
        {
          "id": "MDRW2100012804.1.33",
          "form": "거기에 라면을 끓이는게 시작이죠ㅎ",
          "original_form": "거기에 라면을 끓이는게 시작이죠ㅎ",
          "speaker_id": "1",
          "time": "20210825 01:52"
        },
        {
          "id": "MDRW2100012804.1.34",
          "form": "황제라면 끓여먹어보셨어요?",
          "original_form": "황제라면 끓여먹어보셨어요?",
          "speaker_id": "2",
          "time": "20210825 01:52"
        },
        {
          "id": "MDRW2100012804.1.35",
          "form": "마늘은 국물에 다진마늘 티 두스푼",
          "original_form": "마늘은 국물에 다진마늘 티 두스푼",
          "speaker_id": "1",
          "time": "20210825 01:52"
        },
        {
          "id": "MDRW2100012804.1.36",
          "form": "우와ㅎㅎ",
          "original_form": "우와ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:53"
        },
        {
          "id": "MDRW2100012804.1.37",
          "form": "마지막 다된 라면 위에 한스푼 올리고요",
          "original_form": "마지막 다된 라면 위에 한스푼 올리고요",
          "speaker_id": "1",
          "time": "20210825 01:53"
        },
        {
          "id": "MDRW2100012804.1.38",
          "form": "그렇게까지하는데 맛이 없을리가 없죠ㅎ",
          "original_form": "그렇게까지하는데 맛이 없을리가 없죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:53"
        },
        {
          "id": "MDRW2100012804.1.39",
          "form": "고추가루와 청양고추는 디폴트로 넣어요",
          "original_form": "고추가루와 청양고추는 디폴트로 넣어요",
          "speaker_id": "1",
          "time": "20210825 01:53"
        },
        {
          "id": "MDRW2100012804.1.40",
          "form": "고명은 뭐들어가요?",
          "original_form": "고명은 뭐들어가요?",
          "speaker_id": "2",
          "time": "20210825 01:53"
        },
        {
          "id": "MDRW2100012804.1.41",
          "form": "그쵸ㅎㅎ",
          "original_form": "그쵸ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 01:53"
        },
        {
          "id": "MDRW2100012804.1.42",
          "form": "고명은 딱히...",
          "original_form": "고명은 딱히...",
          "speaker_id": "1",
          "time": "20210825 01:54"
        },
        {
          "id": "MDRW2100012804.1.43",
          "form": "라면 아야기가 나와서 말인데ㅎ",
          "original_form": "라면 아야기가 나와서 말인데ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:54"
        },
        {
          "id": "MDRW2100012804.1.44",
          "form": "마른 오징어 있으면 다리하나를 썰어넣어요 감칠맛나라고",
          "original_form": "마른 오징어 있으면 다리하나를 썰어넣어요 감칠맛나라고",
          "speaker_id": "1",
          "time": "20210825 01:54"
        },
        {
          "id": "MDRW2100012804.1.45",
          "form": "전에 한창 유행했을꺼에요 황제라면",
          "original_form": "전에 한창 유행했을꺼에요 황제라면",
