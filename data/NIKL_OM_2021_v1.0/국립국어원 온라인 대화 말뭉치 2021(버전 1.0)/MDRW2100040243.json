      "utterance": [
        {
          "id": "MDRW2100040243.1.1",
          "form": "건강히 지내고 계신가요?",
          "original_form": "건강히 지내고 계신가요?",
          "speaker_id": "1",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100040243.1.2",
          "form": "아뇨 요즘 통 운동을 못하네요",
          "original_form": "아뇨 요즘 통 운동을 못하네요",
          "speaker_id": "2",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100040243.1.3",
          "form": "운동 하시는거 있나요?",
          "original_form": "운동 하시는거 있나요?",
          "speaker_id": "2",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100040243.1.4",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100040243.1.5",
          "form": "저는 필라테스 주3회 정도 하는데",
          "original_form": "저는 필라테스 주3회 정도 하는데",
          "speaker_id": "1",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100040243.1.6",
          "form": "어제 백신접종을 해서 다음주는 쉬려고요",
          "original_form": "어제 백신접종을 해서 다음주는 쉬려고요",
          "speaker_id": "1",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100040243.1.7",
          "form": "필라테스 많이 하시더라구요!",
          "original_form": "필라테스 많이 하시더라구요!",
          "speaker_id": "2",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100040243.1.8",
          "form": "배신 저도 이제 맞아야되는데 걱정되네요",
          "original_form": "배신 저도 이제 맞아야되는데 걱정되네요",
          "speaker_id": "2",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100040243.1.9",
          "form": "네 저는 어제 2차 맞았는데",
          "original_form": "네 저는 어제 2차 맞았는데",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100040243.1.10",
          "form": "1차때보단 좀 더 반응이 있어서",
          "original_form": "1차때보단 좀 더 반응이 있어서",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100040243.1.11",
          "form": "약먹고 쉬고 있어요",
          "original_form": "약먹고 쉬고 있어요",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100040243.1.12",
          "form": "벌써 이차 맞으셨나요?",
          "original_form": "벌써 이차 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100040243.1.13",
          "form": "네 어제 2차 백신 접종했어요",
          "original_form": "네 어제 2차 백신 접종했어요",
          "speaker_id": "1",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100040243.1.14",
          "form": "화이자 맞으셨나요?",
          "original_form": "화이자 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100040243.1.15",
          "form": "네 화이자 맞았어요",
          "original_form": "네 화이자 맞았어요",
          "speaker_id": "1",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100040243.1.16",
          "form": "아직 안 맞아서 백신 뭐 맞게될지 궁금하네요",
          "original_form": "아직 안 맞아서 백신 뭐 맞게될지 궁금하네요",
          "speaker_id": "2",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100040243.1.17",
          "form": "아 아직 안맞으셨으면",
          "original_form": "아 아직 안맞으셨으면",
          "speaker_id": "1",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100040243.1.18",
          "form": "그냥 mRNA라고만 뜬다던데",
          "original_form": "그냥 mRNA라고만 뜬다던데",
          "speaker_id": "1",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100040243.1.19",
          "form": "그런게 떴었는지도 기억이 안나네요 ㅋㅋ",
          "original_form": "그런게 떴었는지도 기억이 안나네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100040243.1.20",
          "form": "ㅎㅎ언제 1차 접종이신데요?",
          "original_form": "ㅎㅎ언제 1차 접종이신데요?",
          "speaker_id": "1",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100040243.1.21",
          "form": "예약할때 화이자 모더나라고는 안뜬게 확실한데",
          "original_form": "예약할때 화이자 모더나라고는 안뜬게 확실한데",
          "speaker_id": "2",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100040243.1.22",
          "form": "담주화욜이요",
          "original_form": "담주화욜이요",
