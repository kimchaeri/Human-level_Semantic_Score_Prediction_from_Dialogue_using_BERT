      "utterance": [
        {
          "id": "MDRW2100011300.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 10:51"
        },
        {
          "id": "MDRW2100011300.1.2",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20210824 10:52"
        },
        {
          "id": "MDRW2100011300.1.3",
          "form": "아침은 드셨나요?",
          "original_form": "아침은 드셨나요?",
          "speaker_id": "2",
          "time": "20210824 10:52"
        },
        {
          "id": "MDRW2100011300.1.4",
          "form": "네네 저는 볶음밥이요",
          "original_form": "네네 저는 볶음밥이요",
          "speaker_id": "1",
          "time": "20210824 11:00"
        },
        {
          "id": "MDRW2100011300.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210824 11:00"
        },
        {
          "id": "MDRW2100011300.1.6",
          "form": "저는 빵먹었어요:)",
          "original_form": "저는 빵먹었어요:)",
          "speaker_id": "2",
          "time": "20210824 11:02"
        },
        {
          "id": "MDRW2100011300.1.7",
          "form": "볶음밥은 직접 하신건가요?",
          "original_form": "볶음밥은 직접 하신건가요?",
          "speaker_id": "2",
          "time": "20210824 11:02"
        },
        {
          "id": "MDRW2100011300.1.8",
          "form": "아뇨 냉동볶음밥이요ㅋㅋㅋ",
          "original_form": "아뇨 냉동볶음밥이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:08"
        },
        {
          "id": "MDRW2100011300.1.9",
          "form": "빵도 맛있겠어요 식빵인가요?",
          "original_form": "빵도 맛있겠어요 식빵인가요?",
          "speaker_id": "1",
          "time": "20210824 11:08"
        },
        {
          "id": "MDRW2100011300.1.10",
          "form": "아항 냉동볶음밥 ㅋㅋ",
          "original_form": "아항 냉동볶음밥 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:10"
        },
        {
          "id": "MDRW2100011300.1.11",
          "form": "새우?김치? ㅋㅋ",
          "original_form": "새우?김치? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:10"
        },
        {
          "id": "MDRW2100011300.1.12",
          "form": "저는 양파치즈브레드요ㅋㅋ",
          "original_form": "저는 양파치즈브레드요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:11"
        },
        {
          "id": "MDRW2100011300.1.13",
          "form": "새우요ㅋㅋㅋㅋ",
          "original_form": "새우요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:18"
        },
        {
          "id": "MDRW2100011300.1.14",
          "form": "맛있나요? 양파치즈브레드",
          "original_form": "맛있나요? 양파치즈브레드",
          "speaker_id": "1",
          "time": "20210824 11:18"
        },
        {
          "id": "MDRW2100011300.1.15",
          "form": "오 새볶 맛있었겠네요!",
          "original_form": "오 새볶 맛있었겠네요!",
          "speaker_id": "2",
          "time": "20210824 11:19"
        },
        {
          "id": "MDRW2100011300.1.16",
          "form": "빠바 양파치즈브레드\r\n전자렌지에 살짝 돌려먹음\r\n완전 맛나영ㅋㅋ",
          "original_form": "빠바 양파치즈브레드\r\n전자렌지에 살짝 돌려먹음\r\n완전 맛나영ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:20"
        },
        {
          "id": "MDRW2100011300.1.17",
          "form": "오 좋네요ㅋㅋㅋ",
          "original_form": "오 좋네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100011300.1.18",
          "form": "점심은 뭐드시나요?",
          "original_form": "점심은 뭐드시나요?",
          "speaker_id": "1",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100011300.1.19",
          "form": "저는 라면이땡기네요",
          "original_form": "저는 라면이땡기네요",
          "speaker_id": "1",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100011300.1.20",
          "form": "저는 볶음밥이 땡기네요?ㅋㅋ",
          "original_form": "저는 볶음밥이 땡기네요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:25"
        },
        {
          "id": "MDRW2100011300.1.21",
          "form": "어떤 라면 드시려구요?",
          "original_form": "어떤 라면 드시려구요?",
          "speaker_id": "2",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100011300.1.22",
          "form": "저 짜파게티요ㅋㅋㅋㅋ",
          "original_form": "저 짜파게티요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100011300.1.23",
          "form": "ㅋㅋ짜파게티는 언제 먹어도 맛있졍",
          "original_form": "ㅋㅋ짜파게티는 언제 먹어도 맛있졍",
          "speaker_id": "2",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100011300.1.24",
          "form": "2개 끓이셔야겠어요 ㅋㅋ",
          "original_form": "2개 끓이셔야겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100011300.1.25",
          "form": "한개 반이 딱 좋은데\r\n한개반으로 출시좀 해줌 좋겠어여",
          "original_form": "한개 반이 딱 좋은데\r\n한개반으로 출시좀 해줌 좋겠어여",
          "speaker_id": "2",
          "time": "20210824 11:33"
        },
        {
          "id": "MDRW2100011300.1.26",
          "form": "제말이요",
          "original_form": "제말이요",
          "speaker_id": "1",
          "time": "20210824 11:40"
        },
        {
          "id": "MDRW2100011300.1.27",
          "form": "두개는 많고",
          "original_form": "두개는 많고",
          "speaker_id": "1",
          "time": "20210824 11:40"
        },
        {
          "id": "MDRW2100011300.1.28",
          "form": "하나는 부족해요ㅠㅠ",
          "original_form": "하나는 부족해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 11:40"
        }
      ]
    }
