      "utterance": [
        {
          "id": "MDRW2100017208.1.1",
          "form": "좋아하는 가수 있나요?",
          "original_form": "좋아하는 가수 있나요?",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017208.1.2",
          "form": "아니요 ㅠㅠ 있으세요?",
          "original_form": "아니요 ㅠㅠ 있으세요?",
          "speaker_id": "2",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017208.1.3",
          "form": "저는 웬만한 가수 다 좋아해요 ㅋㅋ",
          "original_form": "저는 웬만한 가수 다 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017208.1.4",
          "form": "보시는 드라마 있나요?",
          "original_form": "보시는 드라마 있나요?",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017208.1.5",
          "form": "펜트하우스랑 광자매봐요 ㅋㅋㅋ",
          "original_form": "펜트하우스랑 광자매봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017208.1.6",
          "form": "펜트하우스 시즌2까지는 봣는데",
          "original_form": "펜트하우스 시즌2까지는 봣는데",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017208.1.7",
          "form": "3은 아직 안보고있어요 ㅋㅋ",
          "original_form": "3은 아직 안보고있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017208.1.8",
          "form": "다 끝나면 보실 예정이신가요?ㅋㅋ",
          "original_form": "다 끝나면 보실 예정이신가요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017208.1.9",
          "form": "네 그러려구요 ㅋㅋㅋㅋ",
          "original_form": "네 그러려구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017208.1.10",
          "form": "막장 좋아하시나봐요?",
          "original_form": "막장 좋아하시나봐요?",
          "speaker_id": "1",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017208.1.11",
          "form": "네 ㅋㅋㅋㅋ막장이 흥미로워서요",
          "original_form": "네 ㅋㅋㅋㅋ막장이 흥미로워서요",
          "speaker_id": "2",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017208.1.12",
          "form": "넷플에 꽃들의집도 재밌대요 막장 ㅋㅋㅋ",
          "original_form": "넷플에 꽃들의집도 재밌대요 막장 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017208.1.13",
          "form": "제목만 들어본거같아요 외국건가요??",
          "original_form": "제목만 들어본거같아요 외국건가요??",
          "speaker_id": "2",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017208.1.14",
          "form": "네 어느나란지는 기억이 안나네요ㅠ",
          "original_form": "네 어느나란지는 기억이 안나네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017208.1.15",
          "form": "나중에 보려고 찜하기만 해놔서요 ㅋㅋ",
          "original_form": "나중에 보려고 찜하기만 해놔서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017208.1.16",
          "form": "저도 예고편 한번 봐야겠네요",
          "original_form": "저도 예고편 한번 봐야겠네요",
          "speaker_id": "2",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017208.1.17",
          "form": "넷플릭스는 찜만 하게돼요 ㅋㅋㅋ",
          "original_form": "넷플릭스는 찜만 하게돼요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017208.1.18",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋ 재생은 하지 않고 찜만",
