          "original_form": "갈틱폰...",
          "speaker_id": "2",
          "time": "20210826 18:51"
        },
        {
          "id": "MDRW2100017343.1.5",
          "form": "스듀 나 아직도",
          "original_form": "스듀 나 아직도",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017343.1.6",
          "form": "진저섬? 못 갔어",
          "original_form": "진저섬? 못 갔어",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017343.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017343.1.8",
          "form": "ㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017343.1.9",
          "form": "난 친구 섬에서 감",
          "original_form": "난 친구 섬에서 감",
          "speaker_id": "2",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017343.1.10",
          "form": "업데이트 이후로 스듀를 별로 안 해서",
          "original_form": "업데이트 이후로 스듀를 별로 안 해서",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017343.1.11",
          "form": "이번에 마크만 좀 한 거 같아",
          "original_form": "이번에 마크만 좀 한 거 같아",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017343.1.12",
          "form": "스듀 ㄱ?",
          "original_form": "스듀 ㄱ?",
          "speaker_id": "2",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017343.1.13",
          "form": "다음에 같이 하자",
          "original_form": "다음에 같이 하자",
          "speaker_id": "1",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017343.1.14",
          "form": "name3도 마크 산 거 같던데 다음에 같이 하자고 하자",
          "original_form": "&name3&도 마크 산 거 같던데 다음에 같이 하자고 하자",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017343.1.15",
          "form": "오오 좋다",
          "original_form": "오오 좋다",
          "speaker_id": "2",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017343.1.16",
          "form": "알바하느라 바쁜가봐",
          "original_form": "알바하느라 바쁜가봐",
          "speaker_id": "1",
          "time": "20210826 19:02"
        },
        {
