      "utterance": [
        {
          "id": "MDRW2100016394.1.1",
          "form": "학창시절로 돌아가고 싶으신가요?",
          "original_form": "학창시절로 돌아가고 싶으신가요?",
          "speaker_id": "1",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100016394.1.2",
          "form": "음 아뇨 군대 다시가야해서..",
          "original_form": "음 아뇨 군대 다시가야해서..",
          "speaker_id": "2",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100016394.1.3",
          "form": "아 그러시군요 ",
          "original_form": "아 그러시군요 ",
          "speaker_id": "1",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100016394.1.4",
          "form": "동아리 하셨었나요",
          "original_form": "동아리 하셨었나요",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016394.1.5",
          "form": "대학시절에만 해봤어요 가가구님은요",
          "original_form": "대학시절에만 해봤어요 가가구님은요",
          "speaker_id": "1",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100016394.1.6",
          "form": "저도 학생때 해봤어요",
          "original_form": "저도 학생때 해봤어요",
          "speaker_id": "2",
          "time": "20210826 13:30"
        },
        {
          "id": "MDRW2100016394.1.7",
          "form": "학생 때 어떤 것 해보셨나요",
          "original_form": "학생 때 어떤 것 해보셨나요",
          "speaker_id": "1",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100016394.1.8",
          "form": "볼링동아리 했었어요ㅇㅇ",
          "original_form": "볼링동아리 했었어요ㅇㅇ",
          "speaker_id": "2",
          "time": "20210826 13:44"
        },
        {
          "id": "MDRW2100016394.1.9",
          "form": "오 그럼 볼링 잘 치실까요?",
          "original_form": "오 그럼 볼링 잘 치실까요?",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016394.1.10",
          "form": "아뇨 잠깐하다 그만둬서ㅎㅎ",
          "original_form": "아뇨 잠깐하다 그만둬서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016394.1.11",
          "form": "그렇군요 그래도 즐거운 추억이었겠어요",
          "original_form": "그렇군요 그래도 즐거운 추억이었겠어요",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016394.1.12",
          "form": "네네  그립긴하네요",
          "original_form": "네네  그립긴하네요",
          "speaker_id": "2",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016394.1.13",
          "form": "기억에 남는 과제 있나요?",
          "original_form": "기억에 남는 과제 있나요?",
