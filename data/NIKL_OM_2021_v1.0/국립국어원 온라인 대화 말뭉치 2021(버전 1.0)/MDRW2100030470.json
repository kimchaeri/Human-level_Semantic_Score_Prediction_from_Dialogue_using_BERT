      "utterance": [
        {
          "id": "MDRW2100030470.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100030470.1.2",
          "form": "스포츠경기 많이보시나요?",
          "original_form": "스포츠경기 많이보시나요?",
          "speaker_id": "1",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100030470.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100030470.1.4",
          "form": "아뇨 평소엔 잘 안보는 편이에요ㅜ",
          "original_form": "아뇨 평소엔 잘 안보는 편이에요ㅜ",
          "speaker_id": "2",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100030470.1.5",
          "form": "앗 저도요ㅜ",
          "original_form": "앗 저도요ㅜ",
          "speaker_id": "1",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100030470.1.6",
          "form": "이번 올림픽 때 그래도",
          "original_form": "이번 올림픽 때 그래도",
          "speaker_id": "1",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100030470.1.7",
          "form": "최근 올림픽땐 몇몇 종목 경기들 재미있게 봤지만..",
          "original_form": "최근 올림픽땐 몇몇 종목 경기들 재미있게 봤지만..",
          "speaker_id": "2",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100030470.1.8",
          "form": "배구가 재밌어서 조금봤어요",
          "original_form": "배구가 재밌어서 조금봤어요",
          "speaker_id": "1",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100030470.1.9",
          "form": "와!",
          "original_form": "와!",
          "speaker_id": "1",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100030470.1.10",
          "form": "공통점이네요",
          "original_form": "공통점이네요",
          "speaker_id": "1",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100030470.1.11",
          "form": "배구랑 양궁 좀 본 것 같아요",
          "original_form": "배구랑 양궁 좀 본 것 같아요",
          "speaker_id": "1",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100030470.1.12",
          "form": "그럼 name2님은 직접 경기를 보러간적은 없으신가요?",
          "original_form": "그럼 &name2&님은 직접 경기를 보러간적은 없으신가요?",
          "speaker_id": "1",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100030470.1.13",
          "form": "저도 양궁으로 시작해서 배구로 끝났어요!ㅋㅋㅋ",
          "original_form": "저도 양궁으로 시작해서 배구로 끝났어요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:38"
        },
        {
          "id": "MDRW2100030470.1.14",
          "form": "네 아쉽게도 아직 직관은 한번도 못해봤어요ㅜ",
          "original_form": "네 아쉽게도 아직 직관은 한번도 못해봤어요ㅜ",
          "speaker_id": "2",
          "time": "20210904 01:38"
        },
        {
          "id": "MDRW2100030470.1.15",
          "form": "기회되면 배구 경기 직관 하러 가보고 싶네요 ㅎㅎ",
          "original_form": "기회되면 배구 경기 직관 하러 가보고 싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:38"
        },
        {
          "id": "MDRW2100030470.1.16",
          "form": "게다가 최근에는 코로나때문에 스포츠경기 직관도 많이 줄었더라구요ㅜㅜ",
          "original_form": "게다가 최근에는 코로나때문에 스포츠경기 직관도 많이 줄었더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100030470.1.17",
          "form": "저두요~!",
          "original_form": "저두요~!",
          "speaker_id": "1",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100030470.1.18",
          "form": "배구가 이렇게 재밌는지 몰랐어요",
          "original_form": "배구가 이렇게 재밌는지 몰랐어요",
          "speaker_id": "1",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100030470.1.19",
          "form": "식빵언니 은퇴한다던데 그래도 재밌었으면 좋겠네요",
          "original_form": "식빵언니 은퇴한다던데 그래도 재밌었으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100030470.1.20",
          "form": "맞아요ㅜ 그나마 직관도 관중수 줄여서 받고 그러는것 같더라구요ㅜㅜ",
          "original_form": "맞아요ㅜ 그나마 직관도 관중수 줄여서 받고 그러는것 같더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 01:40"
        },
        {
          "id": "MDRW2100030470.1.21",
          "form": "나중에 야구장도 가보고싶어요",
          "original_form": "나중에 야구장도 가보고싶어요",
          "speaker_id": "1",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100030470.1.22",
          "form": "야구장에서 치킨먹는게 그렇게 맛있대요",
          "original_form": "야구장에서 치킨먹는게 그렇게 맛있대요",
          "speaker_id": "1",
          "time": "20210904 01:42"
        },
        {
          "id": "MDRW2100030470.1.23",
          "form": "야구장하면 역시 치맥인것 같아요 ㅋㅋ",
          "original_form": "야구장하면 역시 치맥인것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:42"
        },
        {
          "id": "MDRW2100030470.1.24",
          "form": "저도 나중에 가족들이랑 야구장 한번 가보자 했었는데 아직도 못가보고 있어요ㅜ",
          "original_form": "저도 나중에 가족들이랑 야구장 한번 가보자 했었는데 아직도 못가보고 있어요ㅜ",
          "speaker_id": "2",
          "time": "20210904 01:43"
        },
        {
          "id": "MDRW2100030470.1.25",
          "form": "그러게요ㅜㅜ 코로나 끝나면 야구장도 꽉차겠죠?",
          "original_form": "그러게요ㅜㅜ 코로나 끝나면 야구장도 꽉차겠죠?",
          "speaker_id": "1",
          "time": "20210904 01:44"
        },
        {
          "id": "MDRW2100030470.1.26",
          "form": "그때 한번가보고싶네요 왁자지껄한 분위기 즐기러",
          "original_form": "그때 한번가보고싶네요 왁자지껄한 분위기 즐기러",
          "speaker_id": "1",
          "time": "20210904 01:44"
        },
        {
          "id": "MDRW2100030470.1.27",
          "form": "저도 그때가 기대되네요 ㅎㅎ",
          "original_form": "저도 그때가 기대되네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100030470.1.28",
          "form": "최근엔 바깥에서 그런 왁자한 분위기 느껴본지도 오래되서ㅜ",
          "original_form": "최근엔 바깥에서 그런 왁자한 분위기 느껴본지도 오래되서ㅜ",
          "speaker_id": "2",
          "time": "20210904 01:47"
        },
        {
          "id": "MDRW2100030470.1.29",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 01:49"
        }
      ]
