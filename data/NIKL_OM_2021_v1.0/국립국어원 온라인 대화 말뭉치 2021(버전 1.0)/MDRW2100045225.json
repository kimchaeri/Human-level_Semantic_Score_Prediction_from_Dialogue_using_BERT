      "utterance": [
        {
          "id": "MDRW2100045225.1.1",
          "form": "연어초밥 좋아하시나요?",
          "original_form": "연어초밥 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045225.1.2",
          "form": "연어나 장어초밥은 좋아하세요?",
          "original_form": "연어나 장어초밥은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045225.1.3",
          "form": "앗 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045225.1.4",
          "form": "연어초밥도 없어서못먹어요",
          "original_form": "연어초밥도 없어서못먹어요",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045225.1.5",
          "form": "앗 ㅋㅋㅋㅋ 저도요",
          "original_form": "앗 ㅋㅋㅋㅋ 저도요",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045225.1.6",
          "form": "장어도 잘먹어요 ㅋㅋㅋㅋ",
          "original_form": "장어도 잘먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045225.1.7",
          "form": "초밥먹은지 좀 됏는데 먹고싶어지네요 ㅋㅋㅋㅋㅋ",
          "original_form": "초밥먹은지 좀 됏는데 먹고싶어지네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045225.1.8",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045225.1.9",
          "form": "여름이라 한동안 자제중이었는데",
          "original_form": "여름이라 한동안 자제중이었는데",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045225.1.10",
          "form": "날 싸늘해지면 초밥먹으러 갈까봐요 ㅋㅋㅋ",
          "original_form": "날 싸늘해지면 초밥먹으러 갈까봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045225.1.11",
          "form": "맞아요 여름에는 날것 조심스러워서 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 여름에는 날것 조심스러워서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045225.1.12",
          "form": "해산물 좋아하세요?",
          "original_form": "해산물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045225.1.13",
          "form": "네 엄청 좋아해요 ㅋㅋㅋ",
          "original_form": "네 엄청 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045225.1.14",
          "form": "ㅋㅋㅋㅋㅋ저도 엄청좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋ저도 엄청좋아해요",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045225.1.15",
          "form": "해산물중에 뭘 특히 좋아하세요?",
          "original_form": "해산물중에 뭘 특히 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045225.1.16",
          "form": "갑각류?요 ㅋㅋㅋㅋㅋ",
          "original_form": "갑각류?요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045225.1.17",
          "form": "새우나 게 쪽 ㅋㅋㅋㅋ",
          "original_form": "새우나 게 쪽 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045225.1.18",
          "form": "저도저돜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도저돜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045225.1.19",
          "form": "게 진짜 좋아해요",
          "original_form": "게 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045225.1.20",
          "form": "대게ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "대게ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045225.1.21",
          "form": "맞아요 대게...",
          "original_form": "맞아요 대게...",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045225.1.22",
          "form": "요즘 통통한 대게 보기 힘든거같아요",
          "original_form": "요즘 통통한 대게 보기 힘든거같아요",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045225.1.23",
          "form": "아직 철이 아니라서 그런거 아닐까요??",
          "original_form": "아직 철이 아니라서 그런거 아닐까요??",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045225.1.24",
          "form": "대게가 아직 철이 아닌가요?",
          "original_form": "대게가 아직 철이 아닌가요?",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045225.1.25",
          "form": "대게 보통 12월~2월이 제철이라고 알고 잇어요",
          "original_form": "대게 보통 12월~2월이 제철이라고 알고 잇어요",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045225.1.26",
          "form": "아하 역시 다들 추워져야 철이네요",
          "original_form": "아하 역시 다들 추워져야 철이네요",
