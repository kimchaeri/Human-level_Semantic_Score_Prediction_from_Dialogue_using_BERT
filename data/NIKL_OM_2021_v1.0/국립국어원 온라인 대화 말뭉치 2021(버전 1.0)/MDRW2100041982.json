      "utterance": [
        {
          "id": "MDRW2100041982.1.1",
          "form": "오늘 학교 다녀오셨나요?",
          "original_form": "오늘 학교 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041982.1.2",
          "form": "네 방금 끝나서 집에 가는 중이에요",
          "original_form": "네 방금 끝나서 집에 가는 중이에요",
          "speaker_id": "2",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100041982.1.3",
          "form": "님은 학교 다녀오셨나요?",
          "original_form": "님은 학교 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100041982.1.4",
          "form": "헉 늦게 끝나네요 ㅜㅜ",
          "original_form": "헉 늦게 끝나네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100041982.1.5",
          "form": "저흰 비대면이에요",
          "original_form": "저흰 비대면이에요",
          "speaker_id": "1",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100041982.1.6",
          "form": "헉 부러워요",
          "original_form": "헉 부러워요",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100041982.1.7",
          "form": "저희는 대면이 좀 있어요 \r\nㅠㅠ",
          "original_form": "저희는 대면이 좀 있어요 \r\nㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100041982.1.8",
          "form": "대학생이세요?",
          "original_form": "대학생이세요?",
          "speaker_id": "1",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100041982.1.9",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100041982.1.10",
          "form": "저도예요!",
          "original_form": "저도예요!",
          "speaker_id": "1",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100041982.1.11",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100041982.1.12",
          "form": "뭐 전공하시나요",
          "original_form": "뭐 전공하시나요",
          "speaker_id": "1",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100041982.1.13",
          "form": "저는 해양학 전공이에요!",
          "original_form": "저는 해양학 전공이에요!",
          "speaker_id": "2",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100041982.1.14",
          "form": "헉 멋있어요",
          "original_form": "헉 멋있어요",
          "speaker_id": "1",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100041982.1.15",
          "form": "뭘 배우는 전공인가요...?",
          "original_form": "뭘 배우는 전공인가요...?",
          "speaker_id": "1",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100041982.1.16",
          "form": "바다랑 관련된건 다 배워요",
          "original_form": "바다랑 관련된건 다 배워요",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041982.1.17",
          "form": "물화생지 다...",
          "original_form": "물화생지 다...",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041982.1.18",
          "form": "님은 무슨 전공인가요??",
          "original_form": "님은 무슨 전공인가요??",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041982.1.19",
          "form": "저는 교육 전공해요",
          "original_form": "저는 교육 전공해요",
          "speaker_id": "1",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100041982.1.20",
          "form": "멋있어요",
          "original_form": "멋있어요",
          "speaker_id": "2",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100041982.1.21",
          "form": "동아리도 하시나요?",
          "original_form": "동아리도 하시나요?",
          "speaker_id": "2",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100041982.1.22",
          "form": "했다가 그만뒀어요... 비대면이라 하기가 힘들더라고요",
          "original_form": "했다가 그만뒀어요... 비대면이라 하기가 힘들더라고요",
          "speaker_id": "1",
          "time": "20210906 18:21"
        }
      ]
