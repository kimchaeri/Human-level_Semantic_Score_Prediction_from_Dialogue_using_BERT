      "utterance": [
        {
          "id": "MDRW2100019149.1.1",
          "form": "언니는 기억에 남는 여행지가 있나요!",
          "original_form": "언니는 기억에 남는 여행지가 있나요!",
          "speaker_id": "1",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019149.1.2",
          "form": "하아 이젠별로생각도안나네요",
          "original_form": "하아 이젠별로생각도안나네요",
          "speaker_id": "2",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019149.1.3",
          "form": "좀지나면 10년되겟어요ㅡㅡ ㅋㅋㅋㅋ그냥",
          "original_form": "좀지나면 10년되겟어요ㅡㅡ ㅋㅋㅋㅋ그냥",
          "speaker_id": "2",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019149.1.4",
          "form": "ㅋㅋㅋㅋ아웃기",
          "original_form": "ㅋㅋㅋㅋ아웃기",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019149.1.5",
          "form": "필리핀에 바다놀던게남군",
          "original_form": "필리핀에 바다놀던게남군",
          "speaker_id": "2",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019149.1.6",
          "form": "필리핀 바다 사진 보면 무섭더라",
          "original_form": "필리핀 바다 사진 보면 무섭더라",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019149.1.7",
          "form": "뭔가 한가운데 떠있는거같아",
          "original_form": "뭔가 한가운데 떠있는거같아",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019149.1.8",
          "form": "진짜 나즁엔 몇개국안남을라나 그럼 사람들이 몰리면 또 가라앉은거아닌가",
          "original_form": "진짜 나즁엔 몇개국안남을라나 그럼 사람들이 몰리면 또 가라앉은거아닌가",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019149.1.9",
          "form": "새로운 행성이필요하다!!!",
          "original_form": "새로운 행성이필요하다!!!",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019149.1.10",
          "form": "ㅋㅋㅋ우리 살아있을땐 아니겠지..?",
          "original_form": "ㅋㅋㅋ우리 살아있을땐 아니겠지..?",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019149.1.11",
          "form": "아냐 그건..ㅋㅋㅋ아닌고같고",
          "original_form": "아냐 그건..ㅋㅋㅋ아닌고같고",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019149.1.12",
          "form": "난 그거 먹고싶다",
          "original_form": "난 그거 먹고싶다",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019149.1.13",
          "form": "판시?",
          "original_form": "판시?",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019149.1.14",
          "form": "팬시?엿나 얇은면볶음 ㅋㅋㅋ",
          "original_form": "팬시?엿나 얇은면볶음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019149.1.15",
          "form": "아!! 맞아",
          "original_form": "아!! 맞아",
          "speaker_id": "1",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019149.1.16",
          "form": "깔라만시 뿌려먹고!!!",
          "original_form": "깔라만시 뿌려먹고!!!",
          "speaker_id": "1",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019149.1.17",
          "form": "맞아 ㅋㅋㅋㅋㅋ그거초록이이름이",
          "original_form": "맞아 ㅋㅋㅋㅋㅋ그거초록이이름이",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019149.1.18",
          "form": "그리고 우리 점심 먹고 방에 올라가서 망고 하나씩 먹고 참 좋은 먹부림이었다",
          "original_form": "그리고 우리 점심 먹고 방에 올라가서 망고 하나씩 먹고 참 좋은 먹부림이었다",
          "speaker_id": "1",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019149.1.19",
          "form": "뭐엿지 깔라만시라햇나",
          "original_form": "뭐엿지 깔라만시라햇나",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019149.1.20",
          "form": "사실 모르겠어 ㅋㅋㅋㅋ",
          "original_form": "사실 모르겠어 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019149.1.21",
          "form": "건망고도 엄청먹고 ㅋㅋㅋㅋㅋ진짜 믹스커피도 엄청먹엇는데",
          "original_form": "건망고도 엄청먹고 ㅋㅋㅋㅋㅋ진짜 믹스커피도 엄청먹엇는데",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019149.1.22",
          "form": "ㅋㅋ주말마다 마트가서 과자 털어오고",
          "original_form": "ㅋㅋ주말마다 마트가서 과자 털어오고",
