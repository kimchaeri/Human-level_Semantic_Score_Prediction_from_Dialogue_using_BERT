      "utterance": [
        {
          "id": "MDRW2100019165.1.1",
          "form": "여기는 비와",
          "original_form": "여기는 비와",
          "speaker_id": "1",
          "time": "20210827 15:50"
        },
        {
          "id": "MDRW2100019165.1.2",
          "form": "여기도 좀 오네..",
          "original_form": "여기도 좀 오네..",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019165.1.3",
          "form": "비오더니 너무 쌀쌀해졌어 오늘 긴팔입었어",
          "original_form": "비오더니 너무 쌀쌀해졌어 오늘 긴팔입었어",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019165.1.4",
          "form": "나는 아직 반팔인데",
          "original_form": "나는 아직 반팔인데",
          "speaker_id": "2",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019165.1.5",
          "form": "헐 안 추워?",
          "original_form": "헐 안 추워?",
          "speaker_id": "1",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019165.1.6",
          "form": "비올때만 춥고 아닐땐 너무 습하고 중간이 없어",
          "original_form": "비올때만 춥고 아닐땐 너무 습하고 중간이 없어",
          "speaker_id": "1",
          "time": "20210827 15:55"
        },
        {
          "id": "MDRW2100019165.1.7",
          "form": "요즘날씨에 제습기는 필수인듯",
          "original_form": "요즘날씨에 제습기는 필수인듯",
          "speaker_id": "2",
          "time": "20210827 15:55"
        },
        {
          "id": "MDRW2100019165.1.8",
          "form": "ㅇㅇ 제습기 오늘도 틀고 나옴",
          "original_form": "ㅇㅇ 제습기 오늘도 틀고 나옴",
          "speaker_id": "1",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100019165.1.9",
          "form": "제습기 매일돌리는중인데 그래서 그런가 전기세도 많이 나오네..ㅠ",
          "original_form": "제습기 매일돌리는중인데 그래서 그런가 전기세도 많이 나오네..ㅠ",
          "speaker_id": "2",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100019165.1.10",
          "form": "제습기ㅠ 근데 어쩔수 없지ㅠ",
          "original_form": "제습기ㅠ 근데 어쩔수 없지ㅠ",
          "speaker_id": "1",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019165.1.11",
          "form": "빨리 겨울와서 얼어죽고싶다",
          "original_form": "빨리 겨울와서 얼어죽고싶다",
          "speaker_id": "2",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019165.1.12",
          "form": "벌레가 너무싫어",
          "original_form": "벌레가 너무싫어",
          "speaker_id": "2",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019165.1.13",
          "form": "극단적이잖아",
          "original_form": "극단적이잖아",
          "speaker_id": "1",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019165.1.14",
