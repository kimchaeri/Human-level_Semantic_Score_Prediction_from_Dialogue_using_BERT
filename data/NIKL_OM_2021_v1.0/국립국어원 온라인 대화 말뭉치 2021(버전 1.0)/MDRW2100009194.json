      "utterance": [
        {
          "id": "MDRW2100009194.1.1",
          "form": "싸이월드 아세요?",
          "original_form": "싸이월드 아세요?",
          "speaker_id": "1",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009194.1.2",
          "form": "그럼요~ 열심히 했죠",
          "original_form": "그럼요~ 열심히 했죠",
          "speaker_id": "2",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009194.1.3",
          "form": "다시 부활하나봐요",
          "original_form": "다시 부활하나봐요",
          "speaker_id": "1",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009194.1.4",
          "form": "했다고 하던데요? 전 아직 접속 전이긴 합니다만",
          "original_form": "했다고 하던데요? 전 아직 접속 전이긴 합니다만",
          "speaker_id": "2",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009194.1.5",
          "form": "아 그런가요?",
          "original_form": "아 그런가요?",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009194.1.6",
          "form": "다시보기 두럽네요",
          "original_form": "다시보기 두럽네요",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009194.1.7",
          "form": "특히  다이어리요~",
          "original_form": "특히  다이어리요~",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009194.1.8",
          "form": "다이어리ㅋㅋ",
          "original_form": "다이어리ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009194.1.9",
          "form": "제가 싸이 하는 정성으로 블로그를 했으면 떼돈 벌었을거예요",
          "original_form": "제가 싸이 하는 정성으로 블로그를 했으면 떼돈 벌었을거예요",
          "speaker_id": "2",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009194.1.10",
          "form": "싸이질ㅋ",
          "original_form": "싸이질ㅋ",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009194.1.11",
          "form": "하나하나 꾸미고 사고",
          "original_form": "하나하나 꾸미고 사고",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009194.1.12",
          "form": "혹시 포도알 기억 나세요?",
          "original_form": "혹시 포도알 기억 나세요?",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009194.1.13",
          "form": "포도알?",
          "original_form": "포도알?",
          "speaker_id": "1",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009194.1.14",
          "form": "가물가물",
          "original_form": "가물가물",
          "speaker_id": "1",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009194.1.15",
          "form": "다들 포도알은 잘 기억 못하나봐요",
          "original_form": "다들 포도알은 잘 기억 못하나봐요",
          "speaker_id": "2",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009194.1.16",
          "form": "일기쓰면 줬던것 같은디 그걸로 일기장에 쓰는.스티커를 교환했던것 같아요",
          "original_form": "일기쓰면 줬던것 같은디 그걸로 일기장에 쓰는.스티커를 교환했던것 같아요",
          "speaker_id": "2",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009194.1.17",
          "form": "ㅎㅎ 진짜 아기자기하네요",
          "original_form": "ㅎㅎ 진짜 아기자기하네요",
          "speaker_id": "1",
          "time": "20210822 22:14"
        }
      ]
