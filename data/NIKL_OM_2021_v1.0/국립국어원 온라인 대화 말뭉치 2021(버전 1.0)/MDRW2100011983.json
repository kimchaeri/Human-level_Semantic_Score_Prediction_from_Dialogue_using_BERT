      "utterance": [
        {
          "id": "MDRW2100011983.1.1",
          "form": "요즘 코로나라 이직하는사람이 진짜 많다더라구요!",
          "original_form": "요즘 코로나라 이직하는사람이 진짜 많다더라구요!",
          "speaker_id": "2",
          "time": "20210824 18:07"
        },
        {
          "id": "MDRW2100011983.1.2",
          "form": "맞아요 코로나라 일자리구하기도 힘들고",
          "original_form": "맞아요 코로나라 일자리구하기도 힘들고",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011983.1.3",
          "form": "지금 강제로 쉬고있는데 걱정이에요",
          "original_form": "지금 강제로 쉬고있는데 걱정이에요",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011983.1.4",
          "form": "ㅠㅠㅠ윽..여름님도 쉬고계시군요",
          "original_form": "ㅠㅠㅠ윽..여름님도 쉬고계시군요",
          "speaker_id": "2",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011983.1.5",
          "form": "저는 쉬는것까진 아닌데 일이 많이 줄었어요 그래서",
          "original_form": "저는 쉬는것까진 아닌데 일이 많이 줄었어요 그래서",
          "speaker_id": "2",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100011983.1.6",
          "form": "새롭게 구직을 해야하나..고민이예요",
          "original_form": "새롭게 구직을 해야하나..고민이예요",
          "speaker_id": "2",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100011983.1.7",
          "form": "ㅜ진짜 먹고살기 넘힘드네요",
          "original_form": "ㅜ진짜 먹고살기 넘힘드네요",
          "speaker_id": "1",
          "time": "20210824 18:11"
        },
        {
          "id": "MDRW2100011983.1.8",
          "form": "이렇게쉴때 알바라도 할까 봤는데 알바자리도 별로없고요ㅜㅜ",
          "original_form": "이렇게쉴때 알바라도 할까 봤는데 알바자리도 별로없고요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 18:11"
        },
        {
          "id": "MDRW2100011983.1.9",
          "form": "맞아요.. 알바도 구하기 어렵고",
          "original_form": "맞아요.. 알바도 구하기 어렵고",
          "speaker_id": "2",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011983.1.10",
          "form": "또 괜찮은건 이미 자리가 다 차있더라구요",
          "original_form": "또 괜찮은건 이미 자리가 다 차있더라구요",
          "speaker_id": "2",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011983.1.11",
          "form": "그래도 곧 추석이라 추석때 잠시 자리 많이 나지않을까요?",
          "original_form": "그래도 곧 추석이라 추석때 잠시 자리 많이 나지않을까요?",
          "speaker_id": "2",
          "time": "20210824 18:22"
        },
        {
          "id": "MDRW2100011983.1.12",
          "form": "네 추석대목을 한번 노려봐야겠어요 ㅋㅋ",
          "original_form": "네 추석대목을 한번 노려봐야겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:24"
        },
        {
          "id": "MDRW2100011983.1.13",
          "form": "예전에 명절 아르바이트 해본적있는데 쏠쏠하더라구요 힘들긴한데",
          "original_form": "예전에 명절 아르바이트 해본적있는데 쏠쏠하더라구요 힘들긴한데",
          "speaker_id": "1",
          "time": "20210824 18:24"
        },
        {
          "id": "MDRW2100011983.1.14",
          "form": "보통 명절에는 서서 일하고 사람많고 그러니까 ㅠㅠ..",
          "original_form": "보통 명절에는 서서 일하고 사람많고 그러니까 ㅠㅠ..",
          "speaker_id": "2",
          "time": "20210824 18:25"
        },
        {
          "id": "MDRW2100011983.1.15",
          "form": "물론 전만치는 아닐꺼같지만 그래도 대목은 대목이니까요!",
          "original_form": "물론 전만치는 아닐꺼같지만 그래도 대목은 대목이니까요!",
          "speaker_id": "2",
          "time": "20210824 18:25"
        },
        {
          "id": "MDRW2100011983.1.16",
          "form": "저는 친구 대타 가고있어욬ㅋㅋㅋ..근근히 알바로 연명하는",
          "original_form": "저는 친구 대타 가고있어욬ㅋㅋㅋ..근근히 알바로 연명하는",
          "speaker_id": "2",
          "time": "20210824 18:25"
        },
        {
          "id": "MDRW2100011983.1.17",
          "form": "맞아요 코로나라 좀줄긴하겠지만",
          "original_form": "맞아요 코로나라 좀줄긴하겠지만",
          "speaker_id": "1",
          "time": "20210824 18:27"
        },
        {
          "id": "MDRW2100011983.1.18",
          "form": "대타 알바 어떤거하세요",
          "original_form": "대타 알바 어떤거하세요",
          "speaker_id": "1",
          "time": "20210824 18:27"
        },
        {
          "id": "MDRW2100011983.1.19",
          "form": "커피요! 카페는 아닌데 머신기가 많아서",
          "original_form": "커피요! 카페는 아닌데 머신기가 많아서",
          "speaker_id": "2",
          "time": "20210824 18:28"
        },
        {
          "id": "MDRW2100011983.1.20",
          "form": "그거 관리해주고 그런거예요! ㅎ.ㅎ",
          "original_form": "그거 관리해주고 그런거예요! ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:28"
        },
        {
          "id": "MDRW2100011983.1.21",
          "form": "일이 그리 힘들진 않은데 일찍일어나야해요..",
          "original_form": "일이 그리 힘들진 않은데 일찍일어나야해요..",
          "speaker_id": "2",
          "time": "20210824 18:28"
        },
        {
          "id": "MDRW2100011983.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ7시출근이라서요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ7시출근이라서요!",
          "speaker_id": "2",
          "time": "20210824 18:29"
        },
        {
          "id": "MDRW2100011983.1.23",
          "form": "헉 7시면 진짜 이르긴하네요",
          "original_form": "헉 7시면 진짜 이르긴하네요",
          "speaker_id": "1",
          "time": "20210824 18:36"
        },
        {
          "id": "MDRW2100011983.1.24",
          "form": "카페오픈알바때 8시까지출근이었는데 진짜 아침에일어나는게 젤힘들었었던...",
          "original_form": "카페오픈알바때 8시까지출근이었는데 진짜 아침에일어나는게 젤힘들었었던...",
          "speaker_id": "1",
          "time": "20210824 18:36"
        },
        {
          "id": "MDRW2100011983.1.25",
          "form": "그거빼면 그래도 괜찮아요!ㅎ.ㅎ..",
          "original_form": "그거빼면 그래도 괜찮아요!ㅎ.ㅎ..",
          "speaker_id": "2",
          "time": "20210824 18:36"
        },
        {
          "id": "MDRW2100011983.1.26",
          "form": "카페알바도 맞아요 일찍이죠?",
          "original_form": "카페알바도 맞아요 일찍이죠?",
          "speaker_id": "2",
          "time": "20210824 18:36"
        },
        {
          "id": "MDRW2100011983.1.27",
          "form": "네 출근고객들 겨냥해야돼서 일찍열더라구요",
          "original_form": "네 출근고객들 겨냥해야돼서 일찍열더라구요",
          "speaker_id": "1",
          "time": "20210824 18:37"
        }
      ]
    }
