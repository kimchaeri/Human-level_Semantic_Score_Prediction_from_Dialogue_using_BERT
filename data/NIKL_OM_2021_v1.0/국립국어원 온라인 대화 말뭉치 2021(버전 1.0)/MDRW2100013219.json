      "utterance": [
        {
          "id": "MDRW2100013219.1.1",
          "form": "요즘 드라마 재밌는거 있음?",
          "original_form": "요즘 드라마 재밌는거 있음?",
          "speaker_id": "1",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100013219.1.2",
          "form": "오 없어",
          "original_form": "오 없어",
          "speaker_id": "2",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100013219.1.3",
          "form": "넷플릭스의 그레이스 앤 프랭키",
          "original_form": "넷플릭스의 그레이스 앤 프랭키",
