      "utterance": [
        {
          "id": "MDRW2100003437.1.1",
          "form": "혹시 반려동물 키우는거 있으세요?",
          "original_form": "혹시 반려동물 키우는거 있으세요?",
          "speaker_id": "1",
          "time": "20210820 23:06"
        },
        {
          "id": "MDRW2100003437.1.2",
          "form": "아니요 name1님은요?",
          "original_form": "아니요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210820 23:06"
        },
        {
          "id": "MDRW2100003437.1.3",
          "form": "저희집은 강아지키워요",
          "original_form": "저희집은 강아지키워요",
          "speaker_id": "1",
          "time": "20210820 23:07"
        },
        {
          "id": "MDRW2100003437.1.4",
          "form": "1마리요?",
          "original_form": "1마리요?",
          "speaker_id": "2",
          "time": "20210820 23:08"
        },
        {
          "id": "MDRW2100003437.1.5",
          "form": "넵 시츄 한마리 키워요",
          "original_form": "넵 시츄 한마리 키워요",
          "speaker_id": "1",
          "time": "20210820 23:08"
        },
        {
          "id": "MDRW2100003437.1.6",
          "form": "키운지 오래되셨나요?",
          "original_form": "키운지 오래되셨나요?",
          "speaker_id": "2",
          "time": "20210820 23:09"
        },
        {
          "id": "MDRW2100003437.1.7",
          "form": "1살부터 키웠는데 이제 9살이에요",
          "original_form": "1살부터 키웠는데 이제 9살이에요",
          "speaker_id": "1",
          "time": "20210820 23:09"
        },
        {
          "id": "MDRW2100003437.1.8",
          "form": "와~오래 키우셨네요~",
          "original_form": "와~오래 키우셨네요~",
          "speaker_id": "2",
          "time": "20210820 23:10"
        },
        {
          "id": "MDRW2100003437.1.9",
          "form": "혹시 키우시고 싶은동물 없어요?",
          "original_form": "혹시 키우시고 싶은동물 없어요?",
          "speaker_id": "1",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003437.1.10",
          "form": "올빼미도 예쁘던데",
          "original_form": "올빼미도 예쁘던데",
          "speaker_id": "1",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003437.1.11",
          "form": "저도 강아지 키우고 싶어요",
          "original_form": "저도 강아지 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003437.1.12",
          "form": "강아지 키우기 어렵나요?",
          "original_form": "강아지 키우기 어렵나요?",
          "speaker_id": "2",
          "time": "20210820 23:12"
        },
        {
          "id": "MDRW2100003437.1.13",
          "form": "털 날리는것만 빼면 좋은거같아요",
          "original_form": "털 날리는것만 빼면 좋은거같아요",
          "speaker_id": "1",
          "time": "20210820 23:13"
        },
        {
          "id": "MDRW2100003437.1.14",
          "form": "지금도 제옆에서 자고있는데 귀엽네요",
          "original_form": "지금도 제옆에서 자고있는데 귀엽네요",
          "speaker_id": "1",
          "time": "20210820 23:13"
        },
        {
          "id": "MDRW2100003437.1.15",
          "form": "진짜 귀여울것 같아요~~~~",
          "original_form": "진짜 귀여울것 같아요~~~~",
          "speaker_id": "2",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003437.1.16",
          "form": "코고는 소리도 들리네요",
          "original_form": "코고는 소리도 들리네요",
          "speaker_id": "1",
          "time": "20210820 23:15"
        },
        {
          "id": "MDRW2100003437.1.17",
          "form": "강아지가 코도 골아요?ㅋㅋ",
