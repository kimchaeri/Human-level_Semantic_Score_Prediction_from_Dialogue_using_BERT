      "utterance": [
        {
          "id": "MDRW2100046363.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20211019 17:42"
        },
        {
          "id": "MDRW2100046363.1.2",
          "form": "안녕하세요! 요즘 즐겨 하시는 취미가 있으신가요?",
          "original_form": "안녕하세요! 요즘 즐겨 하시는 취미가 있으신가요?",
          "speaker_id": "2",
          "time": "20211019 17:42"
        },
        {
          "id": "MDRW2100046363.1.3",
          "form": "헬스해요",
          "original_form": "헬스해요",
          "speaker_id": "1",
          "time": "20211019 17:43"
        },
        {
          "id": "MDRW2100046363.1.4",
          "form": "어떤 운동 주로하시나요?",
          "original_form": "어떤 운동 주로하시나요?",
          "speaker_id": "2",
          "time": "20211019 17:43"
        },
        {
          "id": "MDRW2100046363.1.5",
          "form": "요즘은 유산소만해요",
          "original_form": "요즘은 유산소만해요",
          "speaker_id": "1",
          "time": "20211019 17:44"
        },
        {
          "id": "MDRW2100046363.1.6",
          "form": "로잉머신",
          "original_form": "로잉머신",
          "speaker_id": "1",
          "time": "20211019 17:44"
        },
        {
          "id": "MDRW2100046363.1.7",
          "form": "로잉머신도 유산소운동인가요?",
          "original_form": "로잉머신도 유산소운동인가요?",
          "speaker_id": "2",
          "time": "20211019 17:45"
        },
        {
          "id": "MDRW2100046363.1.8",
          "form": "옙",
          "original_form": "옙",
          "speaker_id": "1",
          "time": "20211019 17:45"
        },
        {
          "id": "MDRW2100046363.1.9",
          "form": "유산소 + 약간의 근력운동",
          "original_form": "유산소 + 약간의 근력운동",
          "speaker_id": "1",
          "time": "20211019 17:45"
        },
        {
          "id": "MDRW2100046363.1.10",
          "form": "하체와 등 단련이 되고",
          "original_form": "하체와 등 단련이 되고",
          "speaker_id": "1",
          "time": "20211019 17:45"
        },
        {
          "id": "MDRW2100046363.1.11",
          "form": "심폐운동도 됩니다",
          "original_form": "심폐운동도 됩니다",
          "speaker_id": "1",
          "time": "20211019 17:45"
        },
        {
          "id": "MDRW2100046363.1.12",
          "form": "심폐운동되는건 처음알았네요 저도 헬스장가면 이 기구로 운동하는데 근력운동만 되는 줄 알았어요 ㅎㅎ",
          "original_form": "심폐운동되는건 처음알았네요 저도 헬스장가면 이 기구로 운동하는데 근력운동만 되는 줄 알았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 17:46"
        },
        {
          "id": "MDRW2100046363.1.13",
          "form": "다른 근력운동은 안하시나요?",
          "original_form": "다른 근력운동은 안하시나요?",
          "speaker_id": "2",
          "time": "20211019 17:46"
        },
        {
          "id": "MDRW2100046363.1.14",
          "form": "했었는데 살이 안빠져서 유산소만 한동안 하려구요",
          "original_form": "했었는데 살이 안빠져서 유산소만 한동안 하려구요",
          "speaker_id": "1",
          "time": "20211019 17:46"
        },
        {
          "id": "MDRW2100046363.1.15",
          "form": "그럼 달리기는 안하시나요?",
          "original_form": "그럼 달리기는 안하시나요?",
          "speaker_id": "2",
          "time": "20211019 17:46"
        },
        {
          "id": "MDRW2100046363.1.16",
          "form": "목디스크때문에 달리기는 안해요 ㅎㅎ",
          "original_form": "목디스크때문에 달리기는 안해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211019 17:46"
        },
        {
          "id": "MDRW2100046363.1.17",
          "form": "앗 목디스크있으면 달리기 하면 안되는군요..",
          "original_form": "앗 목디스크있으면 달리기 하면 안되는군요..",
          "speaker_id": "2",
          "time": "20211019 17:47"
        },
        {
          "id": "MDRW2100046363.1.18",
          "form": "아무래도 불편해서",
          "original_form": "아무래도 불편해서{emoji:…}",
          "speaker_id": "1",
          "time": "20211019 17:47"
        },
        {
          "id": "MDRW2100046363.1.19",
          "form": "그럼 로잉머신으로만 운동하시는건가요?",
          "original_form": "그럼 로잉머신으로만 운동하시는건가요?",
          "speaker_id": "2",
          "time": "20211019 17:47"
        },
        {
          "id": "MDRW2100046363.1.20",
          "form": "네 최근에는 그렇게 하고 있어요",
          "original_form": "네 최근에는 그렇게 하고 있어요",
          "speaker_id": "1",
          "time": "20211019 17:47"
        },
        {
          "id": "MDRW2100046363.1.21",
