        },
        {
          "id": "MDRW2100046199.1.4",
          "form": "어제 드디어..",
          "original_form": "어제 드디어..",
          "speaker_id": "2",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046199.1.5",
          "form": "개막이었는데",
          "original_form": "개막이었는데",
          "speaker_id": "2",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046199.1.6",
          "form": "평 괜찮나보더라고여",
          "original_form": "평 괜찮나보더라고여",
          "speaker_id": "2",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046199.1.7",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046199.1.8",
          "form": "내용이어떤가7지",
          "original_form": "내용이어떤가7지",
          "speaker_id": "1",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046199.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ궁금하긴해",
          "original_form": "ㅋㅋㅋㅋㅋㅋ궁금하긴해",
          "speaker_id": "1",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046199.1.10",
          "form": "내용 자체는",
          "original_form": "내용 자체는",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046199.1.11",
          "form": "오르페우스랑 에우리디케 설화",
          "original_form": "오르페우스랑 에우리디케 설화",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046199.1.12",
          "form": "배경만 현대로 바꾼 그런거던데",
          "original_form": "배경만 현대로 바꾼 그런거던데",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046199.1.13",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046199.1.14",
          "form": "오오오오옹",
          "original_form": "오오오오옹",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046199.1.15",
          "form": "그리고 무대 폼을 안 바꾸고",
          "original_form": "그리고 무대 폼을 안 바꾸고",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046199.1.16",
          "form": "코멧처럼",
          "original_form": "코멧처럼",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046199.1.17",
          "form": "같은 배경에서 계속 쓰면서",
          "original_form": "같은 배경에서 계속 쓰면서",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046199.1.18",
          "form": "배우들 연기로 커버한다고?",
          "original_form": "배우들 연기로 커버한다고?",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046199.1.19",
          "form": "오....",
          "original_form": "오....",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046199.1.20",
          "form": "뭐 그런 얘기는 들었읍니다",
          "original_form": "뭐 그런 얘기는 들었읍니다",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046199.1.21",
          "form": "작품 자체는",
          "original_form": "작품 자체는",
