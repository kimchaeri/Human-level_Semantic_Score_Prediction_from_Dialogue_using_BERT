      "utterance": [
        {
          "id": "MDRW2100017466.1.1",
          "form": "군대요..?",
          "original_form": "군대요..?",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017466.1.2",
          "form": "군대 안 다녀와봐서 잘 모르는,,",
          "original_form": "군대 안 다녀와봐서 잘 모르는,,",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017466.1.3",
          "form": "가야하나요 안가도 되나요",
          "original_form": "가야하나요 안가도 되나요",
          "speaker_id": "2",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017466.1.4",
          "form": "안가는 쪽이요",
          "original_form": "안가는 쪽이요",
          "speaker_id": "1",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017466.1.5",
          "form": "가셔야하나요?",
          "original_form": "가셔야하나요?",
          "speaker_id": "1",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017466.1.6",
          "form": "다녀오셨나요?",
          "original_form": "다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017466.1.7",
          "form": "!! 저도 안가는 쪽이요",
          "original_form": "!! 저도 안가는 쪽이요",
          "speaker_id": "2",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017466.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017466.1.9",
          "form": "군대라",
          "original_form": "군대라",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017466.1.10",
          "form": "오~ 넹ㅎㅎ",
          "original_form": "오~ 넹ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017466.1.11",
          "form": "군대간 남친이 있던 경험이 있으신가요",
          "original_form": "군대간 남친이 있던 경험이 있으신가요",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017466.1.12",
          "form": "고무신? 경험",
          "original_form": "고무신? 경험",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017466.1.13",
          "form": "있으신가요",
          "original_form": "있으신가요",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017466.1.14",
          "form": "쓰읍... 넵",
          "original_form": "쓰읍... 넵",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017466.1.15",
          "form": "그래서 상병 때 헤어지고",
          "original_form": "그래서 상병 때 헤어지고",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017466.1.16",
          "form": "그러면 좋아하는 연예인이 군대간 경험은요?",
          "original_form": "그러면 좋아하는 연예인이 군대간 경험은요?",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017466.1.17",
          "form": "헉..ㅠㅠ",
          "original_form": "헉..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017466.1.18",
          "form": "어떻게든 맞춰본다",
          "original_form": "어떻게든 맞춰본다",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017466.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017466.1.20",
          "form": "연예인 별로 안좋아해서 없었는데",
          "original_form": "연예인 별로 안좋아해서 없었는데",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017466.1.21",
          "form": "오케이.",
          "original_form": "오케이.",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017466.1.22",
          "form": "그럴 수 있다",
          "original_form": "그럴 수 있다",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017466.1.23",
          "form": "최근에 보낼 일이 생겼네요ㅎㅎㅠ",
          "original_form": "최근에 보낼 일이 생겼네요ㅎㅎㅠ",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017466.1.24",
          "form": "그러면 군대리아 드셔보셨",
          "original_form": "그러면 군대리아 드셔보셨",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017466.1.25",
          "form": "허억",
          "original_form": "허억",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017466.1.26",
          "form": "갑자기 흥미진진한데요",
          "original_form": "갑자기 흥미진진한데요",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017466.1.27",
          "form": "누구 좋아하시나요",
          "original_form": "누구 좋아하시나요",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017466.1.28",
          "form": "저.. 온앤오프라고,, 혹시 아실까요",
          "original_form": "저.. 온앤오프라고,, 혹시 아실까요",
          "speaker_id": "1",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017466.1.29",
          "form": "로드투킹덤에 나왔었는데",
          "original_form": "로드투킹덤에 나왔었는데",
          "speaker_id": "1",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017466.1.30",
          "form": "오 알아요 알아요",
          "original_form": "오 알아요 알아요",
          "speaker_id": "2",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017466.1.31",
          "form": "오~~",
          "original_form": "오~~",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017466.1.32",
          "form": "오마이걸",
          "original_form": "오마이걸",
          "speaker_id": "2",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017466.1.33",
          "form": "유아랑",
          "original_form": "유아랑",
          "speaker_id": "2",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017466.1.34",
          "form": "맞습니다 맞아요",
          "original_form": "맞습니다 맞아요",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017466.1.35",
          "form": "같은무대하신분들이었나요??",
          "original_form": "같은무대하신분들이었나요??",
          "speaker_id": "2",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017466.1.36",
          "form": "오 그 무대 너무 멋지더라고요",
          "original_form": "오 그 무대 너무 멋지더라고요",
          "speaker_id": "2",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017466.1.37",
          "form": "사실 옴걸 좋아해서 보기시작한건데",
          "original_form": "사실 옴걸 좋아해서 보기시작한건데",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017466.1.38",
          "form": "그분들중에 벌써 군대가요???",
          "original_form": "그분들중에 벌써 군대가요???",
          "speaker_id": "2",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017466.1.39",
          "form": "맏이들 나이가ㅠㅠ",
          "original_form": "맏이들 나이가ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017466.1.40",
          "form": "94년생인데",
          "original_form": "94년생인데",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
