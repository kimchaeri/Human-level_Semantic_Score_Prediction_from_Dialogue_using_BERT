      "utterance": [
        {
          "id": "MDRW2100024258.1.1",
          "form": "넹넹 내일은 name2님의 맛집을 캐든 집으로 처들어가든 둘중 하나는 쟁취하겠어요!!",
          "original_form": "넹넹 내일은 &name2&님의 맛집을 캐든 집으로 처들어가든 둘중 하나는 쟁취하겠어요!!",
          "speaker_id": "1",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100024258.1.2",
          "form": "어그레시브하시네요",
          "original_form": "어그레시브하시네요",
          "speaker_id": "2",
          "time": "20210831 09:27"
        },
        {
          "id": "MDRW2100024258.1.3",
          "form": "맘에 듭니다",
          "original_form": "맘에 듭니다",
          "speaker_id": "2",
          "time": "20210831 09:27"
        },
        {
          "id": "MDRW2100024258.1.4",
          "form": "오늘 아침은 뭐드실려고하나요?",
          "original_form": "오늘 아침은 뭐드실려고하나요?",
          "speaker_id": "2",
          "time": "20210831 09:27"
        },
        {
          "id": "MDRW2100024258.1.5",
          "form": "원래 아침은 안 먹어서요",
          "original_form": "원래 아침은 안 먹어서요",
          "speaker_id": "1",
          "time": "20210831 09:28"
        },
        {
          "id": "MDRW2100024258.1.6",
          "form": "어제 야식을 늦게 먹기도 했고",
          "original_form": "어제 야식을 늦게 먹기도 했고",
          "speaker_id": "1",
          "time": "20210831 09:28"
        },
        {
          "id": "MDRW2100024258.1.7",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210831 09:28"
        },
        {
          "id": "MDRW2100024258.1.8",
          "form": "아 전 아메한잔 내리고 롤리폴리 오예스 하나",
          "original_form": "아 전 아메한잔 내리고 롤리폴리 오예스 하나",
          "speaker_id": "2",
          "time": "20210831 09:28"
        },
        {
          "id": "MDRW2100024258.1.9",
          "form": "야식 드셨어요?",
          "original_form": "야식 드셨어요?",
          "speaker_id": "2",
          "time": "20210831 09:29"
        },
        {
          "id": "MDRW2100024258.1.10",
          "form": "아침부터 당 충전 제대로 하셨군요",
          "original_form": "아침부터 당 충전 제대로 하셨군요",
          "speaker_id": "1",
          "time": "20210831 09:29"
        },
        {
          "id": "MDRW2100024258.1.11",
          "form": "네 술자리가 짧아 허전해서ㅜ",
          "original_form": "네 술자리가 짧아 허전해서ㅜ",
          "speaker_id": "1",
          "time": "20210831 09:29"
        },
        {
          "id": "MDRW2100024258.1.12",
          "form": "집 와서 닭껍질튀김과 한잔 했죠",
          "original_form": "집 와서 닭껍질튀김과 한잔 했죠",
          "speaker_id": "1",
          "time": "20210831 09:29"
        },
        {
          "id": "MDRW2100024258.1.13",
          "form": "닭껍질튀김 드세요?",
          "original_form": "닭껍질튀김 드세요?",
          "speaker_id": "1",
          "time": "20210831 09:30"
        },
        {
          "id": "MDRW2100024258.1.14",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "2",
          "time": "20210831 10:17"
        },
        {
          "id": "MDRW2100024258.1.15",
          "form": "통닭 껍데기만 튀긴건가요?",
          "original_form": "통닭 껍데기만 튀긴건가요?",
          "speaker_id": "2",
          "time": "20210831 10:17"
        },
        {
          "id": "MDRW2100024258.1.16",
          "form": "네네 kfc에서 시켜먹었어요",
          "original_form": "네네 kfc에서 시켜먹었어요",
          "speaker_id": "1",
          "time": "20210831 10:17"
        },
        {
          "id": "MDRW2100024258.1.17",
          "form": "오 kfc가고싶다",
          "original_form": "오 kfc가고싶다",
          "speaker_id": "2",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100024258.1.18",
          "form": "한창 닭껍질열풍일 때 안 드셨군요",
          "original_form": "한창 닭껍질열풍일 때 안 드셨군요",
          "speaker_id": "1",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100024258.1.19",
          "form": "네 유행 잘 안타는 성격입니다",
          "original_form": "네 유행 잘 안타는 성격입니다",
          "speaker_id": "2",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100024258.1.20",
          "form": "동네에 kfc가 없어요",
          "original_form": "동네에 kfc가 없어요",
          "speaker_id": "2",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100024258.1.21",
          "form": "저는 원래 오돌뼈 닭껍질 닭모래집튀김 이런거 좋아해서요",
          "original_form": "저는 원래 오돌뼈 닭껍질 닭모래집튀김 이런거 좋아해서요",
          "speaker_id": "1",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100024258.1.22",
          "form": "시골이라",
          "original_form": "시골이라",
          "speaker_id": "2",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100024258.1.23",
          "form": "아ㅜ",
          "original_form": "아ㅜ",
          "speaker_id": "1",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100024258.1.24",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100024258.1.25",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "2",
          "time": "20210831 10:19"
        },
        {
          "id": "MDRW2100024258.1.26",
          "form": "치킨집중 닭껍질튀김 하는곳 은근 있어요",
          "original_form": "치킨집중 닭껍질튀김 하는곳 은근 있어요",
          "speaker_id": "1",
          "time": "20210831 10:19"
        },
        {
          "id": "MDRW2100024258.1.27",
          "form": "침고임",
          "original_form": "침고임",
          "speaker_id": "2",
          "time": "20210831 10:19"
        },
        {
          "id": "MDRW2100024258.1.28",
          "form": "호불호 없음 드셔보세요!",
          "original_form": "호불호 없음 드셔보세요!",
          "speaker_id": "1",
          "time": "20210831 10:19"
        },
        {
          "id": "MDRW2100024258.1.29",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210831 10:19"
        },
        {
          "id": "MDRW2100024258.1.30",
          "form": "짭짤 바삭해서 술안주로 딱이에요",
          "original_form": "짭짤 바삭해서 술안주로 딱이에요",
          "speaker_id": "1",
          "time": "20210831 10:19"
        },
        {
          "id": "MDRW2100024258.1.31",
          "form": "오늘 저녁에 와이프 꼬셔보게씁니다",
          "original_form": "오늘 저녁에 와이프 꼬셔보게씁니다",
          "speaker_id": "2",
          "time": "20210831 10:19"
        }
      ]
    }
