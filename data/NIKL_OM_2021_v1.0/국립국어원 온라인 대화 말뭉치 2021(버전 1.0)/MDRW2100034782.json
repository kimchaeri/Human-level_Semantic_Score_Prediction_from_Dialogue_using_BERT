      "utterance": [
        {
          "id": "MDRW2100034782.1.1",
          "form": "안녕하세요~ 식사하셨나요?",
          "original_form": "안녕하세요~ 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100034782.1.2",
          "form": "네 피자먹었어요 뭐드셨어요",
          "original_form": "네 피자먹었어요 뭐드셨어요",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100034782.1.3",
          "form": "저는 양념갈비 먹었어요!",
          "original_form": "저는 양념갈비 먹었어요!",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100034782.1.4",
          "form": "어디 피자 드셨나요?",
          "original_form": "어디 피자 드셨나요?",
          "speaker_id": "1",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100034782.1.5",
          "form": "청년피자먹었어요",
          "original_form": "청년피자먹었어요",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100034782.1.6",
          "form": "우와 양념갈비 외식? 배달?",
          "original_form": "우와 양념갈비 외식? 배달?",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100034782.1.7",
          "form": "집에 재워둔 게 있어서 구워먹었어요!",
          "original_form": "집에 재워둔 게 있어서 구워먹었어요!",
          "speaker_id": "1",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100034782.1.8",
          "form": "갈비 좋아하시나요?",
          "original_form": "갈비 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100034782.1.9",
          "form": "네너무좋아하죠 와 직접 하셨구나",
          "original_form": "네너무좋아하죠 와 직접 하셨구나",
          "speaker_id": "2",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100034782.1.10",
          "form": "가끔 양념 사다가 재워 먹어요 요즘에는 양념이 워낙 잘 나와서ㅎㅎ",
          "original_form": "가끔 양념 사다가 재워 먹어요 요즘에는 양념이 워낙 잘 나와서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100034782.1.11",
          "form": "요리 좋아하세요?",
          "original_form": "요리 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100034782.1.12",
          "form": "좋아는 하는데 갈비까지는 아직",
          "original_form": "좋아는 하는데 갈비까지는 아직",
          "speaker_id": "2",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100034782.1.13",
          "form": "김밥 김치볶음밥 된장찌개 김치찌개 정도해요",
          "original_form": "김밥 김치볶음밥 된장찌개 김치찌개 정도해요",
          "speaker_id": "2",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100034782.1.14",
          "form": "다 너무 맛있죠! 한식 좋아하세요?",
          "original_form": "다 너무 맛있죠! 한식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100034782.1.15",
          "form": "네네 한식도 좋아하고 다좋아요",
          "original_form": "네네 한식도 좋아하고 다좋아요",
          "speaker_id": "2",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100034782.1.16",
          "form": "마라훠궈 좋아하구",
          "original_form": "마라훠궈 좋아하구",
          "speaker_id": "2",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100034782.1.17",
          "form": "헉 저도 마라 좋아해요! 배달 시키면 대부분 마라탕 먹는 거 같아요ㅎㅎ",
          "original_form": "헉 저도 마라 좋아해요! 배달 시키면 대부분 마라탕 먹는 거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100034782.1.18",
          "form": "청년 피자 맛있나요? 저희는 도미노에서 많이 먹어서요!",
          "original_form": "청년 피자 맛있나요? 저희는 도미노에서 많이 먹어서요!",
          "speaker_id": "1",
          "time": "20210828 19:39"
        },
        {
          "id": "MDRW2100034782.1.19",
          "form": "오 우리집도 도미노에요 오늘는 청년 시켜봤어요",
          "original_form": "오 우리집도 도미노에요 오늘는 청년 시켜봤어요",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034782.1.20",
          "form": "청년도 괜찮나요?",
          "original_form": "청년도 괜찮나요?",
          "speaker_id": "1",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100034782.1.21",
          "form": "네청년도 도미노좋아하심 좋아하실거같아요",
          "original_form": "네청년도 도미노좋아하심 좋아하실거같아요",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100034782.1.22",
          "form": "다음에 한번 먹어봐야겠어요ㅎㅎ",
          "original_form": "다음에 한번 먹어봐야겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 19:56"
        }
      ]
    }
