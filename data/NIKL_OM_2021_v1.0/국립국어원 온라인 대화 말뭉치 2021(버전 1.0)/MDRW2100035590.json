      "utterance": [
        {
          "id": "MDRW2100035590.1.1",
          "form": "동아리활동 해보셨나요?",
          "original_form": "동아리활동 해보셨나요?",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035590.1.2",
          "form": "네 저는 대학다닐때 전체동아리는 아니고 과 안에 있는 연극부 했었어요",
          "original_form": "네 저는 대학다닐때 전체동아리는 아니고 과 안에 있는 연극부 했었어요",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035590.1.3",
          "form": "아 저는 클래식기타요",
          "original_form": "아 저는 클래식기타요",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100035590.1.4",
          "form": "오 지금도 기억하시나요?",
          "original_form": "오 지금도 기억하시나요?",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035590.1.5",
          "form": "아니여 다잊어버렸네요ㅎ",
          "original_form": "아니여 다잊어버렸네요ㅎ",
          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100035590.1.6",
          "form": "ㅎㅎ 학교에서 그럼 같이 합주? 하고 그러나요",
          "original_form": "ㅎㅎ 학교에서 그럼 같이 합주? 하고 그러나요",
          "speaker_id": "2",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100035590.1.7",
          "form": "네 공연도했는데",
          "original_form": "네 공연도했는데",
          "speaker_id": "1",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100035590.1.8",
          "form": "이젠 못할듯요ㅎ",
          "original_form": "이젠 못할듯요ㅎ",
          "speaker_id": "1",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100035590.1.9",
          "form": "와 공연까지 ㅋㅋㅋ 그럼 로망스 막 이런 곡도 치셨나요 ㅎㅎ",
          "original_form": "와 공연까지 ㅋㅋㅋ 그럼 로망스 막 이런 곡도 치셨나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100035590.1.10",
          "form": "앗 로망스로 연주했어요ㅎ",
          "original_form": "앗 로망스로 연주했어요ㅎ",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100035590.1.11",
          "form": "오 역시 ㅋㅋㅋ 뭔가 클래식기타 하면 떠오르는 곡이었는데",
          "original_form": "오 역시 ㅋㅋㅋ 뭔가 클래식기타 하면 떠오르는 곡이었는데",
          "speaker_id": "2",
          "time": "20210901 12:08"
        },
        {
          "id": "MDRW2100035590.1.12",
          "form": "그게 대표곡ㅎㅎ",
          "original_form": "그게 대표곡ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:58"
        },
        {
          "id": "MDRW2100035590.1.13",
          "form": "근데 난이도가 좀 있는 편이죠?",
          "original_form": "근데 난이도가 좀 있는 편이죠?",
          "speaker_id": "2",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100035590.1.14",
          "form": "네 클래식기타 자체가 어렵더라구요",
          "original_form": "네 클래식기타 자체가 어렵더라구요",
          "speaker_id": "1",
