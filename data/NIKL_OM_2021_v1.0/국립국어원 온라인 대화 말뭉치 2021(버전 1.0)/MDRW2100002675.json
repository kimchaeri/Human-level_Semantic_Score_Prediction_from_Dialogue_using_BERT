      "utterance": [
        {
          "id": "MDRW2100002675.1.1",
          "form": "커뮤니티 하는 거 있니?",
          "original_form": "커뮤니티 하는 거 있니?",
          "speaker_id": "1",
          "time": "20210820 14:41"
        },
        {
          "id": "MDRW2100002675.1.2",
          "form": "있니?",
          "original_form": "있니?",
          "speaker_id": "1",
          "time": "20210820 14:41"
        },
        {
          "id": "MDRW2100002675.1.3",
          "form": "나는 다음카페 인기글을 주로 봐",
          "original_form": "나는 다음카페 인기글을 주로 봐",
          "speaker_id": "2",
          "time": "20210820 14:41"
        },
        {
          "id": "MDRW2100002675.1.4",
          "form": "다음카페 재미있지.",
          "original_form": "다음카페 재미있지.",
          "speaker_id": "1",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002675.1.5",
          "form": "거기서 모임도 할 수 있는거야?",
          "original_form": "거기서 모임도 할 수 있는거야?",
          "speaker_id": "1",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002675.1.6",
          "form": "인기글을 보면 뉴스 볼 필요가없어",
          "original_form": "인기글을 보면 뉴스 볼 필요가없어",
          "speaker_id": "2",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002675.1.7",
          "form": "정기모임을 가지는 커뮤니티들도 있더라고.",
          "original_form": "정기모임을 가지는 커뮤니티들도 있더라고.",
          "speaker_id": "1",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002675.1.8",
          "form": "아니 그냥 그날 핫한 주제들이 올라와",
          "original_form": "아니 그냥 그날 핫한 주제들이 올라와",
          "speaker_id": "2",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002675.1.9",
          "form": "맞아 세상 이슈가 다 있더라.",
          "original_form": "맞아 세상 이슈가 다 있더라.",
          "speaker_id": "1",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002675.1.10",
          "form": "우와 그럼 새로운사람들을 만나기 좋겠다",
          "original_form": "우와 그럼 새로운사람들을 만나기 좋겠다",
          "speaker_id": "2",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002675.1.11",
          "form": "너는 커뮤니티를 하니?",
          "original_form": "너는 커뮤니티를 하니?",
          "speaker_id": "2",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002675.1.12",
          "form": "아 그러면 커뮤니티 사람들은 만나본 적 없어?",
          "original_form": "아 그러면 커뮤니티 사람들은 만나본 적 없어?",
          "speaker_id": "1",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002675.1.13",
          "form": "나는 커뮤니티를 하지 않아.",
          "original_form": "나는 커뮤니티를 하지 않아.",
          "speaker_id": "1",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002675.1.14",
          "form": "응 만나본적 없어",
          "original_form": "응 만나본적 없어",
          "speaker_id": "2",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002675.1.15",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002675.1.16",
          "form": "트위터 같은곳에서는 오프라인 만남도 자주 한대",
          "original_form": "트위터 같은곳에서는 오프라인 만남도 자주 한대",
          "speaker_id": "2",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002675.1.17",
          "form": "신기하다.",
          "original_form": "신기하다.",
          "speaker_id": "1",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002675.1.18",
          "form": "혹시 덕질상대같은건 없니",
          "original_form": "혹시 덕질상대같은건 없니",
          "speaker_id": "2",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002675.1.19",
          "form": "트위터는 덕후들이 많이하는것같아",
          "original_form": "트위터는 덕후들이 많이하는것같아",
          "speaker_id": "2",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002675.1.20",
          "form": "나도 커뮤니티 구경을 해 보고 싶은데 다음을 설치해볼까",
          "original_form": "나도 커뮤니티 구경을 해 보고 싶은데 다음을 설치해볼까",
          "speaker_id": "1",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002675.1.21",
          "form": "아 아쉽게도 나는 깊게 좋아하는 것이 없어서.",
          "original_form": "아 아쉽게도 나는 깊게 좋아하는 것이 없어서.",
          "speaker_id": "1",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002675.1.22",
          "form": "너는 트위터도 하는거니?",
          "original_form": "너는 트위터도 하는거니?",
          "speaker_id": "1",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002675.1.23",
          "form": "다음카페 재밋어! 어플 설치해봐",
          "original_form": "다음카페 재밋어! 어플 설치해봐",
          "speaker_id": "2",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002675.1.24",
          "form": "인기글은 가입하지않고도 볼 수 있어",
          "original_form": "인기글은 가입하지않고도 볼 수 있어",
          "speaker_id": "2",
          "time": "20210820 14:44"
        }
      ]
