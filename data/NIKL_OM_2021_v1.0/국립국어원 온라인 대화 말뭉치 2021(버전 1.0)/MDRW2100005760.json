      "utterance": [
        {
          "id": "MDRW2100005760.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 11:13"
        },
        {
          "id": "MDRW2100005760.1.2",
          "form": "안녕하세요\n가족 중 누가 군대에 갔나요?",
          "original_form": "안녕하세요\n가족 중 누가 군대에 갔나요?",
          "speaker_id": "2",
          "time": "20210907 11:14"
        },
        {
          "id": "MDRW2100005760.1.3",
          "form": "음 갔다제대했어요",
          "original_form": "음 갔다제대했어요",
          "speaker_id": "1",
          "time": "20210907 11:15"
        },
        {
          "id": "MDRW2100005760.1.4",
          "form": "누구있으신가요?",
          "original_form": "누구있으신가요?",
          "speaker_id": "1",
          "time": "20210907 11:15"
        },
        {
          "id": "MDRW2100005760.1.5",
          "form": "두 아들이 군대에 있어요",
          "original_form": "두 아들이 군대에 있어요",
          "speaker_id": "2",
          "time": "20210907 11:16"
        },
        {
          "id": "MDRW2100005760.1.6",
          "form": "육군인가요",
          "original_form": "육군인가요",
          "speaker_id": "1",
          "time": "20210907 11:16"
        },
        {
          "id": "MDRW2100005760.1.7",
          "form": "네 한명은 포병이구 한명은 경비소대에서 근무해요",
          "original_form": "네 한명은 포병이구 한명은 경비소대에서 근무해요",
          "speaker_id": "2",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100005760.1.8",
          "form": "하유 그러시군요",
          "original_form": "하유 그러시군요",
          "speaker_id": "1",
          "time": "20210907 11:26"
        },
        {
          "id": "MDRW2100005760.1.9",
          "form": "잘아시겠어요",
          "original_form": "잘아시겠어요",
          "speaker_id": "1",
          "time": "20210907 11:26"
        },
        {
          "id": "MDRW2100005760.1.10",
          "form": "잘몰라요\n걱정안하게 다들 잘 지낸다고 해서요",
          "original_form": "잘몰라요\n걱정안하게 다들 잘 지낸다고 해서요",
          "speaker_id": "2",
          "time": "20210907 11:28"
        },
        {
          "id": "MDRW2100005760.1.11",
          "form": "그럴거에요!",
          "original_form": "그럴거에요!",
          "speaker_id": "1",
          "time": "20210907 11:29"
        },
        {
          "id": "MDRW2100005760.1.12",
          "form": "가족분은 군생활 잘하고 제대한거지요?",
          "original_form": "가족분은 군생활 잘하고 제대한거지요?",
          "speaker_id": "2",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100005760.1.13",
          "form": "네네 잘 하고 건강히왔어요",
          "original_form": "네네 잘 하고 건강히왔어요",
          "speaker_id": "1",
          "time": "20210907 11:34"
        },
        {
          "id": "MDRW2100005760.1.14",
          "form": "다행이네요\n지금은 많이좋아졌다고는 하지만 군대에서 갇혀지내는게 무지 힘들거에요",
          "original_form": "다행이네요\n지금은 많이좋아졌다고는 하지만 군대에서 갇혀지내는게 무지 힘들거에요",
          "speaker_id": "2",
          "time": "20210907 11:36"
        },
        {
          "id": "MDRW2100005760.1.15",
          "form": "맞아요ㅠㅜ한창나이에",
          "original_form": "맞아요ㅠㅜ한창나이에",
          "speaker_id": "1",
