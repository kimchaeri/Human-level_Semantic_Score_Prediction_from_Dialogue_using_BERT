      "utterance": [
        {
          "id": "MDRW2100002483.1.1",
          "form": "다이어트하시나요",
          "original_form": "다이어트하시나요",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100002483.1.2",
          "form": "할것같아요?",
          "original_form": "할것같아요?",
          "speaker_id": "1",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100002483.1.3",
          "form": "할수도잇ㅈㅡ죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "할수도잇ㅈㅡ죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100002483.1.4",
          "form": "늦게 답장하시네",
          "original_form": "늦게 답장하시네",
          "speaker_id": "1",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100002483.1.5",
          "form": "늦잠자셨나요",
          "original_form": "늦잠자셨나요",
          "speaker_id": "1",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100002483.1.6",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100002483.1.7",
          "form": "밥안드시나요",
          "original_form": "밥안드시나요",
          "speaker_id": "2",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100002483.1.8",
          "form": "건강식드세요",
          "original_form": "건강식드세요",
          "speaker_id": "2",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100002483.1.9",
          "form": "쉬는날에도 수면주기  지켜주세요",
          "original_form": "쉬는날에도 수면주기  지켜주세요",
          "speaker_id": "1",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100002483.1.10",
          "form": "출근일처럼 인나요",
          "original_form": "출근일처럼 인나요",
          "speaker_id": "1",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100002483.1.11",
          "form": "초밥갑니다",
          "original_form": "초밥갑니다",
          "speaker_id": "1",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100002483.1.12",
          "form": "부러우셔서 그런거죠",
          "original_form": "부러우셔서 그런거죠",
          "speaker_id": "2",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100002483.1.13",
          "form": "맛점하세여",
          "original_form": "맛점하세여",
          "speaker_id": "2",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100002483.1.14",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100002483.1.15",
          "form": "요즘 운동 안 하시나요",
          "original_form": "요즘 운동 안 하시나요",
          "speaker_id": "1",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100002483.1.16",
          "form": "코로나땜에 운동못가요",
          "original_form": "코로나땜에 운동못가요",
          "speaker_id": "2",
          "time": "20210827 11:53"
        },
        {
          "id": "MDRW2100002483.1.17",
          "form": "비와서 밖에서도 운동못하고",
          "original_form": "비와서 밖에서도 운동못하고",
          "speaker_id": "2",
          "time": "20210827 11:53"
        },
        {
          "id": "MDRW2100002483.1.18",
          "form": "홈트하시죠",
          "original_form": "홈트하시죠",
          "speaker_id": "1",
          "time": "20210827 11:53"
        },
        {
          "id": "MDRW2100002483.1.19",
          "form": "오..싫아요",
          "original_form": "오..싫아요",
          "speaker_id": "2",
          "time": "20210827 11:54"
        },
        {
          "id": "MDRW2100002483.1.20",
          "form": "강아지가 홈트하면 구하러옴",
          "original_form": "강아지가 홈트하면 구하러옴",
          "speaker_id": "2",
          "time": "20210827 11:54"
        },
        {
          "id": "MDRW2100002483.1.21",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
