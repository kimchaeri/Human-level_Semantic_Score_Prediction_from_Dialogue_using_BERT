      "utterance": [
        {
          "id": "MDRW2100045008.1.1",
          "form": "어 낼까지 휴가야",
          "original_form": "어 낼까지 휴가야",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045008.1.2",
          "form": "넌 낼 무슨일 있니",
          "original_form": "넌 낼 무슨일 있니",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045008.1.3",
          "form": "나는 내일 공부하지 집에서",
          "original_form": "나는 내일 공부하지 집에서",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045008.1.4",
          "form": "할 공부가 많으니",
          "original_form": "할 공부가 많으니",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045008.1.5",
          "form": "웅 좀 있어 집에서 할생각이야ㅠ 엄마는?",
          "original_form": "웅 좀 있어 집에서 할생각이야ㅠ 엄마는?",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045008.1.6",
          "form": "휴간데 뭐해? 집에 있을거?",
          "original_form": "휴간데 뭐해? 집에 있을거?",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045008.1.7",
          "form": "아직 까진 집에 있을 생각이야",
          "original_form": "아직 까진 집에 있을 생각이야",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045008.1.8",
          "form": "글쿤 그럼 낼 같이 집에서 밥먹어야 겠네 ㅋㅋ",
          "original_form": "글쿤 그럼 낼 같이 집에서 밥먹어야 겠네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045008.1.9",
          "form": "아무래도 비오니까 운동 같은것도 못하자나",
          "original_form": "아무래도 비오니까 운동 같은것도 못하자나",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045008.1.10",
          "form": "아침에 비안오면 조금 하던지",
          "original_form": "아침에 비안오면 조금 하던지",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045008.1.11",
          "form": "옹 아침에 나 깨워줘 그럼~~",
          "original_form": "옹 아침에 나 깨워줘 그럼~~",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045008.1.12",
          "form": "알았어 깨우면 잘일어나고",
          "original_form": "알았어 깨우면 잘일어나고",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045008.1.13",
          "form": "당연하지~ 오늘 내방 침대에서 잘거얌",
          "original_form": "당연하지~ 오늘 내방 침대에서 잘거얌",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045008.1.14",
          "form": "좋은꿈꾸고 낼 활동하자",
          "original_form": "좋은꿈꾸고 낼 활동하자",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045008.1.15",
          "form": "좋아~! 내일도 엄마 화이팅",
          "original_form": "좋아~! 내일도 엄마 화이팅",
          "speaker_id": "1",
