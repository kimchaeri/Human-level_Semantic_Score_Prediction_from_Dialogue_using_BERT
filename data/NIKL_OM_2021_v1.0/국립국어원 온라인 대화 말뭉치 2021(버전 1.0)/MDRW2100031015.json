      "utterance": [
        {
          "id": "MDRW2100031015.1.1",
          "form": "학교다닐때 어떠셨나요",
          "original_form": "학교다닐때 어떠셨나요",
          "speaker_id": "2",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100031015.1.2",
          "form": "학교다닐때 숙제가 그렇게 싫었어요",
          "original_form": "학교다닐때 숙제가 그렇게 싫었어요",
          "speaker_id": "1",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100031015.1.3",
          "form": "저도요 ㅋㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100031015.1.4",
          "form": "과연 좋아하는 학생이 있었을까요",
          "original_form": "과연 좋아하는 학생이 있었을까요",
          "speaker_id": "1",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100031015.1.5",
          "form": "없었을거같네요",
          "original_form": "없었을거같네요",
          "speaker_id": "2",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100031015.1.6",
          "form": "그쵸 모범생도 안좋아했을거 같아요",
          "original_form": "그쵸 모범생도 안좋아했을거 같아요",
          "speaker_id": "1",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100031015.1.7",
          "form": "맞아요 공부 잘하는것과 좋아하는건 다르죠",
          "original_form": "맞아요 공부 잘하는것과 좋아하는건 다르죠",
          "speaker_id": "2",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100031015.1.8",
          "form": "급식먹는 시간이 제일 좋았어요",
          "original_form": "급식먹는 시간이 제일 좋았어요",
          "speaker_id": "1",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100031015.1.9",
          "form": "급식먹을때 뛰어가야하죠",
          "original_form": "급식먹을때 뛰어가야하죠",
          "speaker_id": "2",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100031015.1.10",
          "form": "빨리 먹으려고 ㅎㅎ",
          "original_form": "빨리 먹으려고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100031015.1.11",
          "form": "종치자마자 친구들하고 뛰어가고 그랬죠",
          "original_form": "종치자마자 친구들하고 뛰어가고 그랬죠",
          "speaker_id": "2",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100031015.1.12",
          "form": "그땐 그런게 그렇게 재밌었어요",
          "original_form": "그땐 그런게 그렇게 재밌었어요",
          "speaker_id": "1",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100031015.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100031015.1.14",
          "form": "급식이 학교생활의 큰 즐거움이었죠 ㅎㅎ",
          "original_form": "급식이 학교생활의 큰 즐거움이었죠 ㅎㅎ",
