      "utterance": [
        {
          "id": "MDRW2100045871.1.1",
          "form": "그니까 또 기웃기웃거리고ㅜㅜ",
          "original_form": "그니까 또 기웃기웃거리고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045871.1.2",
          "form": "진짜 왜그러는지 모르겠어요",
          "original_form": "진짜 왜그러는지 모르겠어요",
          "speaker_id": "1",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045871.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045871.1.4",
          "form": "악순횐 ㅋㅋ",
          "original_form": "악순횐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045871.1.5",
          "form": "악순환",
          "original_form": "악순환",
          "speaker_id": "1",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045871.1.6",
          "form": "진짜요ㅋㅋㅋㅋㅋ",
          "original_form": "진짜요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045871.1.7",
          "form": "근데 자제가 안 돼요",
          "original_form": "근데 자제가 안 돼요",
          "speaker_id": "2",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045871.1.8",
          "form": "또 튀는옷은 자주입으면 맨날입는거같아서 ㅋㅋ",
          "original_form": "또 튀는옷은 자주입으면 맨날입는거같아서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045871.1.9",
          "form": "그죠ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그죠ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045871.1.10",
          "form": "텀을 길게 둬야하는데",
          "original_form": "텀을 길게 둬야하는데",
          "speaker_id": "2",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045871.1.11",
          "form": "그러면 또 계절이 가고ㅠㅠ",
          "original_form": "그러면 또 계절이 가고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045871.1.12",
          "form": "그래서 또 그러면 원피스 위에 입을 니트조끼 보고 그러고있네요",
          "original_form": "그래서 또 그러면 원피스 위에 입을 니트조끼 보고 그러고있네요",
          "speaker_id": "1",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045871.1.13",
          "form": "맞아요ㅠㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045871.1.14",
          "form": "다른옷처럼보이게 ㅋㅋ",
          "original_form": "다른옷처럼보이게 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045871.1.15",
          "form": "제 맘이랑 똑같으시네요ㅋㅋㅋㅋ",
          "original_form": "제 맘이랑 똑같으시네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045871.1.16",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045871.1.17",
          "form": "그래도 긴팔원피스는 겨울에도 목티에입고..",
          "original_form": "그래도 긴팔원피스는 겨울에도 목티에입고..",
          "speaker_id": "1",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045871.1.18",
          "form": "이게 택배 vip들의 루틴...",
          "original_form": "이게 택배 vip들의 루틴...",
          "speaker_id": "2",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045871.1.19",
          "form": "그러면되지않을까싶어서",
          "original_form": "그러면되지않을까싶어서",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045871.1.20",
          "form": "맞아요! 현명한 쇼핑이라고요",
          "original_form": "맞아요! 현명한 쇼핑이라고요",
          "speaker_id": "2",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045871.1.21",
          "form": "목티랑 입음 되죠",
          "original_form": "목티랑 입음 되죠",
          "speaker_id": "2",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045871.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045871.1.23",
          "form": "조끼도 있고!",
          "original_form": "조끼도 있고!",
          "speaker_id": "2",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045871.1.24",
          "form": "크 잘 샀네",
          "original_form": "크 잘 샀네",
          "speaker_id": "2",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045871.1.25",
          "form": "그래서 여름빼고 다입는다고생각하면",
          "original_form": "그래서 여름빼고 다입는다고생각하면",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045871.1.26",
          "form": "이거 완전 쇼핑왕이네요",
          "original_form": "이거 완전 쇼핑왕이네요",
          "speaker_id": "2",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045871.1.27",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045871.1.28",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045871.1.29",
          "form": "당당해집시다",
          "original_form": "당당해집시다",
          "speaker_id": "2",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045871.1.30",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045871.1.31",
          "form": "재질감른 좀얇아도",
          "original_form": "재질감른 좀얇아도",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045871.1.32",
          "form": "택배만 어떻게 하면ㅋㅋㅋㅋ",
          "original_form": "택배만 어떻게 하면ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045871.1.33",
          "form": "택배포인트 적립하고 계시죠?",
          "original_form": "택배포인트 적립하고 계시죠?",
          "speaker_id": "2",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100045871.1.34",
          "form": "아뇨그게뭐죠...?",
          "original_form": "아뇨그게뭐죠...?",
          "speaker_id": "1",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100045871.1.35",
          "form": "택배파인더 어플 깔면 적립가능해요",
