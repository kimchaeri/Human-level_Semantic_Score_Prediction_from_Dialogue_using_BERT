      "utterance": [
        {
          "id": "MDRW2100020500.1.1",
          "form": "전 요즘 치킨 유목민이랍니다",
          "original_form": "전 요즘 치킨 유목민이랍니다",
          "speaker_id": "2",
          "time": "20210828 08:30"
        },
        {
          "id": "MDRW2100020500.1.2",
          "form": "유목민이라",
          "original_form": "유목민이라",
          "speaker_id": "1",
          "time": "20210828 08:30"
        },
        {
          "id": "MDRW2100020500.1.3",
          "form": "씁",
          "original_form": "씁",
          "speaker_id": "1",
          "time": "20210828 08:30"
        },
        {
          "id": "MDRW2100020500.1.4",
          "form": "이사를 왔거든요",
          "original_form": "이사를 왔거든요",
          "speaker_id": "2",
          "time": "20210828 08:30"
        },
        {
          "id": "MDRW2100020500.1.5",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210828 08:30"
        },
        {
          "id": "MDRW2100020500.1.6",
          "form": "슬프다",
          "original_form": "슬프다",
          "speaker_id": "1",
          "time": "20210828 08:30"
        },
        {
          "id": "MDRW2100020500.1.7",
          "form": "이사오기 전에는 종국이 치킨이 최애였는데...",
          "original_form": "이사오기 전에는 종국이 치킨이 최애였는데...",
          "speaker_id": "2",
          "time": "20210828 08:30"
        },
        {
          "id": "MDRW2100020500.1.8",
          "form": "기존의 맛집을 떠나보내시기 아쉬우셨겠어요",
          "original_form": "기존의 맛집을 떠나보내시기 아쉬우셨겠어요",
          "speaker_id": "1",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020500.1.9",
          "form": "그 분점만 맛있는 거였어요..",
          "original_form": "그 분점만 맛있는 거였어요..",
          "speaker_id": "2",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020500.1.10",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020500.1.11",
          "form": "이사가면 이제 모든걸 비교하게되더라구요",
          "original_form": "이사가면 이제 모든걸 비교하게되더라구요",
          "speaker_id": "1",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020500.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020500.1.13",
          "form": "아.. 전에살던데는 이런맛이었는데",
          "original_form": "아.. 전에살던데는 이런맛이었는데",
          "speaker_id": "1",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020500.1.14",
          "form": "아.. 부족해 부족하다고!!!",
          "original_form": "아.. 부족해 부족하다고!!!",
          "speaker_id": "1",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020500.1.15",
          "form": "정말 아쉬운거만 더 생각이 나더라구요",
          "original_form": "정말 아쉬운거만 더 생각이 나더라구요",
          "speaker_id": "2",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020500.1.16",
          "form": "정답이에요",
          "original_form": "정답이에요",
          "speaker_id": "2",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020500.1.17",
          "form": "피자로 한번 가볼까요?",
          "original_form": "피자로 한번 가볼까요?",
          "speaker_id": "2",
          "time": "20210828 08:32"
        },
        {
          "id": "MDRW2100020500.1.18",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210828 08:32"
        },
        {
          "id": "MDRW2100020500.1.19",
          "form": "전 피자도 좋거든요",
          "original_form": "전 피자도 좋거든요",
          "speaker_id": "1",
          "time": "20210828 08:32"
        },
        {
          "id": "MDRW2100020500.1.20",
          "form": "약간 탄수화물의 완전체",
          "original_form": "약간 탄수화물의 완전체",
          "speaker_id": "1",
          "time": "20210828 08:32"
        },
        {
          "id": "MDRW2100020500.1.21",
          "form": "어디꺼 좋아하시나요",
          "original_form": "어디꺼 좋아하시나요",
          "speaker_id": "2",
          "time": "20210828 08:32"
        },
        {
          "id": "MDRW2100020500.1.22",
          "form": "전 사실 입맛이 저렴해서",
          "original_form": "전 사실 입맛이 저렴해서",
          "speaker_id": "1",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020500.1.23",
          "form": "피자스쿨좋아합니다",
          "original_form": "피자스쿨좋아합니다",
          "speaker_id": "1",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020500.1.24",
          "form": "앗 역시 피자스쿨이죠",
          "original_form": "앗 역시 피자스쿨이죠",
          "speaker_id": "2",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020500.1.25",
          "form": "맛과 가격을 모두 만족시키는",
          "original_form": "맛과 가격을 모두 만족시키는",
          "speaker_id": "2",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020500.1.26",
          "form": "ㅋㅋㅋㅋ 비싼거 맛있긴 한데 돌고돌아 다시 피자스쿨입학을 하죠",
          "original_form": "ㅋㅋㅋㅋ 비싼거 맛있긴 한데 돌고돌아 다시 피자스쿨입학을 하죠",
          "speaker_id": "1",
          "time": "20210828 08:33"
        }
      ]
    }
