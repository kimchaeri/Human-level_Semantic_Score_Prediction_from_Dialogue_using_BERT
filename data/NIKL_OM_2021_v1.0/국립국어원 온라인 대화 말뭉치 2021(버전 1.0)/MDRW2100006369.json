      "utterance": [
        {
          "id": "MDRW2100006369.1.1",
          "form": "물뿌리개를 하나 사야겠어",
          "original_form": "물뿌리개를 하나 사야겠어",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100006369.1.2",
          "form": "밭에 있는 건 너무 오래 썼어",
          "original_form": "밭에 있는 건 너무 오래 썼어",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100006369.1.3",
          "form": "지원금으로 뭐 살까요",
          "original_form": "지원금으로 뭐 살까요",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100006369.1.4",
          "form": "너희들 필요한 거 사",
          "original_form": "너희들 필요한 거 사",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100006369.1.5",
          "form": "맛있는거나 사먹을까요",
          "original_form": "맛있는거나 사먹을까요",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100006369.1.6",
          "form": "전부 다 그렇게 쓸 순 없잖아",
          "original_form": "전부 다 그렇게 쓸 순 없잖아",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100006369.1.7",
          "form": "안경살까요?",
          "original_form": "안경살까요?",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100006369.1.8",
          "form": "안경 바꿀 때가 됐나",
          "original_form": "안경 바꿀 때가 됐나",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100006369.1.9",
          "form": "네 돋보기 바꾸세요",
          "original_form": "네 돋보기 바꾸세요",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100006369.1.10",
          "form": "아니면 선글라스",
          "original_form": "아니면 선글라스",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100006369.1.11",
          "form": "돋보기는 바꾼지 그렇게 오래되지 않았어",
          "original_form": "돋보기는 바꾼지 그렇게 오래되지 않았어",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100006369.1.12",
          "form": "그냥 일상용 안경을 바꾸셔도 될거 같아요",
          "original_form": "그냥 일상용 안경을 바꾸셔도 될거 같아요",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100006369.1.13",
          "form": "안경을  하고 싶어도 마땅히 괜찮은 데가 없어",
          "original_form": "안경을  하고 싶어도 마땅히 괜찮은 데가 없어",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100006369.1.14",
          "form": "그게 문제예요",
          "original_form": "그게 문제예요",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100006369.1.15",
          "form": "동네에서  괜찮은 안경점을 하나 찾아보자",
          "original_form": "동네에서  괜찮은 안경점을 하나 찾아보자",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100006369.1.16",
          "form": "네 한번 찾아보고 없으면",
          "original_form": "네 한번 찾아보고 없으면",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100006369.1.17",
          "form": "어쩔수 없어요 병원비 하세요",
          "original_form": "어쩔수 없어요 병원비 하세요",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100006369.1.18",
          "form": "그래 치과에서 쓰지 뭐",
          "original_form": "그래 치과에서 쓰지 뭐",
          "speaker_id": "1",
          "time": "20210908 01:15"
        }
      ]
