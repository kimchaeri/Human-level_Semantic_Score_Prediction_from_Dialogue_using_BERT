      "utterance": [
        {
          "id": "MDRW2100044278.1.1",
          "form": "거기 줄 너무 길더라",
          "original_form": "거기 줄 너무 길더라",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044278.1.2",
          "form": "헐 아직도 길어...?",
          "original_form": "헐 아직도 길어...?",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044278.1.3",
          "form": "응 저저번주에 갔을 때 오픈 10분 전인데 줄이 가게를 둘러싸서",
          "original_form": "응 저저번주에 갔을 때 오픈 10분 전인데 줄이 가게를 둘러싸서",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044278.1.4",
          "form": "걍 포기함",
          "original_form": "걍 포기함",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044278.1.5",
          "form": "........하긴 그 연남도 오픈전에 줄선다고 들음",
          "original_form": "........하긴 그 연남도 오픈전에 줄선다고 들음",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044278.1.6",
          "form": "다들 진심이네...",
          "original_form": "다들 진심이네...",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044278.1.7",
          "form": "대신 크로넛 맛집 찾음",
          "original_form": "대신 크로넛 맛집 찾음",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044278.1.8",
          "form": "비비드라는 곳인데 혹시 앎??",
          "original_form": "비비드라는 곳인데 혹시 앎??",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044278.1.9",
          "form": "헐 뭔가오 첨들음",
          "original_form": "헐 뭔가오 첨들음",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044278.1.10",
          "form": "가로수길에 있대 본점이",
          "original_form": "가로수길에 있대 본점이",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044278.1.11",
          "form": "크로플 도넛이라는데 맛있더라구",
          "original_form": "크로플 도넛이라는데 맛있더라구",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044278.1.12",
          "form": "헐 가로수길",
          "original_form": "헐 가로수길",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044278.1.13",
          "form": "검색해보니 맛이 읍을수 없는 비주얼이다",
          "original_form": "검색해보니 맛이 읍을수 없는 비주얼이다",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044278.1.14",
          "form": "그쳐",
          "original_form": "그쳐",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044278.1.15",
          "form": "가십셔",
          "original_form": "가십셔",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044278.1.16",
          "form": "먹으로 가십셔 나대신",
          "original_form": "먹으로 가십셔 나대신",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044278.1.17",
          "form": "가로수길은 멀어오....",
          "original_form": "가로수길은 멀어오....",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044278.1.18",
          "form": "본점은 더 맛날거야",
          "original_form": "본점은 더 맛날거야",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044278.1.19",
          "form": "앗...",
          "original_form": "앗...",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044278.1.20",
          "form": "오세오 동탄",
          "original_form": "오세오 동탄",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044278.1.21",
          "form": "버스배차가 미쳐돌아가는",
          "original_form": "버스배차가 미쳐돌아가는",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044278.1.22",
          "form": "오세오 동탄",
          "original_form": "오세오 동탄",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044278.1.23",
          "form": "파롤앤랑그 본점 갈 생각 없음??",
          "original_form": "파롤앤랑그 본점 갈 생각 없음??",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044278.1.24",
          "form": "나 대신 먹어줘...",
          "original_form": "나 대신 먹어줘...",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044278.1.25",
          "form": "앗.....일찍 일어나는 새가 아니라",
          "original_form": "앗.....일찍 일어나는 새가 아니라",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044278.1.26",
          "form": "제가 미안하구...",
          "original_form": "제가 미안하구...",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044278.1.27",
          "form": "동탄가서 백화점 구경하고 싶당",
          "original_form": "동탄가서 백화점 구경하고 싶당",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044278.1.28",
          "form": "이제 오후에 6인제한이니 만나도 될듯 싶어오",
          "original_form": "이제 오후에 6인제한이니 만나도 될듯 싶어오",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044278.1.29",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044278.1.30",
          "form": "그러네 진짜",
          "original_form": "그러네 진짜",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044278.1.31",
          "form": "이제 조개구이 먹을 수 있겠다 ㅋㅋㅋㅋ",
          "original_form": "이제 조개구이 먹을 수 있겠다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044278.1.32",
          "form": "우리 성심당 가자",
          "original_form": "우리 성심당 가자",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044278.1.33",
          "form": "지역통일햌ㅋㅋㅋ어디 갈거야",
          "original_form": "지역통일햌ㅋㅋㅋ어디 갈거야",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044278.1.34",
          "form": "대전 조개구이 찾아야 하나",
          "original_form": "대전 조개구이 찾아야 하나",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044278.1.35",
          "form": "전국일주 그즈ㅏㅏㅏㅏ",
          "original_form": "전국일주 그즈ㅏㅏㅏㅏ",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044278.1.36",
          "form": "비빔칼국수 먹고싶다 ㅋㅋㅋㅋ",
          "original_form": "비빔칼국수 먹고싶다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044278.1.37",
          "form": "아잌ㅋㅋㅋㅋㅋ",
          "original_form": "아잌ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044278.1.38",
          "form": "난 조개구이랑 곱창.....",
          "original_form": "난 조개구이랑 곱창.....",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044278.1.39",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044278.1.40",
          "form": "비빔칼국슈도 궁금하긴혀",
          "original_form": "비빔칼국슈도 궁금하긴혀",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044278.1.41",
          "form": "아침 조개구이 저녁 곱창",
          "original_form": "아침 조개구이 저녁 곱창",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044278.1.42",
          "form": "점심 비빔칼국수",
          "original_form": "점심 비빔칼국수",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044278.1.43",
          "form": "앜ㅋㅋㅋㅋ아침부터 만나 달리는 건가오",
          "original_form": "앜ㅋㅋㅋㅋ아침부터 만나 달리는 건가오",
