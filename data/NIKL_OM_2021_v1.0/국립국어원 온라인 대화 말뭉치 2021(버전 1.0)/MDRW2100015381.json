      "utterance": [
        {
          "id": "MDRW2100015381.1.1",
          "form": "연애할 생각없냐",
          "original_form": "연애할 생각없냐",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015381.1.2",
          "form": "아직 없는데",
          "original_form": "아직 없는데",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015381.1.3",
          "form": "아니야 긍적적으로 생각해봐",
          "original_form": "아니야 긍적적으로 생각해봐",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015381.1.4",
          "form": "남자가 없다고!",
          "original_form": "남자가 없다고!",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015381.1.5",
          "form": "초록이 애기 넘 이쁘더라",
          "original_form": "초록이 애기 넘 이쁘더라",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015381.1.6",
          "form": "응 많이 이뻐해",
          "original_form": "응 많이 이뻐해",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015381.1.7",
          "form": "나도 손녀 보고싶다",
          "original_form": "나도 손녀 보고싶다",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015381.1.8",
          "form": "예정이 없는데",
          "original_form": "예정이 없는데",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015381.1.9",
          "form": "아니야 나이먹어서 어쩔라고",
          "original_form": "아니야 나이먹어서 어쩔라고",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015381.1.10",
          "form": "왜애ㅠ",
          "original_form": "왜애ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015381.1.11",
          "form": "걱정되서 그런다",
          "original_form": "걱정되서 그런다",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015381.1.12",
          "form": "잘 살거니까 걱정허지마슈",
          "original_form": "잘 살거니까 걱정허지마슈",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015381.1.13",
          "form": "난 니네 있지만 너 나중에 약 발라줄사람 없을까봐 그런다",
          "original_form": "난 니네 있지만 너 나중에 약 발라줄사람 없을까봐 그런다",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015381.1.14",
          "form": "아 걱정말라니까 잘 살게찡",
          "original_form": "아 걱정말라니까 잘 살게찡",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
