      "utterance": [
        {
          "id": "MDRW2100025858.1.1",
          "form": "체인지데이즈 성호 출연자에 대해서 어떻게 생각하시나요",
          "original_form": "체인지데이즈 성호 출연자에 대해서 어떻게 생각하시나요",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025858.1.2",
          "form": "저요...진짜... 미쳐요 제가",
          "original_form": "저요...진짜... 미쳐요 제가",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025858.1.3",
          "form": "혈압이올라서요",
          "original_form": "혈압이올라서요",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025858.1.4",
          "form": "이해가안돼요",
          "original_form": "이해가안돼요",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025858.1.5",
          "form": "저두요.. 어떻게 사람이 그럴수가있죠...?",
          "original_form": "저두요.. 어떻게 사람이 그럴수가있죠...?",
          "speaker_id": "1",
          "time": "20210901 17:50"
        },
        {
          "id": "MDRW2100025858.1.6",
          "form": "저진짜 어제 넷플로 보는데 화나서 혼자 욕하면서봤어요",
          "original_form": "저진짜 어제 넷플로 보는데 화나서 혼자 욕하면서봤어요",
          "speaker_id": "1",
          "time": "20210901 17:50"
        },
        {
          "id": "MDRW2100025858.1.7",
          "form": "저도 진짜ㅋ 여러번 놀랐습니다..",
          "original_form": "저도 진짜ㅋ 여러번 놀랐습니다..",
          "speaker_id": "2",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100025858.1.8",
          "form": "내 마음이 흔들리고 있습니다",
          "original_form": "내 마음이 흔들리고 있습니다",
          "speaker_id": "1",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100025858.1.9",
          "form": "여기부분진짜......",
          "original_form": "여기부분진짜......",
          "speaker_id": "1",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100025858.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 염 병 하더라귱요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 염 병 하더라귱요",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025858.1.11",
          "form": "그맘  상미한테좀  쓰지",
          "original_form": "그맘  상미한테좀  쓰지",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025858.1.12",
          "form": "그니까요... 그리고 뒤에서 그렇게 호박씨 까놓구선 상미랑 방에있을때는",
          "original_form": "그니까요... 그리고 뒤에서 그렇게 호박씨 까놓구선 상미랑 방에있을때는",
          "speaker_id": "1",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100025858.1.13",
          "form": "갸륵하게 걱정하는척... 갸륵....갸륵...",
          "original_form": "갸륵하게 걱정하는척... 갸륵....갸륵...",
          "speaker_id": "1",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100025858.1.14",
          "form": "그니까 ㅋㅋㅋㅋ아방송보면 어떨까 정말",
          "original_form": "그니까 ㅋㅋㅋㅋ아방송보면 어떨까 정말",
          "speaker_id": "2",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025858.1.15",
          "form": "그니까요 같이 본방챙겨봐야될텐데.... ^^",
          "original_form": "그니까요 같이 본방챙겨봐야될텐데.... ^^",
          "speaker_id": "1",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025858.1.16",
          "form": "아침에 부엌에서 나가서 얘기하자 하는것도 ㅋㅋㅋㅋ저진짜 피가 거꾸로 솟앗어요......",
          "original_form": "아침에 부엌에서 나가서 얘기하자 하는것도 ㅋㅋㅋㅋ저진짜 피가 거꾸로 솟앗어요......",
          "speaker_id": "1",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100025858.1.17",
          "form": "ㅋㅋ ㄹㅇ 지짜 사귀나??",
          "original_form": "ㅋㅋ ㄹㅇ 지짜 사귀나??",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100025858.1.18",
          "form": "진짜 돌은 것 같았어요...",
          "original_form": "진짜 돌은 것 같았어요...",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025858.1.19",
          "form": "그리고 그때 부엌에 진록이랑 민선있었자나요",
          "original_form": "그리고 그때 부엌에 진록이랑 민선있었자나요",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025858.1.20",
          "form": "그러면 딱보고 눈치로 지는 거기 가지말았어야지",
          "original_form": "그러면 딱보고 눈치로 지는 거기 가지말았어야지",
          "speaker_id": "1",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100025858.1.21",
          "form": "거길 꼭 들어가서....",
          "original_form": "거길 꼭 들어가서....",
          "speaker_id": "1",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100025858.1.22",
          "form": "ㅋㅋㅋㅋ 그니까요 뇌로 피가 너무 몰리더라구요",
          "original_form": "ㅋㅋㅋㅋ 그니까요 뇌로 피가 너무 몰리더라구요",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100025858.1.23",
          "form": "그리구 둘이 나와서 차타자마자",
          "original_form": "그리구 둘이 나와서 차타자마자",
          "speaker_id": "1",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100025858.1.24",
          "form": "웃을때..................",
          "original_form": "웃을때..................",
          "speaker_id": "1",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100025858.1.25",
          "form": "ㅋㅋㅋㅋㅋ 비밀연애하는줄~~",
          "original_form": "ㅋㅋㅋㅋㅋ 비밀연애하는줄~~",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025858.1.26",
          "form": "근데 다시킨거면 진짜 넘 메소드연기아님까",
          "original_form": "근데 다시킨거면 진짜 넘 메소드연기아님까",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025858.1.27",
          "form": "그니까요... 그들이 그렇게 연기를 그렇게나 잘하면...",
          "original_form": "그니까요... 그들이 그렇게 연기를 그렇게나 잘하면...",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100025858.1.28",
          "form": "지금 연기하는 20대 배우들보다 연기.잘하는듯...",
          "original_form": "지금 연기하는 20대 배우들보다 연기.잘하는듯...",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100025858.1.29",
          "form": "ㅋㅋㅋㄹㅇ이욬ㅋㅋㅋㅋ 제작진하고싶다진짜 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㄹㅇ이욬ㅋㅋㅋㅋ 제작진하고싶다진짜 ㅋㅋㅋ",
