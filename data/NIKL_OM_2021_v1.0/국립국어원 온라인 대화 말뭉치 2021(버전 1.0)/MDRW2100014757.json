      "utterance": [
        {
          "id": "MDRW2100014757.1.1",
          "form": "안녕?",
          "original_form": "안녕?",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014757.1.2",
          "form": "뮤지컬 보고싶어",
          "original_form": "뮤지컬 보고싶어",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014757.1.3",
          "form": "어떤거",
          "original_form": "어떤거",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014757.1.4",
          "form": "말을해보라우",
          "original_form": "말을해보라우",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014757.1.5",
          "form": "명성황후",
          "original_form": "명성황후",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014757.1.6",
          "form": "로얄석에서",
          "original_form": "로얄석에서",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014757.1.7",
          "form": "스트레스 확",
          "original_form": "스트레스 확",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014757.1.8",
          "form": "김준수 나오는 엑스칼리버 보고싶음 나는",
          "original_form": "김준수 나오는 엑스칼리버 보고싶음 나는",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014757.1.9",
          "form": "보러가",
          "original_form": "보러가",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014757.1.10",
          "form": "엄마는 왤케 부정적이야",
          "original_form": "엄마는 왤케 부정적이야",
          "speaker_id": "1",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014757.1.11",
          "form": "친절하게 말해줘",
          "original_form": "친절하게 말해줘",
          "speaker_id": "1",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014757.1.12",
          "form": "뭐가 부정적이냐?",
          "original_form": "뭐가 부정적이냐?",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014757.1.13",
          "form": "친절하게 말해주라고",
          "original_form": "친절하게 말해주라고",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014757.1.14",
          "form": "준수 노래 잘하더라",
          "original_form": "준수 노래 잘하더라",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014757.1.15",
          "form": "ㅇㅇ 티켓팅 성공해서 보러가야지",
          "original_form": "ㅇㅇ 티켓팅 성공해서 보러가야지",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014757.1.16",
          "form": "나도 가보고 싶어 내것도 부탁해",
          "original_form": "나도 가보고 싶어 내것도 부탁해",
          "speaker_id": "2",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014757.1.17",
          "form": "ㅇㅋ 기대해",
          "original_form": "ㅇㅋ 기대해",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014757.1.18",
          "form": "로얄석에서 보고싶다",
          "original_form": "로얄석에서 보고싶다",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
