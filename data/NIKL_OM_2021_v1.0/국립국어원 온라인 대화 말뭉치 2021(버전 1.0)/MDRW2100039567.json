      "utterance": [
        {
          "id": "MDRW2100039567.1.1",
          "form": "놀러가고싶다",
          "original_form": "놀러가고싶다",
          "speaker_id": "1",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039567.1.2",
          "form": "원래도 가고 싶었지만 더 격하게...!",
          "original_form": "원래도 가고 싶었지만 더 격하게...!",
          "speaker_id": "1",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039567.1.3",
          "form": "나 부산 가고시퍼",
          "original_form": "나 부산 가고시퍼",
          "speaker_id": "2",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039567.1.4",
          "form": "부산 계속 계획만 세우고 못 갔네....",
          "original_form": "부산 계속 계획만 세우고 못 갔네....",
          "speaker_id": "1",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039567.1.5",
          "form": "괴롭...",
          "original_form": "괴롭...",
          "speaker_id": "2",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039567.1.6",
          "form": "코로나 진정...진정되면...ㅠㅠ",
          "original_form": "코로나 진정...진정되면...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039567.1.7",
          "form": "케텍스도 내일로 타고 가면 되는데...ㅠㅠ",
          "original_form": "케텍스도 내일로 타고 가면 되는데...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 19:13"
        },
        {
          "id": "MDRW2100039567.1.8",
          "form": "다들 백신 맞으면 가자",
          "original_form": "다들 백신 맞으면 가자",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039567.1.9",
          "form": "백신 맞고 2주 후 던가?",
          "original_form": "백신 맞고 2주 후 던가?",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039567.1.10",
          "form": "ㅇㅇ그렇대",
          "original_form": "ㅇㅇ그렇대",
          "speaker_id": "2",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039567.1.11",
          "form": "그러면 언제 쯤인가",
          "original_form": "그러면 언제 쯤인가",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
