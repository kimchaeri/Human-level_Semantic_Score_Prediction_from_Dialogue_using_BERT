      "utterance": [
        {
          "id": "MDRW2100022362.1.1",
          "form": "볶아서 먹으면 더 맛있을거 같기도하고",
          "original_form": "볶아서 먹으면 더 맛있을거 같기도하고",
          "speaker_id": "2",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022362.1.2",
          "form": "볶음밥 해드셔야죠",
          "original_form": "볶음밥 해드셔야죠",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022362.1.3",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022362.1.4",
          "form": "ㅋㅋㅋ 캬 맛잘알이시네요",
          "original_form": "ㅋㅋㅋ 캬 맛잘알이시네요",
          "speaker_id": "2",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022362.1.5",
          "form": "양념엔 볶음밥이에요",
          "original_form": "양념엔 볶음밥이에요",
          "speaker_id": "1",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022362.1.6",
          "form": "그럼 저 잠깐 참치뽈찜 넣고올게요",
          "original_form": "그럼 저 잠깐 참치뽈찜 넣고올게요",
          "speaker_id": "2",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022362.1.7",
          "form": "그러세요",
          "original_form": "그러세요",
          "speaker_id": "1",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022362.1.8",
          "form": "오 조리법 인터넷에 찾아보니까",
          "original_form": "오 조리법 인터넷에 찾아보니까",
          "speaker_id": "2",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022362.1.9",
          "form": "끓는 물에 10분 중탕이래요",
          "original_form": "끓는 물에 10분 중탕이래요",
          "speaker_id": "2",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022362.1.10",
          "form": "중탕이면",
          "original_form": "중탕이면",
          "speaker_id": "1",
          "time": "20210829 13:06"
        },
        {
          "id": "MDRW2100022362.1.11",
          "form": "그냥 팩넣고 끓이시면 될거같아요",
          "original_form": "그냥 팩넣고 끓이시면 될거같아요",
          "speaker_id": "1",
          "time": "20210829 13:06"
        },
        {
          "id": "MDRW2100022362.1.12",
          "form": "그래서 팩 통째로 넣고왔어요 헷",
          "original_form": "그래서 팩 통째로 넣고왔어요 헷",
          "speaker_id": "2",
          "time": "20210829 13:06"
        },
        {
          "id": "MDRW2100022362.1.13",
          "form": "근데 아직 해동이 덜된건지 원래 참치뽈찜이 단단한건지 모르겠네요",
          "original_form": "근데 아직 해동이 덜된건지 원래 참치뽈찜이 단단한건지 모르겠네요",
          "speaker_id": "2",
          "time": "20210829 13:07"
        },
        {
          "id": "MDRW2100022362.1.14",
          "form": "약간 냉동상태라",
          "original_form": "약간 냉동상태라",
          "speaker_id": "1",
          "time": "20210829 13:07"
        },
        {
          "id": "MDRW2100022362.1.15",
          "form": "단단한게 있을거같아요",
          "original_form": "단단한게 있을거같아요",
          "speaker_id": "1",
          "time": "20210829 13:07"
        },
        {
          "id": "MDRW2100022362.1.16",
          "form": "이거 어제 배송 왔는데 살짝 녹아서 와서",
          "original_form": "이거 어제 배송 왔는데 살짝 녹아서 와서",
          "speaker_id": "2",
          "time": "20210829 13:07"
        },
        {
          "id": "MDRW2100022362.1.17",
          "form": "그래서 그냥 냉장실에 넣어놨어요",
          "original_form": "그래서 그냥 냉장실에 넣어놨어요",
          "speaker_id": "2",
          "time": "20210829 13:07"
        },
        {
          "id": "MDRW2100022362.1.18",
          "form": "음 그럼 뽈찜이 단단한가보다",
          "original_form": "음 그럼 뽈찜이 단단한가보다",
          "speaker_id": "1",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100022362.1.19",
          "form": "근데 왜 뽈찜 고르셨어요",
          "original_form": "근데 왜 뽈찜 고르셨어요",
          "speaker_id": "1",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100022362.1.20",
          "form": "수원왕갈비맛이라니 좀 기대되네요",
          "original_form": "수원왕갈비맛이라니 좀 기대되네요",
          "speaker_id": "2",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100022362.1.21",
          "form": "어디서 맛있다고 소문이났나",
          "original_form": "어디서 맛있다고 소문이났나",
          "speaker_id": "1",
          "time": "20210829 13:08"
        }
      ]
