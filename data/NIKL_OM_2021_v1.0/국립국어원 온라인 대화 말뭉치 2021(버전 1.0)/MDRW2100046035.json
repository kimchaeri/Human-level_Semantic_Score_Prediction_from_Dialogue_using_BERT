      "utterance": [
        {
          "id": "MDRW2100046035.1.1",
          "form": "영어 수업은",
          "original_form": "영어 수업은",
          "speaker_id": "2",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100046035.1.2",
          "form": "언제예욬ㅋㅋㅋㅋ",
          "original_form": "언제예욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100046035.1.3",
          "form": "내일요....",
          "original_form": "내일요....",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100046035.1.4",
          "form": "그나마다행인건",
          "original_form": "그나마다행인건",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100046035.1.5",
          "form": "1교시였는데",
          "original_form": "1교시였는데",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100046035.1.6",
          "form": "3교시로 바꿔서",
          "original_form": "3교시로 바꿔서",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100046035.1.7",
          "form": "마음의 준비를 할수있어요",
          "original_form": "마음의 준비를 할수있어요",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100046035.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ그냥",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그냥",
          "speaker_id": "2",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100046035.1.9",
          "form": "늦잠자는거 아닌가요",
          "original_form": "늦잠자는거 아닌가요",
          "speaker_id": "2",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100046035.1.10",
          "form": "그게그거죠뭨ㅋㅋ",
          "original_form": "그게그거죠뭨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046035.1.11",
          "form": "수업너무 지루해요",
          "original_form": "수업너무 지루해요",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046035.1.12",
          "form": "지금 학교세요?",
          "original_form": "지금 학교세요?",
          "speaker_id": "2",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046035.1.13",
          "form": "ㄴㄴ집에서",
          "original_form": "ㄴㄴ집에서",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046035.1.14",
          "form": "화상수업중이에요",
          "original_form": "화상수업중이에요",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046035.1.15",
          "form": "카메라 켜져있는데",
          "original_form": "카메라 켜져있는데",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046035.1.16",
          "form": "수업안듣고 딴짓해도 돼요?",
          "original_form": "수업안듣고 딴짓해도 돼요?",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046035.1.17",
          "form": "ㅋㅋㅋ어차피",
          "original_form": "ㅋㅋㅋ어차피",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046035.1.18",
          "form": "얼굴만보여서요",
          "original_form": "얼굴만보여서요",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046035.1.19",
          "form": "그리고 지금 다른학생한테",
          "original_form": "그리고 지금 다른학생한테",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046035.1.20",
          "form": "질문중이라",
          "original_form": "질문중이라",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046035.1.21",
          "form": "수업 몇시간짜리에요?",
          "original_form": "수업 몇시간짜리에요?",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046035.1.22",
          "form": "오늘이 가장적어요",
          "original_form": "오늘이 가장적어요",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046035.1.23",
          "form": "지금은 2시간짜리",
          "original_form": "지금은 2시간짜리",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046035.1.24",
          "form": "오ㅋㅋㅋ그리고 또있어요?",
          "original_form": "오ㅋㅋㅋ그리고 또있어요?",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046035.1.25",
          "form": "오후두신가",
          "original_form": "오후두신가",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046035.1.26",
          "form": "한시간짜리 있는데",
          "original_form": "한시간짜리 있는데",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046035.1.27",
          "form": "안할수도있어요,ㅋ",
          "original_form": "안할수도있어요,ㅋ",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046035.1.28",
          "form": "오 하루에 3시간수업",
          "original_form": "오 하루에 3시간수업",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046035.1.29",
          "form": "나때는..상상도못할..",
          "original_form": "나때는..상상도못할..",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046035.1.30",
          "form": "근데 두시수업이면",
          "original_form": "근데 두시수업이면",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046035.1.31",
          "form": "좀 애매하다",
          "original_form": "좀 애매하다",
          "speaker_id": "2",
          "time": "20210908 09:52"
        }
      ]
    }
  ]
