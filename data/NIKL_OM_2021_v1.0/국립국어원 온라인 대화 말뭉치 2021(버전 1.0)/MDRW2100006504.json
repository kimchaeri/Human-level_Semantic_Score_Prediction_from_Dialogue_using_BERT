      "utterance": [
        {
          "id": "MDRW2100006504.1.1",
          "form": "초밥먹고싶다..",
          "original_form": "초밥먹고싶다..",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100006504.1.2",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100006504.1.3",
          "form": "집 가까이 있는 초밥집 망해서 아쉽..",
          "original_form": "집 가까이 있는 초밥집 망해서 아쉽..",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100006504.1.4",
          "form": "ㅋㅋ거긴 망할만했다",
          "original_form": "ㅋㅋ거긴 망할만했다",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100006504.1.5",
          "form": "왜? 먹어봤었나?",
          "original_form": "왜? 먹어봤었나?",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100006504.1.6",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100006504.1.7",
          "form": "아 손님이 없어서?ㅠㅠ",
          "original_form": "아 손님이 없어서?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100006504.1.8",
          "form": "그래 넘 없더라",
          "original_form": "그래 넘 없더라",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100006504.1.9",
          "form": "웅웅..근처에 뭐가 넘 없어서 그런듯",
          "original_form": "웅웅..근처에 뭐가 넘 없어서 그런듯",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100006504.1.10",
          "form": "먹고싶으면 바닷가가야지",
          "original_form": "먹고싶으면 바닷가가야지",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100006504.1.11",
          "form": "ㅋㅋ넘 멀다!",
          "original_form": "ㅋㅋ넘 멀다!",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100006504.1.12",
          "form": "아니면 좀 비싼거",
          "original_form": "아니면 좀 비싼거",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100006504.1.13",
