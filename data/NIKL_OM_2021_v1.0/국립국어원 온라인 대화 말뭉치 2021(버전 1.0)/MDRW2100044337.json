      "utterance": [
        {
          "id": "MDRW2100044337.1.1",
          "form": "중고거래하시나욤",
          "original_form": "중고거래하시나욤",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044337.1.2",
          "form": "유튜브 알고리즘은 대단한것같습니다 ㅋㅋ",
          "original_form": "유튜브 알고리즘은 대단한것같습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044337.1.3",
          "form": "네! 저 가끔 당근합니다",
          "original_form": "네! 저 가끔 당근합니다",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044337.1.4",
          "form": "주로 사기만 하지만요ㅎㅎ",
          "original_form": "주로 사기만 하지만요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044337.1.5",
          "form": "중고거래 하시나요?",
          "original_form": "중고거래 하시나요?",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044337.1.6",
          "form": "유튜브 왜보나 했는데 빠지니 헤어나올수 없네요ㅜ",
          "original_form": "유튜브 왜보나 했는데 빠지니 헤어나올수 없네요ㅜ",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044337.1.7",
          "form": "저도 당근 종종 애용해요",
          "original_form": "저도 당근 종종 애용해요",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044337.1.8",
          "form": "ㅋㅋ그쵸 혹시 프리미엄하셨나요?",
          "original_form": "ㅋㅋ그쵸 혹시 프리미엄하셨나요?",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044337.1.9",
          "form": "아뇽ㅎㅎ",
          "original_form": "아뇽ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044337.1.10",
          "form": "ㅎㅎㅎ아 안하셨으면 비추천드립니다..ㅎ",
          "original_form": "ㅎㅎㅎ아 안하셨으면 비추천드립니다..ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044337.1.11",
          "form": "중고나라 가끔 하다가 당근마켓하니 더 좋더라구요",
          "original_form": "중고나라 가끔 하다가 당근마켓하니 더 좋더라구요",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044337.1.12",
          "form": "전 1개월무료로 했다가 코가 꿰였어요...",
          "original_form": "전 1개월무료로 했다가 코가 꿰였어요...",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044337.1.13",
          "form": "오우..",
          "original_form": "오우..",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044337.1.14",
          "form": "구매 유혹을 참고 있어요ㅜㅜ",
          "original_form": "구매 유혹을 참고 있어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044337.1.15",
          "form": "ㅋㅋ당근에서 어떤거 주로 사거나 파시나요?",
          "original_form": "ㅋㅋ당근에서 어떤거 주로 사거나 파시나요?",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044337.1.16",
          "form": "아 ㅜㅜ맞아요...이게 구독이니까",
          "original_form": "아 ㅜㅜ맞아요...이게 구독이니까",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044337.1.17",
          "form": "아이용품 같은거 팔거나 사거나 해요",
          "original_form": "아이용품 같은거 팔거나 사거나 해요",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044337.1.18",
          "form": "책도 팔고",
          "original_form": "책도 팔고",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044337.1.19",
          "form": "발담그는 순간..ㅋㅋㅋ 죽을때까지 낼지도..",
          "original_form": "발담그는 순간..ㅋㅋㅋ 죽을때까지 낼지도..",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044337.1.20",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044337.1.21",
          "form": "살때도 있구요",
          "original_form": "살때도 있구요",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044337.1.22",
          "form": "그러시군요! 책 좋아하세요?:)",
          "original_form": "그러시군요! 책 좋아하세요?:)",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044337.1.23",
          "form": "그래서 참고 광고 봐요",
          "original_form": "그래서 참고 광고 봐요",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044337.1.24",
          "form": "읽으려고 노력하는편이요ㅠ",
          "original_form": "읽으려고 노력하는편이요ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044337.1.25",
          "form": "ㅎㅎ현명하십니다",
          "original_form": "ㅎㅎ현명하십니다",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044337.1.26",
          "form": "아 ㅎㅎ 저도 책 보려고 노력하는편인데",
          "original_form": "아 ㅎㅎ 저도 책 보려고 노력하는편인데",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044337.1.27",
          "form": "쉽지않죠 ㅜㅜ",
          "original_form": "쉽지않죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044337.1.28",
          "form": "주로 어떤 장르를 구매하시나요(?)",
          "original_form": "주로 어떤 장르를 구매하시나요(?)",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044337.1.29",
          "form": "잘 읽히는 소설 주로 사요ㅋ",
          "original_form": "잘 읽히는 소설 주로 사요ㅋ",
          "speaker_id": "1",
          "time": "20210907 22:29"
        }
      ]
    }
