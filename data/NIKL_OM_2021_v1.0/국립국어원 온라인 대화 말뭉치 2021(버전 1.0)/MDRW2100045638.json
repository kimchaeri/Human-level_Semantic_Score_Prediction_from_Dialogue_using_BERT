      "utterance": [
        {
          "id": "MDRW2100045638.1.1",
          "form": "여행 좋아하세오?",
          "original_form": "여행 좋아하세오?",
          "speaker_id": "1",
          "time": "20210908 04:35"
        },
        {
          "id": "MDRW2100045638.1.2",
          "form": "잘 안가기는 하는데 막상가면 엄청 좋아해요!",
          "original_form": "잘 안가기는 하는데 막상가면 엄청 좋아해요!",
          "speaker_id": "2",
          "time": "20210908 04:35"
        },
        {
          "id": "MDRW2100045638.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ어디가 좋으셧어요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ어디가 좋으셧어요?",
          "speaker_id": "1",
          "time": "20210908 04:35"
        },
        {
          "id": "MDRW2100045638.1.4",
          "form": "어렸을때 부모님이랑 갔던 강원도가 기억에 남아요!",
          "original_form": "어렸을때 부모님이랑 갔던 강원도가 기억에 남아요!",
          "speaker_id": "2",
          "time": "20210908 04:36"
        },
        {
          "id": "MDRW2100045638.1.5",
          "form": "강원도 어디가셧어요?",
          "original_form": "강원도 어디가셧어요?",
          "speaker_id": "1",
          "time": "20210908 04:37"
        },
        {
          "id": "MDRW2100045638.1.6",
          "form": "정동진? 그쪽 갔었는데",
          "original_form": "정동진? 그쪽 갔었는데",
          "speaker_id": "2",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045638.1.7",
          "form": "엄마 지인분이 열차일을 하셔서 기관실에 들어가 본적이 있어요!",
          "original_form": "엄마 지인분이 열차일을 하셔서 기관실에 들어가 본적이 있어요!",
          "speaker_id": "2",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045638.1.8",
          "form": "오 정동진 일출예쁘죠",
          "original_form": "오 정동진 일출예쁘죠",
          "speaker_id": "1",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045638.1.9",
          "form": "일출 궁금하네요 가봤어도 자느라 본적이 없고든요ㅠㅠ",
          "original_form": "일출 궁금하네요 가봤어도 자느라 본적이 없고든요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045638.1.10",
          "form": "일출예뻐요 추천해요",
          "original_form": "일출예뻐요 추천해요",
          "speaker_id": "1",
          "time": "20210908 04:42"
        },
        {
          "id": "MDRW2100045638.1.11",
          "form": "올해는 좀 잠잠해져서 일출 보러갈 수 있으면 좋겠네요ㅠㅠㅠㅠ",
          "original_form": "올해는 좀 잠잠해져서 일출 보러갈 수 있으면 좋겠네요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:43"
        },
        {
          "id": "MDRW2100045638.1.12",
          "form": "맞아요 코로나가정말 ㅠㅠ",
          "original_form": "맞아요 코로나가정말 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 04:43"
        },
        {
          "id": "MDRW2100045638.1.13",
          "form": "피곤하긴해도 보면 너무 좋을 것 같아요ㅠㅠ",
          "original_form": "피곤하긴해도 보면 너무 좋을 것 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:43"
        },
        {
          "id": "MDRW2100045638.1.14",
          "form": "코로나때문에 못하는게 너무 많아요 ㅠㅠ",
          "original_form": "코로나때문에 못하는게 너무 많아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 04:44"
        },
        {
          "id": "MDRW2100045638.1.15",
          "form": "그러니까요ㅠㅠ 작년에 여행 계획 잡았던것도 코로나로 다 취소하고",
          "original_form": "그러니까요ㅠㅠ 작년에 여행 계획 잡았던것도 코로나로 다 취소하고",
          "speaker_id": "2",
          "time": "20210908 04:44"
        },
        {
          "id": "MDRW2100045638.1.16",
          "form": "작년부터 어딜 가지도 못해서 답답하네요ㅠㅠ",
          "original_form": "작년부터 어딜 가지도 못해서 답답하네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:45"
        }
      ]
