      "utterance": [
        {
          "id": "MDRW2100001079.1.1",
          "form": "요즘 학교생활 어때요?",
          "original_form": "요즘 학교생활 어때요?",
          "speaker_id": "1",
          "time": "20210723 10:39"
        },
        {
          "id": "MDRW2100001079.1.2",
          "form": "지금은 방학인데 ㅋㅋ",
          "original_form": "지금은 방학인데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 10:40"
        },
        {
          "id": "MDRW2100001079.1.3",
          "form": "코로나로 계속 화상수업해요",
          "original_form": "코로나로 계속 화상수업해요",
          "speaker_id": "2",
          "time": "20210723 10:40"
        },
        {
          "id": "MDRW2100001079.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210723 10:41"
        },
        {
          "id": "MDRW2100001079.1.5",
          "form": "아하 수도권이시구나",
          "original_form": "아하 수도권이시구나",
          "speaker_id": "1",
          "time": "20210723 10:41"
        },
        {
          "id": "MDRW2100001079.1.6",
          "form": "저도 최근에 방학했어요!",
          "original_form": "저도 최근에 방학했어요!",
          "speaker_id": "1",
          "time": "20210723 10:41"
        },
        {
          "id": "MDRW2100001079.1.7",
          "form": "아 지방은 계속 등교한다고 들었어요 ㅋㅋ",
          "original_form": "아 지방은 계속 등교한다고 들었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 10:42"
        },
        {
          "id": "MDRW2100001079.1.8",
          "form": "저도 학교가고 싶은데 ㅠ",
          "original_form": "저도 학교가고 싶은데 ㅠ",
          "speaker_id": "2",
          "time": "20210723 10:42"
        },
        {
          "id": "MDRW2100001079.1.9",
          "form": "수도권은 아무래도 더 심각해서요",
          "original_form": "수도권은 아무래도 더 심각해서요",
          "speaker_id": "2",
          "time": "20210723 10:42"
        },
        {
          "id": "MDRW2100001079.1.10",
          "form": "그쵸... 진짜 너무 심각해요",
          "original_form": "그쵸... 진짜 너무 심각해요",
          "speaker_id": "1",
          "time": "20210723 10:43"
        },
        {
          "id": "MDRW2100001079.1.11",
          "form": "계속 화상수업니까 친구들 만나고싶으시겠다..ㅠ",
          "original_form": "계속 화상수업니까 친구들 만나고싶으시겠다..ㅠ",
          "speaker_id": "1",
          "time": "20210723 10:44"
        },
        {
          "id": "MDRW2100001079.1.12",
          "form": "작년 친구들은 수업 끝나고 보긴하는데요 ㅎㅎ",
          "original_form": "작년 친구들은 수업 끝나고 보긴하는데요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210723 10:45"
        },
        {
          "id": "MDRW2100001079.1.13",
          "form": "올해 같은 반 애들은 올초에 잠깐 등교했을때 빼놓고는 본 적이 없네요 ㅋㅋ",
          "original_form": "올해 같은 반 애들은 올초에 잠깐 등교했을때 빼놓고는 본 적이 없네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 10:45"
        },
        {
          "id": "MDRW2100001079.1.14",
          "form": "아이고ㅠㅠㅠ 다시 정상등교하면 많이 어색하겠네요ㅠ",
          "original_form": "아이고ㅠㅠㅠ 다시 정상등교하면 많이 어색하겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210723 10:46"
        },
        {
          "id": "MDRW2100001079.1.15",
          "form": "ㅋㅋㅋㅋ넵",
          "original_form": "ㅋㅋㅋㅋ넵",
          "speaker_id": "2",
          "time": "20210723 10:49"
        },
        {
          "id": "MDRW2100001079.1.16",
          "form": "이러다가 졸업할수도요 ㅠ",
          "original_form": "이러다가 졸업할수도요 ㅠ",
          "speaker_id": "2",
          "time": "20210723 10:49"
        },
        {
          "id": "MDRW2100001079.1.17",
          "form": "아이고ㅋㅋ ㅠㅠㅠ",
          "original_form": "아이고ㅋㅋ ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210723 10:49"
        },
        {
          "id": "MDRW2100001079.1.18",
          "form": "얼른 코로나가 끝나야할텐데",
          "original_form": "얼른 코로나가 끝나야할텐데",
          "speaker_id": "1",
          "time": "20210723 10:50"
        },
        {
          "id": "MDRW2100001079.1.19",
          "form": "4단계라서 답답한게 한두가지가 아닐텐데 너무 고생이네요",
          "original_form": "4단계라서 답답한게 한두가지가 아닐텐데 너무 고생이네요",
          "speaker_id": "1",
          "time": "20210723 10:50"
        },
        {
          "id": "MDRW2100001079.1.20",
          "form": "근데 4단계나 전단계나 똑같은것같아요 ㅋㅋ",
          "original_form": "근데 4단계나 전단계나 똑같은것같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 10:51"
        },
        {
          "id": "MDRW2100001079.1.21",
          "form": "아ㅋㅋㅋㅋ 그래요?",
          "original_form": "아ㅋㅋㅋㅋ 그래요?",
          "speaker_id": "1",
          "time": "20210723 10:51"
        },
        {
          "id": "MDRW2100001079.1.22",
          "form": "넵넵 별로 달라진걸 못 느끼겟네요ㅎㅎ",
          "original_form": "넵넵 별로 달라진걸 못 느끼겟네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210723 10:51"
        },
        {
          "id": "MDRW2100001079.1.23",
          "form": "name1님은 계속 등교하셨던거에요?",
          "original_form": "&name1&님은 계속 등교하셨던거에요?",
