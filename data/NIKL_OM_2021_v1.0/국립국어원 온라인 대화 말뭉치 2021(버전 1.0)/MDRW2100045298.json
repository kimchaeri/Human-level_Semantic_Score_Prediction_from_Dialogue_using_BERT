      "utterance": [
        {
          "id": "MDRW2100045298.1.1",
          "form": "맞아.... 저도 그 생각 했다네요",
          "original_form": "맞아.... 저도 그 생각 했다네요",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045298.1.2",
          "form": "또 날애님이 한창 좋아하던거 있는데",
          "original_form": "또 날애님이 한창 좋아하던거 있는데",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045298.1.3",
          "form": "시오슈가였나",
          "original_form": "시오슈가였나",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045298.1.4",
          "form": "혹시 .. 아 맞아 그거그거",
          "original_form": "혹시 .. 아 맞아 그거그거",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045298.1.5",
          "form": "이름이 기억이 안나",
          "original_form": "이름이 기억이 안나",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045298.1.6",
          "form": "날애님이 맛있게 먹길래 나도 결국 한입했잖니",
          "original_form": "날애님이 맛있게 먹길래 나도 결국 한입했잖니",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045298.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045298.1.8",
          "form": "나도...시오슈가<이것밖에 기억안나",
          "original_form": "나도...시오슈가<이것밖에 기억안나",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045298.1.9",
          "form": "내용 돌아방스던데",
          "original_form": "내용 돌아방스던데",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045298.1.10",
          "form": "근데 진짜............뭐지............싶긴 내용이긴 하더라 그치...",
          "original_form": "근데 진짜............뭐지............싶긴 내용이긴 하더라 그치...",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045298.1.11",
          "form": "이렇게 막장일 수가...?",
          "original_form": "이렇게 막장일 수가...?",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045298.1.12",
          "form": "시오를 왜 그렇게 그렷는지도 좀 이해 안감",
          "original_form": "시오를 왜 그렇게 그렷는지도 좀 이해 안감",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045298.1.13",
          "form": "진짜로.............? 막장이 아닌 인물이 없음",
          "original_form": "진짜로.............? 막장이 아닌 인물이 없음",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045298.1.14",
          "form": "애가 무슨 유딩인데",
          "original_form": "애가 무슨 유딩인데",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045298.1.15",
          "form": "어른들이 애한테 안식을 찾고",
          "original_form": "어른들이 애한테 안식을 찾고",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045298.1.16",
          "form": "그니까.... 나 나중에 웬 고딩남자애가",
          "original_form": "그니까.... 나 나중에 웬 고딩남자애가",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045298.1.17",
          "form": "그냥 다 돌아버린 무언가 됐음",
          "original_form": "그냥 다 돌아버린 무언가 됐음",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045298.1.18",
          "form": "집 나온 시오한테 반하는거보고",
          "original_form": "집 나온 시오한테 반하는거보고",
