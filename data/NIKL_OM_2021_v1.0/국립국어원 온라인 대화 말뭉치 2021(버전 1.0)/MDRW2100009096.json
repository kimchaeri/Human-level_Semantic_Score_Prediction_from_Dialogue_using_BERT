      "utterance": [
        {
          "id": "MDRW2100009096.1.1",
          "form": "코로나가 가게를 바꿨어요",
          "original_form": "코로나가 가게를 바꿨어요",
          "speaker_id": "2",
          "time": "20210822 21:09"
        },
        {
          "id": "MDRW2100009096.1.2",
          "form": "예전에 친구가 자기가 맛있게 해주겠다고해서",
          "original_form": "예전에 친구가 자기가 맛있게 해주겠다고해서",
          "speaker_id": "1",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009096.1.3",
          "form": "셀프로 떡볶이 가져가게 하는데",
          "original_form": "셀프로 떡볶이 가져가게 하는데",
          "speaker_id": "2",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009096.1.4",
          "form": "두끼갔었는데 맛이 그냥저냥 그랬던기억이 있어요 ㅎ",
          "original_form": "두끼갔었는데 맛이 그냥저냥 그랬던기억이 있어요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009096.1.5",
          "form": "누가 많이 담나 유튜브 나오고 했어요",
          "original_form": "누가 많이 담나 유튜브 나오고 했어요",
          "speaker_id": "2",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009096.1.6",
          "form": "맞아요 ㅜㅡ",
          "original_form": "맞아요 ㅜㅡ",
          "speaker_id": "2",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009096.1.7",
          "form": "헉 진짜요?",
          "original_form": "헉 진짜요?",
          "speaker_id": "1",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009096.1.8",
          "form": "맛은 별로인데",
          "original_form": "맛은 별로인데",
          "speaker_id": "2",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009096.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009096.1.10",
          "form": "잼나요",
          "original_form": "잼나요",
          "speaker_id": "2",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009096.1.11",
          "form": "찾아봐야겧어요",
          "original_form": "찾아봐야겧어요",
          "speaker_id": "1",
          "time": "20210822 21:11"
        },
        {
          "id": "MDRW2100009096.1.12",
          "form": "애랑 둘이서 막 어찌 많이 담나 연구 했었어요 ㅋㅋ",
          "original_form": "애랑 둘이서 막 어찌 많이 담나 연구 했었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:11"
        },
        {
          "id": "MDRW2100009096.1.13",
          "form": "납작당면을 옆으로 누워 담으면 많이 들어가요 ㅋㅋ",
          "original_form": "납작당면을 옆으로 누워 담으면 많이 들어가요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:11"
        },
        {
          "id": "MDRW2100009096.1.14",
          "form": "두끼는 망하는거아니예요?ㅎ",
          "original_form": "두끼는 망하는거아니예요?ㅎ",
          "speaker_id": "1",
          "time": "20210822 21:11"
        },
        {
          "id": "MDRW2100009096.1.15",
          "form": "튀김은 눅눅해지 마지막에 꾹꾹 눌러담고요",
          "original_form": "튀김은 눅눅해지 마지막에 꾹꾹 눌러담고요",
          "speaker_id": "2",
          "time": "20210822 21:12"
        },
        {
          "id": "MDRW2100009096.1.16",
          "form": "그러게요 ㅡㅡ",
          "original_form": "그러게요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 21:12"
        },
        {
          "id": "MDRW2100009096.1.17",
          "form": "못담는 사람도 있지 않을까요",
          "original_form": "못담는 사람도 있지 않을까요",
          "speaker_id": "2",
          "time": "20210822 21:12"
        },
        {
          "id": "MDRW2100009096.1.18",
          "form": "어차피 부페라",
          "original_form": "어차피 부페라",
          "speaker_id": "2",
          "time": "20210822 21:12"
        },
        {
          "id": "MDRW2100009096.1.19",
          "form": "맘대로 배터지게 먹는거지 같은이치죠 뭐 이러나 저러나",
          "original_form": "맘대로 배터지게 먹는거지 같은이치죠 뭐 이러나 저러나",
          "speaker_id": "2",
          "time": "20210822 21:12"
        },
        {
          "id": "MDRW2100009096.1.20",
          "form": "튀김은 마지막이 중요하겠네요",
          "original_form": "튀김은 마지막이 중요하겠네요",
          "speaker_id": "1",
          "time": "20210822 21:13"
        },
        {
          "id": "MDRW2100009096.1.21",
          "form": "저희집 이틀 먹었네요 ㅋㅋ",
          "original_form": "저희집 이틀 먹었네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:13"
        },
        {
          "id": "MDRW2100009096.1.22",
          "form": "눅눅해지지않으려면요",
          "original_form": "눅눅해지지않으려면요",
          "speaker_id": "1",
          "time": "20210822 21:13"
        },
        {
          "id": "MDRW2100009096.1.23",
          "form": "우와 ㅋㅋㅋㅋ",
          "original_form": "우와 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:13"
        },
        {
          "id": "MDRW2100009096.1.24",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:13"
        },
        {
          "id": "MDRW2100009096.1.25",
          "form": "오뎅도 사이에 넣고",
          "original_form": "오뎅도 사이에 넣고",
          "speaker_id": "2",
          "time": "20210822 21:13"
        },
        {
          "id": "MDRW2100009096.1.26",
          "form": "집에 있는건 패스해야 많이 들어가요",
          "original_form": "집에 있는건 패스해야 많이 들어가요",
          "speaker_id": "2",
          "time": "20210822 21:13"
        },
        {
          "id": "MDRW2100009096.1.27",
          "form": "저렴한건 빼고 ㅎㅎ",
          "original_form": "저렴한건 빼고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:14"
        },
        {
          "id": "MDRW2100009096.1.28",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 21:14"
        },
        {
          "id": "MDRW2100009096.1.29",
          "form": "정말 고도의 기술이 필요해요 ㅋㅋ",
          "original_form": "정말 고도의 기술이 필요해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:14"
        },
        {
          "id": "MDRW2100009096.1.30",
          "form": "나름 재미 맛집이죠 ㅎㅎ",
          "original_form": "나름 재미 맛집이죠 ㅎㅎ",
          "speaker_id": "2",
