      "utterance": [
        {
          "id": "MDRW2100020543.1.1",
          "form": "저는 해물 안먹는걸로 넘 시달려서 그런지",
          "original_form": "저는 해물 안먹는걸로 넘 시달려서 그런지",
          "speaker_id": "2",
          "time": "20210828 09:32"
        },
        {
          "id": "MDRW2100020543.1.2",
          "form": "알러지나 채식주의보고 뭐라하는거",
          "original_form": "알러지나 채식주의보고 뭐라하는거",
          "speaker_id": "2",
          "time": "20210828 09:32"
        },
        {
          "id": "MDRW2100020543.1.3",
          "form": "제가 다 싫더라구요",
          "original_form": "제가 다 싫더라구요",
          "speaker_id": "2",
          "time": "20210828 09:32"
        },
        {
          "id": "MDRW2100020543.1.4",
          "form": "취향존중이 안되는 느낌",
          "original_form": "취향존중이 안되는 느낌",
          "speaker_id": "1",
          "time": "20210828 09:32"
        },
        {
          "id": "MDRW2100020543.1.5",
          "form": "남 먹는거에 오지랖이 심한 느낌 ㅠ",
          "original_form": "남 먹는거에 오지랖이 심한 느낌 ㅠ",
          "speaker_id": "2",
          "time": "20210828 09:32"
        },
        {
          "id": "MDRW2100020543.1.6",
          "form": "굳이 그렇게 비꼴거까진없는데",
          "original_form": "굳이 그렇게 비꼴거까진없는데",
          "speaker_id": "1",
          "time": "20210828 09:32"
        },
        {
          "id": "MDRW2100020543.1.7",
          "form": "채식주의자들한테 유독 박하더라구요",
          "original_form": "채식주의자들한테 유독 박하더라구요",
          "speaker_id": "1",
          "time": "20210828 09:32"
        },
        {
          "id": "MDRW2100020543.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 09:33"
        },
        {
          "id": "MDRW2100020543.1.9",
          "form": "알러지야 눈에 보이면 멈추는데 베지테리언은..",
          "original_form": "알러지야 눈에 보이면 멈추는데 베지테리언은..",
          "speaker_id": "1",
          "time": "20210828 09:33"
        },
        {
          "id": "MDRW2100020543.1.10",
          "form": "채식주의자들이 채식하면 나한테도 이득인데",
          "original_form": "채식주의자들이 채식하면 나한테도 이득인데",
          "speaker_id": "2",
          "time": "20210828 09:33"
        },
        {
          "id": "MDRW2100020543.1.11",
          "form": "그게 아니라 약간 아니꼬워하는 느낌",
          "original_form": "그게 아니라 약간 아니꼬워하는 느낌",
          "speaker_id": "1",
          "time": "20210828 09:33"
        },
        {
          "id": "MDRW2100020543.1.12",
          "form": "왜 취존을 못하는건지,,",
          "original_form": "왜 취존을 못하는건지,,",
          "speaker_id": "2",
          "time": "20210828 09:33"
        },
        {
          "id": "MDRW2100020543.1.13",
          "form": "어..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "어..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 09:33"
        },
        {
          "id": "MDRW2100020543.1.14",
          "form": "듣고보니 그러네요",
          "original_form": "듣고보니 그러네요",
          "speaker_id": "1",
          "time": "20210828 09:33"
        },
        {
          "id": "MDRW2100020543.1.15",
          "form": "ㅋㅋㅋㅋ생각의 전환",
          "original_form": "ㅋㅋㅋㅋ생각의 전환",
          "speaker_id": "1",
          "time": "20210828 09:33"
        },
        {
          "id": "MDRW2100020543.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ저만 이런생각 하나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저만 이런생각 하나요",
          "speaker_id": "2",
          "time": "20210828 09:33"
        },
        {
          "id": "MDRW2100020543.1.17",
          "form": "네ㅋㅋㅋㅋ저 한번도 못해봤어욥",
          "original_form": "네ㅋㅋㅋㅋ저 한번도 못해봤어욥",
          "speaker_id": "1",
          "time": "20210828 09:34"
        },
        {
          "id": "MDRW2100020543.1.18",
          "form": "방금 머리가 띵해진 기분",
          "original_form": "방금 머리가 띵해진 기분",
          "speaker_id": "1",
          "time": "20210828 09:34"
        },
        {
          "id": "MDRW2100020543.1.19",
          "form": "앗글쿤뇨ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗글쿤뇨ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:34"
        },
        {
          "id": "MDRW2100020543.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ역시 한수배우고갑니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ역시 한수배우고갑니다",
          "speaker_id": "1",
          "time": "20210828 09:34"
        },
        {
