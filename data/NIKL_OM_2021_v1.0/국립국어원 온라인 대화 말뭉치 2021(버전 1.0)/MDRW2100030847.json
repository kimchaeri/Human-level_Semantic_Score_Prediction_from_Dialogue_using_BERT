      "utterance": [
        {
          "id": "MDRW2100030847.1.1",
          "form": "네 점심 먹었어요!",
          "original_form": "네 점심 먹었어요!",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030847.1.2",
          "form": "머드셧어요",
          "original_form": "머드셧어요",
          "speaker_id": "1",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030847.1.3",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030847.1.4",
          "form": "저는 사과 먹었어요 ㅎㅎ",
          "original_form": "저는 사과 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030847.1.5",
          "form": "사과만요?",
          "original_form": "사과만요?",
          "speaker_id": "1",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030847.1.6",
          "form": "네! 사과요",
          "original_form": "네! 사과요",
          "speaker_id": "2",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030847.1.7",
          "form": "좀 큰 사과였거든요",
          "original_form": "좀 큰 사과였거든요",
          "speaker_id": "2",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030847.1.8",
          "form": "아하 전 솥밥",
          "original_form": "아하 전 솥밥",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030847.1.9",
          "form": "맛있겠다 ㅠ",
          "original_form": "맛있겠다 ㅠ",
          "speaker_id": "2",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030847.1.10",
          "form": "직접 해 드신거에요?",
          "original_form": "직접 해 드신거에요?",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030847.1.11",
          "form": "사뭇어요",
          "original_form": "사뭇어요",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030847.1.12",
          "form": "17000",
          "original_form": "17000",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030847.1.13",
          "form": "비싸요",
          "original_form": "비싸요",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030847.1.14",
          "form": "ㅠ 가격대가 왜이렇게 올라갔을까요 ㅠㅠ",
          "original_form": "ㅠ 가격대가 왜이렇게 올라갔을까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030847.1.15",
          "form": "ㅜㅡㅜ 밥값비싸요",
          "original_form": "ㅜㅡㅜ 밥값비싸요",
          "speaker_id": "1",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030847.1.16",
          "form": "어제 김치찜 시켜먹는데 가격표 보니까 1인분에 17000원 이더라구요",
          "original_form": "어제 김치찜 시켜먹는데 가격표 보니까 1인분에 17000원 이더라구요",
          "speaker_id": "2",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030847.1.17",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030847.1.18",
          "form": "넘하다",
          "original_form": "넘하다",
          "speaker_id": "1",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030847.1.19",
          "form": "그쵸!",
          "original_form": "그쵸!",
          "speaker_id": "2",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030847.1.20",
          "form": "물론 맛은 있었지만",
          "original_form": "물론 맛은 있었지만",
          "speaker_id": "2",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030847.1.21",
          "form": "고긴 마니들엇나",
          "original_form": "고긴 마니들엇나",
          "speaker_id": "1",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030847.1.22",
          "form": "가격 너무 사악해여 ㅠ",
          "original_form": "가격 너무 사악해여 ㅠ",
          "speaker_id": "2",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030847.1.23",
          "form": "고기가 적당히 들었던 것 같아요",
          "original_form": "고기가 적당히 들었던 것 같아요",
          "speaker_id": "2",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100030847.1.24",
          "form": "김치반 고기반",
          "original_form": "김치반 고기반",
          "speaker_id": "2",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100030847.1.25",
          "form": "그래서 ㅇㅎ요즘 거의해머거요",
          "original_form": "그래서 ㅇㅎ요즘 거의해머거요",
          "speaker_id": "1",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100030847.1.26",
          "form": "맛있는 레시피 있으면 공유해주세요 ",
          "original_form": "맛있는 레시피 있으면 공유해주세요 ",
          "speaker_id": "2",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030847.1.27",
          "form": "ㅋㅋㅋ 그냥 시판 파스타 해먹는데ㅠㅠ",
          "original_form": "ㅋㅋㅋ 그냥 시판 파스타 해먹는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030847.1.28",
          "form": "요리허접이용...",
          "original_form": "요리허접이용...",
          "speaker_id": "1",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030847.1.29",
          "form": "파스타 말고 다른거도 좋아용",
          "original_form": "파스타 말고 다른거도 좋아용",
          "speaker_id": "2",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100030847.1.30",
          "form": "ㅋㅋㅋㅋ 사골국물에 만두넣어 끓여먹어욬ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 사골국물에 만두넣어 끓여먹어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100030847.1.31",
          "form": "아핳 ㅋㅋㅋㅋ 만둣국도 좋아요 ㅋㅋ",
          "original_form": "아핳 ㅋㅋㅋㅋ 만둣국도 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100030847.1.32",
          "form": "시판용 만두중에 하가우 만두",
          "original_form": "시판용 만두중에 하가우 만두",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100030847.1.33",
          "form": "ㅋㅋ오 첨들어바요",
          "original_form": "ㅋㅋ오 첨들어바요",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100030847.1.34",
          "form": "까만거 포장지 있는데",
          "original_form": "까만거 포장지 있는데",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100030847.1.35",
          "form": "마싯나요",
          "original_form": "마싯나요",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100030847.1.36",
          "form": "고기?",
          "original_form": "고기?",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100030847.1.37",
          "form": "그거 새우만두 진짜 존맛탱이에요!",
          "original_form": "그거 새우만두 진짜 존맛탱이에요!",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100030847.1.38",
          "form": "헉 굳굳",
          "original_form": "헉 굳굳",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100030847.1.39",
          "form": "새우만두라니 기대용",
          "original_form": "새우만두라니 기대용",
          "speaker_id": "1",
          "time": "20210904 15:12"
        }
      ]
    }
