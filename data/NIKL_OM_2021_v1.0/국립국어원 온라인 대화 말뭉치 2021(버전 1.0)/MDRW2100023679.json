      "utterance": [
        {
          "id": "MDRW2100023679.1.1",
          "form": "오 뭐야 별로야 뭐여",
          "original_form": "오 뭐야 별로야 뭐여",
          "speaker_id": "2",
          "time": "20210830 14:29"
        },
        {
          "id": "MDRW2100023679.1.2",
          "form": "예의없다는",
          "original_form": "예의없다는",
          "speaker_id": "1",
          "time": "20210830 14:29"
        },
        {
          "id": "MDRW2100023679.1.3",
          "form": "그런인식퍼졌으면",
          "original_form": "그런인식퍼졌으면",
          "speaker_id": "1",
          "time": "20210830 14:29"
        },
        {
          "id": "MDRW2100023679.1.4",
          "form": "좋겠어",
          "original_form": "좋겠어",
          "speaker_id": "1",
          "time": "20210830 14:29"
        },
        {
          "id": "MDRW2100023679.1.5",
          "form": "결혼식에 너무 허례허식이 많아",
          "original_form": "결혼식에 너무 허례허식이 많아",
          "speaker_id": "2",
          "time": "20210830 14:30"
        },
        {
          "id": "MDRW2100023679.1.6",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210830 14:31"
        },
        {
          "id": "MDRW2100023679.1.7",
          "form": "남자는 정장에 보타이 매고 피부 정리만 하면 되는데",
          "original_form": "남자는 정장에 보타이 매고 피부 정리만 하면 되는데",
          "speaker_id": "2",
          "time": "20210830 14:31"
        },
        {
          "id": "MDRW2100023679.1.8",
          "form": "그냥결혼식",
          "original_form": "그냥결혼식",
          "speaker_id": "1",
          "time": "20210830 14:31"
        },
        {
          "id": "MDRW2100023679.1.9",
          "form": "지들끼리만했으면",
          "original_form": "지들끼리만했으면",
          "speaker_id": "1",
          "time": "20210830 14:31"
        },
        {
          "id": "MDRW2100023679.1.10",
          "form": "좋겠어",
          "original_form": "좋겠어",
          "speaker_id": "1",
          "time": "20210830 14:31"
        },
        {
          "id": "MDRW2100023679.1.11",
          "form": "여자는 드레스에 새벽부터 나와서 화장 받고",
          "original_form": "여자는 드레스에 새벽부터 나와서 화장 받고",
          "speaker_id": "2",
          "time": "20210830 14:31"
        },
        {
          "id": "MDRW2100023679.1.12",
          "form": "청첩장돌리지말고",
          "original_form": "청첩장돌리지말고",
          "speaker_id": "1",
          "time": "20210830 14:31"
        },
        {
          "id": "MDRW2100023679.1.13",
          "form": "하객들도 꾸미고 와야함",
          "original_form": "하객들도 꾸미고 와야함",
          "speaker_id": "2",
          "time": "20210830 14:31"
        },
        {
          "id": "MDRW2100023679.1.14",
          "form": "어차피지들선택이라",
          "original_form": "어차피지들선택이라",
          "speaker_id": "1",
          "time": "20210830 14:31"
        },
        {
          "id": "MDRW2100023679.1.15",
          "form": "욕하고싶지도않음",
          "original_form": "욕하고싶지도않음",
          "speaker_id": "1",
          "time": "20210830 14:32"
        },
        {
          "id": "MDRW2100023679.1.16",
          "form": "그냥나한테피해만없었으면",
          "original_form": "그냥나한테피해만없었으면",
          "speaker_id": "1",
          "time": "20210830 14:32"
        },
        {
          "id": "MDRW2100023679.1.17",
          "form": "함",
          "original_form": "함",
          "speaker_id": "1",
          "time": "20210830 14:32"
        },
        {
          "id": "MDRW2100023679.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:32"
        },
        {
          "id": "MDRW2100023679.1.19",
          "form": "아 결혼식 하니까 생각난건데",
          "original_form": "아 결혼식 하니까 생각난건데",
          "speaker_id": "2",
          "time": "20210830 14:32"
        },
        {
          "id": "MDRW2100023679.1.20",
          "form": "송은이 하객짤 봄?",
          "original_form": "송은이 하객짤 봄?",
          "speaker_id": "2",
          "time": "20210830 14:32"
        },
        {
          "id": "MDRW2100023679.1.21",
          "form": "ㄹㅇㅇㅇ엉ㅇ",
          "original_form": "ㄹㅇㅇㅇ엉ㅇ",
          "speaker_id": "1",
          "time": "20210830 14:32"
        },
        {
          "id": "MDRW2100023679.1.22",
          "form": "몰라",
          "original_form": "몰라",
          "speaker_id": "1",
          "time": "20210830 14:32"
        },
        {
          "id": "MDRW2100023679.1.23",
          "form": "그건뭐임,",
          "original_form": "그건뭐임,",
          "speaker_id": "1",
