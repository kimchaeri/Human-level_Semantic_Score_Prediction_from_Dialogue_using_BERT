      "utterance": [
        {
          "id": "MDRW2100005746.1.1",
          "form": "책 뭐 좋아하세요?",
          "original_form": "책 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 10:32"
        },
        {
          "id": "MDRW2100005746.1.2",
          "form": "소설책 좋아해요",
          "original_form": "소설책 좋아해요",
          "speaker_id": "2",
          "time": "20210907 10:33"
        },
        {
          "id": "MDRW2100005746.1.3",
          "form": "에세이류도",
          "original_form": "에세이류도",
          "speaker_id": "2",
          "time": "20210907 10:33"
        },
        {
          "id": "MDRW2100005746.1.4",
          "form": "오 소설은 어떤 종류요?",
          "original_form": "오 소설은 어떤 종류요?",
          "speaker_id": "1",
          "time": "20210907 10:34"
        },
        {
          "id": "MDRW2100005746.1.5",
          "form": "크게 가리진 않는데",
          "original_form": "크게 가리진 않는데",
          "speaker_id": "2",
          "time": "20210907 10:34"
        },
        {
          "id": "MDRW2100005746.1.6",
          "form": "요즘은 여성작가들의 책을 주로 봐요",
          "original_form": "요즘은 여성작가들의 책을 주로 봐요",
          "speaker_id": "2",
          "time": "20210907 10:35"
        },
        {
          "id": "MDRW2100005746.1.7",
          "form": "오 전 판타지종류면 왠만하면 다 재밌게 보는거같아요",
          "original_form": "오 전 판타지종류면 왠만하면 다 재밌게 보는거같아요",
          "speaker_id": "1",
          "time": "20210907 10:35"
        },
        {
          "id": "MDRW2100005746.1.8",
          "form": "오 재미있게 본 책이 있으신가요?",
          "original_form": "오 재미있게 본 책이 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 10:39"
        },
        {
          "id": "MDRW2100005746.1.9",
          "form": "나름 유명한 소설인데 이영도작 눈물을 마시는 새 재밌게봤어요",
          "original_form": "나름 유명한 소설인데 이영도작 눈물을 마시는 새 재밌게봤어요",
          "speaker_id": "1",
          "time": "20210907 11:42"
        },
        {
          "id": "MDRW2100005746.1.10",
          "form": "아 들어본 적 있는 작품이예요",
          "original_form": "아 들어본 적 있는 작품이예요",
          "speaker_id": "2",
          "time": "20210907 11:52"
        },
        {
          "id": "MDRW2100005746.1.11",
          "form": "어떤 내용인가요?",
          "original_form": "어떤 내용인가요?",
          "speaker_id": "2",
          "time": "20210907 11:52"
        },
        {
          "id": "MDRW2100005746.1.12",
          "form": "한번에 설명하긴 어려운책인데.. 반전도 있어서 그런데 한번보시면 재밌게보실거예요!",
          "original_form": "한번에 설명하긴 어려운책인데.. 반전도 있어서 그런데 한번보시면 재밌게보실거예요!",
          "speaker_id": "1",
          "time": "20210907 11:55"
        },
        {
          "id": "MDRW2100005746.1.13",
          "form": "아아 한번 읽어볼게요!!",
          "original_form": "아아 한번 읽어볼게요!!",
          "speaker_id": "2",
          "time": "20210907 12:00"
        },
        {
          "id": "MDRW2100005746.1.14",
          "form": "책 읽는 것 좋아하시나요",
          "original_form": "책 읽는 것 좋아하시나요",
          "speaker_id": "2",
          "time": "20210907 12:00"
        },
        {
          "id": "MDRW2100005746.1.15",
          "form": "네좋아해요 근데요즘은 잘 못보는중이에요...!!ㅋㅋ",
          "original_form": "네좋아해요 근데요즘은 잘 못보는중이에요...!!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:03"
        },
        {
          "id": "MDRW2100005746.1.16",
          "form": "아 저는 도서관에 신작들이 잘 들어와서",
          "original_form": "아 저는 도서관에 신작들이 잘 들어와서",
          "speaker_id": "2",
          "time": "20210907 12:44"
        },
        {
          "id": "MDRW2100005746.1.17",
          "form": "도서관에서 자주 빌려보고 있어요",
          "original_form": "도서관에서 자주 빌려보고 있어요",
          "speaker_id": "2",
          "time": "20210907 12:44"
        },
        {
          "id": "MDRW2100005746.1.18",
          "form": "오 신작중에 좋아하는 거 있어요?",
          "original_form": "오 신작중에 좋아하는 거 있어요?",
          "speaker_id": "1",
          "time": "20210907 12:49"
        },
        {
          "id": "MDRW2100005746.1.19",
          "form": "최근 절멸이란 책을 빌려봤어요",
          "original_form": "최근 절멸이란 책을 빌려봤어요",
          "speaker_id": "2",
          "time": "20210907 13:02"
        }
      ]
