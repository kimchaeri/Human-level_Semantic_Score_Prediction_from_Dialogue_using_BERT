      "utterance": [
        {
          "id": "MDRW2100005713.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210823 14:56"
        },
        {
          "id": "MDRW2100005713.1.2",
          "form": "혹시 또 유튜브 보시는 거 있으세요?",
          "original_form": "혹시 또 유튜브 보시는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210823 14:56"
        },
        {
          "id": "MDRW2100005713.1.3",
          "form": "전 해쭈님이랑 예랑가랑 좋아해요",
          "original_form": "전 해쭈님이랑 예랑가랑 좋아해요",
          "speaker_id": "2",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100005713.1.4",
          "form": "파르페님은요?",
          "original_form": "파르페님은요?",
          "speaker_id": "2",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100005713.1.5",
          "form": "아 두 분 다 구독하고 있어요!",
          "original_form": "아 두 분 다 구독하고 있어요!",
          "speaker_id": "1",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100005713.1.6",
          "form": "둘 다 보면서 한참 웃고 힐링돼요",
          "original_form": "둘 다 보면서 한참 웃고 힐링돼요",
          "speaker_id": "1",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100005713.1.7",
          "form": "맞아요 너무 웃겨요",
          "original_form": "맞아요 너무 웃겨요",
          "speaker_id": "2",
          "time": "20210823 15:21"
        },
        {
          "id": "MDRW2100005713.1.8",
          "form": "음악은 자주 들으세요?",
          "original_form": "음악은 자주 들으세요?",
          "speaker_id": "2",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100005713.1.9",
          "form": "자주 듣는 편이에요! 요즘은 자빱 사단 노래를 더 듣는 거 같긴한데ㅎㅎ 노래가 좀 좋아야죠!",
          "original_form": "자주 듣는 편이에요! 요즘은 자빱 사단 노래를 더 듣는 거 같긴한데ㅎㅎ 노래가 좀 좋아야죠!",
          "speaker_id": "1",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100005713.1.10",
          "form": "고고 님은 노래 자주 들으시나요?",
          "original_form": "고고 님은 노래 자주 들으시나요?",
          "speaker_id": "1",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100005713.1.11",
          "form": "ㅋㅋㅋ전 자빱사단 노래 공부할 때 많이 들어요",
          "original_form": "ㅋㅋㅋ전 자빱사단 노래 공부할 때 많이 들어요",
          "speaker_id": "2",
          "time": "20210823 15:26"
        },
        {
          "id": "MDRW2100005713.1.12",
          "form": "네 인디노래 좋아해서 인디가수들 거 주로 들어요",
          "original_form": "네 인디노래 좋아해서 인디가수들 거 주로 들어요",
          "speaker_id": "2",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100005713.1.13",
          "form": "인디 음악 좋죠! 정우 님이나 민수 님 노래 좋아해요",
          "original_form": "인디 음악 좋죠! 정우 님이나 민수 님 노래 좋아해요",
          "speaker_id": "1",
          "time": "20210823 15:30"
        },
        {
          "id": "MDRW2100005713.1.14",
          "form": "오 저도 민수 님 노래 좋아해요!",
          "original_form": "오 저도 민수 님 노래 좋아해요!",
          "speaker_id": "2",
          "time": "20210823 15:39"
        },
        {
          "id": "MDRW2100005713.1.15",
          "form": "요즘엔 유라 님 노래에 빠졌어요",
          "original_form": "요즘엔 유라 님 노래에 빠졌어요",
          "speaker_id": "2",
          "time": "20210823 15:39"
        },
        {
          "id": "MDRW2100005713.1.16",
          "form": "유라 님 노래 추천해주실 수 있나요?",
          "original_form": "유라 님 노래 추천해주실 수 있나요?",
          "speaker_id": "1",
          "time": "20210823 15:41"
        },
        {
          "id": "MDRW2100005713.1.17",
          "form": "춤, my, 하양 추천해요",
          "original_form": "춤, my, 하양 추천해요",
          "speaker_id": "2",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100005713.1.18",
          "form": "춤은 휘수님이 댄스커버 하시기도 하셨어요",
          "original_form": "춤은 휘수님이 댄스커버 하시기도 하셨어요",
          "speaker_id": "2",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100005713.1.19",
          "form": "꼭 들어볼게요!",
          "original_form": "꼭 들어볼게요!",
          "speaker_id": "1",
          "time": "20210823 15:49"
        },
        {
          "id": "MDRW2100005713.1.20",
          "form": "안예은 님 노래 좋아하시나요?",
          "original_form": "안예은 님 노래 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 15:49"
        },
        {
          "id": "MDRW2100005713.1.21",
          "form": "잘 안 듣는 편인데 이번 창귀는 여러 번 들어봤어요",
          "original_form": "잘 안 듣는 편인데 이번 창귀는 여러 번 들어봤어요",
          "speaker_id": "2",
          "time": "20210823 15:55"
        },
        {
          "id": "MDRW2100005713.1.22",
          "form": "역시 안예은 님 노래는 컨셉이 독보적이에요",
          "original_form": "역시 안예은 님 노래는 컨셉이 독보적이에요",
          "speaker_id": "2",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100005713.1.23",
          "form": "저도 이번 창귀는 자주 들었어요",
          "original_form": "저도 이번 창귀는 자주 들었어요",
          "speaker_id": "1",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100005713.1.24",
          "form": "소재도 그렇고 독특하고 매력적인 노래더라고요",
          "original_form": "소재도 그렇고 독특하고 매력적인 노래더라고요",
          "speaker_id": "1",
          "time": "20210823 15:58"
        }
      ]
    }
