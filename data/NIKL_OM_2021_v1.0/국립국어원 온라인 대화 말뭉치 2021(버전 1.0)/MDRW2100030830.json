      "utterance": [
        {
          "id": "MDRW2100030830.1.1",
          "form": "단소 만ㄷㄴ것도 있는데",
          "original_form": "단소 만ㄷㄴ것도 있는데",
          "speaker_id": "2",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030830.1.2",
          "form": "그건 아직 집에 있죠!! 맞아요",
          "original_form": "그건 아직 집에 있죠!! 맞아요",
          "speaker_id": "1",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030830.1.3",
          "form": "대나무 박물관 가서 만든거",
          "original_form": "대나무 박물관 가서 만든거",
          "speaker_id": "2",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030830.1.4",
          "form": "근데 제가 만들었달까",
          "original_form": "근데 제가 만들었달까",
          "speaker_id": "1",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030830.1.5",
          "form": "다 만들어주신...",
          "original_form": "다 만들어주신...",
          "speaker_id": "1",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030830.1.6",
          "form": "애들이 만들면 돈받고 안팔지",
          "original_form": "애들이 만들면 돈받고 안팔지",
          "speaker_id": "2",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030830.1.7",
          "form": "그래도 재미는 있었어요 ㅋㅋㅋ 소리도 잘나고 냄새도 좋고",
          "original_form": "그래도 재미는 있었어요 ㅋㅋㅋ 소리도 잘나고 냄새도 좋고",
          "speaker_id": "1",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100030830.1.8",
          "form": "그거 고등학교 때까지 썼나",
          "original_form": "그거 고등학교 때까지 썼나",
          "speaker_id": "2",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100030830.1.9",
          "form": "지금도 있긴 한데",
          "original_form": "지금도 있긴 한데",
          "speaker_id": "2",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100030830.1.10",
          "form": "저 그걸로 수행평가 점수 높게 받았잖아요 ㅎㅎ 어릴때 만든거",
          "original_form": "저 그걸로 수행평가 점수 높게 받았잖아요 ㅎㅎ 어릴때 만든거",
          "speaker_id": "1",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100030830.1.11",
          "form": "4천원인가 8천원짜리 오래 써서 다행이다",
          "original_form": "4천원인가 8천원짜리 오래 써서 다행이다",
          "speaker_id": "2",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100030830.1.12",
          "form": "만들길 잘한거같아요 ㅋ 수제라고 하니까 애들이 부러워해서",
          "original_form": "만들길 잘한거같아요 ㅋ 수제라고 하니까 애들이 부러워해서",
          "speaker_id": "1",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100030830.1.13",
          "form": "부럽다고 해야하나?? 신기해해서",
          "original_form": "부럽다고 해야하나?? 신기해해서",
          "speaker_id": "1",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100030830.1.14",
          "form": "다들 플라스틱 단소 사왔어?",
          "original_form": "다들 플라스틱 단소 사왔어?",
          "speaker_id": "2",
          "time": "20210904 13:11"
        },
        {
          "id": "MDRW2100030830.1.15",
          "form": "그정도까진 아니지만 그렇다고 해도 수제는 저밖에ㅜ없어가지구",
          "original_form": "그정도까진 아니지만 그렇다고 해도 수제는 저밖에ㅜ없어가지구",
          "speaker_id": "1",
          "time": "20210904 13:11"
        },
        {
          "id": "MDRW2100030830.1.16",
          "form": "좋았네요 ㅎㅎ 자랑돼서",
          "original_form": "좋았네요 ㅎㅎ 자랑돼서",
          "speaker_id": "1",
          "time": "20210904 13:11"
        }
