      "utterance": [
        {
          "id": "MDRW2100023344.1.1",
          "form": "시장가서 찹싼 도나스도 사 무야지ㅋㅋ",
          "original_form": "시장가서 찹싼 도나스도 사 무야지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:28"
        },
        {
          "id": "MDRW2100023344.1.2",
          "form": "장날에 비나 안 왔으면",
          "original_form": "장날에 비나 안 왔으면",
          "speaker_id": "1",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023344.1.3",
          "form": "그니까ㅋㅋ 비 때문에 장 안선지 꾀 된듯ㅋ",
          "original_form": "그니까ㅋㅋ 비 때문에 장 안선지 꾀 된듯ㅋ",
          "speaker_id": "2",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023344.1.4",
          "form": "장에 파는 줄 알았음 오늘 가는건데",
          "original_form": "장에 파는 줄 알았음 오늘 가는건데",
          "speaker_id": "1",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023344.1.5",
          "form": "진짜 되게 먹고싶은갑네ㅋㅋ",
          "original_form": "진짜 되게 먹고싶은갑네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023344.1.6",
          "form": "생각났을때 먹어야디 ㅋㅋㅋ 최근 유일하게 먹고싶은거라",
          "original_form": "생각났을때 먹어야디 ㅋㅋㅋ 최근 유일하게 먹고싶은거라",
          "speaker_id": "1",
          "time": "20210830 00:40"
        },
        {
          "id": "MDRW2100023344.1.7",
          "form": "소스에 야채비벼서 만두로 싸먹음 맛있지ㅋㅋ 장에 안간지 진짜 오래됐네ㅋㅋ",
          "original_form": "소스에 야채비벼서 만두로 싸먹음 맛있지ㅋㅋ 장에 안간지 진짜 오래됐네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:41"
        },
        {
          "id": "MDRW2100023344.1.8",
          "form": "갑자기 군침이 싹 도는구만",
          "original_form": "갑자기 군침이 싹 도는구만",
          "speaker_id": "1",
          "time": "20210830 00:43"
        },
        {
          "id": "MDRW2100023344.1.9",
          "form": "군침 돌기 딱 좋은 시간이네ㅋㅋ",
          "original_form": "군침 돌기 딱 좋은 시간이네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:53"
        },
        {
          "id": "MDRW2100023344.1.10",
          "form": "오늘은 또 무엇을 먹나",
          "original_form": "오늘은 또 무엇을 먹나",
          "speaker_id": "1",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100023344.1.11",
          "form": "난 초코그레놀라 우유에 말아먹었다ㅋㅋ",
          "original_form": "난 초코그레놀라 우유에 말아먹었다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023344.1.12",
          "form": "맛있겠다 리필해서 또먹구",
          "original_form": "맛있겠다 리필해서 또먹구",
          "speaker_id": "1",
          "time": "20210830 12:19"
        },
        {
          "id": "MDRW2100023344.1.13",
          "form": "당영하지ㅋㅋㅋㅋ  리필 안하면 너무 매정하잖아ㅋㅋㅋ",
          "original_form": "당영하지ㅋㅋㅋㅋ  리필 안하면 너무 매정하잖아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:29"
        },
        {
          "id": "MDRW2100023344.1.14",
          "form": "전어회 먹을랬는데 오늘 횟집 쉬는 날이더라",
          "original_form": "전어회 먹을랬는데 오늘 횟집 쉬는 날이더라",
          "speaker_id": "1",
