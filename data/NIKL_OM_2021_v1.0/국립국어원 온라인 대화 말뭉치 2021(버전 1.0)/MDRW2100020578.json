      "utterance": [
        {
          "id": "MDRW2100020578.1.1",
          "form": "아아 맞아요",
          "original_form": "아아 맞아요",
          "speaker_id": "2",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020578.1.2",
          "form": "한번 앉았다 내려놓으면",
          "original_form": "한번 앉았다 내려놓으면",
          "speaker_id": "2",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020578.1.3",
          "form": "털이 수북....",
          "original_form": "털이 수북....",
          "speaker_id": "2",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020578.1.4",
          "form": "털을 진짜 뿜어요",
          "original_form": "털을 진짜 뿜어요",
          "speaker_id": "2",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020578.1.5",
          "form": "검은옷 못입는다고 그러더라구요ㅋㅋ",
          "original_form": "검은옷 못입는다고 그러더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020578.1.6",
          "form": "마자요 진짜 뿜는다고 표현할정도라고 하더라구요",
          "original_form": "마자요 진짜 뿜는다고 표현할정도라고 하더라구요",
          "speaker_id": "1",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020578.1.7",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020578.1.8",
          "form": "진짜 미쳤어요",
          "original_form": "진짜 미쳤어요",
          "speaker_id": "2",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020578.1.9",
          "form": "사방팔방에 털 굴러다녀요",
          "original_form": "사방팔방에 털 굴러다녀요",
          "speaker_id": "2",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020578.1.10",
          "form": "감안하고 키우셔야해요 ㅠㅠ",
          "original_form": "감안하고 키우셔야해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020578.1.11",
          "form": "테이프  필수더라구요",
          "original_form": "테이프  필수더라구요",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020578.1.12",
          "form": "찍찍이....",
          "original_form": "찍찍이....",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020578.1.13",
          "form": "한달에 거의  한박스 산 것 같아요",
          "original_form": "한달에 거의  한박스 산 것 같아요",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020578.1.14",
          "form": "키우기 쉬운 동물은 없네요ㅜ",
          "original_form": "키우기 쉬운 동물은 없네요ㅜ",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020578.1.15",
          "form": "햄스터가 그나마 쉬워요",
          "original_form": "햄스터가 그나마 쉬워요",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020578.1.16",
          "form": "와..그돈도 꽤 나가겠네요",
          "original_form": "와..그돈도 꽤 나가겠네요",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020578.1.17",
          "form": "거북이랑 ㅎㅎ",
          "original_form": "거북이랑 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020578.1.18",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020578.1.19",
          "form": "그니까요 햄스터도 괜찮죠",
          "original_form": "그니까요 햄스터도 괜찮죠",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020578.1.20",
          "form": "요새는 물고기도 많이 키우더라구요",
          "original_form": "요새는 물고기도 많이 키우더라구요",
          "speaker_id": "1",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020578.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020578.1.22",
          "form": "근데 전 역시 서로 통하는 무언가",
          "original_form": "근데 전 역시 서로 통하는 무언가",
          "speaker_id": "2",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020578.1.23",
          "form": "마자요 교감이 되는 동물이  좋죠",
          "original_form": "마자요 교감이 되는 동물이  좋죠",
          "speaker_id": "1",
          "time": "20210828 10:11"
        },
        {
          "id": "MDRW2100020578.1.24",
          "form": "그런게 좋아서 고생하더라도 개가 제일 좋아요",
          "original_form": "그런게 좋아서 고생하더라도 개가 제일 좋아요",
          "speaker_id": "2",
          "time": "20210828 10:11"
        },
        {
          "id": "MDRW2100020578.1.25",
          "form": "마자요 저두요!",
          "original_form": "마자요 저두요!",
          "speaker_id": "1",
          "time": "20210828 10:11"
        }
