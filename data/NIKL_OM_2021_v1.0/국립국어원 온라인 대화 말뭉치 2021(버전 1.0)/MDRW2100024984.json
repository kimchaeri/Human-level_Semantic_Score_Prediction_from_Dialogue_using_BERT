      "utterance": [
        {
          "id": "MDRW2100024984.1.1",
          "form": "자 그럼 저희 학교앞 최고존엄 맛집에 대해서 얘기해 봅시다",
          "original_form": "자 그럼 저희 학교앞 최고존엄 맛집에 대해서 얘기해 봅시다",
          "speaker_id": "1",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100024984.1.2",
          "form": "포.포.나.무.",
          "original_form": "포.포.나.무.",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100024984.1.3",
          "form": "인정!",
          "original_form": "인정!",
          "speaker_id": "1",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100024984.1.4",
          "form": "name2씨 서왕만두 알아요?",
          "original_form": "&name2&씨 서왕만두 알아요?",
          "speaker_id": "1",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100024984.1.5",
          "form": "아니요아니요",
          "original_form": "아니요아니요",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100024984.1.6",
          "form": "어딨는거에요???",
          "original_form": "어딨는거에요???",
          "speaker_id": "2",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100024984.1.7",
          "form": "그거 훈카츠쪽에?",
          "original_form": "그거 훈카츠쪽에?",
          "speaker_id": "1",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100024984.1.8",
          "form": "있는데 진짜 맛있어요 ㅜㅜ",
          "original_form": "있는데 진짜 맛있어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100024984.1.9",
          "form": "언닌 보통 어떤만두먹어요???",
          "original_form": "언닌 보통 어떤만두먹어요???",
          "speaker_id": "2",
          "time": "20210901 07:28"
        },
        {
          "id": "MDRW2100024984.1.10",
          "form": "거기서요!",
          "original_form": "거기서요!",
          "speaker_id": "2",
          "time": "20210901 07:28"
        },
        {
          "id": "MDRW2100024984.1.11",
          "form": "거기 다 맛있어요 군만두랑 새우만두랑 교자!!",
          "original_form": "거기 다 맛있어요 군만두랑 새우만두랑 교자!!",
          "speaker_id": "1",
          "time": "20210901 08:53"
        },
        {
          "id": "MDRW2100024984.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 08:53"
        },
        {
          "id": "MDRW2100024984.1.13",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100024984.1.14",
          "form": "언니 오늘 출근하셨겠네요!!",
          "original_form": "언니 오늘 출근하셨겠네요!!",
          "speaker_id": "2",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100024984.1.15",
          "form": "맞아요 ㅋㅋㅋ 지금 오트밀먹고있어요",
          "original_form": "맞아요 ㅋㅋㅋ 지금 오트밀먹고있어요",
          "speaker_id": "1",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100024984.1.16",
          "form": "왕 전 할리스왔어요ㅋㅋ",
          "original_form": "왕 전 할리스왔어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:36"
        },
        {
          "id": "MDRW2100024984.1.17",
          "form": "저 이번주에 ㅂㅈㅎ교수님이랑 상담해요ㅋㅋ",
          "original_form": "저 이번주에 ㅂㅈㅎ교수님이랑 상담해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:36"
        },
        {
          "id": "MDRW2100024984.1.18",
          "form": "ㅋㅋㅋㅋㅋ 언제가요??",
          "original_form": "ㅋㅋㅋㅋㅋ 언제가요??",
          "speaker_id": "1",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100024984.1.19",
          "form": "잘 돼야할텐데...ㅜㅜ",
          "original_form": "잘 돼야할텐데...ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100024984.1.20",
          "form": "금요일이요!!!",
          "original_form": "금요일이요!!!",
          "speaker_id": "2",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100024984.1.21",
          "form": "그렇군요! 파이팅이에요!!",
          "original_form": "그렇군요! 파이팅이에요!!",
          "speaker_id": "1",
          "time": "20210901 15:48"
        },
        {
          "id": "MDRW2100024984.1.22",
          "form": "할리스는 왜갔어요? 뭐 마시러??",
          "original_form": "할리스는 왜갔어요? 뭐 마시러??",
          "speaker_id": "1",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100024984.1.23",
          "form": "커피마시면서 강의들으려고요ㅋㅋㅋ",
          "original_form": "커피마시면서 강의들으려고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100024984.1.24",
          "form": "거의 놀다시피하고 오긴했어요ㅋㅋ",
          "original_form": "거의 놀다시피하고 오긴했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100024984.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ 개강 초에 놀아야죠 할리스는 딸기... 뭐가 유명하던데!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 개강 초에 놀아야죠 할리스는 딸기... 뭐가 유명하던데!!",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100024984.1.26",
          "form": "ㅜㅜㅜ 배고프다",
          "original_form": "ㅜㅜㅜ 배고프다",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100024984.1.27",
          "form": "언니 몇시 퇴근이에요???",
          "original_form": "언니 몇시 퇴근이에요???",
          "speaker_id": "2",
          "time": "20210901 17:45"
        },
        {
          "id": "MDRW2100024984.1.28",
          "form": "이제 저녁먹을시간 된거같은데ㅋㅋ",
          "original_form": "이제 저녁먹을시간 된거같은데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:45"
        },
        {
          "id": "MDRW2100024984.1.29",
          "form": "글쎄요...ㅜ",
          "original_form": "글쎄요...ㅜ",
          "speaker_id": "1",
          "time": "20210901 18:06"
        },
        {
          "id": "MDRW2100024984.1.30",
          "form": "저 일단 저녁먹으러 오ㅓㅆ어요 !",
          "original_form": "저 일단 저녁먹으러 오ㅓㅆ어요 !",
          "speaker_id": "1",
          "time": "20210901 18:06"
        },
        {
          "id": "MDRW2100024984.1.31",
          "form": "오늘 저녁.. 오믈렛...ㅎㅎㅎㅎ",
          "original_form": "오늘 저녁.. 오믈렛...ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100024984.1.32",
          "form": "언니네양식당인가요???",
          "original_form": "언니네양식당인가요???",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100024984.1.33",
          "form": "아뇨! 하숙집 밥이요 ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨! 하숙집 밥이요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100024984.1.34",
          "form": "저 항상 하숙집 밥... 먹어요...",
          "original_form": "저 항상 하숙집 밥... 먹어요...",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100024984.1.35",
          "form": "아아맞다",
          "original_form": "아아맞다",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100024984.1.36",
          "form": "여전히 맛있나요???",
          "original_form": "여전히 맛있나요???",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100024984.1.37",
          "form": "맛있다고 들었었던거같은데!",
          "original_form": "맛있다고 들었었던거같은데!",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100024984.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋ 오늘 맛있었어요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 오늘 맛있었어요!",
