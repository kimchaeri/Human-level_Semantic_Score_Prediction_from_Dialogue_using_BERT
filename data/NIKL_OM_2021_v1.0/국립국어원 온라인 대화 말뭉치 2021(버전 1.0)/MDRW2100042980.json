      "utterance": [
        {
          "id": "MDRW2100042980.1.1",
          "form": "빵 좋아하시나요",
          "original_form": "빵 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 12:24"
        },
        {
          "id": "MDRW2100042980.1.2",
          "form": "저 사랑하잖아요",
          "original_form": "저 사랑하잖아요",
          "speaker_id": "2",
          "time": "20210907 12:28"
        },
        {
          "id": "MDRW2100042980.1.3",
          "form": "마라탕다음이 빵이에요",
          "original_form": "마라탕다음이 빵이에요",
          "speaker_id": "2",
          "time": "20210907 12:28"
        },
        {
          "id": "MDRW2100042980.1.4",
          "form": "name1님은 어떠세요",
          "original_form": "&name1&님은 어떠세요",
          "speaker_id": "2",
          "time": "20210907 12:28"
        },
        {
          "id": "MDRW2100042980.1.5",
          "form": "아나 마라탕 오늘 넘 먹고싶네요",
          "original_form": "아나 마라탕 오늘 넘 먹고싶네요",
          "speaker_id": "1",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042980.1.6",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042980.1.7",
          "form": "저는 뻥 조아해서 빵회사에서 일하게되었어요",
          "original_form": "저는 뻥 조아해서 빵회사에서 일하게되었어요",
          "speaker_id": "1",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042980.1.8",
          "form": "캐스팅당해가지고",
          "original_form": "캐스팅당해가지고",
          "speaker_id": "1",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042980.1.9",
          "form": "ㅇㄴ 마라탕 진짜 먹고싶네요",
          "original_form": "ㅇㄴ 마라탕 진짜 먹고싶네요",
          "speaker_id": "1",
          "time": "20210907 12:30"
        },
        {
          "id": "MDRW2100042980.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:32"
        },
        {
          "id": "MDRW2100042980.1.11",
          "form": "헉 대박이다",
          "original_form": "헉 대박이다",
          "speaker_id": "2",
          "time": "20210907 12:32"
        },
        {
          "id": "MDRW2100042980.1.12",
          "form": "완전고스펙빵쟁이",
          "original_form": "완전고스펙빵쟁이",
          "speaker_id": "2",
          "time": "20210907 12:32"
        },
        {
          "id": "MDRW2100042980.1.13",
          "form": "먹기만하는 저랑은 다르시군요",
          "original_form": "먹기만하는 저랑은 다르시군요",
          "speaker_id": "2",
          "time": "20210907 12:33"
        },
        {
          "id": "MDRW2100042980.1.14",
          "form": "고스펙빵쟁이로 추천하는빵있나요",
          "original_form": "고스펙빵쟁이로 추천하는빵있나요",
          "speaker_id": "2",
          "time": "20210907 12:33"
        },
        {
          "id": "MDRW2100042980.1.15",
          "form": "쓰읍 패션5 빵 맛있더라고요",
          "original_form": "쓰읍 패션5 빵 맛있더라고요",
          "speaker_id": "1",
          "time": "20210907 12:33"
        },
        {
          "id": "MDRW2100042980.1.16",
          "form": "그 파바에서 한남동에 차린 빵집인데",
          "original_form": "그 파바에서 한남동에 차린 빵집인데",
          "speaker_id": "1",
          "time": "20210907 12:33"
        },
        {
          "id": "MDRW2100042980.1.17",
          "form": "맛없는 파바 빵이랑 다르던데요",
          "original_form": "맛없는 파바 빵이랑 다르던데요",
          "speaker_id": "1",
          "time": "20210907 12:33"
        },
        {
          "id": "MDRW2100042980.1.18",
          "form": "그거랑 이제 대전에 또 개인빵집 맛난곳 많고",
          "original_form": "그거랑 이제 대전에 또 개인빵집 맛난곳 많고",
          "speaker_id": "1",
          "time": "20210907 12:33"
        },
        {
          "id": "MDRW2100042980.1.19",
          "form": "개인빵집중에 그 몽블랑 잘하는 집 있는데 순간 기억이 안나네요",
          "original_form": "개인빵집중에 그 몽블랑 잘하는 집 있는데 순간 기억이 안나네요",
          "speaker_id": "1",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042980.1.20",
          "form": "대전은 전반적으로 빵퀄이 좋은것같아요",
          "original_form": "대전은 전반적으로 빵퀄이 좋은것같아요",
          "speaker_id": "2",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042980.1.21",
          "form": "몽블랑",
          "original_form": "몽블랑{emoji:♡}",
          "speaker_id": "2",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042980.1.22",
          "form": "그쵸 성심당이",
          "original_form": "그쵸 성심당이",
          "speaker_id": "1",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042980.1.23",
          "form": "올려놓은 괴물들",
          "original_form": "올려놓은 괴물들",
          "speaker_id": "1",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042980.1.24",
          "form": "꼭 기억해내주세요",
          "original_form": "꼭 기억해내주세요",
          "speaker_id": "2",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042980.1.25",
          "form": "지금 빵쟁이한명의 인생이",
          "original_form": "지금 빵쟁이한명의 인생이",
          "speaker_id": "2",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042980.1.26",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042980.1.27",
          "form": "기억..해...기억해..!",
          "original_form": "기억..해...기억해..!",
          "speaker_id": "1",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042980.1.28",
          "form": "개인 빵집 다 본인 이름 내걸고 하잖아요",
          "original_form": "개인 빵집 다 본인 이름 내걸고 하잖아요",
          "speaker_id": "1",
          "time": "20210907 12:35"
        },
        {
          "id": "MDRW2100042980.1.29",
          "form": "서울에 유명한 브라운 거기도 맛있고",
          "original_form": "서울에 유명한 브라운 거기도 맛있고",
          "speaker_id": "1",
          "time": "20210907 12:35"
        },
        {
          "id": "MDRW2100042980.1.30",
          "form": "쓰읍....다맛있어하네",
          "original_form": "쓰읍....다맛있어하네",
          "speaker_id": "1",
          "time": "20210907 12:35"
        },
        {
          "id": "MDRW2100042980.1.31",
          "form": "마라탕 광인으로서 마라탕도 추천해주세려",
          "original_form": "마라탕 광인으로서 마라탕도 추천해주세려",
          "speaker_id": "1",
          "time": "20210907 12:35"
        },
        {
          "id": "MDRW2100042980.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210907 12:37"
        },
        {
          "id": "MDRW2100042980.1.33",
          "form": "저 마라탕먹은곳중에",
          "original_form": "저 마라탕먹은곳중에",
          "speaker_id": "2",
          "time": "20210907 12:37"
        },
        {
          "id": "MDRW2100042980.1.34",
          "form": "아직도 종종떠오르는곳은",
          "original_form": "아직도 종종떠오르는곳은",
          "speaker_id": "2",
          "time": "20210907 12:38"
        },
        {
          "id": "MDRW2100042980.1.35",
          "form": "두근",
          "original_form": "두근",
          "speaker_id": "1",
          "time": "20210907 12:38"
        },
        {
          "id": "MDRW2100042980.1.36",
          "form": "고대앞에 있는 마라탕집인데",
          "original_form": "고대앞에 있는 마라탕집인데",
          "speaker_id": "2",
          "time": "20210907 12:38"
        },
        {
          "id": "MDRW2100042980.1.37",
          "form": "거기 밥이 찰밥이에요",
          "original_form": "거기 밥이 찰밥이에요",
          "speaker_id": "2",
          "time": "20210907 12:38"
        },
        {
          "id": "MDRW2100042980.1.38",
          "form": "밥도줘요???",
          "original_form": "밥도줘요???",
          "speaker_id": "1",
          "time": "20210907 12:38"
        },
