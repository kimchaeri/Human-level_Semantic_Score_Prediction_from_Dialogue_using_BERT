      "utterance": [
        {
          "id": "MDRW2100002582.1.1",
          "form": "중고 거래해요?",
          "original_form": "중고 거래해요?",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002582.1.2",
          "form": "가끔 이용합니다",
          "original_form": "가끔 이용합니다",
          "speaker_id": "2",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002582.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002582.1.4",
          "form": "어떤걸 주로 구매하시나요",
          "original_form": "어떤걸 주로 구매하시나요",
          "speaker_id": "2",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002582.1.5",
          "form": "최근에 제습기요",
          "original_form": "최근에 제습기요",
          "speaker_id": "1",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002582.1.6",
          "form": "어떠셨어요?",
          "original_form": "어떠셨어요?",
          "speaker_id": "2",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002582.1.7",
          "form": "제습기 있음 좋다던데",
          "original_form": "제습기 있음 좋다던데",
          "speaker_id": "2",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002582.1.8",
          "form": "저는 제습기가없거든요",
          "original_form": "저는 제습기가없거든요",
          "speaker_id": "2",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002582.1.9",
          "form": "근데 틀면 더워져요",
          "original_form": "근데 틀면 더워져요",
          "speaker_id": "1",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002582.1.10",
          "form": "더워요? 습하지않아서 시원하다던데",
          "original_form": "더워요? 습하지않아서 시원하다던데",
          "speaker_id": "2",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002582.1.11",
          "form": "건조되니 온도가 올라가더라구요",
          "original_form": "건조되니 온도가 올라가더라구요",
          "speaker_id": "1",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002582.1.12",
          "form": "아 그런 단점이..",
          "original_form": "아 그런 단점이..",
          "speaker_id": "2",
          "time": "20210820 13:49"
        },
        {
          "id": "MDRW2100002582.1.13",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210820 13:49"
        },
        {
          "id": "MDRW2100002582.1.14",
          "form": "하나 사려했는데 고민해봐야겠어요",
          "original_form": "하나 사려했는데 고민해봐야겠어요",
          "speaker_id": "2",
          "time": "20210820 13:49"
        },
        {
          "id": "MDRW2100002582.1.15",
          "form": "물도 빨리찬다던데 진짜 그런가요?",
          "original_form": "물도 빨리찬다던데 진짜 그런가요?",
          "speaker_id": "2",
          "time": "20210820 13:49"
        },
        {
          "id": "MDRW2100002582.1.16",
          "form": "네 습도는 진짜 잡아줘요",
          "original_form": "네 습도는 진짜 잡아줘요",
          "speaker_id": "1",
          "time": "20210820 13:50"
        }
      ]
