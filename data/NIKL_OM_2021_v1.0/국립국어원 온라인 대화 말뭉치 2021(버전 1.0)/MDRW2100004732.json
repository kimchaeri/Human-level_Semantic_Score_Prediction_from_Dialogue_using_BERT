      "utterance": [
        {
          "id": "MDRW2100004732.1.1",
          "form": "털난방석 귀여워요",
          "original_form": "털난방석 귀여워요",
          "speaker_id": "2",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004732.1.2",
          "form": "오늘 폭우가 쏟아져서 자다가 헐레벌떡 창문닫았어요",
          "original_form": "오늘 폭우가 쏟아져서 자다가 헐레벌떡 창문닫았어요",
          "speaker_id": "2",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004732.1.3",
          "form": "헉 저두요 자다가 쏴아 소리가 귀에 투다다다 박히길래 놀랐네요",
          "original_form": "헉 저두요 자다가 쏴아 소리가 귀에 투다다다 박히길래 놀랐네요",
          "speaker_id": "1",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004732.1.4",
          "form": "그리고 너무 추웠어요",
          "original_form": "그리고 너무 추웠어요",
          "speaker_id": "1",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004732.1.5",
          "form": "요즘 날씨가 정말 이상해요",
          "original_form": "요즘 날씨가 정말 이상해요",
          "speaker_id": "1",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004732.1.6",
          "form": "어젠 여름이었는데 오늘은 겨울같아요",
          "original_form": "어젠 여름이었는데 오늘은 겨울같아요",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004732.1.7",
          "form": "그러니까말에요",
          "original_form": "그러니까말에요",
          "speaker_id": "1",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004732.1.8",
          "form": "저번에는 쨍쨍하늘에 천둥이 치더라고요",
          "original_form": "저번에는 쨍쨍하늘에 천둥이 치더라고요",
          "speaker_id": "1",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004732.1.9",
          "form": "헉 맞아요 보라색 천둥",
          "original_form": "헉 맞아요 보라색 천둥",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004732.1.10",
          "form": "색은 못봤는데 보라색이라니.. 지구종말이 머지 않았나봐요",
          "original_form": "색은 못봤는데 보라색이라니.. 지구종말이 머지 않았나봐요",
          "speaker_id": "1",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004732.1.11",
          "form": "종말!!!!! 멋저요 제가 바로 원하는 거예요 사탄님이 강림하신다",
          "original_form": "종말!!!!! 멋저요 제가 바로 원하는 거예요 사탄님이 강림하신다",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004732.1.12",
          "form": "비오는날엔 항상 배달음식 먹고싶어요",
          "original_form": "비오는날엔 항상 배달음식 먹고싶어요",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004732.1.13",
          "form": "정말 인정요 남이해준 음식이 젤맛있어요",
          "original_form": "정말 인정요 남이해준 음식이 젤맛있어요",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004732.1.14",
          "form": "비가 또 왔다가 해가 나네요",
          "original_form": "비가 또 왔다가 해가 나네요",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004732.1.15",
          "form": "태풍도 올 예정이래요",
          "original_form": "태풍도 올 예정이래요",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004732.1.16",
          "form": "으악 아까도 비바람 너무 심했는데",
          "original_form": "으악 아까도 비바람 너무 심했는데",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004732.1.17",
          "form": "태풍까지요??",
          "original_form": "태풍까지요??",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004732.1.18",
          "form": "엄니 퇴근 후에 왔으면",
          "original_form": "엄니 퇴근 후에 왔으면",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004732.1.19",
          "form": "ㅜㅠ 날씨가 정말이지 뱅글뱅글돌아가네요",
          "original_form": "ㅜㅠ 날씨가 정말이지 뱅글뱅글돌아가네요",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004732.1.20",
          "form": "일하고 퇴근하는 가족이있으면 걱정이죠  이번 태풍도",
          "original_form": "일하고 퇴근하는 가족이있으면 걱정이죠  이번 태풍도",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004732.1.21",
          "form": "다른나라로 잘 비껴갔으면 좋겠어요",
          "original_form": "다른나라로 잘 비껴갔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004732.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아 너무 웃겨요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아 너무 웃겨요",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004732.1.23",
          "form": "맞아요 이번에도 태풍흡수 잘 해주길..",
          "original_form": "맞아요 이번에도 태풍흡수 잘 해주길..",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004732.1.24",
          "form": "내일은 비 안온다고 하던데",
          "original_form": "내일은 비 안온다고 하던데",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004732.1.25",
          "form": "빨래해야겠네요",
          "original_form": "빨래해야겠네요",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004732.1.26",
          "form": "빨래타임~~~~ 장마땐 꿉꿉한 냄새나서 싫어요!",
          "original_form": "빨래타임~~~~ 장마땐 꿉꿉한 냄새나서 싫어요!",
          "speaker_id": "1",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004732.1.27",
          "form": "날씨가 빨리 선선한 가을이 되면 좋겠어요",
          "original_form": "날씨가 빨리 선선한 가을이 되면 좋겠어요",
          "speaker_id": "1",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004732.1.28",
          "form": "가을 젤 좋아했는데 고작 일주일밖에 안 돼서 슬퍼요~",
          "original_form": "가을 젤 좋아했는데 고작 일주일밖에 안 돼서 슬퍼요~",
          "speaker_id": "2",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004732.1.29",
          "form": "어떤 계절을 가장 좋아하세요?",
          "original_form": "어떤 계절을 가장 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004732.1.30",
          "form": "저는 여름이 좋아요 색감이 예뻐서 좋은데",
          "original_form": "저는 여름이 좋아요 색감이 예뻐서 좋은데",
          "speaker_id": "1",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004732.1.31",
          "form": "막상 여름되면 그렇게 싫을수가없어요",
          "original_form": "막상 여름되면 그렇게 싫을수가없어요",
          "speaker_id": "1",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004732.1.32",
          "form": "가을짱!",
          "original_form": "가을짱!",
          "speaker_id": "1",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004732.1.33",
          "form": "봄은 먼지랑 꽃가루때문에 목이 맛가요",
          "original_form": "봄은 먼지랑 꽃가루때문에 목이 맛가요",
          "speaker_id": "1",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004732.1.34",
          "form": "가을이 점점줄어드는거같아서 슬퍼요",
          "original_form": "가을이 점점줄어드는거같아서 슬퍼요",
          "speaker_id": "1",
          "time": "20210821 19:02"
        }
      ]
    }
