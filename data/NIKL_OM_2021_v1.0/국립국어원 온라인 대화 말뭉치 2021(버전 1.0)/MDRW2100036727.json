      "utterance": [
        {
          "id": "MDRW2100036727.1.1",
          "form": "저는 설거지가 참 싫더라구요..",
          "original_form": "저는 설거지가 참 싫더라구요..",
          "speaker_id": "2",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100036727.1.2",
          "form": "설거지...너무 싫죠",
          "original_form": "설거지...너무 싫죠",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100036727.1.3",
          "form": "금방금방 또생기고",
          "original_form": "금방금방 또생기고",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100036727.1.4",
          "form": "기름기있는 음식은 어찌나많은지...",
          "original_form": "기름기있는 음식은 어찌나많은지...",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100036727.1.5",
          "form": "맞아요 뜨거운물로 씻어야하는데 저희 온수 고장났어서 꼭 보일러 틀고 해야하거든요ㅠ",
          "original_form": "맞아요 뜨거운물로 씻어야하는데 저희 온수 고장났어서 꼭 보일러 틀고 해야하거든요ㅠ",
          "speaker_id": "2",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100036727.1.6",
          "form": "귀찮고.. 그러더라구요",
          "original_form": "귀찮고.. 그러더라구요",
          "speaker_id": "2",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100036727.1.7",
          "form": "헉 고장나셨구나...진짜 귀찮으시겠다",
          "original_form": "헉 고장나셨구나...진짜 귀찮으시겠다",
          "speaker_id": "1",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100036727.1.8",
          "form": "안그래도 귀찮은일인데...",
          "original_form": "안그래도 귀찮은일인데...",
          "speaker_id": "1",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100036727.1.9",
          "form": "그니까요!!",
          "original_form": "그니까요!!",
          "speaker_id": "2",
          "time": "20210831 10:35"
        },
        {
          "id": "MDRW2100036727.1.10",
          "form": "원래는 뜨거운뮬하면 나왔는데 안그러니 참 안먹을수도 없구요..",
          "original_form": "원래는 뜨거운뮬하면 나왔는데 안그러니 참 안먹을수도 없구요..",
          "speaker_id": "2",
          "time": "20210831 10:36"
        },
        {
          "id": "MDRW2100036727.1.11",
          "form": "name1님은 청소 좋아하세용?",
          "original_form": "&name1&님은 청소 좋아하세용?",
          "speaker_id": "2",
          "time": "20210831 10:36"
        },
        {
          "id": "MDRW2100036727.1.12",
          "form": "그죠 또 한식은 어찌나 또 기름진게 많은지...",
          "original_form": "그죠 또 한식은 어찌나 또 기름진게 많은지...",
          "speaker_id": "1",
          "time": "20210831 10:36"
        },
        {
          "id": "MDRW2100036727.1.13",
          "form": "청소요?좋아하지는 않는데",
          "original_form": "청소요?좋아하지는 않는데",
          "speaker_id": "1",
          "time": "20210831 10:36"
        },
        {
          "id": "MDRW2100036727.1.14",
          "form": "하루에 한번정도 해요",
          "original_form": "하루에 한번정도 해요",
          "speaker_id": "1",
          "time": "20210831 10:36"
        },
        {
          "id": "MDRW2100036727.1.15",
          "form": "그쵸ㅠ 청소기도 돌리구.. 쓸고 닦고",
          "original_form": "그쵸ㅠ 청소기도 돌리구.. 쓸고 닦고",
          "speaker_id": "2",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100036727.1.16",
          "form": "자고 일어나면 먼지가 어디있다 그리 오는건지 싶어오",
          "original_form": "자고 일어나면 먼지가 어디있다 그리 오는건지 싶어오",
          "speaker_id": "2",
          "time": "20210831 10:43"
        },
        {
          "id": "MDRW2100036727.1.17",
          "form": "머리카락은 또 어찌나 떨어지는지...",
          "original_form": "머리카락은 또 어찌나 떨어지는지...",
          "speaker_id": "1",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100036727.1.18",
          "form": "머리카락하고 전쟁이예요",
          "original_form": "머리카락하고 전쟁이예요",
          "speaker_id": "1",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100036727.1.19",
          "form": "어후 맞아요",
          "original_form": "어후 맞아요",
          "speaker_id": "2",
          "time": "20210831 10:47"
        },
        {
          "id": "MDRW2100036727.1.20",
          "form": "길면 하나도 여러개 역할을 하죠..",
          "original_form": "길면 하나도 여러개 역할을 하죠..",
          "speaker_id": "2",
          "time": "20210831 10:47"
        },
        {
          "id": "MDRW2100036727.1.21",
          "form": "저는 욕실청소가 제일싫어요...",
          "original_form": "저는 욕실청소가 제일싫어요...",
          "speaker_id": "1",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100036727.1.22",
          "form": "아.. 그쵸",
          "original_form": "아.. 그쵸",
          "speaker_id": "2",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100036727.1.23",
          "form": "씻고 머리카락이라도 잘 버려줌 좋은데말이죠",
          "original_form": "씻고 머리카락이라도 잘 버려줌 좋은데말이죠",
          "speaker_id": "2",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100036727.1.24",
          "form": "뒷처리는 필수인데!",
          "original_form": "뒷처리는 필수인데!",
          "speaker_id": "1",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100036727.1.25",
          "form": "그니까요!!",
          "original_form": "그니까요!!",
          "speaker_id": "2",
          "time": "20210831 11:04"
        }
      ]
    }
