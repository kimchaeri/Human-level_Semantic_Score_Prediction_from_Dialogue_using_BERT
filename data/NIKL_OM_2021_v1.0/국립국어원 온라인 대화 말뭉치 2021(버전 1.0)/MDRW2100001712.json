        {
          "id": "MDRW2100001712.1.2",
          "form": "강아지요!",
          "original_form": "강아지요!",
          "speaker_id": "2",
          "time": "20210819 18:24"
        },
        {
          "id": "MDRW2100001712.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210819 18:24"
        },
        {
          "id": "MDRW2100001712.1.4",
          "form": "강아지! 저는 고양이에요 몇살이에요?",
          "original_form": "강아지! 저는 고양이에요 몇살이에요?",
          "speaker_id": "1",
          "time": "20210819 18:25"
        },
        {
          "id": "MDRW2100001712.1.5",
          "form": "강아지 3살이요ㅎㅎㅎ",
          "original_form": "강아지 3살이요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001712.1.6",
          "form": "오 고양이 귀엽겠네용",
          "original_form": "오 고양이 귀엽겠네용",
          "speaker_id": "2",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001712.1.7",
          "form": "네 너무 귀여운데~ 불러도 안와요^^",
          "original_form": "네 너무 귀여운데~ 불러도 안와요^^",
          "speaker_id": "1",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001712.1.8",
          "form": "강아지는 다르겟죠?",
          "original_form": "강아지는 다르겟죠?",
          "speaker_id": "1",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001712.1.9",
          "form": "ㅋㅋ보통 강아지들은 잘오던뎈ㅋㅋ 저희집강아지는 의심이많아서 안와요 ㅡㅡㅋ",
          "original_form": "ㅋㅋ보통 강아지들은 잘오던뎈ㅋㅋ 저희집강아지는 의심이많아서 안와요 ㅡㅡㅋ",
          "speaker_id": "2",
          "time": "20210819 18:35"
        },
        {
          "id": "MDRW2100001712.1.10",
          "form": "정말 어이가없는 강아지랍니닼ㅋㅋㅋ",
          "original_form": "정말 어이가없는 강아지랍니닼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:36"
        },
        {
          "id": "MDRW2100001712.1.11",
          "form": "ㅋㅋㅋㅋ아이고ㅋㅋㅋ 근대귀엽네요 강아지 이름이 뭐에요?",
          "original_form": "ㅋㅋㅋㅋ아이고ㅋㅋㅋ 근대귀엽네요 강아지 이름이 뭐에요?",
          "speaker_id": "1",
          "time": "20210819 18:36"
        },
        {
          "id": "MDRW2100001712.1.12",
          "form": "ㅋㅋㅋㅋname3요!",
          "original_form": "ㅋㅋㅋㅋ&name3&요!",
          "speaker_id": "2",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100001712.1.13",
          "form": "고양이는 이름이머에여??",
          "original_form": "고양이는 이름이머에여??",
          "speaker_id": "2",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100001712.1.14",
          "form": "그래서 name2이라고 하셨나보네용ㅋㅋ name4에요",
          "original_form": "그래서 &name2&이라고 하셨나보네용ㅋㅋ &name4&에요",
          "speaker_id": "1",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100001712.1.15",
          "form": "ㅎㅎ네맞아요 name2",
          "original_form": "ㅎㅎ네맞아요 &name2&",
