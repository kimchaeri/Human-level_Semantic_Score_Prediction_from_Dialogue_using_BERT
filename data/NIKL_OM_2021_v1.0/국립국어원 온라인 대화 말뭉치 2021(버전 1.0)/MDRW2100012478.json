      "utterance": [
        {
          "id": "MDRW2100012478.1.1",
          "form": "육출 하셨습니꺼??",
          "original_form": "육출 하셨습니꺼??",
          "speaker_id": "2",
          "time": "20210825 07:57"
        },
        {
          "id": "MDRW2100012478.1.2",
          "form": "아침에 바쁘제이?",
          "original_form": "아침에 바쁘제이?",
          "speaker_id": "2",
          "time": "20210825 07:57"
        },
        {
          "id": "MDRW2100012478.1.3",
          "form": "응 바쁘지 다들 비슷하지뭐ㅋㅋ",
          "original_form": "응 바쁘지 다들 비슷하지뭐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:14"
        },
        {
          "id": "MDRW2100012478.1.4",
          "form": "아침부터 머리감고 화장실청소하고나옴ㅋㅋㅋ",
          "original_form": "아침부터 머리감고 화장실청소하고나옴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:14"
        },
        {
          "id": "MDRW2100012478.1.5",
          "form": "보이면 바로 해야된다 ㅋㅋㅋㅋ안카믄.계속 밀림 ㅋㅋㅋ",
          "original_form": "보이면 바로 해야된다 ㅋㅋㅋㅋ안카믄.계속 밀림 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:15"
        },
        {
          "id": "MDRW2100012478.1.6",
          "form": "우린 둘찌 차타러왔다 ㅋㅋㅋㅋ",
          "original_form": "우린 둘찌 차타러왔다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:15"
        },
        {
          "id": "MDRW2100012478.1.7",
          "form": "둘찌 일찍 차타고가네ㅋㅋ",
          "original_form": "둘찌 일찍 차타고가네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:16"
        },
        {
          "id": "MDRW2100012478.1.8",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210825 08:16"
        },
        {
          "id": "MDRW2100012478.1.9",
          "form": "평소에도 이렇게 일찍가나?",
          "original_form": "평소에도 이렇게 일찍가나?",
          "speaker_id": "1",
          "time": "20210825 08:17"
        },
        {
          "id": "MDRW2100012478.1.10",
          "form": "차는 25분쯤오는데..늘 8시전에 나가있지ㅠㅠ",
          "original_form": "차는 25분쯤오는데..늘 8시전에 나가있지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 08:26"
        },
        {
          "id": "MDRW2100012478.1.11",
          "form": "밖에 있는게 오히려 편함 ㅋㅋㅋㅋ",
          "original_form": "밖에 있는게 오히려 편함 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:26"
        },
        {
          "id": "MDRW2100012478.1.12",
          "form": "밖에 비온다이~",
          "original_form": "밖에 비온다이~",
          "speaker_id": "2",
          "time": "20210825 08:27"
        },
        {
          "id": "MDRW2100012478.1.13",
          "form": "와우ㅋㅋ진짜 빨리가네ㅋㅋ",
          "original_form": "와우ㅋㅋ진짜 빨리가네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:27"
        },
        {
          "id": "MDRW2100012478.1.14",
          "form": "응 조금씩온다더라ㅋㅋ",
          "original_form": "응 조금씩온다더라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:27"
        },
        {
          "id": "MDRW2100012478.1.15",
          "form": "아침 뭐먹고가?",
          "original_form": "아침 뭐먹고가?",
          "speaker_id": "1",
          "time": "20210825 08:27"
        },
        {
          "id": "MDRW2100012478.1.16",
          "form": "두찌는 초코우유 한잔 ㅋㅋㅋㅋㅋ",
          "original_form": "두찌는 초코우유 한잔 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:27"
        },
        {
          "id": "MDRW2100012478.1.17",
          "form": "첫찌만 밥주고 ㅋㅋㅋ",
          "original_form": "첫찌만 밥주고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:28"
        },
        {
          "id": "MDRW2100012478.1.18",
          "form": "가서 죽먹으라함 ㅋㅋㅋㅋ",
          "original_form": "가서 죽먹으라함 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:28"
        },
        {
          "id": "MDRW2100012478.1.19",
          "form": "ㅋㅋㅋ아 웃겨ㅋㅋ",
          "original_form": "ㅋㅋㅋ아 웃겨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:29"
        },
        {
          "id": "MDRW2100012478.1.20",
          "form": "간단하게 주네ㅋㅋ",
          "original_form": "간단하게 주네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:29"
        },
        {
          "id": "MDRW2100012478.1.21",
          "form": "맞다 어린이집은 죽을 주니",
          "original_form": "맞다 어린이집은 죽을 주니",
          "speaker_id": "1",
          "time": "20210825 08:29"
        },
        {
          "id": "MDRW2100012478.1.22",
          "form": "마음이 편하다ㅋㅋ한번씩은 대충먹여도ㅋ",
          "original_form": "마음이 편하다ㅋㅋ한번씩은 대충먹여도ㅋ",
          "speaker_id": "1",
          "time": "20210825 08:29"
        },
        {
          "id": "MDRW2100012478.1.23",
          "form": "첨엔 밥 주다가 서서히 힘들어져서 ㅋㅋㅋㅋ",
          "original_form": "첨엔 밥 주다가 서서히 힘들어져서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:30"
        },
        {
          "id": "MDRW2100012478.1.24",
          "form": "초코우유로 대체 ㅋㅋㅋㅋ",
          "original_form": "초코우유로 대체 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:31"
        },
        {
          "id": "MDRW2100012478.1.25",
          "form": "맞다",
          "original_form": "맞다",
          "speaker_id": "1",
          "time": "20210825 08:31"
        },
        {
          "id": "MDRW2100012478.1.26",
          "form": "이렇게 일찍이면 힘들것같아",
          "original_form": "이렇게 일찍이면 힘들것같아",
          "speaker_id": "1",
          "time": "20210825 08:31"
        },
        {
          "id": "MDRW2100012478.1.27",
          "form": "첫찌 학교가믄  둘다 같이 데리고 나가야하니 ㅋㅋㅋㅋㅋ",
          "original_form": "첫찌 학교가믄  둘다 같이 데리고 나가야하니 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:31"
        },
        {
          "id": "MDRW2100012478.1.28",
          "form": "첫찌도 첨엔 안줬는데 ㅋㅋㅋㅋ",
          "original_form": "첫찌도 첨엔 안줬는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:32"
        },
        {
          "id": "MDRW2100012478.1.29",
          "form": "배고프다고 ㅋㅋㅋㅋ",
          "original_form": "배고프다고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:32"
        },
        {
          "id": "MDRW2100012478.1.30",
          "form": "무정한 엄마 ㅋㅋㅋㅋ",
          "original_form": "무정한 엄마 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:32"
        },
        {
          "id": "MDRW2100012478.1.31",
          "form": "아 웃겨 그래",
          "original_form": "아 웃겨 그래",
          "speaker_id": "1",
          "time": "20210825 08:32"
        },
        {
          "id": "MDRW2100012478.1.32",
          "form": "좀 챙겨주자ㅋㅋ",
          "original_form": "좀 챙겨주자ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:32"
        },
        {
          "id": "MDRW2100012478.1.33",
          "form": "우리는 눈만뜨면 먹을거준다",
          "original_form": "우리는 눈만뜨면 먹을거준다",
          "speaker_id": "1",
          "time": "20210825 08:32"
        },
        {
          "id": "MDRW2100012478.1.34",
          "form": "바로 먹는다",
          "original_form": "바로 먹는다",
          "speaker_id": "1",
          "time": "20210825 08:32"
        },
        {
          "id": "MDRW2100012478.1.35",
          "form": "애들이ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "애들이ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:32"
        },
        {
          "id": "MDRW2100012478.1.36",
          "form": "점심전까지 암것듀 안먹으니 배가 고픈가보더라규~",
          "original_form": "점심전까지 암것듀 안먹으니 배가 고픈가보더라규~",
          "speaker_id": "2",
          "time": "20210825 08:32"
        },
        {
          "id": "MDRW2100012478.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ멋지다~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ멋지다~",
          "speaker_id": "2",
          "time": "20210825 08:33"
        },
        {
          "id": "MDRW2100012478.1.38",
          "form": "난도 잘먹음 진짜 고민해서도 챙겨주겠는데 ㅋㅋㅋ",
          "original_form": "난도 잘먹음 진짜 고민해서도 챙겨주겠는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:33"
        },
        {
          "id": "MDRW2100012478.1.39",
          "form": "맞어. 대충이라도 바로줌ㅋㅋ",
          "original_form": "맞어. 대충이라도 바로줌ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:33"
        },
        {
          "id": "MDRW2100012478.1.40",
          "form": "맨날 물말아달라함 ㅋㅋ",
          "original_form": "맨날 물말아달라함 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:33"
        },
        {
          "id": "MDRW2100012478.1.41",
          "form": "ㅋㅋㅋ아 웃겨ㅋㅋ",
          "original_form": "ㅋㅋㅋ아 웃겨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:33"
        },
        {
          "id": "MDRW2100012478.1.42",
          "form": "내 둘째 준비시키고 톡할게ㅋ",
          "original_form": "내 둘째 준비시키고 톡할게ㅋ",
          "speaker_id": "1",
          "time": "20210825 08:43"
        }
      ]
