        },
        {
          "id": "MDRW2100021261.1.2",
          "form": "네 엄청 좋아해요",
          "original_form": "네 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100021261.1.3",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021261.1.4",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100021261.1.5",
          "form": "최근에 어떤거 사셨어요?",
          "original_form": "최근에 어떤거 사셨어요?",
          "speaker_id": "1",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100021261.1.6",
          "form": "최근에 둘째옷이요",
          "original_form": "최근에 둘째옷이요",
          "speaker_id": "2",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100021261.1.7",
          "form": "뭐사셨어요?",
          "original_form": "뭐사셨어요?",
          "speaker_id": "2",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100021261.1.8",
          "form": "신발이요!",
          "original_form": "신발이요!",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100021261.1.9",
          "form": "오 신발 운동화? 샌들? 슬리퍼?",
          "original_form": "오 신발 운동화? 샌들? 슬리퍼?",
