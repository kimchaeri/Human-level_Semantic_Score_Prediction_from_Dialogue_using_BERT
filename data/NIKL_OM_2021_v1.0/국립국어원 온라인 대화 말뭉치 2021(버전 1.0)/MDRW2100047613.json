      "utterance": [
        {
          "id": "MDRW2100047613.1.1",
          "form": "한참 무르익는 반주하고 계시겠어요~ ㅋㅋ",
          "original_form": "한참 무르익는 반주하고 계시겠어요~ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100047613.1.2",
          "form": "죄다 원플원으로 샀더니 양이 안 줄어요ㅎㅎ",
          "original_form": "죄다 원플원으로 샀더니 양이 안 줄어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100047613.1.3",
          "form": "하하와 김종국 그려진 닭근위볶음 물건이네요",
          "original_form": "하하와 김종국 그려진 닭근위볶음 물건이네요",
          "speaker_id": "1",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100047613.1.4",
          "form": "name2님은 디저트로 뭐 드셨나요",
          "original_form": "&name2&님은 디저트로 뭐 드셨나요",
          "speaker_id": "1",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100047613.1.5",
          "form": "저는 오늘 느즈막히 라떼를 먹었어요! ㅎㅎ",
          "original_form": "저는 오늘 느즈막히 라떼를 먹었어요! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100047613.1.6",
          "form": "아직도 좀 남아서 마시고 있습니당 ㅋㅋㅋ",
          "original_form": "아직도 좀 남아서 마시고 있습니당 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100047613.1.7",
          "form": "얼음도 간간히 씹으며!",
          "original_form": "얼음도 간간히 씹으며!",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100047613.1.8",
          "form": "직접 만들어 드셨나요?",
          "original_form": "직접 만들어 드셨나요?",
          "speaker_id": "1",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100047613.1.9",
          "form": "라떼요? 아니요 ㅎㅎㅎㅎ 배달시켰어요!",
          "original_form": "라떼요? 아니요 ㅎㅎㅎㅎ 배달시켰어요!",
          "speaker_id": "2",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100047613.1.10",
          "form": "ㅎㅎ 역시 배달의 민족ㅎㅎ",
          "original_form": "ㅎㅎ 역시 배달의 민족ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100047613.1.11",
          "form": "슬슬 안주를 먹다보니 저도 흑당밀크티가 당기네요",
          "original_form": "슬슬 안주를 먹다보니 저도 흑당밀크티가 당기네요",
          "speaker_id": "1",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100047613.1.12",
          "form": "근데 일리터 마시면 물배 터질 것 같아요ㅎㅎ",
          "original_form": "근데 일리터 마시면 물배 터질 것 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100047613.1.13",
          "form": "흑당밀크티 ㅋㅋㅋㅋ 1리터짜리는 안됩니다아ㅏㅏ",
          "original_form": "흑당밀크티 ㅋㅋㅋㅋ 1리터짜리는 안됩니다아ㅏㅏ",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100047613.1.14",
          "form": "앗ㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100047613.1.15",
          "form": "하지만 거긴 일리터밖에 안 파는 걸요ㅜ",
          "original_form": "하지만 거긴 일리터밖에 안 파는 걸요ㅜ",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047613.1.16",
          "form": "거기가 젤 제 취향인데",
          "original_form": "거기가 젤 제 취향인데",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047613.1.17",
          "form": "라떼는 무슨 사이즈로 시키셨나요",
          "original_form": "라떼는 무슨 사이즈로 시키셨나요",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047613.1.18",
          "form": "배달음료는 대부분 양이 많던데",
          "original_form": "배달음료는 대부분 양이 많던데",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047613.1.19",
          "form": "저도 여기가 빅사이즈라서 큰거 시켰는데 1리터는 아닌거같아요 ㅎㅎ",
          "original_form": "저도 여기가 빅사이즈라서 큰거 시켰는데 1리터는 아닌거같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100047613.1.20",
          "form": "그냥 메가커피같은 사이즈?",
          "original_form": "그냥 메가커피같은 사이즈?",
          "speaker_id": "2",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100047613.1.21",
          "form": "메가커피도 크잖아요ㅎㅎㅎ",
          "original_form": "메가커피도 크잖아요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100047613.1.22",
          "form": "저는 먹고싶은데 양이 많으면 좀 따라먹고 얼음빼고 뚜껑닫아서 보관해둬요!",
          "original_form": "저는 먹고싶은데 양이 많으면 좀 따라먹고 얼음빼고 뚜껑닫아서 보관해둬요!",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100047613.1.23",
          "form": "좀 느긋하게 마시고 싶으면 전 걍 얼음 빼달라고 그래요",
          "original_form": "좀 느긋하게 마시고 싶으면 전 걍 얼음 빼달라고 그래요",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100047613.1.24",
          "form": "얼음 못 씹는건 좀 아쉽지만ㅜ",
          "original_form": "얼음 못 씹는건 좀 아쉽지만ㅜ",
          "speaker_id": "1",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100047613.1.25",
          "form": "저도 그래야겠어요",
          "original_form": "저도 그래야겠어요",
          "speaker_id": "1",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100047613.1.26",
          "form": "그럼 시키라는 거죠?",
          "original_form": "그럼 시키라는 거죠?",
          "speaker_id": "1",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100047613.1.27",
          "form": "저는 안 마시려고 했는데 조이님이 방법을 알려주시니 어쩔수없이 흑당밀크티 시켜야겠네요",
          "original_form": "저는 안 마시려고 했는데 조이님이 방법을 알려주시니 어쩔수없이 흑당밀크티 시켜야겠네요",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100047613.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100047613.1.29",
          "form": "이렇게되나요 ㅋㅋㅋㅋ",
          "original_form": "이렇게되나요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100047613.1.30",
          "form": "시키세요!!!!",
          "original_form": "시키세요!!!!",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100047613.1.31",
          "form": "흑당밀크티 플랙스 갑시다!",
          "original_form": "흑당밀크티 플랙스 갑시다!",
