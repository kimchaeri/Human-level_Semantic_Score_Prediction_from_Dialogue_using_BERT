      "utterance": [
        {
          "id": "MDRW2100034367.1.1",
          "form": "츄있는 그룹맞나요?",
          "original_form": "츄있는 그룹맞나요?",
          "speaker_id": "1",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100034367.1.2",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100034367.1.3",
          "form": "이달소는 잘  모르시는군요!",
          "original_form": "이달소는 잘  모르시는군요!",
          "speaker_id": "2",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100034367.1.4",
          "form": "네 요새 아이돌을 잘 몰라요 트와이스 이런 그룹만..ㅋㅋㅋ",
          "original_form": "네 요새 아이돌을 잘 몰라요 트와이스 이런 그룹만..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100034367.1.5",
          "form": "이달소 직캠도 보시나요??",
          "original_form": "이달소 직캠도 보시나요??",
          "speaker_id": "1",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100034367.1.6",
          "form": "네 직캠도 보죠!",
          "original_form": "네 직캠도 보죠!",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100034367.1.7",
          "form": "다보진않지만",
          "original_form": "다보진않지만",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100034367.1.8",
          "form": "개인직캠도 보세요??",
          "original_form": "개인직캠도 보세요??",
          "speaker_id": "1",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100034367.1.9",
          "form": "요새는 직캠도 잘나오더라구요",
          "original_form": "요새는 직캠도 잘나오더라구요",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100034367.1.10",
          "form": "네 개인직캠봐요",
          "original_form": "네 개인직캠봐요",
          "speaker_id": "2",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100034367.1.11",
          "form": "맞아요 hdr??로도 나오더라구요",
          "original_form": "맞아요 hdr??로도 나오더라구요",
          "speaker_id": "1",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100034367.1.12",
          "form": "주로 누구보세요 그 중에서??",
          "original_form": "주로 누구보세요 그 중에서??",
          "speaker_id": "1",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100034367.1.13",
          "form": "그니까요ㅋㅋㅋ",
          "original_form": "그니까요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100034367.1.14",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "1",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100034367.1.15",
          "form": "저는 올리비아혜랑",
          "original_form": "저는 올리비아혜랑",
          "speaker_id": "2",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100034367.1.16",
          "form": "이브요!",
          "original_form": "이브요!",
          "speaker_id": "2",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100034367.1.17",
          "form": "아시나요?ㅋㅋ",
          "original_form": "아시나요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100034367.1.18",
          "form": "이달소에서는 츄랑 희진만 알아요 ㅋㅋㅋㅋ희진 이름 맞나요..?",
          "original_form": "이달소에서는 츄랑 희진만 알아요 ㅋㅋㅋㅋ희진 이름 맞나요..?",
          "speaker_id": "1",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100034367.1.19",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100034367.1.20",
          "form": "제일  유명하죠 둘이!",
          "original_form": "제일  유명하죠 둘이!",
