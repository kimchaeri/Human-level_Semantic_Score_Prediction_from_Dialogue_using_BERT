      "utterance": [
        {
          "id": "MDRW2100023161.1.1",
          "form": "레이저는 좀 회복이 오래 걸리더라구요",
          "original_form": "레이저는 좀 회복이 오래 걸리더라구요",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023161.1.2",
          "form": "아 저도 몇 번 여드름흉터 치료 받았다가",
          "original_form": "아 저도 몇 번 여드름흉터 치료 받았다가",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023161.1.3",
          "form": "미미한 효과조차도 못느껴서 중단했어요..",
          "original_form": "미미한 효과조차도 못느껴서 중단했어요..",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023161.1.4",
          "form": "저는 움푹패어있는거였거든요",
          "original_form": "저는 움푹패어있는거였거든요",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023161.1.5",
          "form": "저도 움푹 패어잇는건데",
          "original_form": "저도 움푹 패어잇는건데",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023161.1.6",
          "form": "생각해보니 여드름흉터 아니고 수두흉터였네",
          "original_form": "생각해보니 여드름흉터 아니고 수두흉터였네",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023161.1.7",
          "form": "좀 심한데는 레이저로 깎고",
          "original_form": "좀 심한데는 레이저로 깎고",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023161.1.8",
          "form": "이번에 방학때는 채우는 시술을 햇어요",
          "original_form": "이번에 방학때는 채우는 시술을 햇어요",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023161.1.9",
          "form": "크로스필링? 이런거엿는데",
          "original_form": "크로스필링? 이런거엿는데",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023161.1.10",
          "form": "어머 지금 어떻게 됐어요?",
          "original_form": "어머 지금 어떻게 됐어요?",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023161.1.11",
          "form": "없어졌어요?",
          "original_form": "없어졌어요?",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023161.1.12",
          "form": "지난 8월동안 딱지 엄청 앉앗다가",
          "original_form": "지난 8월동안 딱지 엄청 앉앗다가",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023161.1.13",
          "form": "지금은 딱지는 없어졋고 흉터는 좀 채워진거 같긴한데",
          "original_form": "지금은 딱지는 없어졋고 흉터는 좀 채워진거 같긴한데",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023161.1.14",
          "form": "아..딱지 난 안되겠다.. 딱지만 보면 뜯고싶어져요",
          "original_form": "아..딱지 난 안되겠다.. 딱지만 보면 뜯고싶어져요",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023161.1.15",
          "form": "다 회복되는덴 6개월정도라고해서",
          "original_form": "다 회복되는덴 6개월정도라고해서",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023161.1.16",
          "form": "아직은 좀 불긋불긋해여",
          "original_form": "아직은 좀 불긋불긋해여",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023161.1.17",
          "form": "휴ㅠㅠ 그럼 지금 진행중이시구나",
          "original_form": "휴ㅠㅠ 그럼 지금 진행중이시구나",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023161.1.18",
          "form": "혹시 토닝에 대해서 알려줄 수 있어요?",
          "original_form": "혹시 토닝에 대해서 알려줄 수 있어요?",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023161.1.19",
          "form": "토닝레이저!받아요",
          "original_form": "토닝레이저!받아요",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023161.1.20",
          "form": "까맣게 착색된 것도 여드름흉터 맞죠?",
          "original_form": "까맣게 착색된 것도 여드름흉터 맞죠?",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023161.1.21",
          "form": "네네넹",
          "original_form": "네네넹",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023161.1.22",
          "form": "토닝이랑 화이트닝이랑 다른거에요?",
          "original_form": "토닝이랑 화이트닝이랑 다른거에요?",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023161.1.23",
          "form": "화이트닝은 모르겟는데 토닝은 색소를 좀 연하게 만드는거니깐",
          "original_form": "화이트닝은 모르겟는데 토닝은 색소를 좀 연하게 만드는거니깐",
          "speaker_id": "2",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100023161.1.24",
          "form": "피부톤 자체가 밝아지는게 아니라 점들이 희미해지는 거 아닐까요",
          "original_form": "피부톤 자체가 밝아지는게 아니라 점들이 희미해지는 거 아닐까요",
          "speaker_id": "2",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100023161.1.25",
          "form": "그거라도 하고 싶네요 ㅜㅜㅜ",
          "original_form": "그거라도 하고 싶네요 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023161.1.26",
          "form": "그러면 피부과 고르는 거 중요해요?",
          "original_form": "그러면 피부과 고르는 거 중요해요?",
          "speaker_id": "1",
          "time": "20210829 22:24"
        }
      ]
    }
