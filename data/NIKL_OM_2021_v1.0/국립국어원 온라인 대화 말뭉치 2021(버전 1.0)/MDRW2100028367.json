      "utterance": [
        {
          "id": "MDRW2100028367.1.1",
          "form": "혹시 펜트하우스 봤어요?",
          "original_form": "혹시 펜트하우스 봤어요?",
          "speaker_id": "2",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100028367.1.2",
          "form": "아니요 근데 워낙 인기라",
          "original_form": "아니요 근데 워낙 인기라",
          "speaker_id": "1",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100028367.1.3",
          "form": "그것도 정말 극본이 엉망이에요",
          "original_form": "그것도 정말 극본이 엉망이에요",
          "speaker_id": "2",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100028367.1.4",
          "form": "대충 신문 기사 보고 줄거리만 알아요",
          "original_form": "대충 신문 기사 보고 줄거리만 알아요",
          "speaker_id": "1",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100028367.1.5",
          "form": "시즌 거듭할수록",
          "original_form": "시즌 거듭할수록",
          "speaker_id": "1",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100028367.1.6",
          "form": "막장이라고 들었어요",
          "original_form": "막장이라고 들었어요",
          "speaker_id": "1",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100028367.1.7",
          "form": "TV 프로그램들도 조금 작품성 있게 쓰면 좋을 텐데",
          "original_form": "TV 프로그램들도 조금 작품성 있게 쓰면 좋을 텐데",
          "speaker_id": "2",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100028367.1.8",
          "form": "자극적이어야 인기 있으니까요",
          "original_form": "자극적이어야 인기 있으니까요",
          "speaker_id": "1",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100028367.1.9",
          "form": "그래도 펜트하우스는 너무 심했어요",
          "original_form": "그래도 펜트하우스는 너무 심했어요",
          "speaker_id": "2",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100028367.1.10",
          "form": "안 그래도 비판 기사가 많이 나오더라고요",
          "original_form": "안 그래도 비판 기사가 많이 나오더라고요",
          "speaker_id": "1",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100028367.1.11",
          "form": "예전에 아내의 유혹에서 점 찍고 돌아오는 건 양반 이었어",
          "original_form": "예전에 아내의 유혹에서 점 찍고 돌아오는 건 양반 이었어",
          "speaker_id": "2",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100028367.1.12",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100028367.1.13",
          "form": "정말 개연성 없이 자극적이기만하니",
          "original_form": "정말 개연성 없이 자극적이기만하니",
          "speaker_id": "2",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100028367.1.14",
          "form": "아 그게 같은 작가 거예요?",
          "original_form": "아 그게 같은 작가 거예요?",
          "speaker_id": "1",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100028367.1.15",
          "form": "내가 알기로는 그래요",
          "original_form": "내가 알기로는 그래요",
          "speaker_id": "2",
          "time": "20210902 19:26"
