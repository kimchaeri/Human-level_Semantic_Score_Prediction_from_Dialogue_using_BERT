      "utterance": [
        {
          "id": "MDRW2100045643.1.1",
          "form": "아이스홍시요?",
          "original_form": "아이스홍시요?",
          "speaker_id": "2",
          "time": "20210908 04:37"
        },
        {
          "id": "MDRW2100045643.1.2",
          "form": "넵 잘 얼려둔 홍시가 함께",
          "original_form": "넵 잘 얼려둔 홍시가 함께",
          "speaker_id": "1",
          "time": "20210908 04:38"
        },
        {
          "id": "MDRW2100045643.1.3",
          "form": "적당히 녹여서 먹으면 맛있어요!",
          "original_form": "적당히 녹여서 먹으면 맛있어요!",
          "speaker_id": "1",
          "time": "20210908 04:38"
        },
        {
          "id": "MDRW2100045643.1.4",
          "form": "홍시도 주는지는 몰랐어요",
          "original_form": "홍시도 주는지는 몰랐어요",
          "speaker_id": "2",
          "time": "20210908 04:38"
        },
        {
          "id": "MDRW2100045643.1.5",
          "form": "확실히 비싼건 다른가봐요",
          "original_form": "확실히 비싼건 다른가봐요",
          "speaker_id": "1",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045643.1.6",
          "form": "그 밖에 불고기반찬 데리야끼 치킨 비스무리한거 메추리알 같은 반찬도 기억에 남네요!",
          "original_form": "그 밖에 불고기반찬 데리야끼 치킨 비스무리한거 메추리알 같은 반찬도 기억에 남네요!",
          "speaker_id": "1",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045643.1.7",
          "form": "다양하네요!",
          "original_form": "다양하네요!",
          "speaker_id": "2",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045643.1.8",
          "form": "어른들 드려도 괜찮겠어요",
          "original_form": "어른들 드려도 괜찮겠어요",
          "speaker_id": "2",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045643.1.9",
          "form": "어른들도 잘 드시더라구요!",
          "original_form": "어른들도 잘 드시더라구요!",
          "speaker_id": "1",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045643.1.10",
          "form": "고기반찬을 많이 넣어서 젊은이의 입맛도 잡았지만",
          "original_form": "고기반찬을 많이 넣어서 젊은이의 입맛도 잡았지만",
          "speaker_id": "1",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045643.1.11",
          "form": "젓갈같은 어른들이 좋아하실 만한(편견) 반찬도 많았거든요!",
          "original_form": "젓갈같은 어른들이 좋아하실 만한(편견) 반찬도 많았거든요!",
          "speaker_id": "1",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045643.1.12",
          "form": "오 좋네요!ㅋㅋㅋㅋ",
          "original_form": "오 좋네요!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:41"
        },
        {
          "id": "MDRW2100045643.1.13",
          "form": "저녁은 요?",
          "original_form": "저녁은 요?",
          "speaker_id": "2",
          "time": "20210908 04:41"
        },
        {
          "id": "MDRW2100045643.1.14",
          "form": "저녁은 뚜레쥬르 샌드위치요!",
          "original_form": "저녁은 뚜레쥬르 샌드위치요!",
          "speaker_id": "1",
          "time": "20210908 04:41"
        },
        {
          "id": "MDRW2100045643.1.15",
          "form": "고민하다가 불고기 맛으로 골랐어요ㅋㅋㅋㅋㅋ",
          "original_form": "고민하다가 불고기 맛으로 골랐어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:41"
        },
        {
          "id": "MDRW2100045643.1.16",
          "form": "로제치킨이 맛있어보이긴 했는데 점심에 본도시락을 거하게 먹고 먹기엔 칼로리가 쎄더라구요...",
          "original_form": "로제치킨이 맛있어보이긴 했는데 점심에 본도시락을 거하게 먹고 먹기엔 칼로리가 쎄더라구요...",
          "speaker_id": "1",
          "time": "20210908 04:42"
        },
        {
          "id": "MDRW2100045643.1.17",
          "form": "뚜레쥬르 샌드위치... 다 맛있긴 해요",
          "original_form": "뚜레쥬르 샌드위치... 다 맛있긴 해요",
          "speaker_id": "1",
          "time": "20210908 04:43"
        },
        {
          "id": "MDRW2100045643.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:44"
        },
        {
          "id": "MDRW2100045643.1.19",
          "form": "샌드위치 먹고싶어요",
          "original_form": "샌드위치 먹고싶어요",
          "speaker_id": "2",
          "time": "20210908 04:44"
        },
        {
          "id": "MDRW2100045643.1.20",
          "form": "게다가 샌드위치사면 아메리카노도 무료제공이라",
          "original_form": "게다가 샌드위치사면 아메리카노도 무료제공이라",
          "speaker_id": "1",
          "time": "20210908 04:44"
        },
        {
          "id": "MDRW2100045643.1.21",
          "form": "아침으로 딱 좋겠어요ㅋㅋㅋㅋㅋ",
          "original_form": "아침으로 딱 좋겠어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:44"
        },
        {
          "id": "MDRW2100045643.1.22",
          "form": "헐 몰랐어요",
          "original_form": "헐 몰랐어요",
          "speaker_id": "2",
          "time": "20210908 04:45"
        },
        {
          "id": "MDRW2100045643.1.23",
          "form": "뚜레주르 좋은 기획하네용",
          "original_form": "뚜레주르 좋은 기획하네용",
          "speaker_id": "2",
          "time": "20210908 04:45"
        },
        {
          "id": "MDRW2100045643.1.24",
          "form": "제가 가는 지점만 그런건지도 모르겠어요",
          "original_form": "제가 가는 지점만 그런건지도 모르겠어요",
          "speaker_id": "1",
          "time": "20210908 04:45"
        },
        {
          "id": "MDRW2100045643.1.25",
          "form": "어차피 제근처엔 없어요",
          "original_form": "어차피 제근처엔 없어요",
          "speaker_id": "2",
          "time": "20210908 04:45"
        },
        {
          "id": "MDRW2100045643.1.26",
          "form": "그런데 딱 이득보는 것 같은 마케팅이라 윈윈인 것 같아요",
          "original_form": "그런데 딱 이득보는 것 같은 마케팅이라 윈윈인 것 같아요",
          "speaker_id": "1",
          "time": "20210908 04:46"
        },
        {
          "id": "MDRW2100045643.1.27",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210908 04:46"
        }
      ]
    }
