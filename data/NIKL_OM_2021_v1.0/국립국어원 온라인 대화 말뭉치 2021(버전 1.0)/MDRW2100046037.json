        },
        {
          "id": "MDRW2100046037.1.2",
          "form": "오 눈썹 잘 그리시나봐요",
          "original_form": "오 눈썹 잘 그리시나봐요",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046037.1.3",
          "form": "잘못그려성ㅎㅎ..받았어요",
          "original_form": "잘못그려성ㅎㅎ..받았어요",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046037.1.4",
          "form": "아 문신은 안 하시고요?",
          "original_form": "아 문신은 안 하시고요?",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046037.1.5",
          "form": "넹넹ㅎㅎ",
          "original_form": "넹넹ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046037.1.6",
          "form": "저는 잘 못그려서 문신 고민하고 있어요",
          "original_form": "저는 잘 못그려서 문신 고민하고 있어요",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046037.1.7",
          "form": "문신도이쁘더라고요",
          "original_form": "문신도이쁘더라고요",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046037.1.8",
          "form": "그쵸 근데 근데 어느 가게가 잘하는지 모르겠어서 계속 유보 중이에요",
          "original_form": "그쵸 근데 근데 어느 가게가 잘하는지 모르겠어서 계속 유보 중이에요",
          "speaker_id": "2",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046037.1.9",
          "form": "아하주변지인들은어떤가요",
          "original_form": "아하주변지인들은어떤가요",
          "speaker_id": "1",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046037.1.10",
          "form": "저는잘된애보면혹해요",
          "original_form": "저는잘된애보면혹해요",
          "speaker_id": "1",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046037.1.11",
          "form": "주변에 문신운 없규 속눈썹 연장한 애는 봤어요",
          "original_form": "주변에 문신운 없규 속눈썹 연장한 애는 봤어요",
          "speaker_id": "2",
          "time": "20210908 10:03"
        },
        {
          "id": "MDRW2100046037.1.12",
          "form": "저도그거봤는데이쁘더라고오ㅡ",
          "original_form": "저도그거봤는데이쁘더라고오ㅡ",
          "speaker_id": "1",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046037.1.13",
          "form": "생각보다 자연스럽더라구요 저도 혹했잖아요ㅋㅋㅋㅋ",
          "original_form": "생각보다 자연스럽더라구요 저도 혹했잖아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046037.1.14",
          "form": "ㅎㅎㅎ조만간할거같아요",
          "original_form": "ㅎㅎㅎ조만간할거같아요",
