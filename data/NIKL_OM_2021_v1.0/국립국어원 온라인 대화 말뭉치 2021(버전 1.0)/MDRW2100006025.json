      "utterance": [
        {
          "id": "MDRW2100006025.1.1",
          "form": "백신맞으셨나요",
          "original_form": "백신맞으셨나요",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100006025.1.2",
          "form": "저는 우선접종자였는데",
          "original_form": "저는 우선접종자였는데",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100006025.1.3",
          "form": "미뤘다가 예약이 안돼서",
          "original_form": "미뤘다가 예약이 안돼서",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100006025.1.4",
          "form": "아직 못맞았어요",
          "original_form": "아직 못맞았어요",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100006025.1.5",
          "form": "백신 맞으셨어요?",
          "original_form": "백신 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100006025.1.6",
          "form": "네 저는 맞았습니다 그럼 이제 예약순번이 언제 오는건가요?",
          "original_form": "네 저는 맞았습니다 그럼 이제 예약순번이 언제 오는건가요?",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100006025.1.7",
          "form": "음 예약하려고 들어가면",
          "original_form": "음 예약하려고 들어가면",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100006025.1.8",
          "form": "대상자가 아니라 뜨더라구요",
          "original_form": "대상자가 아니라 뜨더라구요",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100006025.1.9",
          "form": "그래서 잔여백신잡는거아니면",
          "original_form": "그래서 잔여백신잡는거아니면",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100006025.1.10",
          "form": "아직 안되는거같아요",
          "original_form": "아직 안되는거같아요",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100006025.1.11",
          "form": "그러시군요 선호하시는 백신 종류 있나요",
          "original_form": "그러시군요 선호하시는 백신 종류 있나요",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100006025.1.12",
          "form": "음 저는 모더나가 나을거같아요",
          "original_form": "음 저는 모더나가 나을거같아요",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100006025.1.13",
          "form": "근데 워낙 요즘수량이 부족해서",
          "original_form": "근데 워낙 요즘수량이 부족해서",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100006025.1.14",
          "form": "주변은 화이자 대부분 맞더라구요",
          "original_form": "주변은 화이자 대부분 맞더라구요",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100006025.1.15",
          "form": "그러게요 주변도 화이자가 대부분인데 선호는 모더나가 높은 것 같았어요",
          "original_form": "그러게요 주변도 화이자가 대부분인데 선호는 모더나가 높은 것 같았어요",
          "speaker_id": "1",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100006025.1.16",
          "form": "모더나가 좀더 델타변이에 강하다고 하네요",
          "original_form": "모더나가 좀더 델타변이에 강하다고 하네요",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100006025.1.17",
          "form": "근데 중소라서 그럱",
          "original_form": "근데 중소라서 그럱",
          "speaker_id": "2",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100006025.1.18",
          "form": "확실히 수급에 문제가 있는거같아요",
          "original_form": "확실히 수급에 문제가 있는거같아요",
          "speaker_id": "2",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100006025.1.19",
          "form": "주변에 모더나 맞은 사람 딱 한명 봤어요",
          "original_form": "주변에 모더나 맞은 사람 딱 한명 봤어요",
          "speaker_id": "1",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100006025.1.20",
          "form": "전 저희아빠만 봤어요",
          "original_form": "전 저희아빠만 봤어요",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100006025.1.21",
          "form": "동네병원에 전화했더니",
          "original_form": "동네병원에 전화했더니",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100006025.1.22",
          "form": "나이많다고 모더나줬다네요",
          "original_form": "나이많다고 모더나줬다네요",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100006025.1.23",
          "form": "같이온 20대는 화이자주고",
          "original_form": "같이온 20대는 화이자주고",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100006025.1.24",
          "form": "아 연령대별로 또 다르군요 \n이번에 순차공급되면 물량이 더 풀리겠죠...",
          "original_form": "아 연령대별로 또 다르군요 \n이번에 순차공급되면 물량이 더 풀리겠죠...",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100006025.1.25",
          "form": "네 제때공급돼서",
          "original_form": "네 제때공급돼서",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100006025.1.26",
          "form": "6주로 미뤄진것도",
          "original_form": "6주로 미뤄진것도",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100006025.1.27",
          "form": "빨리 정상화되길",
          "original_form": "빨리 정상화되길",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100006025.1.28",
          "form": "접종 간격이 6주로 미뤄진게 공급차질때문이라면서요?",
          "original_form": "접종 간격이 6주로 미뤄진게 공급차질때문이라면서요?",
          "speaker_id": "1",
          "time": "20210907 19:47"
        }
      ]
