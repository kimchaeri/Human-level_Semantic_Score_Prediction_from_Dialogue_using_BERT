      "utterance": [
        {
          "id": "MDRW2100037347.1.1",
          "form": "이게 그나마 연예인과 비슷한",
          "original_form": "이게 그나마 연예인과 비슷한",
          "speaker_id": "1",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100037347.1.2",
          "form": "오 연예인 맞네요!!!",
          "original_form": "오 연예인 맞네요!!!",
          "speaker_id": "1",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100037347.1.3",
          "form": "수영장 샤워실에서 연예인 본적 있어요 ㅋㅋ",
          "original_form": "수영장 샤워실에서 연예인 본적 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037347.1.4",
          "form": "우와 ㅋㅋㅋㅋㅋ",
          "original_form": "우와 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037347.1.5",
          "form": "연예인도 수영장 샤워실을 이용하는군요 ㅋㅋㅋ",
          "original_form": "연예인도 수영장 샤워실을 이용하는군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100037347.1.6",
          "form": "네 좀비싼 사설 수영장이었거든요",
          "original_form": "네 좀비싼 사설 수영장이었거든요",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100037347.1.7",
          "form": "해 주신다는 연예인 이야기는 뭐예요?",
          "original_form": "해 주신다는 연예인 이야기는 뭐예요?",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037347.1.8",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037347.1.9",
          "form": "저는 연예인들 방송 이미지가",
          "original_form": "저는 연예인들 방송 이미지가",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037347.1.10",
          "form": "가짜라는걸 잘 안답니다!!!",
          "original_form": "가짜라는걸 잘 안답니다!!!",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037347.1.11",
          "form": "그냥 누구라고 말 안 하고 사건위주로 얘기 하시면 될 거 같아요",
          "original_form": "그냥 누구라고 말 안 하고 사건위주로 얘기 하시면 될 거 같아요",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037347.1.12",
          "form": "아 그렇군요 그럼 다음 페이지에서 말하겠습니다 ㅎㅎㅎ",
          "original_form": "아 그렇군요 그럼 다음 페이지에서 말하겠습니다 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037347.1.13",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037347.1.14",
          "form": "어떤 연예인이랑",
          "original_form": "어떤 연예인이랑",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037347.1.15",
          "form": "같은 고등학교였는데",
          "original_form": "같은 고등학교였는데",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037347.1.16",
          "form": "저희 학교가 연예활동을 못하게해서",
          "original_form": "저희 학교가 연예활동을 못하게해서",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037347.1.17",
          "form": "네 실제로 같이 다니셨나 봐요",
          "original_form": "네 실제로 같이 다니셨나 봐요",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037347.1.18",
          "form": "그 연예인은 2학년 도중에 예술고등학교로 전학을 갔거든요",
          "original_form": "그 연예인은 2학년 도중에 예술고등학교로 전학을 갔거든요",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037347.1.19",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037347.1.20",
          "form": "근데 뭐 별거는 아니지만...",
          "original_form": "근데 뭐 별거는 아니지만...",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037347.1.21",
          "form": "저희 1학년 때",
          "original_form": "저희 1학년 때",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037347.1.22",
          "form": "그 연예인이 사진부 동아리였는데",
          "original_form": "그 연예인이 사진부 동아리였는데",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037347.1.23",
          "form": "양아치 언니들이 연예인 팔짱 양옆으로 끼고",
          "original_form": "양아치 언니들이 연예인 팔짱 양옆으로 끼고",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037347.1.24",
          "form": "1학년 교실 하나하나 순회하면서",
          "original_form": "1학년 교실 하나하나 순회하면서",
          "speaker_id": "1",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100037347.1.25",
          "form": "\"사진부 오면 이 오빠 있어~\"",
          "original_form": "\"사진부 오면 이 오빠 있어~\"",
          "speaker_id": "1",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100037347.1.26",
          "form": "이러더라구요",
          "original_form": "이러더라구요",
          "speaker_id": "1",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100037347.1.27",
          "form": "그러면 그 연예인은 씨익 웃고",
          "original_form": "그러면 그 연예인은 씨익 웃고",
          "speaker_id": "1",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100037347.1.28",
          "form": "자기 눈에 예쁜 여자애는 머리 만지고 가고 ㅋㅋㅋㅋ;;",
          "original_form": "자기 눈에 예쁜 여자애는 머리 만지고 가고 ㅋㅋㅋㅋ;;",
          "speaker_id": "1",
          "time": "20210831 22:55"
        }
      ]
