          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005369.1.9",
          "form": "진짜 막",
          "original_form": "진짜 막",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005369.1.10",
          "form": "대진대 가면 엄마한테 대진대",
