      "utterance": [
        {
          "id": "MDRW2100034612.1.1",
          "form": "컴백이라서 엄청 투자한거 같은데 좋은지 모르겠다",
          "original_form": "컴백이라서 엄청 투자한거 같은데 좋은지 모르겠다",
          "speaker_id": "2",
          "time": "20210828 16:19"
        },
        {
          "id": "MDRW2100034612.1.2",
          "form": "완전 다 라이브는 아니네",
          "original_form": "완전 다 라이브는 아니네",
          "speaker_id": "2",
          "time": "20210828 16:20"
        },
        {
          "id": "MDRW2100034612.1.3",
          "form": "요새 다 그렇드라. 그래도 하는게 어디임",
          "original_form": "요새 다 그렇드라. 그래도 하는게 어디임",
          "speaker_id": "1",
          "time": "20210828 16:20"
        },
        {
          "id": "MDRW2100034612.1.4",
          "form": "마스크 안들고 있는 시간이 다 많은듯하다",
          "original_form": "마스크 안들고 있는 시간이 다 많은듯하다",
          "speaker_id": "2",
          "time": "20210828 16:21"
        },
        {
          "id": "MDRW2100034612.1.5",
          "form": "마스크 아니고 마이크 ㅋㅋ",
          "original_form": "마스크 아니고 마이크 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:21"
        },
        {
          "id": "MDRW2100034612.1.6",
          "form": "ㅋㅋ ㅋ 끝으로 갈수록 아는 사람이네",
          "original_form": "ㅋㅋ ㅋ 끝으로 갈수록 아는 사람이네",
          "speaker_id": "1",
          "time": "20210828 16:21"
        },
        {
          "id": "MDRW2100034612.1.7",
          "form": "아까 나온 애들아니가?",
          "original_form": "아까 나온 애들아니가?",
          "speaker_id": "2",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100034612.1.8",
          "form": "맞다. 얘네 이름은 들어봄",
          "original_form": "맞다. 얘네 이름은 들어봄",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100034612.1.9",
          "form": "난 애들도 첨",
          "original_form": "난 애들도 첨",
          "speaker_id": "2",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100034612.1.10",
          "form": "이름만 들어보고 노래는 모름ㅋ",
          "original_form": "이름만 들어보고 노래는 모름ㅋ",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100034612.1.11",
          "form": "다 모르겠다 나오는 애들",
          "original_form": "다 모르겠다 나오는 애들",
          "speaker_id": "2",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100034612.1.12",
          "form": "화장을 떡칠했네",
          "original_form": "화장을 떡칠했네",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100034612.1.13",
          "form": "ㅋㅋ 그것도 보이더나",
          "original_form": "ㅋㅋ 그것도 보이더나",
          "speaker_id": "2",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100034612.1.14",
          "form": "얼굴이 허옇다",
          "original_form": "얼굴이 허옇다",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100034612.1.15",
          "form": "눈화장을 다 엄청 진하게 했네",
          "original_form": "눈화장을 다 엄청 진하게 했네",
