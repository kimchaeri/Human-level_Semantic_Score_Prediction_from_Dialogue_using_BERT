      "utterance": [
        {
          "id": "MDRW2100007395.1.1",
          "form": "헉..",
          "original_form": "헉..",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007395.1.2",
          "form": "치우기 힘들었겠어요",
          "original_form": "치우기 힘들었겠어요",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007395.1.3",
          "form": "맞슴다 맞슴다",
          "original_form": "맞슴다 맞슴다",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007395.1.4",
          "form": "원래 닭은 모래로 씻겨야하는데",
          "original_form": "원래 닭은 모래로 씻겨야하는데",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007395.1.5",
          "form": "제가 맨날 코박고 있어서",
          "original_form": "제가 맨날 코박고 있어서",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007395.1.6",
          "form": "모래요?",
          "original_form": "모래요?",
          "speaker_id": "1",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007395.1.7",
          "form": "아버지가 name3이를 엘라스틴으로 씻겼거든요",
          "original_form": "아버지가 &name3&이를 엘라스틴으로 씻겼거든요",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007395.1.8",
          "form": "ㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007395.1.9",
          "form": "바로 말려주고",
          "original_form": "바로 말려주고",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007395.1.10",
          "form": "그래서 우리 name3이 지나가면",
          "original_form": "그래서 우리 &name3&이 지나가면",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007395.1.11",
          "form": "샴푸로ㅋㅋㅋ 씻어도 되나?",
          "original_form": "샴푸로ㅋㅋㅋ 씻어도 되나?",
          "speaker_id": "1",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007395.1.12",
          "form": "너무 재밌다",
          "original_form": "너무 재밌다",
          "speaker_id": "1",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007395.1.13",
          "form": "엘라스틴 냄새나고 그랬는데",
          "original_form": "엘라스틴 냄새나고 그랬는데",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007395.1.14",
          "form": "추억에 젖네요",
          "original_form": "추억에 젖네요",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007395.1.15",
          "form": "광고하나 찍었어야했는데",
          "original_form": "광고하나 찍었어야했는데",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007395.1.16",
          "form": "엘라스틴을 써요~",
          "original_form": "엘라스틴을 써요~",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007395.1.17",
          "form": "아쉽다",
          "original_form": "아쉽다",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007395.1.18",
          "form": "닭은 원래 모래로 씻겨소",
          "original_form": "닭은 원래 모래로 씻겨소",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007395.1.19",
          "form": "누래지거든요",
          "original_form": "누래지거든요",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007395.1.20",
          "form": "하지만 우리 name3이는 표백제였답니다",
          "original_form": "하지만 우리 &name3&이는 표백제였답니다",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007395.1.21",
          "form": "군계일학이라고",
          "original_form": "군계일학이라고",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007395.1.22",
          "form": "수컷들 사이에서 알아볼 수 있었답니다",
          "original_form": "수컷들 사이에서 알아볼 수 있었답니다",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007395.1.23",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007395.1.24",
          "form": "빛이난다",
          "original_form": "빛이난다",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007395.1.25",
          "form": "빛이나",
          "original_form": "빛이나",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007395.1.26",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007395.1.27",
          "form": "듣기만해도 독보적이네요",
          "original_form": "듣기만해도 독보적이네요",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007395.1.28",
          "form": "오랜만에 name3이 자랑해서 기분이 좋내요",
          "original_form": "오랜만에 &name3&이 자랑해서 기분이 좋내요",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007395.1.29",
          "form": "감자 자랑도 해주세요",
          "original_form": "감자 자랑도 해주세요",
          "speaker_id": "2",
