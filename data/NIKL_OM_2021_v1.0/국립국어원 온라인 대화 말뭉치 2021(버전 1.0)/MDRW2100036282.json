      "utterance": [
        {
          "id": "MDRW2100036282.1.1",
          "form": "일일드라마 보세요?",
          "original_form": "일일드라마 보세요?",
          "speaker_id": "2",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100036282.1.2",
          "form": "저 마지막 드라마가 멜로가체질입니다,,",
          "original_form": "저 마지막 드라마가 멜로가체질입니다,,",
          "speaker_id": "1",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100036282.1.3",
          "form": "이 드라마 보셨나요",
          "original_form": "이 드라마 보셨나요",
          "speaker_id": "1",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100036282.1.4",
          "form": "앗 노래만 기억이 나욥ㅠ",
          "original_form": "앗 노래만 기억이 나욥ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100036282.1.5",
          "form": "음 그럼 최신영화는",
          "original_form": "음 그럼 최신영화는",
          "speaker_id": "1",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100036282.1.6",
          "form": "어떤거보셨어요?!",
          "original_form": "어떤거보셨어요?!",
          "speaker_id": "1",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100036282.1.7",
          "form": "재미없는줄알고 안봤는데 뒤쪽에 좀 봤는데 재밌더라구요",
          "original_form": "재미없는줄알고 안봤는데 뒤쪽에 좀 봤는데 재밌더라구요",
          "speaker_id": "2",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100036282.1.8",
          "form": "잔잔바리로 재밌었어요ㅋㅋ",
          "original_form": "잔잔바리로 재밌었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100036282.1.9",
          "form": "제 최신은 영화관 기준 아이언맨 이에욬ㅋㅋㅋ",
          "original_form": "제 최신은 영화관 기준 아이언맨 이에욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:00"
        },
        {
          "id": "MDRW2100036282.1.10",
          "form": "그니까용!",
          "original_form": "그니까용!",
          "speaker_id": "2",
          "time": "20210830 21:00"
        },
        {
          "id": "MDRW2100036282.1.11",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100036282.1.12",
          "form": "ㅋㅋㅋㄱㄱ아 저는 영화관가서 안본지 오래됐어요",
          "original_form": "ㅋㅋㅋㄱㄱ아 저는 영화관가서 안본지 오래됐어요",
          "speaker_id": "1",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100036282.1.13",
          "form": "크루엘라봤어요",
          "original_form": "크루엘라봤어요",
          "speaker_id": "1",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100036282.1.14",
          "form": "네이버 시리즈온으로",
          "original_form": "네이버 시리즈온으로",
          "speaker_id": "1",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100036282.1.15",
          "form": "오! 저도 그거 보려했는데 쿠폰인거 모르거 다른거로 날려버렸어요ㅜㅜㅠ",
          "original_form": "오! 저도 그거 보려했는데 쿠폰인거 모르거 다른거로 날려버렸어요ㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100036282.1.16",
          "form": "재밌으셨나요?",
          "original_form": "재밌으셨나요?",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100036282.1.17",
          "form": "네 재밌더라고요!",
          "original_form": "네 재밌더라고요!",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100036282.1.18",
          "form": "모가디슈도 재밌다던데",
          "original_form": "모가디슈도 재밌다던데",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100036282.1.19",
          "form": "플랫폼뜨면 볼것같아요ㅋㅋ",
          "original_form": "플랫폼뜨면 볼것같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100036282.1.20",
          "form": "오! 그거 저도 추천받았어요!!",
          "original_form": "오! 그거 저도 추천받았어요!!",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036282.1.21",
          "form": "블랙위도우도",
          "original_form": "블랙위도우도",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036282.1.22",
          "form": "다운은받아놨는데",
          "original_form": "다운은받아놨는데",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036282.1.23",
          "form": "아직안봤어요,,",
          "original_form": "아직안봤어요,,",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036282.1.24",
          "form": "그것도 순위 높던데",
          "original_form": "그것도 순위 높던데",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100036282.1.25",
          "form": "갑자기 볼게 막 넘치고 있어요ㅋㅋ",
          "original_form": "갑자기 볼게 막 넘치고 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100036282.1.26",
          "form": "근데 맨날 유튜브만 보다보니 긴거보려면",
          "original_form": "근데 맨날 유튜브만 보다보니 긴거보려면",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036282.1.27",
          "form": "마음의준비가 필요하네요",
          "original_form": "마음의준비가 필요하네요",
          "speaker_id": "1",
          "time": "20210830 21:47"
        }
      ]
    }
