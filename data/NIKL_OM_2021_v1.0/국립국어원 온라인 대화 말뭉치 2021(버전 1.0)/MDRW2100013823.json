      "utterance": [
        {
          "id": "MDRW2100013823.1.1",
          "form": "경주 놀러 가구싶다",
          "original_form": "경주 놀러 가구싶다",
          "speaker_id": "1",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013823.1.2",
          "form": "갑자기 경주는 왜",
          "original_form": "갑자기 경주는 왜",
          "speaker_id": "2",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013823.1.3",
          "form": "구냥 ㅋㅋ 저번에 갔을때 넘 좋았다",
          "original_form": "구냥 ㅋㅋ 저번에 갔을때 넘 좋았다",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013823.1.4",
          "form": "그랬나",
          "original_form": "그랬나",
          "speaker_id": "2",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013823.1.5",
          "form": "웅ㅋ 코로나 땜에 ㅠ가기 좀그렇지만",
          "original_form": "웅ㅋ 코로나 땜에 ㅠ가기 좀그렇지만",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013823.1.6",
          "form": "마스크 잘 쓰고 있음 괜찮다",
          "original_form": "마스크 잘 쓰고 있음 괜찮다",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013823.1.7",
          "form": "웅 ㅠ 좀만 확진자 줄어들면 친구랑 가야겠다",
          "original_form": "웅 ㅠ 좀만 확진자 줄어들면 친구랑 가야겠다",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013823.1.8",
          "form": "엄마랑 가자",
          "original_form": "엄마랑 가자",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013823.1.9",
          "form": "됐다 ㅋ 엄마는 별로 안좋아하잖아",
          "original_form": "됐다 ㅋ 엄마는 별로 안좋아하잖아",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013823.1.10",
          "form": "나도 좋아한다",
          "original_form": "나도 좋아한다",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013823.1.11",
          "form": "그럴까 그럼?ㅋ 경주에 맛집투어 가자",
          "original_form": "그럴까 그럼?ㅋ 경주에 맛집투어 가자",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013823.1.12",
          "form": "경주 가면은 황남빵 먹어야지",
          "original_form": "경주 가면은 황남빵 먹어야지",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013823.1.13",
          "form": "그렇지 ㅋㅋ 맛나겠다",
          "original_form": "그렇지 ㅋㅋ 맛나겠다",
          "speaker_id": "1",
          "time": "20210825 16:20"
