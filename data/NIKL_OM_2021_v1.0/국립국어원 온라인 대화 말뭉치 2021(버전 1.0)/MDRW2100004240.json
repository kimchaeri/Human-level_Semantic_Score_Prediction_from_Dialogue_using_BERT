        {
          "id": "MDRW2100004240.1.1",
          "form": "점심 뭐 먹었어?",
          "original_form": "점심 뭐 먹었어?",
          "speaker_id": "1",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004240.1.2",
          "form": "김치찌개!",
          "original_form": "김치찌개!",
          "speaker_id": "2",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004240.1.3",
          "form": "점심에 뭐 먹었어??",
          "original_form": "점심에 뭐 먹었어??",
          "speaker_id": "2",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004240.1.4",
          "form": "나는 어제 저녁에 반찬 만들어놔서 그거랑 밥먹었어",
          "original_form": "나는 어제 저녁에 반찬 만들어놔서 그거랑 밥먹었어",
          "speaker_id": "1",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004240.1.5",
          "form": "연근조림이랑 팽이버섯 베이컨말이!",
          "original_form": "연근조림이랑 팽이버섯 베이컨말이!",
          "speaker_id": "1",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004240.1.6",
          "form": "김치찌개 맛있겠다~",
          "original_form": "김치찌개 맛있겠다~",
          "speaker_id": "1",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004240.1.7",
          "form": "김치찌개랑 계란말이 같이먹으면 밥한공기 뚝딱인데",
          "original_form": "김치찌개랑 계란말이 같이먹으면 밥한공기 뚝딱인데",
          "speaker_id": "1",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004240.1.8",
          "form": "우와 되게 잘 챙겨먹었네!ㅎㅎㅎ멋쩌",
          "original_form": "우와 되게 잘 챙겨먹었네!ㅎㅎㅎ멋쩌",
          "speaker_id": "2",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004240.1.9",
          "form": "그치그치 계란말이도 할걸!!!ㅋㅋㅋㅋ",
          "original_form": "그치그치 계란말이도 할걸!!!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004240.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ요즘 요리하는거 좋아서 이것저것 자주 만들어",
          "original_form": "ㅋㅋㅋㅋㅋㅋ요즘 요리하는거 좋아서 이것저것 자주 만들어",
          "speaker_id": "1",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004240.1.11",
          "form": "계란말이 잘 해??",
          "original_form": "계란말이 잘 해??",
          "speaker_id": "1",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004240.1.12",
          "form": "못하진 않는데 잘하지도 않는거같앜ㅋㅋㅋ",
          "original_form": "못하진 않는데 잘하지도 않는거같앜ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004240.1.13",
          "form": "계란찜을 더 자주 해먹어!",
          "original_form": "계란찜을 더 자주 해먹어!",
          "speaker_id": "2",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004240.1.14",
          "form": "계란찜도 맛있겠다",
          "original_form": "계란찜도 맛있겠다",
          "speaker_id": "1",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004240.1.15",
          "form": "나도 계란말이 잘 못해서 주로 후라이 해먹어!",
          "original_form": "나도 계란말이 잘 못해서 주로 후라이 해먹어!",
          "speaker_id": "1",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004240.1.16",
          "form": "저녁엔 뭐 먹을거야??",
          "original_form": "저녁엔 뭐 먹을거야??",
          "speaker_id": "2",
          "time": "20210821 15:43"
        },
        {
          "id": "MDRW2100004240.1.17",
          "form": "저녁에는 국수 해먹으려고",
          "original_form": "저녁에는 국수 해먹으려고",
          "speaker_id": "1",
          "time": "20210821 15:43"
        },
        {
          "id": "MDRW2100004240.1.18",
          "form": "너는 뭐먹을거야?",
          "original_form": "너는 뭐먹을거야?",
          "speaker_id": "1",
          "time": "20210821 15:43"
        },
        {
          "id": "MDRW2100004240.1.19",
          "form": "나는 오랜만에 족발 시켜보려구",
          "original_form": "나는 오랜만에 족발 시켜보려구",
          "speaker_id": "2",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004240.1.20",
          "form": "같이 오는 막국수가 기대돼",
          "original_form": "같이 오는 막국수가 기대돼",
          "speaker_id": "2",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004240.1.21",
          "form": "완전 맛있겠다",
          "original_form": "완전 맛있겠다",
          "speaker_id": "1",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004240.1.22",
          "form": "막국수 너무 좋지",
          "original_form": "막국수 너무 좋지",
          "speaker_id": "1",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004240.1.23",
          "form": "나는 동치미냉면 육수 사와서",
          "original_form": "나는 동치미냉면 육수 사와서",
          "speaker_id": "1",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004240.1.24",
          "form": "소면 사와가지고 그걸로 국수 해먹으려고",
          "original_form": "소면 사와가지고 그걸로 국수 해먹으려고",
          "speaker_id": "1",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004240.1.25",
          "form": "와 냉면 육수까지 맛있겠다",
          "original_form": "와 냉면 육수까지 맛있겠다",
          "speaker_id": "2",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004240.1.26",
          "form": "우리 다 저녁에 면 먹겠네ㅎㅎ",
          "original_form": "우리 다 저녁에 면 먹겠네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004240.1.27",
          "form": "저녁 맛있게 먹어!!",
          "original_form": "저녁 맛있게 먹어!!",
          "speaker_id": "2",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004240.1.28",
          "form": "너도 맛있게 먹어!!",
