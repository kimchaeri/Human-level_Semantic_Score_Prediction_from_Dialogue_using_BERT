      "utterance": [
        {
          "id": "MDRW2100032662.1.1",
          "form": "영상이니 마블얘기 그대로..ㅋㅋ",
          "original_form": "영상이니 마블얘기 그대로..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032662.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 토르 라그나로크 편도 좋아합니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 토르 라그나로크 편도 좋아합니다",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032662.1.3",
          "form": "토르가 다좋은데 액션씬이 생각보다 노잼이라 아쉬워요",
          "original_form": "토르가 다좋은데 액션씬이 생각보다 노잼이라 아쉬워요",
          "speaker_id": "1",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100032662.1.4",
          "form": "전 일단 어벤져스에서의 비주얼 충격이...ㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "전 일단 어벤져스에서의 비주얼 충격이...ㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 00:30"
        },
        {
          "id": "MDRW2100032662.1.5",
          "form": "살찐 그모습ㅋㅋㅋㅋㅋ",
          "original_form": "살찐 그모습ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 01:27"
        },
        {
          "id": "MDRW2100032662.1.6",
          "form": "잘자용",
          "original_form": "잘자용",
          "speaker_id": "1",
          "time": "20210906 01:27"
        },
        {
          "id": "MDRW2100032662.1.7",
          "form": "그럼 아이언맨 시리즈중에 몇편을 제일 좋아하세요?",
          "original_form": "그럼 아이언맨 시리즈중에 몇편을 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 10:05"
        },
        {
          "id": "MDRW2100032662.1.8",
          "form": "3편이요 메리크리스마스!",
          "original_form": "3편이요 메리크리스마스!",
          "speaker_id": "1",
          "time": "20210906 11:10"
        },
        {
          "id": "MDRW2100032662.1.9",
          "form": "3편이시군요 ㅋㅋㅋㅋ",
          "original_form": "3편이시군요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:03"
        },
        {
          "id": "MDRW2100032662.1.10",
          "form": "그럼 아이언맨 다음으로 좋아하는 캐릭터는 있으세요?",
          "original_form": "그럼 아이언맨 다음으로 좋아하는 캐릭터는 있으세요?",
          "speaker_id": "2",
          "time": "20210906 12:03"
        },
        {
          "id": "MDRW2100032662.1.11",
          "form": "비운의 헐크랄까...",
          "original_form": "비운의 헐크랄까...",
          "speaker_id": "1",
          "time": "20210906 12:17"
        },
        {
          "id": "MDRW2100032662.1.12",
          "form": "헐크 ㅠㅠ짠내나죠 ㅠㅠ",
          "original_form": "헐크 ㅠㅠ짠내나죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 13:54"
        },
        {
          "id": "MDRW2100032662.1.13",
          "form": "넷플 dp 재밌더라구요 보셨나요",
          "original_form": "넷플 dp 재밌더라구요 보셨나요",
          "speaker_id": "1",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100032662.1.14",
          "form": "아니요 아직요 엄청화제던데요",
          "original_form": "아니요 아직요 엄청화제던데요",
          "speaker_id": "2",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100032662.1.15",
          "form": "꼭보세요 너무 재밌습니다",
          "original_form": "꼭보세요 너무 재밌습니다",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100032662.1.16",
          "form": "슬퍼요ㅜ",
          "original_form": "슬퍼요ㅜ",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100032662.1.17",
          "form": "네 꼭 볼게요",
          "original_form": "네 꼭 볼게요",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100032662.1.18",
          "form": "엄청 화제라서 궁금햇엇어요 ㅋㅋㅋ",
          "original_form": "엄청 화제라서 궁금햇엇어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100032662.1.19",
          "form": "하지만 전 킹익출시..",
          "original_form": "하지만 전 킹익출시..",
