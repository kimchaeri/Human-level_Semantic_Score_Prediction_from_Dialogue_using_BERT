      "utterance": [
        {
          "id": "MDRW2100014597.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014597.1.2",
          "form": "배달음식 좋아하세요?",
          "original_form": "배달음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014597.1.3",
          "form": "배달음식 좋아해요!",
          "original_form": "배달음식 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014597.1.4",
          "form": "치킨 좋아합니다",
          "original_form": "치킨 좋아합니다",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014597.1.5",
          "form": "배달음식 어떤거 좋아하세요?",
          "original_form": "배달음식 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014597.1.6",
          "form": "헉 저도 치킨이요. 좋아하는 프랜차이즈 있나요",
          "original_form": "헉 저도 치킨이요. 좋아하는 프랜차이즈 있나요",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014597.1.7",
          "form": "저 뿌링클 좋아해요",
          "original_form": "저 뿌링클 좋아해요",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014597.1.8",
          "form": "약간 소스파에요",
          "original_form": "약간 소스파에요",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014597.1.9",
          "form": "헉 너무 맛잇죠",
          "original_form": "헉 너무 맛잇죠",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014597.1.10",
          "form": "굽네치킨의 고바도 맛잇죠?",
          "original_form": "굽네치킨의 고바도 맛잇죠?",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014597.1.11",
          "form": "고바 맛있다고 들었는데 먹어봐야겠네요",
          "original_form": "고바 맛있다고 들었는데 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014597.1.12",
          "form": "먹던것만 먹던 사람이라",
          "original_form": "먹던것만 먹던 사람이라",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014597.1.13",
          "form": "혹시 어린이 입맛에게 더 추천할만한 치킨 있을까요?",
          "original_form": "혹시 어린이 입맛에게 더 추천할만한 치킨 있을까요?",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014597.1.14",
          "form": "치킨더홈 양념치킨이 왼전 달달하고 옛날 양념 맛이라서 추천해요",
          "original_form": "치킨더홈 양념치킨이 왼전 달달하고 옛날 양념 맛이라서 추천해요",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100014597.1.15",
          "form": "그리고 교촌 허니 콤보도 너무 좋고요",
          "original_form": "그리고 교촌 허니 콤보도 너무 좋고요",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100014597.1.16",
          "form": "오 옛날 양념맛 먹고싶었었는데  한번시켜먹어야겠네요",
          "original_form": "오 옛날 양념맛 먹고싶었었는데  한번시켜먹어야겠네요",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100014597.1.17",
          "form": "허니콤보 진짜 맛있죠",
          "original_form": "허니콤보 진짜 맛있죠",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100014597.1.18",
          "form": "떡볶이 브랜드는 뭐 좋아하시죠?",
          "original_form": "떡볶이 브랜드는 뭐 좋아하시죠?",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100014597.1.19",
          "form": "저 엽떡 좋아하는데",
          "original_form": "저 엽떡 좋아하는데",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100014597.1.20",
          "form": "배떡 로제 맛있다더라구요",
          "original_form": "배떡 로제 맛있다더라구요",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100014597.1.21",
          "form": "먹어봤는데 맛나요!",
          "original_form": "먹어봤는데 맛나요!",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100014597.1.22",
          "form": "엽떡은 매운단계 뭐하세요",
          "original_form": "엽떡은 매운단계 뭐하세요",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100014597.1.23",
          "form": "저 매운거 잘못먹어서",
          "original_form": "저 매운거 잘못먹어서",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100014597.1.24",
          "form": "순한맛 먹어요ㅋㅋㅋㅋ",
          "original_form": "순한맛 먹어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100014597.1.25",
          "form": "ㅋㅋㅋㅋ 떡볶이 토핑 조합은 뭐좋아시나요",
          "original_form": "ㅋㅋㅋㅋ 떡볶이 토핑 조합은 뭐좋아시나요",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100014597.1.26",
          "form": "매운거 잘드세요?",
          "original_form": "매운거 잘드세요?",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100014597.1.27",
          "form": "네네 잘먹어요 ㅎㅎ 적당히",
          "original_form": "네네 잘먹어요 ㅎㅎ 적당히",
          "speaker_id": "1",
          "time": "20210825 22:37"
        }
      ]
    }
