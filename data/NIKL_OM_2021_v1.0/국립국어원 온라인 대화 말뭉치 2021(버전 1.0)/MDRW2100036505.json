      "utterance": [
        {
          "id": "MDRW2100036505.1.1",
          "form": "음 예전에!",
          "original_form": "음 예전에!",
          "speaker_id": "2",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036505.1.2",
          "form": "나는 아이패드에서 굿노트라는 제품을 구입한 적 앴어",
          "original_form": "나는 아이패드에서 굿노트라는 제품을 구입한 적 앴어",
          "speaker_id": "1",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036505.1.3",
          "form": "사진보정 어플을 구매한적있어",
          "original_form": "사진보정 어플을 구매한적있어",
          "speaker_id": "2",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036505.1.4",
          "form": "그리고 요즘은 뽀모도로라는 어플을 사고싶어",
          "original_form": "그리고 요즘은 뽀모도로라는 어플을 사고싶어",
          "speaker_id": "1",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036505.1.5",
          "form": "아 굿노트는 나도 잇어",
          "original_form": "아 굿노트는 나도 잇어",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036505.1.6",
          "form": "시간관리에 좋더라",
          "original_form": "시간관리에 좋더라",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036505.1.7",
          "form": "그건 어던 어플이야?",
          "original_form": "그건 어던 어플이야?",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036505.1.8",
          "form": "사진보정 어떤 어플인데?",
          "original_form": "사진보정 어떤 어플인데?",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036505.1.9",
          "form": "나도 사 줄 수 있어?",
          "original_form": "나도 사 줄 수 있어?",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036505.1.10",
          "form": "아날로그 파리라고 들어본적있지?",
          "original_form": "아날로그 파리라고 들어본적있지?",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036505.1.11",
          "form": "이제 유행지나서 못써",
          "original_form": "이제 유행지나서 못써",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036505.1.12",
          "form": "뽀모도로는 시간을 정해놓고 공부와 휴식을 번갈아하는 공부법같아",
          "original_form": "뽀모도로는 시간을 정해놓고 공부와 휴식을 번갈아하는 공부법같아",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036505.1.13",
          "form": "사실 잘 몰라",
          "original_form": "사실 잘 몰라",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036505.1.14",
          "form": "어플 이름도 뽀모도로야",
          "original_form": "어플 이름도 뽀모도로야",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036505.1.15",
          "form": "우와 신기하다",
          "original_form": "우와 신기하다",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036505.1.16",
          "form": "근데 to do list 라고 깔리는 거 같기도 해",
          "original_form": "근데 to do list 라고 깔리는 거 같기도 해",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036505.1.17",
          "form": "그럼 체크리스트 같은 어플인가?",
          "original_form": "그럼 체크리스트 같은 어플인가?",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036505.1.18",
          "form": "근데 무료버전도 괜찮더라",
          "original_form": "근데 무료버전도 괜찮더라",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036505.1.19",
          "form": "아하아하 그런 종류의 어플이구나",
          "original_form": "아하아하 그런 종류의 어플이구나",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036505.1.20",
          "form": "응 대신 시간을 재 줘",
          "original_form": "응 대신 시간을 재 줘",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036505.1.21",
          "form": "제한시간내에 해야해",
          "original_form": "제한시간내에 해야해",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036505.1.22",
          "form": "그럼 공부하는데 많은 도움이 되겠다",
          "original_form": "그럼 공부하는데 많은 도움이 되겠다",
          "speaker_id": "2",
          "time": "20210830 23:56"
        }
      ]
