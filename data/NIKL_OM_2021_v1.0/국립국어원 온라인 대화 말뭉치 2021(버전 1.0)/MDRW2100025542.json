      "utterance": [
        {
          "id": "MDRW2100025542.1.1",
          "form": "name2님 점심 뭐 드셨나요?!",
          "original_form": "&name2&님 점심 뭐 드셨나요?!",
          "speaker_id": "1",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025542.1.2",
          "form": "저는 냉동볶음밥을 먹었습니다name1님은요??",
          "original_form": "저는 냉동볶음밥을 먹었습니다&name1&님은요??",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025542.1.3",
          "form": "오오 전자레인지에 돌려 드셨나요?",
          "original_form": "오오 전자레인지에 돌려 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025542.1.4",
          "form": "저는 도시락을 먹었어요!",
          "original_form": "저는 도시락을 먹었어요!",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025542.1.5",
          "form": "두부랑... 병아리콩자반이랑...",
          "original_form": "두부랑... 병아리콩자반이랑...",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025542.1.6",
          "form": "어쩌다보니 채식 위주네요 ㅋㅋㅋㅋㅋ",
          "original_form": "어쩌다보니 채식 위주네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025542.1.7",
          "form": "병아리 콩자반이름이너무기엽네요 ㅠㅠ",
          "original_form": "병아리 콩자반이름이너무기엽네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025542.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025542.1.9",
          "form": "저는 콩을 안좋아해요..",
          "original_form": "저는 콩을 안좋아해요..",
          "speaker_id": "2",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025542.1.10",
          "form": "앗 저도 사실 좋아하지는 않는데",
          "original_form": "앗 저도 사실 좋아하지는 않는데",
          "speaker_id": "1",
          "time": "20210901 13:19"
        },
        {
          "id": "MDRW2100025542.1.11",
          "form": "반찬으로 단짠단짠 맛을 내면 괜찮다구요!!",
          "original_form": "반찬으로 단짠단짠 맛을 내면 괜찮다구요!!",
          "speaker_id": "1",
          "time": "20210901 13:19"
        },
        {
          "id": "MDRW2100025542.1.12",
          "form": "요즘은 4인 제한 때문에",
          "original_form": "요즘은 4인 제한 때문에",
          "speaker_id": "1",
          "time": "20210901 13:20"
        },
        {
          "id": "MDRW2100025542.1.13",
          "form": "맛집 가는 것도 자유롭지 않네요...",
          "original_form": "맛집 가는 것도 자유롭지 않네요...",
          "speaker_id": "1",
          "time": "20210901 13:20"
        },
        {
          "id": "MDRW2100025542.1.14",
          "form": "그래도 갈사람은 다가더라구요",
          "original_form": "그래도 갈사람은 다가더라구요",
          "speaker_id": "2",
          "time": "20210901 13:28"
        },
        {
          "id": "MDRW2100025542.1.15",
          "form": "먼가 작년2.5보다 느슨해진기분이예요 ㅠㅠ",
          "original_form": "먼가 작년2.5보다 느슨해진기분이예요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 13:28"
        },
        {
          "id": "MDRW2100025542.1.16",
          "form": "아...  다들 지쳐서 그런가봐요",
          "original_form": "아...  다들 지쳐서 그런가봐요",
          "speaker_id": "1",
          "time": "20210901 13:43"
        },
        {
          "id": "MDRW2100025542.1.17",
          "form": "흑 세상에 맛볼 음식은 많은데",
          "original_form": "흑 세상에 맛볼 음식은 많은데",
          "speaker_id": "1",
          "time": "20210901 13:43"
        },
        {
          "id": "MDRW2100025542.1.18",
          "form": "name2님은 친구 만나면 주로 어떤 음식을 드시나요~?",
          "original_form": "&name2&님은 친구 만나면 주로 어떤 음식을 드시나요~?",
          "speaker_id": "1",
          "time": "20210901 13:43"
        },
        {
          "id": "MDRW2100025542.1.19",
          "form": "저는 여러가지 먹어요",
          "original_form": "저는 여러가지 먹어요",
          "speaker_id": "2",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100025542.1.20",
          "form": "초밥 파스타 이런것들??",
          "original_form": "초밥 파스타 이런것들??",
          "speaker_id": "2",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100025542.1.21",
          "form": "가리질않아서",
          "original_form": "가리질않아서",
          "speaker_id": "2",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100025542.1.22",
          "form": "오오 저도 좋아하는 것들이에요!!!",
          "original_form": "오오 저도 좋아하는 것들이에요!!!",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025542.1.23",
          "form": "태국음식 좋아하시나요?",
          "original_form": "태국음식 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025542.1.24",
          "form": "헉 쌀국수는",
          "original_form": "헉 쌀국수는",
          "speaker_id": "2",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025542.1.25",
          "form": "최악이였어요!",
          "original_form": "최악이였어요!",
          "speaker_id": "2",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025542.1.26",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025542.1.27",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025542.1.28",
          "form": "쌀국수 안좋아하시는 분 처음 봐요!! 혹기",
          "original_form": "쌀국수 안좋아하시는 분 처음 봐요!! 혹기",
          "speaker_id": "1",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025542.1.29",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "1",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025542.1.30",
          "form": "고수를 드신거 아니예요?!",
          "original_form": "고수를 드신거 아니예요?!",
          "speaker_id": "1",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025542.1.31",
          "form": "아니예요!!",
          "original_form": "아니예요!!",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025542.1.32",
          "form": "그 쌀국수가 애기분유맛..?",
          "original_form": "그 쌀국수가 애기분유맛..?",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025542.1.33",
          "form": "그런맛이 나더라구요 ㅠㅠㅠ",
          "original_form": "그런맛이 나더라구요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025542.1.34",
          "form": "아 진짜요?!",
          "original_form": "아 진짜요?!",
          "speaker_id": "1",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100025542.1.35",
          "form": "우와 저는 그런 생각 안해봤는데",
          "original_form": "우와 저는 그런 생각 안해봤는데",
          "speaker_id": "1",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100025542.1.36",
          "form": "아 그러고보니 태국식 쌀국수가",
          "original_form": "아 그러고보니 태국식 쌀국수가",
          "speaker_id": "1",
          "time": "20210901 14:00"
        },
        {
          "id": "MDRW2100025542.1.37",
          "form": "그럴 수는 있겠어요 대부분 좀 걸쭉한 경향이 있어서요!",
          "original_form": "그럴 수는 있겠어요 대부분 좀 걸쭉한 경향이 있어서요!",
          "speaker_id": "1",
          "time": "20210901 14:00"
        },
        {
          "id": "MDRW2100025542.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 맛이 너무 충격적이였어요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 맛이 너무 충격적이였어요!",
