      "utterance": [
        {
          "id": "MDRW2100004104.1.1",
          "form": "name2님 배달음식 쟈주 드시나요?",
          "original_form": "&name2&님 배달음식 쟈주 드시나요?",
          "speaker_id": "1",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004104.1.2",
          "form": "제가 해먹지를 못해서",
          "original_form": "제가 해먹지를 못해서",
          "speaker_id": "2",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004104.1.3",
          "form": "본의아니게 자주 먹습니다",
          "original_form": "본의아니게 자주 먹습니다",
          "speaker_id": "2",
          "time": "20210821 15:00"
        },
        {
          "id": "MDRW2100004104.1.4",
          "form": "옼ㅋㅋㅋㅋㅋ 자취하면서 배달 빈도가 늘어나신건가요",
          "original_form": "옼ㅋㅋㅋㅋㅋ 자취하면서 배달 빈도가 늘어나신건가요",
          "speaker_id": "1",
          "time": "20210821 15:00"
        },
        {
          "id": "MDRW2100004104.1.5",
          "form": "저도 자취할때 배달의 노예..",
          "original_form": "저도 자취할때 배달의 노예..",
          "speaker_id": "1",
          "time": "20210821 15:00"
        },
        {
          "id": "MDRW2100004104.1.6",
          "form": "그렇죠ㅋㅋㅋ",
          "original_form": "그렇죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:00"
        },
        {
          "id": "MDRW2100004104.1.7",
          "form": "집에서는 거의 안시켜먹었는데",
          "original_form": "집에서는 거의 안시켜먹었는데",
          "speaker_id": "2",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004104.1.8",
          "form": "자취하니까 어쩔 수 없네요",
          "original_form": "자취하니까 어쩔 수 없네요",
          "speaker_id": "2",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004104.1.9",
          "form": "오늘 저녁에는 전을 시켜먹을까 고민중이에요",
          "original_form": "오늘 저녁에는 전을 시켜먹을까 고민중이에요",
          "speaker_id": "2",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004104.1.10",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "1",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004104.1.11",
          "form": "어떤 전 좋아하세요?",
          "original_form": "어떤 전 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004104.1.12",
          "form": "저는 육전이랑 버섯전 좋아해요",
          "original_form": "저는 육전이랑 버섯전 좋아해요",
          "speaker_id": "2",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004104.1.13",
          "form": "파전은 별로 안좋아하고...",
          "original_form": "파전은 별로 안좋아하고...",
          "speaker_id": "2",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004104.1.14",
          "form": "와 육전 전 중의 전이죠",
          "original_form": "와 육전 전 중의 전이죠",
          "speaker_id": "1",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004104.1.15",
          "form": "그러고보니 오늘 점심에 엄마가 깻잎전이랑 가지전 하셨긴 했어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그러고보니 오늘 점심에 엄마가 깻잎전이랑 가지전 하셨긴 했어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004104.1.16",
          "form": "역시 비에는 전인가봐요",
          "original_form": "역시 비에는 전인가봐요",
          "speaker_id": "2",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004104.1.17",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004104.1.18",
          "form": "얼마전에 김치전도 먹었어요",
          "original_form": "얼마전에 김치전도 먹었어요",
          "speaker_id": "1",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004104.1.19",
          "form": "와 맛있겠다",
          "original_form": "와 맛있겠다",
          "speaker_id": "2",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004104.1.20",
          "form": "저 김치전도 좋아해요",
          "original_form": "저 김치전도 좋아해요",
          "speaker_id": "2",
          "time": "20210821 15:05"
        },
        {
          "id": "MDRW2100004104.1.21",
          "form": "바삭바삭한 김치전은 언제먹어도 좋어요",
          "original_form": "바삭바삭한 김치전은 언제먹어도 좋어요",
          "speaker_id": "1",
          "time": "20210821 15:05"
        },
        {
          "id": "MDRW2100004104.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 15:05"
        },
        {
          "id": "MDRW2100004104.1.23",
          "form": "얼마전에 집에 갔을때",
          "original_form": "얼마전에 집에 갔을때",
          "speaker_id": "2",
          "time": "20210821 15:05"
        },
        {
          "id": "MDRW2100004104.1.24",
          "form": "또 먹고싶네요",
          "original_form": "또 먹고싶네요",
          "speaker_id": "1",
          "time": "20210821 15:05"
        },
        {
