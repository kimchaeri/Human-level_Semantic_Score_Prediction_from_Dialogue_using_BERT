        },
        {
          "id": "MDRW2100004104.1.52",
          "form": "ㅋㅋㅋㅋㅋㅋ저도 그렇게 생각해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도 그렇게 생각해요",
