      "utterance": [
        {
          "id": "MDRW2100045632.1.1",
          "form": "맞아요 대세는 레트로죠",
          "original_form": "맞아요 대세는 레트로죠",
          "speaker_id": "1",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045632.1.2",
          "form": "아우터라는 과자 기억하시나요?",
          "original_form": "아우터라는 과자 기억하시나요?",
          "speaker_id": "1",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045632.1.3",
          "form": "얜 그당시 획기적으로 플라스틱통에 담겨져있었어요",
          "original_form": "얜 그당시 획기적으로 플라스틱통에 담겨져있었어요",
          "speaker_id": "1",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045632.1.4",
          "form": "아 그거 진짜 맛있어요",
          "original_form": "아 그거 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045632.1.5",
          "form": "그 통에든거 맞죠",
          "original_form": "그 통에든거 맞죠",
          "speaker_id": "2",
          "time": "20210908 04:29"
        },
        {
          "id": "MDRW2100045632.1.6",
          "form": "그거는진짜 기억나요!!",
          "original_form": "그거는진짜 기억나요!!",
          "speaker_id": "2",
          "time": "20210908 04:29"
        },
        {
          "id": "MDRW2100045632.1.7",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210908 04:30"
        },
        {
          "id": "MDRW2100045632.1.8",
          "form": "그 광고도 손 안쓰고 입에 털어넣는? 그런 광고였어요",
          "original_form": "그 광고도 손 안쓰고 입에 털어넣는? 그런 광고였어요",
          "speaker_id": "1",
          "time": "20210908 04:30"
        },
        {
          "id": "MDRW2100045632.1.9",
          "form": "이거 진짜 맛있었는데ㅜㅜㅜㅜ",
          "original_form": "이거 진짜 맛있었는데ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 04:30"
        },
        {
          "id": "MDRW2100045632.1.10",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045632.1.11",
          "form": "저 이거 소풍갈때 챙겨갔던것 같아요",
          "original_form": "저 이거 소풍갈때 챙겨갔던것 같아요",
          "speaker_id": "2",
          "time": "20210908 04:33"
        },
        {
          "id": "MDRW2100045632.1.12",
          "form": "기억에 남아있능거보면 진짜 자주먹었나 싶네요 ㅋㅋㅋ",
          "original_form": "기억에 남아있능거보면 진짜 자주먹었나 싶네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:33"
        },
        {
          "id": "MDRW2100045632.1.13",
          "form": "전 원픽 과자가 아우터였어요ㅋㅋㅋㅋㅋ",
          "original_form": "전 원픽 과자가 아우터였어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:33"
        },
        {
          "id": "MDRW2100045632.1.14",
          "form": "거기에 팬돌이...!",
          "original_form": "거기에 팬돌이...!",
          "speaker_id": "1",
          "time": "20210908 04:33"
        },
        {
          "id": "MDRW2100045632.1.15",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210908 04:35"
        },
        {
          "id": "MDRW2100045632.1.16",
          "form": "역시 뭘좀 아시는분",
          "original_form": "역시 뭘좀 아시는분",
          "speaker_id": "2",
          "time": "20210908 04:35"
        },
        {
          "id": "MDRW2100045632.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:35"
        },
        {
          "id": "MDRW2100045632.1.18",
          "form": "팬돌이 분홍색이 솜사탕 맛이라고 해서 항상 기대했지만",
          "original_form": "팬돌이 분홍색이 솜사탕 맛이라고 해서 항상 기대했지만",
          "speaker_id": "1",
          "time": "20210908 04:36"
        },
        {
          "id": "MDRW2100045632.1.19",
          "form": "솜사탕 맛은 아니었던 기억이 있어욬ㅋㅋㅋ",
          "original_form": "솜사탕 맛은 아니었던 기억이 있어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:36"
        },
        {
          "id": "MDRW2100045632.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:36"
        },
        {
          "id": "MDRW2100045632.1.21",
          "form": "저는 무슨색을 좋아했는지는 기억이 안나네요",
          "original_form": "저는 무슨색을 좋아했는지는 기억이 안나네요",
          "speaker_id": "2",
          "time": "20210908 04:37"
        },
        {
          "id": "MDRW2100045632.1.22",
          "form": "그치만 그 패키지가 정말 귀여웟어요",
          "original_form": "그치만 그 패키지가 정말 귀여웟어요",
          "speaker_id": "2",
          "time": "20210908 04:37"
        },
        {
          "id": "MDRW2100045632.1.23",
          "form": "맞아요ㅋㅋㅋㅋ 팬돌이는 소풍 국룰 음료죠",
          "original_form": "맞아요ㅋㅋㅋㅋ 팬돌이는 소풍 국룰 음료죠",
          "speaker_id": "1",
          "time": "20210908 04:38"
        },
        {
          "id": "MDRW2100045632.1.24",
          "form": "그 뚜껑에 소주먹듯이 따라 먹어야했죠ㅋㅋㅋㅋ",
          "original_form": "그 뚜껑에 소주먹듯이 따라 먹어야했죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045632.1.25",
          "form": "아 진짜 그거 정말",
          "original_form": "아 진짜 그거 정말",
          "speaker_id": "2",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045632.1.26",
          "form": "많이 했었는데ㅋㅋㅋㅋㅋㅋ",
          "original_form": "많이 했었는데ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045632.1.27",
          "form": "지금 생각해보면 너무 웃기네요 ㅋㅋㅋㅋ",
          "original_form": "지금 생각해보면 너무 웃기네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045632.1.28",
          "form": "그니까욬ㅋㅋㅋㅋㅋ 애기들이 어른들 따라한다고ㅋㅋㅋㅋ",
          "original_form": "그니까욬ㅋㅋㅋㅋㅋ 애기들이 어른들 따라한다고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045632.1.29",
          "form": "지금생각해보면 그때 부모님들이 왜 말렸는지 이해가가욬ㅋㅋㅋ",
          "original_form": "지금생각해보면 그때 부모님들이 왜 말렸는지 이해가가욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045632.1.30",
          "form": "얼마나 웃겼을까..",
          "original_form": "얼마나 웃겼을까..",
          "speaker_id": "2",
          "time": "20210908 04:41"
        },
        {
          "id": "MDRW2100045632.1.31",
          "form": "그게 뭐 좋은 거라고 말이죠ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그게 뭐 좋은 거라고 말이죠ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:41"
        },
        {
          "id": "MDRW2100045632.1.32",
          "form": "그래도 그때는 그게 재미있었으니까~ㅋㅋㅋㅋ",
          "original_form": "그래도 그때는 그게 재미있었으니까~ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:42"
        },
        {
          "id": "MDRW2100045632.1.33",
          "form": "추억으로 남았으니 좋은거죠 뭐~",
          "original_form": "추억으로 남았으니 좋은거죠 뭐~",
          "speaker_id": "2",
          "time": "20210908 04:42"
        },
        {
          "id": "MDRW2100045632.1.34",
          "form": "저 너무 졸려서!!! 자러갈게요!!! 오늘 감사했습니다!!",
          "original_form": "저 너무 졸려서!!! 자러갈게요!!! 오늘 감사했습니다!!",
          "speaker_id": "2",
