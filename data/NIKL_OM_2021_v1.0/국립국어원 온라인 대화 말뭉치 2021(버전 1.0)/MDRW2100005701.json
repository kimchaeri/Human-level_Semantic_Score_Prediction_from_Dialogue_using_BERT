      "utterance": [
        {
          "id": "MDRW2100005701.1.1",
          "form": "배달 자주 시켜드시나요?",
          "original_form": "배달 자주 시켜드시나요?",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005701.1.2",
          "form": "저 주1회정도?",
          "original_form": "저 주1회정도?",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005701.1.3",
          "form": "몽이님은요?",
          "original_form": "몽이님은요?",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005701.1.4",
          "form": "저도 1.2회정도요ㅋ 주말은 한번은 꼭 시키구요ㅋ",
          "original_form": "저도 1.2회정도요ㅋ 주말은 한번은 꼭 시키구요ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005701.1.5",
          "form": "ㅋㅋ배민너무중독돼요",
          "original_form": "ㅋㅋ배민너무중독돼요",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005701.1.6",
          "form": "주로 어떤거시키세요? 전 카페류",
          "original_form": "주로 어떤거시키세요? 전 카페류",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005701.1.7",
          "form": "아 저는 주로 빵이나 밥때울 음식요 ㅋㅋ 치킨 족발 피자",
          "original_form": "아 저는 주로 빵이나 밥때울 음식요 ㅋㅋ 치킨 족발 피자",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100005701.1.8",
          "form": "ㅋㅋㅋ약간 정통파시네요",
          "original_form": "ㅋㅋㅋ약간 정통파시네요",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005701.1.9",
          "form": "피자브랜드 추천해주세용",
          "original_form": "피자브랜드 추천해주세용",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005701.1.10",
          "form": "정통파 인가요??? ㅋㅋㅋㅋ 피자 저희는 도미노만 먹어요",
          "original_form": "정통파 인가요??? ㅋㅋㅋㅋ 피자 저희는 도미노만 먹어요",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100005701.1.11",
          "form": "저는 맨날 라떼나 먹태 이런 이상한거시켜요ㅋㅋ",
          "original_form": "저는 맨날 라떼나 먹태 이런 이상한거시켜요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100005701.1.12",
          "form": "도미노... 부자시네요",
          "original_form": "도미노... 부자시네요",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100005701.1.13",
          "form": "먹태는 술안주로 최고죠",
          "original_form": "먹태는 술안주로 최고죠",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100005701.1.14",
          "form": "저 먹태랑 결혼하려고요",
          "original_form": "저 먹태랑 결혼하려고요",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100005701.1.15",
          "form": "ㅋㅋㅋㅋㅋ 켁 ㅋㅋㅋㅋ 그르지마세요",
          "original_form": "ㅋㅋㅋㅋㅋ 켁 ㅋㅋㅋㅋ 그르지마세요",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100005701.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ근데 맥주랑 먹으면 진짜 좋은듯요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ근데 맥주랑 먹으면 진짜 좋은듯요",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100005701.1.17",
          "form": "아니면 꿀막걸리랑",
          "original_form": "아니면 꿀막걸리랑",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100005701.1.18",
          "form": "최고죠 마요네즈에 간장 청양고추 넣고.. 소스를",
          "original_form": "최고죠 마요네즈에 간장 청양고추 넣고.. 소스를",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100005701.1.19",
          "form": "아... 뭘좀아시네요",
          "original_form": "아... 뭘좀아시네요",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100005701.1.20",
          "form": "전 마요네즈에 와사비ㅎㅎ",
          "original_form": "전 마요네즈에 와사비ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100005701.1.21",
          "form": "먹는거에는 진심입니다.ㅋㅋㅋㅋ",
          "original_form": "먹는거에는 진심입니다.ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:14"
        }
      ]
