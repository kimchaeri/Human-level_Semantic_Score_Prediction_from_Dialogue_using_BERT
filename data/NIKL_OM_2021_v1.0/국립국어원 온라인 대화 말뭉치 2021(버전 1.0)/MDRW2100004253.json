      "utterance": [
        {
          "id": "MDRW2100004253.1.1",
          "form": "여름 휴가 어디로 다녀왔어?",
          "original_form": "여름 휴가 어디로 다녀왔어?",
          "speaker_id": "1",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004253.1.2",
          "form": "난 제주도갔다왔어!",
          "original_form": "난 제주도갔다왔어!",
          "speaker_id": "2",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004253.1.3",
          "form": "호캉스긴했지만 재밌었어",
          "original_form": "호캉스긴했지만 재밌었어",
          "speaker_id": "2",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004253.1.4",
          "form": "오!! 좋았겠다",
          "original_form": "오!! 좋았겠다",
          "speaker_id": "1",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004253.1.5",
          "form": "나는 영덕 다녀왔어",
          "original_form": "나는 영덕 다녀왔어",
          "speaker_id": "1",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004253.1.6",
          "form": "대게 먹고 왔냐고 물어봐도 되나? ㅋㅋㅋ",
          "original_form": "대게 먹고 왔냐고 물어봐도 되나? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004253.1.7",
          "form": "너무 많이들어본질문일거같아",
          "original_form": "너무 많이들어본질문일거같아",
          "speaker_id": "2",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004253.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004253.1.9",
          "form": "대게는 안먹고 홍게탕 먹고왔어",
          "original_form": "대게는 안먹고 홍게탕 먹고왔어",
          "speaker_id": "1",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004253.1.10",
          "form": "대게 파는집이 진짜 엄청 많더라",
          "original_form": "대게 파는집이 진짜 엄청 많더라",
          "speaker_id": "1",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004253.1.11",
          "form": "아 비도오는데 홍게탕 괴롭다...",
          "original_form": "아 비도오는데 홍게탕 괴롭다...",
          "speaker_id": "2",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004253.1.12",
          "form": "놀러간사람이 한번씩만 대게먹어줘도 대게 파는집 차릴만하지~",
          "original_form": "놀러간사람이 한번씩만 대게먹어줘도 대게 파는집 차릴만하지~",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004253.1.13",
          "form": "나도 가서 꼭 성게알비빔밥먹겠다했는데",
          "original_form": "나도 가서 꼭 성게알비빔밥먹겠다했는데",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004253.1.14",
          "form": "제일맛있었던건 갈치조림이였어...",
          "original_form": "제일맛있었던건 갈치조림이였어...",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004253.1.15",
          "form": "바닷가라 그런지 해산물 요리 엄청 많더라 진짜",
          "original_form": "바닷가라 그런지 해산물 요리 엄청 많더라 진짜",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004253.1.16",
          "form": "갈치조림도 맛있었겠다",
          "original_form": "갈치조림도 맛있었겠다",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004253.1.17",
          "form": "하루에도 백번씩만들어야돼서그런지 상향평준화돼있는느낌",
          "original_form": "하루에도 백번씩만들어야돼서그런지 상향평준화돼있는느낌",
          "speaker_id": "2",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004253.1.18",
          "form": "성게알 비빔밥은 못먹었어?",
          "original_form": "성게알 비빔밥은 못먹었어?",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004253.1.19",
          "form": "같이먹었는데 성게알비빔밥은 남겼어 ㅋㅋㅋ",
          "original_form": "같이먹었는데 성게알비빔밥은 남겼어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004253.1.20",
          "form": "지금 다시주면 싹싹긁어먹을텐데",
          "original_form": "지금 다시주면 싹싹긁어먹을텐데",
          "speaker_id": "2",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004253.1.21",
          "form": "먹을게 너무많아서...",
          "original_form": "먹을게 너무많아서...",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004253.1.22",
          "form": "제주도 너무 좋지 맛있는것도 많고...",
          "original_form": "제주도 너무 좋지 맛있는것도 많고...",
          "speaker_id": "1",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004253.1.23",
          "form": "나는 제주도에서 먹은 고기국수가 아직도 생각이 나...",
          "original_form": "나는 제주도에서 먹은 고기국수가 아직도 생각이 나...",
          "speaker_id": "1",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004253.1.24",
          "form": "헉 그렇게 맛있어??",
          "original_form": "헉 그렇게 맛있어??",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004253.1.25",
          "form": "고기넣은 국수인거같아서 건너뛰었는데... 아쉽다",
          "original_form": "고기넣은 국수인거같아서 건너뛰었는데... 아쉽다",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004253.1.26",
          "form": "유명하다고 하는데 가서 먹었는데 국물이 너무 맛있는거야 면도 맛있고 반찬으로 준 겉절이 김치도 맛있고",
          "original_form": "유명하다고 하는데 가서 먹었는데 국물이 너무 맛있는거야 면도 맛있고 반찬으로 준 겉절이 김치도 맛있고",
          "speaker_id": "1",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004253.1.27",
          "form": "나도 다시가고싶다 ㅠㅜ 진짜 남는건 먹을거랑 바다야.... 사진이랑 ㅋㅋㅋ",
          "original_form": "나도 다시가고싶다 ㅠㅜ 진짜 남는건 먹을거랑 바다야.... 사진이랑 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004253.1.28",
          "form": "맞아... 나는 전에 제주도 갔을때",
          "original_form": "맞아... 나는 전에 제주도 갔을때",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004253.1.29",
          "form": "시장 갔거든",
          "original_form": "시장 갔거든",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004253.1.30",
          "form": "근데 귤을 엄청 팔았는데",
          "original_form": "근데 귤을 엄청 팔았는데",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004253.1.31",
          "form": "지나다닐때마다 맛보라고 귤을 줘서",
          "original_form": "지나다닐때마다 맛보라고 귤을 줘서",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004253.1.32",
          "form": "귤 안샀는데도 거의 한소쿠리 먹었어",
          "original_form": "귤 안샀는데도 거의 한소쿠리 먹었어",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004253.1.33",
          "form": "나도 귤 진짜좋아하는데",
          "original_form": "나도 귤 진짜좋아하는데",
          "speaker_id": "2",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004253.1.34",
          "form": "진짜 제주도 아는사람만있어도 귤 얻어먹을수있대 ㅋㅋㅋ",
          "original_form": "진짜 제주도 아는사람만있어도 귤 얻어먹을수있대 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:56"
        }
      ]
    }
