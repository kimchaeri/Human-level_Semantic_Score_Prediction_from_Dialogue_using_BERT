      "utterance": [
        {
          "id": "MDRW2100030275.1.1",
          "form": "네 당연하죠 안녕히 주무세요 ㅋㅋ",
          "original_form": "네 당연하죠 안녕히 주무세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030275.1.2",
          "form": "네 ㅎㅎ 내일 뵈용!! 아마도 저녁에!!",
          "original_form": "네 ㅎㅎ 내일 뵈용!! 아마도 저녁에!!",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030275.1.3",
          "form": "안녕히 주무세요!!",
          "original_form": "안녕히 주무세요!!",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030275.1.4",
          "form": "네 저도 아마 저녁에는 할수 있을거 같아요 좋은꿈 꾸세요!!",
          "original_form": "네 저도 아마 저녁에는 할수 있을거 같아요 좋은꿈 꾸세요!!",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030275.1.5",
          "form": "3일을 연락할 수없었네요 ㅠㅠ 넘 바빴어요 ㅠㅠ",
          "original_form": "3일을 연락할 수없었네요 ㅠㅠ 넘 바빴어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100030275.1.6",
          "form": "최근에 쇼핑하신거 있으신가요? ㅎㅎ",
          "original_form": "최근에 쇼핑하신거 있으신가요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100030275.1.7",
          "form": "주말에 많이 바쁘셨었나보네요!! 저 요즘에 뭘 샀을까요 정신이 없어서 생각도 안나네요 ㅋㅋㅋㅋ",
          "original_form": "주말에 많이 바쁘셨었나보네요!! 저 요즘에 뭘 샀을까요 정신이 없어서 생각도 안나네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100030275.1.8",
          "form": "아 구두 샀어요",
          "original_form": "아 구두 샀어요",
          "speaker_id": "1",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100030275.1.9",
          "form": "구두요?ㅎㅎ 어떤거 사셨어요??ㅎㅎ",
          "original_form": "구두요?ㅎㅎ 어떤거 사셨어요??ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100030275.1.10",
          "form": "낮은 검은구두가 없어서 하나 샀어요 ㅎㅎ 토요일에 결혼식 다녀오는 바람에요 ㅋㅋㅋ",
          "original_form": "낮은 검은구두가 없어서 하나 샀어요 ㅎㅎ 토요일에 결혼식 다녀오는 바람에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100030275.1.11",
          "form": "아 ㅠㅠ 결혼식 갈려면 사야죠 ㅠ",
          "original_form": "아 ㅠㅠ 결혼식 갈려면 사야죠 ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100030275.1.12",
          "form": "예전에 신던 굽높은 구두밖에 없어서 도저히 못신겠더라고요",
          "original_form": "예전에 신던 굽높은 구두밖에 없어서 도저히 못신겠더라고요",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100030275.1.13",
          "form": "요즘에 뭐 사신거 있으세요??",
          "original_form": "요즘에 뭐 사신거 있으세요??",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100030275.1.14",
          "form": "저도 유튜브보고 구두를 사긴했어요",
          "original_form": "저도 유튜브보고 구두를 사긴했어요",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100030275.1.15",
          "form": "근데 비가와서 한번도 못신었네요 ㅠ",
          "original_form": "근데 비가와서 한번도 못신었네요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100030275.1.16",
          "form": "신으니까 엄청 편하던데 오래신어봐야 알텐데",
          "original_form": "신으니까 엄청 편하던데 오래신어봐야 알텐데",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100030275.1.17",
          "form": "오 유튜브 보고 사셨구나",
          "original_form": "오 유튜브 보고 사셨구나",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100030275.1.18",
          "form": "맞아요 저도 새로 산게 한 두시간 신었더니 아직 모르겠어요ㅋㅋ",
          "original_form": "맞아요 저도 새로 산게 한 두시간 신었더니 아직 모르겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100030275.1.19",
          "form": "ㅋㅋㅋ 이쁘기도하고 그 분이  편하다고하길래..",
          "original_form": "ㅋㅋㅋ 이쁘기도하고 그 분이  편하다고하길래..",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100030275.1.20",
          "form": "집에서 신었을땐 편했지만...",
          "original_form": "집에서 신었을땐 편했지만...",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100030275.1.21",
          "form": "실전이 궁금합니다",
          "original_form": "실전이 궁금합니다",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100030275.1.22",
          "form": "요번주에는 계속 비가 온다니까 못신으실거 같네요 가을이 지나기전에 예쁘게 신으시면 좋겠네요!!",
          "original_form": "요번주에는 계속 비가 온다니까 못신으실거 같네요 가을이 지나기전에 예쁘게 신으시면 좋겠네요!!",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100030275.1.23",
          "form": "ㅋㅋ 실전이 중요합니다",
          "original_form": "ㅋㅋ 실전이 중요합니다",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100030275.1.24",
          "form": "그쵸오 ㅠㅠㅠ 그랬으면 좋겠습니다... 비가 정말 그칠생각을 안해요",
          "original_form": "그쵸오 ㅠㅠㅠ 그랬으면 좋겠습니다... 비가 정말 그칠생각을 안해요",
          "speaker_id": "2",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100030275.1.25",
          "form": "뻑하면와요 ㅠㅠ",
          "original_form": "뻑하면와요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100030275.1.26",
          "form": "그거말고는 음..금방 녹차꽃빵 주문했어요",
          "original_form": "그거말고는 음..금방 녹차꽃빵 주문했어요",
          "speaker_id": "2",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100030275.1.27",
          "form": "하동꺼",
          "original_form": "하동꺼",
          "speaker_id": "2",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100030275.1.28",
          "form": "친구가 맛있다구해서 ㅎㅎ",
          "original_form": "친구가 맛있다구해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100030275.1.29",
          "form": "녹차꽃빵은 처음 들어봤어요!!",
          "original_form": "녹차꽃빵은 처음 들어봤어요!!",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100030275.1.30",
          "form": "저도 처음 ㅎㅎ 맛없으면 친구를 혼내킬려구요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 처음 ㅎㅎ 맛없으면 친구를 혼내킬려구요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100030275.1.31",
          "form": "이런걸 사라고하다니..!!!",
          "original_form": "이런걸 사라고하다니..!!!",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100030275.1.32",
          "form": "지역간식 하니까 생각났는데 쑥보리빵라고 제주에서 팔던것도 맛있었어요",
          "original_form": "지역간식 하니까 생각났는데 쑥보리빵라고 제주에서 팔던것도 맛있었어요",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100030275.1.33",
          "form": "그런것두 있나요?",
          "original_form": "그런것두 있나요?",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100030275.1.34",
          "form": "검색해봅니다",
          "original_form": "검색해봅니다",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100030275.1.35",
          "form": "ㅋㅋㅋㅋ 혼나지 않으셨으면...",
          "original_form": "ㅋㅋㅋㅋ 혼나지 않으셨으면...",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100030275.1.36",
          "form": "넹 맛있었어요",
          "original_form": "넹 맛있었어요",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100030275.1.37",
          "form": "오 맛있어보여요",
          "original_form": "오 맛있어보여요",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100030275.1.38",
          "form": "택배주문하기엔 넘많고",
          "original_form": "택배주문하기엔 넘많고",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100030275.1.39",
          "form": "맛이보장이란되니",
          "original_form": "맛이보장이란되니",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100030275.1.40",
          "form": "쑥이 한창 붐처럼 음료도 나오고 빵도 나오고 할때 사먹었던거 같아요",
          "original_form": "쑥이 한창 붐처럼 음료도 나오고 빵도 나오고 할때 사먹었던거 같아요",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100030275.1.41",
          "form": "제주가면 꼭 먹어볼께요!",
          "original_form": "제주가면 꼭 먹어볼께요!",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100030275.1.42",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100030275.1.43",
          "form": "ㅋㅋㅋ 나중에 기회되면 한번 네!!",
          "original_form": "ㅋㅋㅋ 나중에 기회되면 한번 네!!",
          "speaker_id": "1",
