      "utterance": [
        {
          "id": "MDRW2100043222.1.1",
          "form": "아직은 보고 싶지 않아!",
          "original_form": "아직은 보고 싶지 않아!",
          "speaker_id": "2",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043222.1.2",
          "form": "키 노래 태연이 피쳐링한 거 들어봤어? 키 앨범 나온다더라~~",
          "original_form": "키 노래 태연이 피쳐링한 거 들어봤어? 키 앨범 나온다더라~~",
          "speaker_id": "2",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100043222.1.3",
          "form": "hate that 좋더라",
          "original_form": "hate that 좋더라",
          "speaker_id": "1",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100043222.1.4",
          "form": "가을에 듣기 딱 좋은 거 같아~",
          "original_form": "가을에 듣기 딱 좋은 거 같아~",
          "speaker_id": "1",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100043222.1.5",
          "form": "가을 플리 추천해줄래",
          "original_form": "가을 플리 추천해줄래",
          "speaker_id": "1",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100043222.1.6",
          "form": "가을하면 아무래도 태연의 계절인 것 같아",
          "original_form": "가을하면 아무래도 태연의 계절인 것 같아",
          "speaker_id": "2",
          "time": "20210907 17:00"
        },
        {
          "id": "MDRW2100043222.1.7",
          "form": "태연 발라드는 무조건 다 들어야 하고",
          "original_form": "태연 발라드는 무조건 다 들어야 하고",
          "speaker_id": "2",
          "time": "20210907 17:00"
        },
        {
          "id": "MDRW2100043222.1.8",
          "form": "아무래도 가을은 지널찾의 계절이지",
          "original_form": "아무래도 가을은 지널찾의 계절이지",
          "speaker_id": "2",
          "time": "20210907 17:00"
        },
        {
          "id": "MDRW2100043222.1.9",
          "form": "지금 널 찾아가고 있어~~~~",
          "original_form": "지금 널 찾아가고 있어~~~~",
          "speaker_id": "2",
          "time": "20210907 17:01"
        },
        {
          "id": "MDRW2100043222.1.10",
          "form": "11:11도 좋지",
          "original_form": "11:11도 좋지",
          "speaker_id": "1",
          "time": "20210907 17:01"
        },
        {
          "id": "MDRW2100043222.1.11",
          "form": "세븐틴 이번 신곡도 좋더라",
          "original_form": "세븐틴 이번 신곡도 좋더라",
          "speaker_id": "1",
          "time": "20210907 17:01"
        },
        {
          "id": "MDRW2100043222.1.12",
          "form": "맞아 이번 신곡 중에 Heaven's Cloud도 좋아",
          "original_form": "맞아 이번 신곡 중에 Heaven's Cloud도 좋아",
          "speaker_id": "2",
          "time": "20210907 17:05"
        },
        {
          "id": "MDRW2100043222.1.13",
          "form": "그건 모르겠고",
          "original_form": "그건 모르겠고",
          "speaker_id": "1",
          "time": "20210907 17:07"
        },
        {
          "id": "MDRW2100043222.1.14",
          "form": "요네즈 켄시의 lemon도 좋더라",
          "original_form": "요네즈 켄시의 lemon도 좋더라",
          "speaker_id": "1",
          "time": "20210907 17:07"
        },
        {
          "id": "MDRW2100043222.1.15",
          "form": "눈 올때 듣고 싶어",
          "original_form": "눈 올때 듣고 싶어",
          "speaker_id": "1",
          "time": "20210907 17:08"
        },
        {
          "id": "MDRW2100043222.1.16",
          "form": "아 lemon이 일본 유재석 노래야?",
          "original_form": "아 lemon이 일본 유재석 노래야?",
          "speaker_id": "2",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100043222.1.17",
          "form": "걔는 호시노겐이야 ㅡㅡ",
          "original_form": "걔는 호시노겐이야 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210907 17:17"
        },
        {
          "id": "MDRW2100043222.1.18",
          "form": "요네즈 켄시는... 천재라고",
          "original_form": "요네즈 켄시는... 천재라고",
          "speaker_id": "1",
          "time": "20210907 17:17"
        },
        {
          "id": "MDRW2100043222.1.19",
          "form": "kanden이란 노래도 정말 좋아",
          "original_form": "kanden이란 노래도 정말 좋아",
          "speaker_id": "1",
          "time": "20210907 17:17"
        },
        {
          "id": "MDRW2100043222.1.20",
          "form": "몰라 들을 의향 없어",
          "original_form": "몰라 들을 의향 없어",
          "speaker_id": "2",
          "time": "20210907 17:18"
        },
        {
          "id": "MDRW2100043222.1.21",
          "form": "돌도팽이나 들어",
          "original_form": "돌도팽이나 들어",
          "speaker_id": "2",
          "time": "20210907 17:18"
        },
        {
          "id": "MDRW2100043222.1.22",
          "form": "유튜브 최고 뮤직",
          "original_form": "유튜브 최고 뮤직",
          "speaker_id": "2",
          "time": "20210907 17:18"
        },
        {
          "id": "MDRW2100043222.1.23",
          "form": "햄튜브 짱이지 최고의 유튜버",
          "original_form": "햄튜브 짱이지 최고의 유튜버",
          "speaker_id": "1",
          "time": "20210907 17:19"
        },
        {
          "id": "MDRW2100043222.1.24",
          "form": "허챠밍이랑 해쭈도 재밌어",
          "original_form": "허챠밍이랑 해쭈도 재밌어",
          "speaker_id": "1",
          "time": "20210907 17:19"
        },
        {
          "id": "MDRW2100043222.1.25",
          "form": "허챠밍 밀키트 또 내달라구~~",
          "original_form": "허챠밍 밀키트 또 내달라구~~",
          "speaker_id": "2",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100043222.1.26",
          "form": "허챠밍 먹방하는 거 보면 진짜 입맛 다셔져",
          "original_form": "허챠밍 먹방하는 거 보면 진짜 입맛 다셔져",
          "speaker_id": "2",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100043222.1.27",
          "form": "면 먹방이 짱이지!",
          "original_form": "면 먹방이 짱이지!",
          "speaker_id": "1",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100043222.1.28",
          "form": "허챠밍처럼 요리 잘하는 사람이 되고싶다",
          "original_form": "허챠밍처럼 요리 잘하는 사람이 되고싶다",
          "speaker_id": "1",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100043222.1.29",
          "form": "나도 허챠밍 같은 사람 되고 싶다..",
          "original_form": "나도 허챠밍 같은 사람 되고 싶다..",
          "speaker_id": "2",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100043222.1.30",
          "form": "그림도 잘 그리잖아",
          "original_form": "그림도 잘 그리잖아",
          "speaker_id": "2",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100043222.1.31",
          "form": "게임도 잘 하고",
          "original_form": "게임도 잘 하고",
          "speaker_id": "2",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100043222.1.32",
          "form": "못하는 건 공포게임 말곤 없는 듯",
          "original_form": "못하는 건 공포게임 말곤 없는 듯",
          "speaker_id": "2",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100043222.1.33",
          "form": "그리고 휴먼 폴 플랫",
          "original_form": "그리고 휴먼 폴 플랫",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100043222.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100043222.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아 허챠밍이랑 햄튜브 휴폴플 또 해달라고~~~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아 허챠밍이랑 햄튜브 휴폴플 또 해달라고~~~",
          "speaker_id": "2",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100043222.1.36",
          "form": "진짜 재밌는데~",
          "original_form": "진짜 재밌는데~",
          "speaker_id": "1",
          "time": "20210907 17:33"
        },
        {
          "id": "MDRW2100043222.1.37",
          "form": "햄튜브는 게임도 재밌게 잘하니까",
          "original_form": "햄튜브는 게임도 재밌게 잘하니까",
          "speaker_id": "1",
          "time": "20210907 17:34"
        },
        {
          "id": "MDRW2100043222.1.38",
          "form": "햄튜브 진짜 게임 왜 이렇게 잘하냐구~~",
          "original_form": "햄튜브 진짜 게임 왜 이렇게 잘하냐구~~",
