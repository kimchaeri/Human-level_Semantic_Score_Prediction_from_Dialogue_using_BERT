        },
        {
          "id": "MDRW2100028759.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028759.1.3",
          "form": "최근 중고거래 하셨나요?",
          "original_form": "최근 중고거래 하셨나요?",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028759.1.4",
          "form": "중고로는 한 게 없고... 인터넷으로 비타민 산 게 가장 최근에 한 쇼핑이네요. 근래 인터넷으로 뭐 구매하신 거 있나요?",
          "original_form": "중고로는 한 게 없고... 인터넷으로 비타민 산 게 가장 최근에 한 쇼핑이네요. 근래 인터넷으로 뭐 구매하신 거 있나요?",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028759.1.5",
          "form": "저는 비타민 많이 샀어요 건강땜에ㅠ",
          "original_form": "저는 비타민 많이 샀어요 건강땜에ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028759.1.6",
          "form": "저도 중고는 잘 안사게되네요 ㅎㅎ",
          "original_form": "저도 중고는 잘 안사게되네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028759.1.7",
          "form": "최근에 뭐 구매하셨나용",
          "original_form": "최근에 뭐 구매하셨나용",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028759.1.8",
          "form": "저도 건강 때문에ㅋㅋㅋㅋ",
          "original_form": "저도 건강 때문에ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028759.1.9",
          "form": "다른 영양제도 사보려다 말았는데",
          "original_form": "다른 영양제도 사보려다 말았는데",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028759.1.10",
          "form": "아까 얘기했듯이 비타민이 제일 최근이요!",
          "original_form": "아까 얘기했듯이 비타민이 제일 최근이요!",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028759.1.11",
          "form": "맞아요 ㅋㅋ루테인도 사고 역시 건강이 최고에요",
          "original_form": "맞아요 ㅋㅋ루테인도 사고 역시 건강이 최고에요",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028759.1.12",
          "form": "뭐 사고싶으신건 있으신가요 ㅎㅎ",
          "original_form": "뭐 사고싶으신건 있으신가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028759.1.13",
          "form": "집에 과자 쟁여둔 게 다 떨어져서 과자쇼핑하고 싶네요... 혹시 저처럼 간식거리 사두시는 편인가요?",
          "original_form": "집에 과자 쟁여둔 게 다 떨어져서 과자쇼핑하고 싶네요... 혹시 저처럼 간식거리 사두시는 편인가요?",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028759.1.14",
          "form": "네 전 아주 대량으로 산답니다",
          "original_form": "네 전 아주 대량으로 산답니다",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028759.1.15",
          "form": "최근에 꼬북칩 엄청많이샀어요",
          "original_form": "최근에 꼬북칩 엄청많이샀어요",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028759.1.16",
          "form": "너모맛있어요,,",
          "original_form": "너모맛있어요,,",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028759.1.17",
          "form": "꼬북칩 그... 무슨맛이 되게 유행 아니었나요?",
          "original_form": "꼬북칩 그... 무슨맛이 되게 유행 아니었나요?",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028759.1.18",
          "form": "츄러스맛!",
          "original_form": "츄러스맛!",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028759.1.19",
          "form": "또 무슨 과자 사셨어요?",
          "original_form": "또 무슨 과자 사셨어요?",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028759.1.20",
          "form": "맞아요 ㅠㅠ엉엉이거너무맛있어요",
          "original_form": "맞아요 ㅠㅠ엉엉이거너무맛있어요",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028759.1.21",
          "form": "그리고 그냥 스윙칩먹어요 ㅋㅋ",
          "original_form": "그리고 그냥 스윙칩먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028759.1.22",
          "form": "과자뭐좋아하세요?",
          "original_form": "과자뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028759.1.23",
          "form": "저는 감자칩 종류 다 좋아해요ㅋㅋㅋㅋㅋ 스윙칩 포카칩 프링글스 같은 거",
          "original_form": "저는 감자칩 종류 다 좋아해요ㅋㅋㅋㅋㅋ 스윙칩 포카칩 프링글스 같은 거",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028759.1.24",
          "form": "포카칩 두가지맛중에 뭐좋아하세요?",
          "original_form": "포카칩 두가지맛중에 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028759.1.25",
          "form": "맞아요 저는 어니언맛이요 ㅋㅋ감자칩은 어니언맛이 국룰일것같아요",
          "original_form": "맞아요 저는 어니언맛이요 ㅋㅋ감자칩은 어니언맛이 국룰일것같아요",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028759.1.26",
          "form": "최근에 편의점서 싸게 팔더라구요 ㅎㅎㅎ",
          "original_form": "최근에 편의점서 싸게 팔더라구요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028759.1.27",
          "form": "저도 어니언이요ㅠㅠ",
          "original_form": "저도 어니언이요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028759.1.28",
          "form": "그러고보니 편의점 신상 자주 드시는 편인가요",
          "original_form": "그러고보니 편의점 신상 자주 드시는 편인가요",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028759.1.29",
          "form": "전 오늘도 새로 나온 과자 집어와서 먹는 중이라.",
          "original_form": "전 오늘도 새로 나온 과자 집어와서 먹는 중이라.",
          "speaker_id": "2",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028759.1.30",
          "form": "저완전많이사먹어서",
          "original_form": "저완전많이사먹어서",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028759.1.31",
          "form": "인스타그램에 리뷰도 남겼어요",
          "original_form": "인스타그램에 리뷰도 남겼어요",
          "speaker_id": "1",
          "time": "20210902 21:58"
        }
      ]
