      "utterance": [
        {
          "id": "MDRW2100022907.1.1",
          "form": "name2님 반려동물 있으세요?",
          "original_form": "&name2&님 반려동물 있으세요?",
          "speaker_id": "1",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022907.1.2",
          "form": "저는 없어용ㅠㅍ",
          "original_form": "저는 없어용ㅠㅍ",
          "speaker_id": "2",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022907.1.3",
          "form": "하지만 언젠간 키우고싶어서",
          "original_form": "하지만 언젠간 키우고싶어서",
          "speaker_id": "2",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022907.1.4",
          "form": "유튜브로 영상 많이봅니당",
          "original_form": "유튜브로 영상 많이봅니당",
          "speaker_id": "2",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022907.1.5",
          "form": "저두 없어요 ㅠ",
          "original_form": "저두 없어요 ㅠ",
          "speaker_id": "1",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022907.1.6",
          "form": "그럼 예비 반려인이시군요",
          "original_form": "그럼 예비 반려인이시군요",
          "speaker_id": "1",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022907.1.7",
          "form": "넹넹ㅎㅎ",
          "original_form": "넹넹ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022907.1.8",
          "form": "유튜브로",
          "original_form": "유튜브로",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022907.1.9",
          "form": "진짜 다양하게봐요",
          "original_form": "진짜 다양하게봐요",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022907.1.10",
          "form": "햄스터 미어캣 강아지 고양이 오리ㅋㅋㅋ",
          "original_form": "햄스터 미어캣 강아지 고양이 오리ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022907.1.11",
          "form": "오리도요?",
          "original_form": "오리도요?",
          "speaker_id": "1",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022907.1.12",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022907.1.13",
          "form": "국내에 몇분 안되시는데",
          "original_form": "국내에 몇분 안되시는데",
          "speaker_id": "2",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022907.1.14",
          "form": "반려오리 키우시는 분들이ㅋㅋㅋ",
          "original_form": "반려오리 키우시는 분들이ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022907.1.15",
          "form": "근데 유튜브 하셔서",
          "original_form": "근데 유튜브 하셔서",
          "speaker_id": "2",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022907.1.16",
          "form": "매주 보고있아용",
          "original_form": "매주 보고있아용",
          "speaker_id": "2",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022907.1.17",
          "form": "오 그렇군요 전 가끔 골든리트리버나 진도 라쿤 봐요",
          "original_form": "오 그렇군요 전 가끔 골든리트리버나 진도 라쿤 봐요",
          "speaker_id": "1",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022907.1.18",
          "form": "와 라쿤!ㅋㅋㅋㅋ 라쿤하면 솜사탕 물에 씻어먹는거 생각나요ㅋㅋㅋ",
          "original_form": "와 라쿤!ㅋㅋㅋㅋ 라쿤하면 솜사탕 물에 씻어먹는거 생각나요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022907.1.19",
          "form": "골든리트리버는 진짜 천사같아요",
          "original_form": "골든리트리버는 진짜 천사같아요",
          "speaker_id": "2",
          "time": "20210829 19:08"
        },
        {
          "id": "MDRW2100022907.1.20",
          "form": "골든은 정말 천사죠ㅎㅎ 라쿤은 말썽꾸러기고요",
          "original_form": "골든은 정말 천사죠ㅎㅎ 라쿤은 말썽꾸러기고요",
          "speaker_id": "1",
          "time": "20210829 19:09"
        },
        {
          "id": "MDRW2100022907.1.21",
          "form": "둘이 같이 키우면",
          "original_form": "둘이 같이 키우면",
          "speaker_id": "2",
          "time": "20210829 19:09"
        },
        {
          "id": "MDRW2100022907.1.22",
          "form": "골댕이 마음속 옐로카드가",
          "original_form": "골댕이 마음속 옐로카드가",
          "speaker_id": "2",
          "time": "20210829 19:09"
        },
        {
          "id": "MDRW2100022907.1.23",
          "form": "백장은 쓸것같아요ㅋㅋㅋㅋ",
          "original_form": "백장은 쓸것같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:09"
        },
        {
          "id": "MDRW2100022907.1.24",
          "form": "의외로 잘 맞을지 몰라요 라쿤이 상대동물에게 애교가 많더라구요",
          "original_form": "의외로 잘 맞을지 몰라요 라쿤이 상대동물에게 애교가 많더라구요",
          "speaker_id": "1",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100022907.1.25",
          "form": "아 진짜요? 와아 넘 사랑스럽다",
          "original_form": "아 진짜요? 와아 넘 사랑스럽다",
          "speaker_id": "2",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100022907.1.26",
          "form": "저 라쿤 영상은 거의 안봤는데",
          "original_form": "저 라쿤 영상은 거의 안봤는데",
          "speaker_id": "2",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100022907.1.27",
          "form": "오늘부터 봐야겠어요ㅋㅋㅋ",
          "original_form": "오늘부터 봐야겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022907.1.28",
          "form": "보세요 매력덩어리에요",
          "original_form": "보세요 매력덩어리에요",
          "speaker_id": "1",
          "time": "20210829 19:12"
        }
      ]
    }
  ]
