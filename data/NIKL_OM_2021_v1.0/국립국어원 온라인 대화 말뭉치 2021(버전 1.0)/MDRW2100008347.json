      "utterance": [
        {
          "id": "MDRW2100008347.1.1",
          "form": "오이 좋아하시나 봐요",
          "original_form": "오이 좋아하시나 봐요",
          "speaker_id": "2",
          "time": "20210822 15:18"
        },
        {
          "id": "MDRW2100008347.1.2",
          "form": "저는 자두 좋아해요",
          "original_form": "저는 자두 좋아해요",
          "speaker_id": "2",
          "time": "20210822 15:19"
        },
        {
          "id": "MDRW2100008347.1.3",
          "form": "오이는 진리죠",
          "original_form": "오이는 진리죠",
          "speaker_id": "1",
          "time": "20210822 15:20"
        },
        {
          "id": "MDRW2100008347.1.4",
          "form": "길고 두꺼운 오이가 맛있어요",
          "original_form": "길고 두꺼운 오이가 맛있어요",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008347.1.5",
          "form": "그렇죠?",
          "original_form": "그렇죠?",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008347.1.6",
          "form": "바나나도 좋아해요",
          "original_form": "바나나도 좋아해요",
          "speaker_id": "2",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008347.1.7",
          "form": "바나나는 부드럽죠",
          "original_form": "바나나는 부드럽죠",
          "speaker_id": "1",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008347.1.8",
          "form": "부드럽고 달콤한 황도 먹고 싶네요",
          "original_form": "부드럽고 달콤한 황도 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008347.1.9",
          "form": "여름과일로",
          "original_form": "여름과일로",
          "speaker_id": "2",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008347.1.10",
          "form": "복숭아 맛있죠 먹어야겠어요",
          "original_form": "복숭아 맛있죠 먹어야겠어요",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008347.1.11",
          "form": "곧 가을이니 배도 나오겠네요",
          "original_form": "곧 가을이니 배도 나오겠네요",
          "speaker_id": "2",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008347.1.12",
          "form": "배는 진작 나왔어요",
          "original_form": "배는 진작 나왔어요",
          "speaker_id": "1",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008347.1.13",
          "form": "그랬나요 배가 제철인가요 벌써",
          "original_form": "그랬나요 배가 제철인가요 벌써",
          "speaker_id": "2",
          "time": "20210822 16:49"
        },
        {
          "id": "MDRW2100008347.1.14",
          "form": "음 유머인데.... 뭐 여튼..수박땡기네요",
          "original_form": "음 유머인데.... 뭐 여튼..수박땡기네요",
          "speaker_id": "1",
          "time": "20210822 16:50"
        },
        {
          "id": "MDRW2100008347.1.15",
          "form": "이제 수박 못 먹으니...",
          "original_form": "이제 수박 못 먹으니...",
          "speaker_id": "1",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008347.1.16",
          "form": "수박이 요즘 비싸요",
          "original_form": "수박이 요즘 비싸요",
          "speaker_id": "2",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008347.1.17",
          "form": "하나 드셔보세쇼",
          "original_form": "하나 드셔보세쇼",
          "speaker_id": "1",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008347.1.18",
          "form": "커피 좋아하세요",
          "original_form": "커피 좋아하세요",
          "speaker_id": "2",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008347.1.19",
          "form": "아뇨 카페인에 약해서",
          "original_form": "아뇨 카페인에 약해서",
          "speaker_id": "1",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008347.1.20",
          "form": "배고프네요 맛있는거 먹고파요",
          "original_form": "배고프네요 맛있는거 먹고파요",
          "speaker_id": "1",
          "time": "20210831 18:09"
        }
      ]
