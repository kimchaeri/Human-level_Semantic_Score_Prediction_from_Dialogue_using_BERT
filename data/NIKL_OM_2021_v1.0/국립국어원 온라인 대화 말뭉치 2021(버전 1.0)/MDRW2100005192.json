      "utterance": [
        {
          "id": "MDRW2100005192.1.1",
          "form": "2학년이요!",
          "original_form": "2학년이요!",
          "speaker_id": "2",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005192.1.2",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005192.1.3",
          "form": "오 저는 새내기예요!",
          "original_form": "오 저는 새내기예요!",
          "speaker_id": "1",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005192.1.4",
          "form": "내년이면 고3이시군여.......",
          "original_form": "내년이면 고3이시군여.......",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005192.1.5",
          "form": "네.......",
          "original_form": "네.......",
          "speaker_id": "2",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005192.1.6",
          "form": "네........ 괜찮아요 체감상으론 1년이 일주일 같아요",
          "original_form": "네........ 괜찮아요 체감상으론 1년이 일주일 같아요",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005192.1.7",
          "form": "(위로가 아닌가...)",
          "original_form": "(위로가 아닌가...)",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005192.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ괜차나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ괜차나요",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005192.1.9",
          "form": "요즘 학교다니기 힘들어요",
          "original_form": "요즘 학교다니기 힘들어요",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005192.1.10",
          "form": "코로나때문에 학교를 반밖에 못가서",
          "original_form": "코로나때문에 학교를 반밖에 못가서",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005192.1.11",
          "form": "그쳐ㅠㅠ 비대면수업 많죠...",
          "original_form": "그쳐ㅠㅠ 비대면수업 많죠...",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005192.1.12",
          "form": "오프라인 적응할라하면 온라인이고..ㅠㅜ",
          "original_form": "오프라인 적응할라하면 온라인이고..ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005192.1.13",
          "form": "공부에 집중하기 힘드시겠어요ㅠㅠ",
          "original_form": "공부에 집중하기 힘드시겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005192.1.14",
          "form": "어쩔 수 없죠 또 저만 이런게 아니니까..ㅠ",
          "original_form": "어쩔 수 없죠 또 저만 이런게 아니니까..ㅠ",
          "speaker_id": "2",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005192.1.15",
          "form": "그렇죠... 그래도 다 같은 상황이라",
          "original_form": "그렇죠... 그래도 다 같은 상황이라",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005192.1.16",
          "form": "생기부 적거나 이런 게 혼자만 그런 게 아니라",
          "original_form": "생기부 적거나 이런 게 혼자만 그런 게 아니라",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005192.1.17",
          "form": "그렇게 막 걱정 안 하셔도 될 거예요,,",
          "original_form": "그렇게 막 걱정 안 하셔도 될 거예요,,",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005192.1.18",
          "form": "name1님은 학교 어때요?",
          "original_form": "&name1&님은 학교 어때요?",
          "speaker_id": "2",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005192.1.19",
          "form": "거의 비대면 수업이긴 한데",
          "original_form": "거의 비대면 수업이긴 한데",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005192.1.20",
          "form": "저희 학교가 그나마 대면 많은 편이ㅔㅇ요",
          "original_form": "저희 학교가 그나마 대면 많은 편이ㅔㅇ요",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005192.1.21",
          "form": "그리고 저는 공대라 프로젝트나 실험 수업이 있어수",
          "original_form": "그리고 저는 공대라 프로젝트나 실험 수업이 있어수",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005192.1.22",
          "form": "아아ㅏㅏ 어쩔 수 없네요",
          "original_form": "아아ㅏㅏ 어쩔 수 없네요",
          "speaker_id": "2",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005192.1.23",
          "form": "강의 듣는 인원수 상관없이 대면이었어요...",
          "original_form": "강의 듣는 인원수 상관없이 대면이었어요...",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005192.1.24",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005192.1.25",
          "form": "지금쯤 개강하지 않아요??",
          "original_form": "지금쯤 개강하지 않아요??",
          "speaker_id": "2",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005192.1.26",
          "form": "9월 1일에 개강해요!",
          "original_form": "9월 1일에 개강해요!",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005192.1.27",
          "form": "개강장례식 곧 치르겠네요...",
          "original_form": "개강장례식 곧 치르겠네요...",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005192.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005192.1.29",
          "form": "공대는 방학 길어요??",
          "original_form": "공대는 방학 길어요??",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005192.1.30",
          "form": "방학은 거의 다 똑같아요",
          "original_form": "방학은 거의 다 똑같아요",
          "speaker_id": "1",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100005192.1.31",
          "form": "개강은 다들 똑같고",
          "original_form": "개강은 다들 똑같고",
          "speaker_id": "1",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100005192.1.32",
          "form": "종강이 시험 날짜에 따라 다른데",
          "original_form": "종강이 시험 날짜에 따라 다른데",
          "speaker_id": "1",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100005192.1.33",
          "form": "차이나봤자 일주일 정도?",
          "original_form": "차이나봤자 일주일 정도?",
          "speaker_id": "1",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100005192.1.34",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "2",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005192.1.35",
          "form": "이런얘기 되게 신기하네요ㅋㅋㅋㅋ",
          "original_form": "이런얘기 되게 신기하네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005192.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋ재밌네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ재밌네요",
          "speaker_id": "1",
          "time": "20210821 21:27"
        }
      ]
