        },
        {
          "id": "MDRW2100023301.1.2",
          "form": "타이밍 맞추는게 어렵군여",
          "original_form": "타이밍 맞추는게 어렵군여",
          "speaker_id": "2",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023301.1.3",
          "form": "그럼 집가면 전화해달라고 하세여 ㅋㅋ",
          "original_form": "그럼 집가면 전화해달라고 하세여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023301.1.4",
          "form": "ㅎㅎㅎ오 좋은 방안!!!",
          "original_form": "ㅎㅎㅎ오 좋은 방안!!!",
          "speaker_id": "1",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023301.1.5",
          "form": "감사합니당 ㅎㅎㅎ",
          "original_form": "감사합니당 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023301.1.6",
          "form": "계속 카톡할수도 없고",
          "original_form": "계속 카톡할수도 없고",
          "speaker_id": "2",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023301.1.7",
          "form": "카톡을 선호하는 거 같아요 ㅎㅎ",
          "original_form": "카톡을 선호하는 거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023301.1.8",
          "form": "어우 자러가야겟다",
          "original_form": "어우 자러가야겟다",
          "speaker_id": "1",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023301.1.9",
          "form": "그래두 자기전에 10분이라도 목소리를 들어줘야 ...ㅋㅋ",
          "original_form": "그래두 자기전에 10분이라도 목소리를 들어줘야 ...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023301.1.10",
          "form": "오늘 덕분에 마니 채웟네여 ㅋㅋㅋㅋ",
          "original_form": "오늘 덕분에 마니 채웟네여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023301.1.11",
          "form": "우선 자고 또 얘기하죠 ㅋㅋ",
          "original_form": "우선 자고 또 얘기하죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023301.1.12",
          "form": "마자여 자기전에 목소리 소중해여!",
          "original_form": "마자여 자기전에 목소리 소중해여!",
          "speaker_id": "1",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023301.1.13",
          "form": "넹 ㅌㅌㅋㅋ안녕히 주무세요",
          "original_form": "넹 ㅌㅌㅋㅋ안녕히 주무세요",
          "speaker_id": "1",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100023301.1.14",
          "form": "굿밤!",
          "original_form": "굿밤!",
          "speaker_id": "2",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100023301.1.15",
          "form": "저녁 드셨나여?",
          "original_form": "저녁 드셨나여?",
          "speaker_id": "2",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100023301.1.16",
          "form": "넹넹 간단히!!!",
          "original_form": "넹넹 간단히!!!",
          "speaker_id": "1",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100023301.1.17",
          "form": "이제 산책 나갈거에용 남친이랑 !!",
          "original_form": "이제 산책 나갈거에용 남친이랑 !!",
          "speaker_id": "1",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100023301.1.18",
          "form": "집이 진짜 가까운가봐요 ㅎㅎ",
          "original_form": "집이 진짜 가까운가봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100023301.1.19",
          "form": "넹 옆동네!ㅋㅋㅋ",
          "original_form": "넹 옆동네!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100023301.1.20",
          "form": "산책 잘하고 오셔요 ㅋㅋ 산책데이트 부럽네요",
          "original_form": "산책 잘하고 오셔요 ㅋㅋ 산책데이트 부럽네요",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023301.1.21",
          "form": "크크 잠깐 걸었는데두 무더운 날씨네요",
          "original_form": "크크 잠깐 걸었는데두 무더운 날씨네요",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100023301.1.22",
          "form": "밖에 덥나요?",
          "original_form": "밖에 덥나요?",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100023301.1.23",
          "form": "네네 너무 후텁지근해오",
          "original_form": "네네 너무 후텁지근해오",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100023301.1.24",
          "form": "낼부터 비온다던데",
          "original_form": "낼부터 비온다던데",
          "speaker_id": "2",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100023301.1.25",
          "form": "비올라고 그러나봐요",
          "original_form": "비올라고 그러나봐요",
          "speaker_id": "2",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100023301.1.26",
          "form": "ㅋㅋㅋㅋ일기예보 맨날 비만 그려져있어요",
          "original_form": "ㅋㅋㅋㅋ일기예보 맨날 비만 그려져있어요",
          "speaker_id": "1",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100023301.1.27",
          "form": "지겨워요",
          "original_form": "지겨워요",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100023301.1.28",
          "form": "데이트에 방해되는 비",
          "original_form": "데이트에 방해되는 비",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100023301.1.29",
          "form": "그니까여...",
          "original_form": "그니까여...",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100023301.1.30",
          "form": "저 사귀고나서 데이트 맨날 비왔어여",
          "original_form": "저 사귀고나서 데이트 맨날 비왔어여",
          "speaker_id": "1",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100023301.1.31",
          "form": "설마....",
          "original_form": "설마....",
          "speaker_id": "2",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100023301.1.32",
          "form": "진짜루..안온날이 없어요...",
          "original_form": "진짜루..안온날이 없어요...",
          "speaker_id": "1",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100023301.1.33",
          "form": "비를 몰고다니시는?",
          "original_form": "비를 몰고다니시는?",
          "speaker_id": "2",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100023301.1.34",
          "form": "좀 그런 편인가...ㅋㅋㅋㅋ",
          "original_form": "좀 그런 편인가...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100023301.1.35",
          "form": "가을 장마래요",
          "original_form": "가을 장마래요",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100023301.1.36",
          "form": "비 시른데ㅜㅜ",
          "original_form": "비 시른데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100023301.1.37",
          "form": "ㅎㅎ그니까요 제탓이 아니에요",
          "original_form": "ㅎㅎ그니까요 제탓이 아니에요",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100023301.1.38",
          "form": "ㅋㅋㅋㅋㅋ 그럴까요?",
          "original_form": "ㅋㅋㅋㅋㅋ 그럴까요?",
          "speaker_id": "2",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100023301.1.39",
          "form": "필라테스는 언제 하세요?",
          "original_form": "필라테스는 언제 하세요?",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100023301.1.40",
          "form": "백신 맞은 날에두 데이트하세용?",
          "original_form": "백신 맞은 날에두 데이트하세용?",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100023301.1.41",
          "form": "저 화목에 해요",
          "original_form": "저 화목에 해요",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100023301.1.42",
          "form": "여친하고 필라테스 하는것도 똑같네",
          "original_form": "여친하고 필라테스 하는것도 똑같네",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100023301.1.43",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100023301.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100023301.1.45",
          "form": "여친 이번주 수욜에 2차래요",
          "original_form": "여친 이번주 수욜에 2차래요",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100023301.1.46",
          "form": "이번주 주말엔 제가 모시러가야겠어요",
          "original_form": "이번주 주말엔 제가 모시러가야겠어요",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100023301.1.47",
          "form": "수욜 우왕 일찍 맞네요",
          "original_form": "수욜 우왕 일찍 맞네요",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100023301.1.48",
          "form": "전 수업 땜에 토욜이라",
          "original_form": "전 수업 땜에 토욜이라",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100023301.1.49",
          "form": "name1은 원격이에요?",
          "original_form": "&name1&은 원격이에요?",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100023301.1.50",
          "form": "아님 대면이에요?",
          "original_form": "아님 대면이에요?",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100023301.1.51",
          "form": "지금은 원격이용 ㅜ",
          "original_form": "지금은 원격이용 ㅜ",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100023301.1.52",
          "form": "여친하고 같네요",
          "original_form": "여친하고 같네요",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100023301.1.53",
          "form": "담임이시죠?",
          "original_form": "담임이시죠?",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100023301.1.54",
          "form": "지금은 담임안해욬ㅋㅋㅋㅋ",
          "original_form": "지금은 담임안해욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100023301.1.55",
          "form": "오 좋으네요",
          "original_form": "오 좋으네요",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100023301.1.56",
          "form": "이번주에 백신2차 맞으셔서 데이트 주말에 못하시겠군여",
