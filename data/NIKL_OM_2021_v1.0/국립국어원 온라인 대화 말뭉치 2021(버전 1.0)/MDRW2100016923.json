      "utterance": [
        {
          "id": "MDRW2100016923.1.1",
          "form": "음.. 군대!",
          "original_form": "음.. 군대!",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016923.1.2",
          "form": "남자이신가요~?!",
          "original_form": "남자이신가요~?!",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016923.1.3",
          "form": "아뇨 여자입니다 ~",
          "original_form": "아뇨 여자입니다 ~",
          "speaker_id": "2",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016923.1.4",
          "form": "군대에 대해 어떻게 생각 하시나용..?!ㅎㅎ",
          "original_form": "군대에 대해 어떻게 생각 하시나용..?!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 10:17"
        },
        {
          "id": "MDRW2100016923.1.5",
          "form": "ㅋㅋ ㅋㅋㅋㅋ나름의 도피처라고 생각해요",
          "original_form": "ㅋㅋ ㅋㅋㅋㅋ나름의 도피처라고 생각해요",
          "speaker_id": "2",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100016923.1.6",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100016923.1.7",
          "form": "ㅋㅋㅋㅋ음 저 해병대 체험했을때",
          "original_form": "ㅋㅋㅋㅋ음 저 해병대 체험했을때",
          "speaker_id": "1",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100016923.1.8",
          "form": "별로 좋지않아서 그냥 힘들겠구나...? 싶어용ㅋㅋ",
          "original_form": "별로 좋지않아서 그냥 힘들겠구나...? 싶어용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100016923.1.9",
          "form": "ㅋㅋㅋ헉",
          "original_form": "ㅋㅋㅋ헉",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100016923.1.10",
          "form": "해병대 체험도 했어요??",
          "original_form": "해병대 체험도 했어요??",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100016923.1.11",
          "form": "ㅋㅋㅋ그 옛날에 중학교땐가 야영을 해병대로 갔는데",
          "original_form": "ㅋㅋㅋ그 옛날에 중학교땐가 야영을 해병대로 갔는데",
          "speaker_id": "1",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100016923.1.12",
          "form": "우와 진짜 근육통에 최악이였어욬ㅋㅋ",
          "original_form": "우와 진짜 근육통에 최악이였어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100016923.1.13",
          "form": "11m에서 떨어지는거?! 했는데",
          "original_form": "11m에서 떨어지는거?! 했는데",
          "speaker_id": "1",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100016923.1.14",
          "form": "고소공포증있으니 무섭더라그여ㅠ",
          "original_form": "고소공포증있으니 무섭더라그여ㅠ",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100016923.1.15",
          "form": "헐ㅋㅋㅋㅋㅋㅋㅋㅋ 대박..",
          "original_form": "헐ㅋㅋㅋㅋㅋㅋㅋㅋ 대박..",
          "speaker_id": "2",
          "time": "20210829 19:09"
        },
        {
          "id": "MDRW2100016923.1.16",
          "form": "저때도 무슨 극기훈련인데....",
          "original_form": "저때도 무슨 극기훈련인데....",
          "speaker_id": "2",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100016923.1.17",
          "form": "너무 싫었어요",
          "original_form": "너무 싫었어요",
          "speaker_id": "2",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100016923.1.18",
          "form": "진짜 저도 극기훈련? 이런거 하니까",
          "original_form": "진짜 저도 극기훈련? 이런거 하니까",
          "speaker_id": "1",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100016923.1.19",
          "form": "군대가기도 싫더라구요",
          "original_form": "군대가기도 싫더라구요",
          "speaker_id": "1",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100016923.1.20",
          "form": "요즘 군대 논란도 많고ㅠ",
          "original_form": "요즘 군대 논란도 많고ㅠ",
          "speaker_id": "1",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100016923.1.21",
          "form": "그렇죠....",
          "original_form": "그렇죠....",
          "speaker_id": "2",
          "time": "20210830 22:30"
        },
        {
          "id": "MDRW2100016923.1.22",
          "form": "근ㄷ0 그래도 예전보다는 편해보여요",
          "original_form": "근ㄷ0 그래도 예전보다는 편해보여요",
          "speaker_id": "2",
          "time": "20210830 22:30"
        },
        {
          "id": "MDRW2100016923.1.23",
          "form": "맞아요 약간 핸드폰도 쓸수있고 예전보다는 나아보인다구 해야되낭!",
          "original_form": "맞아요 약간 핸드폰도 쓸수있고 예전보다는 나아보인다구 해야되낭!",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100016923.1.24",
          "form": "ㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100016923.1.25",
          "form": "자유로워보여요",
          "original_form": "자유로워보여요",
          "speaker_id": "2",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100016923.1.26",
          "form": "그래도 가는것자체는 힘들겠죠ㅠ? 괴롭힘도 있는것같두ㅠ",
          "original_form": "그래도 가는것자체는 힘들겠죠ㅠ? 괴롭힘도 있는것같두ㅠ",
          "speaker_id": "1",
          "time": "20210901 00:32"
        },
        {
          "id": "MDRW2100016923.1.27",
          "form": "그렇겠죠....??!",
          "original_form": "그렇겠죠....??!",
          "speaker_id": "2",
          "time": "20210901 01:22"
        },
        {
          "id": "MDRW2100016923.1.28",
          "form": "군대...어렵네요",
          "original_form": "군대...어렵네요",
