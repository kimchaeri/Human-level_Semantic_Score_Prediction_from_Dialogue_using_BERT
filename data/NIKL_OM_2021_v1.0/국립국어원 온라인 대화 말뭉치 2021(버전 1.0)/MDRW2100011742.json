      "utterance": [
        {
          "id": "MDRW2100011742.1.1",
          "form": "사랑에 눈 멀어서 가시밭길 가는 느낌이에요",
          "original_form": "사랑에 눈 멀어서 가시밭길 가는 느낌이에요",
          "speaker_id": "2",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100011742.1.2",
          "form": "연애가 이득될게 전혀없네요",
          "original_form": "연애가 이득될게 전혀없네요",
          "speaker_id": "1",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100011742.1.3",
          "form": "가시밭길ㅋㅋㅋㅋ 맞아요 사랑이 뭐길래",
          "original_form": "가시밭길ㅋㅋㅋㅋ 맞아요 사랑이 뭐길래",
          "speaker_id": "1",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100011742.1.4",
          "form": "뭔가 연애하는 커플보면",
          "original_form": "뭔가 연애하는 커플보면",
          "speaker_id": "2",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100011742.1.5",
          "form": "아슬아슬해요",
          "original_form": "아슬아슬해요",
          "speaker_id": "2",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100011742.1.6",
          "form": "오 네네",
          "original_form": "오 네네",
          "speaker_id": "1",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100011742.1.7",
          "form": "맞아요 제가 다 불안하고",
          "original_form": "맞아요 제가 다 불안하고",
          "speaker_id": "1",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100011742.1.8",
          "form": "안전이별조차 보장되어있지 않잖아요",
          "original_form": "안전이별조차 보장되어있지 않잖아요",
          "speaker_id": "1",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100011742.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100011742.1.10",
          "form": "주위는 사람을 너무 못믿는거라 하던데",
          "original_form": "주위는 사람을 너무 못믿는거라 하던데",
          "speaker_id": "2",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100011742.1.11",
          "form": "솔직히 어떻게 믿나요...",
          "original_form": "솔직히 어떻게 믿나요...",
          "speaker_id": "2",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100011742.1.12",
          "form": "아 저도 그소리들어요",
          "original_form": "아 저도 그소리들어요",
          "speaker_id": "1",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100011742.1.13",
          "form": "완전공감",
          "original_form": "완전공감",
          "speaker_id": "1",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100011742.1.14",
          "form": "약간 강박 취급받아요",
          "original_form": "약간 강박 취급받아요",
          "speaker_id": "1",
          "time": "20210824 15:40"
        },
        {
          "id": "MDRW2100011742.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 15:40"
        },
        {
          "id": "MDRW2100011742.1.16",
          "form": "좀 얜 부정적이다",
          "original_form": "좀 얜 부정적이다",
          "speaker_id": "2",
          "time": "20210824 15:40"
        },
        {
          "id": "MDRW2100011742.1.17",
          "form": "근데 사실 부정적인건 맞긴해욬ㅋㅋㅋ",
          "original_form": "근데 사실 부정적인건 맞긴해욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:40"
        },
        {
          "id": "MDRW2100011742.1.18",
          "form": "헐 저랑 존똑",
          "original_form": "헐 저랑 존똑",
          "speaker_id": "1",
          "time": "20210824 15:40"
        },
        {
          "id": "MDRW2100011742.1.19",
          "form": "왜 일어나지 않은일을 걱정하느냐",
          "original_form": "왜 일어나지 않은일을 걱정하느냐",
          "speaker_id": "1",
          "time": "20210824 15:40"
        },
        {
          "id": "MDRW2100011742.1.20",
          "form": "사는게 다 비슷하군요",
          "original_form": "사는게 다 비슷하군요",
          "speaker_id": "2",
          "time": "20210824 15:40"
        },
        {
          "id": "MDRW2100011742.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 15:41"
        },
        {
          "id": "MDRW2100011742.1.22",
          "form": "ㄴㅋㅋㅋㅋㅋㅋㅋ 그러게요 다 똑같이 사네요",
          "original_form": "ㄴㅋㅋㅋㅋㅋㅋㅋ 그러게요 다 똑같이 사네요",
          "speaker_id": "1",
          "time": "20210824 15:41"
        },
        {
          "id": "MDRW2100011742.1.23",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:41"
        },
        {
          "id": "MDRW2100011742.1.24",
          "form": "근데 제 눈에는 너무 안일하게 보이기만해요",
          "original_form": "근데 제 눈에는 너무 안일하게 보이기만해요",
          "speaker_id": "2",
          "time": "20210824 15:41"
        },
        {
          "id": "MDRW2100011742.1.25",
          "form": "여자가 피해볼게 너무 많죠...",
          "original_form": "여자가 피해볼게 너무 많죠...",
          "speaker_id": "1",
          "time": "20210824 15:42"
        },
        {
          "id": "MDRW2100011742.1.26",
          "form": "저는 가끔 궁금한게",
          "original_form": "저는 가끔 궁금한게",
          "speaker_id": "1",
          "time": "20210824 15:42"
        },
        {
          "id": "MDRW2100011742.1.27",
          "form": "만약 여성우월주의 사회라면 제가 연애를 택할지 궁금해요 양치님은 어떠세요",
          "original_form": "만약 여성우월주의 사회라면 제가 연애를 택할지 궁금해요 양치님은 어떠세요",
          "speaker_id": "1",
          "time": "20210824 15:43"
        },
        {
          "id": "MDRW2100011742.1.28",
          "form": "오 지금 여성들이 갖게되는 부담과 희생이",
          "original_form": "오 지금 여성들이 갖게되는 부담과 희생이",
          "speaker_id": "2",
          "time": "20210824 15:43"
        },
        {
          "id": "MDRW2100011742.1.29",
          "form": "역전된 사회 말씀하시는건가요?",
          "original_form": "역전된 사회 말씀하시는건가요?",
          "speaker_id": "2",
          "time": "20210824 15:43"
        },
        {
          "id": "MDRW2100011742.1.30",
          "form": "그 이갈리아의 딸들 보셨나요?",
          "original_form": "그 이갈리아의 딸들 보셨나요?",
          "speaker_id": "1",
          "time": "20210824 15:43"
        },
        {
          "id": "MDRW2100011742.1.31",
          "form": "자세히는 몰라요",
          "original_form": "자세히는 몰라요",
          "speaker_id": "2",
          "time": "20210824 15:43"
        },
        {
          "id": "MDRW2100011742.1.32",
          "form": "남자가 순결 강요받고",
          "original_form": "남자가 순결 강요받고",
          "speaker_id": "1",
          "time": "20210824 15:43"
        },
        {
          "id": "MDRW2100011742.1.33",
          "form": "애낳는거 위대한 일이고 승진하고",
          "original_form": "애낳는거 위대한 일이고 승진하고",
          "speaker_id": "1",
          "time": "20210824 15:44"
        },
        {
          "id": "MDRW2100011742.1.34",
          "form": "그런 사회거든요",
          "original_form": "그런 사회거든요",
          "speaker_id": "1",
          "time": "20210824 15:44"
        },
        {
          "id": "MDRW2100011742.1.35",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210824 15:44"
        },
        {
          "id": "MDRW2100011742.1.36",
          "form": "남자가 집안일 다하고 가정폭력당해요",
          "original_form": "남자가 집안일 다하고 가정폭력당해요",
          "speaker_id": "1",
          "time": "20210824 15:44"
        }
