      "utterance": [
        {
          "id": "MDRW2100021094.1.1",
          "form": "제주도 간다고?",
          "original_form": "제주도 간다고?",
          "speaker_id": "1",
          "time": "20210828 15:49"
        },
        {
          "id": "MDRW2100021094.1.2",
          "form": "남편일 연관된거야?",
          "original_form": "남편일 연관된거야?",
          "speaker_id": "1",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100021094.1.3",
          "form": "친구들과 우도도가고 맛집도 갈거야",
          "original_form": "친구들과 우도도가고 맛집도 갈거야",
          "speaker_id": "2",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100021094.1.4",
          "form": "이번여행은 친구들과 가는구나?",
          "original_form": "이번여행은 친구들과 가는구나?",
          "speaker_id": "1",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100021094.1.5",
          "form": "서귀포에 방예약하고",
          "original_form": "서귀포에 방예약하고",
          "speaker_id": "2",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100021094.1.6",
          "form": "우도 들어가?",
          "original_form": "우도 들어가?",
          "speaker_id": "1",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100021094.1.7",
          "form": "몇명이 가는데?",
          "original_form": "몇명이 가는데?",
          "speaker_id": "1",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100021094.1.8",
          "form": "운동친구들과 같이가기로했어",
          "original_form": "운동친구들과 같이가기로했어",
          "speaker_id": "2",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021094.1.9",
          "form": "4명이 같이가",
          "original_form": "4명이 같이가",
          "speaker_id": "2",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021094.1.10",
          "form": "오호~ 완전 좋겠다.",
          "original_form": "오호~ 완전 좋겠다.",
          "speaker_id": "1",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021094.1.11",
          "form": "비많이 안오면 우도 들어가려고",
          "original_form": "비많이 안오면 우도 들어가려고",
          "speaker_id": "2",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021094.1.12",
          "form": "1박? 2박?",
          "original_form": "1박? 2박?",
          "speaker_id": "1",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021094.1.13",
          "form": "2박3일",
          "original_form": "2박3일",
          "speaker_id": "2",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021094.1.14",
          "form": "우도주막? 그 프로그램 찍은곳 들릴수 있겠다",
          "original_form": "우도주막? 그 프로그램 찍은곳 들릴수 있겠다",
          "speaker_id": "1",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021094.1.15",
          "form": "2박3일 좋다",
          "original_form": "2박3일 좋다",
          "speaker_id": "1",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100021094.1.16",
          "form": "그것때문에. 우도도 가려고해",
          "original_form": "그것때문에. 우도도 가려고해",
          "speaker_id": "2",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100021094.1.17",
          "form": "비온다고해서 걱정이야",
          "original_form": "비온다고해서 걱정이야",
          "speaker_id": "2",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100021094.1.18",
          "form": "지난번 먹었던 회 생각나네",
          "original_form": "지난번 먹었던 회 생각나네",
          "speaker_id": "1",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100021094.1.19",
          "form": "오랜만에 가는 친구때문에 관광지도 조금 들를듯",
          "original_form": "오랜만에 가는 친구때문에 관광지도 조금 들를듯",
          "speaker_id": "2",
          "time": "20210828 15:54"
        },
        {
          "id": "MDRW2100021094.1.20",
          "form": "여행은 날씨가 50퍼 이상인데",
          "original_form": "여행은 날씨가 50퍼 이상인데",
          "speaker_id": "1",
          "time": "20210828 15:54"
        },
        {
          "id": "MDRW2100021094.1.21",
          "form": "6시이후에 차도 같이 못탄데",
          "original_form": "6시이후에 차도 같이 못탄데",
          "speaker_id": "2",
          "time": "20210828 15:54"
        },
        {
          "id": "MDRW2100021094.1.22",
          "form": "렌트 예약도 했어?",
          "original_form": "렌트 예약도 했어?",
          "speaker_id": "1",
          "time": "20210828 15:54"
        },
        {
          "id": "MDRW2100021094.1.23",
          "form": "싸게했어",
          "original_form": "싸게했어",
          "speaker_id": "2",
          "time": "20210828 15:55"
        },
        {
          "id": "MDRW2100021094.1.24",
          "form": "맛집은 어디갈꺼야?",
          "original_form": "맛집은 어디갈꺼야?",
          "speaker_id": "1",
          "time": "20210828 15:55"
        },
        {
          "id": "MDRW2100021094.1.25",
          "form": "2인실로 방도 두개",
          "original_form": "2인실로 방도 두개",
          "speaker_id": "2",
          "time": "20210828 15:55"
        },
        {
          "id": "MDRW2100021094.1.26",
          "form": "2인실 방 두개? 오호~",
          "original_form": "2인실 방 두개? 오호~",
          "speaker_id": "1",
          "time": "20210828 15:55"
        },
        {
          "id": "MDRW2100021094.1.27",
          "form": "시장에서 회먹고 걸어 다닐것같아",
          "original_form": "시장에서 회먹고 걸어 다닐것같아",
          "speaker_id": "2",
          "time": "20210828 15:55"
        },
        {
          "id": "MDRW2100021094.1.28",
          "form": "4명이 같이 못잔데 4단계라서",
          "original_form": "4명이 같이 못잔데 4단계라서",
          "speaker_id": "2",
          "time": "20210828 15:56"
        },
        {
          "id": "MDRW2100021094.1.29",
          "form": "그렇지 제주도 회가 최고긴 하더라",
          "original_form": "그렇지 제주도 회가 최고긴 하더라",
          "speaker_id": "1",
          "time": "20210828 15:56"
        },
        {
          "id": "MDRW2100021094.1.30",
          "form": "오메기떡은 현지서 먹고 사오는건 아닌거같아",
          "original_form": "오메기떡은 현지서 먹고 사오는건 아닌거같아",
          "speaker_id": "1",
          "time": "20210828 15:56"
        },
        {
          "id": "MDRW2100021094.1.31",
          "form": "이번에는 아무것도 안사오려고",
          "original_form": "이번에는 아무것도 안사오려고",
          "speaker_id": "2",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100021094.1.32",
          "form": "사온거 아직 냉동실에 남았어",
          "original_form": "사온거 아직 냉동실에 남았어",
          "speaker_id": "1",
          "time": "20210828 15:57"
        }
      ]
    }
