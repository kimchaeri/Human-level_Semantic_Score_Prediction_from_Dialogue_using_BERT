      "utterance": [
        {
          "id": "MDRW2100000906.1.1",
          "form": "다이어트 하세여?",
          "original_form": "다이어트 하세여?",
          "speaker_id": "1",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100000906.1.2",
          "form": "하려고 하는데...",
          "original_form": "하려고 하는데...",
          "speaker_id": "2",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100000906.1.3",
          "form": "쉽지 않아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "쉽지 않아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100000906.1.4",
          "form": "ㅋㅋㅋㅋ찌는건 넘 쉬운데말이죠 ..",
          "original_form": "ㅋㅋㅋㅋ찌는건 넘 쉬운데말이죠 ..",
          "speaker_id": "1",
          "time": "20210825 12:09"
        },
        {
          "id": "MDRW2100000906.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ정말요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ정말요",
          "speaker_id": "2",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100000906.1.6",
          "form": "다이어트 하세요?",
          "original_form": "다이어트 하세요?",
          "speaker_id": "2",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100000906.1.7",
          "form": "하고는 있는뎅 천천히 유지하면서 빼려고",
          "original_form": "하고는 있는뎅 천천히 유지하면서 빼려고",
          "speaker_id": "1",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100000906.1.8",
          "form": "운동하고 그래용 ㅎㅎ",
          "original_form": "운동하고 그래용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100000906.1.9",
          "form": "무슨 운동 하시나요",
          "original_form": "무슨 운동 하시나요",
          "speaker_id": "2",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100000906.1.10",
          "form": "원래는 오래걷는거 햇는데 요즘 비가와서",
          "original_form": "원래는 오래걷는거 햇는데 요즘 비가와서",
          "speaker_id": "1",
          "time": "20210825 12:12"
        },
        {
          "id": "MDRW2100000906.1.11",
          "form": "집에서 홈트합니당 ㅎㅎ",
          "original_form": "집에서 홈트합니당 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:13"
        },
        {
          "id": "MDRW2100000906.1.12",
          "form": "아하 저도 홈트해요",
          "original_form": "아하 저도 홈트해요",
          "speaker_id": "2",
          "time": "20210825 12:13"
        },
        {
          "id": "MDRW2100000906.1.13",
          "form": "누구 영상 보시나요",
          "original_form": "누구 영상 보시나요",
          "speaker_id": "2",
          "time": "20210825 12:13"
        },
        {
          "id": "MDRW2100000906.1.14",
          "form": "저는 땅끄부부랑 에이핏요!",
          "original_form": "저는 땅끄부부랑 에이핏요!",
          "speaker_id": "1",
          "time": "20210825 12:15"
        },
        {
          "id": "MDRW2100000906.1.15",
          "form": "님은 ?",
          "original_form": "님은 ?",
          "speaker_id": "1",
          "time": "20210825 12:15"
        },
        {
          "id": "MDRW2100000906.1.16",
          "form": "저도 땅끄요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 땅끄요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:15"
        },
        {
          "id": "MDRW2100000906.1.17",
          "form": "매운맛 그거 하면 죽어요",
          "original_form": "매운맛 그거 하면 죽어요",
