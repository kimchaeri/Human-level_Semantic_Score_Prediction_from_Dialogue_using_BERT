        {
          "id": "MDRW2100030398.1.1",
          "form": "엽기적인 그녀도 원래는 소설이었는데",
          "original_form": "엽기적인 그녀도 원래는 소설이었는데",
          "speaker_id": "2",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030398.1.2",
          "form": "그러게요 그립네요",
          "original_form": "그러게요 그립네요",
          "speaker_id": "1",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030398.1.3",
          "form": "웹소설 기억나나요",
          "original_form": "웹소설 기억나나요",
          "speaker_id": "2",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030398.1.4",
          "form": "인터넷 소설 이었죠",
          "original_form": "인터넷 소설 이었죠",
