      "utterance": [
        {
          "id": "MDRW2100032896.1.1",
          "form": "안 먹어봐서 모르겠어",
          "original_form": "안 먹어봐서 모르겠어",
          "speaker_id": "2",
          "time": "20210906 04:11"
        },
        {
          "id": "MDRW2100032896.1.2",
          "form": "박카스는 맛있는데",
          "original_form": "박카스는 맛있는데",
          "speaker_id": "2",
          "time": "20210906 04:11"
        },
        {
          "id": "MDRW2100032896.1.3",
          "form": "박카스 아이스크림은 ㅋ;;",
          "original_form": "박카스 아이스크림은 ㅋ;;",
          "speaker_id": "2",
          "time": "20210906 04:11"
        },
        {
          "id": "MDRW2100032896.1.4",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 04:12"
        },
        {
          "id": "MDRW2100032896.1.5",
          "form": "아니 취향존중ㅠ",
          "original_form": "아니 취향존중ㅠ",
          "speaker_id": "1",
          "time": "20210906 04:12"
        },
        {
          "id": "MDRW2100032896.1.6",
          "form": "파인애플피자는어때",
          "original_form": "파인애플피자는어때",
          "speaker_id": "1",
          "time": "20210906 04:12"
        },
        {
          "id": "MDRW2100032896.1.7",
          "form": "김치찌개 좋아해?",
          "original_form": "김치찌개 좋아해?",
          "speaker_id": "2",
          "time": "20210906 04:12"
        },
        {
          "id": "MDRW2100032896.1.8",
          "form": "파피 먹는데",
          "original_form": "파피 먹는데",
          "speaker_id": "2",
          "time": "20210906 04:12"
        },
        {
          "id": "MDRW2100032896.1.9",
          "form": "내 돈주곤 안 먹어",
          "original_form": "내 돈주곤 안 먹어",
          "speaker_id": "2",
          "time": "20210906 04:12"
        },
        {
          "id": "MDRW2100032896.1.10",
          "form": "흠...",
          "original_form": "흠...",
          "speaker_id": "1",
          "time": "20210906 04:13"
        },
        {
          "id": "MDRW2100032896.1.11",
          "form": "마라탕은?",
          "original_form": "마라탕은?",
          "speaker_id": "1",
          "time": "20210906 04:13"
        },
        {
          "id": "MDRW2100032896.1.12",
          "form": "호불호음식 다 간다",
          "original_form": "호불호음식 다 간다",
          "speaker_id": "1",
          "time": "20210906 04:13"
        },
        {
          "id": "MDRW2100032896.1.13",
          "form": "그냥 먹는데 가끔?? 맛집 마라탕만 먹음",
          "original_form": "그냥 먹는데 가끔?? 맛집 마라탕만 먹음",
          "speaker_id": "2",
          "time": "20210906 04:13"
        },
        {
          "id": "MDRW2100032896.1.14",
          "form": "ㅋㅋㅋ나 편식 안 해",
          "original_form": "ㅋㅋㅋ나 편식 안 해",
          "speaker_id": "2",
          "time": "20210906 04:13"
        },
        {
          "id": "MDRW2100032896.1.15",
          "form": "ㄹㅇ 입에 넣어주면 삼키긴 함",
          "original_form": "ㄹㅇ 입에 넣어주면 삼키긴 함",
          "speaker_id": "2",
          "time": "20210906 04:13"
        },
        {
          "id": "MDRW2100032896.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ어른들이 좋아하겠다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ어른들이 좋아하겠다",
          "speaker_id": "1",
          "time": "20210906 04:13"
        },
        {
          "id": "MDRW2100032896.1.17",
          "form": "하... 지금은 그냥 배고프고",
          "original_form": "하... 지금은 그냥 배고프고",
          "speaker_id": "1",
          "time": "20210906 04:13"
        },
        {
          "id": "MDRW2100032896.1.18",
          "form": "라면이 삼삼하다",
          "original_form": "라면이 삼삼하다",
          "speaker_id": "1",
          "time": "20210906 04:13"
        },
        {
          "id": "MDRW2100032896.1.19",
          "form": "배고파서 곧 잘듯... 내일 맛잇는거먹게",
          "original_form": "배고파서 곧 잘듯... 내일 맛잇는거먹게",
          "speaker_id": "1",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032896.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아 잘 먹어서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아 잘 먹어서",
          "speaker_id": "2",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032896.1.21",
          "form": "이뻐함",
          "original_form": "이뻐함",
          "speaker_id": "2",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032896.1.22",
          "form": "헐.......",
          "original_form": "헐.......",
          "speaker_id": "2",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032896.1.23",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032896.1.24",
          "form": "근데 나도 자야하긴 해 1교시라",
          "original_form": "근데 나도 자야하긴 해 1교시라",
          "speaker_id": "2",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032896.1.25",
          "form": "어른들우잘먹으면이뻐함",
          "original_form": "어른들우잘먹으면이뻐함",
          "speaker_id": "1",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032896.1.26",
          "form": "아침 뭐먹지",
          "original_form": "아침 뭐먹지",
          "speaker_id": "2",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032896.1.27",
          "form": "우리 또 볼수있겠지?",
          "original_form": "우리 또 볼수있겠지?",
          "speaker_id": "1",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032896.1.28",
          "form": "먹을 시간 없을듯",
          "original_form": "먹을 시간 없을듯",
          "speaker_id": "2",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032896.1.29",
          "form": "학식ㅋㅋ",
          "original_form": "학식ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032896.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아니 비대면이에요 언니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아니 비대면이에요 언니",
          "speaker_id": "2",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032896.1.31",
          "form": "아니면ㅍ",
          "original_form": "아니면ㅍ",
          "speaker_id": "1",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032896.1.32",
          "form": "빵",
          "original_form": "빵",
          "speaker_id": "1",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032896.1.33",
          "form": "또 말 걸게 ㅎ",
          "original_form": "또 말 걸게 ㅎ",
          "speaker_id": "2",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032896.1.34",
          "form": "아 빵 먹어야겠다",
          "original_form": "아 빵 먹어야겠다",
          "speaker_id": "2",
          "time": "20210906 04:14"
        },
        {
          "id": "MDRW2100032896.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ나이들었네... 내가",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ나이들었네... 내가",
          "speaker_id": "1",
          "time": "20210906 04:15"
        },
        {
          "id": "MDRW2100032896.1.36",
          "form": "푹자고 맛잇는아침먹어",
          "original_form": "푹자고 맛잇는아침먹어",
          "speaker_id": "1",
          "time": "20210906 04:15"
        },
        {
          "id": "MDRW2100032896.1.37",
          "form": "올리부치아바타에 발사믹이랑 올리브유랑 리코타",
          "original_form": "올리부치아바타에 발사믹이랑 올리브유랑 리코타",
