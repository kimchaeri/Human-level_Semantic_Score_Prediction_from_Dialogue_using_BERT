          "time": "20210907 11:18"
        },
        {
          "id": "MDRW2100042910.1.7",
          "form": "진짜 그론 것 같음",
          "original_form": "진짜 그론 것 같음",
          "speaker_id": "2",
          "time": "20210907 11:18"
        },
        {
          "id": "MDRW2100042910.1.8",
          "form": "그래서학생들도",
          "original_form": "그래서학생들도",
          "speaker_id": "1",
          "time": "20210907 11:18"
        },
        {
          "id": "MDRW2100042910.1.9",
