      "utterance": [
        {
          "id": "MDRW2100042998.1.1",
          "form": "기억에남는 여행지 잇으신가요",
          "original_form": "기억에남는 여행지 잇으신가요",
          "speaker_id": "1",
          "time": "20210907 12:52"
        },
        {
          "id": "MDRW2100042998.1.2",
          "form": "모든 여행이 다 나름의 추억이 있어서",
          "original_form": "모든 여행이 다 나름의 추억이 있어서",
          "speaker_id": "2",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100042998.1.3",
          "form": "그것도맞네용",
          "original_form": "그것도맞네용",
          "speaker_id": "1",
          "time": "20210907 13:04"
        },
        {
          "id": "MDRW2100042998.1.4",
          "form": "name1님은 어디 여행지가 좋으셨어요?",
          "original_form": "&name1&님은 어디 여행지가 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210907 13:04"
        },
        {
          "id": "MDRW2100042998.1.5",
          "form": "저도 다좋앗어서",
          "original_form": "저도 다좋앗어서",
          "speaker_id": "1",
          "time": "20210907 13:04"
        },
        {
          "id": "MDRW2100042998.1.6",
          "form": "젤 최근에 여름휴가 울릉도 독도 다녀왓어용",
          "original_form": "젤 최근에 여름휴가 울릉도 독도 다녀왓어용",
          "speaker_id": "1",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100042998.1.7",
          "form": "오 저도 울릉도 독도 가보고 싶었는데!!",
          "original_form": "오 저도 울릉도 독도 가보고 싶었는데!!",
          "speaker_id": "2",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100042998.1.8",
          "form": "배 타고 가신거죠?",
          "original_form": "배 타고 가신거죠?",
          "speaker_id": "2",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100042998.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 13:08"
        },
        {
          "id": "MDRW2100042998.1.10",
          "form": "독도입도성공해서욤",
          "original_form": "독도입도성공해서욤",
          "speaker_id": "1",
          "time": "20210907 13:08"
        },
        {
          "id": "MDRW2100042998.1.11",
          "form": "운이 좋아야 갈 수 있다던데",
          "original_form": "운이 좋아야 갈 수 있다던데",
          "speaker_id": "2",
          "time": "20210907 13:08"
        },
        {
          "id": "MDRW2100042998.1.12",
          "form": "축하드려요",
          "original_form": "축하드려요",
          "speaker_id": "2",
          "time": "20210907 13:08"
        },
        {
          "id": "MDRW2100042998.1.13",
          "form": "숙박이나 이런 것도 괜찮았나요?",
          "original_form": "숙박이나 이런 것도 괜찮았나요?",
          "speaker_id": "2",
          "time": "20210907 13:08"
        },
        {
          "id": "MDRW2100042998.1.14",
          "form": "아 그런건 진짜",
          "original_form": "아 그런건 진짜",
          "speaker_id": "1",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100042998.1.15",
          "form": "내려노으셔야해용",
          "original_form": "내려노으셔야해용",
          "speaker_id": "1",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100042998.1.16",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100042998.1.17",
          "form": "물가가많이비싼데",
          "original_form": "물가가많이비싼데",
          "speaker_id": "1",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100042998.1.18",
          "form": "시설은열악한편이에요 ㅎㅎ",
          "original_form": "시설은열악한편이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100042998.1.19",
          "form": "독도에 들어갔다는 걸로",
          "original_form": "독도에 들어갔다는 걸로",
          "speaker_id": "2",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100042998.1.20",
          "form": "만족해야하나봐요",
          "original_form": "만족해야하나봐요",
          "speaker_id": "2",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100042998.1.21",
          "form": "그래도 강추드림다!!!!",
          "original_form": "그래도 강추드림다!!!!",
