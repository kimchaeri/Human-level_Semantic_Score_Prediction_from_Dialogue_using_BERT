      "utterance": [
        {
          "id": "MDRW2100011650.1.1",
          "form": "꿈의집 오스틴이랑은",
          "original_form": "꿈의집 오스틴이랑은",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011650.1.2",
          "form": "꿈의 정원이",
          "original_form": "꿈의 정원이",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011650.1.3",
          "form": "요새 잘지내니?",
          "original_form": "요새 잘지내니?",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011650.1.4",
          "form": "세계 최고의 게임임",
          "original_form": "세계 최고의 게임임",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011650.1.5",
          "form": "무슨소리야?",
          "original_form": "무슨소리야?",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011650.1.6",
          "form": "그리고 집이 아니라 정원임,,,",
          "original_form": "그리고 집이 아니라 정원임,,,",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011650.1.7",
          "form": "올해로 16주년을 맞이하는",
          "original_form": "올해로 16주년을 맞이하는",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011650.1.8",
          "form": "내 오스틴 무시하지 말라",
          "original_form": "내 오스틴 무시하지 말라",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011650.1.9",
          "form": "테일즈런너야말로",
          "original_form": "테일즈런너야말로",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011650.1.10",
          "form": "세계최고게임이지",
          "original_form": "세계최고게임이지",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011650.1.11",
          "form": "난 대 저택 정원을 소유하고 있는",
          "original_form": "난 대 저택 정원을 소유하고 있는",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011650.1.12",
          "form": "레벨 6000대의 사람이다",
          "original_form": "레벨 6000대의 사람이다",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011650.1.13",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011650.1.14",
          "form": "난 테런 플레이시간만",
          "original_form": "난 테런 플레이시간만",
          "speaker_id": "1",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011650.1.15",
          "form": "6400시간이다",
          "original_form": "6400시간이다",
          "speaker_id": "1",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011650.1.16",
          "form": "오스틴의 대머리 얼마나 멋진지 넌 모를꺼다",
          "original_form": "오스틴의 대머리 얼마나 멋진지 넌 모를꺼다",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011650.1.17",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011650.1.18",
          "form": "잠옷 입었을때 얼마나 역겨운데",
          "original_form": "잠옷 입었을때 얼마나 역겨운데",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011650.1.19",
          "form": "name2님은 대머리캐릭터가 취향이시구나",
          "original_form": "&name2&님은 대머리캐릭터가 취향이시구나",
          "speaker_id": "1",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011650.1.20",
          "form": "다른게임은 안하시나요",
          "original_form": "다른게임은 안하시나요",
          "speaker_id": "1",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011650.1.21",
          "form": "그 역겨움을 이겨내는 난",
          "original_form": "그 역겨움을 이겨내는 난",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011650.1.22",
          "form": "진정한 승자지",
          "original_form": "진정한 승자지",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011650.1.23",
          "form": "카페 게임 하다가",
          "original_form": "카페 게임 하다가",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011650.1.24",
          "form": "그만둠ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그만둠ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011650.1.25",
          "form": "난 모바일게임은 안한지오래도ㅔㄴ듯",
          "original_form": "난 모바일게임은 안한지오래도ㅔㄴ듯",
          "speaker_id": "1",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011650.1.26",
          "form": "컴퓨터 좋은거로 맞춰서 오버워치 하고싶다",
          "original_form": "컴퓨터 좋은거로 맞춰서 오버워치 하고싶다",
          "speaker_id": "1",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011650.1.27",
          "form": "손님이 빨리 딜라고 재촉하는게 너무 짜증남",
          "original_form": "손님이 빨리 딜라고 재촉하는게 너무 짜증남",
