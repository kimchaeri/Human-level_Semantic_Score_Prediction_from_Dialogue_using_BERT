      "utterance": [
        {
          "id": "MDRW2100021041.1.1",
          "form": "싸이월드 많이 하셨나요",
          "original_form": "싸이월드 많이 하셨나요",
          "speaker_id": "1",
          "time": "20210828 15:16"
        },
        {
          "id": "MDRW2100021041.1.2",
          "form": "ㅋㅋㅋ한참 했었져 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ한참 했었져 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:17"
        },
        {
          "id": "MDRW2100021041.1.3",
          "form": "뭐 많이 하셨나요",
          "original_form": "뭐 많이 하셨나요",
          "speaker_id": "1",
          "time": "20210828 15:18"
        },
        {
          "id": "MDRW2100021041.1.4",
          "form": "그냥 일촌평 쓰는거랑 방명록 쓰는 거요 ㅋㅋㅋ",
          "original_form": "그냥 일촌평 쓰는거랑 방명록 쓰는 거요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100021041.1.5",
          "form": "사진업로드도 하셨나요,",
          "original_form": "사진업로드도 하셨나요,",
          "speaker_id": "1",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021041.1.6",
          "form": "넼ㅋㅋㅋ 흑역사가 가득하죠 아주",
          "original_form": "넼ㅋㅋㅋ 흑역사가 가득하죠 아주",
          "speaker_id": "2",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021041.1.7",
          "form": "교환일기는 안쓰셨나요, 다이어리 ㅋ",
          "original_form": "교환일기는 안쓰셨나요, 다이어리 ㅋ",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100021041.1.8",
          "form": "앝ㅋㅋㅋ 썼져  다 하셨던거죠?",
          "original_form": "앝ㅋㅋㅋ 썼져  다 하셨던거죠?",
          "speaker_id": "2",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100021041.1.9",
          "form": "네 동기들이랑도 하고...커플이랑도 했나..기억이...ㅋ",
          "original_form": "네 동기들이랑도 하고...커플이랑도 했나..기억이...ㅋ",
          "speaker_id": "1",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100021041.1.10",
          "form": "저두 친구들이랑도 하고 ㅋㅋㅋ",
          "original_form": "저두 친구들이랑도 하고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100021041.1.11",
          "form": "파도타기 기억나세요?",
          "original_form": "파도타기 기억나세요?",
          "speaker_id": "1",
          "time": "20210828 15:31"
        },
        {
          "id": "MDRW2100021041.1.12",
          "form": "넹 자주 했어요 ㅋㅋㅋ",
          "original_form": "넹 자주 했어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021041.1.13",
          "form": "싸이월드 다시 해보실 건가여?",
          "original_form": "싸이월드 다시 해보실 건가여?",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021041.1.14",
          "form": "흑역사가 가득하지만 궁금하긴 해요",
          "original_form": "흑역사가 가득하지만 궁금하긴 해요",
          "speaker_id": "1",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021041.1.15",
          "form": "부활하면 얼른 로그인해서 자료 다 정리하려구요^^",
          "original_form": "부활하면 얼른 로그인해서 자료 다 정리하려구요^^",
          "speaker_id": "2",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021041.1.16",
          "form": "아직 부활전인보네요^^",
          "original_form": "아직 부활전인보네요^^",
          "speaker_id": "1",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021041.1.17",
          "form": "부활한다고 했다가 중국 디도스공격인가 뭔가 해서 미뤄지더라구요",
          "original_form": "부활한다고 했다가 중국 디도스공격인가 뭔가 해서 미뤄지더라구요",
          "speaker_id": "2",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021041.1.18",
