      "utterance": [
        {
          "id": "MDRW2100015552.1.1",
          "form": "앗ㅋㅋㅋㅋㅋ그르게",
          "original_form": "앗ㅋㅋㅋㅋㅋ그르게",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015552.1.2",
          "form": "뭐 가지고 싶은거 잇어?",
          "original_form": "뭐 가지고 싶은거 잇어?",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015552.1.3",
          "form": "아니 아직!",
          "original_form": "아니 아직!",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015552.1.4",
          "form": "흠 너한테 필요한게 뭘까",
          "original_form": "흠 너한테 필요한게 뭘까",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015552.1.5",
          "form": "너 마음?ㅎ",
          "original_form": "너 마음?ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015552.1.6",
          "form": "그건 별로고 먹거리랑 생활용품중에 뭐가 좋아?",
          "original_form": "그건 별로고 먹거리랑 생활용품중에 뭐가 좋아?",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015552.1.7",
          "form": "음 먹는 게 좋아ㅎㅎㅎ",
          "original_form": "음 먹는 게 좋아ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015552.1.8",
          "form": "너 뭐조아하지?",
          "original_form": "너 뭐조아하지?",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015552.1.9",
          "form": "난 디저트류 좋아해",
          "original_form": "난 디저트류 좋아해",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015552.1.10",
          "form": "근데 옷 종류도 좋고 생활용품도 좋앙",
          "original_form": "근데 옷 종류도 좋고 생활용품도 좋앙",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015552.1.11",
          "form": "그러면 너 편하게 입을만한 옷이 별로 없댔으니까 옷사줄게",
          "original_form": "그러면 너 편하게 입을만한 옷이 별로 없댔으니까 옷사줄게",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015552.1.12",
          "form": "헐 나 같은 거지를 위해ㅠㅠ",
          "original_form": "헐 나 같은 거지를 위해ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015552.1.13",
          "form": "웅..?",
          "original_form": "웅..?",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015552.1.14",
          "form": "반팔이랑 긴팔중에 뭐줄까?",
          "original_form": "반팔이랑 긴팔중에 뭐줄까?",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015552.1.15",
          "form": "ㅠㅠㅠ나 원피스나 반지 사주랑!",
          "original_form": "ㅠㅠㅠ나 원피스나 반지 사주랑!",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015552.1.16",
          "form": "모야 편한 옷 사주고시퍼",
          "original_form": "모야 편한 옷 사주고시퍼",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015552.1.17",
          "form": "너 원피스랑 반지많잖아",
          "original_form": "너 원피스랑 반지많잖아",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015552.1.18",
          "form": "쳇",
          "original_form": "쳇",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015552.1.19",
          "form": "이제 가을이니까 긴팔 사주랑",
          "original_form": "이제 가을이니까 긴팔 사주랑",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015552.1.20",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015552.1.21",
          "form": "색상은?",
          "original_form": "색상은?",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015552.1.22",
          "form": "검은색이나 흰색이나 하늘색으로!",
          "original_form": "검은색이나 흰색이나 하늘색으로!",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015552.1.23",
          "form": "검정은 너 많으니까 흰색이랑 하늘색 살까?",
          "original_form": "검정은 너 많으니까 흰색이랑 하늘색 살까?",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015552.1.24",
          "form": "흰색!",
          "original_form": "흰색!",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015552.1.25",
          "form": "둘다 사줄게!",
          "original_form": "둘다 사줄게!",
          "speaker_id": "1",
          "time": "20210826 00:04"
        }
      ]
    }
  ]
