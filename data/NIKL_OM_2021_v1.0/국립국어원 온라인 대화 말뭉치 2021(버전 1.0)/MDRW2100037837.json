      "utterance": [
        {
          "id": "MDRW2100037837.1.1",
          "form": "감우 호두 알베도 다 싫어.....",
          "original_form": "감우 호두 알베도 다 싫어.....",
          "speaker_id": "2",
          "time": "20210901 19:24"
        },
        {
          "id": "MDRW2100037837.1.2",
          "form": "호두도 좀 구린게",
          "original_form": "호두도 좀 구린게",
          "speaker_id": "2",
          "time": "20210901 19:24"
        },
        {
          "id": "MDRW2100037837.1.3",
          "form": "걔도 스킬쓰면 피 닳는데",
          "original_form": "걔도 스킬쓰면 피 닳는데",
          "speaker_id": "2",
          "time": "20210901 19:24"
        },
        {
          "id": "MDRW2100037837.1.4",
          "form": "필드에 불까지 붙어서",
          "original_form": "필드에 불까지 붙어서",
          "speaker_id": "2",
          "time": "20210901 19:24"
        },
        {
          "id": "MDRW2100037837.1.5",
          "form": "겁나 닳아",
          "original_form": "겁나 닳아",
          "speaker_id": "2",
          "time": "20210901 19:24"
        },
        {
          "id": "MDRW2100037837.1.6",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210901 19:24"
        },
        {
          "id": "MDRW2100037837.1.7",
          "form": "그래도 딜러로 나름 상위 티어던데",
          "original_form": "그래도 딜러로 나름 상위 티어던데",
          "speaker_id": "1",
          "time": "20210901 19:25"
        },
        {
          "id": "MDRW2100037837.1.8",
          "form": "그치만 나 호두 체험할때",
          "original_form": "그치만 나 호두 체험할때",
          "speaker_id": "2",
          "time": "20210901 19:25"
        },
        {
          "id": "MDRW2100037837.1.9",
          "form": "밥 겁나 먹였어",
          "original_form": "밥 겁나 먹였어",
          "speaker_id": "2",
          "time": "20210901 19:25"
        },
        {
          "id": "MDRW2100037837.1.10",
          "form": "난 호두 체험",
          "original_form": "난 호두 체험",
          "speaker_id": "1",
          "time": "20210901 19:25"
        },
        {
          "id": "MDRW2100037837.1.11",
          "form": "퍼즐 못풀어서",
          "original_form": "퍼즐 못풀어서",
          "speaker_id": "1",
          "time": "20210901 19:25"
        },
        {
          "id": "MDRW2100037837.1.12",
          "form": "나도ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나도ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:25"
        },
        {
          "id": "MDRW2100037837.1.13",
          "form": "맵 빙글빙글 돌앗던 기억밖에...",
          "original_form": "맵 빙글빙글 돌앗던 기억밖에...",
          "speaker_id": "1",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100037837.1.14",
          "form": "거기만 한 여덟바퀴를",
          "original_form": "거기만 한 여덟바퀴를",
          "speaker_id": "2",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100037837.1.15",
          "form": "도는데",
          "original_form": "도는데",
          "speaker_id": "2",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100037837.1.16",
          "form": "중간에 불붙여서 길 터야하는부분",
          "original_form": "중간에 불붙여서 길 터야하는부분",
          "speaker_id": "2",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100037837.1.17",
          "form": "스킬 쓸때마다 애가",
          "original_form": "스킬 쓸때마다 애가",
          "speaker_id": "2",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100037837.1.18",
          "form": "다 죽)어가는거야......",
          "original_form": "다 죽)어가는거야......",
          "speaker_id": "2",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100037837.1.19",
          "form": "물론 소는 그때",
          "original_form": "물론 소는 그때",
          "speaker_id": "2",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100037837.1.20",
          "form": "체력 증가버프걸려있어서",
          "original_form": "체력 증가버프걸려있어서",
          "speaker_id": "2",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100037837.1.21",
          "form": "둘이 체감차이 크게 나는걸수도 있긴ㅊ한데",
          "original_form": "둘이 체감차이 크게 나는걸수도 있긴ㅊ한데",
          "speaker_id": "2",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100037837.1.22",
          "form": "ㅇㅇ 소 체험판이 좀 사기긴 햇어",
          "original_form": "ㅇㅇ 소 체험판이 좀 사기긴 햇어",
          "speaker_id": "1",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100037837.1.23",
          "form": "와 라이덴",
          "original_form": "와 라이덴",
          "speaker_id": "1",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100037837.1.24",
          "form": "2돌하면 사기캐된대",
          "original_form": "2돌하면 사기캐된대",
          "speaker_id": "1",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100037837.1.25",
          "form": "그치만",
          "original_form": "그치만",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100037837.1.26",
          "form": "2돌을 억케해요......",
          "original_form": "2돌을 억케해요......",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100037837.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100037837.1.28",
          "form": "그리고 벤티랑 종려는",
          "original_form": "그리고 벤티랑 종려는",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100037837.1.29",
          "form": "그냥 사기캐엿는데",
          "original_form": "그냥 사기캐엿는데",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100037837.1.30",
          "form": "그래서 그 급은 아니라고 말 나오긷 하나봐",
          "original_form": "그래서 그 급은 아니라고 말 나오긷 하나봐",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100037837.1.31",
          "form": "왜 너는 2돌 해야 사기캐니",
          "original_form": "왜 너는 2돌 해야 사기캐니",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100037837.1.32",
          "form": "역시 구관이 명관인가",
          "original_form": "역시 구관이 명관인가",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100037837.1.33",
          "form": "ㅋㅋㅋㅋㅋ 초대 신이 아니라서...?",
          "original_form": "ㅋㅋㅋㅋㅋ 초대 신이 아니라서...?",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100037837.1.34",
          "form": "그닉가....",
          "original_form": "그닉가....",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100037837.1.35",
          "form": "2돌하면 상대 저항력 무시하고 데미지 넣는대",
          "original_form": "2돌하면 상대 저항력 무시하고 데미지 넣는대",
          "speaker_id": "1",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100037837.1.36",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100037837.1.37",
          "form": "개사기",
          "original_form": "개사기",
          "speaker_id": "2",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100037837.1.38",
          "form": "근데 여기서",
          "original_form": "근데 여기서",
          "speaker_id": "2",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100037837.1.39",
          "form": "추가 2천장을한대도",
          "original_form": "추가 2천장을한대도",
          "speaker_id": "2",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100037837.1.40",
          "form": "라이덴이 둘 나오진",
          "original_form": "라이덴이 둘 나오진",
          "speaker_id": "2",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100037837.1.41",
          "form": "않을텐데",
          "original_form": "않을텐데",
          "speaker_id": "2",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100037837.1.42",
          "form": "그걸 억케해........",
          "original_form": "그걸 억케해........",
          "speaker_id": "2",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100037837.1.43",
          "form": "그렇긴 하지 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그렇긴 하지 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:07"
        },
        {
          "id": "MDRW2100037837.1.44",
          "form": "2돌하려면 최대 4천장...",
          "original_form": "2돌하려면 최대 4천장...",
