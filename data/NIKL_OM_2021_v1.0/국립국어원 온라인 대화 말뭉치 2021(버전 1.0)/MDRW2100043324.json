      "utterance": [
        {
          "id": "MDRW2100043324.1.1",
          "form": "잘생긴 사람이랑 결혼하면 좋겠어 ㅋㅋㅋ",
          "original_form": "잘생긴 사람이랑 결혼하면 좋겠어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100043324.1.2",
          "form": "오 ㅋㅋㅋ 난 귀여운사람이 좋아",
          "original_form": "오 ㅋㅋㅋ 난 귀여운사람이 좋아",
          "speaker_id": "2",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100043324.1.3",
          "form": "우와 이상형 있어?,,",
          "original_form": "우와 이상형 있어?,,",
          "speaker_id": "1",
          "time": "20210907 17:41"
        },
        {
          "id": "MDRW2100043324.1.4",
          "form": "연상인데 귀여운 사람!!",
          "original_form": "연상인데 귀여운 사람!!",
          "speaker_id": "2",
          "time": "20210907 17:41"
        },
        {
          "id": "MDRW2100043324.1.5",
          "form": "네 이상형은?",
          "original_form": "네 이상형은?",
          "speaker_id": "2",
          "time": "20210907 17:41"
        },
        {
          "id": "MDRW2100043324.1.6",
          "form": "우와 그렇구나",
          "original_form": "우와 그렇구나",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100043324.1.7",
          "form": "나는 잘생기고 똑똑하고 말 잘 통하는 동갑",
          "original_form": "나는 잘생기고 똑똑하고 말 잘 통하는 동갑",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100043324.1.8",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100043324.1.9",
          "form": "다 보는 대신에 키는 안봐 ㅋㅋㅋㅋ",
          "original_form": "다 보는 대신에 키는 안봐 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:43"
        },
        {
          "id": "MDRW2100043324.1.10",
          "form": "오 ㅋㅋㅋㅋ 잘생긴 사람이 극악이잖아",
          "original_form": "오 ㅋㅋㅋㅋ 잘생긴 사람이 극악이잖아",
          "speaker_id": "2",
          "time": "20210907 17:44"
        },
        {
          "id": "MDRW2100043324.1.11",
          "form": "한국에서..연예인 말고 찾기 힘들텐데...",
          "original_form": "한국에서..연예인 말고 찾기 힘들텐데...",
          "speaker_id": "2",
          "time": "20210907 17:44"
        },
        {
          "id": "MDRW2100043324.1.12",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043324.1.13",
          "form": "연예인말곤 없지ㅠㅠ슬프다 ㅋㅋ",
          "original_form": "연예인말곤 없지ㅠㅠ슬프다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043324.1.14",
          "form": "ㅠㅠ ... 이렇게 된거 연예인 꼬시기 도전하자",
          "original_form": "ㅠㅠ ... 이렇게 된거 연예인 꼬시기 도전하자",
          "speaker_id": "2",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043324.1.15",
          "form": "할 수 있다!!!",
          "original_form": "할 수 있다!!!",
          "speaker_id": "2",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043324.1.16",
          "form": "앜ㅋㅋㅋㅋㅋ 그래",
          "original_form": "앜ㅋㅋㅋㅋㅋ 그래",
          "speaker_id": "1",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043324.1.17",
          "form": "지금 남친이랑 결혼 하고 싶은데",
          "original_form": "지금 남친이랑 결혼 하고 싶은데",
          "speaker_id": "1",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043324.1.18",
          "form": "우리 해외 장거리여",
          "original_form": "우리 해외 장거리여",
          "speaker_id": "1",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043324.1.19",
          "form": "헐 진짜???",
          "original_form": "헐 진짜???",
          "speaker_id": "2",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043324.1.20",
