      "utterance": [
        {
          "id": "MDRW2100037786.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100037786.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037786.1.3",
          "form": "공연 보는거 좋아하세요?",
          "original_form": "공연 보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100037786.1.4",
          "form": "좋아해요!",
          "original_form": "좋아해요!",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100037786.1.5",
          "form": "공연 자주 보세요??",
          "original_form": "공연 자주 보세요??",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100037786.1.6",
          "form": "공연 좋아하지만 자주 보지는 못 해요ㅜㅜ",
          "original_form": "공연 좋아하지만 자주 보지는 못 해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100037786.1.7",
          "form": "저도 자주 못 봐요ㅠㅠ",
          "original_form": "저도 자주 못 봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100037786.1.8",
          "form": "시간 맞추기가 어렵네요ㅠ",
          "original_form": "시간 맞추기가 어렵네요ㅠ",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100037786.1.9",
          "form": "맞아요 시간 맞추기도 힘들고\r\n수도권이 아니다보니 공연 기회도 많이 없구요",
          "original_form": "맞아요 시간 맞추기도 힘들고\r\n수도권이 아니다보니 공연 기회도 많이 없구요",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100037786.1.10",
          "form": "그쵸ㅠ 지방에는 늘 하던 공연만 하고 그러니까 더 기회가 없는 것 같아요!",
          "original_form": "그쵸ㅠ 지방에는 늘 하던 공연만 하고 그러니까 더 기회가 없는 것 같아요!",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100037786.1.11",
          "form": "맞아요ㅠㅠ\r\n마음같아서는 뮤지컬 연극 콘서트들 다양하게 즐기고 싶은데 그러지 못하니 아쉬워요ㅜㅜ",
          "original_form": "맞아요ㅠㅠ\r\n마음같아서는 뮤지컬 연극 콘서트들 다양하게 즐기고 싶은데 그러지 못하니 아쉬워요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100037786.1.12",
          "form": "그쵸! 공급이 한정적이니 재수요가 없죠...",
          "original_form": "그쵸! 공급이 한정적이니 재수요가 없죠...",
          "speaker_id": "2",
          "time": "20210901 20:18"
        },
        {
          "id": "MDRW2100037786.1.13",
          "form": "이걸 좀 알아주면 좋으련만 ㅠㅠ",
          "original_form": "이걸 좀 알아주면 좋으련만 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 20:18"
        },
        {
          "id": "MDRW2100037786.1.14",
          "form": "유명한 배우들은 바라지도않아요 ㅋㅋㅋㅋ 공연만 다양하게 해줬으면 좋겠어요ㅠ",
          "original_form": "유명한 배우들은 바라지도않아요 ㅋㅋㅋㅋ 공연만 다양하게 해줬으면 좋겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100037786.1.15",
          "form": "맞아요ㅠㅠ name2님도 지방에 거주에 하시나봐요\r\n이 고충에 공감하시는거보니ㅜㅜ",
          "original_form": "맞아요ㅠㅠ &name2&님도 지방에 거주에 하시나봐요\r\n이 고충에 공감하시는거보니ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100037786.1.16",
          "form": "네 지방인입니다ㅠㅠ",
          "original_form": "네 지방인입니다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100037786.1.17",
          "form": "뮤지컬 좋아하고싶은데 기회를 안주네요 ㅋㅋㅋㅋ",
          "original_form": "뮤지컬 좋아하고싶은데 기회를 안주네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100037786.1.18",
          "form": "그러니까요ㅋㅋㅋ",
          "original_form": "그러니까요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100037786.1.19",
          "form": "한 번씩 서울로 올라가야 볼 수 있을텐데",
          "original_form": "한 번씩 서울로 올라가야 볼 수 있을텐데",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100037786.1.20",
          "form": "가려면 시간도 내야하고 왕복 차비도 들고..\r\n마음먹고 가야하네요ㅜㅜ",
          "original_form": "가려면 시간도 내야하고 왕복 차비도 들고..\r\n마음먹고 가야하네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 20:45"
        }
      ]
    }
