      "utterance": [
        {
          "id": "MDRW2100046243.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211018 13:25"
        },
        {
          "id": "MDRW2100046243.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211018 14:05"
        },
        {
          "id": "MDRW2100046243.1.3",
          "form": "공연 관람하는거 좋아하시나요?",
          "original_form": "공연 관람하는거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20211018 14:05"
        },
        {
          "id": "MDRW2100046243.1.4",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20211018 14:06"
        },
        {
          "id": "MDRW2100046243.1.5",
          "form": "뮤지컬이나 연극도 좋아하시나요?!",
          "original_form": "뮤지컬이나 연극도 좋아하시나요?!",
          "speaker_id": "2",
          "time": "20211018 14:06"
        },
        {
          "id": "MDRW2100046243.1.6",
          "form": "네 둘다 좋아해요",
          "original_form": "네 둘다 좋아해요",
          "speaker_id": "1",
          "time": "20211018 14:07"
        },
        {
          "id": "MDRW2100046243.1.7",
          "form": "우와 저도 좋아해요!",
          "original_form": "우와 저도 좋아해요!",
          "speaker_id": "2",
          "time": "20211018 14:07"
        },
        {
          "id": "MDRW2100046243.1.8",
          "form": "제일 재밌게 보신 뮤지컬이 뭐에요?",
          "original_form": "제일 재밌게 보신 뮤지컬이 뭐에요?",
          "speaker_id": "2",
          "time": "20211018 14:07"
        },
        {
          "id": "MDRW2100046243.1.9",
          "form": "제목이 잘 기억이 안나네요",
          "original_form": "제목이 잘 기억이 안나네요",
          "speaker_id": "1",
          "time": "20211018 14:07"
        },
        {
          "id": "MDRW2100046243.1.10",
          "form": "무슨 도시인데..",
          "original_form": "무슨 도시인데..",
          "speaker_id": "1",
          "time": "20211018 14:07"
        },
        {
          "id": "MDRW2100046243.1.11",
          "form": "아 뭐더라",
          "original_form": "아 뭐더라",
          "speaker_id": "2",
          "time": "20211018 14:07"
        },
        {
          "id": "MDRW2100046243.1.12",
          "form": "두도시이야기인가?!",
          "original_form": "두도시이야기인가?!",
          "speaker_id": "2",
          "time": "20211018 14:07"
        },
        {
          "id": "MDRW2100046243.1.13",
          "form": "아닌것같에요ㅋㅋ",
          "original_form": "아닌것같에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20211018 14:08"
        },
        {
          "id": "MDRW2100046243.1.14",
          "form": "제목이 도시가 아닐수도있어요",
          "original_form": "제목이 도시가 아닐수도있어요",
          "speaker_id": "1",
          "time": "20211018 14:08"
        },
        {
          "id": "MDRW2100046243.1.15",
          "form": "오 뭐지",
          "original_form": "오 뭐지",
          "speaker_id": "2",
          "time": "20211018 14:08"
        },
        {
          "id": "MDRW2100046243.1.16",
          "form": "첫장면에서 검은의상 입고 시작되는건데",
          "original_form": "첫장면에서 검은의상 입고 시작되는건데",
          "speaker_id": "1",
          "time": "20211018 14:09"
        },
        {
          "id": "MDRW2100046243.1.17",
          "form": "잘생긱이 안나네요",
          "original_form": "잘생긱이 안나네요",
          "speaker_id": "1",
          "time": "20211018 14:09"
        },
        {
          "id": "MDRW2100046243.1.18",
          "form": "뭐지 엄청 궁금하네요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "뭐지 엄청 궁금하네요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211018 14:09"
        },
        {
          "id": "MDRW2100046243.1.19",
          "form": "저도 궁금하네요ㅋㅋ",
          "original_form": "저도 궁금하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20211018 14:09"
        },
        {
          "id": "MDRW2100046243.1.20",
          "form": "대학로 소극장 좋아하시나요",
          "original_form": "대학로 소극장 좋아하시나요",
          "speaker_id": "1",
          "time": "20211018 14:10"
        },
        {
          "id": "MDRW2100046243.1.21",
          "form": "저는 한 번 밖에 못봤어요 ㅠㅠ",
          "original_form": "저는 한 번 밖에 못봤어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211018 14:10"
        },
        {
          "id": "MDRW2100046243.1.22",
          "form": "뭐더라..",
          "original_form": "뭐더라..",
          "speaker_id": "2",
          "time": "20211018 14:10"
        },
        {
          "id": "MDRW2100046243.1.23",
          "form": "미드나잇이라는",
          "original_form": "미드나잇이라는",
          "speaker_id": "2",
          "time": "20211018 14:10"
        },
        {
          "id": "MDRW2100046243.1.24",
          "form": "뮤지컬이었던거같아요",
          "original_form": "뮤지컬이었던거같아요",
          "speaker_id": "2",
          "time": "20211018 14:10"
        },
        {
          "id": "MDRW2100046243.1.25",
          "form": "좁은공간에서 좁게 보는것도 나름 좋아요ㅋㅋ",
          "original_form": "좁은공간에서 좁게 보는것도 나름 좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20211018 14:10"
        },
        {
          "id": "MDRW2100046243.1.26",
          "form": "근데 저는 개인적으로 대극장 뮤지컬이 더 취향이더라구요 ㅎㅎㅎ",
          "original_form": "근데 저는 개인적으로 대극장 뮤지컬이 더 취향이더라구요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211018 14:10"
        }
      ]
