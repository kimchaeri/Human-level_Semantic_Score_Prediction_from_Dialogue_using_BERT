      "utterance": [
        {
          "id": "MDRW2100044850.1.1",
          "form": "굽네도 맛있죠",
          "original_form": "굽네도 맛있죠",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044850.1.2",
          "form": "굽네에선 어떤 메누 가장 좋아하세요?",
          "original_form": "굽네에선 어떤 메누 가장 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044850.1.3",
          "form": "고 추 바사삭",
          "original_form": "고 추 바사삭",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044850.1.4",
          "form": "이요",
          "original_form": "이요",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044850.1.5",
          "form": "ㅋㅋㅋㅋ저두요 고 추 바사삭 최고!!",
          "original_form": "ㅋㅋㅋㅋ저두요 고 추 바사삭 최고!!",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044850.1.6",
          "form": "네네 맛있죠",
          "original_form": "네네 맛있죠",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044850.1.7",
          "form": "사실 치킨은 가리지않고 다 좋아해요",
          "original_form": "사실 치킨은 가리지않고 다 좋아해요",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044850.1.8",
          "form": "ㅋㅋ저두요 ㅋㅋㅋ 원래 굽네에",
          "original_form": "ㅋㅋ저두요 ㅋㅋㅋ 원래 굽네에",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044850.1.9",
          "form": "마라볼케이노도 맛있었는데 이제 단종인거보더라구요ㅠㅠ",
          "original_form": "마라볼케이노도 맛있었는데 이제 단종인거보더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044850.1.10",
          "form": "아 그런가요",
          "original_form": "아 그런가요",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044850.1.11",
          "form": "마라 개성있는데",
          "original_form": "마라 개성있는데",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044850.1.12",
          "form": "네 적당히 마라맛 나서 좋았엇어요ㅠㅠㅠ",
          "original_form": "네 적당히 마라맛 나서 좋았엇어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044850.1.13",
          "form": "전 못먹어봤어요",
          "original_form": "전 못먹어봤어요",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044850.1.14",
          "form": "마라좋아하시면 좋아하셨을텐데ㅠ",
          "original_form": "마라좋아하시면 좋아하셨을텐데ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044850.1.15",
          "form": "아 아쉽네요",
          "original_form": "아 아쉽네요",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044850.1.16",
          "form": "아직 남아잇는 지점도 있더라구요 혹시모르니 체크해보세요 ㅋㅋㅋ",
          "original_form": "아직 남아잇는 지점도 있더라구요 혹시모르니 체크해보세요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044850.1.17",
          "form": "아 알아봐야겠어요",
          "original_form": "아 알아봐야겠어요",
