      "utterance": [
        {
          "id": "MDRW2100030379.1.1",
          "form": "ㅎㅎㅎ 나중에 차이나죠",
          "original_form": "ㅎㅎㅎ 나중에 차이나죠",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100030379.1.2",
          "form": "ㅎㅎ 해도 안되는건 없잖아요ㅎㅎ",
          "original_form": "ㅎㅎ 해도 안되는건 없잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100030379.1.3",
          "form": "꼭 보여줘야죵ㅎㅎ 전 제가 실증나지않는 이상 계속할것같애요ㅎ",
          "original_form": "꼭 보여줘야죵ㅎㅎ 전 제가 실증나지않는 이상 계속할것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100030379.1.4",
          "form": "저도요 ㅎㅎ",
          "original_form": "저도요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030379.1.5",
          "form": "name1님이 많이 다독주세요ㅎ 멋진아빠가될수있게ㅎㅎ",
          "original_form": "&name1&님이 많이 다독주세요ㅎ 멋진아빠가될수있게ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030379.1.6",
          "form": "돈돈모에선 머니메이트래요ㅎ",
          "original_form": "돈돈모에선 머니메이트래요ㅎ",
          "speaker_id": "2",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030379.1.7",
          "form": "ㅎㅎㅎ 머니메이트 좋네요",
          "original_form": "ㅎㅎㅎ 머니메이트 좋네요",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030379.1.8",
          "form": "근데 뭐 그런 거창한거 지어서 뭐해요ㅎ 그냥 목표가 같은 사람이지ㅎ",
          "original_form": "근데 뭐 그런 거창한거 지어서 뭐해요ㅎ 그냥 목표가 같은 사람이지ㅎ",
          "speaker_id": "2",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030379.1.9",
          "form": "우선 차량부터 받고나서 다음목표세워야지ㅎㅎ",
          "original_form": "우선 차량부터 받고나서 다음목표세워야지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030379.1.10",
          "form": "근데 오늘 돈돈모에서 청약 당첨됬다고 하는데 부럽더라구요ㅠㅍ",
          "original_form": "근데 오늘 돈돈모에서 청약 당첨됬다고 하는데 부럽더라구요ㅠㅍ",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030379.1.11",
          "form": "네네 누가 먼저 차타나 봐야겟네여 ㅋㅋ",
          "original_form": "네네 누가 먼저 차타나 봐야겟네여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030379.1.12",
          "form": "저희도 언젠가 ㅎㅎ",
          "original_form": "저희도 언젠가 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030379.1.13",
          "form": "솔직히 차는 기대안해요ㅎㅎ 그건 천운을 타고나야지만 되는거니깐 ㅡㅎ",
          "original_form": "솔직히 차는 기대안해요ㅎㅎ 그건 천운을 타고나야지만 되는거니깐 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030379.1.14",
          "form": "ㅎㅎㅎ 글쵸 ㅋ",
          "original_form": "ㅎㅎㅎ 글쵸 ㅋ",
          "speaker_id": "1",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030379.1.15",
          "form": "근데 돈모으고 지출방어할수있는건 우리가할수잇으니깐요ㅎ 부수입도 계속발생할수있으니깐 잘될꺼애요ㅎ",
          "original_form": "근데 돈모으고 지출방어할수있는건 우리가할수잇으니깐요ㅎ 부수입도 계속발생할수있으니깐 잘될꺼애요ㅎ",
          "speaker_id": "2",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030379.1.16",
          "form": "많이 도와주셔야해요ㅎ 저도 많이도울께요ㅎ 우리 가족들을 위해 힘내봐요ㅎ",
          "original_form": "많이 도와주셔야해요ㅎ 저도 많이도울께요ㅎ 우리 가족들을 위해 힘내봐요ㅎ",
          "speaker_id": "2",
