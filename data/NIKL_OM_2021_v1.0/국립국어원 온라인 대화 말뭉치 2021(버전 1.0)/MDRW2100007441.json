      "utterance": [
        {
          "id": "MDRW2100007441.1.1",
          "form": "name2님 학생이세요?",
          "original_form": "&name2&님 학생이세요?",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007441.1.2",
          "form": "아니요 ㅋㅋㅋ근데 제가 학교다니던때 얘기하면될거같아욧",
          "original_form": "아니요 ㅋㅋㅋ근데 제가 학교다니던때 얘기하면될거같아욧",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007441.1.3",
          "form": "아 졸업하신거예요?",
          "original_form": "아 졸업하신거예요?",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007441.1.4",
          "form": "학교다닐때 동아리 활동 해보셨어요?",
          "original_form": "학교다닐때 동아리 활동 해보셨어요?",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007441.1.5",
          "form": "아니여 ㅜㅜ 제대로해본느낌이 드는건 없네요 그냥 영화감상만 했어요",
          "original_form": "아니여 ㅜㅜ 제대로해본느낌이 드는건 없네요 그냥 영화감상만 했어요",
          "speaker_id": "2",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007441.1.6",
          "form": "전공이 뭐였나요?",
          "original_form": "전공이 뭐였나요?",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007441.1.7",
          "form": "저 인문계고 나와서 전문대를 가니까 아무것도없던데요..",
          "original_form": "저 인문계고 나와서 전문대를 가니까 아무것도없던데요..",
          "speaker_id": "2",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007441.1.8",
          "form": "전문대도 전공은 있자나요",
          "original_form": "전문대도 전공은 있자나요",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007441.1.9",
          "form": "유아교육과갔다가 안맞아서 팅겨나오고",
          "original_form": "유아교육과갔다가 안맞아서 팅겨나오고",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007441.1.10",
          "form": "자퇴하려고했는데 경영으로옮겨서 졸업은했어요",
          "original_form": "자퇴하려고했는데 경영으로옮겨서 졸업은했어요",
          "speaker_id": "2",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007441.1.11",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007441.1.12",
          "form": "아 전공선택이 중요하긴 하죠",
          "original_form": "아 전공선택이 중요하긴 하죠",
          "speaker_id": "1",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007441.1.13",
          "form": "전 공대나와서",
          "original_form": "전 공대나와서",
          "speaker_id": "1",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007441.1.14",
          "form": "저는 수포자라 공대가기가 좀 두려웠어요",
          "original_form": "저는 수포자라 공대가기가 좀 두려웠어요",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007441.1.15",
          "form": "유아교육과도 힘든줄알았으면 공대로 가보는건데",
          "original_form": "유아교육과도 힘든줄알았으면 공대로 가보는건데",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007441.1.16",
          "form": "애기 좋아하지않음 유아교육과는 힘들듯요",
          "original_form": "애기 좋아하지않음 유아교육과는 힘들듯요",
          "speaker_id": "1",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007441.1.17",
          "form": "전 시험스트레스가 넘심했어요",
          "original_form": "전 시험스트레스가 넘심했어요",
          "speaker_id": "1",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007441.1.18",
          "form": "시험문제가 어려울거같아요",
          "original_form": "시험문제가 어려울거같아요",
          "speaker_id": "2",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007441.1.19",
          "form": "그니까 시험이겠죠",
          "original_form": "그니까 시험이겠죠",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007441.1.20",
          "form": "요즘은 코로나로 학교도 못가던데 그래도 저희때가 좋은거같네요",
          "original_form": "요즘은 코로나로 학교도 못가던데 그래도 저희때가 좋은거같네요",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007441.1.21",
          "form": "그만큼 졸업학점이 줄었던데요",
          "original_form": "그만큼 졸업학점이 줄었던데요",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007441.1.22",
          "form": "허들을 낮춘듯",
          "original_form": "허들을 낮춘듯",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007441.1.23",
          "form": "아그래요?",
          "original_form": "아그래요?",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007441.1.24",
          "form": "학교를 못가니 등록금은 내야되고 요즘학생들보면 안타까워요",
          "original_form": "학교를 못가니 등록금은 내야되고 요즘학생들보면 안타까워요",
          "speaker_id": "1",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007441.1.25",
          "form": "등록금은 낮춰야될텐데..ㅋㅋㅋㅋ",
