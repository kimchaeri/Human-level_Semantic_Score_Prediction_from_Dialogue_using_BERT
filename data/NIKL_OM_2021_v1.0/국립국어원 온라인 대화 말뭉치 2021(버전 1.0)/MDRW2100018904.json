      "utterance": [
        {
          "id": "MDRW2100018904.1.1",
          "form": "최근에 영화관 가보셨어요?",
          "original_form": "최근에 영화관 가보셨어요?",
          "speaker_id": "1",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100018904.1.2",
          "form": "마지막으로 크루엘라 봤네요!!ㅎㅎ",
          "original_form": "마지막으로 크루엘라 봤네요!!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100018904.1.3",
          "form": "와와 그거 재밌다해서 보려고 했는데 결국 내려갔네요 ㅜㅜ",
          "original_form": "와와 그거 재밌다해서 보려고 했는데 결국 내려갔네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100018904.1.4",
          "form": "저는 최근에 모가디슈!",
          "original_form": "저는 최근에 모가디슈!",
          "speaker_id": "1",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100018904.1.5",
          "form": "오 그거 재밌나요?",
          "original_form": "오 그거 재밌나요?",
          "speaker_id": "2",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100018904.1.6",
          "form": "엄청 홍보도 많이하고 그러던데",
          "original_form": "엄청 홍보도 많이하고 그러던데",
          "speaker_id": "2",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100018904.1.7",
          "form": "네 흥미진진해요",
          "original_form": "네 흥미진진해요",
          "speaker_id": "1",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100018904.1.8",
          "form": "답답한 부분이 별로 없고 연기도 좋고",
          "original_form": "답답한 부분이 별로 없고 연기도 좋고",
          "speaker_id": "1",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100018904.1.9",
          "form": "오 답답한부분 없는게 좋네요!!",
          "original_form": "오 답답한부분 없는게 좋네요!!",
          "speaker_id": "2",
          "time": "20210827 17:30"
        },
        {
          "id": "MDRW2100018904.1.10",
          "form": "영화 답답하면 중간에 나가고싶어져요ㅜㅠ",
          "original_form": "영화 답답하면 중간에 나가고싶어져요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210827 17:30"
        },
        {
          "id": "MDRW2100018904.1.11",
          "form": "맞아요 안그래도 답답한 공간이라ㅋㅋㅋ",
          "original_form": "맞아요 안그래도 답답한 공간이라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100018904.1.12",
          "form": "ㅋㅋㅋㅋㅋ근데 극장에서 팝콘까지 먹어야되는데 못먹어서 아쉽지않나요?ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ근데 극장에서 팝콘까지 먹어야되는데 못먹어서 아쉽지않나요?ㅠ",
          "speaker_id": "2",
          "time": "20210827 17:33"
        },
        {
          "id": "MDRW2100018904.1.13",
          "form": "네 너무 입이 심심해요ㅋㅋㅋ 요즘 사람도 아예 없던데 ㅠㅠ",
          "original_form": "네 너무 입이 심심해요ㅋㅋㅋ 요즘 사람도 아예 없던데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:35"
        },
        {
          "id": "MDRW2100018904.1.14",
          "form": "그쵸ㅠㅜㅠ 저도 진짜보고싶은거 아니면 잘 안가게되던데 다들 그런가봐요",
          "original_form": "그쵸ㅠㅜㅠ 저도 진짜보고싶은거 아니면 잘 안가게되던데 다들 그런가봐요",
          "speaker_id": "2",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100018904.1.15",
          "form": "ㅋㅋㅋ이제 전 넷플릭스 때문에 더 안가게 되더라구여ㅋㅋ 그냥 누워서 봐야겠다하며",
          "original_form": "ㅋㅋㅋ이제 전 넷플릭스 때문에 더 안가게 되더라구여ㅋㅋ 그냥 누워서 봐야겠다하며",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100018904.1.16",
          "form": "ㅋㅋㅋㄱㄲㅋㅋ맞아요 보고싶은 시간에 보고싶은거 보고싶은곳에서 보니까용ㅎㅎ",
          "original_form": "ㅋㅋㅋㄱㄲㅋㅋ맞아요 보고싶은 시간에 보고싶은거 보고싶은곳에서 보니까용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100018904.1.17",
          "form": "가끔 극장 가고싶어질때만 가게돼요",
          "original_form": "가끔 극장 가고싶어질때만 가게돼요",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100018904.1.18",
          "form": "마자여 엄청 블록버스터 영화같은거",
          "original_form": "마자여 엄청 블록버스터 영화같은거",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100018904.1.19",
          "form": "ㅋㅋㅋㅋㅋ맞아요ㅎㅎㅎ영화관에서 봐야할것같은 영화!",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요ㅎㅎㅎ영화관에서 봐야할것같은 영화!",
          "speaker_id": "2",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100018904.1.20",
          "form": "영화관 근데 이제 없어질거 같아요 ㅋㅋㅋㅋ",
          "original_form": "영화관 근데 이제 없어질거 같아요 ㅋㅋㅋㅋ",
