      "utterance": [
        {
          "id": "MDRW2100016130.1.1",
          "form": "동물 키우시나요",
          "original_form": "동물 키우시나요",
          "speaker_id": "2",
          "time": "20210826 10:27"
        },
        {
          "id": "MDRW2100016130.1.2",
          "form": "네 아까 허스키키운다고 말씀드렸어요",
          "original_form": "네 아까 허스키키운다고 말씀드렸어요",
          "speaker_id": "1",
          "time": "20210826 10:27"
        },
        {
          "id": "MDRW2100016130.1.3",
          "form": "아 ~ ㅎ",
          "original_form": "아 ~ ㅎ",
          "speaker_id": "2",
          "time": "20210826 10:28"
        },
        {
          "id": "MDRW2100016130.1.4",
          "form": "또 키우고싶은 동물",
          "original_form": "또 키우고싶은 동물",
          "speaker_id": "2",
          "time": "20210826 10:28"
        },
        {
          "id": "MDRW2100016130.1.5",
          "form": "있으신가요?",
          "original_form": "있으신가요?",
          "speaker_id": "2",
          "time": "20210826 10:28"
        },
        {
          "id": "MDRW2100016130.1.6",
          "form": "음 아직은 없어요",
          "original_form": "음 아직은 없어요",
          "speaker_id": "1",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100016130.1.7",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100016130.1.8",
          "form": "전 고양잉뇨!!",
          "original_form": "전 고양잉뇨!!",
          "speaker_id": "2",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016130.1.9",
          "form": "고양이 좋아하시나봐요",
          "original_form": "고양이 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100016130.1.10",
          "form": "어떤 점을 좋아하시나요",
          "original_form": "어떤 점을 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100016130.1.11",
          "form": "저 귀여운 발바닥..이요..",
          "original_form": "저 귀여운 발바닥..이요..",
          "speaker_id": "2",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016130.1.12",
          "form": "근데 다 좋아요",
          "original_form": "근데 다 좋아요",
          "speaker_id": "2",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016130.1.13",
          "form": "하긴 고양이는 귀엽게 생겨서",
          "original_form": "하긴 고양이는 귀엽게 생겨서",
          "speaker_id": "1",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016130.1.14",
          "form": "다들 좋아하더라구요",
          "original_form": "다들 좋아하더라구요",
          "speaker_id": "1",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016130.1.15",
          "form": "ㅋㅋㅋㅋ네 귀여윤 걸",
          "original_form": "ㅋㅋㅋㅋ네 귀여윤 걸",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100016130.1.16",
          "form": "싫어하는 사람은 없죠",
          "original_form": "싫어하는 사람은 없죠",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100016130.1.17",
          "form": "만약 기른다면 이름은 뭐라고 지어주고싶나요",
          "original_form": "만약 기른다면 이름은 뭐라고 지어주고싶나요",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016130.1.18",
          "form": "음.. 이름까진 생각을 못했어요",
          "original_form": "음.. 이름까진 생각을 못했어요",
          "speaker_id": "2",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016130.1.19",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016130.1.20",
          "form": "전.. 고양이를 키우겠다는 생각을 해보지는 않아서..",
          "original_form": "전.. 고양이를 키우겠다는 생각을 해보지는 않아서..",
          "speaker_id": "1",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016130.1.21",
          "form": "어.. 뭐라고 답을 드려야 할지",
          "original_form": "어.. 뭐라고 답을 드려야 할지",
          "speaker_id": "1",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016130.1.22",
          "form": "동물을 키운다면",
          "original_form": "동물을 키운다면",
          "speaker_id": "2",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016130.1.23",
          "form": "뭐라고 이름 짓고싶으세요?",
          "original_form": "뭐라고 이름 짓고싶으세요?",
          "speaker_id": "2",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016130.1.24",
          "form": "아.. 죄송해요..\r\n제가 name3 이외에 뭘 키운다는 생각을 안해봐서",
          "original_form": "아.. 죄송해요..\r\n제가 &name3& 이외에 뭘 키운다는 생각을 안해봐서",
          "speaker_id": "1",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016130.1.25",
          "form": "자꾸 답변이 엇나가네요ㅠ",
          "original_form": "자꾸 답변이 엇나가네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016130.1.26",
          "form": "아하 아니에요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아하 아니에요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016130.1.27",
          "form": "그럴 수 있죠!",
          "original_form": "그럴 수 있죠!",
          "speaker_id": "2",
          "time": "20210826 12:17"
        }
      ]
    }
