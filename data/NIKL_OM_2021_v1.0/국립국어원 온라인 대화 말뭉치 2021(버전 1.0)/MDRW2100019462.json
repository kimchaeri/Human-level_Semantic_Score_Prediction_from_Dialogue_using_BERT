      "utterance": [
        {
          "id": "MDRW2100019462.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019462.1.2",
          "form": "혹시 봉사활동 하고 계세요?",
          "original_form": "혹시 봉사활동 하고 계세요?",
          "speaker_id": "1",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019462.1.3",
          "form": "요새는 안해요ㅎㅎ",
          "original_form": "요새는 안해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019462.1.4",
          "form": "예전엔 했었어요",
          "original_form": "예전엔 했었어요",
          "speaker_id": "2",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019462.1.5",
          "form": "그렇구나~ 어떤 봉사활동 했어요?",
          "original_form": "그렇구나~ 어떤 봉사활동 했어요?",
          "speaker_id": "1",
          "time": "20210827 19:35"
        },
        {
          "id": "MDRW2100019462.1.6",
          "form": "교육봉사 했어요~",
          "original_form": "교육봉사 했어요~",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019462.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019462.1.8",
          "form": "어! 저도 교육봉사 했어요~",
          "original_form": "어! 저도 교육봉사 했어요~",
          "speaker_id": "1",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019462.1.9",
          "form": "혹시 지역아동센터에서 하셨어요?ㅎㅎ",
          "original_form": "혹시 지역아동센터에서 하셨어요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019462.1.10",
          "form": "아뇨 전 학교랑 연계해서요!",
          "original_form": "아뇨 전 학교랑 연계해서요!",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019462.1.11",
          "form": "초등학교에서 했었어요",
          "original_form": "초등학교에서 했었어요",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019462.1.12",
          "form": "오~ 대박!",
          "original_form": "오~ 대박!",
          "speaker_id": "1",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019462.1.13",
          "form": "교대 나오셨나봐요!",
          "original_form": "교대 나오셨나봐요!",
          "speaker_id": "1",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019462.1.14",
          "form": "아뇨 교대는 아닙니당ㅎㅎ",
          "original_form": "아뇨 교대는 아닙니당ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019462.1.15",
          "form": "어쩌다 기회가 생겨서ㅎㅎ",
          "original_form": "어쩌다 기회가 생겨서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019462.1.16",
          "form": "오 그러시구나~",
          "original_form": "오 그러시구나~",
          "speaker_id": "1",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019462.1.17",
          "form": "방과후수업 같은 걸로 하신거에요?",
          "original_form": "방과후수업 같은 걸로 하신거에요?",
          "speaker_id": "1",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019462.1.18",
          "form": "그냥 애기들 멘토링 같은거였어용",
          "original_form": "그냥 애기들 멘토링 같은거였어용",
          "speaker_id": "2",
          "time": "20210828 12:03"
        },
        {
          "id": "MDRW2100019462.1.19",
          "form": "일대일로요ㅎㅎ",
          "original_form": "일대일로요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:03"
        },
        {
          "id": "MDRW2100019462.1.20",
          "form": "아아 ~",
          "original_form": "아아 ~",
          "speaker_id": "1",
          "time": "20210828 12:04"
        },
        {
          "id": "MDRW2100019462.1.21",
          "form": "초등학교에서 했어요 그거를?",
          "original_form": "초등학교에서 했어요 그거를?",
          "speaker_id": "1",
          "time": "20210828 12:05"
        },
        {
          "id": "MDRW2100019462.1.22",
          "form": "네네 초등학교요ㅎㅎ",
          "original_form": "네네 초등학교요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100019462.1.23",
          "form": "애들 말도 잘 듣고 귀여웠어요",
          "original_form": "애들 말도 잘 듣고 귀여웠어요",
          "speaker_id": "2",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100019462.1.24",
          "form": "꺅 몇학년이였어요?",
          "original_form": "꺅 몇학년이였어요?",
          "speaker_id": "1",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100019462.1.25",
          "form": "초등학교 1학년이랑 3학년이용~",
          "original_form": "초등학교 1학년이랑 3학년이용~",
