      "utterance": [
        {
          "id": "MDRW2100024375.1.1",
          "form": "헐 부엉이도요?",
          "original_form": "헐 부엉이도요?",
          "speaker_id": "2",
          "time": "20210831 02:59"
        },
        {
          "id": "MDRW2100024375.1.2",
          "form": "어디가면 막 플라밍고라고 홍학 걔도 갖다놓고",
          "original_form": "어디가면 막 플라밍고라고 홍학 걔도 갖다놓고",
          "speaker_id": "1",
          "time": "20210831 02:59"
        },
        {
          "id": "MDRW2100024375.1.3",
          "form": "일본 반려동물문화 수준있다더니 그런 것도 아닌가봐요..",
          "original_form": "일본 반려동물문화 수준있다더니 그런 것도 아닌가봐요..",
          "speaker_id": "2",
          "time": "20210831 02:59"
        },
        {
          "id": "MDRW2100024375.1.4",
          "form": "ㅋㅋㅋ부엉이키울때",
          "original_form": "ㅋㅋㅋ부엉이키울때",
          "speaker_id": "1",
          "time": "20210831 02:59"
        },
        {
          "id": "MDRW2100024375.1.5",
          "form": "자격증이 필요하긴한데",
          "original_form": "자격증이 필요하긴한데",
          "speaker_id": "1",
          "time": "20210831 02:59"
        },
        {
          "id": "MDRW2100024375.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210831 02:59"
        },
        {
          "id": "MDRW2100024375.1.7",
          "form": "카페에서 막 다리묶어두고 하나하나 전시해두듯이하는건",
          "original_form": "카페에서 막 다리묶어두고 하나하나 전시해두듯이하는건",
          "speaker_id": "1",
          "time": "20210831 02:59"
        },
        {
          "id": "MDRW2100024375.1.8",
          "form": "어머나",
          "original_form": "어머나",
          "speaker_id": "2",
          "time": "20210831 02:59"
        },
        {
          "id": "MDRW2100024375.1.9",
          "form": "좀 아닌것같앗어요..",
          "original_form": "좀 아닌것같앗어요..",
          "speaker_id": "1",
          "time": "20210831 02:59"
        },
        {
          "id": "MDRW2100024375.1.10",
          "form": "동물학대 아닌가요..",
          "original_form": "동물학대 아닌가요..",
          "speaker_id": "2",
          "time": "20210831 03:00"
        },
        {
          "id": "MDRW2100024375.1.11",
          "form": "저는 홍학 걔가",
          "original_form": "저는 홍학 걔가",
          "speaker_id": "1",
          "time": "20210831 03:00"
        },
        {
          "id": "MDRW2100024375.1.12",
          "form": "막 돌아다녀갖고 ㅋㅋㅋ",
          "original_form": "막 돌아다녀갖고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 03:00"
        },
        {
          "id": "MDRW2100024375.1.13",
          "form": "ㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 03:00"
        },
        {
          "id": "MDRW2100024375.1.14",
          "form": "그리고 어디나라든간에 정말  품종 충격적인것같아요",
          "original_form": "그리고 어디나라든간에 정말  품종 충격적인것같아요",
          "speaker_id": "1",
          "time": "20210831 03:00"
        },
        {
          "id": "MDRW2100024375.1.15",
          "form": "무늬 조금 특이하다고 몇천만원되고",
          "original_form": "무늬 조금 특이하다고 몇천만원되고",
          "speaker_id": "1",
          "time": "20210831 03:00"
        },
        {
          "id": "MDRW2100024375.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 03:00"
        },
        {
          "id": "MDRW2100024375.1.17",
          "form": "오드아이들도",
          "original_form": "오드아이들도",
          "speaker_id": "2",
          "time": "20210831 03:00"
        },
        {
          "id": "MDRW2100024375.1.18",
          "form": "특이하다고 인기많고..",
          "original_form": "특이하다고 인기많고..",
          "speaker_id": "2",
          "time": "20210831 03:00"
        },
        {
          "id": "MDRW2100024375.1.19",
          "form": "ㅜ 그리고 그걸로 장사하고",
          "original_form": "ㅜ 그리고 그걸로 장사하고",
          "speaker_id": "1",
          "time": "20210831 03:00"
        },
        {
          "id": "MDRW2100024375.1.20",
          "form": "저는 청력안좋대서",
          "original_form": "저는 청력안좋대서",
          "speaker_id": "1",
          "time": "20210831 03:01"
        },
        {
          "id": "MDRW2100024375.1.21",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210831 03:01"
        },
        {
          "id": "MDRW2100024375.1.22",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 03:01"
        },
        {
          "id": "MDRW2100024375.1.23",
          "form": "그리고 막 머리 두개 예쁘게 난",
          "original_form": "그리고 막 머리 두개 예쁘게 난",
