      "utterance": [
        {
          "id": "MDRW2100003651.1.1",
          "form": "여긴 비가 엄청많이와요",
          "original_form": "여긴 비가 엄청많이와요",
          "speaker_id": "1",
          "time": "20210821 09:52"
        },
        {
          "id": "MDRW2100003651.1.2",
          "form": "여기두요",
          "original_form": "여기두요",
          "speaker_id": "2",
          "time": "20210821 09:53"
        },
        {
          "id": "MDRW2100003651.1.3",
          "form": "장마가 시작일까요",
          "original_form": "장마가 시작일까요",
          "speaker_id": "2",
          "time": "20210821 09:53"
        },
        {
          "id": "MDRW2100003651.1.4",
          "form": "계속 장마소리를 듣긴했는데 ㅜ 비가 진짜 쏟아지네요",
          "original_form": "계속 장마소리를 듣긴했는데 ㅜ 비가 진짜 쏟아지네요",
          "speaker_id": "1",
          "time": "20210821 09:54"
        },
        {
          "id": "MDRW2100003651.1.5",
          "form": "작년처럼 긴 장마는 아니었으면..",
          "original_form": "작년처럼 긴 장마는 아니었으면..",
          "speaker_id": "1",
          "time": "20210821 09:54"
        },
        {
          "id": "MDRW2100003651.1.6",
          "form": "그래도 시원해서 좋아요",
          "original_form": "그래도 시원해서 좋아요",
          "speaker_id": "2",
          "time": "20210821 09:57"
        },
        {
          "id": "MDRW2100003651.1.7",
          "form": "너무 시웡해요",
          "original_form": "너무 시웡해요",
          "speaker_id": "2",
          "time": "20210821 09:57"
        },
        {
          "id": "MDRW2100003651.1.8",
          "form": "그건그래요 창문조금열어두니 엄청시원하네요 ㅋㅋㅋㅋ",
          "original_form": "그건그래요 창문조금열어두니 엄청시원하네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 09:58"
        },
        {
          "id": "MDRW2100003651.1.9",
          "form": "비가 집안에있을땐 시원하고 좋은데 비올때 밖에있는건 싫어요 ㅋㅋㅋ",
          "original_form": "비가 집안에있을땐 시원하고 좋은데 비올때 밖에있는건 싫어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 09:58"
        },
        {
          "id": "MDRW2100003651.1.10",
          "form": "어 공감입니다",
          "original_form": "어 공감입니다",
          "speaker_id": "2",
          "time": "20210821 09:59"
        },
        {
          "id": "MDRW2100003651.1.11",
          "form": "비 올땐 집콕이 짱이죠 ㅎ",
          "original_form": "비 올땐 집콕이 짱이죠 ㅎ",
          "speaker_id": "2",
          "time": "20210821 09:59"
        },
        {
          "id": "MDRW2100003651.1.12",
          "form": "맞아요 이불둘러쓰고 빗소리들으면서 뒹굴거리는게 짱이져",
          "original_form": "맞아요 이불둘러쓰고 빗소리들으면서 뒹굴거리는게 짱이져",
          "speaker_id": "1",
          "time": "20210821 09:59"
        },
        {
          "id": "MDRW2100003651.1.13",
          "form": "그래도 비가오니 공기가 맑아지는기분이에요",
          "original_form": "그래도 비가오니 공기가 맑아지는기분이에요",
          "speaker_id": "1",
          "time": "20210821 10:00"
        },
        {
          "id": "MDRW2100003651.1.14",
          "form": "맞아요 ㅜㅜ 일주일 내내 온다는데",
          "original_form": "맞아요 ㅜㅜ 일주일 내내 온다는데",
          "speaker_id": "2",
          "time": "20210821 10:02"
        },
        {
          "id": "MDRW2100003651.1.15",
          "form": "그래도 조금은 멈춰줬으면,, 특히 평일은",
          "original_form": "그래도 조금은 멈춰줬으면,, 특히 평일은",
          "speaker_id": "2",
          "time": "20210821 10:02"
        },
        {
          "id": "MDRW2100003651.1.16",
          "form": "일주일내내라니 좀만 적게왔으면 좋겠어요 ㅜㅜ ㅜ",
          "original_form": "일주일내내라니 좀만 적게왔으면 좋겠어요 ㅜㅜ ㅜ",
          "speaker_id": "1",
          "time": "20210821 10:11"
        },
        {
          "id": "MDRW2100003651.1.17",
          "form": "그러니까요 !!!!!",
          "original_form": "그러니까요 !!!!!",
          "speaker_id": "2",
          "time": "20210821 10:15"
        },
        {
          "id": "MDRW2100003651.1.18",
          "form": "평일에는 할일도 많은데 비라니",
          "original_form": "평일에는 할일도 많은데 비라니",
          "speaker_id": "2",
          "time": "20210821 10:15"
        },
        {
          "id": "MDRW2100003651.1.19",
          "form": "출퇴근할때도 넘찝찝하고여ㅜㅜ",
          "original_form": "출퇴근할때도 넘찝찝하고여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 10:22"
        },
        {
          "id": "MDRW2100003651.1.20",
          "form": "그 젖은부분이 살에닿이는게 넘싫어요",
          "original_form": "그 젖은부분이 살에닿이는게 넘싫어요",
          "speaker_id": "1",
          "time": "20210821 10:22"
        },
        {
          "id": "MDRW2100003651.1.21",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210821 11:32"
        },
        {
          "id": "MDRW2100003651.1.22",
          "form": "습기도 너무 낭낭....",
          "original_form": "습기도 너무 낭낭....",
          "speaker_id": "2",
          "time": "20210821 11:32"
        },
        {
          "id": "MDRW2100003651.1.23",
          "form": "제습기 돌려야겠어요",
          "original_form": "제습기 돌려야겠어요",
