      "utterance": [
        {
          "id": "MDRW2100009377.1.1",
          "form": "오늘은 조금 덥더라구요ㅠ",
          "original_form": "오늘은 조금 덥더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009377.1.2",
          "form": "네 비오는데도 후덥지근ㅜㅜ",
          "original_form": "네 비오는데도 후덥지근ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009377.1.3",
          "form": "지금 에어컨이 돌아가고 있어여ㅋㅋㅋ",
          "original_form": "지금 에어컨이 돌아가고 있어여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009377.1.4",
          "form": "저도 에어컨 켰네여ㅋㅋ끝난줄알았는데",
          "original_form": "저도 에어컨 켰네여ㅋㅋ끝난줄알았는데",
          "speaker_id": "1",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009377.1.5",
          "form": "그러니까요ㅋㅋ여태안키고 잘버텼는데",
          "original_form": "그러니까요ㅋㅋ여태안키고 잘버텼는데",
          "speaker_id": "2",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009377.1.6",
          "form": "요며칠ㅋㅋㅋ",
          "original_form": "요며칠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009377.1.7",
          "form": "오늘은 안되겠더라구요",
          "original_form": "오늘은 안되겠더라구요",
          "speaker_id": "2",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009377.1.8",
          "form": "맞아요 선풍기로는 안되는 날씨에요ㅎㅎ",
          "original_form": "맞아요 선풍기로는 안되는 날씨에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009377.1.9",
          "form": "에혀ㅡ가을이온줄 알았는데 오다 도망간건지",
          "original_form": "에혀ㅡ가을이온줄 알았는데 오다 도망간건지",
          "speaker_id": "2",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009377.1.10",
          "form": "비와도 습도만 높으러같은 예감도 들어요ㅠㅜ",
          "original_form": "비와도 습도만 높으러같은 예감도 들어요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009377.1.11",
          "form": "ㅜㅠ비좀 그만 왔으면",
          "original_form": "ㅜㅠ비좀 그만 왔으면",
          "speaker_id": "2",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009377.1.12",
          "form": "장마라는게 안믿기네요ㅎㅎ 옷도 어떻게 입을지 고민되구..",
          "original_form": "장마라는게 안믿기네요ㅎㅎ 옷도 어떻게 입을지 고민되구..",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009377.1.13",
          "form": "ㅠㅠ 비가 너무 오니까 습하고 찝찝하고",
          "original_form": "ㅠㅠ 비가 너무 오니까 습하고 찝찝하고",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009377.1.14",
          "form": "맞아요ㅠㅠ우산들고 다니기도 귀찮구",
          "original_form": "맞아요ㅠㅠ우산들고 다니기도 귀찮구",
          "speaker_id": "1",
          "time": "20210822 23:41"
        }
      ]
