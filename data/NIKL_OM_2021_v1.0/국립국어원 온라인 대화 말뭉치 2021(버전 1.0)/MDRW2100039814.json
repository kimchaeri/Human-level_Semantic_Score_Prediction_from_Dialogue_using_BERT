      "utterance": [
        {
          "id": "MDRW2100039814.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039814.1.2",
          "form": "안녕하세요!!!!!!",
          "original_form": "안녕하세요!!!!!!",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039814.1.3",
          "form": "게임 좋아하시나요",
          "original_form": "게임 좋아하시나요",
          "speaker_id": "1",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100039814.1.4",
          "form": "아 즐겨하진 않는데 가끔 심즈는 해요!",
          "original_form": "아 즐겨하진 않는데 가끔 심즈는 해요!",
          "speaker_id": "2",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100039814.1.5",
          "form": "심즈 재밌나요",
          "original_form": "심즈 재밌나요",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039814.1.6",
          "form": "저는 집꾸미는게 재밌더라고요!!!",
          "original_form": "저는 집꾸미는게 재밌더라고요!!!",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039814.1.7",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039814.1.8",
          "form": "엄청 좋아해요",
          "original_form": "엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039814.1.9",
          "form": "롤 주로 합니다",
          "original_form": "롤 주로 합니다",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039814.1.10",
          "form": "아하 롤 ~ 들어는 봤는데 어떤게임인가요?",
          "original_form": "아하 롤 ~ 들어는 봤는데 어떤게임인가요?",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100039814.1.11",
          "form": "5대5로 상대진영을 먼저 부수는 게임이에요",
          "original_form": "5대5로 상대진영을 먼저 부수는 게임이에요",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100039814.1.12",
          "form": "아~ 방금 대충 검색해봤어여 ㅎ",
          "original_form": "아~ 방금 대충 검색해봤어여 ㅎ",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100039814.1.13",
          "form": "저런거군요 ㅎ",
          "original_form": "저런거군요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100039814.1.14",
          "form": "모바일게임도 하세요?",
          "original_form": "모바일게임도 하세요?",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100039814.1.15",
          "form": "모바일게임도 하죠ㅎㅎ",
          "original_form": "모바일게임도 하죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100039814.1.16",
          "form": "모바일은 퍼즐겜 위주로 하고 있습니다",
          "original_form": "모바일은 퍼즐겜 위주로 하고 있습니다",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100039814.1.17",
          "form": "퍼즐겜!",
          "original_form": "퍼즐겜!",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100039814.1.18",
          "form": "최근에 광고보고 잠깐해본게 있는데 물 색깔별로 따르는",
          "original_form": "최근에 광고보고 잠깐해본게 있는데 물 색깔별로 따르는",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100039814.1.19",
          "form": "아 그거 제목은 모르겠는데",
          "original_form": "아 그거 제목은 모르겠는데",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100039814.1.20",
          "form": "뭔진 알거 같아요",
          "original_form": "뭔진 알거 같아요",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100039814.1.21",
          "form": "많이 어렵나요",
          "original_form": "많이 어렵나요",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100039814.1.22",
          "form": "아뇨 ㅎ 할만해요 머리쓰다보면 시간도 금방가고",
          "original_form": "아뇨 ㅎ 할만해요 머리쓰다보면 시간도 금방가고",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100039814.1.23",
          "form": "킬링타임용 같아요",
          "original_form": "킬링타임용 같아요",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100039814.1.24",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039814.1.25",
          "form": "한번 해볼만 하겠네요",
          "original_form": "한번 해볼만 하겠네요",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039814.1.26",
          "form": "네네 시간나실때 해보세요ㅋㅋㅋ물 따르는소리가 좋더라고요",
          "original_form": "네네 시간나실때 해보세요ㅋㅋㅋ물 따르는소리가 좋더라고요",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039814.1.27",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "1",
