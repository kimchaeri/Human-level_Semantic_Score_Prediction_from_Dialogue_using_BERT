      "utterance": [
        {
          "id": "MDRW2100040927.1.1",
          "form": "밥먹어야죠?",
          "original_form": "밥먹어야죠?",
          "speaker_id": "2",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100040927.1.2",
          "form": "오늘 밀푀유나베 해먹을거에요",
          "original_form": "오늘 밀푀유나베 해먹을거에요",
          "speaker_id": "1",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100040927.1.3",
          "form": "꺄 언제요?",
          "original_form": "꺄 언제요?",
          "speaker_id": "2",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100040927.1.4",
          "form": "한 세네시쯤?!",
          "original_form": "한 세네시쯤?!",
          "speaker_id": "1",
          "time": "20210905 13:14"
        },
        {
          "id": "MDRW2100040927.1.5",
          "form": "오 ㅋㅋㅋ지금은 배안고파요?",
          "original_form": "오 ㅋㅋㅋ지금은 배안고파요?",
          "speaker_id": "2",
          "time": "20210905 13:14"
        },
        {
          "id": "MDRW2100040927.1.6",
          "form": "너무 배고파요",
          "original_form": "너무 배고파요",
          "speaker_id": "1",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040927.1.7",
          "form": "어제 사온 케이크라도 한입 먹으려구요",
          "original_form": "어제 사온 케이크라도 한입 먹으려구요",
          "speaker_id": "1",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040927.1.8",
          "form": "근데왜 지금 안먹구 있다먹어요!!",
          "original_form": "근데왜 지금 안먹구 있다먹어요!!",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040927.1.9",
          "form": "엄마아빠가 늦게먹어요 점심 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "엄마아빠가 늦게먹어요 점심 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040927.1.10",
          "form": "근데 스콘 냉장고에 넣어둬도 ...되죵??",
          "original_form": "근데 스콘 냉장고에 넣어둬도 ...되죵??",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040927.1.11",
          "form": "푸석해질까요?",
          "original_form": "푸석해질까요?",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040927.1.12",
          "form": "음 얼마나 넣어둘거에요? 반나절? 하루이상?",
          "original_form": "음 얼마나 넣어둘거에요? 반나절? 하루이상?",
          "speaker_id": "2",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100040927.1.13",
          "form": "이미 넣은지 반나절은 지났네요...ㅜㅜㅋㅋㅋㅋㅋ",
          "original_form": "이미 넣은지 반나절은 지났네요...ㅜㅜㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100040927.1.14",
          "form": "아항 에프에 돌려먹으면 더 맛있을텐데ㅠ",
          "original_form": "아항 에프에 돌려먹으면 더 맛있을텐데ㅠ",
          "speaker_id": "2",
          "time": "20210905 13:20"
        },
        {
          "id": "MDRW2100040927.1.15",
          "form": "아 에프!! 에프로 몇분 돌려요??",
          "original_form": "아 에프!! 에프로 몇분 돌려요??",
          "speaker_id": "1",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100040927.1.16",
          "form": "180도 5분정도 하시면 될거에요 ㅋㅋㅋ 거의 모든거에 적용가능한 온도 ㅋㅋㅋㅋ",
          "original_form": "180도 5분정도 하시면 될거에요 ㅋㅋㅋ 거의 모든거에 적용가능한 온도 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100040927.1.17",
          "form": "저기서 시간만 2~3분 플마 ㅋㅋㅋ",
          "original_form": "저기서 시간만 2~3분 플마 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:25"
        },
        {
          "id": "MDRW2100040927.1.18",
          "form": "에프로 180도 5분정도 돌려보세용",
          "original_form": "에프로 180도 5분정도 돌려보세용",
          "speaker_id": "2",
          "time": "20210905 13:25"
        },
        {
          "id": "MDRW2100040927.1.19",
