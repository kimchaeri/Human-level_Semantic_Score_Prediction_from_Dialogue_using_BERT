        {
          "id": "MDRW2100034698.1.1",
          "form": "나 사과 한박스 샀눈데",
          "original_form": "나 사과 한박스 샀눈데",
          "speaker_id": "2",
          "time": "20210828 17:41"
        },
        {
          "id": "MDRW2100034698.1.2",
          "form": "와 하루지나니 다썩는거야,;;",
          "original_form": "와 하루지나니 다썩는거야,;;",
          "speaker_id": "2",
          "time": "20210828 17:41"
        },
        {
          "id": "MDRW2100034698.1.3",
          "form": "응응 헉",
          "original_form": "응응 헉",
          "speaker_id": "1",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100034698.1.4",
          "form": "거기에서샀나?",
          "original_form": "거기에서샀나?",
          "speaker_id": "1",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100034698.1.5",
          "form": "나원래 거기 밴드 가입해서",
          "original_form": "나원래 거기 밴드 가입해서",
          "speaker_id": "2",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100034698.1.6",
          "form": "배달 시켜먹었는데 5번정도 그랬음...",
          "original_form": "배달 시켜먹었는데 5번정도 그랬음...",
          "speaker_id": "2",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100034698.1.7",
          "form": "누가봐도 시들한거 배달해주데..",
          "original_form": "누가봐도 시들한거 배달해주데..",
          "speaker_id": "2",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100034698.1.8",
          "form": "양파 한망샀는데 한개빼고 다썩고 ㅋㅋㅋㅋ",
          "original_form": "양파 한망샀는데 한개빼고 다썩고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100034698.1.9",
          "form": "탈퇴함 ㅋㅋㅋ",
          "original_form": "탈퇴함 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100034698.1.10",
          "form": "가서보고사야됨~",
          "original_form": "가서보고사야됨~",
