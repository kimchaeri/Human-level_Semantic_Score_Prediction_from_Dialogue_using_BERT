          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100034698.1.13",
          "form": "그건좀그렇다",
          "original_form": "그건좀그렇다",
          "speaker_id": "1",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100034698.1.14",
          "form": "가서 보고사야겠구나",
          "original_form": "가서 보고사야겠구나",
          "speaker_id": "1",
          "time": "20210828 17:49"
