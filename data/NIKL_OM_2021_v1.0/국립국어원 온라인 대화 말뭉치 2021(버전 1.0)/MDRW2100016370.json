      "utterance": [
        {
          "id": "MDRW2100016370.1.1",
          "form": "점심 드셨나여?",
          "original_form": "점심 드셨나여?",
          "speaker_id": "1",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016370.1.2",
          "form": "좀 이따 먹으려구요",
          "original_form": "좀 이따 먹으려구요",
          "speaker_id": "2",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016370.1.3",
          "form": "식사메뉴정하셨나요",
          "original_form": "식사메뉴정하셨나요",
          "speaker_id": "2",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016370.1.4",
          "form": "전 김치찌개 이미 먹었어요 ㅎ",
          "original_form": "전 김치찌개 이미 먹었어요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016370.1.5",
          "form": "뭐 드실껀가요?",
          "original_form": "뭐 드실껀가요?",
          "speaker_id": "1",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016370.1.6",
          "form": "저 라면생각중이에요",
          "original_form": "저 라면생각중이에요",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016370.1.7",
          "form": "점심으로 김치찌개 드셨구나 어땠나요",
          "original_form": "점심으로 김치찌개 드셨구나 어땠나요",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016370.1.8",
          "form": "네 맛있었어요",
          "original_form": "네 맛있었어요",
          "speaker_id": "1",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016370.1.9",
          "form": "살짝 짜긴 했지만,,",
          "original_form": "살짝 짜긴 했지만,,",
          "speaker_id": "1",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016370.1.10",
          "form": "무슨 라면 드실건가요",
          "original_form": "무슨 라면 드실건가요",
          "speaker_id": "1",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016370.1.11",
          "form": "앗 짜셨구나",
          "original_form": "앗 짜셨구나",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016370.1.12",
          "form": "저 음 오늘은 비빔면이 먹고싶어서",
          "original_form": "저 음 오늘은 비빔면이 먹고싶어서",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016370.1.13",
          "form": "비빔면 먹으려구요",
          "original_form": "비빔면 먹으려구요",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016370.1.14",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016370.1.15",
          "form": "맛있겠어요",
          "original_form": "맛있겠어요",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016370.1.16",
          "form": "비빔면 어떤거 좋아하세요",
          "original_form": "비빔면 어떤거 좋아하세요",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016370.1.17",
          "form": "전 기본 팔도요 !!!",
          "original_form": "전 기본 팔도요 !!!",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016370.1.18",
          "form": "name2은여?",
          "original_form": "&name2&은여?",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016370.1.19",
          "form": "전 요즘 진 비빔면 맛있더라고요",
          "original_form": "전 요즘 진 비빔면 맛있더라고요",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016370.1.20",
          "form": "비빔면에 계란넣어드시나요",
          "original_form": "비빔면에 계란넣어드시나요",
          "speaker_id": "2",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016370.1.21",
          "form": "아뇨 ~ 전 그냥 먹어요 ㅋㅋ",
          "original_form": "아뇨 ~ 전 그냥 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100016370.1.22",
          "form": "넣어드세요?",
          "original_form": "넣어드세요?",
          "speaker_id": "1",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100016370.1.23",
          "form": "네 전 넣어 먹는거 좋아해요",
          "original_form": "네 전 넣어 먹는거 좋아해요",
          "speaker_id": "2",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016370.1.24",
          "form": "좀 더 맛있는 기분",
          "original_form": "좀 더 맛있는 기분",
          "speaker_id": "2",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016370.1.25",
          "form": "오홍.. 저도 다음에",
          "original_form": "오홍.. 저도 다음에",
          "speaker_id": "1",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100016370.1.26",
          "form": "저녁메뉴정하셨나요",
          "original_form": "저녁메뉴정하셨나요",
