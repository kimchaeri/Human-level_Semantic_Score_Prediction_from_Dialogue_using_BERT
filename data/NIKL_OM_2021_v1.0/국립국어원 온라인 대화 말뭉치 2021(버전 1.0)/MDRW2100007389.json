      "utterance": [
        {
          "id": "MDRW2100007389.1.1",
          "form": "마트가서 장 보면 꼭 사는 물품이 있나요?",
          "original_form": "마트가서 장 보면 꼭 사는 물품이 있나요?",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007389.1.2",
          "form": "저 두부요!",
          "original_form": "저 두부요!",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007389.1.3",
          "form": "두부 진짜 거의 매일 먹어요ㅋㅋㅋㅋ",
          "original_form": "두부 진짜 거의 매일 먹어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007389.1.4",
          "form": "두부좋아하시나봐요",
          "original_form": "두부좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007389.1.5",
          "form": "두부의 매력은 뭔가요?",
          "original_form": "두부의 매력은 뭔가요?",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007389.1.6",
          "form": "여기저기 넣어 먹기도 좋고",
          "original_form": "여기저기 넣어 먹기도 좋고",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007389.1.7",
          "form": "단백질도 풍부해서요!",
          "original_form": "단백질도 풍부해서요!",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007389.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007389.1.9",
          "form": "국 찌개 부침 활용도가 높은 거 같아요",
          "original_form": "국 찌개 부침 활용도가 높은 거 같아요",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007389.1.10",
          "form": "또 다른건 뭐 사세요?",
          "original_form": "또 다른건 뭐 사세요?",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007389.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007389.1.12",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007389.1.13",
          "form": "말고는 그때그때 사고 싶은 거 사는 거 같아요",
          "original_form": "말고는 그때그때 사고 싶은 거 사는 거 같아요",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007389.1.14",
          "form": "name1님은 자주 구매하는 물품 있으세요?",
          "original_form": "&name1&님은 자주 구매하는 물품 있으세요?",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007389.1.15",
          "form": "저는 삼겹살 꼭 사는 편인거같아요",
          "original_form": "저는 삼겹살 꼭 사는 편인거같아요",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007389.1.16",
          "form": "장볼 때 꼭 담는 편",
          "original_form": "장볼 때 꼭 담는 편",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007389.1.17",
          "form": "오 삼겹살! 맛있겠어요",
          "original_form": "오 삼겹살! 맛있겠어요",
          "speaker_id": "2",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007389.1.18",
          "form": "삼겹살 사서 구워먹으면 너무 좋은거같아요",
          "original_form": "삼겹살 사서 구워먹으면 너무 좋은거같아요",
          "speaker_id": "1",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007389.1.19",
          "form": "삼겹살 하니 생각났어요",
          "original_form": "삼겹살 하니 생각났어요",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007389.1.20",
          "form": "저 샤브샤브 좋아해서 샤브샤브 재료 많이 사요!",
          "original_form": "저 샤브샤브 좋아해서 샤브샤브 재료 많이 사요!",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007389.1.21",
          "form": "샤브샤브 재료면",
          "original_form": "샤브샤브 재료면",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007389.1.22",
          "form": "고기 콩나물 두부 청경채 이런거요?",
          "original_form": "고기 콩나물 두부 청경채 이런거요?",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007389.1.23",
          "form": "네네! 알배추랑 청경채 숙주 이런거 사고",
          "original_form": "네네! 알배추랑 청경채 숙주 이런거 사고",
          "speaker_id": "2",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007389.1.24",
          "form": "그 소스중에 참소스라는 소스 아세요?",
          "original_form": "그 소스중에 참소스라는 소스 아세요?",
          "speaker_id": "2",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007389.1.25",
          "form": "완전좋아해요 그거",
          "original_form": "완전좋아해요 그거",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007389.1.26",
          "form": "맛이 진짜 취향저격",
          "original_form": "맛이 진짜 취향저격",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007389.1.27",
          "form": "그쵸!! 그거 꼭 사서 같이 먹어요",
          "original_form": "그쵸!! 그거 꼭 사서 같이 먹어요",
