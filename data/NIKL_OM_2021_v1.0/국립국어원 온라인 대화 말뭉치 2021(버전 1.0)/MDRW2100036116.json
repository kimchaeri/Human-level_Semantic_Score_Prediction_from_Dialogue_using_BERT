        },
        {
          "id": "MDRW2100036116.1.2",
          "form": "양치하심여?",
          "original_form": "양치하심여?",
          "speaker_id": "1",
          "time": "20210830 16:34"
        },
        {
          "id": "MDRW2100036116.1.3",
          "form": "열심히 하고잇어요 ㅠ",
          "original_form": "열심히 하고잇어요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 16:34"
        },
        {
          "id": "MDRW2100036116.1.4",
          "form": "지금요?",
          "original_form": "지금요?",
          "speaker_id": "1",
          "time": "20210830 16:34"
        },
        {
          "id": "MDRW2100036116.1.5",
          "form": "네.. 양치안하면 찜찜해거요",
          "original_form": "네.. 양치안하면 찜찜해거요",
          "speaker_id": "2",
          "time": "20210830 16:34"
        },
        {
          "id": "MDRW2100036116.1.6",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100036116.1.7",
          "form": "늦게하시네",
          "original_form": "늦게하시네",
          "speaker_id": "1",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100036116.1.8",
          "form": "밥을 늦게먹어서..\r\nname1님은 하셧어요?",
          "original_form": "밥을 늦게먹어서..\r\n&name1&님은 하셧어요?",
          "speaker_id": "2",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100036116.1.9",
          "form": "지금 하는중이요",
          "original_form": "지금 하는중이요",
          "speaker_id": "1",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100036116.1.10",
          "form": "늦게하시네",
          "original_form": "늦게하시네",
          "speaker_id": "2",
          "time": "20210830 16:36"
        },
        {
          "id": "MDRW2100036116.1.11",
          "form": "입천장 닦았어요",
          "original_form": "입천장 닦았어요",
          "speaker_id": "1",
          "time": "20210830 16:36"
        },
        {
          "id": "MDRW2100036116.1.12",
          "form": "아.. 입천장.. 매우 중요하죠. 잇몸도하시죠?",
          "original_form": "아.. 입천장.. 매우 중요하죠. 잇몸도하시죠?",
          "speaker_id": "2",
          "time": "20210830 16:38"
        },
        {
          "id": "MDRW2100036116.1.13",
          "form": "이제뱉어요",
          "original_form": "이제뱉어요",
          "speaker_id": "1",
          "time": "20210830 16:38"
        },
        {
          "id": "MDRW2100036116.1.14",
