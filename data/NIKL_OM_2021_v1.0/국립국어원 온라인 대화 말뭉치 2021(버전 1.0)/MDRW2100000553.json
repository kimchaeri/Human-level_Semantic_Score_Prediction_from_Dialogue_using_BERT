      "utterance": [
        {
          "id": "MDRW2100000553.1.1",
          "form": "저 곧 이사하거든요",
          "original_form": "저 곧 이사하거든요",
          "speaker_id": "2",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000553.1.2",
          "form": "그래서 쇼핑 엄청 하는 중이에요",
          "original_form": "그래서 쇼핑 엄청 하는 중이에요",
          "speaker_id": "2",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000553.1.3",
          "form": "오~ 이것저것 새로 구입하시는 거에요?",
          "original_form": "오~ 이것저것 새로 구입하시는 거에요?",
          "speaker_id": "1",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000553.1.4",
          "form": "네! 본가에서 나가는 거라",
          "original_form": "네! 본가에서 나가는 거라",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000553.1.5",
          "form": "싹 다 구매해야 하거든요!",
          "original_form": "싹 다 구매해야 하거든요!",
          "speaker_id": "2",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000553.1.6",
          "form": "아!!! 자취?시작하시는 거구나!!! 대단하세요~",
          "original_form": "아!!! 자취?시작하시는 거구나!!! 대단하세요~",
          "speaker_id": "1",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000553.1.7",
          "form": "그럼 살 게 진짜 많겠네요~",
          "original_form": "그럼 살 게 진짜 많겠네요~",
          "speaker_id": "1",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000553.1.8",
          "form": "네! 가전도 가구도 몽땅",
          "original_form": "네! 가전도 가구도 몽땅",
          "speaker_id": "2",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000553.1.9",
          "form": "근데 이렇게 돈이 많이 들 줄 몰랐어요 정말",
          "original_form": "근데 이렇게 돈이 많이 들 줄 몰랐어요 정말",
          "speaker_id": "2",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000553.1.10",
          "form": "냉장고도 에어컨도 엄청 비싸요",
          "original_form": "냉장고도 에어컨도 엄청 비싸요",
          "speaker_id": "2",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000553.1.11",
          "form": "그쵸그쵸..ㅠㅠ 한두푼 짜리가 아니다보니..",
          "original_form": "그쵸그쵸..ㅠㅠ 한두푼 짜리가 아니다보니..",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000553.1.12",
          "form": "큰돈 나가시겠어요..",
          "original_form": "큰돈 나가시겠어요..",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000553.1.13",
          "form": "네 모아둔 총알 다 소진중이에요",
          "original_form": "네 모아둔 총알 다 소진중이에요",
          "speaker_id": "2",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000553.1.14",
          "form": "집에 있을 때는 몰랐는데",
          "original_form": "집에 있을 때는 몰랐는데",
          "speaker_id": "2",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000553.1.15",
          "form": "정말 작은 것들도 살 게 많더라고요",
          "original_form": "정말 작은 것들도 살 게 많더라고요",
          "speaker_id": "2",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000553.1.16",
          "form": "치약 칫솔 이런 것도 다 사야하고",
          "original_form": "치약 칫솔 이런 것도 다 사야하고",
          "speaker_id": "2",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000553.1.17",
          "form": "쇼핑리스트가 어마어마하겠어요!!",
          "original_form": "쇼핑리스트가 어마어마하겠어요!!",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000553.1.18",
          "form": "계속 인터넷으로 주문하시는 건가요?",
          "original_form": "계속 인터넷으로 주문하시는 건가요?",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000553.1.19",
          "form": "네! 오프라인에서 구경하고",
          "original_form": "네! 오프라인에서 구경하고",
          "speaker_id": "2",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000553.1.20",
          "form": "구매는 보통 온라인에서!",
          "original_form": "구매는 보통 온라인에서!",
          "speaker_id": "2",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000553.1.21",
          "form": "확실히 온라인이 저렴해요!",
          "original_form": "확실히 온라인이 저렴해요!",
          "speaker_id": "2",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000553.1.22",
          "form": "오~ 현명한 소비하시네요!!",
          "original_form": "오~ 현명한 소비하시네요!!",
          "speaker_id": "1",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000553.1.23",
          "form": "당근도 애용해요!",
          "original_form": "당근도 애용해요!",
          "speaker_id": "2",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000553.1.24",
          "form": "당근 쓰세요?",
          "original_form": "당근 쓰세요?",
          "speaker_id": "2",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000553.1.25",
          "form": "당근 한 두번 해봤어요~",
          "original_form": "당근 한 두번 해봤어요~",
          "speaker_id": "1",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000553.1.26",
          "form": "당근 개발한 사람 상 줘야 해요!",
          "original_form": "당근 개발한 사람 상 줘야 해요!",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000553.1.27",
          "form": "완전 유용!",
          "original_form": "완전 유용!",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000553.1.28",
          "form": "안그래도 당근마켓 잘 되고 있는 거 같더라고요~",
          "original_form": "안그래도 당근마켓 잘 되고 있는 거 같더라고요~",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000553.1.29",
          "form": "투자도 억단위로 받고~",
          "original_form": "투자도 억단위로 받고~",
          "speaker_id": "1",
          "time": "20210819 17:28"
        }
      ]
