      "utterance": [
        {
          "id": "MDRW2100017479.1.1",
          "form": "갖고싶은 물건 있어요?",
          "original_form": "갖고싶은 물건 있어요?",
          "speaker_id": "1",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017479.1.2",
          "form": "요즘 인형이 가지고싶더라구요",
          "original_form": "요즘 인형이 가지고싶더라구요",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017479.1.3",
          "form": "무슨 인형이요?",
          "original_form": "무슨 인형이요?",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017479.1.4",
          "form": "귀여운 쿼카 인형이요",
          "original_form": "귀여운 쿼카 인형이요",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017479.1.5",
          "form": "아 그거 봤어요! 귀엽더라고요~ 쿼카 좋아하세요?",
          "original_form": "아 그거 봤어요! 귀엽더라고요~ 쿼카 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017479.1.6",
          "form": "완전 좋아해요!",
          "original_form": "완전 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017479.1.7",
          "form": "최근에 무슨 물건 구매했어요",
          "original_form": "최근에 무슨 물건 구매했어요",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017479.1.8",
          "form": "저 최근에 폰케이스 샀어요!",
          "original_form": "저 최근에 폰케이스 샀어요!",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017479.1.9",
          "form": "무슨무늬인가요?",
          "original_form": "무슨무늬인가요?",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017479.1.10",
          "form": "쿼카가 그려져있어요!",
          "original_form": "쿼카가 그려져있어요!",
          "speaker_id": "2",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017479.1.11",
          "form": "쿼카 많이 좋아하시는구나! 당근마켓 사용해봤어요?",
          "original_form": "쿼카 많이 좋아하시는구나! 당근마켓 사용해봤어요?",
          "speaker_id": "1",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017479.1.12",
          "form": "몇번 사용해봤어요!",
          "original_form": "몇번 사용해봤어요!",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017479.1.13",
          "form": "판매해봤어요?",
          "original_form": "판매해봤어요?",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017479.1.14",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017479.1.15",
          "form": "당근마켓에서 책 팔아봤어요",
          "original_form": "당근마켓에서 책 팔아봤어요",
          "speaker_id": "2",
