      "utterance": [
        {
          "id": "MDRW2100038860.1.1",
          "form": "제일 하기 싫은 집안일이 뭐에요?",
          "original_form": "제일 하기 싫은 집안일이 뭐에요?",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100038860.1.2",
          "form": "저는 화장실청소?",
          "original_form": "저는 화장실청소?",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100038860.1.3",
          "form": "아 근데 그냥 다하기 싫어요ㅋㅋㅋ",
          "original_form": "아 근데 그냥 다하기 싫어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100038860.1.4",
          "form": "그렇죠 다 하기 싫죠ㅋㅋㅋ",
          "original_form": "그렇죠 다 하기 싫죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100038860.1.5",
          "form": "정말 돈이 많아서",
          "original_form": "정말 돈이 많아서",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100038860.1.6",
          "form": "안 하고 싶어요",
          "original_form": "안 하고 싶어요",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100038860.1.7",
          "form": "그쵸 전면 외주화",
          "original_form": "그쵸 전면 외주화",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100038860.1.8",
          "form": "집안일 자동화 로봇 언제 나오나ㅜㅜㅜ",
          "original_form": "집안일 자동화 로봇 언제 나오나ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100038860.1.9",
          "form": "정말 빨리 나왔으면 좋겠어요ㅋㅋㅋㅋ",
          "original_form": "정말 빨리 나왔으면 좋겠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100038860.1.10",
          "form": "로봇청소기는 영 성능이 별로더라구요 저는",
          "original_form": "로봇청소기는 영 성능이 별로더라구요 저는",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100038860.1.11",
          "form": "삶의 질이 높아질 텐데ㅋㅋㅋ",
          "original_form": "삶의 질이 높아질 텐데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100038860.1.12",
          "form": "저도 로봇 청소기는",
          "original_form": "저도 로봇 청소기는",
          "speaker_id": "1",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100038860.1.13",
          "form": "당근으로 보냈어요ㅋㅋ",
          "original_form": "당근으로 보냈어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100038860.1.14",
          "form": "완전 수직상승이죠",
          "original_form": "완전 수직상승이죠",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100038860.1.15",
          "form": "ㅋㅋㅋㅋ당근",
          "original_form": "ㅋㅋㅋㅋ당근",
          "speaker_id": "2",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038860.1.16",
          "form": "바닥에 물건 치우다가 더 스트레스 받더라구요ㅋㅋ",
          "original_form": "바닥에 물건 치우다가 더 스트레스 받더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038860.1.17",
          "form": "다른집에서는 쓰임새있게 잘 쓰이길",
          "original_form": "다른집에서는 쓰임새있게 잘 쓰이길",
          "speaker_id": "2",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038860.1.18",
          "form": "치우는 김에 그냥 내가 청소기 돌리는 게 더 낫더라구요",
          "original_form": "치우는 김에 그냥 내가 청소기 돌리는 게 더 낫더라구요",
          "speaker_id": "1",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038860.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038860.1.20",
          "form": "백번동감입니다",
          "original_form": "백번동감입니다",
