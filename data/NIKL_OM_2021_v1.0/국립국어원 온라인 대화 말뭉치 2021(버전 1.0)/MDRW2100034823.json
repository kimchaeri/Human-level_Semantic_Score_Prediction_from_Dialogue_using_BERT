      "utterance": [
        {
          "id": "MDRW2100034823.1.1",
          "form": "네네 ㅋㅋ 바운스볼 잘했었어욬ㅋㄱ",
          "original_form": "네네 ㅋㅋ 바운스볼 잘했었어욬ㅋㄱ",
          "speaker_id": "2",
          "time": "20210828 20:07"
        },
        {
          "id": "MDRW2100034823.1.2",
          "form": "애니팡은 못했구요 ㅋㅋ",
          "original_form": "애니팡은 못했구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:07"
        },
        {
          "id": "MDRW2100034823.1.3",
          "form": "애니팡 이후 캔디팡도 유행했었죠ㅋㅋㅋㅋ",
          "original_form": "애니팡 이후 캔디팡도 유행했었죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:10"
        },
        {
          "id": "MDRW2100034823.1.4",
          "form": "아 캔디팡 ㅋㅋ 오랜만이에요",
          "original_form": "아 캔디팡 ㅋㅋ 오랜만이에요",
          "speaker_id": "2",
          "time": "20210828 20:11"
        },
        {
          "id": "MDRW2100034823.1.5",
          "form": "전 그런 게임을 못해요",
          "original_form": "전 그런 게임을 못해요",
          "speaker_id": "2",
          "time": "20210828 20:11"
        },
        {
          "id": "MDRW2100034823.1.6",
          "form": "세개 맞춰서 터트리는거 ㅋㅋ",
          "original_form": "세개 맞춰서 터트리는거 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:11"
        },
        {
          "id": "MDRW2100034823.1.7",
          "form": "혹시 템플런도 아시나요?",
          "original_form": "혹시 템플런도 아시나요?",
          "speaker_id": "1",
          "time": "20210828 20:15"
        },
        {
          "id": "MDRW2100034823.1.8",
          "form": "와 진짜 추억이네요 ㅋㅋㅋㄱ헐",
          "original_form": "와 진짜 추억이네요 ㅋㅋㅋㄱ헐",
          "speaker_id": "2",
          "time": "20210828 20:15"
        },
        {
          "id": "MDRW2100034823.1.9",
          "form": "열심히 했는데 ㅋㅋㅋㅋ",
          "original_form": "열심히 했는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:15"
        },
        {
          "id": "MDRW2100034823.1.10",
          "form": "진짜 쉬는시간마다 애들이 다 갤럭시2, 노트2 붙잡고 했던ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 쉬는시간마다 애들이 다 갤럭시2, 노트2 붙잡고 했던ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:19"
        },
        {
          "id": "MDRW2100034823.1.11",
          "form": "아 폰도 추억이네요 ㅋㅋㅋㅋ",
          "original_form": "아 폰도 추억이네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:20"
        },
        {
          "id": "MDRW2100034823.1.12",
          "form": "그거 막 기록 잘나오면 자랑하고 그랬잖아요 ㅋㅋ",
          "original_form": "그거 막 기록 잘나오면 자랑하고 그랬잖아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100034823.1.13",
          "form": "맞아요ㅠㅜㅠ 꼭 반에 한 명씩 엄청난 고수가 있죠ㅋㅋ",
          "original_form": "맞아요ㅠㅜㅠ 꼭 반에 한 명씩 엄청난 고수가 있죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100034823.1.14",
          "form": "전 그 고수를 부러워했죠 항상 ㅋㅋ",
          "original_form": "전 그 고수를 부러워했죠 항상 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:25"
        },
        {
          "id": "MDRW2100034823.1.15",
          "form": "속도만 빨라지면 떨어졌어요 ㅋㅋ",
          "original_form": "속도만 빨라지면 떨어졌어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:25"
        },
        {
          "id": "MDRW2100034823.1.16",
          "form": "속도가 높아지면 어....어?  하다가 쥭....ㅠㅠ",
          "original_form": "속도가 높아지면 어....어?  하다가 쥭....ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100034823.1.17",
          "form": "맞아요 ㅋㅋㅋㅋ 그 드래곤 플라이트도 아시나요?",
          "original_form": "맞아요 ㅋㅋㅋㅋ 그 드래곤 플라이트도 아시나요?",
          "speaker_id": "2",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100034823.1.18",
          "form": "알아요!! 그거 레벨업 시키느라 많이 했었어요ㅋㅋㅋ",
          "original_form": "알아요!! 그거 레벨업 시키느라 많이 했었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:40"
        },
        {
          "id": "MDRW2100034823.1.19",
          "form": "맞죠 ㅋㅋㄱ 그것도 막 옵티머스 폰 그런걸로 한거같아요 ㅋㅋㅋ",
          "original_form": "맞죠 ㅋㅋㄱ 그것도 막 옵티머스 폰 그런걸로 한거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
