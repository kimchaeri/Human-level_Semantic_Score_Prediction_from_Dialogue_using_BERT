        },
        {
          "id": "MDRW2100013173.1.2",
          "form": "오늘 일요일이라서?",
          "original_form": "오늘 일요일이라서?",
          "speaker_id": "1",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100013173.1.3",
          "form": "을",
          "original_form": "을",
          "speaker_id": "2",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100013173.1.4",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100013173.1.5",
          "form": "내일 가겠네",
          "original_form": "내일 가겠네",
          "speaker_id": "1",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100013173.1.6",
          "form": "어때?",
          "original_form": "어때?",
          "speaker_id": "1",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100013173.1.7",
          "form": "싫어 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "싫어 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100013173.1.8",
          "form": "내일 가면 몇 시에 마쳐?",
          "original_form": "내일 가면 몇 시에 마쳐?",
          "speaker_id": "1",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100013173.1.9",
          "form": "아마 5시 전에?",
          "original_form": "아마 5시 전에?",
          "speaker_id": "2",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100013173.1.10",
          "form": "요즘은 방과후 안하지",
          "original_form": "요즘은 방과후 안하지",
          "speaker_id": "1",
          "time": "20210905 15:19"
        },
        {
