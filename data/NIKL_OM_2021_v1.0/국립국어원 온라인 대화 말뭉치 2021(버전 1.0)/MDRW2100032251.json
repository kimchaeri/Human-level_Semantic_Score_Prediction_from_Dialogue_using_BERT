      "utterance": [
        {
          "id": "MDRW2100032251.1.1",
          "form": "페이커라는 프로게이머 아세요",
          "original_form": "페이커라는 프로게이머 아세요",
          "speaker_id": "1",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100032251.1.2",
          "form": "리그오브레전드에서 제일 유명한 선수예요 외국인들도 다들 알고 있고요",
          "original_form": "리그오브레전드에서 제일 유명한 선수예요 외국인들도 다들 알고 있고요",
          "speaker_id": "1",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100032251.1.3",
          "form": "게임도 스포츠 이지효",
          "original_form": "게임도 스포츠 이지효",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100032251.1.4",
          "form": "심성도 착하고 책도 많이 읽고 생각이 2인 친구 이지요",
          "original_form": "심성도 착하고 책도 많이 읽고 생각이 2인 친구 이지요",
          "speaker_id": "1",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100032251.1.5",
          "form": "베이커를 모를 수가 없지요",
          "original_form": "베이커를 모를 수가 없지요",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100032251.1.6",
          "form": "이 스포츠도 스포츠의 한 종류 이지요",
          "original_form": "이 스포츠도 스포츠의 한 종류 이지요",
          "speaker_id": "1",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100032251.1.7",
          "form": "전 세계 탑 이잖아요",
          "original_form": "전 세계 탑 이잖아요",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100032251.1.8",
          "form": "요즘은 페이커 선수 폼이 좀 떨어진 것 같아요",
          "original_form": "요즘은 페이커 선수 폼이 좀 떨어진 것 같아요",
          "speaker_id": "1",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032251.1.9",
          "form": "겸손 하고요",
          "original_form": "겸손 하고요",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032251.1.10",
          "form": "예전에는 범접할 수 없었는데 나이가 되면 어쩔 수 없나 봐요",
          "original_form": "예전에는 범접할 수 없었는데 나이가 되면 어쩔 수 없나 봐요",
          "speaker_id": "1",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032251.1.11",
          "form": "요새는 안봐서 모르겠네요",
          "original_form": "요새는 안봐서 모르겠네요",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032251.1.12",
          "form": "하지만 그래도 저는 페이커 응원하고 좋아합니다",
          "original_form": "하지만 그래도 저는 페이커 응원하고 좋아합니다",
          "speaker_id": "1",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032251.1.13",
          "form": "외모가 늙었다는 건가요",
          "original_form": "외모가 늙었다는 건가요",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032251.1.14",
          "form": "실력이 떨어졌나요",
          "original_form": "실력이 떨어졌나요",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032251.1.15",
          "form": "아니요 10대가 아니고 20대 초중반으로 나이가 들면서",
          "original_form": "아니요 10대가 아니고 20대 초중반으로 나이가 들면서",
          "speaker_id": "1",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032251.1.16",
          "form": "손의 피지컬 같은 것들이 조금 무뎌졌다는 것이지요",
          "original_form": "손의 피지컬 같은 것들이 조금 무뎌졌다는 것이지요",
          "speaker_id": "1",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032251.1.17",
          "form": "20대면 아직 애기 얘기하네요",
          "original_form": "20대면 아직 애기 얘기하네요",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032251.1.18",
          "form": "아직 군대도 안 갔을 거예요",
          "original_form": "아직 군대도 안 갔을 거예요",
          "speaker_id": "1",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032251.1.19",
          "form": "임요환은 아직도 하죠",
          "original_form": "임요환은 아직도 하죠",
          "speaker_id": "2",
          "time": "20210905 14:31"
