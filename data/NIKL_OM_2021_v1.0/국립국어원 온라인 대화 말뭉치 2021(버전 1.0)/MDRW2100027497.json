          "speaker_id": "1",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027497.1.3",
          "form": "우오~",
          "original_form": "우오~",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027497.1.4",
          "form": "가을이네요",
          "original_form": "가을이네요",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027497.1.5",
          "form": "가을...",
          "original_form": "가을...",
          "speaker_id": "1",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027497.1.6",
          "form": "요 며칠 비 엄청 내렸는데",
          "original_form": "요 며칠 비 엄청 내렸는데",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027497.1.7",
          "form": "얀님네는 어땠는지",
          "original_form": "얀님네는 어땠는지",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027497.1.8",
          "form": "헐 맞아요",
          "original_form": "헐 맞아요",
          "speaker_id": "1",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027497.1.9",
          "form": "어제도",
          "original_form": "어제도",
          "speaker_id": "1",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027497.1.10",
          "form": "비왔어요",
          "original_form": "비왔어요",
          "speaker_id": "1",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027497.1.11",
          "form": "맞어맞어",
          "original_form": "맞어맞어",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027497.1.12",
          "form": "우산 없으면 안되겠더라구요",
          "original_form": "우산 없으면 안되겠더라구요",
          "speaker_id": "1",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027497.1.13",
          "form": "저는 그제? 좀 멀리 나갔다오려고 했는데 비가 내리 와가지구",
          "original_form": "저는 그제? 좀 멀리 나갔다오려고 했는데 비가 내리 와가지구",
          "speaker_id": "2",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027497.1.14",
          "form": "허얼",
          "original_form": "허얼",
          "speaker_id": "1",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027497.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027497.1.16",
          "form": "꼭 집 나갈때만 비와",
          "original_form": "꼭 집 나갈때만 비와",
          "speaker_id": "2",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027497.1.17",
          "form": "그거 봤어요",
          "original_form": "그거 봤어요",
          "speaker_id": "1",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027497.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027497.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027497.1.20",
          "form": "어디 가려구 햇는데 비온다고 올리셧던 거",
          "original_form": "어디 가려구 햇는데 비온다고 올리셧던 거",
          "speaker_id": "1",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027497.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027497.1.22",
          "form": "그래서 요즘에는 비도 많이오고 해선지",
          "original_form": "그래서 요즘에는 비도 많이오고 해선지",
          "speaker_id": "2",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027497.1.23",
          "form": "밤에 춥더라구요",
          "original_form": "밤에 춥더라구요",
          "speaker_id": "2",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027497.1.24",
          "form": "맞아요 저 여름에 창문 열어놓고 자는데",
          "original_form": "맞아요 저 여름에 창문 열어놓고 자는데",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027497.1.25",
          "form": "요즘엔... 못하겠어요",
          "original_form": "요즘엔... 못하겠어요",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027497.1.26",
          "form": "저두...",
          "original_form": "저두...",
          "speaker_id": "2",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027497.1.27",
          "form": "감기 걸릴듯",
          "original_form": "감기 걸릴듯",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027497.1.28",
          "form": "아니면 뭐 걸치고 자거나 해야겠는",
          "original_form": "아니면 뭐 걸치고 자거나 해야겠는",
          "speaker_id": "2",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027497.1.29",
          "form": "마자요 ㅋㅋㅋㅋ가디건 같은거",
          "original_form": "마자요 ㅋㅋㅋㅋ가디건 같은거",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027497.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아",
          "speaker_id": "2",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027497.1.31",
          "form": "근데 이번 가을도 엄청 후딱 갈 것 같어요",
          "original_form": "근데 이번 가을도 엄청 후딱 갈 것 같어요",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027497.1.32",
          "form": "벌써 9월이란게 실감이 나는...",
          "original_form": "벌써 9월이란게 실감이 나는...",
          "speaker_id": "2",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027497.1.33",
          "form": "...금방 겨울이겟죠",
          "original_form": "...금방 겨울이겟죠",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027497.1.34",
          "form": "꺅 붕어빵",
          "original_form": "꺅 붕어빵",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027497.1.35",
          "form": "또 금방 추워지려나... 웅",
          "original_form": "또 금방 추워지려나... 웅",
          "speaker_id": "2",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027497.1.36",
          "form": "붕어빵좋아",
          "original_form": "붕어빵좋아",
          "speaker_id": "2",
          "time": "20210902 13:12"
        },
