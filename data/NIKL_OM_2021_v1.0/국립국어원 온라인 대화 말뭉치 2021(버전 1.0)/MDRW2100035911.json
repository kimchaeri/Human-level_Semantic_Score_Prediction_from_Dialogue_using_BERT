      "utterance": [
        {
          "id": "MDRW2100035911.1.1",
          "form": "오 혹시 뷰티 관심많으신가요?",
          "original_form": "오 혹시 뷰티 관심많으신가요?",
          "speaker_id": "2",
          "time": "20210830 11:19"
        },
        {
          "id": "MDRW2100035911.1.2",
          "form": "뷰티에 관심 많고 뷰티쪽에서 일을 했었어요~",
          "original_form": "뷰티에 관심 많고 뷰티쪽에서 일을 했었어요~",
          "speaker_id": "1",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100035911.1.3",
          "form": "결혼하고 애 낳고는",
          "original_form": "결혼하고 애 낳고는",
          "speaker_id": "1",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100035911.1.4",
          "form": "말 안해도 아시죠ㅠㅋㅋ",
          "original_form": "말 안해도 아시죠ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100035911.1.5",
          "form": "오 대박! 저 코덕이에요 ㅋㅋㅋㅋㅋ",
          "original_form": "오 대박! 저 코덕이에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:22"
        },
        {
          "id": "MDRW2100035911.1.6",
          "form": "우와 뷰티쪽일하셧다니 부러워요ㅠ",
          "original_form": "우와 뷰티쪽일하셧다니 부러워요ㅠ",
          "speaker_id": "2",
          "time": "20210830 11:23"
        },
        {
          "id": "MDRW2100035911.1.7",
          "form": "근데 좋아하는걸 직업으로삼으면 힘들어진다드라구요",
          "original_form": "근데 좋아하는걸 직업으로삼으면 힘들어진다드라구요",
          "speaker_id": "2",
          "time": "20210830 11:23"
        },
        {
          "id": "MDRW2100035911.1.8",
          "form": "맞아요 화장하는 거 엄청 좋아했는데",
          "original_form": "맞아요 화장하는 거 엄청 좋아했는데",
          "speaker_id": "1",
          "time": "20210830 11:23"
        },
        {
          "id": "MDRW2100035911.1.9",
          "form": "내가 좋아하는 화장 가 직업적으로 해야 하는 화장은 다르니",
          "original_form": "내가 좋아하는 화장 가 직업적으로 해야 하는 화장은 다르니",
          "speaker_id": "1",
          "time": "20210830 11:23"
        },
        {
          "id": "MDRW2100035911.1.10",
          "form": "흥미를 잃었어요ㅠ",
          "original_form": "흥미를 잃었어요ㅠ",
          "speaker_id": "1",
          "time": "20210830 11:23"
        },
        {
          "id": "MDRW2100035911.1.11",
          "form": "어떤 브랜드 좋아하세요",
          "original_form": "어떤 브랜드 좋아하세요",
          "speaker_id": "1",
          "time": "20210830 11:23"
        },
        {
          "id": "MDRW2100035911.1.12",
          "form": "아 저런.. ,,ㅠㅠㅠ 저는 국내는 페리페라 어퓨좋아하구 해외는 로라 에스티로더 좋아해여ㅎㅎ",
          "original_form": "아 저런.. ,,ㅠㅠㅠ 저는 국내는 페리페라 어퓨좋아하구 해외는 로라 에스티로더 좋아해여ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 11:26"
        },
        {
          "id": "MDRW2100035911.1.13",
          "form": "로라메르시에는 진짜 화장품 좋어하시는분만 아시던데 ㅋㅋㅋㅋㅋ",
          "original_form": "로라메르시에는 진짜 화장품 좋어하시는분만 아시던데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:28"
        },
        {
          "id": "MDRW2100035911.1.14",
          "form": "코덕맞으십니당 ㅋㅋㅋㅋ",
          "original_form": "코덕맞으십니당 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:28"
        },
        {
          "id": "MDRW2100035911.1.15",
          "form": "앜ㅋㅋㅋㅋ 그런가요?ㅋㅋㅋㅋ 갠적으로 섀도 질이 넘사라 넘좋아해요~ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ 그런가요?ㅋㅋㅋㅋ 갠적으로 섀도 질이 넘사라 넘좋아해요~ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:31"
        },
        {
          "id": "MDRW2100035911.1.16",
          "form": "맞아요 펄 질감이 다르죠^^",
          "original_form": "맞아요 펄 질감이 다르죠^^",
          "speaker_id": "1",
          "time": "20210830 11:32"
        },
        {
          "id": "MDRW2100035911.1.17",
          "form": "열심히 꾸미고 가꾸셔요~",
          "original_form": "열심히 꾸미고 가꾸셔요~",
          "speaker_id": "1",
          "time": "20210830 11:32"
        },
        {
          "id": "MDRW2100035911.1.18",
          "form": "여자는 진짜 그럴때가 제잉 행벅한듯요~",
          "original_form": "여자는 진짜 그럴때가 제잉 행벅한듯요~",
          "speaker_id": "1",
          "time": "20210830 11:32"
        },
        {
          "id": "MDRW2100035911.1.19",
          "form": "맞아요 ㅠㅠ 혹시 결혼하시고 애낳으시고도 화장 자주 하시나요??",
          "original_form": "맞아요 ㅠㅠ 혹시 결혼하시고 애낳으시고도 화장 자주 하시나요??",
          "speaker_id": "2",
          "time": "20210830 11:35"
        },
        {
          "id": "MDRW2100035911.1.20",
          "form": "사람 나름인 거 같은데 저는 잘 안 해요",
          "original_form": "사람 나름인 거 같은데 저는 잘 안 해요",
          "speaker_id": "1",
          "time": "20210830 11:44"
        },
        {
          "id": "MDRW2100035911.1.21",
          "form": "지우기도 귀찮고 아기한테도 묻고ㅠ",
          "original_form": "지우기도 귀찮고 아기한테도 묻고ㅠ",
          "speaker_id": "1",
          "time": "20210830 11:44"
        },
        {
          "id": "MDRW2100035911.1.22",
          "form": "그러다 보니 이제 애기가 커도 귀찮아요ㅠ",
          "original_form": "그러다 보니 이제 애기가 커도 귀찮아요ㅠ",
          "speaker_id": "1",
          "time": "20210830 11:44"
        },
        {
          "id": "MDRW2100035911.1.23",
          "form": "아하 ㅋㅋㅋㅋ 듣자하니 저두 결혼전에 최대치로 꾸미고다녀야겠어요ㅠㅠ",
          "original_form": "아하 ㅋㅋㅋㅋ 듣자하니 저두 결혼전에 최대치로 꾸미고다녀야겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035911.1.24",
          "form": "결혼해서도 잘 꾸미 실 거 같아요%%",
          "original_form": "결혼해서도 잘 꾸미 실 거 같아요%%",
