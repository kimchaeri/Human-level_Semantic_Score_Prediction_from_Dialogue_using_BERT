      "utterance": [
        {
          "id": "MDRW2100046182.1.1",
          "form": "동인비 쿠폰주네요",
          "original_form": "동인비 쿠폰주네요",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046182.1.2",
          "form": "자생크림",
          "original_form": "자생크림",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046182.1.3",
          "form": "어디서요?",
          "original_form": "어디서요?",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046182.1.4",
          "form": "카톡 플친에서요",
          "original_form": "카톡 플친에서요",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046182.1.5",
          "form": "7미리예요",
          "original_form": "7미리예요",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046182.1.6",
          "form": "아 받아놔야겠네요",
          "original_form": "아 받아놔야겠네요",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046182.1.7",
          "form": "백화점 가야되는거겠죠?",
          "original_form": "백화점 가야되는거겠죠?",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046182.1.8",
          "form": "아마 그럴듯요",
          "original_form": "아마 그럴듯요",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046182.1.9",
          "form": "자꾸 받으러만가서",
          "original_form": "자꾸 받으러만가서",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046182.1.10",
          "form": "미안해지네요 ㅎ",
          "original_form": "미안해지네요 ㅎ",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046182.1.11",
          "form": "우선 받아놓고 고민해야겠어요",
          "original_form": "우선 받아놓고 고민해야겠어요",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046182.1.12",
          "form": "동인비가 전반적으로 괜찮은듯",
          "original_form": "동인비가 전반적으로 괜찮은듯",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046182.1.13",
          "form": "저두 그래도",
          "original_form": "저두 그래도",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046182.1.14",
          "form": "싱글에센스 좋은거같아요",
          "original_form": "싱글에센스 좋은거같아요",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046182.1.15",
          "form": "제품이 괜찮아서",
          "original_form": "제품이 괜찮아서",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046182.1.16",
          "form": "다쓰면 본품사볼까하는데",
          "original_form": "다쓰면 본품사볼까하는데",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046182.1.17",
          "form": "팩도 좋아요",
          "original_form": "팩도 좋아요",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046182.1.18",
          "form": "샘플이 줄지가 않아요",
          "original_form": "샘플이 줄지가 않아요",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046182.1.19",
          "form": "계속 주니 ㅋㅋ",
          "original_form": "계속 주니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046182.1.20",
          "form": "본품 넘비싸지않아요?",
          "original_form": "본품 넘비싸지않아요?",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046182.1.21",
          "form": "비싸겠죠 ㅎ",
          "original_form": "비싸겠죠 ㅎ",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046182.1.22",
          "form": "아직 저도 샘플 넘많아서ㅋ",
          "original_form": "아직 저도 샘플 넘많아서ㅋ",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046182.1.23",
          "form": "그냥 샘플만 써도 충분할듯해요",
          "original_form": "그냥 샘플만 써도 충분할듯해요",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046182.1.24",
          "form": "계속주니 ㅋㅋ",
          "original_form": "계속주니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046182.1.25",
          "form": "팩이 젤타입이였는데 시트로 바꼈더라구요",
          "original_form": "팩이 젤타입이였는데 시트로 바꼈더라구요",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046182.1.26",
          "form": "예전게 더좋은듯",
          "original_form": "예전게 더좋은듯",
          "speaker_id": "1",
          "time": "20210908 11:05"
        }
      ]
