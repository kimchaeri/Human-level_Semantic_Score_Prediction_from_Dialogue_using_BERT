          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100013202.1.6",
          "form": "오 어디 꺼 드셨어요?",
          "original_form": "오 어디 꺼 드셨어요?",
          "speaker_id": "2",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100013202.1.7",
          "form": "파파존스요",
          "original_form": "파파존스요",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100013202.1.8",
          "form": "전 집에서 밥이랑 반찬이랑 해서 먹었어요.",
          "original_form": "전 집에서 밥이랑 반찬이랑 해서 먹었어요.",
          "speaker_id": "2",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100013202.1.9",
          "form": "새로 가입해서 25%쿠폰 받았어요",
          "original_form": "새로 가입해서 25%쿠폰 받았어요",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100013202.1.10",
          "form": "토마토 달걀 볶음 했어요",
          "original_form": "토마토 달걀 볶음 했어요",
          "speaker_id": "2",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100013202.1.11",
          "form": "오 그거 좋아하시나봐요",
          "original_form": "오 그거 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100013202.1.12",
          "form": "맛있더라구요",
          "original_form": "맛있더라구요",
