      "utterance": [
        {
          "id": "MDRW2100031118.1.1",
          "form": "오늘 10km 걷고 집 와서 파스타 팔팔 끼려먹었는데",
          "original_form": "오늘 10km 걷고 집 와서 파스타 팔팔 끼려먹었는데",
          "speaker_id": "2",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100031118.1.2",
          "form": "지금껏 만들어 먹은 것중에서 제일 맛있었어",
          "original_form": "지금껏 만들어 먹은 것중에서 제일 맛있었어",
          "speaker_id": "2",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100031118.1.3",
          "form": "역시 최고의 반찬은 시장이다",
          "original_form": "역시 최고의 반찬은 시장이다",
          "speaker_id": "2",
          "time": "20210905 22:15"
        },
        {
          "id": "MDRW2100031118.1.4",
          "form": "우와 대단하다 무슨 10키로나 걸었냐",
          "original_form": "우와 대단하다 무슨 10키로나 걸었냐",
          "speaker_id": "1",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100031118.1.5",
          "form": "무적다리네",
          "original_form": "무적다리네",
          "speaker_id": "1",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100031118.1.6",
          "form": "저녁ㅇㅡ로 파스타먹었니",
          "original_form": "저녁ㅇㅡ로 파스타먹었니",
          "speaker_id": "1",
          "time": "20210905 23:58"
        },
        {
          "id": "MDRW2100031118.1.7",
          "form": "응 오늘도 토마토파스타 먹었는데",
          "original_form": "응 오늘도 토마토파스타 먹었는데",
          "speaker_id": "2",
          "time": "20210906 00:03"
        },
        {
          "id": "MDRW2100031118.1.8",
          "form": "사실 뭘 먹어도 상관은 없었지만 소스 까놓은 게 좀 오래돼서 얼른 다 먹으려고 파스타 했어",
          "original_form": "사실 뭘 먹어도 상관은 없었지만 소스 까놓은 게 좀 오래돼서 얼른 다 먹으려고 파스타 했어",
          "speaker_id": "2",
          "time": "20210906 00:03"
        },
        {
          "id": "MDRW2100031118.1.9",
          "form": "넌 저녁 뭐 먹었니",
          "original_form": "넌 저녁 뭐 먹었니",
          "speaker_id": "2",
          "time": "20210906 00:03"
        },
        {
          "id": "MDRW2100031118.1.10",
          "form": "내가 맞춰볼개",
          "original_form": "내가 맞춰볼개",
          "speaker_id": "2",
          "time": "20210906 00:03"
        },
        {
          "id": "MDRW2100031118.1.11",
          "form": "어때",
          "original_form": "어때",
          "speaker_id": "2",
          "time": "20210906 00:04"
        },
        {
          "id": "MDRW2100031118.1.12",
          "form": "좋다 오늘저녁도 맞춰봐",
          "original_form": "좋다 오늘저녁도 맞춰봐",
          "speaker_id": "1",
          "time": "20210906 00:04"
        },
        {
          "id": "MDRW2100031118.1.13",
          "form": "매운 음식 먹었니?",
          "original_form": "매운 음식 먹었니?",
          "speaker_id": "2",
          "time": "20210906 00:04"
        },
        {
          "id": "MDRW2100031118.1.14",
          "form": "매운양념인데 안매웠어",
          "original_form": "매운양념인데 안매웠어",
          "speaker_id": "1",
          "time": "20210906 00:05"
        },
        {
          "id": "MDRW2100031118.1.15",
          "form": "빨간색 음식이야?",
          "original_form": "빨간색 음식이야?",
          "speaker_id": "2",
          "time": "20210906 00:06"
        },
        {
          "id": "MDRW2100031118.1.16",
          "form": "응 빨간색음식이야",
          "original_form": "응 빨간색음식이야",
          "speaker_id": "1",
          "time": "20210906 00:07"
        },
        {
          "id": "MDRW2100031118.1.17",
          "form": "국물요리야?",
          "original_form": "국물요리야?",
          "speaker_id": "2",
          "time": "20210906 00:07"
        },
        {
          "id": "MDRW2100031118.1.18",
          "form": "아니 국물이 거의 없어",
          "original_form": "아니 국물이 거의 없어",
          "speaker_id": "1",
          "time": "20210906 00:08"
        },
        {
          "id": "MDRW2100031118.1.19",
          "form": "그럼 찜 요리야?",
          "original_form": "그럼 찜 요리야?",
          "speaker_id": "2",
          "time": "20210906 00:09"
        },
        {
          "id": "MDRW2100031118.1.20",
          "form": "찜요리는 아니야",
          "original_form": "찜요리는 아니야",
          "speaker_id": "1",
          "time": "20210906 00:09"
        },
        {
          "id": "MDRW2100031118.1.21",
          "form": "그럼 볶음요리니?",
          "original_form": "그럼 볶음요리니?",
          "speaker_id": "2",
          "time": "20210906 00:09"
        },
        {
          "id": "MDRW2100031118.1.22",
          "form": "아마 볶아서 나올거야",
          "original_form": "아마 볶아서 나올거야",
          "speaker_id": "1",
          "time": "20210906 00:10"
        },
        {
          "id": "MDRW2100031118.1.23",
          "form": "양념 묻혀서 구운요리야?",
          "original_form": "양념 묻혀서 구운요리야?",
          "speaker_id": "2",
          "time": "20210906 00:10"
        },
        {
          "id": "MDRW2100031118.1.24",
          "form": "그런 방법도 있을것같아",
          "original_form": "그런 방법도 있을것같아",
          "speaker_id": "1",
          "time": "20210906 00:11"
        },
        {
          "id": "MDRW2100031118.1.25",
          "form": "재료를 삶기도 해?",
          "original_form": "재료를 삶기도 해?",
          "speaker_id": "2",
          "time": "20210906 00:11"
        },
        {
          "id": "MDRW2100031118.1.26",
          "form": "재료는 뭐든 가능해",
          "original_form": "재료는 뭐든 가능해",
          "speaker_id": "1",
          "time": "20210906 00:12"
        }
      ]
    }
  ]
