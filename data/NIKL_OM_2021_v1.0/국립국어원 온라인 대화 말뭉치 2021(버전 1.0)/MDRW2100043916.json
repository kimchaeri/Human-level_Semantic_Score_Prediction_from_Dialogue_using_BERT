      "utterance": [
        {
          "id": "MDRW2100043916.1.1",
          "form": "좋아하는 음식있으세요?",
          "original_form": "좋아하는 음식있으세요?",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043916.1.2",
          "form": "치킨이요!!!",
          "original_form": "치킨이요!!!",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100043916.1.3",
          "form": "어느브랜드 치킨좋아하세요?",
          "original_form": "어느브랜드 치킨좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100043916.1.4",
          "form": "네네치킨이요!!",
          "original_form": "네네치킨이요!!",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100043916.1.5",
          "form": "네네치킨 한 번도 안먹어봤는데",
          "original_form": "네네치킨 한 번도 안먹어봤는데",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100043916.1.6",
          "form": "네네에서 어떤 치킨이 제일 맛있나용",
          "original_form": "네네에서 어떤 치킨이 제일 맛있나용",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100043916.1.7",
          "form": "저는 매콤치즈스노윙 좋아해요!",
          "original_form": "저는 매콤치즈스노윙 좋아해요!",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100043916.1.8",
          "form": "근데 이게 호불호가 좀 갈리더라고요",
          "original_form": "근데 이게 호불호가 좀 갈리더라고요",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100043916.1.9",
          "form": "엇 왜요??",
          "original_form": "엇 왜요??",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100043916.1.10",
          "form": "맵기도하고 자극적인 맛이라",
          "original_form": "맵기도하고 자극적인 맛이라",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100043916.1.11",
          "form": "많이 매운가요??",
          "original_form": "많이 매운가요??",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100043916.1.12",
          "form": "과자시즈닝 느낌이에요ㅋㅋ 그래서 갈리는것같아요",
          "original_form": "과자시즈닝 느낌이에요ㅋㅋ 그래서 갈리는것같아요",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100043916.1.13",
          "form": "저는 극호에요!",
          "original_form": "저는 극호에요!",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100043916.1.14",
          "form": "아아ㅋㅋㅋ나중에 한번 먹어봐야겠네요ㅋㅋ",
          "original_form": "아아ㅋㅋㅋ나중에 한번 먹어봐야겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100043916.1.15",
          "form": "저 과자시즈닝 좋아하거든요",
          "original_form": "저 과자시즈닝 좋아하거든요",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100043916.1.16",
          "form": "매운맛도 좋아한다면 맞을거에요!!",
          "original_form": "매운맛도 좋아한다면 맞을거에요!!",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100043916.1.17",
          "form": "매운맛은.. ㅜㅜ맵찔이라...",
          "original_form": "매운맛은.. ㅜㅜ맵찔이라...",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100043916.1.18",
          "form": "쿨피스놓고 도전해야하려나요ㅋ",
          "original_form": "쿨피스놓고 도전해야하려나요ㅋ",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100043916.1.19",
          "form": "그럼 많이 매우실수도있을것같아요!!",
          "original_form": "그럼 많이 매우실수도있을것같아요!!",
          "speaker_id": "1",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100043916.1.20",
          "form": "쿨피스놓고 드셔보세요ㅋㅋ",
          "original_form": "쿨피스놓고 드셔보세요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:18"
        }
