      "utterance": [
        {
          "id": "MDRW2100000640.1.1",
          "form": "취업도 힘들고 집 사기도 힘드네요",
          "original_form": "취업도 힘들고 집 사기도 힘드네요",
          "speaker_id": "1",
          "time": "20210819 19:11"
        },
        {
          "id": "MDRW2100000640.1.2",
          "form": "집은...ㅋㅋㅋ요즘은 은행집으로 평생 사는거죠.",
          "original_form": "집은...ㅋㅋㅋ요즘은 은행집으로 평생 사는거죠.",
          "speaker_id": "2",
          "time": "20210819 19:12"
        },
        {
          "id": "MDRW2100000640.1.3",
          "form": "그래도 의식주는 해결해야하니 취준을 하고 있지만.",
          "original_form": "그래도 의식주는 해결해야하니 취준을 하고 있지만.",
          "speaker_id": "2",
          "time": "20210819 19:12"
        },
        {
          "id": "MDRW2100000640.1.4",
          "form": "맞아요 의식이라도 벌어야죠",
          "original_form": "맞아요 의식이라도 벌어야죠",
          "speaker_id": "1",
          "time": "20210819 19:12"
        },
        {
          "id": "MDRW2100000640.1.5",
          "form": "요즘은 가끔 아르바이트하는 프리터로 지내는 것도 집만 포기하면 좋지 않나 생각이 들어요.",
          "original_form": "요즘은 가끔 아르바이트하는 프리터로 지내는 것도 집만 포기하면 좋지 않나 생각이 들어요.",
          "speaker_id": "2",
          "time": "20210819 19:13"
        },
        {
          "id": "MDRW2100000640.1.6",
          "form": "워낙 취직이 힘들어서..ㅠㅠ",
          "original_form": "워낙 취직이 힘들어서..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 19:13"
        },
        {
          "id": "MDRW2100000640.1.7",
          "form": "학교 다닐때 대학원을 고민하다가 친구들이 대학원은 도피같다고 해서 취준을 시작했는데 코로나가 이렇게 장기화 될 줄 몰랐죠.",
          "original_form": "학교 다닐때 대학원을 고민하다가 친구들이 대학원은 도피같다고 해서 취준을 시작했는데 코로나가 이렇게 장기화 될 줄 몰랐죠.",
          "speaker_id": "2",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000640.1.8",
          "form": "차라리 대학원이 나은 선택이었었네요.",
          "original_form": "차라리 대학원이 나은 선택이었었네요.",
          "speaker_id": "1",
          "time": "20210819 19:18"
        },
        {
          "id": "MDRW2100000640.1.9",
          "form": "정말로요!ㅋㅋㅋ그래서 또..끝나지 않을 것 같길래 하반기 취직이 안 되면 대학원을 정말 가려구요.",
          "original_form": "정말로요!ㅋㅋㅋ그래서 또..끝나지 않을 것 같길래 하반기 취직이 안 되면 대학원을 정말 가려구요.",
          "speaker_id": "2",
          "time": "20210819 19:21"
        },
        {
          "id": "MDRW2100000640.1.10",
          "form": "하반기 취직 화이팅!",
          "original_form": "하반기 취직 화이팅!",
          "speaker_id": "1",
          "time": "20210819 19:21"
        },
        {
          "id": "MDRW2100000640.1.11",
          "form": "파이팅!ㅋㅋㅋ name1님은 어떠세요?",
          "original_form": "파이팅!ㅋㅋㅋ &name1&님은 어떠세요?",
          "speaker_id": "2",
          "time": "20210819 19:22"
        },
        {
          "id": "MDRW2100000640.1.12",
          "form": "저는 학생인데 미래만 생각하면 암울해요",
          "original_form": "저는 학생인데 미래만 생각하면 암울해요",
          "speaker_id": "1",
          "time": "20210819 19:23"
        },
        {
          "id": "MDRW2100000640.1.13",
          "form": "헉 왜요왜요.",
          "original_form": "헉 왜요왜요.",
          "speaker_id": "2",
          "time": "20210819 19:23"
        },
        {
          "id": "MDRW2100000640.1.14",
          "form": "과가 취업 힘든과기도 하고 동기들도 다 나가고 재미가 없어요",
          "original_form": "과가 취업 힘든과기도 하고 동기들도 다 나가고 재미가 없어요",
          "speaker_id": "1",
          "time": "20210819 19:24"
        },
        {
          "id": "MDRW2100000640.1.15",
          "form": "아앗...그래도 철학과만 아니면...괜찮지 않을까요...?",
          "original_form": "아앗...그래도 철학과만 아니면...괜찮지 않을까요...?",
          "speaker_id": "2",
          "time": "20210819 19:25"
        },
        {
          "id": "MDRW2100000640.1.16",
          "form": "이과긴한데 자연대에요",
          "original_form": "이과긴한데 자연대에요",
          "speaker_id": "1",
          "time": "20210819 19:25"
        },
        {
          "id": "MDRW2100000640.1.17",
          "form": "선배님들보니 다들 공무원으로 가시더라고요",
          "original_form": "선배님들보니 다들 공무원으로 가시더라고요",
          "speaker_id": "1",
          "time": "20210819 19:26"
        },
        {
          "id": "MDRW2100000640.1.18",
          "form": "아...눈물이....",
          "original_form": "아...눈물이....",
          "speaker_id": "2",
          "time": "20210819 19:26"
        },
        {
          "id": "MDRW2100000640.1.19",
          "form": "그럼 탈주해도 문제 없긴하네요...ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "그럼 탈주해도 문제 없긴하네요...ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 19:26"
        },
        {
          "id": "MDRW2100000640.1.20",
          "form": "공기업같은 곳은 어떠세요?",
          "original_form": "공기업같은 곳은 어떠세요?",
          "speaker_id": "2",
          "time": "20210819 19:26"
        },
        {
          "id": "MDRW2100000640.1.21",
          "form": "공무원같은 공무원 아닌 멋진 연봉",
          "original_form": "공무원같은 공무원 아닌 멋진 연봉",
          "speaker_id": "2",
          "time": "20210819 19:27"
        },
        {
          "id": "MDRW2100000640.1.22",
          "form": "꿈의 직장이죠",
          "original_form": "꿈의 직장이죠",
          "speaker_id": "1",
          "time": "20210819 19:29"
        },
        {
          "id": "MDRW2100000640.1.23",
          "form": "진짜 꿈의 직장이더라구요.",
          "original_form": "진짜 꿈의 직장이더라구요.",
          "speaker_id": "2",
          "time": "20210819 19:30"
        },
        {
          "id": "MDRW2100000640.1.24",
          "form": "금융공기업은 초봉이 어마어마 하던데요.",
          "original_form": "금융공기업은 초봉이 어마어마 하던데요.",
          "speaker_id": "2",
          "time": "20210819 19:30"
        },
        {
          "id": "MDRW2100000640.1.25",
          "form": "금융쪽도 생각해봤는데 경제학과가 차지하지않을까요",
          "original_form": "금융쪽도 생각해봤는데 경제학과가 차지하지않을까요",
          "speaker_id": "1",
          "time": "20210819 19:31"
        },
        {
          "id": "MDRW2100000640.1.26",
          "form": "경제학과가 유리하긴하지만",
          "original_form": "경제학과가 유리하긴하지만",
          "speaker_id": "2",
          "time": "20210819 19:34"
        },
        {
          "id": "MDRW2100000640.1.27",
          "form": "전산...!을..!",
          "original_form": "전산...!을..!",
          "speaker_id": "2",
          "time": "20210819 19:34"
        },
        {
          "id": "MDRW2100000640.1.28",
          "form": "물론 전산은 컴공-서버 계통인것 같긴해요..!",
          "original_form": "물론 전산은 컴공-서버 계통인것 같긴해요..!",
          "speaker_id": "2",
          "time": "20210819 19:34"
        },
        {
          "id": "MDRW2100000640.1.29",
          "form": "그래도 쌩 문과 과목보다 낫지 않을까요?",
          "original_form": "그래도 쌩 문과 과목보다 낫지 않을까요?",
          "speaker_id": "2",
          "time": "20210819 19:34"
        },
        {
          "id": "MDRW2100000640.1.30",
          "form": "요즘 대세는 컴공인거같아요",
          "original_form": "요즘 대세는 컴공인거같아요",
          "speaker_id": "1",
          "time": "20210819 19:35"
        },
        {
          "id": "MDRW2100000640.1.31",
          "form": "근데 컴공...좀 ...초봉이 만들어낸 환상 같아요",
          "original_form": "근데 컴공...좀 ...초봉이 만들어낸 환상 같아요",
          "speaker_id": "2",
          "time": "20210819 19:36"
        },
        {
          "id": "MDRW2100000640.1.32",
          "form": "그래도 요즘 수요는 많이 있으니 틀린 말은 아닌거같아요",
          "original_form": "그래도 요즘 수요는 많이 있으니 틀린 말은 아닌거같아요",
          "speaker_id": "1",
          "time": "20210819 19:40"
        },
        {
          "id": "MDRW2100000640.1.33",
          "form": "그건 또 그렇긴하죠. 오래 할 수 있느냐는 제쳐두고 워낙 사람을 많이 뽑기도 하긴하구.",
          "original_form": "그건 또 그렇긴하죠. 오래 할 수 있느냐는 제쳐두고 워낙 사람을 많이 뽑기도 하긴하구.",
          "speaker_id": "2",
          "time": "20210819 19:44"
        },
        {
          "id": "MDRW2100000640.1.34",
          "form": "지금이라도 늦지 않았어요",
          "original_form": "지금이라도 늦지 않았어요",
          "speaker_id": "2",
          "time": "20210819 19:45"
        }
      ]
    }
