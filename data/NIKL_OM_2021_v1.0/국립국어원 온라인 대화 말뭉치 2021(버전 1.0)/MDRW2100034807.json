      "utterance": [
        {
          "id": "MDRW2100034807.1.1",
          "form": "평소에 뭐 자주드세요?",
          "original_form": "평소에 뭐 자주드세요?",
          "speaker_id": "1",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100034807.1.2",
          "form": "저 최근에 꽂힌게 아몬드여서",
          "original_form": "저 최근에 꽂힌게 아몬드여서",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100034807.1.3",
          "form": "아몬드를 자주 구입중인데",
          "original_form": "아몬드를 자주 구입중인데",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100034807.1.4",
          "form": "그 군옥수수아몬드랑",
          "original_form": "그 군옥수수아몬드랑",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100034807.1.5",
          "form": "마늘빵 아몬드",
          "original_form": "마늘빵 아몬드",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100034807.1.6",
          "form": "오 알아요 ㅋㅋㅋ",
          "original_form": "오 알아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034807.1.7",
          "form": "맛이 강하게 나나요??",
          "original_form": "맛이 강하게 나나요??",
          "speaker_id": "1",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034807.1.8",
          "form": "네 강하게 나요!!",
          "original_form": "네 강하게 나요!!",
          "speaker_id": "2",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034807.1.9",
          "form": "허니버터아몬드도 좋아해서 종종 사먹다가",
          "original_form": "허니버터아몬드도 좋아해서 종종 사먹다가",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034807.1.10",
          "form": "새로운 맛 나오면 다 시도해보는데",
          "original_form": "새로운 맛 나오면 다 시도해보는데",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034807.1.11",
          "form": "마늘빵 잘 만들엇어요!!",
          "original_form": "마늘빵 잘 만들엇어요!!",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034807.1.12",
          "form": "오 맛 강한거 좋아하는데 저도 참고할게요 ㅋㅋㅋ",
          "original_form": "오 맛 강한거 좋아하는데 저도 참고할게요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034807.1.13",
          "form": "허니버터도 궁금하더라구요",
          "original_form": "허니버터도 궁금하더라구요",
          "speaker_id": "1",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034807.1.14",
          "form": "그리고 얼마전엔 아몬드 잘못봐서 허니버터땅콩을 시켰는데",
          "original_form": "그리고 얼마전엔 아몬드 잘못봐서 허니버터땅콩을 시켰는데",
          "speaker_id": "2",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100034807.1.15",
          "form": "땅콩보단 아몬드가 더 제 취향이더라구여 ㅋㅋㅋ",
          "original_form": "땅콩보단 아몬드가 더 제 취향이더라구여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100034807.1.16",
          "form": "아종류가 두개에요?!",
          "original_form": "아종류가 두개에요?!",
          "speaker_id": "1",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100034807.1.17",
          "form": "전지현이 광고하는거만 봐서요 ㅋㅋㅋ",
          "original_form": "전지현이 광고하는거만 봐서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100034807.1.18",
          "form": "그 회사에서 땅콩도 내더라구여 ㅋㅋ",
          "original_form": "그 회사에서 땅콩도 내더라구여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100034807.1.19",
          "form": "h는 묵음이야 회사",
          "original_form": "h는 묵음이야 회사",
          "speaker_id": "2",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100034807.1.20",
          "form": "아 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100034807.1.21",
          "form": "그장면이 연상되네요",
          "original_form": "그장면이 연상되네요",
          "speaker_id": "1",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100034807.1.22",
          "form": "네ㅋㅋㅋㅋ 과자를 끊어야겠다 싶어서 찾은게 아몬드인데",
          "original_form": "네ㅋㅋㅋㅋ 과자를 끊어야겠다 싶어서 찾은게 아몬드인데",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100034807.1.23",
          "form": "아마 이것도 살은 마니 찔거라고 생각해오ㅋㅋㅋ",
          "original_form": "아마 이것도 살은 마니 찔거라고 생각해오ㅋㅋㅋ",
          "speaker_id": "2",
