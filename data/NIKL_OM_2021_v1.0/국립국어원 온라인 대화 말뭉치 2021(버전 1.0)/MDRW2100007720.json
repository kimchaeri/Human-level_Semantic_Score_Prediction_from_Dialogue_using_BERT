      "utterance": [
        {
          "id": "MDRW2100007720.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 10:03"
        },
        {
          "id": "MDRW2100007720.1.2",
          "form": "네 무척 좋아해요",
          "original_form": "네 무척 좋아해요",
          "speaker_id": "2",
          "time": "20210822 10:37"
        },
        {
          "id": "MDRW2100007720.1.3",
          "form": "코로나로 요새는 몸사리지만",
          "original_form": "코로나로 요새는 몸사리지만",
          "speaker_id": "2",
          "time": "20210822 10:37"
        },
        {
          "id": "MDRW2100007720.1.4",
          "form": "최근 어디 여행을 하셨나요?",
          "original_form": "최근 어디 여행을 하셨나요?",
          "speaker_id": "2",
          "time": "20210822 10:38"
        },
        {
          "id": "MDRW2100007720.1.5",
          "form": "최근에는 제주도를 다녀왔어요 name2님은요?",
          "original_form": "최근에는 제주도를 다녀왔어요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 10:40"
        },
        {
          "id": "MDRW2100007720.1.6",
          "form": "저는 가평에 애견펜션을 다녀왔어요",
          "original_form": "저는 가평에 애견펜션을 다녀왔어요",
          "speaker_id": "2",
          "time": "20210822 10:44"
        },
        {
          "id": "MDRW2100007720.1.7",
          "form": "친구랑 가셨나요? 아님 혼자?",
          "original_form": "친구랑 가셨나요? 아님 혼자?",
          "speaker_id": "2",
          "time": "20210822 10:44"
        },
        {
          "id": "MDRW2100007720.1.8",
          "form": "강아지랑 같이 가셨나봐요 좋으셨겠다",
          "original_form": "강아지랑 같이 가셨나봐요 좋으셨겠다",
          "speaker_id": "1",
          "time": "20210822 10:47"
        },
        {
          "id": "MDRW2100007720.1.9",
          "form": "저는 친구랑 같이가서 숙소에 박혀있다 왔어요 ㅎㅎ",
          "original_form": "저는 친구랑 같이가서 숙소에 박혀있다 왔어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007720.1.10",
          "form": "하긴 밤늦게 어디 다니려 해도 가게도 안 열고ㅜ",
          "original_form": "하긴 밤늦게 어디 다니려 해도 가게도 안 열고ㅜ",
          "speaker_id": "2",
          "time": "20210822 10:50"
        },
        {
          "id": "MDRW2100007720.1.11",
          "form": "그래도 제주여행이라니 부럽네요",
          "original_form": "그래도 제주여행이라니 부럽네요",
          "speaker_id": "2",
          "time": "20210822 10:50"
        },
        {
          "id": "MDRW2100007720.1.12",
          "form": "회 좋아해서 제주 가는 거 좋아하는데",
          "original_form": "회 좋아해서 제주 가는 거 좋아하는데",
          "speaker_id": "2",
          "time": "20210822 10:51"
        },
        {
          "id": "MDRW2100007720.1.13",
          "form": "호캉스라도 가고 싶네요",
          "original_form": "호캉스라도 가고 싶네요",
          "speaker_id": "2",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100007720.1.14",
          "form": "코로나 진정 되면 어디든 갈 수 있을텐데",
          "original_form": "코로나 진정 되면 어디든 갈 수 있을텐데",
          "speaker_id": "1",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007720.1.15",
          "form": "호캉스도 재밌어 보이던데 도전해보세요",
          "original_form": "호캉스도 재밌어 보이던데 도전해보세요",
          "speaker_id": "1",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007720.1.16",
          "form": "네 요새 호텔 자체 놀거리도 많으니 하반기에 가보려구요",
          "original_form": "네 요새 호텔 자체 놀거리도 많으니 하반기에 가보려구요",
          "speaker_id": "2",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007720.1.17",
          "form": "수영을 못하는 게 좀 아쉽지만요",
          "original_form": "수영을 못하는 게 좀 아쉽지만요",
          "speaker_id": "2",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007720.1.18",
          "form": "수영 좋아하시면 풀빌라에 가보시는 건 어때요?",
          "original_form": "수영 좋아하시면 풀빌라에 가보시는 건 어때요?",
          "speaker_id": "1",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100007720.1.19",
          "form": "독채 수영장 딸린 애견펜션은 가봤는데",
          "original_form": "독채 수영장 딸린 애견펜션은 가봤는데",
          "speaker_id": "2",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100007720.1.20",
          "form": "강아지 위주라 아무래도 시설이 다르죠",
          "original_form": "강아지 위주라 아무래도 시설이 다르죠",
          "speaker_id": "2",
          "time": "20210822 11:04"
        },
        {
          "id": "MDRW2100007720.1.21",
          "form": "혼캉스로 풀빌라 좋네요",
          "original_form": "혼캉스로 풀빌라 좋네요",
          "speaker_id": "2",
          "time": "20210822 11:04"
        },
        {
          "id": "MDRW2100007720.1.22",
          "form": "추천 감사드려요",
          "original_form": "추천 감사드려요",
          "speaker_id": "2",
          "time": "20210822 11:04"
        },
        {
          "id": "MDRW2100007720.1.23",
          "form": "아이고 별말씀을 ㅎㅎ",
          "original_form": "아이고 별말씀을 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100007720.1.24",
          "form": "ㅎㅎname1님도 얼른 코로나 가고 원하는 여행했으면 하네요",
          "original_form": "ㅎㅎ&name1&님도 얼른 코로나 가고 원하는 여행했으면 하네요",
          "speaker_id": "2",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007720.1.25",
          "form": "국내는 여수 부산도 추천이요",
          "original_form": "국내는 여수 부산도 추천이요",
          "speaker_id": "2",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007720.1.26",
          "form": "오 둘다 안가본 곳이에요",
          "original_form": "오 둘다 안가본 곳이에요",
          "speaker_id": "1",
          "time": "20210822 11:07"
        },
        {
          "id": "MDRW2100007720.1.27",
          "form": "저도 도전해볼게요 추천 감사드립니다 ㅎㅎ",
          "original_form": "저도 도전해볼게요 추천 감사드립니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 11:08"
        },
        {
          "id": "MDRW2100007720.1.28",
          "form": "ㅎ  저는 식도락 여행파라. 음식 보고 골라서요",
          "original_form": "ㅎ  저는 식도락 여행파라. 음식 보고 골라서요",
          "speaker_id": "2",
          "time": "20210822 11:08"
        },
        {
          "id": "MDRW2100007720.1.29",
          "form": "아이고 저야말로 별말씀을요",
          "original_form": "아이고 저야말로 별말씀을요",
          "speaker_id": "2",
          "time": "20210822 11:08"
        },
        {
          "id": "MDRW2100007720.1.30",
          "form": "음식 중요하죠! 두 곳 다 맛있는 거 많다고 들었던거 같아요",
          "original_form": "음식 중요하죠! 두 곳 다 맛있는 거 많다고 들었던거 같아요",
          "speaker_id": "1",
          "time": "20210822 11:09"
        },
        {
          "id": "MDRW2100007720.1.31",
          "form": "네 여수 갯장어샤브샤브와 통장어탕 못 먹는 음식 아니면 추천할게요",
          "original_form": "네 여수 갯장어샤브샤브와 통장어탕 못 먹는 음식 아니면 추천할게요",
          "speaker_id": "2",
          "time": "20210822 11:11"
        },
        {
          "id": "MDRW2100007720.1.32",
          "form": "오 갯장어 샤브 완전 맛있을것 같아요!",
          "original_form": "오 갯장어 샤브 완전 맛있을것 같아요!",
          "speaker_id": "1",
          "time": "20210822 11:17"
        },
        {
          "id": "MDRW2100007720.1.33",
          "form": "맛있는 음식 이야기하니 더 빨리 여행가고 싶네요 ㅠㅠ",
          "original_form": "맛있는 음식 이야기하니 더 빨리 여행가고 싶네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 11:17"
        },
        {
          "id": "MDRW2100007720.1.34",
          "form": "ㅎㅎ 얼른 백신 맞고 맛있는 여행 해요!!",
          "original_form": "ㅎㅎ 얼른 백신 맞고 맛있는 여행 해요!!",
          "speaker_id": "2",
          "time": "20210822 11:20"
        },
        {
          "id": "MDRW2100007720.1.35",
          "form": "네 그래야죠! 2차접종이 11월이긴 하지만 ㅠ",
          "original_form": "네 그래야죠! 2차접종이 11월이긴 하지만 ㅠ",
          "speaker_id": "1",
          "time": "20210822 11:21"
        },
        {
          "id": "MDRW2100007720.1.36",
          "form": "저는 2차가 10월...혹시 잔여백신이라도 나오면 꼭  잡으시길 기원할게요!!!",
