        },
        {
          "id": "MDRW2100016190.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016190.1.3",
          "form": "요즘 읽는 책 있으신가요",
          "original_form": "요즘 읽는 책 있으신가요",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016190.1.4",
          "form": "책은 많이 못읽어요ㅠ",
          "original_form": "책은 많이 못읽어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016190.1.5",
          "form": "저도 시간 없어서 잘 못읽어요",
          "original_form": "저도 시간 없어서 잘 못읽어요",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016190.1.6",
          "form": "최근에어떤거 읽으셨나요",
          "original_form": "최근에어떤거 읽으셨나요",
          "speaker_id": "1",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016190.1.7",
          "form": "버드스트라이크라는 책 읽었어요",
          "original_form": "버드스트라이크라는 책 읽었어요",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016190.1.8",
          "form": "구병모 작가님이 쓰셨어요",
          "original_form": "구병모 작가님이 쓰셨어요",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016190.1.9",
          "form": "오 재밌나요",
          "original_form": "오 재밌나요",
          "speaker_id": "1",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016190.1.10",
          "form": "날개달린 종족과 인간의 러브스토리인데",
          "original_form": "날개달린 종족과 인간의 러브스토리인데",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016190.1.11",
          "form": "연애까진 안하고 날개달린 사람이 그냥 도망쳤어요",
          "original_form": "연애까진 안하고 날개달린 사람이 그냥 도망쳤어요",
          "speaker_id": "2",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016190.1.12",
          "form": "이게 뭐야",
          "original_form": "이게 뭐야",
          "speaker_id": "2",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016190.1.13",
          "form": "ㅎㅎ 흥미롭네요",
          "original_form": "ㅎㅎ 흥미롭네요",
          "speaker_id": "1",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100016190.1.14",
          "form": "구병모 작가님이 청소년을 위한 철학적인 판타지를 많이 쓰시는 분인데",
          "original_form": "구병모 작가님이 청소년을 위한 철학적인 판타지를 많이 쓰시는 분인데",
          "speaker_id": "2",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016190.1.15",
          "form": "담에 한번 읽어보세요",
          "original_form": "담에 한번 읽어보세요",
          "speaker_id": "2",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016190.1.16",
          "form": "나름 읽을만합니다",
          "original_form": "나름 읽을만합니다",
          "speaker_id": "2",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016190.1.17",
          "form": "저는 트렌드코리아 책 좋아해요",
          "original_form": "저는 트렌드코리아 책 좋아해요",
          "speaker_id": "1",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016190.1.18",
          "form": "아 매년 찾아 읽으시나요",
          "original_form": "아 매년 찾아 읽으시나요",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016190.1.19",
          "form": "거의 매년보려고 노력해요",
          "original_form": "거의 매년보려고 노력해요",
          "speaker_id": "1",
          "time": "20210826 12:26"
        }
      ]
