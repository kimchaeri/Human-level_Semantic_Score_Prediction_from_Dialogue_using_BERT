      "utterance": [
        {
          "id": "MDRW2100035338.1.1",
          "form": "배달어플 어디꺼 쓰세요?",
          "original_form": "배달어플 어디꺼 쓰세요?",
          "speaker_id": "1",
          "time": "20210829 15:18"
        },
        {
          "id": "MDRW2100035338.1.2",
          "form": "저는 배민 애용해요!",
          "original_form": "저는 배민 애용해요!",
          "speaker_id": "2",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100035338.1.3",
          "form": "오오 저두요!",
          "original_form": "오오 저두요!",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100035338.1.4",
          "form": "제일 깔끔한 편이더라구요",
          "original_form": "제일 깔끔한 편이더라구요",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100035338.1.5",
          "form": "민초단 반가워요 ㅋㅋㅋㅋㅋ",
          "original_form": "민초단 반가워요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100035338.1.6",
          "form": "또 어느 맛 좋아해요?",
          "original_form": "또 어느 맛 좋아해요?",
          "speaker_id": "2",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100035338.1.7",
          "form": "오 민초 좋아하시나요??",
          "original_form": "오 민초 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100035338.1.8",
          "form": "네 ㅋㅋㅋㅋㅋ민초최애에요",
          "original_form": "네 ㅋㅋㅋㅋㅋ민초최애에요",
          "speaker_id": "2",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100035338.1.9",
          "form": "저도요ㅋㅋ 한때 베라에서 민초만 먹었어요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋ 한때 베라에서 민초만 먹었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:00"
        },
        {
          "id": "MDRW2100035338.1.10",
          "form": "초콜릿도 민트 없음 안먹고 그랬어요",
          "original_form": "초콜릿도 민트 없음 안먹고 그랬어요",
          "speaker_id": "1",
          "time": "20210829 16:00"
        },
        {
          "id": "MDRW2100035338.1.11",
          "form": "ㅋㅋㅋㅋㅋ아진짜요? 찐 민트파이네요",
          "original_form": "ㅋㅋㅋㅋㅋ아진짜요? 찐 민트파이네요",
          "speaker_id": "2",
          "time": "20210829 16:04"
        },
        {
          "id": "MDRW2100035338.1.12",
          "form": "저는 나중에 민초로 하프갤런",
          "original_form": "저는 나중에 민초로 하프갤런",
          "speaker_id": "2",
          "time": "20210829 16:04"
        },
        {
          "id": "MDRW2100035338.1.13",
          "form": "담는게 꿈이에욬ㅋㅋㅋㅋ",
          "original_form": "담는게 꿈이에욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:05"
        },
        {
          "id": "MDRW2100035338.1.14",
          "form": "앗ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:07"
        },
        {
          "id": "MDRW2100035338.1.15",
          "form": "그거 너무 멋지신걸요",
          "original_form": "그거 너무 멋지신걸요",
          "speaker_id": "1",
          "time": "20210829 16:07"
        },
        {
          "id": "MDRW2100035338.1.16",
          "form": "전 그 용기는 아직이네욥ㅋㅋ",
          "original_form": "전 그 용기는 아직이네욥ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:07"
        },
        {
          "id": "MDRW2100035338.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저도 용기가없어서 플렉스할때 사야겟어욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저도 용기가없어서 플렉스할때 사야겟어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100035338.1.18",
          "form": "배라가 은근 비싸요 ㅠ",
          "original_form": "배라가 은근 비싸요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100035338.1.19",
          "form": "맞아요ㅜㅜㅜ",
          "original_form": "맞아요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100035338.1.20",
          "form": "별로 안비싼거같은데 비싸더라구요",
          "original_form": "별로 안비싼거같은데 비싸더라구요",
          "speaker_id": "1",
          "time": "20210829 16:13"
        },
        {
          "id": "MDRW2100035338.1.21",
          "form": "그쵸 맛있어서 가격값은 하는데 비싸요 ㅠㅠ",
          "original_form": "그쵸 맛있어서 가격값은 하는데 비싸요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:13"
        },
        {
          "id": "MDRW2100035338.1.22",
          "form": "그렇게보면 대단해요ㅋㅋ",
          "original_form": "그렇게보면 대단해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100035338.1.23",
          "form": "아이스크림으로 그리 독보적이라니",
          "original_form": "아이스크림으로 그리 독보적이라니",
          "speaker_id": "1",
          "time": "20210829 16:19"
        }
      ]
    }
