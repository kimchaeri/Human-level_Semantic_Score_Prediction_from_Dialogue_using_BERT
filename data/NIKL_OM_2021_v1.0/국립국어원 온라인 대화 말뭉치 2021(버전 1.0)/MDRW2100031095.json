      "utterance": [
        {
          "id": "MDRW2100031095.1.1",
          "form": "맛집 가는 거 좋아하세요??",
          "original_form": "맛집 가는 거 좋아하세요??",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031095.1.2",
          "form": "헉 좋아하죠~",
          "original_form": "헉 좋아하죠~",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031095.1.3",
          "form": "디저트도 엄청좋아해서",
          "original_form": "디저트도 엄청좋아해서",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031095.1.4",
          "form": "새로운 동네 가면 그동네 빵집은 꼭 가봐요",
          "original_form": "새로운 동네 가면 그동네 빵집은 꼭 가봐요",
          "speaker_id": "1",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100031095.1.5",
          "form": "오, 디저트 좋아하시는구나",
          "original_form": "오, 디저트 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031095.1.6",
          "form": "저는 쌀의 민족이라 밥집ㅋㅋㅋ",
          "original_form": "저는 쌀의 민족이라 밥집ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031095.1.7",
          "form": "약속 잡히면 맛집 조사해서",
          "original_form": "약속 잡히면 맛집 조사해서",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031095.1.8",
          "form": "리스트 준비합니다",
          "original_form": "리스트 준비합니다",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031095.1.9",
          "form": "우와ㅋㅋㅋ",
          "original_form": "우와ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100031095.1.10",
          "form": "그럼 한식파이신가요?",
          "original_form": "그럼 한식파이신가요?",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100031095.1.11",
          "form": "아, 그건 아니에요 다른 나라 음식도 좋아해요",
          "original_form": "아, 그건 아니에요 다른 나라 음식도 좋아해요",
          "speaker_id": "2",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100031095.1.12",
          "form": "가리는 나라 없습니다",
          "original_form": "가리는 나라 없습니다",
          "speaker_id": "2",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100031095.1.13",
          "form": "인도 멕시코 태국 베트남",
          "original_form": "인도 멕시코 태국 베트남",
          "speaker_id": "2",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100031095.1.14",
          "form": "저두요~!~~",
          "original_form": "저두요~!~~",
          "speaker_id": "1",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100031095.1.15",
          "form": "나열하신것만보면 향신료 잘드시나봐요",
          "original_form": "나열하신것만보면 향신료 잘드시나봐요",
          "speaker_id": "1",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031095.1.16",
          "form": "저도 향신료들어간거 엄청좋아해서요",
          "original_form": "저도 향신료들어간거 엄청좋아해서요",
          "speaker_id": "1",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031095.1.17",
          "form": "현지에서 먹어본적은 없는데 한국에서 파는 정도는 잘 먹어요ㅎㅎ",
          "original_form": "현지에서 먹어본적은 없는데 한국에서 파는 정도는 잘 먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100031095.1.18",
          "form": "그럼혹시",
          "original_form": "그럼혹시",
          "speaker_id": "1",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100031095.1.19",
          "form": "마라탕도좋아하시나요?",
          "original_form": "마라탕도좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100031095.1.20",
          "form": "막 좋아하진 않는데 한번씩 땡기더라구요ㅋㅋㅋ",
          "original_form": "막 좋아하진 않는데 한번씩 땡기더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100031095.1.21",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:30"
        },
        {
          "id": "MDRW2100031095.1.22",
          "form": "얼마전에 샹궈를 처음먹었는데 너무맛있더라구요",
          "original_form": "얼마전에 샹궈를 처음먹었는데 너무맛있더라구요",
          "speaker_id": "1",
          "time": "20210904 17:30"
        },
        {
          "id": "MDRW2100031095.1.23",
          "form": "푹빠져버렸어요",
          "original_form": "푹빠져버렸어요",
          "speaker_id": "1",
          "time": "20210904 17:30"
        },
        {
          "id": "MDRW2100031095.1.24",
          "form": "오, 샹궈는 어떤 거예요??",
          "original_form": "오, 샹궈는 어떤 거예요??",
          "speaker_id": "2",
          "time": "20210904 17:33"
        },
        {
          "id": "MDRW2100031095.1.25",
          "form": "마라랑 당면 고기 야채 듬뿍 넣고 볶은거요!",
          "original_form": "마라랑 당면 고기 야채 듬뿍 넣고 볶은거요!",
          "speaker_id": "1",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100031095.1.26",
          "form": "밥도둑입니당",
          "original_form": "밥도둑입니당",
          "speaker_id": "1",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100031095.1.27",
          "form": "이야...맛있겠어요 ",
          "original_form": "이야...맛있겠어요 ",
          "speaker_id": "2",
          "time": "20210904 17:45"
        }
      ]
    }
  ]
