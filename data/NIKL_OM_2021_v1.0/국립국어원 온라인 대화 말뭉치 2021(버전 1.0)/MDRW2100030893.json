      "utterance": [
        {
          "id": "MDRW2100030893.1.1",
          "form": "요즘 시력이 많이 안좋아졌어요",
          "original_form": "요즘 시력이 많이 안좋아졌어요",
          "speaker_id": "2",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030893.1.2",
          "form": "비염은 코에 뿌리는 스프레이가 있더라고요",
          "original_form": "비염은 코에 뿌리는 스프레이가 있더라고요",
          "speaker_id": "1",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030893.1.3",
          "form": "휴대폰을 너무 많이 봤나 봐요",
          "original_form": "휴대폰을 너무 많이 봤나 봐요",
          "speaker_id": "2",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030893.1.4",
          "form": "게임 때문 아닌가요",
          "original_form": "게임 때문 아닌가요",
          "speaker_id": "1",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030893.1.5",
          "form": "맞아요 휴대폰이 책보다 안 좋죠",
          "original_form": "맞아요 휴대폰이 책보다 안 좋죠",
          "speaker_id": "1",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030893.1.6",
          "form": "그게 일시적으로 효과는 있지만 몸에 좋지 않다고 해서 잘 안 해요",
          "original_form": "그게 일시적으로 효과는 있지만 몸에 좋지 않다고 해서 잘 안 해요",
          "speaker_id": "2",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030893.1.7",
          "form": "TV 보다 안 좋을 거예요",
          "original_form": "TV 보다 안 좋을 거예요",
          "speaker_id": "1",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030893.1.8",
          "form": "게임 때문에 맞습니다",
          "original_form": "게임 때문에 맞습니다",
          "speaker_id": "2",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030893.1.9",
          "form": "눈을 소중하게 생각해야죠",
          "original_form": "눈을 소중하게 생각해야죠",
          "speaker_id": "1",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030893.1.10",
          "form": "자기 전에도 맨날 보고 자요",
          "original_form": "자기 전에도 맨날 보고 자요",
          "speaker_id": "2",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030893.1.11",
          "form": "좋은 안경 쓰시고 하세요",
          "original_form": "좋은 안경 쓰시고 하세요",
          "speaker_id": "1",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030893.1.12",
          "form": "루테인이 챙겨 먹을까 봐요",
          "original_form": "루테인이 챙겨 먹을까 봐요",
          "speaker_id": "2",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030893.1.13",
          "form": "불을 다 끄고 휴대폰 보면 안 좋대요",
          "original_form": "불을 다 끄고 휴대폰 보면 안 좋대요",
          "speaker_id": "1",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030893.1.14",
          "form": "안경은 최고급으로 얻습니다 가격 따라 완전 다르거든요",
          "original_form": "안경은 최고급으로 얻습니다 가격 따라 완전 다르거든요",
          "speaker_id": "2",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030893.1.15",
          "form": "눈에 좋은 것보다 나쁜 걸 하지 마세요",
          "original_form": "눈에 좋은 것보다 나쁜 걸 하지 마세요",
