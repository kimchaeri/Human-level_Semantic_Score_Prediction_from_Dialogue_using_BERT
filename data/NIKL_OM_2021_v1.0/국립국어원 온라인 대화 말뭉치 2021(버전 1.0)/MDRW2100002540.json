      "utterance": [
        {
          "id": "MDRW2100002540.1.1",
          "form": "모가디슈 인성님이 나오시죠ㅎㅎ",
          "original_form": "모가디슈 인성님이 나오시죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002540.1.2",
          "form": "그쵸ㅋㅋㅋ 연기 잘하시더라구요",
          "original_form": "그쵸ㅋㅋㅋ 연기 잘하시더라구요",
          "speaker_id": "2",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002540.1.3",
          "form": "영화비는 요즘 얼마씩 하나요",
          "original_form": "영화비는 요즘 얼마씩 하나요",
          "speaker_id": "1",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002540.1.4",
          "form": "이시국에 영화비 더 올라서",
          "original_form": "이시국에 영화비 더 올라서",
          "speaker_id": "2",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002540.1.5",
          "form": "평일도",
          "original_form": "평일도",
          "speaker_id": "2",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002540.1.6",
          "form": "만원 넘더라구요",
          "original_form": "만원 넘더라구요",
          "speaker_id": "2",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002540.1.7",
          "form": "근데 전 할인행사 하는거만 찾아서 봐서...",
          "original_form": "근데 전 할인행사 하는거만 찾아서 봐서...",
          "speaker_id": "2",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002540.1.8",
          "form": "정가내곤 못보겠어요",
          "original_form": "정가내곤 못보겠어요",
          "speaker_id": "2",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002540.1.9",
          "form": "ㅎㅎ 정가는 마치",
          "original_form": "ㅎㅎ 정가는 마치",
          "speaker_id": "1",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002540.1.10",
          "form": "사기당한 느낌?ㅋㅋ",
          "original_form": "사기당한 느낌?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002540.1.11",
          "form": "ㅋㅋ 그쵸 그런 느낌",
          "original_form": "ㅋㅋ 그쵸 그런 느낌",
          "speaker_id": "2",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002540.1.12",
          "form": "비싸도",
          "original_form": "비싸도",
          "speaker_id": "2",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002540.1.13",
          "form": "너무비싸요",
          "original_form": "너무비싸요",
          "speaker_id": "2",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002540.1.14",
          "form": "영화관이 부담없이 소규모 영화관도 많아지면 좋겠어요",
          "original_form": "영화관이 부담없이 소규모 영화관도 많아지면 좋겠어요",
          "speaker_id": "1",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002540.1.15",
          "form": "그랬으면좋겠네요",
          "original_form": "그랬으면좋겠네요",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002540.1.16",
          "form": "지금은죄다",
          "original_form": "지금은죄다",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002540.1.17",
          "form": "대형영화관이라..",
          "original_form": "대형영화관이라..",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002540.1.18",
          "form": "인질도 재밌다던데",
          "original_form": "인질도 재밌다던데",
          "speaker_id": "1",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002540.1.19",
          "form": "그건 아직 못봤는데",
          "original_form": "그건 아직 못봤는데",
          "speaker_id": "2",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002540.1.20",
          "form": "황정민씨 나오는 작품이죠?",
          "original_form": "황정민씨 나오는 작품이죠?",
          "speaker_id": "2",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002540.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002540.1.22",
          "form": "모가디슈도 꼭 봐바야겠네요",
          "original_form": "모가디슈도 꼭 봐바야겠네요",
          "speaker_id": "1",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002540.1.23",
          "form": "인질 기회되면 봐야겠네요",
          "original_form": "인질 기회되면 봐야겠네요",
          "speaker_id": "2",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002540.1.24",
          "form": "영화계가 좀",
          "original_form": "영화계가 좀",
          "speaker_id": "2",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002540.1.25",
          "form": "살아났으면 좋겠어요",
          "original_form": "살아났으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002540.1.26",
          "form": "진짜 어렵겠어요",
          "original_form": "진짜 어렵겠어요",
          "speaker_id": "1",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002540.1.27",
          "form": "대부분 상영도 못한대요",
          "original_form": "대부분 상영도 못한대요",
          "speaker_id": "1",
