      "utterance": [
        {
          "id": "MDRW2100044184.1.1",
          "form": "여기 아역이 문근영이다",
          "original_form": "여기 아역이 문근영이다",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044184.1.2",
          "form": "맞아요. 연기 잘했어요",
          "original_form": "맞아요. 연기 잘했어요",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044184.1.3",
          "form": "엉 완전 깜찍했다",
          "original_form": "엉 완전 깜찍했다",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044184.1.4",
          "form": "노래도 인기 많았고",
          "original_form": "노래도 인기 많았고",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044184.1.5",
          "form": "조수미가 불렀지",
          "original_form": "조수미가 불렀지",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044184.1.6",
          "form": "아 맞아요. 노래 좋았는데",
          "original_form": "아 맞아요. 노래 좋았는데",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044184.1.7",
          "form": "2003년에 태양의 남쪽 드라마",
          "original_form": "2003년에 태양의 남쪽 드라마",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044184.1.8",
          "form": "여기서부턴 좀 들어본 드라마가 많네요",
          "original_form": "여기서부턴 좀 들어본 드라마가 많네요",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044184.1.9",
          "form": "주말드라마인데 16부작이야 조기종영했나",
          "original_form": "주말드라마인데 16부작이야 조기종영했나",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044184.1.10",
          "form": "주말인데 대개 짧네요",
          "original_form": "주말인데 대개 짧네요",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044184.1.11",
          "form": "이때 성인이엇으니",
          "original_form": "이때 성인이엇으니",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044184.1.12",
          "form": "인기가 없었나 봐요?",
          "original_form": "인기가 없었나 봐요?",
