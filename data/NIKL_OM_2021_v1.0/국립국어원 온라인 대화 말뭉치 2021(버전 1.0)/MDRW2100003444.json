      "utterance": [
        {
          "id": "MDRW2100003444.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 23:10"
        },
        {
          "id": "MDRW2100003444.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210820 23:10"
        },
        {
          "id": "MDRW2100003444.1.3",
          "form": "혹시 해외여행 가보셨어요?",
          "original_form": "혹시 해외여행 가보셨어요?",
          "speaker_id": "1",
          "time": "20210820 23:12"
        },
        {
          "id": "MDRW2100003444.1.4",
          "form": "음 아니요 가려고 했는데 코로나 때문에 가지못했어요",
          "original_form": "음 아니요 가려고 했는데 코로나 때문에 가지못했어요",
          "speaker_id": "2",
          "time": "20210820 23:12"
        },
        {
          "id": "MDRW2100003444.1.5",
          "form": "해외가보신 적 있나요?",
          "original_form": "해외가보신 적 있나요?",
          "speaker_id": "2",
          "time": "20210820 23:12"
        },
        {
          "id": "MDRW2100003444.1.6",
          "form": "저도 가고싶은데 아직 못갔어요",
          "original_form": "저도 가고싶은데 아직 못갔어요",
          "speaker_id": "1",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003444.1.7",
          "form": "어디 가보고 싶으세요?",
          "original_form": "어디 가보고 싶으세요?",
          "speaker_id": "1",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003444.1.8",
          "form": "저는 스페인이요",
          "original_form": "저는 스페인이요",
          "speaker_id": "2",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003444.1.9",
          "form": "가우디 보고싶어서요",
          "original_form": "가우디 보고싶어서요",
          "speaker_id": "2",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003444.1.10",
          "form": "지인님은 어디 가보고싶으세요?",
          "original_form": "지인님은 어디 가보고싶으세요?",
          "speaker_id": "2",
          "time": "20210820 23:15"
        },
        {
          "id": "MDRW2100003444.1.11",
          "form": "저는 일본 가보고 싶네요",
          "original_form": "저는 일본 가보고 싶네요",
          "speaker_id": "1",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003444.1.12",
          "form": "일본이요?",
          "original_form": "일본이요?",
          "speaker_id": "2",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003444.1.13",
          "form": "이번에 슈퍼마리오월드라고 유니버셜스튜디오 안에 생겼다는데",
          "original_form": "이번에 슈퍼마리오월드라고 유니버셜스튜디오 안에 생겼다는데",
          "speaker_id": "1",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003444.1.14",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003444.1.15",
          "form": "들어보셨어요?",
          "original_form": "들어보셨어요?",
          "speaker_id": "1",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003444.1.16",
          "form": "아니요 처음들어봐요",
          "original_form": "아니요 처음들어봐요",
          "speaker_id": "2",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003444.1.17",
          "form": "듣기만해도 설레네요",
          "original_form": "듣기만해도 설레네요",
          "speaker_id": "2",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003444.1.18",
          "form": "사진보면 되게 예쁘더라구요",
          "original_form": "사진보면 되게 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210820 23:19"
        }
