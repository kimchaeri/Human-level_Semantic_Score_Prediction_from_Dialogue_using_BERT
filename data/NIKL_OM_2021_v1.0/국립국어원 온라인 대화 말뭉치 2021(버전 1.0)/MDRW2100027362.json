        {
          "id": "MDRW2100027362.1.1",
          "form": "안녕하세요! 혹시 이상형이 어떻게 되세요?",
          "original_form": "안녕하세요! 혹시 이상형이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100027362.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100027362.1.3",
          "form": "저 잘생긴사람이요",
          "original_form": "저 잘생긴사람이요",
          "speaker_id": "2",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100027362.1.4",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100027362.1.5",
          "form": "아 저도 잘생긴 사람이요 ㅋㅋㅋㅋ 그리고 목소리 좋아야해요",
          "original_form": "아 저도 잘생긴 사람이요 ㅋㅋㅋㅋ 그리고 목소리 좋아야해요",
          "speaker_id": "1",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100027362.1.6",
          "form": "ㅎ 아 뭔지 알죠",
          "original_form": "ㅎ 아 뭔지 알죠",
          "speaker_id": "2",
          "time": "20210902 14:48"
        },
        {
          "id": "MDRW2100027362.1.7",
          "form": "요즘 이상형 있어요?",
          "original_form": "요즘 이상형 있어요?",
          "speaker_id": "2",
          "time": "20210902 14:48"
        },
        {
          "id": "MDRW2100027362.1.8",
          "form": "연예인으로요 ~!",
          "original_form": "연예인으로요 ~!",
          "speaker_id": "2",
          "time": "20210902 14:48"
        },
        {
          "id": "MDRW2100027362.1.9",
          "form": "연예인은 차은우씨가 잘생겼더라구요 ㅎㅎ",
          "original_form": "연예인은 차은우씨가 잘생겼더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100027362.1.10",
          "form": "강동원씨도 좋아해요",
          "original_form": "강동원씨도 좋아해요",
          "speaker_id": "1",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100027362.1.11",
          "form": "아 은우..ㅎ..ㄹ.. 저도요",
          "original_form": "아 은우..ㅎ..ㄹ.. 저도요",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100027362.1.12",
          "form": "아니 어쩜 저랑 같나요ㅋㅋㅋㅋㅋ",
          "original_form": "아니 어쩜 저랑 같나요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100027362.1.13",
          "form": "전 요새 오상욱 이 사람도 마음에 들어요",
          "original_form": "전 요새 오상욱 이 사람도 마음에 들어요",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100027362.1.14",
          "form": "몸이 좋더라구요",
          "original_form": "몸이 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100027362.1.15",
          "form": "보편적 미남의 얼굴들인거같아요 ㅋㅋㅋㅋ",
          "original_form": "보편적 미남의 얼굴들인거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100027362.1.16",
          "form": "오 오상욱씨 ㅎㅎ 보고와야지",
          "original_form": "오 오상욱씨 ㅎㅎ 보고와야지",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100027362.1.17",
          "form": "크 멋있네요 ㅠ 몸 좋은 사람 좋아요",
          "original_form": "크 멋있네요 ㅠ 몸 좋은 사람 좋아요",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100027362.1.18",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "2",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100027362.1.19",
          "form": "몸 좋고 잘생긴게 짱",
          "original_form": "몸 좋고 잘생긴게 짱",
          "speaker_id": "2",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100027362.1.20",
          "form": "근육 좋은 사람 멋있더라구요 ㅎㅎ 자기관리 열심히 하는거같아서",
          "original_form": "근육 좋은 사람 멋있더라구요 ㅎㅎ 자기관리 열심히 하는거같아서",
          "speaker_id": "1",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100027362.1.21",
          "form": "맞습니다 ㅎ ㅎㅎ 저도 그렇게 생각해요",
          "original_form": "맞습니다 ㅎ ㅎㅎ 저도 그렇게 생각해요",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100027362.1.22",
          "form": "휴 저도 좋은 사람 만나야 할텐ㄷ",
          "original_form": "휴 저도 좋은 사람 만나야 할텐ㄷ",
          "speaker_id": "1",
          "time": "20210902 19:00"
        },
        {
          "id": "MDRW2100027362.1.23",
          "form": "ㅔ",
          "original_form": "ㅔ",
          "speaker_id": "1",
          "time": "20210902 19:00"
        },
        {
          "id": "MDRW2100027362.1.24",
          "form": "누구 만나시는 분 있으신가요?",
          "original_form": "누구 만나시는 분 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 19:00"
        },
        {
          "id": "MDRW2100027362.1.25",
          "form": "아뇨 없어요",
          "original_form": "아뇨 없어요",
          "speaker_id": "2",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100027362.1.26",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "2",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100027362.1.27",
          "form": "전 관심이 있는 듯 없어서요ㅋㅋ",
          "original_form": "전 관심이 있는 듯 없어서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100027362.1.28",
          "form": "아 저도요 ㅋㅋㅋ 환상의 남자만 찾고있음",
          "original_form": "아 저도요 ㅋㅋㅋ 환상의 남자만 찾고있음",
          "speaker_id": "1",
          "time": "20210902 19:02"
        },
