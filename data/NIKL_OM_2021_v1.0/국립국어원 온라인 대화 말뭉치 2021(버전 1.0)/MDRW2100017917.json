      "utterance": [
        {
          "id": "MDRW2100017917.1.1",
          "form": "애니 많이보세여?",
          "original_form": "애니 많이보세여?",
          "speaker_id": "1",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017917.1.2",
          "form": "좋아하는 편이예요",
          "original_form": "좋아하는 편이예요",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017917.1.3",
          "form": "최근에 뭐보셨어요?",
          "original_form": "최근에 뭐보셨어요?",
          "speaker_id": "2",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017917.1.4",
          "form": "지브리꺼 비행기 나오는거? 볼려고 준비중..",
          "original_form": "지브리꺼 비행기 나오는거? 볼려고 준비중..",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017917.1.5",
          "form": "그전에 본건.. 진격이요",
          "original_form": "그전에 본건.. 진격이요",
          "speaker_id": "1",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017917.1.6",
          "form": "비행기면 붉은돼지?",
          "original_form": "비행기면 붉은돼지?",
          "speaker_id": "2",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017917.1.7",
          "form": "진격 처음에 진짜 쫄깃쫄깃했는데",
          "original_form": "진격 처음에 진짜 쫄깃쫄깃했는데",
          "speaker_id": "2",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017917.1.8",
          "form": "중간에 하차요",
          "original_form": "중간에 하차요",
          "speaker_id": "2",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017917.1.9",
          "form": "다시 재탑승해야하는데",
          "original_form": "다시 재탑승해야하는데",
          "speaker_id": "2",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017917.1.10",
          "form": "저도 처음에 시즌2까지인가만 보고..",
          "original_form": "저도 처음에 시즌2까지인가만 보고..",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017917.1.11",
          "form": "그뒤로는..",
          "original_form": "그뒤로는..",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017917.1.12",
          "form": "얼마전 에바나와서 그거도 봤네요...",
          "original_form": "얼마전 에바나와서 그거도 봤네요...",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017917.1.13",
          "form": "에바? 가 머예요?",
          "original_form": "에바? 가 머예요?",
          "speaker_id": "2",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017917.1.14",
          "form": "아.. 에반게리온이요..",
          "original_form": "아.. 에반게리온이요..",
          "speaker_id": "1",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017917.1.15",
          "form": "로보트 나오는.. 고전이라면 고전?",
          "original_form": "로보트 나오는.. 고전이라면 고전?",
          "speaker_id": "1",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017917.1.16",
          "form": "아~ 에반게리온",
          "original_form": "아~ 에반게리온",
          "speaker_id": "2",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017917.1.17",
          "form": "노래까지만",
          "original_form": "노래까지만",
          "speaker_id": "2",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017917.1.18",
          "form": "25년만인가... 마지막 극장판이 얼마전 넷플릭스에 풀렸어요..",
          "original_form": "25년만인가... 마지막 극장판이 얼마전 넷플릭스에 풀렸어요..",
          "speaker_id": "1",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017917.1.19",
          "form": "그래요? 넷플 켜놓고 있는 상태인데",
          "original_form": "그래요? 넷플 켜놓고 있는 상태인데",
          "speaker_id": "2",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017917.1.20",
          "form": "찜콩했어요",
          "original_form": "찜콩했어요",
          "speaker_id": "2",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100017917.1.21",
          "form": "근데 에바는 엄청 복잡해서.. \r\n공부안하면... 뭔 쓰레기인가 하실듯..",
          "original_form": "근데 에바는 엄청 복잡해서.. \r\n공부안하면... 뭔 쓰레기인가 하실듯..",
          "speaker_id": "1",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100017917.1.22",
          "form": "넷플릭스 애니 뭐 볼만한거 있나여?",
          "original_form": "넷플릭스 애니 뭐 볼만한거 있나여?",
          "speaker_id": "1",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100017917.1.23",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "2",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100017917.1.24",
          "form": "전 뭐 보셨겠지만 귀멸이요",
          "original_form": "전 뭐 보셨겠지만 귀멸이요",
          "speaker_id": "2",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100017917.1.25",
          "form": "귀여운거 좋아하시면 스폰지밥이랑 마이펫이요",
          "original_form": "귀여운거 좋아하시면 스폰지밥이랑 마이펫이요",
          "speaker_id": "2",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100017917.1.26",
          "form": "귀멸 아직이요.. ㅎㅎㅎㅎ \r\n",
          "original_form": "귀멸 아직이요.. ㅎㅎㅎㅎ \r\n",
          "speaker_id": "1",
          "time": "20210826 22:23"
        }
      ]
    }
