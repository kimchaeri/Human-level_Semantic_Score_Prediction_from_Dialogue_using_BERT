      "utterance": [
        {
          "id": "MDRW2100031043.1.1",
          "form": "코로나 끝나고 제일 하고 싶은거",
          "original_form": "코로나 끝나고 제일 하고 싶은거",
          "speaker_id": "1",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100031043.1.2",
          "form": "나 이탈리아 가고싶어",
          "original_form": "나 이탈리아 가고싶어",
          "speaker_id": "2",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100031043.1.3",
          "form": "해외여행가는거다 진짜...",
          "original_form": "해외여행가는거다 진짜...",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100031043.1.4",
          "form": "오직 이탈리아 뿐이야",
          "original_form": "오직 이탈리아 뿐이야",
          "speaker_id": "2",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100031043.1.5",
          "form": "이탈리아 말고 다른덴 없냐고 이탈리아 노우",
          "original_form": "이탈리아 말고 다른덴 없냐고 이탈리아 노우",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100031043.1.6",
          "form": "전 비행 짧은 곳이 좋아요",
          "original_form": "전 비행 짧은 곳이 좋아요",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100031043.1.7",
          "form": "어디 휴양지 없나",
          "original_form": "어디 휴양지 없나",
          "speaker_id": "2",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100031043.1.8",
          "form": "내가 느낀게 있는데 비행시간 6시간 넘으면 죽을거 같애",
          "original_form": "내가 느낀게 있는데 비행시간 6시간 넘으면 죽을거 같애",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100031043.1.9",
          "form": "가만히만 있어도 좋은곳",
          "original_form": "가만히만 있어도 좋은곳",
          "speaker_id": "2",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031043.1.10",
          "form": "휴양지하면 그...코타키나발루?",
          "original_form": "휴양지하면 그...코타키나발루?",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031043.1.11",
          "form": "거기 너무 작다고 들었던거같은데",
          "original_form": "거기 너무 작다고 들었던거같은데",
          "speaker_id": "2",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031043.1.12",
          "form": "근데 난 이왕 여행은 음식 맛있는 곳으로 가고 싶음",
          "original_form": "근데 난 이왕 여행은 음식 맛있는 곳으로 가고 싶음",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031043.1.13",
          "form": "휴양지 넓어서 뭐해 그냥 바다만 구경하고 음식 먹고 놀다 오는거지",
          "original_form": "휴양지 넓어서 뭐해 그냥 바다만 구경하고 음식 먹고 놀다 오는거지",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031043.1.14",
          "form": "동남아쪽으로 가서 과일천국을 경험을?",
          "original_form": "동남아쪽으로 가서 과일천국을 경험을?",
          "speaker_id": "2",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031043.1.15",
          "form": "관광지면 몰라도",
          "original_form": "관광지면 몰라도",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031043.1.16",
          "form": "제가 과일을 안 조아합니다",
          "original_form": "제가 과일을 안 조아합니다",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031043.1.17",
          "form": "아 어렵네요.",
          "original_form": "아 어렵네요.",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031043.1.18",
          "form": "난 고기나 해산물 풍부한 지역이 좋아",
          "original_form": "난 고기나 해산물 풍부한 지역이 좋아",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031043.1.19",
          "form": "어디가 있지? 정말...",
          "original_form": "어디가 있지? 정말...",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031043.1.20",
          "form": "베트남 좋았다",
          "original_form": "베트남 좋았다",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031043.1.21",
          "form": "근데 향신료 냄새가 너무 셌어",
          "original_form": "근데 향신료 냄새가 너무 셌어",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031043.1.22",
          "form": "베트남 먹방여행?",
          "original_form": "베트남 먹방여행?",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031043.1.23",
          "form": "아 그럼 러시아 어때?러시아 진짜 먹으러가는곳이래",
          "original_form": "아 그럼 러시아 어때?러시아 진짜 먹으러가는곳이래",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031043.1.24",
          "form": "ㅇㅇ거기서 새우 먹었는데 울나라보다 저렴하고 맛잇어",
          "original_form": "ㅇㅇ거기서 새우 먹었는데 울나라보다 저렴하고 맛잇어",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031043.1.25",
          "form": "러시아가?",
          "original_form": "러시아가?",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031043.1.26",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031043.1.27",
          "form": "완전 의외넹",
          "original_form": "완전 의외넹",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031043.1.28",
          "form": "러시아하면 술만 생각나는데ㅋㅋㅋㅋ",
          "original_form": "러시아하면 술만 생각나는데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100031043.1.29",
          "form": "그 유명한 관광지 말고 관광지는 볼거없는데",
          "original_form": "그 유명한 관광지 말고 관광지는 볼거없는데",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100031043.1.30",
          "form": "러시아는 뭐가 맛있대?",
          "original_form": "러시아는 뭐가 맛있대?",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100031043.1.31",
          "form": "해산물이랑 고기랑 다 있얶다고 한거같애",
          "original_form": "해산물이랑 고기랑 다 있얶다고 한거같애",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100031043.1.32",
          "form": "그래서 한국인이 많았다고 ㅋㅋㅋ",
          "original_form": "그래서 한국인이 많았다고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100031043.1.33",
          "form": "신기하넹ㅋㅋㅋㅋ가보고싶긴 하다",
          "original_form": "신기하넹ㅋㅋㅋㅋ가보고싶긴 하다",
          "speaker_id": "1",
          "time": "20210904 15:53"
        }
      ]
