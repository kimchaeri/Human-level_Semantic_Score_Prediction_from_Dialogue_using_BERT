      "utterance": [
        {
          "id": "MDRW2100029848.1.1",
          "form": "빨강머리 백설공주",
          "original_form": "빨강머리 백설공주",
          "speaker_id": "1",
          "time": "20210903 15:06"
        },
        {
          "id": "MDRW2100029848.1.2",
          "form": "여기서 이어서 하세요",
          "original_form": "여기서 이어서 하세요",
          "speaker_id": "1",
          "time": "20210903 15:07"
        },
        {
          "id": "MDRW2100029848.1.3",
          "form": "하 이 만화는 그냥 꽁냥꽁냥한 러브스토리로 갔어야했는데",
          "original_form": "하 이 만화는 그냥 꽁냥꽁냥한 러브스토리로 갔어야했는데",
          "speaker_id": "2",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100029848.1.4",
          "form": "정치로 가버리니까",
          "original_form": "정치로 가버리니까",
          "speaker_id": "2",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100029848.1.5",
          "form": "남주는 하는게 없고",
          "original_form": "남주는 하는게 없고",
          "speaker_id": "2",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100029848.1.6",
          "form": "여주는 뭔가 열심히 하고",
          "original_form": "여주는 뭔가 열심히 하고",
          "speaker_id": "2",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100029848.1.7",
          "form": "오비가 솔직히 매력이 넘치는데",
          "original_form": "오비가 솔직히 매력이 넘치는데",
          "speaker_id": "2",
          "time": "20210903 15:31"
        },
        {
          "id": "MDRW2100029848.1.8",
          "form": "활약도 많고",
          "original_form": "활약도 많고",
          "speaker_id": "2",
          "time": "20210903 15:31"
        },
        {
          "id": "MDRW2100029848.1.9",
          "form": "활용이 쉬우니까 작가도 맛들린 느낍",
          "original_form": "활용이 쉬우니까 작가도 맛들린 느낍",
          "speaker_id": "2",
          "time": "20210903 15:31"
        },
        {
          "id": "MDRW2100029848.1.10",
          "form": "순정만화는 너무 길어지면",
          "original_form": "순정만화는 너무 길어지면",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100029848.1.11",
          "form": "노잼된다",
          "original_form": "노잼된다",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100029848.1.12",
          "form": "이상한거 끼얹지마세요",
          "original_form": "이상한거 끼얹지마세요",
          "speaker_id": "1",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100029848.1.13",
          "form": "그냥 여주가 약제사니까 간단한 에피소드하면되는데",
          "original_form": "그냥 여주가 약제사니까 간단한 에피소드하면되는데",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100029848.1.14",
          "form": "정치랑 관계된 큰 스켈이라",
          "original_form": "정치랑 관계된 큰 스켈이라",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100029848.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 이런거하려면",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 이런거하려면",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100029848.1.16",
          "form": "등장인물 성격들이 모나야 하는데",
          "original_form": "등장인물 성격들이 모나야 하는데",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100029848.1.17",
          "form": "다들 순해서ㅋㅋㅋㅋㅋ",
          "original_form": "다들 순해서ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100029848.1.18",
          "form": "다 착해",
          "original_form": "다 착해",
          "speaker_id": "1",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100029848.1.19",
          "form": "약제사 관랸 에피소드는 별로 없어?",
          "original_form": "약제사 관랸 에피소드는 별로 없어?",
          "speaker_id": "1",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100029848.1.20",
          "form": "나 그거때뭄에 삿는데 ㅠㅠ",
          "original_form": "나 그거때뭄에 삿는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 15:51"
        },
        {
          "id": "MDRW2100029848.1.21",
          "form": "약사의 혼잣말이나 코렛트는 죽기로했다 이것도",
          "original_form": "약사의 혼잣말이나 코렛트는 죽기로했다 이것도",
          "speaker_id": "1",
          "time": "20210903 15:51"
        },
        {
          "id": "MDRW2100029848.1.22",
          "form": "약제사 에피소드 뭘 바라는지 모르겠는데",
          "original_form": "약제사 에피소드 뭘 바라는지 모르겠는데",
          "speaker_id": "2",
          "time": "20210903 15:51"
        },
        {
          "id": "MDRW2100029848.1.23",
          "form": "약제사 느낌이라서 산거",
          "original_form": "약제사 느낌이라서 산거",
          "speaker_id": "1",
          "time": "20210903 15:51"
        },
        {
          "id": "MDRW2100029848.1.24",
          "form": "약사의 혼잣말은 과학을 응용한 가벼운 추리물같은 거고",
          "original_form": "약사의 혼잣말은 과학을 응용한 가벼운 추리물같은 거고",
          "speaker_id": "2",
          "time": "20210903 15:51"
        },
        {
          "id": "MDRW2100029848.1.25",
          "form": "코렛트는 그냥 연애물이고",
          "original_form": "코렛트는 그냥 연애물이고",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029848.1.26",
          "form": "그냥 연애물이군",
          "original_form": "그냥 연애물이군",
          "speaker_id": "1",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029848.1.27",
          "form": "오 군데 다 봣어?",
          "original_form": "오 군데 다 봣어?",
          "speaker_id": "1",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029848.1.28",
          "form": "다독하시네요",
          "original_form": "다독하시네요",
          "speaker_id": "1",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029848.1.29",
          "form": "백설공주는 약제사 해서 약을 다루긴하는데",
          "original_form": "백설공주는 약제사 해서 약을 다루긴하는데",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029848.1.30",
          "form": "ㅇㅇ 약사혼잣말은 블로그 번역부터 봣어",
          "original_form": "ㅇㅇ 약사혼잣말은 블로그 번역부터 봣어",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029848.1.31",
          "form": "흠...",
          "original_form": "흠...",
          "speaker_id": "1",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100029848.1.32",
          "form": "백설공주는 스케일이 크고 잔잔해서",
          "original_form": "백설공주는 스케일이 크고 잔잔해서",
          "speaker_id": "2",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100029848.1.33",
          "form": "아직 안쓸거지만",
          "original_form": "아직 안쓸거지만",
          "speaker_id": "1",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100029848.1.34",
          "form": "추구하는 거랑 캐릭터가 안맞아",
          "original_form": "추구하는 거랑 캐릭터가 안맞아",
          "speaker_id": "2",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100029848.1.35",
          "form": "그림체랑 캐릭터는 맞아",
          "original_form": "그림체랑 캐릭터는 맞아",
          "speaker_id": "2",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100029848.1.36",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "1",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100029848.1.37",
          "form": "읽으면서 같은 감상이 나오려나",
          "original_form": "읽으면서 같은 감상이 나오려나",
          "speaker_id": "1",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100029848.1.38",
          "form": "넌 초반은 그럭저럭 보다가",
          "original_form": "넌 초반은 그럭저럭 보다가",
          "speaker_id": "2",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100029848.1.39",
          "form": "뒤로가면 ?될듯",
          "original_form": "뒤로가면 ?될듯",
          "speaker_id": "2",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100029848.1.40",
          "form": "초반은 옴니버스고 짤막한 약제사 이야기인데",
          "original_form": "초반은 옴니버스고 짤막한 약제사 이야기인데",
          "speaker_id": "2",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100029848.1.41",
          "form": "물론 납치도 당하구",
          "original_form": "물론 납치도 당하구",
          "speaker_id": "2",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100029848.1.42",
          "form": "뒤는 그냥 한 약재를 위해 정치판을 벌리는 거라",
          "original_form": "뒤는 그냥 한 약재를 위해 정치판을 벌리는 거라",
          "speaker_id": "2",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100029848.1.43",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100029848.1.44",
          "form": "판타지 없는 약사물은 미도리... 뭐 잇는데 이게",
          "original_form": "판타지 없는 약사물은 미도리... 뭐 잇는데 이게",
          "speaker_id": "2",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100029848.1.45",
          "form": "찐 약사이긴함",
          "original_form": "찐 약사이긴함",
          "speaker_id": "2",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100029848.1.46",
          "form": "갠적으로 셋중 젤 재미있는 건 약사의 혼잣말",
          "original_form": "갠적으로 셋중 젤 재미있는 건 약사의 혼잣말",
          "speaker_id": "2",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100029848.1.47",
          "form": "미도리?",
          "original_form": "미도리?",
          "speaker_id": "1",
          "time": "20210903 15:56"
        },
        {
          "id": "MDRW2100029848.1.48",
          "form": "약재를 두고 정치판인건 좀 재밋어보인닼ㅋㅌ",
          "original_form": "약재를 두고 정치판인건 좀 재밋어보인닼ㅋㅌ",
          "speaker_id": "1",
          "time": "20210903 15:56"
        },
        {
          "id": "MDRW2100029848.1.49",
          "form": "환상의 약재를 두고",
          "original_form": "환상의 약재를 두고",
          "speaker_id": "1",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100029848.1.50",
          "form": "싸우는건갘ㅋㅋㅋ",
          "original_form": "싸우는건갘ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100029848.1.51",
          "form": "아니 그런것두 아니야",
          "original_form": "아니 그런것두 아니야",
          "speaker_id": "2",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100029848.1.52",
          "form": "약사의 혼잣말은 나도 재밋게 봄",
          "original_form": "약사의 혼잣말은 나도 재밋게 봄",
          "speaker_id": "1",
          "time": "20210903 15:58"
        },
        {
          "id": "MDRW2100029848.1.53",
          "form": "근데 좀 지루해지고잇음",
          "original_form": "근데 좀 지루해지고잇음",
          "speaker_id": "1",
          "time": "20210903 15:58"
        }
      ]
