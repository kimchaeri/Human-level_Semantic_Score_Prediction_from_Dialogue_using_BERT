      "utterance": [
        {
          "id": "MDRW2100008500.1.1",
          "form": "엄마손맛은 언제나 진리같아요",
          "original_form": "엄마손맛은 언제나 진리같아요",
          "speaker_id": "1",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008500.1.2",
          "form": "저는 저희애들한테 그런 손맛이 안될것같아서",
          "original_form": "저는 저희애들한테 그런 손맛이 안될것같아서",
          "speaker_id": "1",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008500.1.3",
          "form": "걱정이긴해요 ㅎㅎ",
          "original_form": "걱정이긴해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008500.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210822 18:34"
        },
        {
          "id": "MDRW2100008500.1.5",
          "form": "저희 엄마처럼 맛난 김치 못 담가 줄것 같아요",
          "original_form": "저희 엄마처럼 맛난 김치 못 담가 줄것 같아요",
          "speaker_id": "2",
          "time": "20210822 18:35"
        },
        {
          "id": "MDRW2100008500.1.6",
          "form": "제김치는 뭔가 시판 김치 맛이 나요 ㅋㅋ",
          "original_form": "제김치는 뭔가 시판 김치 맛이 나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:35"
        },
        {
          "id": "MDRW2100008500.1.7",
          "form": "집에서 했는데 어떻게 이런지 신기해요 ㅋㅋ",
          "original_form": "집에서 했는데 어떻게 이런지 신기해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:35"
        },
        {
          "id": "MDRW2100008500.1.8",
          "form": "손맛이라는게 진짜 있는거같아요",
          "original_form": "손맛이라는게 진짜 있는거같아요",
          "speaker_id": "1",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008500.1.9",
          "form": "맞이요",
          "original_form": "맞이요",
          "speaker_id": "2",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008500.1.10",
          "form": "저 김치가 종갓집 김치 맛나요 ㅋㅋ",
          "original_form": "저 김치가 종갓집 김치 맛나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008500.1.11",
          "form": "전 손맛은 없나봐요 ㅋㅋ",
          "original_form": "전 손맛은 없나봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008500.1.12",
          "form": "똑같이 레시피로 하는데 맛이 다르니까요 ㅜㅠ",
          "original_form": "똑같이 레시피로 하는데 맛이 다르니까요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008500.1.13",
          "form": "저도 똥손이예요 ㅎ",
          "original_form": "저도 똥손이예요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008500.1.14",
          "form": "멸치볶음하나해도 블로그 찾아봐요 ㅎ",
          "original_form": "멸치볶음하나해도 블로그 찾아봐요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008500.1.15",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008500.1.16",
          "form": "저는 더맛있는김치 거기서 겉절이 사먹어요",
          "original_form": "저는 더맛있는김치 거기서 겉절이 사먹어요",
          "speaker_id": "1",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008500.1.17",
          "form": "근데 전 문제가 중간에",
          "original_form": "근데 전 문제가 중간에",
          "speaker_id": "2",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008500.1.18",
          "form": "제맘대로해요 ㅋㅋ",
          "original_form": "제맘대로해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008500.1.19",
          "form": "저는 익은김치보다 겉절이가 맛나더라구요",
          "original_form": "저는 익은김치보다 겉절이가 맛나더라구요",
          "speaker_id": "1",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008500.1.20",
          "form": "오 막나요?",
          "original_form": "오 막나요?",
          "speaker_id": "2",
          "time": "20210822 19:15"
        },
        {
          "id": "MDRW2100008500.1.21",
          "form": "맛나요?",
          "original_form": "맛나요?",
          "speaker_id": "2",
          "time": "20210822 19:15"
        },
        {
          "id": "MDRW2100008500.1.22",
          "form": "거기  김치 맛난가요?",
          "original_form": "거기  김치 맛난가요?",
          "speaker_id": "2",
          "time": "20210822 19:15"
        },
        {
          "id": "MDRW2100008500.1.23",
          "form": "제입맛에는 맞던데 어떤분은 맛없다고 그러시더라구요",
          "original_form": "제입맛에는 맞던데 어떤분은 맛없다고 그러시더라구요",
          "speaker_id": "1",
          "time": "20210822 19:15"
        },
        {
          "id": "MDRW2100008500.1.24",
          "form": "저는 백김치도 시켜서 애들먹이고 있어요",
          "original_form": "저는 백김치도 시켜서 애들먹이고 있어요",
          "speaker_id": "1",
          "time": "20210822 19:15"
        },
        {
          "id": "MDRW2100008500.1.25",
          "form": "아 다 압맛이 틀리긴하죠 ㅎㅎ",
          "original_form": "아 다 압맛이 틀리긴하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008500.1.26",
          "form": "오 애들 백김치 좋죠",
          "original_form": "오 애들 백김치 좋죠",
          "speaker_id": "2",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008500.1.27",
          "form": "맛날거 같은데요 ㅎㅎ",
          "original_form": "맛날거 같은데요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008500.1.28",
          "form": "백김치 하나사놓으니 반찬걱정이 좀 덜해요 ㅋ",
          "original_form": "백김치 하나사놓으니 반찬걱정이 좀 덜해요 ㅋ",
          "speaker_id": "1",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008500.1.29",
          "form": "그럼요 김치 있음 든든해요",
          "original_form": "그럼요 김치 있음 든든해요",
          "speaker_id": "2",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008500.1.30",
          "form": "아이들이 김치 잘먹고 기특하네요",
          "original_form": "아이들이 김치 잘먹고 기특하네요",
          "speaker_id": "2",
          "time": "20210822 19:18"
        }
      ]
