      "utterance": [
        {
          "id": "MDRW2100044810.1.1",
          "form": "작가가 잘못짠거여 ㅋㅋ다들사이좋음",
          "original_form": "작가가 잘못짠거여 ㅋㅋ다들사이좋음",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044810.1.2",
          "form": "글고 기안친구많아 ㅋㅋ",
          "original_form": "글고 기안친구많아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044810.1.3",
          "form": "의외로 사교적임",
          "original_form": "의외로 사교적임",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044810.1.4",
          "form": "그렇구나 티비프로 쩜 모르겠다",
          "original_form": "그렇구나 티비프로 쩜 모르겠다",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044810.1.5",
          "form": "이게 보다봄 대본이란거 알아도 모르는듯돼고 그렇네",
          "original_form": "이게 보다봄 대본이란거 알아도 모르는듯돼고 그렇네",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044810.1.6",
          "form": "ㅇㅇㅋㅋ",
          "original_form": "ㅇㅇㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044810.1.7",
          "form": "나 보는드라마 공감성수치대박이다",
          "original_form": "나 보는드라마 공감성수치대박이다",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044810.1.8",
          "form": "계속 남주한테 들이대는데",
          "original_form": "계속 남주한테 들이대는데",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044810.1.9",
          "form": "공감성수치?",
          "original_form": "공감성수치?",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044810.1.10",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044810.1.11",
          "form": "그니까 내가수치스럽다공 ㅋㅋ",
          "original_form": "그니까 내가수치스럽다공 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044810.1.12",
          "form": "저 여주가하는행동때메",
          "original_form": "저 여주가하는행동때메",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044810.1.13",
          "form": "지금 남주 샤워하는거 훔쳐보려고항ㅅ",
          "original_form": "지금 남주 샤워하는거 훔쳐보려고항ㅅ",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044810.1.14",
          "form": "어떤 짓을 하나싶었어",
          "original_form": "어떤 짓을 하나싶었어",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044810.1.15",
          "form": "오메 대단하다잉",
          "original_form": "오메 대단하다잉",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044810.1.16",
          "form": "너무싫어 ㅋㅋㅋ",
          "original_form": "너무싫어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044810.1.17",
          "form": "왜저럴까싶다ㅋㅋ",
          "original_form": "왜저럴까싶다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044810.1.18",
          "form": "그게 여주 이미지?",
          "original_form": "그게 여주 이미지?",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044810.1.19",
          "form": "자꾸이상한짓해서 남주가 극 ㅎ 혐해",
          "original_form": "자꾸이상한짓해서 남주가 극 ㅎ 혐해",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044810.1.20",
          "form": "행동 독특하긴하다",
          "original_form": "행동 독특하긴하다",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044810.1.21",
          "form": "좋아할래야 좋아할수없어ㅋㅋ",
          "original_form": "좋아할래야 좋아할수없어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044810.1.22",
          "form": "ㅇㅇ 소설속이라 더 막나가는듯",
          "original_form": "ㅇㅇ 소설속이라 더 막나가는듯",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044810.1.23",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044810.1.24",
          "form": "저러고 또 사랑에 빠질듯",
          "original_form": "저러고 또 사랑에 빠질듯",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044810.1.25",
          "form": "실제로 못하는거 소설속에서 이루기",
          "original_form": "실제로 못하는거 소설속에서 이루기",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044810.1.26",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044810.1.27",
          "form": "그런가벼 ㅋㅋㅋ",
          "original_form": "그런가벼 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044810.1.28",
          "form": "로맨스작품인가보네",
          "original_form": "로맨스작품인가보네",
          "speaker_id": "1",
