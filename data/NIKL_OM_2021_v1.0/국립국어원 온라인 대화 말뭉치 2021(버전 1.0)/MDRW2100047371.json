      "utterance": [
        {
          "id": "MDRW2100047371.1.1",
          "form": "제가 예전에",
          "original_form": "제가 예전에",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100047371.1.2",
          "form": "그거 두번째 볼때",
          "original_form": "그거 두번째 볼때",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100047371.1.3",
          "form": "그 남자배우 너무",
          "original_form": "그 남자배우 너무",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100047371.1.4",
          "form": "궁금해서 검색해봤는데",
          "original_form": "궁금해서 검색해봤는데",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100047371.1.5",
          "form": "배우 이름을 모르겠네요",
          "original_form": "배우 이름을 모르겠네요",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100047371.1.6",
          "form": "아무것도 없더라구요 ㅜㅜ",
          "original_form": "아무것도 없더라구요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100047371.1.7",
          "form": "옷의 핏이 ...좋네요",
          "original_form": "옷의 핏이 ...좋네요",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100047371.1.8",
          "form": "뭐든 잘어울리구",
          "original_form": "뭐든 잘어울리구",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100047371.1.9",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100047371.1.10",
          "form": "저도 이름이 생각안나요",
          "original_form": "저도 이름이 생각안나요",
          "speaker_id": "2",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100047371.1.11",
          "form": "진이한",
          "original_form": "진이한",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100047371.1.12",
          "form": "맞아 특이했어요",
          "original_form": "맞아 특이했어요",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100047371.1.13",
          "form": "김이한이라 검색하니 안나왔네요 ㅋㅋ",
          "original_form": "김이한이라 검색하니 안나왔네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100047371.1.14",
          "form": "아 안그래도 찾아봤는데",
          "original_form": "아 안그래도 찾아봤는데",
          "speaker_id": "1",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100047371.1.15",
          "form": "원래뮤지컬배우인가봐요",
          "original_form": "원래뮤지컬배우인가봐요",
          "speaker_id": "1",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100047371.1.16",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100047371.1.17",
          "form": "아 뭐 할까요",
          "original_form": "아 뭐 할까요",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100047371.1.18",
          "form": "어쩐지 목소리가 좋더라니",
          "original_form": "어쩐지 목소리가 좋더라니",
          "speaker_id": "1",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100047371.1.19",
          "form": "얼술도 목소리도 좋은데",
          "original_form": "얼술도 목소리도 좋은데",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100047371.1.20",
          "form": "얼굴",
          "original_form": "얼굴",
          "speaker_id": "2",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100047371.1.21",
          "form": "그쵸ㅋㅋ",
          "original_form": "그쵸ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100047371.1.22",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100047371.1.23",
          "form": "인스타보니 따로 스케쥴은 없는듯요 드라마 한번더 나와줫으면 좋겠어요",
          "original_form": "인스타보니 따로 스케쥴은 없는듯요 드라마 한번더 나와줫으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100047371.1.24",
          "form": "잘나오다 사라지면 또 안보이고 배우도 안불러주면 주인공에서 금새 일반인되고",
          "original_form": "잘나오다 사라지면 또 안보이고 배우도 안불러주면 주인공에서 금새 일반인되고",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100047371.1.25",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100047371.1.26",
          "form": "맞아요..  ㅋㅋㅋ",
          "original_form": "맞아요..  ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100047371.1.27",
          "form": "가끔 생각나요",
          "original_form": "가끔 생각나요",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100047371.1.28",
          "form": "극한직업이예요",
          "original_form": "극한직업이예요",
          "speaker_id": "2",
          "time": "20210826 14:03"
        }
      ]
