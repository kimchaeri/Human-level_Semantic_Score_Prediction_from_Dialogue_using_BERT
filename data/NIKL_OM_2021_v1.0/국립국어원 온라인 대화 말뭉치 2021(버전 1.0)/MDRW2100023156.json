      "utterance": [
        {
          "id": "MDRW2100023156.1.1",
          "form": "아쉽겟네여ㅎ",
          "original_form": "아쉽겟네여ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023156.1.2",
          "form": "아뇨 그때가 언제인데요 ㅋㅋ",
          "original_form": "아뇨 그때가 언제인데요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023156.1.3",
          "form": "그래도 아르바이트하면서 많이 연애하고 그러잖아요ㅎㅎ",
          "original_form": "그래도 아르바이트하면서 많이 연애하고 그러잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023156.1.4",
          "form": "ㅎㅎ ㅎ그렁걸  못해봣네요 ㅋ",
          "original_form": "ㅎㅎ ㅎ그렁걸  못해봣네요 ㅋ",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023156.1.5",
          "form": "에고ㅠ 듣기만해도 엄청 고생하셨을듯ㅠ",
          "original_form": "에고ㅠ 듣기만해도 엄청 고생하셨을듯ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023156.1.6",
          "form": "대단하시다 정말",
          "original_form": "대단하시다 정말",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100023156.1.7",
          "form": "대학교 학자금 그럼 다 아아님이 내셨어요?",
          "original_form": "대학교 학자금 그럼 다 아아님이 내셨어요?",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023156.1.8",
          "form": "네네 근데 뭐 다닌기간은 오래이지도 않고",
          "original_form": "네네 근데 뭐 다닌기간은 오래이지도 않고",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023156.1.9",
          "form": "장학금 받아서",
          "original_form": "장학금 받아서",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023156.1.10",
          "form": "돈은 안냈어요",
          "original_form": "돈은 안냈어요",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023156.1.11",
          "form": "우왑ㅎ",
          "original_form": "우왑ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023156.1.12",
          "form": "대단하시다 정말",
          "original_form": "대단하시다 정말",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023156.1.13",
          "form": "아뇨뇨뇨",
          "original_form": "아뇨뇨뇨",
          "speaker_id": "1",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100023156.1.14",
          "form": "진짜 고생하셨어요 정말 존경해요",
          "original_form": "진짜 고생하셨어요 정말 존경해요",
          "speaker_id": "2",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100023156.1.15",
          "form": "아르바이트 저도 많이는 안해봤지만 남의돈 버는게 쉽지가 않잖아요",
          "original_form": "아르바이트 저도 많이는 안해봤지만 남의돈 버는게 쉽지가 않잖아요",
          "speaker_id": "2",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100023156.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100023156.1.17",
          "form": "쉽게 버는 돈으 없죠",
          "original_form": "쉽게 버는 돈으 없죠",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023156.1.18",
          "form": "앱테크만 봐도 ㅎㅎㅎ",
          "original_form": "앱테크만 봐도 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023156.1.19",
          "form": "맞아요ㅎㅎ 시간을 그만큼 투자해야 하니깐",
          "original_form": "맞아요ㅎㅎ 시간을 그만큼 투자해야 하니깐",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023156.1.20",
          "form": "엄청 힘들죠ㅎ",
          "original_form": "엄청 힘들죠ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023156.1.21",
          "form": "차라리 알바하는게 나은데ㅎㅎ",
          "original_form": "차라리 알바하는게 나은데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023156.1.22",
          "form": "빙고 ㅎㄹ",
          "original_form": "빙고 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023156.1.23",
          "form": "책고르고 있는데  ㅋ 요즘은 어린이들도  경제가 빠르네요",
          "original_form": "책고르고 있는데  ㅋ 요즘은 어린이들도  경제가 빠르네요",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023156.1.24",
          "form": "책제목이 ㅋ",
          "original_form": "책제목이 ㅋ",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023156.1.25",
          "form": "장난감말고 주식 사주세요  네요 ㅋㅋㅋ",
          "original_form": "장난감말고 주식 사주세요  네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023156.1.26",
          "form": "경제 책 한번 사볼까 둘러보는데 ㅋㅋㅋ",
          "original_form": "경제 책 한번 사볼까 둘러보는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023156.1.27",
          "form": "어린이만도 못한거 같아서 급 슬픔 ㅋㅋ",
          "original_form": "어린이만도 못한거 같아서 급 슬픔 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023156.1.28",
          "form": "ㅎㅎㅎㅎㅎㅎㅎㄹ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎㄹ",
          "speaker_id": "2",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100023156.1.29",
          "form": "이제끝났어요ㅠ 남자 두명ㅠ",
          "original_form": "이제끝났어요ㅠ 남자 두명ㅠ",
          "speaker_id": "2",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100023156.1.30",
          "form": "그래도다행에요ㄹ",
          "original_form": "그래도다행에요ㄹ",
          "speaker_id": "2",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100023156.1.31",
          "form": "제가 예전에 이벤트했는데ㅎ 왜 우리 아이들은 경제공부를 안하냐?",
          "original_form": "제가 예전에 이벤트했는데ㅎ 왜 우리 아이들은 경제공부를 안하냐?",
          "speaker_id": "2",
          "time": "20210829 23:13"
        },
        {
          "id": "MDRW2100023156.1.32",
          "form": "이런걸로 장문의 글을 쓴 기억이있네요ㅎ",
          "original_form": "이런걸로 장문의 글을 쓴 기억이있네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 23:13"
        },
        {
          "id": "MDRW2100023156.1.33",
          "form": "결국당첨ㅎㅎㅎㅎ",
          "original_form": "결국당첨ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 23:13"
        },
        {
          "id": "MDRW2100023156.1.34",
          "form": "ㅎㅎ 멋져요 ㅎㅎ",
          "original_form": "ㅎㅎ 멋져요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023156.1.35",
          "form": "ㅎㅎ 다행이네여 그나마 2명이라 ㅎㅎ",
          "original_form": "ㅎㅎ 다행이네여 그나마 2명이라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023156.1.36",
          "form": "집에 이제 가샤야죠~",
          "original_form": "집에 이제 가샤야죠~",
