        },
        {
          "id": "MDRW2100039255.1.2",
          "form": "없어요...",
          "original_form": "없어요...",
          "speaker_id": "2",
          "time": "20210906 20:08"
        },
        {
          "id": "MDRW2100039255.1.3",
          "form": "예술의 전당다녀왔어요",
          "original_form": "예술의 전당다녀왔어요",
          "speaker_id": "1",
          "time": "20210906 20:08"
        },
        {
          "id": "MDRW2100039255.1.4",
          "form": "누구공연인가요",
          "original_form": "누구공연인가요",
          "speaker_id": "2",
          "time": "20210906 20:08"
        },
        {
          "id": "MDRW2100039255.1.5",
          "form": "생각이 안나지만 브람스 였던것 같아요",
          "original_form": "생각이 안나지만 브람스 였던것 같아요",
          "speaker_id": "1",
          "time": "20210906 20:08"
        },
        {
          "id": "MDRW2100039255.1.6",
          "form": "월츠",
          "original_form": "월츠",
          "speaker_id": "1",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100039255.1.7",
          "form": "재즈",
          "original_form": "재즈",
          "speaker_id": "1",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100039255.1.8",
          "form": "클래식이었어요",
          "original_form": "클래식이었어요",
          "speaker_id": "1",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100039255.1.9",
          "form": "저는 전시회 다녀왔어요",
          "original_form": "저는 전시회 다녀왔어요",
          "speaker_id": "2",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100039255.1.10",
          "form": "무슨 전시요?",
          "original_form": "무슨 전시요?",
          "speaker_id": "1",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100039255.1.11",
          "form": "서촌에서 하는 전시였어요",
          "original_form": "서촌에서 하는 전시였어요",
          "speaker_id": "2",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100039255.1.12",
          "form": "아\r\n저두 서촌 사진전 봤어요",
          "original_form": "아\r\n저두 서촌 사진전 봤어요",
          "speaker_id": "1",
          "time": "20210906 20:10"
        },
        {
          "id": "MDRW2100039255.1.13",
          "form": "전시는 어땠어요?",
          "original_form": "전시는 어땠어요?",
          "speaker_id": "1",
          "time": "20210906 20:10"
        },
        {
          "id": "MDRW2100039255.1.14",
          "form": "재밌으셨나요?",
          "original_form": "재밌으셨나요?",
          "speaker_id": "2",
          "time": "20210906 20:10"
        },
        {
          "id": "MDRW2100039255.1.15",
