      "utterance": [
        {
          "id": "MDRW2100013495.1.1",
          "form": "와 방금 문자받고 깜짝 놀랐어",
          "original_form": "와 방금 문자받고 깜짝 놀랐어",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013495.1.2",
          "form": "무슨 문자?",
          "original_form": "무슨 문자?",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013495.1.3",
          "form": "방과 후 선생님이 2분 후에 수업시작한다고 ㅋㅋㅋ",
          "original_form": "방과 후 선생님이 2분 후에 수업시작한다고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013495.1.4",
          "form": "훌 ㅋㅋㅋ 수요일 과학 수업 깜빡했구만",
          "original_form": "훌 ㅋㅋㅋ 수요일 과학 수업 깜빡했구만",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013495.1.5",
          "form": "개학했더니 완전 잊어버리고 있었네",
          "original_form": "개학했더니 완전 잊어버리고 있었네",
          "speaker_id": "1",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013495.1.6",
          "form": "줌 수업에 과학수업까지 하려니 힘들겠네",
          "original_form": "줌 수업에 과학수업까지 하려니 힘들겠네",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013495.1.7",
          "form": "생각도 안하고 있다가 한 대 맞은 느낌 ㅎㅎ",
          "original_form": "생각도 안하고 있다가 한 대 맞은 느낌 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013495.1.8",
          "form": "이런이런 잘 챙겨야지",
          "original_form": "이런이런 잘 챙겨야지",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013495.1.9",
          "form": "다행히 선생님이 진짜 잘 챙겨주셔서 줌 수업 들어갔어 ㅠㅠ",
          "original_form": "다행히 선생님이 진짜 잘 챙겨주셔서 줌 수업 들어갔어 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013495.1.10",
          "form": "다음에도 들을거야?",
          "original_form": "다음에도 들을거야?",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013495.1.11",
          "form": "대면 수업해야 보낼 수 있을 것 같아.. 방과후까지 줌이라 좀 슬퍼",
          "original_form": "대면 수업해야 보낼 수 있을 것 같아.. 방과후까지 줌이라 좀 슬퍼",
          "speaker_id": "1",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013495.1.12",
          "form": "방과후가 줌수업이면 좀 그렇긴 하지..?",
          "original_form": "방과후가 줌수업이면 좀 그렇긴 하지..?",
          "speaker_id": "2",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013495.1.13",
          "form": "큰아들은 그래도 듣고 싶다고는 하는데",
          "original_form": "큰아들은 그래도 듣고 싶다고는 하는데",
          "speaker_id": "1",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013495.1.14",
          "form": "나중에 대면수업할 때 들으면 되지",
          "original_form": "나중에 대면수업할 때 들으면 되지",
          "speaker_id": "2",
          "time": "20210825 14:53"
        },
        {
