      "utterance": [
        {
          "id": "MDRW2100018441.1.1",
          "form": "평소에하던 건 햇엇는데 별로못햇네요",
          "original_form": "평소에하던 건 햇엇는데 별로못햇네요",
          "speaker_id": "2",
          "time": "20210827 01:58"
        },
        {
          "id": "MDRW2100018441.1.2",
          "form": "저는 오늘 바닥청소를 했어요",
          "original_form": "저는 오늘 바닥청소를 했어요",
          "speaker_id": "1",
          "time": "20210827 01:59"
        },
        {
          "id": "MDRW2100018441.1.3",
          "form": "오 저는 설거지를 했었어요.. 그리고 음식물쓰레기를 버렸어요",
          "original_form": "오 저는 설거지를 했었어요.. 그리고 음식물쓰레기를 버렸어요",
          "speaker_id": "2",
          "time": "20210827 02:00"
        },
        {
          "id": "MDRW2100018441.1.4",
          "form": "저희집은 집안일을 번갈아가면서 해요",
          "original_form": "저희집은 집안일을 번갈아가면서 해요",
          "speaker_id": "1",
          "time": "20210827 02:00"
        },
        {
          "id": "MDRW2100018441.1.5",
          "form": "오 그렇균요!!",
          "original_form": "오 그렇균요!!",
          "speaker_id": "2",
          "time": "20210827 02:00"
        },
        {
          "id": "MDRW2100018441.1.6",
          "form": "전 내일은 설거지를 할듯해요",
          "original_form": "전 내일은 설거지를 할듯해요",
          "speaker_id": "1",
          "time": "20210827 02:00"
        },
        {
          "id": "MDRW2100018441.1.7",
          "form": "오... 그렇건요...",
          "original_form": "오... 그렇건요...",
          "speaker_id": "2",
          "time": "20210827 02:01"
        },
        {
          "id": "MDRW2100018441.1.8",
          "form": "설거지가 좋으신가요\n바닥청소가 좋으신가요",
          "original_form": "설거지가 좋으신가요\n바닥청소가 좋으신가요",
          "speaker_id": "2",
          "time": "20210827 02:01"
        },
        {
          "id": "MDRW2100018441.1.9",
          "form": "둘다 싫어요",
          "original_form": "둘다 싫어요",
          "speaker_id": "1",
          "time": "20210827 02:01"
        },
        {
          "id": "MDRW2100018441.1.10",
          "form": "사실 그렇긴하죠 ㅠㅠ",
          "original_form": "사실 그렇긴하죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 02:01"
        },
        {
          "id": "MDRW2100018441.1.11",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210827 02:01"
        },
        {
          "id": "MDRW2100018441.1.12",
          "form": "저는 경우에따라 다른것같아요",
          "original_form": "저는 경우에따라 다른것같아요",
          "speaker_id": "2",
          "time": "20210827 02:01"
        },
        {
          "id": "MDRW2100018441.1.13",
          "form": "양이 작고 많고에 따라 다르긴 하죠",
          "original_form": "양이 작고 많고에 따라 다르긴 하죠",
          "speaker_id": "1",
          "time": "20210827 02:02"
        },
        {
          "id": "MDRW2100018441.1.14",
          "form": "저도 그런편인것같아요 ㅋㅋ",
          "original_form": "저도 그런편인것같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:02"
        },
        {
          "id": "MDRW2100018441.1.15",
          "form": "육아 하세요?",
          "original_form": "육아 하세요?",
