      "utterance": [
        {
          "id": "MDRW2100040077.1.1",
          "form": "어제고민중이던 니트와 바지를 주문했어뇨",
          "original_form": "어제고민중이던 니트와 바지를 주문했어뇨",
          "speaker_id": "1",
          "time": "20210904 06:33"
        },
        {
          "id": "MDRW2100040077.1.2",
          "form": "나 이케아 조명 산거 짱 맘에들어",
          "original_form": "나 이케아 조명 산거 짱 맘에들어",
          "speaker_id": "1",
          "time": "20210904 10:20"
        },
        {
          "id": "MDRW2100040077.1.3",
          "form": "ㄱㅈㅋㅈㄱㅈㅋㅈㅋ",
          "original_form": "ㄱㅈㅋㅈㄱㅈㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210904 10:21"
        },
        {
          "id": "MDRW2100040077.1.4",
          "form": "아니니트바지는패스냐",
          "original_form": "아니니트바지는패스냐",
          "speaker_id": "2",
          "time": "20210904 10:21"
        },
        {
          "id": "MDRW2100040077.1.5",
          "form": "ㅋㄱㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱ",
          "original_form": "ㅋㄱㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱ",
          "speaker_id": "2",
          "time": "20210904 10:21"
        },
        {
          "id": "MDRW2100040077.1.6",
          "form": "조명얼매나이쁘길래궁금",
          "original_form": "조명얼매나이쁘길래궁금",
          "speaker_id": "2",
          "time": "20210904 10:21"
        },
        {
          "id": "MDRW2100040077.1.7",
          "form": "그냥 심플한데 켜듀면 좋아",
          "original_form": "그냥 심플한데 켜듀면 좋아",
          "speaker_id": "1",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100040077.1.8",
          "form": "누워서 폰할때도좋고",
          "original_form": "누워서 폰할때도좋고",
          "speaker_id": "1",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100040077.1.9",
          "form": "니트바지 고민 오지게하다가 샀어",
          "original_form": "니트바지 고민 오지게하다가 샀어",
          "speaker_id": "1",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100040077.1.10",
          "form": "조명은근유용해",
          "original_form": "조명은근유용해",
          "speaker_id": "2",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100040077.1.11",
          "form": "살면서점점느낌",
          "original_form": "살면서점점느낌",
          "speaker_id": "2",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100040077.1.12",
          "form": "니트 무슨색이냐구 베이지",
          "original_form": "니트 무슨색이냐구 베이지",
          "speaker_id": "2",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100040077.1.13",
          "form": "맞지!!",
          "original_form": "맞지!!",
          "speaker_id": "2",
          "time": "20210904 10:25"
        },
        {
          "id": "MDRW2100040077.1.14",
          "form": "바지는 화이트",
          "original_form": "바지는 화이트",
          "speaker_id": "1",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100040077.1.15",
          "form": "아 니트블랙이에요",
          "original_form": "아 니트블랙이에요",
          "speaker_id": "1",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100040077.1.16",
          "form": "아 블랙앤화이트네 멋쟁이예요",
          "original_form": "아 블랙앤화이트네 멋쟁이예요",
          "speaker_id": "2",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100040077.1.17",
          "form": "멋쟁이에요",
          "original_form": "멋쟁이에요",
          "speaker_id": "1",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100040077.1.18",
          "form": "이젠또뭘사볼까요",
          "original_form": "이젠또뭘사볼까요",
          "speaker_id": "1",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100040077.1.19",
          "form": "베레모는용",
          "original_form": "베레모는용",
          "speaker_id": "2",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100040077.1.20",
          "form": "베레모는 아직",
          "original_form": "베레모는 아직",
          "speaker_id": "1",
          "time": "20210904 11:37"
        },
        {
          "id": "MDRW2100040077.1.21",
          "form": "더워서 안샀어요",
          "original_form": "더워서 안샀어요",
          "speaker_id": "1",
          "time": "20210904 11:37"
        },
        {
          "id": "MDRW2100040077.1.22",
          "form": "아ㅋㅋㅋ미리준비해놔야죠 급추워질기세예요",
          "original_form": "아ㅋㅋㅋ미리준비해놔야죠 급추워질기세예요",
          "speaker_id": "2",
          "time": "20210904 11:38"
        },
        {
          "id": "MDRW2100040077.1.23",
          "form": "사면 무슨색살거예요",
          "original_form": "사면 무슨색살거예요",
          "speaker_id": "2",
          "time": "20210904 11:39"
        },
        {
          "id": "MDRW2100040077.1.24",
          "form": "까만색 살거에요",
          "original_form": "까만색 살거에요",
          "speaker_id": "1",
          "time": "20210904 11:39"
        },
        {
          "id": "MDRW2100040077.1.25",
          "form": "시크시크",
          "original_form": "시크시크",
          "speaker_id": "1",
          "time": "20210904 11:39"
        },
        {
          "id": "MDRW2100040077.1.26",
          "form": "오 잘어울리겟당",
          "original_form": "오 잘어울리겟당",
          "speaker_id": "2",
          "time": "20210904 11:40"
        },
        {
          "id": "MDRW2100040077.1.27",
          "form": "잘쑤고다니려나",
          "original_form": "잘쑤고다니려나",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100040077.1.28",
          "form": "잘쓸지는모르겟어",
          "original_form": "잘쓸지는모르겟어",
          "speaker_id": "2",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100040077.1.29",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100040077.1.30",
          "form": "쇼핑계속이어서!  베레모 이쁘긴이쁠거같은데",
          "original_form": "쇼핑계속이어서!  베레모 이쁘긴이쁠거같은데",
          "speaker_id": "2",
          "time": "20210904 13:05"
        }
      ]
    }
