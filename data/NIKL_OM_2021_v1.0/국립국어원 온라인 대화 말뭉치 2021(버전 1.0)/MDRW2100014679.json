      "utterance": [
        {
          "id": "MDRW2100014679.1.1",
          "form": "너 심야괴담회보나?",
          "original_form": "너 심야괴담회보나?",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014679.1.2",
          "form": "아 그거 무서운얘기하는거지",
          "original_form": "아 그거 무서운얘기하는거지",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014679.1.3",
          "form": "안봐ㅋㅋ",
          "original_form": "안봐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014679.1.4",
          "form": "나도 무서운거 싫은데 넘 재밌엌ㅋㅋㅋㅋㅋ",
          "original_form": "나도 무서운거 싫은데 넘 재밌엌ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014679.1.5",
          "form": "근데 자기전엔 안봄",
          "original_form": "근데 자기전엔 안봄",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014679.1.6",
          "form": "꿈에나올까봐ㅋㅋㅋ",
          "original_form": "꿈에나올까봐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014679.1.7",
          "form": "ㅋㅋㅋ아 뭔데 자기전에만 안보면 괜찮?",
          "original_form": "ㅋㅋㅋ아 뭔데 자기전에만 안보면 괜찮?",
          "speaker_id": "1",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014679.1.8",
          "form": "맨.인더다크 2 개봉한다는데",
          "original_form": "맨.인더다크 2 개봉한다는데",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014679.1.9",
          "form": "영화관가서 보고싶어",
          "original_form": "영화관가서 보고싶어",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014679.1.10",
          "form": "너는 보고싶은 영화 없어?",
          "original_form": "너는 보고싶은 영화 없어?",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014679.1.11",
          "form": "나 요새 영화 땡기는게 없음ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나 요새 영화 땡기는게 없음ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014679.1.12",
          "form": "영화관도 간적이...",
          "original_form": "영화관도 간적이...",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014679.1.13",
          "form": "너 취향 영화 요즘 없나?",
          "original_form": "너 취향 영화 요즘 없나?",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014679.1.14",
          "form": "나는 그래도 몇달동안 영화 꽤 봤어",
          "original_form": "나는 그래도 몇달동안 영화 꽤 봤어",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014679.1.15",
          "form": "블랙위도우 킬러의 보디가드 보스베이비 이렇게 !",
          "original_form": "블랙위도우 킬러의 보디가드 보스베이비 이렇게 !",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014679.1.16",
          "form": "오 되게많이봤네??!",
          "original_form": "오 되게많이봤네??!",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014679.1.17",
          "form": "나는 요새 영화보다는 드라마라ㅋㅋㅋㅋㅋ",
          "original_form": "나는 요새 영화보다는 드라마라ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014679.1.18",
          "form": "드라마아니면 예능ㅋㅋㅋ",
          "original_form": "드라마아니면 예능ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014679.1.19",
          "form": "ㅋㅋ나랑 반대네",
          "original_form": "ㅋㅋ나랑 반대네",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100014679.1.20",
          "form": "나는 요즘 드라마랑 예능 챙겨보는게 별로없어",
          "original_form": "나는 요즘 드라마랑 예능 챙겨보는게 별로없어",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100014679.1.21",
          "form": "영화관가서 영화보는게 힐링이야",
          "original_form": "영화관가서 영화보는게 힐링이야",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100014679.1.22",
          "form": "근데 영화비 너무비쌈",
          "original_form": "근데 영화비 너무비쌈",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100014679.1.23",
          "form": "나 메가박스 원래 스크할인됐는데 스크에서 이제 할인적용안해준대ㅋㅋㅋㅋ",
          "original_form": "나 메가박스 원래 스크할인됐는데 스크에서 이제 할인적용안해준대ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100014679.1.24",
          "form": "영화 볼것두 없는데 영화관비싸고 통신사할인도 안되고...ㅎㅎ",
          "original_form": "영화 볼것두 없는데 영화관비싸고 통신사할인도 안되고...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100014679.1.25",
          "form": "요즘 할인적용 다 없애더라",
          "original_form": "요즘 할인적용 다 없애더라",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100014679.1.26",
          "form": "그래서 영화관자체 이벤트 할인 쓰는중",
          "original_form": "그래서 영화관자체 이벤트 할인 쓰는중",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100014679.1.27",
          "form": "쿠폰 엄청 뿌려서 선착안에 들면 이득",
          "original_form": "쿠폰 엄청 뿌려서 선착안에 들면 이득",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100014679.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ그리고 넷플있어서 뭔가 넷플에 올라오길 바라는중ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그리고 넷플있어서 뭔가 넷플에 올라오길 바라는중ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100014679.1.29",
          "form": "하긴 넷플이용하면 뭐ㅋㅋ근데 나는 영화관 자체를 가는걸 즐기는 사람이라서",
          "original_form": "하긴 넷플이용하면 뭐ㅋㅋ근데 나는 영화관 자체를 가는걸 즐기는 사람이라서",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100014679.1.30",
          "form": "영화관 아니면 집중을 못해",
          "original_form": "영화관 아니면 집중을 못해",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100014679.1.31",
          "form": "계속 앞으로 넘겨ㅋㅋㅋ",
          "original_form": "계속 앞으로 넘겨ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100014679.1.32",
          "form": "아 뭔지알아ㅋㅋㅋㅋ",
          "original_form": "아 뭔지알아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 07:41"
        },
        {
          "id": "MDRW2100014679.1.33",
          "form": "넷플이나 티빙 웨이브로 보면 배속으로 보거나 10초건너뛰고ㅋㅋㅋㅋ",
          "original_form": "넷플이나 티빙 웨이브로 보면 배속으로 보거나 10초건너뛰고ㅋㅋㅋㅋ",
          "speaker_id": "2",
