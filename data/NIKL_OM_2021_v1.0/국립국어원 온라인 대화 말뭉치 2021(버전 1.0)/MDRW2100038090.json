        {
          "id": "MDRW2100038090.1.1",
          "form": "요즘 요기요 가 세일을 많이 해",
          "original_form": "요즘 요기요 가 세일을 많이 해",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038090.1.2",
          "form": "배달 어플 625말하는 거야",
          "original_form": "배달 어플 625말하는 거야",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038090.1.3",
          "form": "요기요",
          "original_form": "요기요",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038090.1.4",
          "form": "배달 어플이 쓰면은 편해",
          "original_form": "배달 어플이 쓰면은 편해",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038090.1.5",
          "form": "안 나가도 되고 좋지",
          "original_form": "안 나가도 되고 좋지",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038090.1.6",
          "form": "그리고 은근히 가격이 괜찮아",
          "original_form": "그리고 은근히 가격이 괜찮아",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038090.1.7",
          "form": "배달비 너무 비싸지 않나",
          "original_form": "배달비 너무 비싸지 않나",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038090.1.8",
          "form": "맞아 어떤 건 4000원",
          "original_form": "맞아 어떤 건 4000원",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038090.1.9",
          "form": "더울 때는 배달시키면 좋긴 하더라",
          "original_form": "더울 때는 배달시키면 좋긴 하더라",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038090.1.10",
          "form": "비 오는 날에는 배달이 잘 안 돼",
          "original_form": "비 오는 날에는 배달이 잘 안 돼",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038090.1.11",
          "form": "요즘 요기요는 무슨 행사를 하니",
          "original_form": "요즘 요기요는 무슨 행사를 하니",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038090.1.12",
          "form": "저번 주에는 파리바게트가 반갑",
          "original_form": "저번 주에는 파리바게트가 반갑",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038090.1.13",
          "form": "파리바게트 행사했구나",
          "original_form": "파리바게트 행사했구나",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038090.1.14",
          "form": "이번 주는 굽네",
