      "utterance": [
        {
          "id": "MDRW2100016340.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100016340.1.2",
          "form": "안녕하세여!",
          "original_form": "안녕하세여!",
          "speaker_id": "1",
          "time": "20210826 12:29"
        },
        {
          "id": "MDRW2100016340.1.3",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "2",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100016340.1.4",
          "form": "아니용ㅋㅋㅋ",
          "original_form": "아니용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016340.1.5",
          "form": "종교 있으신가요?",
          "original_form": "종교 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016340.1.6",
          "form": "무교예요!",
          "original_form": "무교예요!",
          "speaker_id": "2",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100016340.1.7",
          "form": "ㅋㅋㅋ무교가 편한거 같아요",
          "original_form": "ㅋㅋㅋ무교가 편한거 같아요",
          "speaker_id": "1",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016340.1.8",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100016340.1.9",
          "form": "절가본적있으세요?",
          "original_form": "절가본적있으세요?",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100016340.1.10",
          "form": "몇번 가본적은 있어요ㅋㅋㅋ",
          "original_form": "몇번 가본적은 있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100016340.1.11",
          "form": "사실 그렇게 종교에 대해서 좋은 감정이 있진 않지만 불교쪽은 괜찮아요ㅋㅋㅋ",
          "original_form": "사실 그렇게 종교에 대해서 좋은 감정이 있진 않지만 불교쪽은 괜찮아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100016340.1.12",
          "form": "절 가면 꽤 힐링도 되고ㅋㅋㅋㅋ",
          "original_form": "절 가면 꽤 힐링도 되고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100016340.1.13",
          "form": "저는 봉은사 가봤어용",
          "original_form": "저는 봉은사 가봤어용",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100016340.1.14",
          "form": "아 봉은사 유명하죠ㅋㅋㅋ",
          "original_form": "아 봉은사 유명하죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016340.1.15",
          "form": "가을쯤에 절 가면 분위기도 좋고 힐링하기 좋은거같아요",
          "original_form": "가을쯤에 절 가면 분위기도 좋고 힐링하기 좋은거같아요",
          "speaker_id": "1",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100016340.1.16",
          "form": "맞아요 스님 종치는 소리 들으면 평온해져요",
          "original_form": "맞아요 스님 종치는 소리 들으면 평온해져요",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016340.1.17",
          "form": "나중에 기회되면 템플스테이도 해보고 싶어요",
          "original_form": "나중에 기회되면 템플스테이도 해보고 싶어요",
          "speaker_id": "1",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100016340.1.18",
          "form": "저도요ㅋㅋ",
          "original_form": "저도요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:47"
        }
      ]
