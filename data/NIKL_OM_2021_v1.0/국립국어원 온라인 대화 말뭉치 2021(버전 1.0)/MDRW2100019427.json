      "utterance": [
        {
          "id": "MDRW2100019427.1.1",
          "form": "좋아하는 콘텐츠나 연예인 있으신가요?",
          "original_form": "좋아하는 콘텐츠나 연예인 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019427.1.2",
          "form": "저는 유튜브나 넷플릭스 많이 봐요!!!",
          "original_form": "저는 유튜브나 넷플릭스 많이 봐요!!!",
          "speaker_id": "2",
          "time": "20210827 19:22"
        },
        {
          "id": "MDRW2100019427.1.3",
          "form": "어떤 거 많이 보시나요?",
          "original_form": "어떤 거 많이 보시나요?",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019427.1.4",
          "form": "요즘은 슬의생도 보고",
          "original_form": "요즘은 슬의생도 보고",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019427.1.5",
          "form": "펜트하우스 재방봐요ㅋㅋ",
          "original_form": "펜트하우스 재방봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019427.1.6",
          "form": "인기작들 골라보시네여",
          "original_form": "인기작들 골라보시네여",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019427.1.7",
          "form": "완전 재밌잖아요ㅋㅋ다음화가 어떻게 될지 궁금하구",
          "original_form": "완전 재밌잖아요ㅋㅋ다음화가 어떻게 될지 궁금하구",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100019427.1.8",
          "form": "오늘 펜트하우스 본방송 어마어마하네요",
          "original_form": "오늘 펜트하우스 본방송 어마어마하네요",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100019427.1.9",
          "form": "왜요? 오늘 펜트하우스 어땠나요?",
          "original_form": "왜요? 오늘 펜트하우스 어땠나요?",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100019427.1.10",
          "form": "오늘도 또 죽었어요",
          "original_form": "오늘도 또 죽었어요",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100019427.1.11",
          "form": "맨날 누군가가 죽네요",
          "original_form": "맨날 누군가가 죽네요",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100019427.1.12",
          "form": "또요?? 페이크 아닐까요?",
          "original_form": "또요?? 페이크 아닐까요?",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100019427.1.13",
          "form": "이번엔 진짜로 죽었어요ㅋㅋㅋ",
          "original_form": "이번엔 진짜로 죽었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100019427.1.14",
          "form": "살아나면....그건.....진짜 사람 아니예요",
          "original_form": "살아나면....그건.....진짜 사람 아니예요",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100019427.1.15",
          "form": "왜요 머리에 트로피 박히고도 살아난 사람도 있지 않았나요?",
          "original_form": "왜요 머리에 트로피 박히고도 살아난 사람도 있지 않았나요?",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100019427.1.16",
          "form": "ㅋㅋㅋㄱㅋㅋㅋ하도 죽고살아나고 해서 안믿기긴 한데",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋ하도 죽고살아나고 해서 안믿기긴 한데",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100019427.1.17",
          "form": "이마에 총맞았으면....",
          "original_form": "이마에 총맞았으면....",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100019427.1.18",
          "form": "가망없지않나요",
          "original_form": "가망없지않나요",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100019427.1.19",
          "form": "이마에 총이요?? 암시아니고 진짜 뚫는게 나왔나요?",
          "original_form": "이마에 총이요?? 암시아니고 진짜 뚫는게 나왔나요?",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100019427.1.20",
          "form": "네네 진짜 총 빡 맞았어요",
          "original_form": "네네 진짜 총 빡 맞았어요",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100019427.1.21",
          "form": "그리고 펜트하우스에서 떨어졌어요",
          "original_form": "그리고 펜트하우스에서 떨어졌어요",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100019427.1.22",
          "form": "와 이건 안죽었으면 불사신이네요",
          "original_form": "와 이건 안죽었으면 불사신이네요",
          "speaker_id": "1",
          "time": "20210827 23:48"
        }
      ]
    }
