      "utterance": [
        {
          "id": "MDRW2100028532.1.1",
          "form": "좋아하는 책있어?",
          "original_form": "좋아하는 책있어?",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028532.1.2",
          "form": "책 안읽어 좋아하는 책있어?",
          "original_form": "책 안읽어 좋아하는 책있어?",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028532.1.3",
          "form": "해리포터",
          "original_form": "해리포터",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028532.1.4",
          "form": "해리포터 시리즈 있잖아 어떤 시리즈 좋아?",
          "original_form": "해리포터 시리즈 있잖아 어떤 시리즈 좋아?",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028532.1.5",
          "form": "마법사의 돌편 좋아해",
          "original_form": "마법사의 돌편 좋아해",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028532.1.6",
          "form": "마법사의 돌편 좋아한다고?",
          "original_form": "마법사의 돌편 좋아한다고?",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028532.1.7",
          "form": "응 봤어?",
          "original_form": "응 봤어?",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028532.1.8",
          "form": "아니 한번도 안봤어",
          "original_form": "아니 한번도 안봤어",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028532.1.9",
          "form": "책 왜 안좋아해?",
          "original_form": "책 왜 안좋아해?",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028532.1.10",
          "form": "그냥 싫어서",
          "original_form": "그냥 싫어서",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028532.1.11",
          "form": "잡지는?",
          "original_form": "잡지는?",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
