      "utterance": [
        {
          "id": "MDRW2100026303.1.1",
          "form": "아 그랬었겠어요",
          "original_form": "아 그랬었겠어요",
          "speaker_id": "2",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026303.1.2",
          "form": "그래도 잘안보이니 더 잘들었을거같아요",
          "original_form": "그래도 잘안보이니 더 잘들었을거같아요",
          "speaker_id": "2",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026303.1.3",
          "form": "네~ 좋았어요ㅎㅎㅎ",
          "original_form": "네~ 좋았어요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026303.1.4",
          "form": "음악은 언제들어도 아름다우니!",
          "original_form": "음악은 언제들어도 아름다우니!",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026303.1.5",
          "form": "특히나 뮤지컬배우가 들려주는 노래라서 더 ㅎㅎㅎ",
          "original_form": "특히나 뮤지컬배우가 들려주는 노래라서 더 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026303.1.6",
          "form": "조승우 좋아하면 영화도 다 보셨겠어요!",
          "original_form": "조승우 좋아하면 영화도 다 보셨겠어요!",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026303.1.7",
          "form": "네ㅋㅋ드라마도 섭렵했죠",
          "original_form": "네ㅋㅋ드라마도 섭렵했죠",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026303.1.8",
          "form": "요즘 조승우가 늙어가는게 보여 안타까워요",
          "original_form": "요즘 조승우가 늙어가는게 보여 안타까워요",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026303.1.9",
          "form": "포스트조승우라고 했던 배우도 결혼하고 그런거보면...",
          "original_form": "포스트조승우라고 했던 배우도 결혼하고 그런거보면...",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026303.1.10",
          "form": "그러네요 세월이..",
          "original_form": "그러네요 세월이..",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026303.1.11",
          "form": "저 왜이렇게 배우 이름이 생각이 안나죠 ㅠㅋㅋㅋ",
          "original_form": "저 왜이렇게 배우 이름이 생각이 안나죠 ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026303.1.12",
          "form": "저분도 공연도 하셨던것 같은데",
          "original_form": "저분도 공연도 하셨던것 같은데",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026303.1.13",
          "form": "포스트 조승우가 있었어요?ㅋㅋ",
          "original_form": "포스트 조승우가 있었어요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026303.1.14",
          "form": "그쵸 나이들어가는게 실감나죠",
          "original_form": "그쵸 나이들어가는게 실감나죠",
          "speaker_id": "2",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026303.1.15",
          "form": "아.. 그 닮았다고 배우있었는데 이름이 생각안나서 실패..",
          "original_form": "아.. 그 닮았다고 배우있었는데 이름이 생각안나서 실패..",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026303.1.16",
          "form": "제가 알기론 그분도 연극했었어요!!",
          "original_form": "제가 알기론 그분도 연극했었어요!!",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026303.1.17",
          "form": "ㅋㅋ 요즘 잘하는 사람이 많죠",
          "original_form": "ㅋㅋ 요즘 잘하는 사람이 많죠",
          "speaker_id": "2",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026303.1.18",
          "form": "연기 잘하는사람은 늘 있는데 인기가 많은가 부각이 되는가 차이인것 같아요",
          "original_form": "연기 잘하는사람은 늘 있는데 인기가 많은가 부각이 되는가 차이인것 같아요",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026303.1.19",
          "form": "똑같은 능력이 있어도 뭔가 로또같은..",
          "original_form": "똑같은 능력이 있어도 뭔가 로또같은..",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026303.1.20",
          "form": "맞아요 시기와 행운이 맞아야하는 것 같아요",
          "original_form": "맞아요 시기와 행운이 맞아야하는 것 같아요",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026303.1.21",
          "form": "요즘 영화관에서 본 영화 있으세요?",
          "original_form": "요즘 영화관에서 본 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026303.1.22",
          "form": "영화관안가고 집에서 다운받아서 보고 있어요",
          "original_form": "영화관안가고 집에서 다운받아서 보고 있어요",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026303.1.23",
          "form": "요즘 영화관 가보셨나요?",
          "original_form": "요즘 영화관 가보셨나요?",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026303.1.24",
          "form": "저는 작년까지는 갔던것 같아요",
          "original_form": "저는 작년까지는 갔던것 같아요",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026303.1.25",
          "form": "원래는 자주보러갔는데 코로나덕분에 vip도 끝났어요 ㅋㅋㅋ",
          "original_form": "원래는 자주보러갔는데 코로나덕분에 vip도 끝났어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026303.1.26",
          "form": "맞아요 저도 포인트 엄청 있는데 쓰질 못하고 있네요",
          "original_form": "맞아요 저도 포인트 엄청 있는데 쓰질 못하고 있네요",
