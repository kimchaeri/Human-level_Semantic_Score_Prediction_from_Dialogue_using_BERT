      "utterance": [
        {
          "id": "MDRW2100021543.1.1",
          "form": "최근에 구매한 물건 있으세요?",
          "original_form": "최근에 구매한 물건 있으세요?",
          "speaker_id": "2",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100021543.1.2",
          "form": "저는.. 최근에 뭘 샀냐면",
          "original_form": "저는.. 최근에 뭘 샀냐면",
          "speaker_id": "1",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100021543.1.3",
          "form": "먹을 것을 많이 샀어요!!!",
          "original_form": "먹을 것을 많이 샀어요!!!",
          "speaker_id": "1",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100021543.1.4",
          "form": "그래놀라 후레이크를 샀답니다",
          "original_form": "그래놀라 후레이크를 샀답니다",
          "speaker_id": "1",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100021543.1.5",
          "form": "name2은요???",
          "original_form": "&name2&은요???",
          "speaker_id": "1",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100021543.1.6",
          "form": "그래놀라 맛있나요? :)어디 그래놀라에요?",
          "original_form": "그래놀라 맛있나요? :)어디 그래놀라에요?",
          "speaker_id": "2",
          "time": "20210829 12:59"
        },
        {
          "id": "MDRW2100021543.1.7",
          "form": "저는 아이패드 파우치를 샀어요!.!",
          "original_form": "저는 아이패드 파우치를 샀어요!.!",
          "speaker_id": "2",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100021543.1.8",
          "form": "켈로그 제품이었어요! ",
          "original_form": "켈로그 제품이었어요! ",
          "speaker_id": "1",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100021543.1.9",
          "form": "아이패드 파우치라니~~~",
          "original_form": "아이패드 파우치라니~~~",
          "speaker_id": "1",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100021543.1.10",
          "form": "혹시 아이패드를 최근에 사셨나요?",
          "original_form": "혹시 아이패드를 최근에 사셨나요?",
          "speaker_id": "1",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100021543.1.11",
          "form": "네 맞아요!!",
          "original_form": "네 맞아요!!",
          "speaker_id": "2",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100021543.1.12",
          "form": "역시 시리얼은 켈로그인가봐요 ㅋㅋㅋ",
          "original_form": "역시 시리얼은 켈로그인가봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100021543.1.13",
          "form": "맞습니다~~~ 거기다가",
          "original_form": "맞습니다~~~ 거기다가",
          "speaker_id": "1",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100021543.1.14",
          "form": "급식판도 같이 줘서",
          "original_form": "급식판도 같이 줘서",
          "speaker_id": "1",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100021543.1.15",
          "form": "얼마나 좋은지 몰라요,,",
          "original_form": "얼마나 좋은지 몰라요,,",
          "speaker_id": "1",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100021543.1.16",
          "form": "아이패드 자랑 좀 해주세요~~~",
          "original_form": "아이패드 자랑 좀 해주세요~~~",
          "speaker_id": "1",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100021543.1.17",
          "form": "ㅋㅋㅋ구입하신 급식판 보고싶은데",
          "original_form": "ㅋㅋㅋ구입하신 급식판 보고싶은데",
          "speaker_id": "2",
          "time": "20210829 15:15"
        },
        {
          "id": "MDRW2100021543.1.18",
          "form": "여기로 사진을 못 보내서 아쉽네요",
          "original_form": "여기로 사진을 못 보내서 아쉽네요",
          "speaker_id": "2",
          "time": "20210829 15:15"
        },
        {
          "id": "MDRW2100021543.1.19",
          "form": "아이패드 가격은 후덜덜하지만 진짜 만족이에요!!",
          "original_form": "아이패드 가격은 후덜덜하지만 진짜 만족이에요!!",
          "speaker_id": "2",
          "time": "20210829 15:15"
        },
        {
          "id": "MDRW2100021543.1.20",
          "form": "아직 드로잉은 별로 못하고있는데",
          "original_form": "아직 드로잉은 별로 못하고있는데",
          "speaker_id": "2",
          "time": "20210829 15:15"
        },
        {
