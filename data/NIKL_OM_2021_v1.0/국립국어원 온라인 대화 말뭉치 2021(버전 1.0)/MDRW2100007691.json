        },
        {
          "id": "MDRW2100007691.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210822 09:48"
        },
        {
          "id": "MDRW2100007691.1.3",
          "form": "키우시나요?",
          "original_form": "키우시나요?",
          "speaker_id": "2",
          "time": "20210822 09:48"
        },
        {
          "id": "MDRW2100007691.1.4",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210822 09:49"
        },
        {
          "id": "MDRW2100007691.1.5",
          "form": "키우실 계획있으시나요?",
          "original_form": "키우실 계획있으시나요?",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007691.1.6",
          "form": "네 여건이",
          "original_form": "네 여건이",
          "speaker_id": "1",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007691.1.7",
          "form": "어떤종류요?",
          "original_form": "어떤종류요?",
          "speaker_id": "2",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007691.1.8",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210822 09:56"
        },
        {
          "id": "MDRW2100007691.1.9",
          "form": "귀엽겠네요",
          "original_form": "귀엽겠네요",
          "speaker_id": "2",
          "time": "20210822 09:59"
        },
        {
          "id": "MDRW2100007691.1.10",
          "form": "관리가 문제죠",
          "original_form": "관리가 문제죠",
          "speaker_id": "1",
          "time": "20210822 10:00"
        },
        {
          "id": "MDRW2100007691.1.11",
          "form": "맞아요 책임감도 크고",
          "original_form": "맞아요 책임감도 크고",
          "speaker_id": "2",
          "time": "20210822 10:43"
        },
        {
          "id": "MDRW2100007691.1.12",
          "form": "님은 어떤거 키우고싶은데요?",
          "original_form": "님은 어떤거 키우고싶은데요?",
          "speaker_id": "1",
          "time": "20210822 11:36"
        },
        {
          "id": "MDRW2100007691.1.13",
          "form": "저는 안키우고싶어요 ㅎ별로 안좋아해서",
