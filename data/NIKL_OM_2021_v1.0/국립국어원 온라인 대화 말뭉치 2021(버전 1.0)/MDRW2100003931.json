      "utterance": [
        {
          "id": "MDRW2100003931.1.1",
          "form": "학교에 몇 시에 등교하나요?",
          "original_form": "학교에 몇 시에 등교하나요?",
          "speaker_id": "1",
          "time": "20210821 13:59"
        },
        {
          "id": "MDRW2100003931.1.2",
          "form": "학교 졸업한지 오래되어서...또르륵",
          "original_form": "학교 졸업한지 오래되어서...또르륵",
          "speaker_id": "2",
          "time": "20210821 14:03"
        },
        {
          "id": "MDRW2100003931.1.3",
          "form": "아 그렇다면 출근 시간은 언제인가요?",
          "original_form": "아 그렇다면 출근 시간은 언제인가요?",
          "speaker_id": "1",
          "time": "20210821 14:03"
        },
        {
          "id": "MDRW2100003931.1.4",
          "form": "6시에 일어나서...7시에 집에서 나갑니다ㅜ",
          "original_form": "6시에 일어나서...7시에 집에서 나갑니다ㅜ",
          "speaker_id": "2",
          "time": "20210821 14:04"
        },
        {
          "id": "MDRW2100003931.1.5",
          "form": "학교다니세요?",
          "original_form": "학교다니세요?",
          "speaker_id": "2",
          "time": "20210821 14:04"
        },
        {
          "id": "MDRW2100003931.1.6",
          "form": "그렇군요 저는 집에서 근무합니다",
          "original_form": "그렇군요 저는 집에서 근무합니다",
          "speaker_id": "1",
          "time": "20210821 14:05"
        },
        {
          "id": "MDRW2100003931.1.7",
          "form": "아침 식사는 하세요?",
          "original_form": "아침 식사는 하세요?",
          "speaker_id": "1",
          "time": "20210821 14:05"
        },
        {
          "id": "MDRW2100003931.1.8",
          "form": "네ㅋㅋ식사는 꼭 해요",
          "original_form": "네ㅋㅋ식사는 꼭 해요",
          "speaker_id": "2",
          "time": "20210821 14:06"
        },
        {
          "id": "MDRW2100003931.1.9",
          "form": "저는 아침 식사로 과일을 먹어요",
          "original_form": "저는 아침 식사로 과일을 먹어요",
          "speaker_id": "1",
          "time": "20210821 14:07"
        },
        {
          "id": "MDRW2100003931.1.10",
          "form": "그러고보면 대학생 때가 제일 좋았던 것 같아요",
          "original_form": "그러고보면 대학생 때가 제일 좋았던 것 같아요",
          "speaker_id": "2",
          "time": "20210821 14:07"
        },
        {
          "id": "MDRW2100003931.1.11",
          "form": "저는 밥이요!",
          "original_form": "저는 밥이요!",
          "speaker_id": "2",
          "time": "20210821 14:07"
        },
        {
          "id": "MDRW2100003931.1.12",
          "form": "대학생 때 과제며 시험이며 힘들긴 했지만ㅋㅋ 나름 그때 자유로웠던 것 같네요",
          "original_form": "대학생 때 과제며 시험이며 힘들긴 했지만ㅋㅋ 나름 그때 자유로웠던 것 같네요",
          "speaker_id": "2",
          "time": "20210821 14:07"
        },
        {
          "id": "MDRW2100003931.1.13",
          "form": "맞아요 학생 때가 좋기는 하죠 그때는 싫었지만요",
          "original_form": "맞아요 학생 때가 좋기는 하죠 그때는 싫었지만요",
          "speaker_id": "1",
          "time": "20210821 14:08"
        },
        {
          "id": "MDRW2100003931.1.14",
          "form": "대학생 때 더 놀러 다닐걸 후회돼요",
          "original_form": "대학생 때 더 놀러 다닐걸 후회돼요",
          "speaker_id": "1",
          "time": "20210821 14:08"
        },
        {
          "id": "MDRW2100003931.1.15",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003931.1.16",
          "form": "대학생 때 동아리활동도 하셨나요?",
          "original_form": "대학생 때 동아리활동도 하셨나요?",
          "speaker_id": "2",
          "time": "20210821 14:10"
        },
        {
          "id": "MDRW2100003931.1.17",
          "form": "네 저는 노래 부르는 동아리 했어요",
          "original_form": "네 저는 노래 부르는 동아리 했어요",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003931.1.18",
          "form": "님은 동아리 활동 하셨나요?",
          "original_form": "님은 동아리 활동 하셨나요?",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003931.1.19",
          "form": "아뇨 저는 안했어요ㅋㅋ동아리가 딱히 끌리는 곳이 없기도 했고 대부분 거의 술만 마시더라구요",
          "original_form": "아뇨 저는 안했어요ㅋㅋ동아리가 딱히 끌리는 곳이 없기도 했고 대부분 거의 술만 마시더라구요",
          "speaker_id": "2",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003931.1.20",
          "form": "노래 잘부르시나봐요!",
          "original_form": "노래 잘부르시나봐요!",
          "speaker_id": "2",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003931.1.21",
          "form": "노래는 못 부르지만 친구들 따라 들어갔어요",
          "original_form": "노래는 못 부르지만 친구들 따라 들어갔어요",
          "speaker_id": "1",
          "time": "20210821 14:15"
        },
        {
          "id": "MDRW2100003931.1.22",
          "form": "맞아요 동아리가 술 많이 마시기는 해요",
          "original_form": "맞아요 동아리가 술 많이 마시기는 해요",
          "speaker_id": "1",
          "time": "20210821 14:15"
        },
        {
          "id": "MDRW2100003931.1.23",
          "form": "저는 술을 못마셔서ㅜ",
          "original_form": "저는 술을 못마셔서ㅜ",
          "speaker_id": "2",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003931.1.24",
          "form": "친구분들하고 동아리 같이 들어갔으면  더 재밌었겠네요",
          "original_form": "친구분들하고 동아리 같이 들어갔으면  더 재밌었겠네요",
          "speaker_id": "2",
          "time": "20210821 14:19"
        }
      ]
    }
