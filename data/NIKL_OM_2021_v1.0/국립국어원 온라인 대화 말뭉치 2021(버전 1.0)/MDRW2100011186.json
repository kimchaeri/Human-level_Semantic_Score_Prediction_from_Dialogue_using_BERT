      "utterance": [
        {
          "id": "MDRW2100011186.1.1",
          "form": "진로 소주 좋아하시나요ㅎㅎ",
          "original_form": "진로 소주 좋아하시나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 02:40"
        },
        {
          "id": "MDRW2100011186.1.2",
          "form": "술을잘 못해요ㅎ",
          "original_form": "술을잘 못해요ㅎ",
          "speaker_id": "2",
          "time": "20210824 02:41"
        },
        {
          "id": "MDRW2100011186.1.3",
          "form": "꺼비는 알아요 그래도ㅋ",
          "original_form": "꺼비는 알아요 그래도ㅋ",
          "speaker_id": "2",
          "time": "20210824 02:41"
        },
        {
          "id": "MDRW2100011186.1.4",
          "form": "곰표맥주는 드셔보셨나요ㅎㅎ",
          "original_form": "곰표맥주는 드셔보셨나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 02:42"
        },
        {
          "id": "MDRW2100011186.1.5",
          "form": "술좋아시는군요?ㅎㅎ",
          "original_form": "술좋아시는군요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 02:42"
        },
        {
          "id": "MDRW2100011186.1.6",
          "form": "곰표 핸드크림은 집에있어요ㅋ",
          "original_form": "곰표 핸드크림은 집에있어요ㅋ",
          "speaker_id": "2",
          "time": "20210824 02:42"
        },
        {
          "id": "MDRW2100011186.1.7",
          "form": "편의점에 곰표맥주가  파나요?",
          "original_form": "편의점에 곰표맥주가  파나요?",
          "speaker_id": "2",
          "time": "20210824 02:43"
        },
        {
          "id": "MDRW2100011186.1.8",
          "form": "헉 곰표 핸드크림도 나오나요??",
          "original_form": "헉 곰표 핸드크림도 나오나요??",
          "speaker_id": "1",
          "time": "20210824 02:43"
        },
        {
          "id": "MDRW2100011186.1.9",
          "form": "아 콜라보로 곰표에서 이벤트당첨되면주더라구요",
          "original_form": "아 콜라보로 곰표에서 이벤트당첨되면주더라구요",
          "speaker_id": "2",
          "time": "20210824 02:43"
        },
        {
          "id": "MDRW2100011186.1.10",
          "form": "에코팩도있고 패딩도있고ㅋ",
          "original_form": "에코팩도있고 패딩도있고ㅋ",
          "speaker_id": "2",
          "time": "20210824 02:44"
        },
        {
          "id": "MDRW2100011186.1.11",
          "form": "아 진짜패딩은 좀 아닌것같아요ㅋㅋㅋ",
          "original_form": "아 진짜패딩은 좀 아닌것같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 02:44"
        },
        {
          "id": "MDRW2100011186.1.12",
          "form": "좀 입기 민망할것같은데",
          "original_form": "좀 입기 민망할것같은데",
          "speaker_id": "2",
          "time": "20210824 02:44"
        },
        {
          "id": "MDRW2100011186.1.13",
          "form": "한때 핫했나봐요",
          "original_form": "한때 핫했나봐요",
          "speaker_id": "2",
          "time": "20210824 02:44"
        },
        {
          "id": "MDRW2100011186.1.14",
          "form": "검색해보시면 뜰거예요ㅎ",
          "original_form": "검색해보시면 뜰거예요ㅎ",
          "speaker_id": "2",
          "time": "20210824 02:45"
        },
        {
          "id": "MDRW2100011186.1.15",
          "form": "줘도 안입을것 같은데ㅎㅎ",
          "original_form": "줘도 안입을것 같은데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 02:45"
        },
        {
          "id": "MDRW2100011186.1.16",
          "form": "ㅋㅋ 저도요ㅎ cu가면 곰표 팝콘도 있자나요",
          "original_form": "ㅋㅋ 저도요ㅎ cu가면 곰표 팝콘도 있자나요",
          "speaker_id": "2",
          "time": "20210824 02:46"
        },
        {
          "id": "MDRW2100011186.1.17",
          "form": "원래 밀가루 회사 아닌가요ㅋㅋㅋ",
          "original_form": "원래 밀가루 회사 아닌가요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 02:48"
        },
        {
          "id": "MDRW2100011186.1.18",
          "form": "맞아요 전소민이 패딩입었었네요ㅎㅎ",
          "original_form": "맞아요 전소민이 패딩입었었네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 02:49"
        },
        {
          "id": "MDRW2100011186.1.19",
          "form": "핵인싸라고",
          "original_form": "핵인싸라고",
          "speaker_id": "2",
          "time": "20210824 02:49"
        },
        {
          "id": "MDRW2100011186.1.20",
          "form": "헐 광고 아닐까요ㅎㅎ",
