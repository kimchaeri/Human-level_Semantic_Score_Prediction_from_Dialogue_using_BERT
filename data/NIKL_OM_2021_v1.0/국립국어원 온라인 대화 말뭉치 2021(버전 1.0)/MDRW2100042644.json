      "utterance": [
        {
          "id": "MDRW2100042644.1.1",
          "form": "점심 먹었니",
          "original_form": "점심 먹었니",
          "speaker_id": "1",
          "time": "20210907 12:59"
        },
        {
          "id": "MDRW2100042644.1.2",
          "form": "아점 먹었지 어제남은 제육에 밥볶았다ㅋㅋ",
          "original_form": "아점 먹었지 어제남은 제육에 밥볶았다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100042644.1.3",
          "form": "나는 오늘도 파스타 먹어야디",
          "original_form": "나는 오늘도 파스타 먹어야디",
          "speaker_id": "1",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100042644.1.4",
          "form": "오늘도 베이컨 넣니?",
          "original_form": "오늘도 베이컨 넣니?",
          "speaker_id": "2",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100042644.1.5",
          "form": "필수필수",
          "original_form": "필수필수",
          "speaker_id": "1",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100042644.1.6",
          "form": "베이컨 많이 넣어주세염ㅋ",
          "original_form": "베이컨 많이 넣어주세염ㅋ",
          "speaker_id": "2",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100042644.1.7",
          "form": "소스 미리 만들어 놓은건데 더 구워서 올려 먹으려고 ㅋㅋㅋ",
          "original_form": "소스 미리 만들어 놓은건데 더 구워서 올려 먹으려고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100042644.1.8",
          "form": "점심으로 먹니?",
          "original_form": "점심으로 먹니?",
          "speaker_id": "2",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100042644.1.9",
          "form": "당연쓰 ㅋㅋㅋ",
          "original_form": "당연쓰 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100042644.1.10",
          "form": "음료는 없니?",
          "original_form": "음료는 없니?",
          "speaker_id": "2",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100042644.1.11",
          "form": "다 먹고 커피 마셔야지",
          "original_form": "다 먹고 커피 마셔야지",
          "speaker_id": "1",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100042644.1.12",
          "form": "커피...좋네...나도 커피맛곱다ㅠ",
          "original_form": "커피...좋네...나도 커피맛곱다ㅠ",
          "speaker_id": "2",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100042644.1.13",
          "form": "마시곱다...",
          "original_form": "마시곱다...",
          "speaker_id": "2",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100042644.1.14",
          "form": "마셔.......행복을 마시자!",
          "original_form": "마셔.......행복을 마시자!",
          "speaker_id": "1",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100042644.1.15",
          "form": "내 속쓰림은 누가 책임져주지?",
          "original_form": "내 속쓰림은 누가 책임져주지?",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100042644.1.16",
          "form": "아 커피마시면 속 쓰릴 정도니",
          "original_form": "아 커피마시면 속 쓰릴 정도니",
          "speaker_id": "1",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100042644.1.17",
          "form": "디카페인 얼른 사자",
          "original_form": "디카페인 얼른 사자",
          "speaker_id": "1",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100042644.1.18",
          "form": "디카페인도 마시니까 좀 쓰리더라 왜그렇지?",
          "original_form": "디카페인도 마시니까 좀 쓰리더라 왜그렇지?",
          "speaker_id": "2",
          "time": "20210907 13:42"
        },
        {
          "id": "MDRW2100042644.1.19",
          "form": "소량은 있어서 그런가",
          "original_form": "소량은 있어서 그런가",
          "speaker_id": "1",
          "time": "20210907 13:43"
        },
        {
          "id": "MDRW2100042644.1.20",
          "form": "나도 전에 식도염일때 디카페인 마셔도 조금 증상 올라오긴했는데 내가 워낙 진하게 타마셔서 그랬던듯",
          "original_form": "나도 전에 식도염일때 디카페인 마셔도 조금 증상 올라오긴했는데 내가 워낙 진하게 타마셔서 그랬던듯",
          "speaker_id": "1",
          "time": "20210907 13:43"
        },
        {
          "id": "MDRW2100042644.1.21",
          "form": "커피를 아예 끊어야하나....",
          "original_form": "커피를 아예 끊어야하나....",
          "speaker_id": "2",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100042644.1.22",
          "form": "카페인 없는 차로 대체 해라 ㅋㅋㅋ",
          "original_form": "카페인 없는 차로 대체 해라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:04"
        }
      ]
