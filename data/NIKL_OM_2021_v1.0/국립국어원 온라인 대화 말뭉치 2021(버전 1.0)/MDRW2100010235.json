      "utterance": [
        {
          "id": "MDRW2100010235.1.1",
          "form": "요즘 머리가 엉망이야.",
          "original_form": "요즘 머리가 엉망이야.",
          "speaker_id": "2",
          "time": "20210823 15:55"
        },
        {
          "id": "MDRW2100010235.1.2",
          "form": "너는 머리카락 관리를 어떻게 하니?",
          "original_form": "너는 머리카락 관리를 어떻게 하니?",
          "speaker_id": "2",
          "time": "20210823 15:55"
        },
        {
          "id": "MDRW2100010235.1.3",
          "form": "어우 나도 그래",
          "original_form": "어우 나도 그래",
          "speaker_id": "1",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010235.1.4",
          "form": "나는 헤어식초를 썼었어. 근데 요새 머리가 넘 많이 빠진ㄷ",
          "original_form": "나는 헤어식초를 썼었어. 근데 요새 머리가 넘 많이 빠진ㄷ",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010235.1.5",
          "form": "그리고 머리가 너무 많이빠져",
          "original_form": "그리고 머리가 너무 많이빠져",
          "speaker_id": "1",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010235.1.6",
          "form": "헤어식초 써봤니",
          "original_form": "헤어식초 써봤니",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010235.1.7",
          "form": "향이 참 좋아.",
          "original_form": "향이 참 좋아.",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010235.1.8",
          "form": "이브로쉐 걸 썼었어",
          "original_form": "이브로쉐 걸 썼었어",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010235.1.9",
          "form": "응 나도 그거 냄새를 좋아해",
          "original_form": "응 나도 그거 냄새를 좋아해",
          "speaker_id": "1",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010235.1.10",
          "form": "다들 머리가 많이 빠지는구나 요즘",
          "original_form": "다들 머리가 많이 빠지는구나 요즘",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010235.1.11",
          "form": "너도 그거 아는구나",
          "original_form": "너도 그거 아는구나",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010235.1.12",
          "form": "그 제품 좋아하니?",
          "original_form": "그 제품 좋아하니?",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010235.1.13",
          "form": "응 좋아해 다들 이브로쉐를 쓰는 것 같아",
          "original_form": "응 좋아해 다들 이브로쉐를 쓰는 것 같아",
          "speaker_id": "1",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010235.1.14",
          "form": "나는 다써서 안쓴지 며칠됐어.",
          "original_form": "나는 다써서 안쓴지 며칠됐어.",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010235.1.15",
          "form": "맞아. 유명하지",
          "original_form": "맞아. 유명하지",
          "speaker_id": "2",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010235.1.16",
          "form": "나는 저번에 샘플을 받았어",
          "original_form": "나는 저번에 샘플을 받았어",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010235.1.17",
          "form": "한때 sns에서 대란이 있었어",
          "original_form": "한때 sns에서 대란이 있었어",
          "speaker_id": "2",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010235.1.18",
          "form": "다음에 또 이벤트하면 너두 알려줄께",
          "original_form": "다음에 또 이벤트하면 너두 알려줄께",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010235.1.19",
          "form": "오 샘플도 있구나",
          "original_form": "오 샘플도 있구나",
          "speaker_id": "2",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010235.1.20",
          "form": "나도 갖고싶다.",
          "original_form": "나도 갖고싶다.",
          "speaker_id": "2",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010235.1.21",
          "form": "응 고마워.",
          "original_form": "응 고마워.",
          "speaker_id": "2",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010235.1.22",
          "form": "배송비만 내고 받았었어",
          "original_form": "배송비만 내고 받았었어",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010235.1.23",
          "form": "나 지금 목에 담이왔어",
          "original_form": "나 지금 목에 담이왔어",
          "speaker_id": "1",
          "time": "20210823 17:15"
        }
      ]
