      "utterance": [
        {
          "id": "MDRW2100023354.1.1",
          "form": "응 꾸미기 좋게 나와서 그런가봐",
          "original_form": "응 꾸미기 좋게 나와서 그런가봐",
          "speaker_id": "2",
          "time": "20210830 01:20"
        },
        {
          "id": "MDRW2100023354.1.2",
          "form": "사전예약 끝나고나니 나한테 물어서 아쉬웠어",
          "original_form": "사전예약 끝나고나니 나한테 물어서 아쉬웠어",
          "speaker_id": "2",
          "time": "20210830 01:20"
        },
        {
          "id": "MDRW2100023354.1.3",
          "form": "신형폰 별로 관심없는데 이건 사고싶더라",
          "original_form": "신형폰 별로 관심없는데 이건 사고싶더라",
          "speaker_id": "1",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100023354.1.4",
          "form": "그치 나도 미니백처럼 꾸민건 맘에 들더라ㅎ",
          "original_form": "그치 나도 미니백처럼 꾸민건 맘에 들더라ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:24"
        },
        {
          "id": "MDRW2100023354.1.5",
          "form": "역시 이뻐야 인기있나봐 ㅋㅋ",
          "original_form": "역시 이뻐야 인기있나봐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100023354.1.6",
          "form": "맞어 디자인이 제일인듯",
          "original_form": "맞어 디자인이 제일인듯",
          "speaker_id": "2",
          "time": "20210830 01:27"
        },
        {
          "id": "MDRW2100023354.1.7",
          "form": "UI는 어느정도 능력치 있으니ㅎ",
          "original_form": "UI는 어느정도 능력치 있으니ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:28"
        },
        {
          "id": "MDRW2100023354.1.8",
          "form": "ㅇㅇ이제 스펙은 다 비슷해서",
          "original_form": "ㅇㅇ이제 스펙은 다 비슷해서",
          "speaker_id": "1",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100023354.1.9",
          "form": "그니께ㅎ\n손바닥만하고 꾸밀수 있어서 여심저격한듯",
          "original_form": "그니께ㅎ\n손바닥만하고 꾸밀수 있어서 여심저격한듯",
          "speaker_id": "2",
          "time": "20210830 02:15"
        },
        {
          "id": "MDRW2100023354.1.10",
          "form": "ㅇㅇ귀엽드라 근데 실용성 따지면 플립말고 폴더?가 나을수도",
          "original_form": "ㅇㅇ귀엽드라 근데 실용성 따지면 플립말고 폴더?가 나을수도",
          "speaker_id": "1",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100023354.1.11",
          "form": "폴더는 무게 좀 있어서 무게 감안하고 사야해ㅎ",
          "original_form": "폴더는 무게 좀 있어서 무게 감안하고 사야해ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:27"
        },
        {
          "id": "MDRW2100023354.1.12",
          "form": "아 손목 아프겠넹ㅠ",
          "original_form": "아 손목 아프겠넹ㅠ",
          "speaker_id": "1",
          "time": "20210830 02:35"
        },
        {
          "id": "MDRW2100023354.1.13",
          "form": "글고 이번꺼는 딱봐도 큼직하다 느낌",
          "original_form": "글고 이번꺼는 딱봐도 큼직하다 느낌",
          "speaker_id": "2",
          "time": "20210830 02:37"
        },
        {
          "id": "MDRW2100023354.1.14",
          "form": "뭔가 묵직하고 남자? 중년느낌 나는거같아\n내 생각",
          "original_form": "뭔가 묵직하고 남자? 중년느낌 나는거같아\n내 생각",
          "speaker_id": "2",
          "time": "20210830 02:37"
        },
        {
          "id": "MDRW2100023354.1.15",
          "form": "글쿠만 폴더블 이거 오타쿠들한테 인기래 ㅋㅋ 게임하기좋아서 만화보기도 좋고",
          "original_form": "글쿠만 폴더블 이거 오타쿠들한테 인기래 ㅋㅋ 게임하기좋아서 만화보기도 좋고",
          "speaker_id": "1",
          "time": "20210830 02:37"
        },
        {
          "id": "MDRW2100023354.1.16",
          "form": "화면이ㅎ중독성 인가보더라",
          "original_form": "화면이ㅎ중독성 인가보더라",
          "speaker_id": "2",
          "time": "20210830 02:38"
        },
        {
          "id": "MDRW2100023354.1.17",
          "form": "회사동생은 게임기 대용이래ㅎ",
          "original_form": "회사동생은 게임기 대용이래ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:38"
        },
        {
          "id": "MDRW2100023354.1.18",
          "form": "폴더블있음 패드 필요없겠어",
          "original_form": "폴더블있음 패드 필요없겠어",
          "speaker_id": "1",
          "time": "20210830 02:39"
        },
        {
          "id": "MDRW2100023354.1.19",
          "form": "그러게ㅎㅎ",
          "original_form": "그러게ㅎㅎ",
          "speaker_id": "2",
