      "utterance": [
        {
          "id": "MDRW2100007298.1.1",
          "form": "여행은 다녀오셨나요? 휴가에",
          "original_form": "여행은 다녀오셨나요? 휴가에",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007298.1.2",
          "form": "이번 여름엔 휴가 못갔어요",
          "original_form": "이번 여름엔 휴가 못갔어요",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007298.1.3",
          "form": "계속 너무 확진자도 많구..",
          "original_form": "계속 너무 확진자도 많구..",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007298.1.4",
          "form": "저도요 ㅜㅜ",
          "original_form": "저도요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007298.1.5",
          "form": "아무데도 못갔어요",
          "original_form": "아무데도 못갔어요",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007298.1.6",
          "form": "네ㅜㅜ 저 물놀이 진짜 좋아하는데",
          "original_form": "네ㅜㅜ 저 물놀이 진짜 좋아하는데",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007298.1.7",
          "form": "저는 가고싶어도",
          "original_form": "저는 가고싶어도",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007298.1.8",
          "form": "아무도 같이 안가려고해서 ㅜㅜ",
          "original_form": "아무도 같이 안가려고해서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007298.1.9",
          "form": "빠지 예약한 것도 다 취소했어요..",
          "original_form": "빠지 예약한 것도 다 취소했어요..",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007298.1.10",
          "form": "ㅜㅜ안가죠",
          "original_form": "ㅜㅜ안가죠",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007298.1.11",
          "form": "저 제주도 ㅜㅜ취소했어요",
          "original_form": "저 제주도 ㅜㅜ취소했어요",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007298.1.12",
          "form": "제주도도 확진자들 엄청 갔는지 난리났더라구요..",
          "original_form": "제주도도 확진자들 엄청 갔는지 난리났더라구요..",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007298.1.13",
          "form": "너무 아쉬워요 ㅜㅜ",
          "original_form": "너무 아쉬워요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007298.1.14",
          "form": "그렇죠 조개좀 잡고 놀라고 했는데 ㅜㅜ",
          "original_form": "그렇죠 조개좀 잡고 놀라고 했는데 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007298.1.15",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007298.1.16",
          "form": "그 선녀탕? 거기도 넘 가보고 싶어요",
          "original_form": "그 선녀탕? 거기도 넘 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007298.1.17",
          "form": "ㅋㅋ이름돞생각 안나네요",
          "original_form": "ㅋㅋ이름돞생각 안나네요",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007298.1.18",
          "form": "전 작년엔 사람 없는 곳만 들렸어요 이름없는",
          "original_form": "전 작년엔 사람 없는 곳만 들렸어요 이름없는",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007298.1.19",
          "form": "애월리 커피숍 아들 엄마가 같이 하는데 아이가ㅠ장애가 있는데 엄마가 가르치시는데 미래를 준비해 주시는것 같았어요",
          "original_form": "애월리 커피숍 아들 엄마가 같이 하는데 아이가ㅠ장애가 있는데 엄마가 가르치시는데 미래를 준비해 주시는것 같았어요",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007298.1.20",
          "form": "그게 기억나네요",
          "original_form": "그게 기억나네요",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007298.1.21",
          "form": "장소보다 사람이 기억나요",
          "original_form": "장소보다 사람이 기억나요",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007298.1.22",
          "form": "그럼 너무 가슴아플 것 같아요",
          "original_form": "그럼 너무 가슴아플 것 같아요",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007298.1.23",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007298.1.24",
          "form": "전망도 참좋고 인심도좋았어요",
          "original_form": "전망도 참좋고 인심도좋았어요",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007298.1.25",
          "form": "제주도는 어딜가도 좋아요",
          "original_form": "제주도는 어딜가도 좋아요",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007298.1.26",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007298.1.27",
          "form": "발길 닿는데로",
          "original_form": "발길 닿는데로",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007298.1.28",
          "form": "여행은 다 사연이 생기고 이야기가 생기고 ㅋㅋ",
          "original_form": "여행은 다 사연이 생기고 이야기가 생기고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007298.1.29",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007298.1.30",
          "form": "저도 올초에 다녀왔는데",
          "original_form": "저도 올초에 다녀왔는데",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007298.1.31",
          "form": "이번에 맛집 선정 실패해서",
          "original_form": "이번에 맛집 선정 실패해서",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007298.1.32",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007298.1.33",
          "form": "아쉬웠습니다 ㅜㅜ",
          "original_form": "아쉬웠습니다 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007298.1.34",
          "form": "맛집이 사실 잘모르겠어요",
          "original_form": "맛집이 사실 잘모르겠어요",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007298.1.35",
          "form": "성산일출봉 일출 보느라고 그 주변에서 숙박했는데",
          "original_form": "성산일출봉 일출 보느라고 그 주변에서 숙박했는데",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007298.1.36",
          "form": "전 그냥 아무데나ㅠ가요",
          "original_form": "전 그냥 아무데나ㅠ가요",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007298.1.37",
          "form": "그 주위에 정말 먹을만한 데 없저라구요",
          "original_form": "그 주위에 정말 먹을만한 데 없저라구요",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007298.1.38",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007298.1.39",
          "form": "인터넷엔 다 광고뿐이어서 ㅜㅜ",
          "original_form": "인터넷엔 다 광고뿐이어서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007298.1.40",
          "form": "돼지 샤브샤브 맛있던데요",
          "original_form": "돼지 샤브샤브 맛있던데요",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007298.1.41",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007298.1.42",
          "form": "저는 갈치조림 먹었다가 완전 실패.",
