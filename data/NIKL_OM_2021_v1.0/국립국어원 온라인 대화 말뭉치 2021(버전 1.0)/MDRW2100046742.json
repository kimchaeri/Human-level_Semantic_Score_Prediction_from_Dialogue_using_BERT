      "utterance": [
        {
          "id": "MDRW2100046742.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 13:05"
        },
        {
          "id": "MDRW2100046742.1.2",
          "form": "좋아하는 가수 있으신가요?",
          "original_form": "좋아하는 가수 있으신가요?",
          "speaker_id": "2",
          "time": "20211104 14:09"
        },
        {
          "id": "MDRW2100046742.1.3",
          "form": "아이유요",
          "original_form": "아이유요",
          "speaker_id": "1",
          "time": "20211104 14:10"
        },
        {
          "id": "MDRW2100046742.1.4",
          "form": "저도 아이유님 너무 좋아해요",
          "original_form": "저도 아이유님 너무 좋아해요",
          "speaker_id": "2",
          "time": "20211104 14:12"
        },
        {
          "id": "MDRW2100046742.1.5",
          "form": "이번에 나온 신곡 들어보셨어요?",
          "original_form": "이번에 나온 신곡 들어보셨어요?",
          "speaker_id": "2",
          "time": "20211104 14:12"
        },
        {
          "id": "MDRW2100046742.1.6",
          "form": "네들어봤어요",
          "original_form": "네들어봤어요",
          "speaker_id": "1",
          "time": "20211104 14:12"
        },
        {
          "id": "MDRW2100046742.1.7",
          "form": "완전 좋더라구요!",
          "original_form": "완전 좋더라구요!",
          "speaker_id": "2",
          "time": "20211104 14:13"
        },
        {
          "id": "MDRW2100046742.1.8",
          "form": "봄에 나온 라일락 도 진짜 좋아했는데 이번 노래도 좋아요 ㅎㅎ",
          "original_form": "봄에 나온 라일락 도 진짜 좋아했는데 이번 노래도 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 14:13"
        },
        {
          "id": "MDRW2100046742.1.9",
          "form": "ㄴㅔ라일락도진짜좋아요",
          "original_form": "ㄴㅔ라일락도진짜좋아요",
          "speaker_id": "1",
          "time": "20211104 14:13"
        },
        {
          "id": "MDRW2100046742.1.10",
          "form": "혹시 그것도 보셨어요?",
          "original_form": "혹시 그것도 보셨어요?",
          "speaker_id": "2",
          "time": "20211104 14:14"
        },
        {
          "id": "MDRW2100046742.1.11",
          "form": "아이유 딩고 라이브!",
          "original_form": "아이유 딩고 라이브!",
          "speaker_id": "2",
          "time": "20211104 14:14"
        },
        {
          "id": "MDRW2100046742.1.12",
          "form": "아뇨못봤어요",
          "original_form": "아뇨못봤어요",
          "speaker_id": "1",
          "time": "20211104 14:14"
        },
        {
          "id": "MDRW2100046742.1.13",
          "form": "유튜브에 있는데 아이유님이 20살부터 낸 노래부터 라일락까지 쭉 부르거든요",
          "original_form": "유튜브에 있는데 아이유님이 20살부터 낸 노래부터 라일락까지 쭉 부르거든요",
          "speaker_id": "2",
          "time": "20211104 14:14"
        },
        {
          "id": "MDRW2100046742.1.14",
          "form": "진짜 좋아요 ㅎㅎ 한번 봐보세요",
          "original_form": "진짜 좋아요 ㅎㅎ 한번 봐보세요",
          "speaker_id": "2",
          "time": "20211104 14:14"
        },
        {
          "id": "MDRW2100046742.1.15",
          "form": "네봐야겠어요ㅋ",
          "original_form": "네봐야겠어요ㅋ",
          "speaker_id": "1",
          "time": "20211104 14:15"
        },
        {
          "id": "MDRW2100046742.1.16",
          "form": "아이유님 무슨 노래가 최애세요?",
          "original_form": "아이유님 무슨 노래가 최애세요?",
          "speaker_id": "2",
          "time": "20211104 14:16"
        },
        {
          "id": "MDRW2100046742.1.17",
          "form": "하루끝이랑 블루밍",
          "original_form": "하루끝이랑 블루밍",
          "speaker_id": "1",
          "time": "20211104 14:16"
        },
        {
          "id": "MDRW2100046742.1.18",
          "form": "지금떠오르는거 이거네요",
          "original_form": "지금떠오르는거 이거네요",
