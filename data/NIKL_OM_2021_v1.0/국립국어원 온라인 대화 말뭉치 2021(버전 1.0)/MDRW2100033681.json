      "utterance": [
        {
          "id": "MDRW2100033681.1.1",
          "form": "name2님 안녕하세요",
          "original_form": "&name2&님 안녕하세요",
          "speaker_id": "1",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100033681.1.2",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100033681.1.3",
          "form": "네 좋아하죠ㅎㅎ",
          "original_form": "네 좋아하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100033681.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100033681.1.5",
          "form": "저도 여행 좋아해요ㅎㅎ",
          "original_form": "저도 여행 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100033681.1.6",
          "form": "최근에 여행 다녀오신 곳은 어디신가요?",
          "original_form": "최근에 여행 다녀오신 곳은 어디신가요?",
          "speaker_id": "1",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100033681.1.7",
          "form": "코로나땜에 아무데도 못가고있어요ㅠㅠ",
          "original_form": "코로나땜에 아무데도 못가고있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100033681.1.8",
          "form": "재작년이 마지막이네요",
          "original_form": "재작년이 마지막이네요",
          "speaker_id": "2",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100033681.1.9",
          "form": "아 아무래도 그렇죠ㅠ",
          "original_form": "아 아무래도 그렇죠ㅠ",
          "speaker_id": "1",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100033681.1.10",
          "form": "어디 여행다녀오셨어요?",
          "original_form": "어디 여행다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100033681.1.11",
          "form": "저는 작년 2월에 라오스 다녀왔는데",
          "original_form": "저는 작년 2월에 라오스 다녀왔는데",
          "speaker_id": "1",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100033681.1.12",
          "form": "정말 코로나 심해지기 직전이라",
          "original_form": "정말 코로나 심해지기 직전이라",
          "speaker_id": "1",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100033681.1.13",
          "form": "(당시엔 예상하지 못했지만)",
          "original_form": "(당시엔 예상하지 못했지만)",
          "speaker_id": "1",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100033681.1.14",
          "form": "딱 터지기 전이네요ㅎㅎ",
          "original_form": "딱 터지기 전이네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100033681.1.15",
          "form": "라오스 좋은가요?",
          "original_form": "라오스 좋은가요?",
          "speaker_id": "2",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100033681.1.16",
          "form": "그 방비엥? 거기가 라오스죠?",
          "original_form": "그 방비엥? 거기가 라오스죠?",
          "speaker_id": "2",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100033681.1.17",
          "form": "네 겨울에 갔는데 라오스는 따뜻하고 물놀이하기도 좋아서",
          "original_form": "네 겨울에 갔는데 라오스는 따뜻하고 물놀이하기도 좋아서",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100033681.1.18",
          "form": "맞아요 방비엥 루앙프라방",
          "original_form": "맞아요 방비엥 루앙프라방",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100033681.1.19",
          "form": "가보곤 싶은데 물가도 싸다그러고",
          "original_form": "가보곤 싶은데 물가도 싸다그러고",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100033681.1.20",
          "form": "이젠 기약이없네요",
          "original_form": "이젠 기약이없네요",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100033681.1.21",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100033681.1.22",
          "form": "당분간 해외여행은 힘들겠죠?",
          "original_form": "당분간 해외여행은 힘들겠죠?",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100033681.1.23",
          "form": "마지막여행이 다낭이였는데",
          "original_form": "마지막여행이 다낭이였는데",
          "speaker_id": "2",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100033681.1.24",
          "form": "오 다낭! 다낭도 좋다고 들었는데",
          "original_form": "오 다낭! 다낭도 좋다고 들었는데",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100033681.1.25",
          "form": "이젠 힘들지싶어요",
          "original_form": "이젠 힘들지싶어요",
          "speaker_id": "2",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100033681.1.26",
          "form": "저는 다낭은 못가봤어요",
          "original_form": "저는 다낭은 못가봤어요",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100033681.1.27",
          "form": "다낭보다 전 캄보디아가 더좋더라구요",
          "original_form": "다낭보다 전 캄보디아가 더좋더라구요",
          "speaker_id": "2",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100033681.1.28",
          "form": "다낭은 패키지로가서 그런가ㅎ",
          "original_form": "다낭은 패키지로가서 그런가ㅎ",
          "speaker_id": "2",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100033681.1.29",
          "form": "아 패키지 여행 다녀오셨군요",
          "original_form": "아 패키지 여행 다녀오셨군요",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100033681.1.30",
          "form": "캄보디아는 자유여행으로 다녀오셨나요?",
          "original_form": "캄보디아는 자유여행으로 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100033681.1.31",
          "form": "네 현지가이드 붙여서 친구랑둘이요",
          "original_form": "네 현지가이드 붙여서 친구랑둘이요",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100033681.1.32",
          "form": "편하고 넘좋더라구요",
          "original_form": "편하고 넘좋더라구요",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100033681.1.33",
          "form": "한국말도잘하고",
          "original_form": "한국말도잘하고",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100033681.1.34",
          "form": "1일1마사지도 하고",
          "original_form": "1일1마사지도 하고",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100033681.1.35",
          "form": "와 좋으셨겠어요",
          "original_form": "와 좋으셨겠어요",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100033681.1.36",
          "form": "동남아에선 마사지 받아줘야죠!",
          "original_form": "동남아에선 마사지 받아줘야죠!",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100033681.1.37",
          "form": "진짜 앙코르와트 일출은 꼭보고오세요",
          "original_form": "진짜 앙코르와트 일출은 꼭보고오세요",
          "speaker_id": "2",
          "time": "20210824 22:55"
        }
      ]
    }
