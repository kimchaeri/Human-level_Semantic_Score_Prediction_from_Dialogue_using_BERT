        },
        {
          "id": "MDRW2100029052.1.2",
          "form": "어디가저렴할까요",
          "original_form": "어디가저렴할까요",
          "speaker_id": "1",
          "time": "20210903 14:56"
        },
        {
          "id": "MDRW2100029052.1.3",
          "form": "순둥이 물티슈 좋아요 쓱이요",
          "original_form": "순둥이 물티슈 좋아요 쓱이요",
          "speaker_id": "2",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100029052.1.4",
          "form": "음 더 싼거..",
          "original_form": "음 더 싼거..",
          "speaker_id": "1",
          "time": "20210903 15:08"
        },
        {
          "id": "MDRW2100029052.1.5",
          "form": "마트가면 100매에 990원 있어요",
          "original_form": "마트가면 100매에 990원 있어요",
          "speaker_id": "2",
          "time": "20210903 15:08"
        },
        {
          "id": "MDRW2100029052.1.6",
          "form": "네 음 인터넷이쌀거같은데요",
