        {
          "id": "MDRW2100042611.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210906 23:38"
        },
        {
          "id": "MDRW2100042611.1.2",
          "form": "물건 많이 사시나요?",
          "original_form": "물건 많이 사시나요?",
          "speaker_id": "2",
          "time": "20210907 10:50"
        },
        {
          "id": "MDRW2100042611.1.3",
          "form": "네 저는주로인터넷에서 마니사용",
          "original_form": "네 저는주로인터넷에서 마니사용",
          "speaker_id": "1",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042611.1.4",
          "form": "저도요 오프라인은 거의 안가는것같아요",
          "original_form": "저도요 오프라인은 거의 안가는것같아요",
          "speaker_id": "2",
          "time": "20210907 10:57"
        },
        {
          "id": "MDRW2100042611.1.5",
          "form": "온라인이 편해요",
          "original_form": "온라인이 편해요",
          "speaker_id": "2",
          "time": "20210907 11:19"
        },
        {
          "id": "MDRW2100042611.1.6",
          "form": "집앞배송ㅋㅋ좋죠",
          "original_form": "집앞배송ㅋㅋ좋죠",
          "speaker_id": "1",
          "time": "20210907 11:32"
        },
        {
          "id": "MDRW2100042611.1.7",
          "form": "받고 싶은 선물 있으세요?",
          "original_form": "받고 싶은 선물 있으세요?",
          "speaker_id": "2",
          "time": "20210907 15:02"
        },
        {
          "id": "MDRW2100042611.1.8",
          "form": "저 플립핸드폰이여ㅠㅠ",
          "original_form": "저 플립핸드폰이여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100042611.1.9",
          "form": "앗 저도ㅎㅎ",
          "original_form": "앗 저도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100042611.1.10",
          "form": "근데 비싸기도 하지만 불량 얘기도 많아서ㅠ",
          "original_form": "근데 비싸기도 하지만 불량 얘기도 많아서ㅠ",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100042611.1.11",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100042611.1.12",
          "form": "이론ㅠㅠ",
          "original_form": "이론ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100042611.1.13",
          "form": "흰지부분이 좀 그렇다고하더라구요",
          "original_form": "흰지부분이 좀 그렇다고하더라구요",
          "speaker_id": "1",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100042611.1.14",
          "form": "아무래도 접히다보니 ㅠㅠ",
          "original_form": "아무래도 접히다보니 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100042611.1.15",
          "form": "갖고싶네여ㅠㅠㅠㅠ",
          "original_form": "갖고싶네여ㅠㅠㅠㅠ",
