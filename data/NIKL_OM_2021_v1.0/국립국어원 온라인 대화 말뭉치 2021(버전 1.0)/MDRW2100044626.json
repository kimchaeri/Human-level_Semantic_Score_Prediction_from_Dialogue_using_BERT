      "utterance": [
        {
          "id": "MDRW2100044626.1.1",
          "form": "집안일중에 하기 싫으신게 있으신가요?",
          "original_form": "집안일중에 하기 싫으신게 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044626.1.2",
          "form": "저 진짜 싫은거 하나 있어요!!",
          "original_form": "저 진짜 싫은거 하나 있어요!!",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044626.1.3",
          "form": "화장실 청소...",
          "original_form": "화장실 청소...",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044626.1.4",
          "form": "화장실 청소는 정말... 힘들고 더러워요...",
          "original_form": "화장실 청소는 정말... 힘들고 더러워요...",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044626.1.5",
          "form": "아... 그렇죠",
          "original_form": "아... 그렇죠",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044626.1.6",
          "form": "사실 화장실청소가 제일 힘들어요 ㅠ",
          "original_form": "사실 화장실청소가 제일 힘들어요 ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044626.1.7",
          "form": "그쵸! 진짜 빨리 더러워지고 창문 없으면 그냥 최 악 이에요",
          "original_form": "그쵸! 진짜 빨리 더러워지고 창문 없으면 그냥 최 악 이에요",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044626.1.8",
          "form": "그거 말고는 뭐 그냥 대충대충 해요 ㅋㅋㅋ",
          "original_form": "그거 말고는 뭐 그냥 대충대충 해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044626.1.9",
          "form": "화장실은 대충 할수없어서 힘들어요",
          "original_form": "화장실은 대충 할수없어서 힘들어요",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044626.1.10",
          "form": "전 청소에 소질이 없어서 집에서는 설겆이당담이에요ㅋㅋㅋㅋ",
          "original_form": "전 청소에 소질이 없어서 집에서는 설겆이당담이에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044626.1.11",
          "form": "저는 주로 음식하면 설거지는 언니가 하는데 ㅋㅋㅋ",
          "original_form": "저는 주로 음식하면 설거지는 언니가 하는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:01"
        },
        {
          "id": "MDRW2100044626.1.12",
          "form": "그래서 음식할때 그릇 막써요 그냥 ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 음식할때 그릇 막써요 그냥 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:01"
        },
        {
          "id": "MDRW2100044626.1.13",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100044626.1.14",
          "form": "그건 제동생과 다르셔요ㅋㅋㅋㅋ",
          "original_form": "그건 제동생과 다르셔요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100044626.1.15",
          "form": "제 동생은 오히려 그릇못꺼내게해요ㅡㅡ",
          "original_form": "제 동생은 오히려 그릇못꺼내게해요ㅡㅡ",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100044626.1.16",
          "form": "플레이팅 중요한대요...",
          "original_form": "플레이팅 중요한대요...",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100044626.1.17",
          "form": "ㅋㅋㅋㅋㅋ 어차피 설거지는 언니가 하기 때문에...",
          "original_form": "ㅋㅋㅋㅋㅋ 어차피 설거지는 언니가 하기 때문에...",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100044626.1.18",
          "form": "ㄴ저는 언니가 못꺼내게 해요 ㅋㅋㅋㅋ",
          "original_form": "ㄴ저는 언니가 못꺼내게 해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100044626.1.19",
          "form": "하지만 음식할때 방에 누워있는자는 자격이 없죠!",
          "original_form": "하지만 음식할때 방에 누워있는자는 자격이 없죠!",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100044626.1.20",
          "form": "그쵸ㅋㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100044626.1.21",
          "form": "입으로만 하지말라는건 의미가 없죠ㅋㅋㅋㅋ",
          "original_form": "입으로만 하지말라는건 의미가 없죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100044626.1.22",
          "form": "ㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100044626.1.23",
          "form": "언니는 맨날 일어나면 저한테",
          "original_form": "언니는 맨날 일어나면 저한테",
          "speaker_id": "1",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100044626.1.24",
          "form": "ㅇㅇ아... 아침 뭐먹어? 이래요 ㅋㅋㅋ",
          "original_form": "ㅇㅇ아... 아침 뭐먹어? 이래요 ㅋㅋㅋ",
