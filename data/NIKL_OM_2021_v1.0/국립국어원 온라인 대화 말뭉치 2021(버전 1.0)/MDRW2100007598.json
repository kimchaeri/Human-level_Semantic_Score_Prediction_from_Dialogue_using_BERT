      "utterance": [
        {
          "id": "MDRW2100007598.1.1",
          "form": "여행가고싶은곳 있어?",
          "original_form": "여행가고싶은곳 있어?",
          "speaker_id": "1",
          "time": "20210822 03:55"
        },
        {
          "id": "MDRW2100007598.1.2",
          "form": "그리스에 가고 싶어.",
          "original_form": "그리스에 가고 싶어.",
          "speaker_id": "2",
          "time": "20210822 03:56"
        },
        {
          "id": "MDRW2100007598.1.3",
          "form": "오 그리스 어디",
          "original_form": "오 그리스 어디",
          "speaker_id": "1",
          "time": "20210822 03:56"
        },
        {
          "id": "MDRW2100007598.1.4",
          "form": "파르테노 신전인가? 거기!",
          "original_form": "파르테노 신전인가? 거기!",
          "speaker_id": "2",
          "time": "20210822 03:57"
        },
        {
          "id": "MDRW2100007598.1.5",
          "form": "파르테논신전 좋지",
          "original_form": "파르테논신전 좋지",
          "speaker_id": "1",
          "time": "20210822 03:57"
        },
        {
          "id": "MDRW2100007598.1.6",
          "form": "사진으로만 봤는데 짱 멋졌어",
          "original_form": "사진으로만 봤는데 짱 멋졌어",
          "speaker_id": "2",
          "time": "20210822 03:57"
        },
        {
          "id": "MDRW2100007598.1.7",
          "form": "그럼 너는 어디 여행하고 싶은 곳 있어?",
          "original_form": "그럼 너는 어디 여행하고 싶은 곳 있어?",
          "speaker_id": "2",
          "time": "20210822 03:57"
        },
        {
          "id": "MDRW2100007598.1.8",
          "form": "나는 그리스 간다면 아테네 가보고싶어. 따로 다른나라를 정한다면..",
          "original_form": "나는 그리스 간다면 아테네 가보고싶어. 따로 다른나라를 정한다면..",
          "speaker_id": "1",
          "time": "20210822 03:57"
        },
        {
          "id": "MDRW2100007598.1.9",
          "form": "사실 세계여행전부하고싶긴한데..",
          "original_form": "사실 세계여행전부하고싶긴한데..",
          "speaker_id": "1",
          "time": "20210822 03:57"
        },
        {
          "id": "MDRW2100007598.1.10",
          "form": "굳이...요즘 끌리는데 꼽자면 몽골? 한번쯤 자연 느껴보고싶다",
          "original_form": "굳이...요즘 끌리는데 꼽자면 몽골? 한번쯤 자연 느껴보고싶다",
          "speaker_id": "1",
          "time": "20210822 03:58"
        },
        {
          "id": "MDRW2100007598.1.11",
          "form": "헐 몽골 나도 가보고 싶다",
          "original_form": "헐 몽골 나도 가보고 싶다",
          "speaker_id": "2",
          "time": "20210822 03:58"
        },
        {
          "id": "MDRW2100007598.1.12",
          "form": "천막 게르 맞나?",
          "original_form": "천막 게르 맞나?",
          "speaker_id": "2",
          "time": "20210822 03:59"
        },
        {
          "id": "MDRW2100007598.1.13",
          "form": "어 맞아맞아",
          "original_form": "어 맞아맞아",
          "speaker_id": "1",
          "time": "20210822 03:59"
        },
        {
          "id": "MDRW2100007598.1.14",
          "form": "거기서 지내는거 재밌을거 같아",
          "original_form": "거기서 지내는거 재밌을거 같아",
          "speaker_id": "2",
          "time": "20210822 03:59"
        },
        {
          "id": "MDRW2100007598.1.15",
          "form": "진짜 초원만 있고 게르에서 자고 텐트도 치고 자고",
          "original_form": "진짜 초원만 있고 게르에서 자고 텐트도 치고 자고",
          "speaker_id": "1",
          "time": "20210822 03:59"
        },
        {
          "id": "MDRW2100007598.1.16",
          "form": "차타고 초원구경하다가.. 화장실은 걍 초원길바닥에 해결하고 다시 차타고 구경하다 양떼보고 게르가서 자고 그런대",
          "original_form": "차타고 초원구경하다가.. 화장실은 걍 초원길바닥에 해결하고 다시 차타고 구경하다 양떼보고 게르가서 자고 그런대",
          "speaker_id": "1",
          "time": "20210822 03:59"
        },
        {
          "id": "MDRW2100007598.1.17",
          "form": "유목민이랑 바디랭귀지로 소통하고 ㅋㅋ",
          "original_form": "유목민이랑 바디랭귀지로 소통하고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:59"
        },
        {
          "id": "MDRW2100007598.1.18",
          "form": "어어ㅋㅋ약간그런거",
          "original_form": "어어ㅋㅋ약간그런거",
          "speaker_id": "1",
          "time": "20210822 03:59"
        },
        {
          "id": "MDRW2100007598.1.19",
          "form": "양떼 끌린다",
          "original_form": "양떼 끌린다",
          "speaker_id": "2",
          "time": "20210822 04:00"
        },
        {
          "id": "MDRW2100007598.1.20",
          "form": "말도 타고",
          "original_form": "말도 타고",
          "speaker_id": "1",
          "time": "20210822 04:00"
        },
        {
          "id": "MDRW2100007598.1.21",
          "form": "헐 말도 끌린다 다그닥다그닥",
          "original_form": "헐 말도 끌린다 다그닥다그닥",
          "speaker_id": "2",
          "time": "20210822 04:00"
        },
        {
          "id": "MDRW2100007598.1.22",
          "form": "초원을 말 타구 달림 완전 좋겠다...",
          "original_form": "초원을 말 타구 달림 완전 좋겠다...",
          "speaker_id": "2",
          "time": "20210822 04:00"
        },
        {
          "id": "MDRW2100007598.1.23",
          "form": "좋을것같았어 네이버에 몽골 여행한 만화 일기가 있는데 그거보니까 너무 끌리더라",
          "original_form": "좋을것같았어 네이버에 몽골 여행한 만화 일기가 있는데 그거보니까 너무 끌리더라",
          "speaker_id": "1",
          "time": "20210822 04:00"
        },
        {
          "id": "MDRW2100007598.1.24",
          "form": "달리는건 아무래도..못하지",
          "original_form": "달리는건 아무래도..못하지",
          "speaker_id": "1",
          "time": "20210822 04:00"
        },
        {
          "id": "MDRW2100007598.1.25",
          "form": "힝ㅜㅜ",
          "original_form": "힝ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 04:00"
        },
        {
          "id": "MDRW2100007598.1.26",
          "form": "이시극만 끝나면 꼭 가보좌",
          "original_form": "이시극만 끝나면 꼭 가보좌",
          "speaker_id": "2",
          "time": "20210822 04:01"
        },
        {
          "id": "MDRW2100007598.1.27",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "1",
          "time": "20210822 04:01"
        }
      ]
    }
