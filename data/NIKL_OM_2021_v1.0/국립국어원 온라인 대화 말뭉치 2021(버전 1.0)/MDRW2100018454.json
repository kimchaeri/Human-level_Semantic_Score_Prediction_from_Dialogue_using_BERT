      "utterance": [
        {
          "id": "MDRW2100018454.1.1",
          "form": "안녕하세요 요즘 갤럭시 워치 핫하던데 혹시 관심 있으신가요ㅋㅋ",
          "original_form": "안녕하세요 요즘 갤럭시 워치 핫하던데 혹시 관심 있으신가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:12"
        },
        {
          "id": "MDRW2100018454.1.2",
          "form": "앗 네넼ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗 네넼ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:15"
        },
        {
          "id": "MDRW2100018454.1.3",
          "form": "헐 저도요ㅋㅋㅋㅋㅋ",
          "original_form": "헐 저도요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:15"
        },
        {
          "id": "MDRW2100018454.1.4",
          "form": "이번 4 체험단 모집도 빨리 마감되곸ㅋㅋㅋ",
          "original_form": "이번 4 체험단 모집도 빨리 마감되곸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:15"
        },
        {
          "id": "MDRW2100018454.1.5",
          "form": "폰은 관심 없으세요?!",
          "original_form": "폰은 관심 없으세요?!",
          "speaker_id": "2",
          "time": "20210827 02:16"
        },
        {
          "id": "MDRW2100018454.1.6",
          "form": "저는 그 크림색 플립이 너무 이쁘더라고요",
          "original_form": "저는 그 크림색 플립이 너무 이쁘더라고요",
          "speaker_id": "2",
          "time": "20210827 02:16"
        },
        {
          "id": "MDRW2100018454.1.7",
          "form": "찾았다 갤럭시 찐팬!",
          "original_form": "찾았다 갤럭시 찐팬!",
          "speaker_id": "1",
          "time": "20210827 02:16"
        },
        {
          "id": "MDRW2100018454.1.8",
          "form": "저도 딱 그 플립 이뻐서 마음에 들여놨어요ㅠ",
          "original_form": "저도 딱 그 플립 이뻐서 마음에 들여놨어요ㅠ",
          "speaker_id": "1",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018454.1.9",
          "form": "무슨색이요?!",
          "original_form": "무슨색이요?!",
          "speaker_id": "2",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018454.1.10",
          "form": "크림색 실물 궁금해요",
          "original_form": "크림색 실물 궁금해요",
          "speaker_id": "1",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018454.1.11",
          "form": "저는 실물로 봤더니",
          "original_form": "저는 실물로 봤더니",
          "speaker_id": "2",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018454.1.12",
          "form": "크림색! 그 아이보리느낌의 색감이 따악~~",
          "original_form": "크림색! 그 아이보리느낌의 색감이 따악~~",
          "speaker_id": "1",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018454.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018454.1.14",
          "form": "하 라벤더랑 그린도..너무 이쁘더라고요",
          "original_form": "하 라벤더랑 그린도..너무 이쁘더라고요",
          "speaker_id": "2",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018454.1.15",
          "form": "이번에 잘 뽑았다..",
          "original_form": "이번에 잘 뽑았다..",
          "speaker_id": "2",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018454.1.16",
          "form": "ㅋㅋㅋㅋ오 이번에 색감 잘 뽑았나봐요",
          "original_form": "ㅋㅋㅋㅋ오 이번에 색감 잘 뽑았나봐요",
          "speaker_id": "1",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018454.1.17",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018454.1.18",
          "form": "라벤더랑 크림 다 이쁘던데",
