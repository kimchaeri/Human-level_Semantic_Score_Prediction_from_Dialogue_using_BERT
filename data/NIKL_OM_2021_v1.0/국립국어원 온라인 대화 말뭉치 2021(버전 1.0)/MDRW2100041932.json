      "utterance": [
        {
          "id": "MDRW2100041932.1.1",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041932.1.2",
          "form": "언니 오늘 한글 성적표? 봤나 ㅋㅋ",
          "original_form": "언니 오늘 한글 성적표? 봤나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041932.1.3",
          "form": "어 오늘 날라왔대",
          "original_form": "어 오늘 날라왔대",
          "speaker_id": "2",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041932.1.4",
          "form": "가르친 보람 있더나??",
          "original_form": "가르친 보람 있더나??",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041932.1.5",
          "form": "엉엌 ㅋㅋ 좋네",
          "original_form": "엉엌 ㅋㅋ 좋네",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041932.1.6",
          "form": "도달 ㅋㅋ",
          "original_form": "도달 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041932.1.7",
          "form": "좋다이 굿굿",
          "original_form": "좋다이 굿굿",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041932.1.8",
          "form": "근데 거의 도달이제?",
          "original_form": "근데 거의 도달이제?",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041932.1.9",
          "form": "잘하고 있다",
          "original_form": "잘하고 있다",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041932.1.10",
          "form": "그런듯 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그런듯 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041932.1.11",
          "form": "맞어 다 도달일듯 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞어 다 도달일듯 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041932.1.12",
          "form": "ㅋㅋㄱㅋ 그래도 잘 받아오면 좋지",
          "original_form": "ㅋㅋㄱㅋ 그래도 잘 받아오면 좋지",
          "speaker_id": "2",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041932.1.13",
          "form": "고생했다 ㅎㅎ",
          "original_form": "고생했다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041932.1.14",
          "form": "웅웅 ㅋㅋ 자기 오늘 잘햇따고",
          "original_form": "웅웅 ㅋㅋ 자기 오늘 잘햇따고",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041932.1.15",
          "form": "상으로 폰을 보여달란다",
          "original_form": "상으로 폰을 보여달란다",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041932.1.16",
          "form": "언니 나 이제 진짜 밥하러 가야되겠다.ㅋㅋ 나중에 톡할게",
          "original_form": "언니 나 이제 진짜 밥하러 가야되겠다.ㅋㅋ 나중에 톡할게",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041932.1.17",
          "form": "재우고 있나??",
          "original_form": "재우고 있나??",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100041932.1.18",
          "form": "이제 책읽어주고 재우려고",
          "original_form": "이제 책읽어주고 재우려고",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100041932.1.19",
          "form": "한권은 신랑이 지금 읽어주고있다ㅋ",
          "original_form": "한권은 신랑이 지금 읽어주고있다ㅋ",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100041932.1.20",
          "form": "좋구만",
          "original_form": "좋구만",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100041932.1.21",
          "form": "어여 육 퇴해라",
          "original_form": "어여 육 퇴해라",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100041932.1.22",
          "form": "난 신랑이 재우러 들어갔다",
          "original_form": "난 신랑이 재우러 들어갔다",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100041932.1.23",
          "form": "좋다잉ㅋ",
          "original_form": "좋다잉ㅋ",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100041932.1.24",
          "form": "나두 이제 들어가야겠다ㅋ",
          "original_form": "나두 이제 들어가야겠다ㅋ",
          "speaker_id": "1",
          "time": "20210906 21:35"
        }
      ]
