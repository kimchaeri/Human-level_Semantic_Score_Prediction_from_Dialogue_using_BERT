      "utterance": [
        {
          "id": "MDRW2100032289.1.1",
          "form": "조미료 자주 쓰는편?",
          "original_form": "조미료 자주 쓰는편?",
          "speaker_id": "2",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100032289.1.2",
          "form": "나는 요리 잘 안좋아하니까 자주 쓰진않은데",
          "original_form": "나는 요리 잘 안좋아하니까 자주 쓰진않은데",
          "speaker_id": "1",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100032289.1.3",
          "form": "조미료맛좋아하지",
          "original_form": "조미료맛좋아하지",
          "speaker_id": "1",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100032289.1.4",
          "form": "나는 다시다 미원 혼다시 안가리고 다써",
          "original_form": "나는 다시다 미원 혼다시 안가리고 다써",
          "speaker_id": "2",
          "time": "20210905 15:23"
        },
        {
          "id": "MDRW2100032289.1.5",
          "form": "다시다랑 미원만 잘넣고 홍합탕하니까 깊은맛나더라ㅋㅋㅋㅋㅋ",
          "original_form": "다시다랑 미원만 잘넣고 홍합탕하니까 깊은맛나더라ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:23"
        },
        {
          "id": "MDRW2100032289.1.6",
          "form": "그지ㅋㅋㄱㄱ부대찌개, 된장, 오뎅탕 등등",
          "original_form": "그지ㅋㅋㄱㄱ부대찌개, 된장, 오뎅탕 등등",
          "speaker_id": "2",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100032289.1.7",
          "form": "찌개랑 국류는 안넣을수가 없더라",
          "original_form": "찌개랑 국류는 안넣을수가 없더라",
          "speaker_id": "2",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100032289.1.8",
          "form": "집에서는 진짜 별수없는듯",
          "original_form": "집에서는 진짜 별수없는듯",
          "speaker_id": "1",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100032289.1.9",
          "form": "등촌볶음밥에도 다시다들어가는거 알아?",
          "original_form": "등촌볶음밥에도 다시다들어가는거 알아?",
          "speaker_id": "2",
          "time": "20210905 15:25"
        },
        {
          "id": "MDRW2100032289.1.10",
          "form": "헐 밥밑에 깔아두는거?",
          "original_form": "헐 밥밑에 깔아두는거?",
          "speaker_id": "1",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100032289.1.11",
          "form": "계란 밑에 넣어둔대",
          "original_form": "계란 밑에 넣어둔대",
          "speaker_id": "2",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100032289.1.12",
          "form": "사실 국물맛이 아니라 다시다 맛인거였음",
          "original_form": "사실 국물맛이 아니라 다시다 맛인거였음",
          "speaker_id": "2",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100032289.1.13",
          "form": "헐 대박...배신감",
          "original_form": "헐 대박...배신감",
          "speaker_id": "1",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100032289.1.14",
          "form": "집에서 넣어서 해봐 다시다 소고기맛 한스푼 넣으니까",
          "original_form": "집에서 넣어서 해봐 다시다 소고기맛 한스푼 넣으니까",
          "speaker_id": "2",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100032289.1.15",
          "form": "그맛 그대로남",
          "original_form": "그맛 그대로남",
          "speaker_id": "2",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100032289.1.16",
          "form": "해봐야겠네....",
          "original_form": "해봐야겠네....",
          "speaker_id": "1",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100032289.1.17",
          "form": "정보 감사",
          "original_form": "정보 감사",
          "speaker_id": "1",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100032289.1.18",
          "form": "가끔 해먹는데 좋아 추천",
          "original_form": "가끔 해먹는데 좋아 추천",
          "speaker_id": "2",
          "time": "20210905 15:29"
        }
