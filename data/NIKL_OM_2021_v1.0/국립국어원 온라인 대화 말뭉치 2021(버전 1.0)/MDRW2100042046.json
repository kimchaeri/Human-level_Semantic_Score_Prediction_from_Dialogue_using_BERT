      "utterance": [
        {
          "id": "MDRW2100042046.1.1",
          "form": "아 옷은 깔별로 사시는구나",
          "original_form": "아 옷은 깔별로 사시는구나",
          "speaker_id": "1",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100042046.1.2",
          "form": "전 맘에 들어서 깔별로 사고픈데",
          "original_form": "전 맘에 들어서 깔별로 사고픈데",
          "speaker_id": "1",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100042046.1.3",
          "form": "또 이쁜게 나오고 해서ㅠㅠ 고민하다 접어요ㅠㅠ",
          "original_form": "또 이쁜게 나오고 해서ㅠㅠ 고민하다 접어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100042046.1.4",
          "form": "ㅋㅋㅋㅋ맘에드는걸 만나는 것이 적기때문에",
          "original_form": "ㅋㅋㅋㅋ맘에드는걸 만나는 것이 적기때문에",
          "speaker_id": "2",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100042046.1.5",
          "form": "깔별로 사신 템이 있나요?",
          "original_form": "깔별로 사신 템이 있나요?",
          "speaker_id": "1",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100042046.1.6",
          "form": "저 기본 셔츠같은 거는 깔별로 사여",
          "original_form": "저 기본 셔츠같은 거는 깔별로 사여",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100042046.1.7",
          "form": "깔별로 산적잇어요?",
          "original_form": "깔별로 산적잇어요?",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100042046.1.8",
          "form": "저도 주로 셔츠가 그래요",
          "original_form": "저도 주로 셔츠가 그래요",
          "speaker_id": "1",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100042046.1.9",
          "form": "하양 파랑",
          "original_form": "하양 파랑",
          "speaker_id": "1",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100042046.1.10",
          "form": "보라",
          "original_form": "보라",
          "speaker_id": "1",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100042046.1.11",
          "form": "아 양말 깔별로 사요 ㅋㅋ",
          "original_form": "아 양말 깔별로 사요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100042046.1.12",
          "form": "오 양말도 깔별로 ㅋㅋㅋㅋ",
          "original_form": "오 양말도 깔별로 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:12"
        },
        {
          "id": "MDRW2100042046.1.13",
          "form": "전 양말이 깔별사면 뭔가 아쉬운 요소라",
          "original_form": "전 양말이 깔별사면 뭔가 아쉬운 요소라",
          "speaker_id": "2",
          "time": "20210906 19:12"
        },
        {
          "id": "MDRW2100042046.1.14",
          "form": "예쁜거 다양하게 신고싶움",
          "original_form": "예쁜거 다양하게 신고싶움",
          "speaker_id": "2",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100042046.1.15",
          "form": "아 양말은 또 다른 기준이 있으시네요 ㅋㅋㅋㅋ",
          "original_form": "아 양말은 또 다른 기준이 있으시네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100042046.1.16",
          "form": "양말쇼핑 안한지 오래되어서",
          "original_form": "양말쇼핑 안한지 오래되어서",
          "speaker_id": "2",
          "time": "20210906 19:15"
        },
        {
          "id": "MDRW2100042046.1.17",
          "form": "양말로 할수없는 얘기가 없는데",
          "original_form": "양말로 할수없는 얘기가 없는데",
          "speaker_id": "2",
          "time": "20210906 19:15"
        },
        {
          "id": "MDRW2100042046.1.18",
          "form": "여름이라 맨발로 다녔어요",
          "original_form": "여름이라 맨발로 다녔어요",
          "speaker_id": "1",
          "time": "20210906 19:15"
        },
        {
          "id": "MDRW2100042046.1.19",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210906 19:15"
        }
