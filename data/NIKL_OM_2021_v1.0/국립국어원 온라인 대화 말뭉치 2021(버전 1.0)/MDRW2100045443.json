      "utterance": [
        {
          "id": "MDRW2100045443.1.1",
          "form": "아담은 잠이 들어버린거에용",
          "original_form": "아담은 잠이 들어버린거에용",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045443.1.2",
          "form": "응ㅇ응응",
          "original_form": "응ㅇ응응",
          "speaker_id": "1",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045443.1.3",
          "form": "근데 대충 머 누가 썼는지는 모르겠는데 아담이 자고 있는 곳에",
          "original_form": "근데 대충 머 누가 썼는지는 모르겠는데 아담이 자고 있는 곳에",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045443.1.4",
          "form": "사해문서라고",
          "original_form": "사해문서라고",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045443.1.5",
          "form": "아담이ㅜ깨어나서 리린(인간) 다 죽일 것임",
          "original_form": "아담이ㅜ깨어나서 리린(인간) 다 죽일 것임",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045443.1.6",
          "form": "머 이런 이야기가 써 있는거임",
          "original_form": "머 이런 이야기가 써 있는거임",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045443.1.7",
          "form": "어 진짜 생각보다.... 설정이 많다 몰랐는데(ㅠㅠ)응ㅇ응응",
          "original_form": "어 진짜 생각보다.... 설정이 많다 몰랐는데(ㅠㅠ)응ㅇ응응",
          "speaker_id": "1",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045443.1.8",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠ 뭐냐... 나 에바 몇년 전에 봤는데 전혀 몰랐던(ㅠㅠㅠ)헛으로봤네....",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠ 뭐냐... 나 에바 몇년 전에 봤는데 전혀 몰랐던(ㅠㅠㅠ)헛으로봤네....",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045443.1.9",
          "form": "그래서 누구냐 보라머리 사령관언니 아빠였나",
          "original_form": "그래서 누구냐 보라머리 사령관언니 아빠였나",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045443.1.10",
          "form": "그 사람이 사해 문서 찾고 죽었나 그랫을걸",
          "original_form": "그 사람이 사해 문서 찾고 죽었나 그랫을걸",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045443.1.11",
          "form": "아....아니 그런 스토리가...",
          "original_form": "아....아니 그런 스토리가...",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045443.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ 아니 뭐야..이제야 좀 이해갈 것 같애 응ㅇ응응...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ 아니 뭐야..이제야 좀 이해갈 것 같애 응ㅇ응응...",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045443.1.13",
          "form": "그래서 그 문서였나에",
          "original_form": "그래서 그 문서였나에",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045443.1.14",
          "form": "나대충봤네...(우럭",
          "original_form": "나대충봤네...(우럭",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045443.1.15",
          "form": "리리스가 잠들어 있는 곳도 써이5음",
          "original_form": "리리스가 잠들어 있는 곳도 써이5음",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045443.1.16",
          "form": "오..응응",
          "original_form": "오..응응",
          "speaker_id": "1",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045443.1.17",
          "form": "그래서 그 아담이 보내올 사도들과 대적 하기 위해 리리스를 떼서 조물락조물락 해가꼬",
          "original_form": "그래서 그 아담이 보내올 사도들과 대적 하기 위해 리리스를 떼서 조물락조물락 해가꼬",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045443.1.18",
          "form": "에바 초호기를 만든것임",
          "original_form": "에바 초호기를 만든것임",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045443.1.19",
          "form": "아......!!!",
          "original_form": "아......!!!",
          "speaker_id": "1",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045443.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ(이마팍팍",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ(이마팍팍",
          "speaker_id": "1",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045443.1.21",
          "form": "그리고 아담이 사도를 리리스에게 보내는 이유는",
          "original_form": "그리고 아담이 사도를 리리스에게 보내는 이유는",
          "speaker_id": "2",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100045443.1.22",
          "form": "대충 둘이 만나면 임팩트 터져서 다 죽는거였던듯?",
          "original_form": "대충 둘이 만나면 임팩트 터져서 다 죽는거였던듯?",
          "speaker_id": "2",
          "time": "20210908 02:37"
        },
        {
