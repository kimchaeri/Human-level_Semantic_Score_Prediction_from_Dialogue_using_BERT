      "utterance": [
        {
          "id": "MDRW2100029763.1.1",
          "form": "종교 있어??",
          "original_form": "종교 있어??",
          "speaker_id": "1",
          "time": "20210903 13:13"
        },
        {
          "id": "MDRW2100029763.1.2",
          "form": "종교는 없네요",
          "original_form": "종교는 없네요",
          "speaker_id": "2",
          "time": "20210903 13:19"
        },
        {
          "id": "MDRW2100029763.1.3",
          "form": "종교 있으신가요?",
          "original_form": "종교 있으신가요?",
          "speaker_id": "2",
          "time": "20210903 13:19"
        },
        {
          "id": "MDRW2100029763.1.4",
          "form": "아 저도 없어요!",
          "original_form": "아 저도 없어요!",
          "speaker_id": "1",
          "time": "20210903 13:22"
        },
        {
          "id": "MDRW2100029763.1.5",
          "form": "부모님은 불교를 믿으시지만요",
          "original_form": "부모님은 불교를 믿으시지만요",
          "speaker_id": "1",
          "time": "20210903 13:22"
        },
        {
          "id": "MDRW2100029763.1.6",
          "form": "아 다행(?)이네요",
          "original_form": "아 다행(?)이네요",
          "speaker_id": "2",
          "time": "20210903 13:26"
        },
        {
          "id": "MDRW2100029763.1.7",
          "form": "종교 가지신 분하고",
          "original_form": "종교 가지신 분하고",
          "speaker_id": "2",
          "time": "20210903 13:27"
        },
        {
          "id": "MDRW2100029763.1.8",
          "form": "종교 주제로 얘기하면",
          "original_form": "종교 주제로 얘기하면",
          "speaker_id": "2",
          "time": "20210903 13:27"
        },
        {
          "id": "MDRW2100029763.1.9",
          "form": "조심스러워지는데",
          "original_form": "조심스러워지는데",
          "speaker_id": "2",
          "time": "20210903 13:27"
        },
        {
          "id": "MDRW2100029763.1.10",
          "form": "그쵸ㅠㅠ",
          "original_form": "그쵸ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 13:27"
        },
        {
          "id": "MDRW2100029763.1.11",
          "form": "제 주변에도 천주교, 기독교, 불교 어느정도 다양하게 있는데",
          "original_form": "제 주변에도 천주교, 기독교, 불교 어느정도 다양하게 있는데",
          "speaker_id": "1",
          "time": "20210903 13:27"
        },
        {
          "id": "MDRW2100029763.1.12",
          "form": "종교활동을 강요하지 않아서 좋은거같아요ㅎㅎ",
          "original_form": "종교활동을 강요하지 않아서 좋은거같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 13:28"
        },
        {
          "id": "MDRW2100029763.1.13",
          "form": "확실히",
          "original_form": "확실히",
          "speaker_id": "2",
          "time": "20210903 13:29"
        },
        {
          "id": "MDRW2100029763.1.14",
          "form": "본인에게 좋은거고",
          "original_form": "본인에게 좋은거고",
          "speaker_id": "2",
          "time": "20210903 13:29"
        },
        {
          "id": "MDRW2100029763.1.15",
          "form": "좋은 마음에서 하는거라지만",
          "original_form": "좋은 마음에서 하는거라지만",
          "speaker_id": "2",
          "time": "20210903 13:29"
        },
        {
          "id": "MDRW2100029763.1.16",
          "form": "종교 권유는 좀...그렇죠",
          "original_form": "종교 권유는 좀...그렇죠",
          "speaker_id": "2",
          "time": "20210903 13:29"
        },
        {
          "id": "MDRW2100029763.1.17",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "1",
          "time": "20210903 13:30"
        },
        {
          "id": "MDRW2100029763.1.18",
          "form": "저는 오히려 상황이 힘들면 주변 사람들에게 털어놓고 이겨내는 편이라",
          "original_form": "저는 오히려 상황이 힘들면 주변 사람들에게 털어놓고 이겨내는 편이라",
          "speaker_id": "1",
          "time": "20210903 13:31"
        },
        {
          "id": "MDRW2100029763.1.19",
          "form": "종교를 믿을만한 계기...? 같은게 없었던 것 같아요",
          "original_form": "종교를 믿을만한 계기...? 같은게 없었던 것 같아요",
          "speaker_id": "1",
          "time": "20210903 13:31"
        },
        {
          "id": "MDRW2100029763.1.20",
          "form": "확실히 그런 믿을 만한 사람이 주변에 있으면",
          "original_form": "확실히 그런 믿을 만한 사람이 주변에 있으면",
          "speaker_id": "2",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029763.1.21",
          "form": "힘들때의 종교의 필요성을 덜 느끼겠네요",
          "original_form": "힘들때의 종교의 필요성을 덜 느끼겠네요",
          "speaker_id": "2",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029763.1.22",
          "form": "저는 그...",
          "original_form": "저는 그...",
          "speaker_id": "2",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029763.1.23",
          "form": "조금 강한 무신론자다 보니",
          "original_form": "조금 강한 무신론자다 보니",
          "speaker_id": "2",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029763.1.24",
          "form": "아 그러시군요...!!",
          "original_form": "아 그러시군요...!!",
          "speaker_id": "1",
          "time": "20210903 13:39"
        },
        {
          "id": "MDRW2100029763.1.25",
          "form": "어떠한 생각으로 무신론자이신건지 여쭤볼수있을까요...??",
          "original_form": "어떠한 생각으로 무신론자이신건지 여쭤볼수있을까요...??",
          "speaker_id": "1",
          "time": "20210903 13:39"
        },
        {
          "id": "MDRW2100029763.1.26",
          "form": "이기적 유전자를 매우 감명 깊게 읽었다보니...",
          "original_form": "이기적 유전자를 매우 감명 깊게 읽었다보니...",
          "speaker_id": "2",
          "time": "20210903 13:50"
        },
        {
          "id": "MDRW2100029763.1.27",
          "form": "리처드 도킨스의 영향을 조금 많이 받았습니다",
          "original_form": "리처드 도킨스의 영향을 조금 많이 받았습니다",
          "speaker_id": "2",
          "time": "20210903 13:50"
        },
        {
          "id": "MDRW2100029763.1.28",
          "form": "오오 저도 한번은 읽어보고 싶은 책이었는데!",
          "original_form": "오오 저도 한번은 읽어보고 싶은 책이었는데!",
          "speaker_id": "1",
          "time": "20210903 13:56"
        },
        {
          "id": "MDRW2100029763.1.29",
          "form": "리처드 도킨스의 다른 책들도 읽어보셨나요?",
          "original_form": "리처드 도킨스의 다른 책들도 읽어보셨나요?",
          "speaker_id": "1",
          "time": "20210903 13:57"
        },
        {
          "id": "MDRW2100029763.1.30",
          "form": "만들어진 신이라던가",
          "original_form": "만들어진 신이라던가",
          "speaker_id": "2",
          "time": "20210903 13:57"
        },
        {
          "id": "MDRW2100029763.1.31",
          "form": "지상 최대의 쇼라던가",
          "original_form": "지상 최대의 쇼라던가",
          "speaker_id": "2",
          "time": "20210903 13:57"
        },
        {
          "id": "MDRW2100029763.1.32",
          "form": "눈 먼 시계공이었나",
          "original_form": "눈 먼 시계공이었나",
          "speaker_id": "2",
          "time": "20210903 13:57"
        },
        {
          "id": "MDRW2100029763.1.33",
          "form": "이건 제목이 가물가물하네요",
          "original_form": "이건 제목이 가물가물하네요",
          "speaker_id": "2",
          "time": "20210903 13:58"
        },
        {
          "id": "MDRW2100029763.1.34",
          "form": "여튼",
          "original_form": "여튼",
          "speaker_id": "2",
          "time": "20210903 13:58"
        },
        {
          "id": "MDRW2100029763.1.35",
          "form": "이렇게들 읽었죠",
          "original_form": "이렇게들 읽었죠",
          "speaker_id": "2",
          "time": "20210903 13:58"
        },
        {
          "id": "MDRW2100029763.1.36",
          "form": "와 저는 개인적으로 어렵게 느껴지던 주제였는데 대단하시네요ㅎㅎ",
          "original_form": "와 저는 개인적으로 어렵게 느껴지던 주제였는데 대단하시네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 14:02"
        }
      ]
    }
