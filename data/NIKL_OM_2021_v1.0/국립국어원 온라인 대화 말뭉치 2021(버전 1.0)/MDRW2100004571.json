      "utterance": [
        {
          "id": "MDRW2100004571.1.1",
          "form": "배달먹어요?",
          "original_form": "배달먹어요?",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004571.1.2",
          "form": "배달은 자주 안먹어요",
          "original_form": "배달은 자주 안먹어요",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004571.1.3",
          "form": "자주 드시나요?",
          "original_form": "자주 드시나요?",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004571.1.4",
          "form": "네 자주",
          "original_form": "네 자주",
          "speaker_id": "1",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004571.1.5",
          "form": "주로 머드세요?",
          "original_form": "주로 머드세요?",
          "speaker_id": "2",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004571.1.6",
          "form": "치킨요",
          "original_form": "치킨요",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004571.1.7",
          "form": "아 치킨은 배달이죠 ㅎ",
          "original_form": "아 치킨은 배달이죠 ㅎ",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004571.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004571.1.9",
          "form": "어느부위좋아요?",
          "original_form": "어느부위좋아요?",
          "speaker_id": "1",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004571.1.10",
          "form": "닭다리요",
          "original_form": "닭다리요",
          "speaker_id": "2",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004571.1.11",
          "form": "진리죠",
          "original_form": "진리죠",
          "speaker_id": "1",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004571.1.12",
          "form": "어느 브랜드꺼주로드세요?",
          "original_form": "어느 브랜드꺼주로드세요?",
          "speaker_id": "2",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004571.1.13",
          "form": "네네뇨",
          "original_form": "네네뇨",
          "speaker_id": "1",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004571.1.14",
          "form": "님은?",
          "original_form": "님은?",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004571.1.15",
          "form": "저는 교촌좋아해요",
          "original_form": "저는 교촌좋아해요",
          "speaker_id": "2",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004571.1.16",
          "form": "허니나 간장치킨이요",
          "original_form": "허니나 간장치킨이요",
          "speaker_id": "2",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004571.1.17",
          "form": "양념? 후라이드? 어느쪽이세요?",
          "original_form": "양념? 후라이드? 어느쪽이세요?",
          "speaker_id": "2",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004571.1.18",
          "form": "후라이드요",
          "original_form": "후라이드요",
          "speaker_id": "1",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004571.1.19",
          "form": "저도 후라이드요 ㅎ",
          "original_form": "저도 후라이드요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:20"
