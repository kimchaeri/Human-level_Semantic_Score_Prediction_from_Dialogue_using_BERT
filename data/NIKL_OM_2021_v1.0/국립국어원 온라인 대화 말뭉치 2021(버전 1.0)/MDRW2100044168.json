      "utterance": [
        {
          "id": "MDRW2100044168.1.1",
          "form": "운동하시나요?",
          "original_form": "운동하시나요?",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044168.1.2",
          "form": "네 필라테스 해요",
          "original_form": "네 필라테스 해요",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044168.1.3",
          "form": "오 대단하세요",
          "original_form": "오 대단하세요",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044168.1.4",
          "form": "저는 헬스했는데 요즘 코로나로쉬고있어요ㅠ",
          "original_form": "저는 헬스했는데 요즘 코로나로쉬고있어요ㅠ",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044168.1.5",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044168.1.6",
          "form": "전 수영 좋아하는데",
          "original_form": "전 수영 좋아하는데",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044168.1.7",
          "form": "코로나로 계속 못갔어요",
          "original_form": "코로나로 계속 못갔어요",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044168.1.8",
          "form": "휴관도 길어지고",
          "original_form": "휴관도 길어지고",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044168.1.9",
          "form": "아쉽겠어요",
          "original_form": "아쉽겠어요",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044168.1.10",
          "form": "네 아쉽죠ㅜ",
          "original_form": "네 아쉽죠ㅜ",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044168.1.11",
          "form": "전 몸이 근질근질하거든요",
          "original_form": "전 몸이 근질근질하거든요",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044168.1.12",
          "form": "어서 가서 몸풀고싶어서ㅜ",
          "original_form": "어서 가서 몸풀고싶어서ㅜ",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044168.1.13",
          "form": "근데 또 수영장에선 마스크 못쓰니까...",
          "original_form": "근데 또 수영장에선 마스크 못쓰니까...",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044168.1.14",
          "form": "대신 필테하시는군요?",
          "original_form": "대신 필테하시는군요?",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044168.1.15",
          "form": "아 운동 좋아하시나봐요",
          "original_form": "아 운동 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044168.1.16",
          "form": "네 몸이 너무 굳는것 같아 필라테스 해요",
          "original_form": "네 몸이 너무 굳는것 같아 필라테스 해요",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044168.1.17",
          "form": "네 저 헬스전에는필라테스 했어요!",
          "original_form": "네 저 헬스전에는필라테스 했어요!",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044168.1.18",
          "form": "와 운동 꾸준히 하셨구나",
          "original_form": "와 운동 꾸준히 하셨구나",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044168.1.19",
          "form": "그런것 치고는 약골인데요..ㅠ 계속하려고 노력합니당ㅜ",
          "original_form": "그런것 치고는 약골인데요..ㅠ 계속하려고 노력합니당ㅜ",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044168.1.20",
          "form": "저야말로ㅠ",
          "original_form": "저야말로ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044168.1.21",
          "form": "필라테스는 어느정도 하셨어요?",
          "original_form": "필라테스는 어느정도 하셨어요?",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044168.1.22",
          "form": "이제 운동 안하면 건강을 해치겠더라고요",
          "original_form": "이제 운동 안하면 건강을 해치겠더라고요",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044168.1.23",
          "form": "맞아요 정신건강도 약해지고요ㅜ",
          "original_form": "맞아요 정신건강도 약해지고요ㅜ",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044168.1.24",
          "form": "저는 필라테스 작년 11월에 시작했어요",
          "original_form": "저는 필라테스 작년 11월에 시작했어요",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044168.1.25",
          "form": "근데 중간에 한달 정도는 코로나 거리두기로 휴관했고",
          "original_form": "근데 중간에 한달 정도는 코로나 거리두기로 휴관했고",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044168.1.26",
          "form": "그정도면 오래하셨군요!",
          "original_form": "그정도면 오래하셨군요!",
          "speaker_id": "2",
          "time": "20210907 22:02"
        }
      ]
    }
  ]
