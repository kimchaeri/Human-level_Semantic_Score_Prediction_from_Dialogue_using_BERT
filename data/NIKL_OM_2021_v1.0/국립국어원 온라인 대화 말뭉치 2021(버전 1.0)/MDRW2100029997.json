      "utterance": [
        {
          "id": "MDRW2100029997.1.1",
          "form": "안녕하세욯!",
          "original_form": "안녕하세욯!",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100029997.1.2",
          "form": "종교있으신가요",
          "original_form": "종교있으신가요",
          "speaker_id": "2",
          "time": "20210903 18:03"
        },
        {
          "id": "MDRW2100029997.1.3",
          "form": "저는 없어요.",
          "original_form": "저는 없어요.",
          "speaker_id": "1",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100029997.1.4",
          "form": "있으신가요?",
          "original_form": "있으신가요?",
          "speaker_id": "1",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100029997.1.5",
          "form": "기독교 모태였는데",
          "original_form": "기독교 모태였는데",
          "speaker_id": "2",
          "time": "20210903 18:06"
        },
        {
          "id": "MDRW2100029997.1.6",
          "form": "지금은 무교에요",
          "original_form": "지금은 무교에요",
          "speaker_id": "2",
          "time": "20210903 18:06"
        },
        {
          "id": "MDRW2100029997.1.7",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "1",
          "time": "20210903 18:07"
        },
        {
          "id": "MDRW2100029997.1.8",
          "form": "특별한 계기가 잇으셨나요",
          "original_form": "특별한 계기가 잇으셨나요",
          "speaker_id": "1",
          "time": "20210903 18:07"
        },
        {
          "id": "MDRW2100029997.1.9",
          "form": "시대랑 많이 안맞는거 같아요",
          "original_form": "시대랑 많이 안맞는거 같아요",
          "speaker_id": "2",
          "time": "20210903 18:09"
        },
        {
          "id": "MDRW2100029997.1.10",
          "form": "틀에 박혔다고 해야할까요",
          "original_form": "틀에 박혔다고 해야할까요",
          "speaker_id": "2",
          "time": "20210903 18:09"
        },
        {
          "id": "MDRW2100029997.1.11",
          "form": "아하..그렇군요",
          "original_form": "아하..그렇군요",
          "speaker_id": "1",
          "time": "20210903 18:10"
        },
        {
          "id": "MDRW2100029997.1.12",
          "form": "언제 쯤에 무교 되셨나요?",
          "original_form": "언제 쯤에 무교 되셨나요?",
          "speaker_id": "1",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100029997.1.13",
          "form": "앋믿기 시작한진 오래됐는데",
          "original_form": "앋믿기 시작한진 오래됐는데",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100029997.1.14",
          "form": "정식 탈출은 코로나 터지고 나서 했어요",
          "original_form": "정식 탈출은 코로나 터지고 나서 했어요",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100029997.1.15",
          "form": "아하 약간 딱 그럴 상황이 되긴 했네요",
          "original_form": "아하 약간 딱 그럴 상황이 되긴 했네요",
          "speaker_id": "1",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100029997.1.16",
          "form": "팬데믹이니까 안나가도 되구...",
          "original_form": "팬데믹이니까 안나가도 되구...",
          "speaker_id": "1",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100029997.1.17",
          "form": "맞아요... 그 김에 다 차단박았어요",
          "original_form": "맞아요... 그 김에 다 차단박았어요",
          "speaker_id": "2",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100029997.1.18",
          "form": "좋은 기회가 됐네요",
          "original_form": "좋은 기회가 됐네요",
          "speaker_id": "1",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100029997.1.19",
          "form": "그러게요 코로나의 유일한 장점이네요",
          "original_form": "그러게요 코로나의 유일한 장점이네요",
          "speaker_id": "2",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100029997.1.20",
          "form": "ㅎㅎ..그러게요",
          "original_form": "ㅎㅎ..그러게요",
          "speaker_id": "1",
          "time": "20210903 18:32"
        }
      ]
    }
