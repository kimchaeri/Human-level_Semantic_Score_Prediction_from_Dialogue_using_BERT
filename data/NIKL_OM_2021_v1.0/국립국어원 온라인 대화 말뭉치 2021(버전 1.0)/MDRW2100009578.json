      "utterance": [
        {
          "id": "MDRW2100009578.1.1",
          "form": "요즘 올림픽선수들 여기저기 나오는듯요",
          "original_form": "요즘 올림픽선수들 여기저기 나오는듯요",
          "speaker_id": "2",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009578.1.2",
          "form": "맞아요 어제 살림남에 김제덕 선수도 나오더라구요",
          "original_form": "맞아요 어제 살림남에 김제덕 선수도 나오더라구요",
          "speaker_id": "1",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009578.1.3",
          "form": "집사부일체에 펜싱4인방이랑 양궁선수들 나오든데요",
          "original_form": "집사부일체에 펜싱4인방이랑 양궁선수들 나오든데요",
          "speaker_id": "2",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009578.1.4",
          "form": "살림남에도 나왔어요?",
          "original_form": "살림남에도 나왔어요?",
          "speaker_id": "2",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009578.1.5",
          "form": "아 저도 그거 조금 봤어요",
          "original_form": "아 저도 그거 조금 봤어요",
          "speaker_id": "1",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009578.1.6",
          "form": "김제덕선수만 ㅋ",
          "original_form": "김제덕선수만 ㅋ",
          "speaker_id": "1",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009578.1.7",
          "form": "막내라그런지 귀엽더라구요",
          "original_form": "막내라그런지 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009578.1.8",
          "form": "순진하고",
          "original_form": "순진하고",
          "speaker_id": "2",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009578.1.9",
          "form": "담주 나오는거 더 재밌겠더라구요 학교 가는거",
          "original_form": "담주 나오는거 더 재밌겠더라구요 학교 가는거",
          "speaker_id": "1",
          "time": "20210823 01:00"
        },
        {
          "id": "MDRW2100009578.1.10",
          "form": "살림남에 나와요? 담주에도?",
          "original_form": "살림남에 나와요? 담주에도?",
          "speaker_id": "2",
          "time": "20210823 01:00"
        },
        {
          "id": "MDRW2100009578.1.11",
          "form": "네 제덕선수나와요 ㅋ",
          "original_form": "네 제덕선수나와요 ㅋ",
          "speaker_id": "1",
          "time": "20210823 01:00"
        },
        {
          "id": "MDRW2100009578.1.12",
          "form": "패널 게스트로 나온게아닌가보네요",
          "original_form": "패널 게스트로 나온게아닌가보네요",
          "speaker_id": "2",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100009578.1.13",
          "form": "담주에 챙겨봐야겠네요",
          "original_form": "담주에 챙겨봐야겠네요",
          "speaker_id": "2",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100009578.1.14",
          "form": "네 고정은 아니라도 지난주 짤막하게 해서 일단 다음주 예고에는 나왔어요",
          "original_form": "네 고정은 아니라도 지난주 짤막하게 해서 일단 다음주 예고에는 나왔어요",
          "speaker_id": "1",
          "time": "20210823 01:02"
        },
        {
          "id": "MDRW2100009578.1.15",
          "form": "담주 집사부일체도 양궁선수들 계속나와요",
          "original_form": "담주 집사부일체도 양궁선수들 계속나와요",
          "speaker_id": "2",
          "time": "20210823 01:04"
        },
        {
          "id": "MDRW2100009578.1.16",
          "form": "챙겨봐야겠네요",
          "original_form": "챙겨봐야겠네요",
          "speaker_id": "2",
          "time": "20210823 01:04"
        },
        {
          "id": "MDRW2100009578.1.17",
          "form": "늦었는데  안주무세요?",
          "original_form": "늦었는데  안주무세요?",
          "speaker_id": "2",
          "time": "20210823 01:04"
        },
        {
          "id": "MDRW2100009578.1.18",
          "form": "이제 자야곘어요ㅠㅠ",
          "original_form": "이제 자야곘어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 01:05"
        },
        {
          "id": "MDRW2100009578.1.19",
          "form": "펜하 마저보고 저도 자야겠어요",
          "original_form": "펜하 마저보고 저도 자야겠어요",
          "speaker_id": "2",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009578.1.20",
          "form": "낼시간날때 또얘기해요^^",
          "original_form": "낼시간날때 또얘기해요^^",
          "speaker_id": "2",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009578.1.21",
          "form": "굿밤요",
          "original_form": "굿밤요",
          "speaker_id": "2",
