      "utterance": [
        {
          "id": "MDRW2100012753.1.1",
          "form": "어디를 가도 우리나라도 벚꽃이 많더라",
          "original_form": "어디를 가도 우리나라도 벚꽃이 많더라",
          "speaker_id": "2",
          "time": "20210825 00:45"
        },
        {
          "id": "MDRW2100012753.1.2",
          "form": "하긴 맞다 ㅋㅋ",
          "original_form": "하긴 맞다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:45"
        },
        {
          "id": "MDRW2100012753.1.3",
          "form": "그래서 경주는 왜?",
          "original_form": "그래서 경주는 왜?",
          "speaker_id": "2",
          "time": "20210825 00:45"
        },
        {
          "id": "MDRW2100012753.1.4",
          "form": "경주 벚꽃은 한번 피고 지는게 아니라 두번 핀다",
          "original_form": "경주 벚꽃은 한번 피고 지는게 아니라 두번 핀다",
          "speaker_id": "1",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100012753.1.5",
          "form": "진짜 그런것도 있어?",
          "original_form": "진짜 그런것도 있어?",
          "speaker_id": "2",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100012753.1.6",
          "form": "어 겹벚꽃이라고 해서 두번 핀다",
          "original_form": "어 겹벚꽃이라고 해서 두번 핀다",
          "speaker_id": "1",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100012753.1.7",
          "form": "대박이다 정말로",
          "original_form": "대박이다 정말로",
          "speaker_id": "2",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100012753.1.8",
          "form": "4월 중순부터 벚꽃이 핀다고 하니 5월이 딱 좋데",
          "original_form": "4월 중순부터 벚꽃이 핀다고 하니 5월이 딱 좋데",
          "speaker_id": "1",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100012753.1.9",
          "form": "진짜 신기하다 겹벚꽃",
          "original_form": "진짜 신기하다 겹벚꽃",
          "speaker_id": "2",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100012753.1.10",
          "form": "응 나두 첨 알았다",
          "original_form": "응 나두 첨 알았다",
          "speaker_id": "1",
          "time": "20210825 00:48"
        },
        {
          "id": "MDRW2100012753.1.11",
          "form": "우리나라에도 정말 볼게 많다니까",
          "original_form": "우리나라에도 정말 볼게 많다니까",
          "speaker_id": "2",
          "time": "20210825 00:48"
        },
        {
          "id": "MDRW2100012753.1.12",
          "form": "그렇네 우리나라 물가가 비싸니 이왕 이돈이면 외국가자 하는거지",
          "original_form": "그렇네 우리나라 물가가 비싸니 이왕 이돈이면 외국가자 하는거지",
          "speaker_id": "1",
          "time": "20210825 00:49"
        },
        {
          "id": "MDRW2100012753.1.13",
          "form": "그래 그게 맞다",
          "original_form": "그래 그게 맞다",
          "speaker_id": "2",
          "time": "20210825 00:49"
        },
        {
          "id": "MDRW2100012753.1.14",
          "form": "임자도 알지",
          "original_form": "임자도 알지",
          "speaker_id": "1",
          "time": "20210825 00:49"
        }
      ]
