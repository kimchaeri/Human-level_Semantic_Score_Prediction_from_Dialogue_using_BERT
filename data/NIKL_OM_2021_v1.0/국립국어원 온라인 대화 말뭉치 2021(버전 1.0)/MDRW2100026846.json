        {
          "id": "MDRW2100026846.1.1",
          "form": "일하시나요",
          "original_form": "일하시나요",
          "speaker_id": "2",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026846.1.2",
          "form": "여행입니다",
          "original_form": "여행입니다",
          "speaker_id": "1",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026846.1.3",
          "form": "여행좋아하시나요",
          "original_form": "여행좋아하시나요",
          "speaker_id": "2",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026846.1.4",
          "form": "잇힝",
          "original_form": "잇힝",
          "speaker_id": "1",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026846.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026846.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026846.1.7",
          "form": "어디가보셨어요",
          "original_form": "어디가보셨어요",
          "speaker_id": "2",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026846.1.8",
          "form": "ㄲㅋ",
          "original_form": "ㄲㅋ",
          "speaker_id": "1",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026846.1.9",
          "form": "제주도에 갔습니다",
          "original_form": "제주도에 갔습니다",
          "speaker_id": "1",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026846.1.10",
          "form": "제주도 좋네요",
          "original_form": "제주도 좋네요",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026846.1.11",
          "form": "국내여행을 많이 해보지않았어요",
          "original_form": "국내여행을 많이 해보지않았어요",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026846.1.12",
          "form": "너무 이뻐요",
          "original_form": "너무 이뻐요",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026846.1.13",
          "form": "제주도 가보셨나요",
          "original_form": "제주도 가보셨나요",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026846.1.14",
          "form": "네 너무 좋더라구요",
          "original_form": "네 너무 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026846.1.15",
          "form": "기념품도 이쁘고",
          "original_form": "기념품도 이쁘고",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026846.1.16",
          "form": "비싼감은있지만요",
          "original_form": "비싼감은있지만요",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026846.1.17",
          "form": "제주돌빵 아시나요?",
          "original_form": "제주돌빵 아시나요?",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026846.1.18",
          "form": "빵은 안먹어봤어요",
          "original_form": "빵은 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026846.1.19",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026846.1.20",
          "form": "돌땡이 모양이라 돌밭에 떨어지면 찾지못합니나",
          "original_form": "돌땡이 모양이라 돌밭에 떨어지면 찾지못합니나",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026846.1.21",
          "form": "제주돌은 시커머니까요",
          "original_form": "제주돌은 시커머니까요",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026846.1.22",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026846.1.23",
          "form": "네 제주도에서 무얼드셨나요",
          "original_form": "네 제주도에서 무얼드셨나요",
          "speaker_id": "1",
          "time": "20210902 00:39"
        }
