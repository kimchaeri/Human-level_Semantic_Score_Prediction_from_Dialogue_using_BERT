      "utterance": [
        {
          "id": "MDRW2100047468.1.1",
          "form": "오늘 바람이 너무 시원해",
          "original_form": "오늘 바람이 너무 시원해",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100047468.1.2",
          "form": "지금 날씨 딱",
          "original_form": "지금 날씨 딱",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100047468.1.3",
          "form": "선선해서",
          "original_form": "선선해서",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100047468.1.4",
          "form": "선풍기 좀만 틀어도 춥다",
          "original_form": "선풍기 좀만 틀어도 춥다",
          "speaker_id": "2",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100047468.1.5",
          "form": "맞아 이제 씻고 나오면 추워",
          "original_form": "맞아 이제 씻고 나오면 추워",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100047468.1.6",
          "form": "옹 그래도 아직은 찬물샤워 하는 듯",
          "original_form": "옹 그래도 아직은 찬물샤워 하는 듯",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100047468.1.7",
          "form": "나도ㅋㅋㅋ 미지근한 물로",
          "original_form": "나도ㅋㅋㅋ 미지근한 물로",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100047468.1.8",
          "form": "그래도 뭔가 끝나려니가",
          "original_form": "그래도 뭔가 끝나려니가",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100047468.1.9",
          "form": "여름이 이렇게 짧았나 싶다",
          "original_form": "여름이 이렇게 짧았나 싶다",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100047468.1.10",
          "form": "작년엔 징하게 안 갔는데 여름이",
          "original_form": "작년엔 징하게 안 갔는데 여름이",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100047468.1.11",
          "form": "그러게 갑자기 기온 내려가서",
          "original_form": "그러게 갑자기 기온 내려가서",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100047468.1.12",
          "form": "순식간에 끝난 느낌",
          "original_form": "순식간에 끝난 느낌",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100047468.1.13",
          "form": "사실 짧진 않았는데",
          "original_form": "사실 짧진 않았는데",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100047468.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100047468.1.15",
          "form": "그래도 9월도 안 돼서 기온 내려간 건 빠른 거 아닌가",
          "original_form": "그래도 9월도 안 돼서 기온 내려간 건 빠른 거 아닌가",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100047468.1.16",
          "form": "일찍 시작하긴 했지만...",
          "original_form": "일찍 시작하긴 했지만...",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100047468.1.17",
          "form": "망각의 동물이야",
          "original_form": "망각의 동물이야",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100047468.1.18",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100047468.1.19",
          "form": "그러고 보니",
          "original_form": "그러고 보니",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100047468.1.20",
          "form": "봄이 진짜 짦고",
          "original_form": "봄이 진짜 짦고",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100047468.1.21",
          "form": "여름 엄청 일찍이었던 듯",
          "original_form": "여름 엄청 일찍이었던 듯",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100047468.1.22",
          "form": "맞아 너무 짧았어",
          "original_form": "맞아 너무 짧았어",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100047468.1.23",
          "form": "봄 가을이 좋은디",
          "original_form": "봄 가을이 좋은디",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100047468.1.24",
          "form": "짧아서 아숩",
          "original_form": "짧아서 아숩",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
