      "utterance": [
        {
          "id": "MDRW2100030376.1.1",
          "form": "주식 관련 책 읽으려고 해도 소설로 빠져요",
          "original_form": "주식 관련 책 읽으려고 해도 소설로 빠져요",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100030376.1.2",
          "form": "기본 몰라도 돼요",
          "original_form": "기본 몰라도 돼요",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030376.1.3",
          "form": "차트만 보기도 해요",
          "original_form": "차트만 보기도 해요",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030376.1.4",
          "form": "소설은 한 번 읽으면 끝까지 다 읽게 되더라고요",
          "original_form": "소설은 한 번 읽으면 끝까지 다 읽게 되더라고요",
          "speaker_id": "2",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030376.1.5",
          "form": "차라리 회사 분석 책을 보세요",
          "original_form": "차라리 회사 분석 책을 보세요",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030376.1.6",
          "form": "회계 같은 거",
          "original_form": "회계 같은 거",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030376.1.7",
          "form": "좋은 생각이에요",
          "original_form": "좋은 생각이에요",
          "speaker_id": "2",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030376.1.8",
          "form": "회계 관련 책은 뭐가 좋나요",
          "original_form": "회계 관련 책은 뭐가 좋나요",
          "speaker_id": "2",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030376.1.9",
          "form": "공시 보는 법",
          "original_form": "공시 보는 법",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030376.1.10",
          "form": "차트만 설명되어 있는 책도 있나요",
          "original_form": "차트만 설명되어 있는 책도 있나요",
          "speaker_id": "2",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030376.1.11",
          "form": "저자 사경인",
          "original_form": "저자 사경인",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030376.1.12",
          "form": "차트는 유목민",
          "original_form": "차트는 유목민",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030376.1.13",
          "form": "1캡처 해 놨어요 나중에 볼게요",
          "original_form": "1캡처 해 놨어요 나중에 볼게요",
          "speaker_id": "2",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030376.1.14",
          "form": "철저히 단타 만 해요",
          "original_form": "철저히 단타 만 해요",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030376.1.15",
          "form": "사경인 회계사에요",
          "original_form": "사경인 회계사에요",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100030376.1.16",
          "form": "회사를 알아내기 해줘요",
          "original_form": "회사를 알아내기 해줘요",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100030376.1.17",
          "form": "좋은 정보 감사합니다",
          "original_form": "좋은 정보 감사합니다",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100030376.1.18",
          "form": "책 10권 보고 시작했어요",
          "original_form": "책 10권 보고 시작했어요",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100030376.1.19",
          "form": "시간 될 때 꼭 읽어볼게요",
          "original_form": "시간 될 때 꼭 읽어볼게요",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100030376.1.20",
          "form": "아니 뭘요",
          "original_form": "아니 뭘요",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100030376.1.21",
          "form": "유튜브로도 공부합니다",
          "original_form": "유튜브로도 공부합니다",
          "speaker_id": "2",
          "time": "20210904 00:19"
        }
      ]
