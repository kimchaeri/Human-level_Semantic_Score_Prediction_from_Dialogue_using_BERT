      "utterance": [
        {
          "id": "MDRW2100014004.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100014004.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014004.1.3",
          "form": "아직은 이리저리 연애할 정신도 없고 사람도 없고 ㅠ",
          "original_form": "아직은 이리저리 연애할 정신도 없고 사람도 없고 ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100014004.1.4",
          "form": "사람도 없고 정신도 없죠",
          "original_form": "사람도 없고 정신도 없죠",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100014004.1.5",
          "form": "코로나땜에 뭐 만날 기회조차도 없고 접점도 없으니",
          "original_form": "코로나땜에 뭐 만날 기회조차도 없고 접점도 없으니",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100014004.1.6",
          "form": "그냥 이렇게 지내고있는거같네요 ㅎㅎ",
          "original_form": "그냥 이렇게 지내고있는거같네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100014004.1.7",
          "form": "ㅋㅋㅋ 맞아요 그대신 제 본업에 더 집중할수 있는거같아서",
          "original_form": "ㅋㅋㅋ 맞아요 그대신 제 본업에 더 집중할수 있는거같아서",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014004.1.8",
          "form": "결과론적으로는 잘됐나 싶기도 하구요",
          "original_form": "결과론적으로는 잘됐나 싶기도 하구요",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014004.1.9",
          "form": "뭐 연애가 인생의 전부는 아니니",
          "original_form": "뭐 연애가 인생의 전부는 아니니",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014004.1.10",
          "form": "오히려 좋을수도 있죠!",
          "original_form": "오히려 좋을수도 있죠!",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014004.1.11",
          "form": "ㅎㅎ 그쵸 그리구 아직은 어려서 그런지\r\n이성보다는 다른 부분의 성취가 더 욕심나는거같아요",
          "original_form": "ㅎㅎ 그쵸 그리구 아직은 어려서 그런지\r\n이성보다는 다른 부분의 성취가 더 욕심나는거같아요",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014004.1.12",
          "form": "이래서 연애를 못하는건가ㅋㅋ",
          "original_form": "이래서 연애를 못하는건가ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014004.1.13",
          "form": "전 오히려 좋다고 생각해요!ㅎㅎ",
          "original_form": "전 오히려 좋다고 생각해요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100014004.1.14",
          "form": "연애는 천천히 해도되지만",
          "original_form": "연애는 천천히 해도되지만",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100014004.1.15",
          "form": "다른 부분 성취는 빨리 하면 할수록 좋죠",
          "original_form": "다른 부분 성취는 빨리 하면 할수록 좋죠",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100014004.1.16",
          "form": "승진이든 뭐든 득이 되는게 많으니까요 ㅎㅎ",
          "original_form": "승진이든 뭐든 득이 되는게 많으니까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014004.1.17",
          "form": "ㅋㅋㅋ 맞는말씀이세요",
          "original_form": "ㅋㅋㅋ 맞는말씀이세요",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014004.1.18",
          "form": "저도 한참 20 21일땐 연애에 목매고 그랬는데",
          "original_form": "저도 한참 20 21일땐 연애에 목매고 그랬는데",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014004.1.19",
          "form": "나이를 조금씩 먹고나니 큰의미가 없다고 생각해서",
          "original_form": "나이를 조금씩 먹고나니 큰의미가 없다고 생각해서",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014004.1.20",
          "form": "나중에 진짜 인연오면 그때 연애하고 지금은 제가 하는거에 집중하고있어요 ㅎㅎ",
          "original_form": "나중에 진짜 인연오면 그때 연애하고 지금은 제가 하는거에 집중하고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014004.1.21",
          "form": "ㅋㅋㅋ 완전 인생조언!",
          "original_form": "ㅋㅋㅋ 완전 인생조언!",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014004.1.22",
          "form": "저도 그럴까봐요 언제든 남자는 생긴다~ 하면서",
          "original_form": "저도 그럴까봐요 언제든 남자는 생긴다~ 하면서",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014004.1.23",
          "form": "살아가다 보면 만나겠죠ㅋㅋ",
          "original_form": "살아가다 보면 만나겠죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014004.1.24",
          "form": "그게 마음 편하고 좋아요!",
          "original_form": "그게 마음 편하고 좋아요!",
          "speaker_id": "2",
          "time": "20210825 17:08"
        }
      ]
    }
  ]
