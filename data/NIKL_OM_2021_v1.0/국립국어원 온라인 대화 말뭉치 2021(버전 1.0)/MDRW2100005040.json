      "utterance": [
        {
          "id": "MDRW2100005040.1.1",
          "form": "name1님 민초 좋아하세요?",
          "original_form": "&name1&님 민초 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 20:08"
        },
        {
          "id": "MDRW2100005040.1.2",
          "form": "네 저는 민초단입니다",
          "original_form": "네 저는 민초단입니다",
          "speaker_id": "1",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100005040.1.3",
          "form": "민초단이신가요?",
          "original_form": "민초단이신가요?",
          "speaker_id": "1",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100005040.1.4",
          "form": "아하....반민초입니다",
          "original_form": "아하....반민초입니다",
          "speaker_id": "2",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100005040.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100005040.1.6",
          "form": "민초를 존중하지만 이해하지 못하죠",
          "original_form": "민초를 존중하지만 이해하지 못하죠",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005040.1.7",
          "form": "같은 곳에 담아 먹을 수 없어요",
          "original_form": "같은 곳에 담아 먹을 수 없어요",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005040.1.8",
          "form": "ㅋㅋㅋㅋ 아 존중해드릴게요 ㅠㅠ!!!",
          "original_form": "ㅋㅋㅋㅋ 아 존중해드릴게요 ㅠㅠ!!!",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005040.1.9",
          "form": "요즘 민초 제품 엄청 나오더라구요",
          "original_form": "요즘 민초 제품 엄청 나오더라구요",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005040.1.10",
          "form": "저는 좋은데 괴로우시겠어요..^_ㅠ",
          "original_form": "저는 좋은데 괴로우시겠어요..^_ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100005040.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ민초는 새로 생기는 거라 괜찮아요 있던 걸 먹으면 되니까요!!!!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ민초는 새로 생기는 거라 괜찮아요 있던 걸 먹으면 되니까요!!!!!",
          "speaker_id": "2",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100005040.1.12",
          "form": "ㅋㅋㅋㅋㅋ 긍정적이시네요!",
          "original_form": "ㅋㅋㅋㅋㅋ 긍정적이시네요!",
          "speaker_id": "1",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100005040.1.13",
          "form": "제친구는 치약왜먹냐며 핍박해요!",
          "original_form": "제친구는 치약왜먹냐며 핍박해요!",
          "speaker_id": "1",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100005040.1.14",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㄱㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㄱㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:18"
        },
        {
          "id": "MDRW2100005040.1.15",
          "form": "이게이게 민초를 먹어보라고 안하고 반민초도 핍박을 안 하면 되는 것인데ㅋㅋㅋㅋㅋ",
          "original_form": "이게이게 민초를 먹어보라고 안하고 반민초도 핍박을 안 하면 되는 것인데ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005040.1.16",
          "form": "그죠 ㅜㅜ!!! 저 혼자 먹겠다는데! 핍박을!",
          "original_form": "그죠 ㅜㅜ!!! 저 혼자 먹겠다는데! 핍박을!",
          "speaker_id": "1",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005040.1.17",
          "form": "카페가는것도 좋아하시나요??",
          "original_form": "카페가는것도 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005040.1.18",
          "form": "싫어하진 않는 것 같아요!!",
          "original_form": "싫어하진 않는 것 같아요!!",
          "speaker_id": "2",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005040.1.19",
          "form": "커피를 못 마셨었는데 최근에 마시기 시작했거든요",
          "original_form": "커피를 못 마셨었는데 최근에 마시기 시작했거든요",
          "speaker_id": "2",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005040.1.20",
          "form": "아 못드셨었구나! 카페인에 약하신가봐요!!",
          "original_form": "아 못드셨었구나! 카페인에 약하신가봐요!!",
          "speaker_id": "1",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005040.1.21",
          "form": "네네ㅠㅠ 근데 어쩔 수 없이 밤을 새려면 마시게 되더라고요ㅋㅋㅋㅋ",
          "original_form": "네네ㅠㅠ 근데 어쩔 수 없이 밤을 새려면 마시게 되더라고요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005040.1.22",
          "form": "공부때문에 밤 새는거에요?ㅠㅠ",
          "original_form": "공부때문에 밤 새는거에요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005040.1.23",
          "form": "그래두 많이는 마시지마세요 저처럼 중독자돼요 ㅠㅡㅠ",
          "original_form": "그래두 많이는 마시지마세요 저처럼 중독자돼요 ㅠㅡㅠ",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005040.1.24",
          "form": "이것저것 하다보니 그러게 되네요ㅠㅠ",
          "original_form": "이것저것 하다보니 그러게 되네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005040.1.25",
          "form": "name1님은 커피 많이 드시나요?",
          "original_form": "&name1&님은 커피 많이 드시나요?",
          "speaker_id": "2",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005040.1.26",
          "form": "제 친구도 엄청 자주 마셔서 친구한테 혈관에 커피 흐르냐고 그랬었는데ㅋㅋㅋㅋ",
          "original_form": "제 친구도 엄청 자주 마셔서 친구한테 혈관에 커피 흐르냐고 그랬었는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005040.1.27",
          "form": "네 저는 커피 하루에 한잔이상은 꼭 마셔요!",
          "original_form": "네 저는 커피 하루에 한잔이상은 꼭 마셔요!",
          "speaker_id": "1",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005040.1.28",
          "form": "너무 마셔서 줄이고있는중이에요",
          "original_form": "너무 마셔서 줄이고있는중이에요",
          "speaker_id": "1",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005040.1.29",
          "form": "제가 혈관에 정말 커피 흐를걸욬ㅋㅋㅋㅋ",
          "original_form": "제가 혈관에 정말 커피 흐를걸욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005040.1.30",
          "form": "와....name1님도 진짜 자주 드시네요!!",
          "original_form": "와....&name1&님도 진짜 자주 드시네요!!",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005040.1.31",
          "form": "자주 마시는 사람들은 안 마시면 잠이 안 깬다고 하더라고요",
          "original_form": "자주 마시는 사람들은 안 마시면 잠이 안 깬다고 하더라고요",
          "speaker_id": "2",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005040.1.32",
          "form": "맞아요 사실 커피마셔도 잘 자는 수준이에요 ㅜㅜ...ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 사실 커피마셔도 잘 자는 수준이에요 ㅜㅜ...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005040.1.33",
          "form": "늘리는건 쉬워도 줄이는건 어렵더라구요",
          "original_form": "늘리는건 쉬워도 줄이는건 어렵더라구요",
          "speaker_id": "1",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005040.1.34",
          "form": "아이고....",
          "original_form": "아이고....",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005040.1.35",
          "form": "그럼 아이스 아메리카노 매일 드시는 거에요??",
          "original_form": "그럼 아이스 아메리카노 매일 드시는 거에요??",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005040.1.36",
          "form": "네 ㅋㅋㅋ 지금도.. 디카페인으로..",
          "original_form": "네 ㅋㅋㅋ 지금도.. 디카페인으로..",
          "speaker_id": "1",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005040.1.37",
          "form": "저에겐 물같은 존재에요..",
          "original_form": "저에겐 물같은 존재에요..",
          "speaker_id": "1",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005040.1.38",
          "form": "물이 더 안먹혀요 ㅠㅠ",
          "original_form": "물이 더 안먹혀요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005040.1.39",
          "form": "앜ㅋㅋㅋㅋㅋ 그래도 디카페인이니까...",
          "original_form": "앜ㅋㅋㅋㅋㅋ 그래도 디카페인이니까...",
          "speaker_id": "2",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005040.1.40",
          "form": "저는 물도 자주 안 마시는데 한 번에 몰아서 마시는 것 같아요",
          "original_form": "저는 물도 자주 안 마시는데 한 번에 몰아서 마시는 것 같아요",
