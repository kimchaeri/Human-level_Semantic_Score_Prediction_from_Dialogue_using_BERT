      "utterance": [
        {
          "id": "MDRW2100040394.1.1",
          "form": "팬들끼리 그냥 기싸움 하려고 하는 느낌..",
          "original_form": "팬들끼리 그냥 기싸움 하려고 하는 느낌..",
          "speaker_id": "2",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100040394.1.2",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "1",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100040394.1.3",
          "form": "진짜 기싸움",
          "original_form": "진짜 기싸움",
          "speaker_id": "1",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100040394.1.4",
          "form": "그말이 딱입니다",
          "original_form": "그말이 딱입니다",
          "speaker_id": "1",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100040394.1.5",
          "form": "초동으로 줄세우고 눈치주고",
          "original_form": "초동으로 줄세우고 눈치주고",
          "speaker_id": "1",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100040394.1.6",
          "form": "같은 팬들끼리도 앨범안사면 눈치주잖아요 ㅋㅋㅋ",
          "original_form": "같은 팬들끼리도 앨범안사면 눈치주잖아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100040394.1.7",
          "form": "전 덕질할때도 돈 안썼는데 눈치주는 거 너무 싫었어요",
          "original_form": "전 덕질할때도 돈 안썼는데 눈치주는 거 너무 싫었어요",
          "speaker_id": "2",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100040394.1.8",
          "form": "앨범 사서 솔직히 어따쓴다고..",
          "original_form": "앨범 사서 솔직히 어따쓴다고..",
          "speaker_id": "2",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100040394.1.9",
          "form": "요즘 씨디로 누가 노래들어요 ㅋㅋㅋ ㅜ",
          "original_form": "요즘 씨디로 누가 노래들어요 ㅋㅋㅋ ㅜ",
          "speaker_id": "2",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100040394.1.10",
          "form": "아 뭔지 알아요 너무 싫어요 그거",
          "original_form": "아 뭔지 알아요 너무 싫어요 그거",
          "speaker_id": "1",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100040394.1.11",
          "form": "그냥 집에 자리차지나 하지..",
          "original_form": "그냥 집에 자리차지나 하지..",
          "speaker_id": "2",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100040394.1.12",
          "form": "앨범 안사면 찐팬아니라는 그 취급",
          "original_form": "앨범 안사면 찐팬아니라는 그 취급",
          "speaker_id": "1",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100040394.1.13",
          "form": "맞아요!!!!!!",
          "original_form": "맞아요!!!!!!",
          "speaker_id": "2",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100040394.1.14",
          "form": "맞아요 쓸모도없는더",
          "original_form": "맞아요 쓸모도없는더",
          "speaker_id": "1",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100040394.1.15",
          "form": "근데 정말 잘하신것같아요 앨범사고 굿즈사고 했던거 진짜 후회돼요",
          "original_form": "근데 정말 잘하신것같아요 앨범사고 굿즈사고 했던거 진짜 후회돼요",
          "speaker_id": "1",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100040394.1.16",
          "form": "돈만쓰고 쓰레기만 늘구",
          "original_form": "돈만쓰고 쓰레기만 늘구",
          "speaker_id": "1",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100040394.1.17",
          "form": "먼 자기 덕질은 뫄뫄를 향한 아가페적인 사랑이고 돈안쓰는 가성비덕질은 가짜덕질인감 ㅋㅋㅋ ㅜ",
          "original_form": "먼 자기 덕질은 뫄뫄를 향한 아가페적인 사랑이고 돈안쓰는 가성비덕질은 가짜덕질인감 ㅋㅋㅋ ㅜ",
          "speaker_id": "2",
          "time": "20210904 18:04"
        },
        {
          "id": "MDRW2100040394.1.18",
          "form": "저는 원래 성향이 쓸모없는 거엔 돈 안 쓰는 성향이거든요 ㅋㅋㅋㅋ 예쁜쓰레기는 그냥 쓰레기일 뿐",
          "original_form": "저는 원래 성향이 쓸모없는 거엔 돈 안 쓰는 성향이거든요 ㅋㅋㅋㅋ 예쁜쓰레기는 그냥 쓰레기일 뿐",
          "speaker_id": "2",
          "time": "20210904 18:04"
        },
        {
          "id": "MDRW2100040394.1.19",
          "form": "인피니트 덕질할땐",
          "original_form": "인피니트 덕질할땐",
          "speaker_id": "2",
          "time": "20210904 18:04"
        },
        {
          "id": "MDRW2100040394.1.20",
          "form": "그래도 앨범 하나씩은 사고",
          "original_form": "그래도 앨범 하나씩은 사고",
          "speaker_id": "2",
          "time": "20210904 18:04"
        },
        {
          "id": "MDRW2100040394.1.21",
          "form": "콘서트하면 포스터 샀었는데",
          "original_form": "콘서트하면 포스터 샀었는데",
          "speaker_id": "2",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100040394.1.22",
          "form": "인피니트 탈덕하니까 그게 너무 아깝더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "인피니트 탈덕하니까 그게 너무 아깝더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100040394.1.23",
          "form": "팔리지도 않고",
          "original_form": "팔리지도 않고",
          "speaker_id": "2",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100040394.1.24",
          "form": "아 진짜에요 어이없어요 돈써야 찐팬취급",
          "original_form": "아 진짜에요 어이없어요 돈써야 찐팬취급",
          "speaker_id": "1",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100040394.1.25",
          "form": "아 맞아요 ㅠㅠㅠㅠㅠ",
          "original_form": "아 맞아요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100040394.1.26",
          "form": "그리구 포스터 사면 또 아끼느라 안쓰는데",
          "original_form": "그리구 포스터 사면 또 아끼느라 안쓰는데",
          "speaker_id": "1",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100040394.1.27",
          "form": "차라리 진작 벽에 붙여버릴걸 그랬어요",
          "original_form": "차라리 진작 벽에 붙여버릴걸 그랬어요",
          "speaker_id": "1",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100040394.1.28",
          "form": "그래서 엑소 좋아할땐 돈 안썼어요 ㅎㅎ",
          "original_form": "그래서 엑소 좋아할땐 돈 안썼어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100040394.1.29",
          "form": "맞아욬ㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100040394.1.30",
          "form": "맨날 돌돌ㅇ말아서 지관통에 보관만 하구..",
          "original_form": "맨날 돌돌ㅇ말아서 지관통에 보관만 하구..",
          "speaker_id": "2",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100040394.1.31",
          "form": "또 사이즈도 커갖고 꺼내보기도 귀찮고 ㅋㅋㅋㅋㅋ",
          "original_form": "또 사이즈도 커갖고 꺼내보기도 귀찮고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100040394.1.32",
          "form": "잘하셨어요 진짜 ㅋㅋㅋ 이게 겪어봐야 자제하게되더라구요 ㅋㅋㅋㅋ",
          "original_form": "잘하셨어요 진짜 ㅋㅋㅋ 이게 겪어봐야 자제하게되더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100040394.1.33",
          "form": "글구 그냥 분위기 자체가 다 사는 분위기니까 팬이면 필요없어두 사야될것같구..",
          "original_form": "글구 그냥 분위기 자체가 다 사는 분위기니까 팬이면 필요없어두 사야될것같구..",
          "speaker_id": "1",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100040394.1.34",
          "form": "name1님이랑 이렇게 얘기하니까 넘 속시원하고 재밌네요 ㅋㅋㅋㅋㅋ",
          "original_form": "&name1&님이랑 이렇게 얘기하니까 넘 속시원하고 재밌네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100040394.1.35",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100040394.1.36",
          "form": "진짜요 저도 속시원해요 ㅋㅋㅋㅋㅋ",
