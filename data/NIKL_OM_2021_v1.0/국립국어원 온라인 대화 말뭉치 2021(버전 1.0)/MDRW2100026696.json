      "utterance": [
        {
          "id": "MDRW2100026696.1.1",
          "form": "아 토요일 확진?",
          "original_form": "아 토요일 확진?",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026696.1.2",
          "form": "이겅가",
          "original_form": "이겅가",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026696.1.3",
          "form": "얼마안댐 아",
          "original_form": "얼마안댐 아",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026696.1.4",
          "form": "아 저번주요?",
          "original_form": "아 저번주요?",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026696.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026696.1.6",
          "form": "울고불고 난리남",
          "original_form": "울고불고 난리남",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026696.1.7",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026696.1.8",
          "form": "그럴만도,,",
          "original_form": "그럴만도,,",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026696.1.9",
          "form": "회사어쩔꺼임",
          "original_form": "회사어쩔꺼임",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026696.1.10",
          "form": "회사다닐꺼아녜요",
          "original_form": "회사다닐꺼아녜요",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026696.1.11",
          "form": "가족들도그렇고",
          "original_form": "가족들도그렇고",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026696.1.12",
          "form": "예체능이라",
          "original_form": "예체능이라",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026696.1.13",
          "form": "알바식? 그런 느낌으로 일다니고",
          "original_form": "알바식? 그런 느낌으로 일다니고",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026696.1.14",
          "form": "프리랜서구나",
          "original_form": "프리랜서구나",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026696.1.15",
          "form": "자취해서ㅋ",
          "original_form": "자취해서ㅋ",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026696.1.16",
          "form": "그나마 다행",
          "original_form": "그나마 다행",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026696.1.17",
          "form": "그러네",
          "original_form": "그러네",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026696.1.18",
          "form": "저 동선겹쳐서 검사한번받앗엇는데",
          "original_form": "저 동선겹쳐서 검사한번받앗엇는데",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026696.1.19",
          "form": "난리낫엇음 ㅋㅋㅋㅋ완전대역죄인",
          "original_form": "난리낫엇음 ㅋㅋㅋㅋ완전대역죄인",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026696.1.20",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026696.1.21",
          "form": "저 친구가",
          "original_form": "저 친구가",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026696.1.22",
          "form": "확진자랑 동선 겹쳣는데",
          "original_form": "확진자랑 동선 겹쳣는데",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026696.1.23",
          "form": "검사결과도아직 안나왓는데 무슨 이미 확진받은사람취급함 ㅠ",
          "original_form": "검사결과도아직 안나왓는데 무슨 이미 확진받은사람취급함 ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026696.1.24",
          "form": "하필 그때 얘랑 다같이 밥먹음",
          "original_form": "하필 그때 얘랑 다같이 밥먹음",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026696.1.25",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026696.1.26",
          "form": "ㄱH쫄림",
          "original_form": "ㄱH쫄림",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026696.1.27",
          "form": "친구가 양성안떳음됏죠뭐",
          "original_form": "친구가 양성안떳음됏죠뭐",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026696.1.28",
          "form": "검사개아프죠",
          "original_form": "검사개아프죠",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026696.1.29",
          "form": "후",
          "original_form": "후",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026696.1.30",
          "form": "에이 뭐 콧구녕 쑤시는 정도는뭐",
          "original_form": "에이 뭐 콧구녕 쑤시는 정도는뭐",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026696.1.31",
          "form": "해쥬는사람이 살살해쥬면안아츰",
          "original_form": "해쥬는사람이 살살해쥬면안아츰",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026696.1.32",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026696.1.33",
          "form": "푹푹 쑤,시는 사람들잇늠",
          "original_form": "푹푹 쑤,시는 사람들잇늠",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026696.1.34",
          "form": "저는한계가왓네뇨",
          "original_form": "저는한계가왓네뇨",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026696.1.35",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026696.1.36",
          "form": "굿밤",
          "original_form": "굿밤",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026696.1.37",
          "form": "너무쑤신다",
          "original_form": "너무쑤신다",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026696.1.38",
          "form": "아 신데렐라시군요",
          "original_form": "아 신데렐라시군요",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026696.1.39",
          "form": "잘자요",
          "original_form": "잘자요",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026696.1.40",
          "form": "ㅋㅋㅋㅋㅋ그러네 어느 순간부터 채팅 삑나시는구나?",
          "original_form": "ㅋㅋㅋㅋㅋ그러네 어느 순간부터 채팅 삑나시는구나?",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026696.1.41",
          "form": "어제오래버팀 ㅋㅋㅋㅋㅋ 11시면 졸리던데ㅠ",
          "original_form": "어제오래버팀 ㅋㅋㅋㅋㅋ 11시면 졸리던데ㅠ",
          "speaker_id": "1",
          "time": "20210902 06:36"
        },
        {
          "id": "MDRW2100026696.1.42",
          "form": "ㅋㅋㅋㅋ군대체질인데요?",
          "original_form": "ㅋㅋㅋㅋ군대체질인데요?",
