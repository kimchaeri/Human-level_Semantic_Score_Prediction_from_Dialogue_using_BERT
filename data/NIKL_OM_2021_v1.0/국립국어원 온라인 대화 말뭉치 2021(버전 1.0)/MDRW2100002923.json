      "utterance": [
        {
          "id": "MDRW2100002923.1.1",
          "form": "최근에 중고거래 한 적 있어?",
          "original_form": "최근에 중고거래 한 적 있어?",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002923.1.2",
          "form": "당근이나 중고나라에서",
          "original_form": "당근이나 중고나라에서",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002923.1.3",
          "form": "보기만 했지 구매는 한적 없어",
          "original_form": "보기만 했지 구매는 한적 없어",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002923.1.4",
          "form": "너는 있어?",
          "original_form": "너는 있어?",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002923.1.5",
          "form": "나는 갤럭시 버즈 프로 인터넷으로 사서",
          "original_form": "나는 갤럭시 버즈 프로 인터넷으로 사서",
          "speaker_id": "1",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002923.1.6",
          "form": "그때 폰 사고 할인쿠폰으로 산거여서",
          "original_form": "그때 폰 사고 할인쿠폰으로 산거여서",
          "speaker_id": "1",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002923.1.7",
          "form": "새상품 팔았어",
          "original_form": "새상품 팔았어",
          "speaker_id": "1",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002923.1.8",
          "form": "오오 이득봤네",
          "original_form": "오오 이득봤네",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002923.1.9",
          "form": "응 몇만원 이득봤어",
          "original_form": "응 몇만원 이득봤어",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002923.1.10",
          "form": "그거말고 내가 쓰던 물건들은",
          "original_form": "그거말고 내가 쓰던 물건들은",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002923.1.11",
          "form": "다른 거래는 해봤어?",
          "original_form": "다른 거래는 해봤어?",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002923.1.12",
          "form": "잘 안팔게되더라",
          "original_form": "잘 안팔게되더라",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002923.1.13",
          "form": "나도 막 팔고 싶은건 있는데 귀찮아서 안하게 되더라",
          "original_form": "나도 막 팔고 싶은건 있는데 귀찮아서 안하게 되더라",
          "speaker_id": "2",
          "time": "20210820 17:01"
        },
        {
          "id": "MDRW2100002923.1.14",
          "form": "난 내가 산 가격보다 내려서 팔기가 싫어",
          "original_form": "난 내가 산 가격보다 내려서 팔기가 싫어",
          "speaker_id": "1",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002923.1.15",
          "form": "난 내가 안쓰는 건 치워버리는게 나은 것 같아서 파는게 좋은 것 같아",
          "original_form": "난 내가 안쓰는 건 치워버리는게 나은 것 같아서 파는게 좋은 것 같아",
          "speaker_id": "2",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002923.1.16",
          "form": "하긴 그래",
          "original_form": "하긴 그래",
          "speaker_id": "1",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002923.1.17",
          "form": "내방에 짐이 넘쳐난다",
          "original_form": "내방에 짐이 넘쳐난다",
          "speaker_id": "1",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002923.1.18",
          "form": "넘쳐나면 좀 치워봐 팔던가",
          "original_form": "넘쳐나면 좀 치워봐 팔던가",
          "speaker_id": "2",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002923.1.19",
          "form": "ㅋㅋ 근데 다 언젠간 필요한거란 말이야",
          "original_form": "ㅋㅋ 근데 다 언젠간 필요한거란 말이야",
          "speaker_id": "1",
          "time": "20210820 17:06"
        }
      ]
