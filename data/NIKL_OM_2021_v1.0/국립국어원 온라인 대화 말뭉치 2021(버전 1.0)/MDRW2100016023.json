      "utterance": [
        {
          "id": "MDRW2100016023.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 08:51"
        },
        {
          "id": "MDRW2100016023.1.2",
          "form": "넵 여행다니는거 좋아합니다!",
          "original_form": "넵 여행다니는거 좋아합니다!",
          "speaker_id": "1",
          "time": "20210826 10:00"
        },
        {
          "id": "MDRW2100016023.1.3",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100016023.1.4",
          "form": "해외는 못가니",
          "original_form": "해외는 못가니",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100016023.1.5",
          "form": "국내로?",
          "original_form": "국내로?",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100016023.1.6",
          "form": "코로나 이후엔 국내여행도 거의 못가긴햇네여ㅠㅠㅠ",
          "original_form": "코로나 이후엔 국내여행도 거의 못가긴햇네여ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100016023.1.7",
          "form": "언제쯤 여행을 가게될까요",
          "original_form": "언제쯤 여행을 가게될까요",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100016023.1.8",
          "form": "코로나 무서워서 원....",
          "original_form": "코로나 무서워서 원....",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100016023.1.9",
          "form": "그니까요ㅠㅠㅠㅠ",
          "original_form": "그니까요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 00:14"
        },
        {
          "id": "MDRW2100016023.1.10",
          "form": "백신도 빨리빨리 맞고 그래야지",
          "original_form": "백신도 빨리빨리 맞고 그래야지",
          "speaker_id": "1",
          "time": "20210827 00:14"
        },
        {
          "id": "MDRW2100016023.1.11",
          "form": "여행도 가고 할텐데요ㅠ",
          "original_form": "여행도 가고 할텐데요ㅠ",
          "speaker_id": "1",
          "time": "20210827 00:14"
        },
        {
          "id": "MDRW2100016023.1.12",
          "form": "지금도 가는사람이 있긴하더라구요 극소수지만",
          "original_form": "지금도 가는사람이 있긴하더라구요 극소수지만",
          "speaker_id": "2",
          "time": "20210827 06:15"
        },
        {
          "id": "MDRW2100016023.1.13",
          "form": "ㅋㅋㅋㅋ저도 국내여행은 짧게 다녀온적은 잇어요ㅠ",
          "original_form": "ㅋㅋㅋㅋ저도 국내여행은 짧게 다녀온적은 잇어요ㅠ",
          "speaker_id": "1",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100016023.1.14",
          "form": "요즘 해외여행은 거의 없죠,,,?",
          "original_form": "요즘 해외여행은 거의 없죠,,,?",
          "speaker_id": "1",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100016023.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210828 08:54"
        },
        {
          "id": "MDRW2100016023.1.16",
          "form": "해외는 여행말고 비지니스면 모를까",
          "original_form": "해외는 여행말고 비지니스면 모를까",
          "speaker_id": "2",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100016023.1.17",
          "form": "잘 못가죠",
          "original_form": "잘 못가죠",
          "speaker_id": "2",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100016023.1.18",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100016023.1.19",
          "form": "어디 출장가는거 아닌 이상에는",
          "original_form": "어디 출장가는거 아닌 이상에는",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100016023.1.20",
          "form": "휴가로 가기엔 무리가 있는 것 같아요",
          "original_form": "휴가로 가기엔 무리가 있는 것 같아요",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100016023.1.21",
          "form": "언제 여행을 갈까나",
          "original_form": "언제 여행을 갈까나",
          "speaker_id": "2",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100016023.1.22",
          "form": "지금까지 어디 다녀오셨어요?",
          "original_form": "지금까지 어디 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100016023.1.23",
          "form": "국내는 여기저기 댕겨봤어용ㅎㅎㅎ",
          "original_form": "국내는 여기저기 댕겨봤어용ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100016023.1.24",
          "form": "전국팔도 한번씩은 다 가본거같네요!",
          "original_form": "전국팔도 한번씩은 다 가본거같네요!",
          "speaker_id": "1",
          "time": "20210828 15:52"
        }
      ]
