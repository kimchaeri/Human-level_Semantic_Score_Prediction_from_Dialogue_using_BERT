      "utterance": [
        {
          "id": "MDRW2100045172.1.1",
          "form": "엘소드 진짜 옛날에 엄청 했는데",
          "original_form": "엘소드 진짜 옛날에 엄청 했는데",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045172.1.2",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045172.1.3",
          "form": "지금은 들어가면 뭐가 뭔지 모를것 같아요",
          "original_form": "지금은 들어가면 뭐가 뭔지 모를것 같아요",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045172.1.4",
          "form": "이브 너무 좋아했어요",
          "original_form": "이브 너무 좋아했어요",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045172.1.5",
          "form": "마클이란게 나왔대요",
          "original_form": "마클이란게 나왔대요",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045172.1.6",
          "form": "마스터 클래스",
          "original_form": "마스터 클래스",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045172.1.7",
          "form": "그건 또 뭐예요?",
          "original_form": "그건 또 뭐예요?",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045172.1.8",
          "form": "몰라요 저도 안한지 좀 됨",
          "original_form": "몰라요 저도 안한지 좀 됨",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045172.1.9",
          "form": "각성이겠죠",
          "original_form": "각성이겠죠",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045172.1.10",
          "form": "4차나 내지 이브 3차 디자인 구려요",
          "original_form": "4차나 내지 이브 3차 디자인 구려요",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045172.1.11",
          "form": "항상 마지막인것처럼 전직 내더니",
          "original_form": "항상 마지막인것처럼 전직 내더니",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045172.1.12",
          "form": "마지막에 마지막에 마지막인가",
          "original_form": "마지막에 마지막에 마지막인가",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045172.1.13",
          "form": "또 전직권으로 돈 벌어먹으려나봐요",
          "original_form": "또 전직권으로 돈 벌어먹으려나봐요",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045172.1.14",
          "form": "마클은 디자인은 안바뀌어요",
          "original_form": "마클은 디자인은 안바뀌어요",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045172.1.15",
          "form": "근대 디자인 구리니까 4차내면 좋겠어요 저는 그냥",
          "original_form": "근대 디자인 구리니까 4차내면 좋겠어요 저는 그냥",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045172.1.16",
          "form": "돈을 공짜로 벌려고 노력했네요",
          "original_form": "돈을 공짜로 벌려고 노력했네요",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045172.1.17",
          "form": "아 디자인으로 먹고 살면서",
          "original_form": "아 디자인으로 먹고 살면서",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045172.1.18",
          "form": "넘하네 ;",
          "original_form": "넘하네 ;",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045172.1.19",
          "form": "신캐팔이만 하면서 ㄹㅇ 넘하죠",
          "original_form": "신캐팔이만 하면서 ㄹㅇ 넘하죠",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045172.1.20",
          "form": "아 설마 신캐도 나왔어요?",
          "original_form": "아 설마 신캐도 나왔어요?",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045172.1.21",
          "form": "제가 했을 때는 그 라비? 까지 있었는데",
          "original_form": "제가 했을 때는 그 라비? 까지 있었는데",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045172.1.22",
          "form": "아 귀엽긴 하더라구요",
          "original_form": "아 귀엽긴 하더라구요",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045172.1.23",
          "form": "그래서 그 캐릭터도 하실거예요?",
          "original_form": "그래서 그 캐릭터도 하실거예요?",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045172.1.24",
          "form": "아뇨..",
          "original_form": "아뇨..",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045172.1.25",
          "form": "근대 이브 4라인 ㄹㅇ 사기래요",
          "original_form": "근대 이브 4라인 ㄹㅇ 사기래요",
          "speaker_id": "2",
