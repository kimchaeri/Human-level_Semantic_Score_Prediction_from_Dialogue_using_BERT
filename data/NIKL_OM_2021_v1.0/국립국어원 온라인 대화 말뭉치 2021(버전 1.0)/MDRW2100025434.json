      "utterance": [
        {
          "id": "MDRW2100025434.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100025434.1.2",
          "form": "저녁드셨나요?!",
          "original_form": "저녁드셨나요?!",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100025434.1.3",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100025434.1.4",
          "form": "어떤거드실예정이세요?!",
          "original_form": "어떤거드실예정이세요?!",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100025434.1.5",
          "form": "저 마라탕 먹으려구요",
          "original_form": "저 마라탕 먹으려구요",
          "speaker_id": "2",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100025434.1.6",
          "form": "드셔본적있나요?",
          "original_form": "드셔본적있나요?",
          "speaker_id": "2",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100025434.1.7",
          "form": "헉 좋아해요 마라탕",
          "original_form": "헉 좋아해요 마라탕",
          "speaker_id": "1",
          "time": "20210903 19:17"
        },
        {
          "id": "MDRW2100025434.1.8",
          "form": "대신맵기를 낮게해서 먹어요",
          "original_form": "대신맵기를 낮게해서 먹어요",
          "speaker_id": "1",
          "time": "20210903 19:17"
        },
        {
          "id": "MDRW2100025434.1.9",
          "form": "맛있을거 같아요",
          "original_form": "맛있을거 같아요",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100025434.1.10",
          "form": "매운거 조아해서ㅋㅋㅋ",
          "original_form": "매운거 조아해서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100025434.1.11",
          "form": "배달시켜드시나요??",
          "original_form": "배달시켜드시나요??",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100025434.1.12",
          "form": "가서드시나요??",
          "original_form": "가서드시나요??",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100025434.1.13",
          "form": "가서 먹어요",
          "original_form": "가서 먹어요",
          "speaker_id": "2",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100025434.1.14",
          "form": "배달은 집에서 냄새가 많이난다구 해서ㅎㅎ",
          "original_form": "배달은 집에서 냄새가 많이난다구 해서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100025434.1.15",
          "form": "마라탕맛집이있나요?!",
          "original_form": "마라탕맛집이있나요?!",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100025434.1.16",
          "form": "지방이라 몇개 없어요ㅋㅋㅋ 제일 평 좋은데 가보려구요",
          "original_form": "지방이라 몇개 없어요ㅋㅋㅋ 제일 평 좋은데 가보려구요",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100025434.1.17",
          "form": "꿔바로우도 같이드시면",
          "original_form": "꿔바로우도 같이드시면",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100025434.1.18",
          "form": "정말맛있을거에요",
          "original_form": "정말맛있을거에요",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100025434.1.19",
          "form": "오ㅋㅋㅋ 시켜볼게요",
          "original_form": "오ㅋㅋㅋ 시켜볼게요",
          "speaker_id": "2",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100025434.1.20",
          "form": "꼭 입에맞으시길바라요!",
          "original_form": "꼭 입에맞으시길바라요!",
          "speaker_id": "1",
          "time": "20210903 19:36"
        }
      ]
