      "utterance": [
        {
          "id": "MDRW2100028961.1.1",
          "form": "인터넷 하면서 보는기능일까요?",
          "original_form": "인터넷 하면서 보는기능일까요?",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028961.1.2",
          "form": "네 화면 분할이라고 해야되나",
          "original_form": "네 화면 분할이라고 해야되나",
          "speaker_id": "1",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028961.1.3",
          "form": "넷플릭스 보다가 톡오면 넷플릭스 나와서 톡보내고 다시 켜고 이래야되서 넘 불편하더라고요ㅠ",
          "original_form": "넷플릭스 보다가 톡오면 넷플릭스 나와서 톡보내고 다시 켜고 이래야되서 넘 불편하더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028961.1.4",
          "form": "안드도 넷플은  분할이 안되나요?!",
          "original_form": "안드도 넷플은  분할이 안되나요?!",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028961.1.5",
          "form": "네ㅠㅠ 티빙도 되고 웨이브도 되는데 넷플은 안되네요ㅠㅠ",
          "original_form": "네ㅠㅠ 티빙도 되고 웨이브도 되는데 넷플은 안되네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028961.1.6",
          "form": "헐 그럼 플립 쫌 땡기는데요 ㅋㅋㅋ",
          "original_form": "헐 그럼 플립 쫌 땡기는데요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028961.1.7",
          "form": "넷플릭스로 보는거 있으신가요?",
          "original_form": "넷플릭스로 보는거 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028961.1.8",
          "form": "브리저튼 잼게 봤어요",
          "original_form": "브리저튼 잼게 봤어요",
          "speaker_id": "2",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028961.1.9",
          "form": "님은요?!",
          "original_form": "님은요?!",
          "speaker_id": "2",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028961.1.10",
          "form": "저는 퀸즈갬빗 재미있더라고요!",
          "original_form": "저는 퀸즈갬빗 재미있더라고요!",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028961.1.11",
          "form": "오 그거 1편 중간까진가 보다 진도를 못냈어요",
          "original_form": "오 그거 1편 중간까진가 보다 진도를 못냈어요",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028961.1.12",
          "form": "평좋더라구요",
          "original_form": "평좋더라구요",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028961.1.13",
          "form": "네 저도 1편까진 하차할려고 했는데 계속보니까 재미있더라고요",
          "original_form": "네 저도 1편까진 하차할려고 했는데 계속보니까 재미있더라고요",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028961.1.14",
          "form": "비밀의 숲은 보셨나요? 이것도 평 좋더라고요",
          "original_form": "비밀의 숲은 보셨나요? 이것도 평 좋더라고요",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028961.1.15",
          "form": "오 함시도를 더해바야",
          "original_form": "오 함시도를 더해바야",
          "speaker_id": "2",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028961.1.16",
          "form": "못봣어요 ㅜㅜ",
          "original_form": "못봣어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028961.1.17",
          "form": "보이스인가 것도 인기던데",
          "original_form": "보이스인가 것도 인기던데",
          "speaker_id": "2",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028961.1.18",
          "form": "뭐 유명한거 많이 못봤네요",
          "original_form": "뭐 유명한거 많이 못봤네요",
          "speaker_id": "2",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028961.1.19",
          "form": "브리저튼 말고 다른건 뭐 보셨어요?",
          "original_form": "브리저튼 말고 다른건 뭐 보셨어요?",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028961.1.20",
          "form": "퀸스갬빗 인가도 인기던데요",
          "original_form": "퀸스갬빗 인가도 인기던데요",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028961.1.21",
          "form": "1편 중간 보다 집중이안대서 ㅋㅋ",
          "original_form": "1편 중간 보다 집중이안대서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028961.1.22",
          "form": "ㅋㅋㅋ무슨 장르 드라마 좋아하세요?",
          "original_form": "ㅋㅋㅋ무슨 장르 드라마 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100028961.1.23",
          "form": "도깨비도 재밋었고",
          "original_form": "도깨비도 재밋었고",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100028961.1.24",
          "form": "아 요즘 dp 인기던데요",
          "original_form": "아 요즘 dp 인기던데요",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100028961.1.25",
          "form": "아 dp 인기 많더라고요",
          "original_form": "아 dp 인기 많더라고요",
          "speaker_id": "1",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100028961.1.26",
          "form": "언제 함 봐야겠어요",
          "original_form": "언제 함 봐야겠어요",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100028961.1.27",
          "form": "아 이제 곧 한국드라마 많이 빠진다더라구요",
          "original_form": "아 이제 곧 한국드라마 많이 빠진다더라구요",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100028961.1.28",
          "form": "괜찮아 사랑이야나 응답하라 시리즈 다들 없어진다고 봤어요ㅠ",
          "original_form": "괜찮아 사랑이야나 응답하라 시리즈 다들 없어진다고 봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100028961.1.29",
          "form": "저 응사 한번도 안봐따는 ㅋㅋㅋㅋㅋ",
          "original_form": "저 응사 한번도 안봐따는 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100028961.1.30",
          "form": "응답 시리즈",
          "original_form": "응답 시리즈",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100028961.1.31",
          "form": "박보검 나오는거 보고싶긴 한데",
          "original_form": "박보검 나오는거 보고싶긴 한데",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100028961.1.32",
          "form": "스포다당함",
          "original_form": "스포다당함",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100028961.1.33",
          "form": "응답시리즈는 너무 남편을 헷갈리게 만들어서 증말",
          "original_form": "응답시리즈는 너무 남편을 헷갈리게 만들어서 증말",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100028961.1.34",
          "form": "근데 혜리가 현실에선 다른분과 사겨서 ㅎㅎ",
          "original_form": "근데 혜리가 현실에선 다른분과 사겨서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100028961.1.35",
          "form": "괜사 도경수가 연기를 잘하더라구요",
          "original_form": "괜사 도경수가 연기를 잘하더라구요",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100028961.1.36",
          "form": "ㅋㅋㅋ맞아요 도경수가 괜사에서 연기를 잘하더라고요",
          "original_form": "ㅋㅋㅋ맞아요 도경수가 괜사에서 연기를 잘하더라고요",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100028961.1.37",
          "form": "괜사 보셨어요?",
          "original_form": "괜사 보셨어요?",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100028961.1.38",
          "form": "네근데 쫌 보다 말았을꺼에요 기억안나능거 보면 ㅋㅋ",
          "original_form": "네근데 쫌 보다 말았을꺼에요 기억안나능거 보면 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100028961.1.39",
          "form": "괜사도 잘만든 드라마죠",
          "original_form": "괜사도 잘만든 드라마죠",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100028961.1.40",
          "form": "잘만든 드라마는 시간이 지나고 봐도 괜찮더라고요",
          "original_form": "잘만든 드라마는 시간이 지나고 봐도 괜찮더라고요",
          "speaker_id": "1",
          "time": "20210902 23:35"
        }
      ]
    }
