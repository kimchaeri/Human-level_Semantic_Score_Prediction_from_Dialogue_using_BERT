      "utterance": [
        {
          "id": "MDRW2100021729.1.1",
          "form": "가족과 사이 좋으세요?",
          "original_form": "가족과 사이 좋으세요?",
          "speaker_id": "2",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021729.1.2",
          "form": "동생이랑은 괜찮은 편이에요",
          "original_form": "동생이랑은 괜찮은 편이에요",
          "speaker_id": "1",
          "time": "20210829 05:51"
        },
        {
          "id": "MDRW2100021729.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 05:51"
        },
        {
          "id": "MDRW2100021729.1.4",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "2",
          "time": "20210829 08:11"
        },
        {
          "id": "MDRW2100021729.1.5",
          "form": "그래도 독립하고싶어요ㅋㅋ",
          "original_form": "그래도 독립하고싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 08:11"
        },
        {
          "id": "MDRW2100021729.1.6",
          "form": "아 독립하면 가사가 늘어나잖아요 평소 집안일 잘 하시는 편인가요?",
          "original_form": "아 독립하면 가사가 늘어나잖아요 평소 집안일 잘 하시는 편인가요?",
          "speaker_id": "1",
          "time": "20210829 08:52"
        },
        {
          "id": "MDRW2100021729.1.7",
          "form": "제 방처소만 하는편이지만 자립심이 필요하구나 싶어서요?",
          "original_form": "제 방처소만 하는편이지만 자립심이 필요하구나 싶어서요?",
          "speaker_id": "2",
          "time": "20210829 09:28"
        },
        {
          "id": "MDRW2100021729.1.8",
          "form": "진취적이시네요",
          "original_form": "진취적이시네요",
          "speaker_id": "1",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100021729.1.9",
          "form": "앗 아닙니당",
          "original_form": "앗 아닙니당",
          "speaker_id": "2",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100021729.1.10",
          "form": "사실 막 지내보고 싶어서예요ㅋㅋㅋ",
          "original_form": "사실 막 지내보고 싶어서예요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:38"
        },
        {
          "id": "MDRW2100021729.1.11",
          "form": "자유도 독립생활의 장점이죠",
          "original_form": "자유도 독립생활의 장점이죠",
          "speaker_id": "1",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100021729.1.12",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100021729.1.13",
          "form": "오늘 날씨가 너무 좋네요 밀린 집안일 해야겠어요",
          "original_form": "오늘 날씨가 너무 좋네요 밀린 집안일 해야겠어요",
          "speaker_id": "1",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100021729.1.14",
          "form": "나이도 들어가니 더 그렇네요",
          "original_form": "나이도 들어가니 더 그렇네요",
          "speaker_id": "2",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100021729.1.15",
          "form": "저도 날 좋을때 빨래 해야겠어욪,",
          "original_form": "저도 날 좋을때 빨래 해야겠어욪,",
          "speaker_id": "2",
          "time": "20210829 13:40"
        },
        {
          "id": "MDRW2100021729.1.16",
          "form": "날 좋을 때 집안일은 역시 빨래죠",
          "original_form": "날 좋을 때 집안일은 역시 빨래죠",
          "speaker_id": "1",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100021729.1.17",
          "form": "맞아요~  한동안 비와서 꿉꿉해 혼났어요",
          "original_form": "맞아요~  한동안 비와서 꿉꿉해 혼났어요",
