      "utterance": [
        {
          "id": "MDRW2100019327.1.1",
          "form": "걱정이 되겠어요",
          "original_form": "걱정이 되겠어요",
          "speaker_id": "2",
          "time": "20210827 17:31"
        },
        {
          "id": "MDRW2100019327.1.2",
          "form": "네ㅠㅠ 갑자기 무슨일이죠",
          "original_form": "네ㅠㅠ 갑자기 무슨일이죠",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019327.1.3",
          "form": "그러게요 검사받아보시고 별일 아니었으면",
          "original_form": "그러게요 검사받아보시고 별일 아니었으면",
          "speaker_id": "2",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019327.1.4",
          "form": "그럴거얌",
          "original_form": "그럴거얌",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019327.1.5",
          "form": "이제준이랑 밥먹고 돌아간다 걸어서",
          "original_form": "이제준이랑 밥먹고 돌아간다 걸어서",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019327.1.6",
          "form": "가까운 병원인가바여",
          "original_form": "가까운 병원인가바여",
          "speaker_id": "2",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019327.1.7",
          "form": "네 좋른강안병원",
          "original_form": "네 좋른강안병원",
          "speaker_id": "1",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019327.1.8",
          "form": "하루 편히 쉬시고 금방 퇴원하시길!",
          "original_form": "하루 편히 쉬시고 금방 퇴원하시길!",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019327.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100019327.1.10",
          "form": "며칠 좀 쉬셨으면",
          "original_form": "며칠 좀 쉬셨으면",
          "speaker_id": "1",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100019327.1.11",
          "form": "엄마 아프면 넘 서러우ㅜ",
          "original_form": "엄마 아프면 넘 서러우ㅜ",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019327.1.12",
          "form": "맞아요 왜케 아픈 가족이많죠ㅠ",
          "original_form": "맞아요 왜케 아픈 가족이많죠ㅠ",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019327.1.13",
          "form": "아빠두 엄마두 봄이두ㅠ",
          "original_form": "아빠두 엄마두 봄이두ㅠ",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019327.1.14",
          "form": "나두ㅠㅜ",
          "original_form": "나두ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019327.1.15",
          "form": "건강합시다 모두모두",
          "original_form": "건강합시다 모두모두",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019327.1.16",
          "form": "그럽시다",
          "original_form": "그럽시다",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019327.1.17",
          "form": "돌아와서 청소기 돌렸어요",
          "original_form": "돌아와서 청소기 돌렸어요",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019327.1.18",
          "form": "역시 부지런한 집안일 요정",
          "original_form": "역시 부지런한 집안일 요정",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019327.1.19",
          "form": "만렙된거같아요 흑흑",
          "original_form": "만렙된거같아요 흑흑",
          "speaker_id": "1",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100019327.1.20",
          "form": "손목아파요",
          "original_form": "손목아파요",
