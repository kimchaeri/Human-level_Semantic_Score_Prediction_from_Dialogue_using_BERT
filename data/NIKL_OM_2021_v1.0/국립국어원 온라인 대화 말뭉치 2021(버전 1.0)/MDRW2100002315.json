      "utterance": [
        {
          "id": "MDRW2100002315.1.1",
          "form": "케이쇼핑 다음달에 뭐사지 ㅋㅋㅋ",
          "original_form": "케이쇼핑 다음달에 뭐사지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:16"
        },
        {
          "id": "MDRW2100002315.1.2",
          "form": "벌써 고민 ㅋㅋㅋ",
          "original_form": "벌써 고민 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:16"
        },
        {
          "id": "MDRW2100002315.1.3",
          "form": "맞다!!!",
          "original_form": "맞다!!!",
          "speaker_id": "2",
          "time": "20210820 09:20"
        },
        {
          "id": "MDRW2100002315.1.4",
          "form": "이번달 새우 잘샀는데 다음달 머사지",
          "original_form": "이번달 새우 잘샀는데 다음달 머사지",
          "speaker_id": "2",
          "time": "20210820 09:20"
        },
        {
          "id": "MDRW2100002315.1.5",
          "form": "그니까 새우 통통하고 실하더라 ㅋㅋㅋㅋ",
          "original_form": "그니까 새우 통통하고 실하더라 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:21"
        },
        {
          "id": "MDRW2100002315.1.6",
          "form": "다음달꺼 미리 좀 생각해야겠다 ㅋㅋㅋ",
          "original_form": "다음달꺼 미리 좀 생각해야겠다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:21"
        },
        {
          "id": "MDRW2100002315.1.7",
          "form": "다음달에 제철과일 사고싶은데",
          "original_form": "다음달에 제철과일 사고싶은데",
          "speaker_id": "2",
          "time": "20210820 09:22"
        },
        {
          "id": "MDRW2100002315.1.8",
          "form": "머 과일 나오려나",
          "original_form": "머 과일 나오려나",
          "speaker_id": "2",
          "time": "20210820 09:22"
        },
        {
          "id": "MDRW2100002315.1.9",
          "form": "9월달 제철 과일???",
          "original_form": "9월달 제철 과일???",
          "speaker_id": "1",
          "time": "20210820 09:22"
        },
        {
          "id": "MDRW2100002315.1.10",
          "form": "뭐있지 ㅋㅋㅋㅋ 나도 몇일전에 제철 과일 쳐봤는데 비슷하더라",
          "original_form": "뭐있지 ㅋㅋㅋㅋ 나도 몇일전에 제철 과일 쳐봤는데 비슷하더라",
          "speaker_id": "1",
          "time": "20210820 09:22"
        },
        {
          "id": "MDRW2100002315.1.11",
          "form": "옥수수 9월이던데",
          "original_form": "옥수수 9월이던데",
          "speaker_id": "1",
          "time": "20210820 09:23"
        },
        {
          "id": "MDRW2100002315.1.12",
          "form": "복숭아랑 뭐도 9월 계속이던가",
          "original_form": "복숭아랑 뭐도 9월 계속이던가",
          "speaker_id": "1",
          "time": "20210820 09:23"
        },
        {
          "id": "MDRW2100002315.1.13",
          "form": "아 복숭아..",
          "original_form": "아 복숭아..",
          "speaker_id": "2",
          "time": "20210820 09:23"
        },
        {
          "id": "MDRW2100002315.1.14",
          "form": "복숭아 이번에 사봤어?",
          "original_form": "복숭아 이번에 사봤어?",
          "speaker_id": "2",
          "time": "20210820 09:23"
        },
        {
          "id": "MDRW2100002315.1.15",
          "form": "응응 이번에 물렁 복숭아 박스로 샀는데",
          "original_form": "응응 이번에 물렁 복숭아 박스로 샀는데",
          "speaker_id": "1",
          "time": "20210820 09:23"
        },
        {
          "id": "MDRW2100002315.1.16",
          "form": "전부 다 상한곳 없이 완전 달달",
          "original_form": "전부 다 상한곳 없이 완전 달달",
          "speaker_id": "1",
          "time": "20210820 09:23"
        },
        {
          "id": "MDRW2100002315.1.17",
          "form": "오ㅎㅎ 물렁복숭아 라고 팔아?",
          "original_form": "오ㅎㅎ 물렁복숭아 라고 팔아?",
          "speaker_id": "2",
          "time": "20210820 09:23"
        },
        {
          "id": "MDRW2100002315.1.18",
          "form": "딱복 이런식으로 물복?",
          "original_form": "딱복 이런식으로 물복?",
          "speaker_id": "2",
          "time": "20210820 09:24"
        },
        {
          "id": "MDRW2100002315.1.19",
          "form": "손에 과즙 줄줄 흘리면서 먹었다 ㅋㅋㅋ",
          "original_form": "손에 과즙 줄줄 흘리면서 먹었다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:24"
        },
        {
          "id": "MDRW2100002315.1.20",
          "form": "아 마트가서 샀어 물렁으로 ㅋㅋㅋㅋ",
          "original_form": "아 마트가서 샀어 물렁으로 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:24"
        },
        {
          "id": "MDRW2100002315.1.21",
          "form": "아아 마트에서ㅎㅎ 물렁이가 맛있지 나도딱복보다는",
          "original_form": "아아 마트에서ㅎㅎ 물렁이가 맛있지 나도딱복보다는",
          "speaker_id": "2",
          "time": "20210820 09:24"
        },
        {
          "id": "MDRW2100002315.1.22",
          "form": "맞아 ㅋㅋㅋ 물렁해야 복숭아",
          "original_form": "맞아 ㅋㅋㅋ 물렁해야 복숭아",
          "speaker_id": "1",
          "time": "20210820 09:25"
        },
        {
          "id": "MDRW2100002315.1.23",
          "form": "복숭아 안 씨앗 빼고",
          "original_form": "복숭아 안 씨앗 빼고",
          "speaker_id": "2",
          "time": "20210820 09:25"
        },
        {
          "id": "MDRW2100002315.1.24",
          "form": "거기에 그릭요거트 넣는게 유행이더라",
          "original_form": "거기에 그릭요거트 넣는게 유행이더라",
          "speaker_id": "2",
          "time": "20210820 09:25"
        },
        {
          "id": "MDRW2100002315.1.25",
          "form": "아아 맞아 !!!",
          "original_form": "아아 맞아 !!!",
          "speaker_id": "1",
          "time": "20210820 09:25"
        },
        {
          "id": "MDRW2100002315.1.26",
          "form": "나는 처음 봤는데 은근 소소한 유행이었나봐",
          "original_form": "나는 처음 봤는데 은근 소소한 유행이었나봐",
          "speaker_id": "1",
          "time": "20210820 09:25"
        },
        {
          "id": "MDRW2100002315.1.27",
          "form": "먹어보고싶긴 하더라",
          "original_form": "먹어보고싶긴 하더라",
          "speaker_id": "1",
          "time": "20210820 09:25"
        },
        {
          "id": "MDRW2100002315.1.28",
          "form": "어제 삼성카드에서 산 타월 왔다 ㅋㅋㅋㅋ",
          "original_form": "어제 삼성카드에서 산 타월 왔다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100002315.1.29",
          "form": "근데 핸드타월이었나...",
          "original_form": "근데 핸드타월이었나...",
          "speaker_id": "1",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100002315.1.30",
          "form": "대충 송월타월만 보고 신청했는데",
          "original_form": "대충 송월타월만 보고 신청했는데",
