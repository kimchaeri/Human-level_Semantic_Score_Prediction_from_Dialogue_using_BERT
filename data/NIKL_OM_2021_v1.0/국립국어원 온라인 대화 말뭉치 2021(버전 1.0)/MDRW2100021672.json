      "utterance": [
        {
          "id": "MDRW2100021672.1.1",
          "form": "일반 단감은 좀 얌전한 맛인데",
          "original_form": "일반 단감은 좀 얌전한 맛인데",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021672.1.2",
          "form": "어렸을 땐 감이 나이먹은 사람들 전용 과일인줄",
          "original_form": "어렸을 땐 감이 나이먹은 사람들 전용 과일인줄",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021672.1.3",
          "form": "알았어ㅋㅋㅋㅋ맛이 너무 단정해서",
          "original_form": "알았어ㅋㅋㅋㅋ맛이 너무 단정해서",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021672.1.4",
          "form": "자극적인 맛을 찾던 시기에는",
          "original_form": "자극적인 맛을 찾던 시기에는",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021672.1.5",
          "form": "그 매력을 몰랐는데",
          "original_form": "그 매력을 몰랐는데",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021672.1.6",
          "form": "단감은 씨앗이 커서",
          "original_form": "단감은 씨앗이 커서",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021672.1.7",
          "form": "먹기편하드라",
          "original_form": "먹기편하드라",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021672.1.8",
          "form": "포도같은 애랑우",
          "original_form": "포도같은 애랑우",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021672.1.9",
          "form": "상대도안돼",
          "original_form": "상대도안돼",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021672.1.10",
          "form": "맞아 씨앗 쏙쏙 빠지는 것도 편하고",
          "original_form": "맞아 씨앗 쏙쏙 빠지는 것도 편하고",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021672.1.11",
          "form": "맛도 뒷맛이 깔끔해서 점점 마음에 들더라",
          "original_form": "맛도 뒷맛이 깔끔해서 점점 마음에 들더라",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021672.1.12",
          "form": "ㅋㅋ발라먹기 귀찮은 과일 순위에",
          "original_form": "ㅋㅋ발라먹기 귀찮은 과일 순위에",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021672.1.13",
          "form": "포도 들지 않을까 싶음",
          "original_form": "포도 들지 않을까 싶음",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021672.1.14",
          "form": "석류도 진짜 발라먹기 헬임",
          "original_form": "석류도 진짜 발라먹기 헬임",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021672.1.15",
          "form": "신기하다 홍시는 어때?",
          "original_form": "신기하다 홍시는 어때?",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021672.1.16",
          "form": "홍시도 좋아해",
          "original_form": "홍시도 좋아해",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021672.1.17",
          "form": "홍시 과일채로 들고 먹는건",
          "original_form": "홍시 과일채로 들고 먹는건",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021672.1.18",
          "form": "싫어하는데",
          "original_form": "싫어하는데",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021672.1.19",
          "form": "큰 컵에 두어 알 정도 발라낸다음에",
          "original_form": "큰 컵에 두어 알 정도 발라낸다음에",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021672.1.20",
          "form": "몰아서 한번에 팍팍 퍼먹으면",
          "original_form": "몰아서 한번에 팍팍 퍼먹으면",
          "speaker_id": "2",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100021672.1.21",
          "form": "낙원이여",
          "original_form": "낙원이여",
          "speaker_id": "2",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100021672.1.22",
          "form": "넌 홍시 좋아하니",
          "original_form": "넌 홍시 좋아하니",
          "speaker_id": "2",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100021672.1.23",
          "form": "난 홍시 별로더라",
          "original_form": "난 홍시 별로더라",
          "speaker_id": "1",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100021672.1.24",
          "form": "홍시가 맛이",
          "original_form": "홍시가 맛이",
          "speaker_id": "1",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100021672.1.25",
          "form": "살짝 떫은게 있어서",
          "original_form": "살짝 떫은게 있어서",
          "speaker_id": "1",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100021672.1.26",
          "form": "난 신맛을 좋아하거든",
          "original_form": "난 신맛을 좋아하거든",
          "speaker_id": "1",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100021672.1.27",
          "form": "오 나도 새콤새콤한 거 좋아해",
          "original_form": "오 나도 새콤새콤한 거 좋아해",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100021672.1.28",
          "form": "너 그럼 자두 좋아하니?",
          "original_form": "너 그럼 자두 좋아하니?",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100021672.1.29",
          "form": "나는 자두  진짜 좋아해",
          "original_form": "나는 자두  진짜 좋아해",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100021672.1.30",
          "form": "난 복숭아 한라봉 이런거 좋아해",
          "original_form": "난 복숭아 한라봉 이런거 좋아해",
          "speaker_id": "1",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100021672.1.31",
          "form": "자두는 뭔가",
          "original_form": "자두는 뭔가",
          "speaker_id": "1",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100021672.1.32",
          "form": "단맛의 빈도가",
          "original_form": "단맛의 빈도가",
          "speaker_id": "1",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100021672.1.33",
          "form": "더 높은 느낌?",
          "original_form": "더 높은 느낌?",
          "speaker_id": "1",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100021672.1.34",
          "form": "맛있어 자두도",
          "original_form": "맛있어 자두도",
          "speaker_id": "1",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100021672.1.35",
          "form": "한라봉도 맛있겠다",
          "original_form": "한라봉도 맛있겠다",
          "speaker_id": "2",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100021672.1.36",
          "form": "한라봉 향이 되게 독특하더라",
          "original_form": "한라봉 향이 되게 독특하더라",
          "speaker_id": "2",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100021672.1.37",
          "form": "약간 시원한 향 나던데",
          "original_form": "약간 시원한 향 나던데",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021672.1.38",
          "form": "귤이나 오렌지랑은 또다른 느낌",
          "original_form": "귤이나 오렌지랑은 또다른 느낌",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021672.1.39",
          "form": "배랑 오렌지 합친거였나",
          "original_form": "배랑 오렌지 합친거였나",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021672.1.40",
          "form": "그렇지않냐",
          "original_form": "그렇지않냐",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021672.1.41",
          "form": "교배종이었던 걸로 기억하는데",
          "original_form": "교배종이었던 걸로 기억하는데",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021672.1.42",
          "form": "있는 거 합쳐서 자꾸 새로운 거 만들어내는 거 뭔가 웃겨",
          "original_form": "있는 거 합쳐서 자꾸 새로운 거 만들어내는 거 뭔가 웃겨",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021672.1.43",
          "form": "가만히 두질 못함",
          "original_form": "가만히 두질 못함",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021672.1.44",
          "form": "근데 문득 드는 생각인데",
          "original_form": "근데 문득 드는 생각인데",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021672.1.45",
          "form": "유전자조작이랑 한라봉같은 거랑은",
          "original_form": "유전자조작이랑 한라봉같은 거랑은",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021672.1.46",
          "form": "다른거니..?",
          "original_form": "다른거니..?",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021672.1.47",
          "form": "모르겠네 근데 품종개량이니까",
          "original_form": "모르겠네 근데 품종개량이니까",
          "speaker_id": "1",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100021672.1.48",
          "form": "아마 유전자 건드리는 쪽일것같아",
          "original_form": "아마 유전자 건드리는 쪽일것같아",
          "speaker_id": "1",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100021672.1.49",
          "form": "그런갑다",
          "original_form": "그런갑다",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021672.1.50",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021672.1.51",
          "form": "어찌 됐든 맛있으면 장땡이여",
          "original_form": "어찌 됐든 맛있으면 장땡이여",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021672.1.52",
          "form": "얘",
          "original_form": "얘",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021672.1.53",
          "form": "나중에 같이",
          "original_form": "나중에 같이",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021672.1.54",
          "form": "맛있는 거 먹으러 가자",
          "original_form": "맛있는 거 먹으러 가자",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021672.1.55",
          "form": "그래 난 카레먹을래",
          "original_form": "그래 난 카레먹을래",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021672.1.56",
          "form": "인도카레에 난먹어야지",
          "original_form": "인도카레에 난먹어야지",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021672.1.57",
          "form": "카레 맛있겠다",
          "original_form": "카레 맛있겠다",
          "speaker_id": "2",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021672.1.58",
          "form": "다음에 인도카레 먹을 때 불러",
          "original_form": "다음에 인도카레 먹을 때 불러",
          "speaker_id": "2",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021672.1.59",
          "form": "같이 돼지파티하자",
          "original_form": "같이 돼지파티하자",
          "speaker_id": "2",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021672.1.60",
          "form": "닭갈비집 어딘가에선",
          "original_form": "닭갈비집 어딘가에선",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021672.1.61",
          "form": "잘살고있기를",
          "original_form": "잘살고있기를",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021672.1.62",
          "form": "언젠가 잊을 때 쯤 다시 나타날 거라 믿어",
          "original_form": "언젠가 잊을 때 쯤 다시 나타날 거라 믿어",
          "speaker_id": "2",
          "time": "20210828 21:52"
        }
      ]
