      "utterance": [
        {
          "id": "MDRW2100024312.1.1",
          "form": "결국 8월에 100그람뺀듯요",
          "original_form": "결국 8월에 100그람뺀듯요",
          "speaker_id": "2",
          "time": "20210831 00:35"
        },
        {
          "id": "MDRW2100024312.1.2",
          "form": "저는 샤브샤브 먹고 꼭 칼국수에ㅋㅋㅋ볶음밥좋아하거든요",
          "original_form": "저는 샤브샤브 먹고 꼭 칼국수에ㅋㅋㅋ볶음밥좋아하거든요",
          "speaker_id": "1",
          "time": "20210831 00:35"
        },
        {
          "id": "MDRW2100024312.1.3",
          "form": "야채랑 고기배터지게먹어서",
          "original_form": "야채랑 고기배터지게먹어서",
          "speaker_id": "2",
          "time": "20210831 00:36"
        },
        {
          "id": "MDRW2100024312.1.4",
          "form": "이건 탄수화물 폭탄이지요?ㅋㅋㅋ이런 조리법은 노노?",
          "original_form": "이건 탄수화물 폭탄이지요?ㅋㅋㅋ이런 조리법은 노노?",
          "speaker_id": "1",
          "time": "20210831 00:36"
        },
        {
          "id": "MDRW2100024312.1.5",
          "form": "칼국수들어갈자리가없어요",
          "original_form": "칼국수들어갈자리가없어요",
          "speaker_id": "2",
          "time": "20210831 00:36"
        },
        {
          "id": "MDRW2100024312.1.6",
          "form": "ㅋㅋㅋㅋ야채가 좋아요",
          "original_form": "ㅋㅋㅋㅋ야채가 좋아요",
          "speaker_id": "1",
          "time": "20210831 00:36"
        },
        {
          "id": "MDRW2100024312.1.7",
          "form": "냉장고에서 생칼국수 코자고있어요",
          "original_form": "냉장고에서 생칼국수 코자고있어요",
          "speaker_id": "2",
          "time": "20210831 00:36"
        },
        {
          "id": "MDRW2100024312.1.8",
          "form": "푹익힌 배추ㅋㅋㅋ",
          "original_form": "푹익힌 배추ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:36"
        },
        {
          "id": "MDRW2100024312.1.9",
          "form": "배추 푹익허",
          "original_form": "배추 푹익허",
          "speaker_id": "2",
          "time": "20210831 00:36"
        },
        {
          "id": "MDRW2100024312.1.10",
          "form": "생칼국수를 안먹다니 ㅋㅋ대박",
          "original_form": "생칼국수를 안먹다니 ㅋㅋ대박",
          "speaker_id": "1",
          "time": "20210831 00:36"
        },
        {
          "id": "MDRW2100024312.1.11",
          "form": "찌 찌뽕",
          "original_form": "찌 찌뽕",
          "speaker_id": "2",
          "time": "20210831 00:36"
        },
        {
          "id": "MDRW2100024312.1.12",
          "form": "배추 푹 익혀서ㅋㅋㅋ소스찍어먹음대박",
          "original_form": "배추 푹 익혀서ㅋㅋㅋ소스찍어먹음대박",
          "speaker_id": "1",
          "time": "20210831 00:36"
        },
        {
          "id": "MDRW2100024312.1.13",
          "form": "최고죠 소스는 사랑이죠",
          "original_form": "최고죠 소스는 사랑이죠",
          "speaker_id": "2",
          "time": "20210831 00:37"
        },
        {
          "id": "MDRW2100024312.1.14",
          "form": "배추의 그 단맛ㅋㅋㅋ역시 미식가ㄲ",
          "original_form": "배추의 그 단맛ㅋㅋㅋ역시 미식가ㄲ",
          "speaker_id": "1",
          "time": "20210831 00:37"
        },
        {
          "id": "MDRW2100024312.1.15",
          "form": "미식가는 다이어트하기너무힘드네요",
          "original_form": "미식가는 다이어트하기너무힘드네요",
          "speaker_id": "2",
          "time": "20210831 00:37"
        },
        {
          "id": "MDRW2100024312.1.16",
          "form": "소스는 뭘로 찍어요?",
          "original_form": "소스는 뭘로 찍어요?",
          "speaker_id": "1",
          "time": "20210831 00:37"
        },
        {
          "id": "MDRW2100024312.1.17",
          "form": "9월엔부디500그람빠지길",
          "original_form": "9월엔부디500그람빠지길",
          "speaker_id": "2",
          "time": "20210831 00:37"
        },
        {
          "id": "MDRW2100024312.1.18",
          "form": "혹시 그냥 간장?",
          "original_form": "혹시 그냥 간장?",
          "speaker_id": "1",
          "time": "20210831 00:37"
        },
        {
          "id": "MDRW2100024312.1.19",
          "form": "참소스와",
          "original_form": "참소스와",
