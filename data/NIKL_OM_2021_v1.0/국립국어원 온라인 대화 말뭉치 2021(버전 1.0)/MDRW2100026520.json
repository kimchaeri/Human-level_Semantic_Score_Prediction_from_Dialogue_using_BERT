      "utterance": [
        {
          "id": "MDRW2100026520.1.1",
          "form": "저는최근에 남색맨투맨 샀어요",
          "original_form": "저는최근에 남색맨투맨 샀어요",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026520.1.2",
          "form": "유와 어디꺼용",
          "original_form": "유와 어디꺼용",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026520.1.3",
          "form": "후아유꺼요!",
          "original_form": "후아유꺼요!",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026520.1.4",
          "form": "귀엽겟다 ㅋㅋㅋ 궁금하네용",
          "original_form": "귀엽겟다 ㅋㅋㅋ 궁금하네용",
          "speaker_id": "2",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026520.1.5",
          "form": "최근에 구매한 물건있으세요?!",
          "original_form": "최근에 구매한 물건있으세요?!",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026520.1.6",
          "form": "바람막이요 ㅋㅋ",
          "original_form": "바람막이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026520.1.7",
          "form": "마사지볼",
          "original_form": "마사지볼",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026520.1.8",
          "form": "바람막이!!!",
          "original_form": "바람막이!!!",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026520.1.9",
          "form": "어디꺼사셨어요?!",
          "original_form": "어디꺼사셨어요?!",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026520.1.10",
          "form": "나이키요! 무난한거 ㅋㅋㅋ",
          "original_form": "나이키요! 무난한거 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026520.1.11",
          "form": "클래식은 영원하죠ㅠ",
          "original_form": "클래식은 영원하죠ㅠ",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026520.1.12",
          "form": "저도 바막사고싶은데 다 비싸더라고요,,",
          "original_form": "저도 바막사고싶은데 다 비싸더라고요,,",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026520.1.13",
          "form": "그죵... 그래듀 세일마니해서삿슴다",
          "original_form": "그죵... 그래듀 세일마니해서삿슴다",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026520.1.14",
          "form": "저번시즌이라 ㅋㅋ",
          "original_form": "저번시즌이라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026520.1.15",
          "form": "무슨색사셨어요?!",
          "original_form": "무슨색사셨어요?!",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026520.1.16",
          "form": "검정이요! ㅋㅋㅋ",
          "original_form": "검정이요! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026520.1.17",
          "form": "밝은거살걸그랫낭",
          "original_form": "밝은거살걸그랫낭",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026520.1.18",
          "form": "심플 베스트! 아뇨 역시검정이죠!!",
          "original_form": "심플 베스트! 아뇨 역시검정이죠!!",
