      "utterance": [
        {
          "id": "MDRW2100024770.1.1",
          "form": "저도 먹는것좋아해요!!!",
          "original_form": "저도 먹는것좋아해요!!!",
          "speaker_id": "1",
          "time": "20210831 18:20"
        },
        {
          "id": "MDRW2100024770.1.2",
          "form": "ㅋㅋㅋ 뭐좋아하세용",
          "original_form": "ㅋㅋㅋ 뭐좋아하세용",
          "speaker_id": "2",
          "time": "20210831 18:20"
        },
        {
          "id": "MDRW2100024770.1.3",
          "form": "치킨이요!!",
          "original_form": "치킨이요!!",
          "speaker_id": "1",
          "time": "20210831 18:20"
        },
        {
          "id": "MDRW2100024770.1.4",
          "form": "우리모두의 치킨!ㅋㅋㅋ 저녁은 드셨나요",
          "original_form": "우리모두의 치킨!ㅋㅋㅋ 저녁은 드셨나요",
          "speaker_id": "2",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100024770.1.5",
          "form": "아니요 아직요ㅜㅠ 9시에 먹을것같아요",
          "original_form": "아니요 아직요ㅜㅠ 9시에 먹을것같아요",
          "speaker_id": "1",
          "time": "20210831 18:56"
        },
        {
          "id": "MDRW2100024770.1.6",
          "form": "배고파요",
          "original_form": "배고파요",
          "speaker_id": "1",
          "time": "20210831 18:56"
        },
        {
          "id": "MDRW2100024770.1.7",
          "form": "헉 일하고계신가요",
          "original_form": "헉 일하고계신가요",
          "speaker_id": "2",
          "time": "20210831 18:56"
        },
        {
          "id": "MDRW2100024770.1.8",
          "form": "일은 아닌데 9시약속이라서요ㅋㅋㅋ",
          "original_form": "일은 아닌데 9시약속이라서요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:56"
        },
        {
          "id": "MDRW2100024770.1.9",
          "form": "아 ㅋㅋㅋ 뭐드시기로 하셨어요",
          "original_form": "아 ㅋㅋㅋ 뭐드시기로 하셨어요",
          "speaker_id": "2",
          "time": "20210831 18:57"
        },
        {
          "id": "MDRW2100024770.1.10",
          "form": "아직안정했어요ㅋㅋ",
          "original_form": "아직안정했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:57"
        },
        {
          "id": "MDRW2100024770.1.11",
          "form": "추천해주세요!",
          "original_form": "추천해주세요!",
          "speaker_id": "1",
          "time": "20210831 18:57"
        },
        {
          "id": "MDRW2100024770.1.12",
          "form": "아 뭐가있지 근데 거긴 10시제한 없나봐용",
          "original_form": "아 뭐가있지 근데 거긴 10시제한 없나봐용",
          "speaker_id": "2",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024770.1.13",
          "form": "밤이니까 치맥 ? 비도오고 이자카야같은데서 나베같은거에 사케한잔...ㅋㅋㅋㅋ",
          "original_form": "밤이니까 치맥 ? 비도오고 이자카야같은데서 나베같은거에 사케한잔...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:59"
        },
        {
          "id": "MDRW2100024770.1.14",
          "form": "앗 둘이서먹어서요ㅋㅋㅋㅋ",
          "original_form": "앗 둘이서먹어서요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:59"
        },
        {
          "id": "MDRW2100024770.1.15",
          "form": "배달음식 시킬건데 어떤게좋을까요???",
