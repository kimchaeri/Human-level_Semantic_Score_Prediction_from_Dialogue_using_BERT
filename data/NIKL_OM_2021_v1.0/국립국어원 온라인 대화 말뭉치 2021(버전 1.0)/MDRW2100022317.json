      "utterance": [
        {
          "id": "MDRW2100022317.1.1",
          "form": "옛날 통닭은 어때요?",
          "original_form": "옛날 통닭은 어때요?",
          "speaker_id": "2",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022317.1.2",
          "form": "나쁘지않아요",
          "original_form": "나쁘지않아요",
          "speaker_id": "1",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022317.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022317.1.4",
          "form": "저는 좀 별로에요",
          "original_form": "저는 좀 별로에요",
          "speaker_id": "2",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022317.1.5",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022317.1.6",
          "form": "길거리에 트럭에 파는 거 뭔지 알아요?",
          "original_form": "길거리에 트럭에 파는 거 뭔지 알아요?",
          "speaker_id": "2",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022317.1.7",
          "form": "음 알아요",
          "original_form": "음 알아요",
          "speaker_id": "1",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022317.1.8",
          "form": "위생이 좀 안좋은 것 같아서요",
          "original_form": "위생이 좀 안좋은 것 같아서요",
          "speaker_id": "2",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022317.1.9",
          "form": "전기구이",
          "original_form": "전기구이",
          "speaker_id": "1",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022317.1.10",
          "form": "그거 저 싫어해요",
          "original_form": "그거 저 싫어해요",
          "speaker_id": "1",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022317.1.11",
          "form": "맛없어서",
          "original_form": "맛없어서",
          "speaker_id": "1",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022317.1.12",
          "form": "전기구이가 젤 별로인 것 같아요 ㅜㅜ",
          "original_form": "전기구이가 젤 별로인 것 같아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022317.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022317.1.14",
          "form": "그쵸 ㅋㅋ 맛없는데 이영자가 한방통닭이라구 해서",
          "original_form": "그쵸 ㅋㅋ 맛없는데 이영자가 한방통닭이라구 해서",
          "speaker_id": "2",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022317.1.15",
          "form": "인기 많아졌어요",
          "original_form": "인기 많아졌어요",
          "speaker_id": "2",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022317.1.16",
          "form": "그 닭 말고",
          "original_form": "그 닭 말고",
          "speaker_id": "1",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022317.1.17",
          "form": "삼겹살은 괜찮은데",
          "original_form": "삼겹살은 괜찮은데",
          "speaker_id": "1",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022317.1.18",
          "form": "아 삼겹살 ㅋㅋ 그거 최고에요",
          "original_form": "아 삼겹살 ㅋㅋ 그거 최고에요",
          "speaker_id": "2",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022317.1.19",
          "form": "닭은... 굽네를먹지 차라리",
          "original_form": "닭은... 굽네를먹지 차라리",
          "speaker_id": "1",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022317.1.20",
          "form": "ㅇㅈ 굽네 오븐치킨이 훨씬 더 나아요 진짜",
          "original_form": "ㅇㅈ 굽네 오븐치킨이 훨씬 더 나아요 진짜",
          "speaker_id": "2",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022317.1.21",
          "form": "근데 요즘 계란 안주네요?",
          "original_form": "근데 요즘 계란 안주네요?",
          "speaker_id": "2",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022317.1.22",
          "form": "그쳐",
          "original_form": "그쳐",
          "speaker_id": "1",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022317.1.23",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022317.1.24",
          "form": "그걸 기억하시다니",
          "original_form": "그걸 기억하시다니",
          "speaker_id": "1",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022317.1.25",
          "form": "예전에 계란줬자나요",
          "original_form": "예전에 계란줬자나요",
          "speaker_id": "1",
          "time": "20210829 12:33"
        }
      ]
