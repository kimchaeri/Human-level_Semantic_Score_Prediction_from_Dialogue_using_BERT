      "utterance": [
        {
          "id": "MDRW2100002198.1.1",
          "form": "받은 선물 중에서 기억남는 선물 있어요?",
          "original_form": "받은 선물 중에서 기억남는 선물 있어요?",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002198.1.2",
          "form": "음 저는 에어팟이요!",
          "original_form": "음 저는 에어팟이요!",
          "speaker_id": "1",
          "time": "20210820 09:01"
        },
        {
          "id": "MDRW2100002198.1.3",
          "form": "상상 못했던 선물이라 더 기억에 남는 거 같아요",
          "original_form": "상상 못했던 선물이라 더 기억에 남는 거 같아요",
          "speaker_id": "1",
          "time": "20210820 09:02"
        },
        {
          "id": "MDRW2100002198.1.4",
          "form": "헉 에어팟 선물이면",
          "original_form": "헉 에어팟 선물이면",
          "speaker_id": "2",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100002198.1.5",
          "form": "저도 기억에 남을 거 같아요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 기억에 남을 거 같아요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100002198.1.6",
          "form": "그쵸 받아서 진짜 잘 썼어요",
          "original_form": "그쵸 받아서 진짜 잘 썼어요",
          "speaker_id": "1",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002198.1.7",
          "form": "재프리님은 어떤 선물이 기억에 남으세요?",
          "original_form": "재프리님은 어떤 선물이 기억에 남으세요?",
          "speaker_id": "1",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002198.1.8",
          "form": "저는 선물보다는 편지 받았을 때 더 기억 남더라구요",
          "original_form": "저는 선물보다는 편지 받았을 때 더 기억 남더라구요",
          "speaker_id": "2",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002198.1.9",
          "form": "편지도 나름 선물이라고 할 수 있겠죠",
          "original_form": "편지도 나름 선물이라고 할 수 있겠죠",
          "speaker_id": "2",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002198.1.10",
          "form": "오 맞아요 편지는 마음이 몽글몽글해요",
          "original_form": "오 맞아요 편지는 마음이 몽글몽글해요",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002198.1.11",
          "form": "최근에 구매한 물건은 뭐 있으세요? 전 방금도 쇼핑하고 왔어요",
          "original_form": "최근에 구매한 물건은 뭐 있으세요? 전 방금도 쇼핑하고 왔어요",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002198.1.12",
          "form": "우와 뭐 쇼핑하고 오셨어요ㅋㅋ!!!",
          "original_form": "우와 뭐 쇼핑하고 오셨어요ㅋㅋ!!!",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002198.1.13",
          "form": "저 아이패드 케이스 샀어요",
          "original_form": "저 아이패드 케이스 샀어요",
          "speaker_id": "1",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002198.1.14",
          "form": "원래 케이스가 낡아서 보라색인데 이제 벗겨져서 흰색이 보여요ㅠ",
          "original_form": "원래 케이스가 낡아서 보라색인데 이제 벗겨져서 흰색이 보여요ㅠ",
          "speaker_id": "1",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002198.1.15",
          "form": "우와 저거 사야하는데.. 어디꺼 사셨나요..",
          "original_form": "우와 저거 사야하는데.. 어디꺼 사셨나요..",
          "speaker_id": "2",
          "time": "20210820 18:06"
        },
        {
          "id": "MDRW2100002198.1.16",
          "form": "전 항상 투명만 꼈었거든요",
          "original_form": "전 항상 투명만 꼈었거든요",
          "speaker_id": "2",
          "time": "20210820 18:06"
        },
        {
          "id": "MDRW2100002198.1.17",
          "form": "저 알리에서 샀어요!",
          "original_form": "저 알리에서 샀어요!",
          "speaker_id": "1",
          "time": "20210820 18:17"
        },
        {
          "id": "MDRW2100002198.1.18",
          "form": "원래 쓰던건 라이노거였는데 되게 오래 썼어요",
          "original_form": "원래 쓰던건 라이노거였는데 되게 오래 썼어요",
          "speaker_id": "1",
          "time": "20210820 18:17"
        },
        {
          "id": "MDRW2100002198.1.19",
          "form": "저도 알리 한번 봐애겠어요",
          "original_form": "저도 알리 한번 봐애겠어요",
          "speaker_id": "2",
          "time": "20210820 18:21"
        },
        {
          "id": "MDRW2100002198.1.20",
          "form": "알리 배송이 오래걸려서 문제지만요 ㅎ",
          "original_form": "알리 배송이 오래걸려서 문제지만요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 18:21"
        },
        {
          "id": "MDRW2100002198.1.21",
          "form": "그쵸 근데 어떤건 엄청 빨리 오더라구요",
          "original_form": "그쵸 근데 어떤건 엄청 빨리 오더라구요",
          "speaker_id": "1",
          "time": "20210820 20:21"
        },
        {
          "id": "MDRW2100002198.1.22",
          "form": "배송이 뭐 걸리냐에 따라 다른가봐요",
          "original_form": "배송이 뭐 걸리냐에 따라 다른가봐요",
          "speaker_id": "1",
          "time": "20210820 20:21"
        },
        {
          "id": "MDRW2100002198.1.23",
          "form": "아 정말요? 빨리 사고싶어지네요 ㅎ",
          "original_form": "아 정말요? 빨리 사고싶어지네요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 20:22"
        }
      ]
