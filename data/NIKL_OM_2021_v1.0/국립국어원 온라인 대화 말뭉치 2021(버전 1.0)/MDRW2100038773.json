      "utterance": [
        {
          "id": "MDRW2100038773.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100038773.1.2",
          "form": "저희 언니는 name2님같은 말 할수 있는 사람이 아닌데 반만 닮았으면 해요",
          "original_form": "저희 언니는 &name2&님같은 말 할수 있는 사람이 아닌데 반만 닮았으면 해요",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100038773.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100038773.1.4",
          "form": "ㅋㅋㅋㅋㅋ저 같이 말한다는게 어떤걸까오?",
          "original_form": "ㅋㅋㅋㅋㅋ저 같이 말한다는게 어떤걸까오?",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100038773.1.5",
          "form": "아까같은 말이요ㅎ 음... 언니 없어도 저랑 대화하면 되죠 이런말?ㅎㅎ",
          "original_form": "아까같은 말이요ㅎ 음... 언니 없어도 저랑 대화하면 되죠 이런말?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100038773.1.6",
          "form": "워낙 매정해서",
          "original_form": "워낙 매정해서",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100038773.1.7",
          "form": "아 그런 느낌을 편안해하시는군요!",
          "original_form": "아 그런 느낌을 편안해하시는군요!",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100038773.1.8",
          "form": "네ㅠ 저 되게 쉬운 사람인데 저희 언니는 그런 말 잘 안해주거든요",
          "original_form": "네ㅠ 저 되게 쉬운 사람인데 저희 언니는 그런 말 잘 안해주거든요",
          "speaker_id": "1",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100038773.1.9",
          "form": "ㅜㅜ 언니 성격이 다정한 말을 못 하는 편인가요?",
          "original_form": "ㅜㅜ 언니 성격이 다정한 말을 못 하는 편인가요?",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038773.1.10",
          "form": "아! 저 키보드 알려드릴까요?",
          "original_form": "아! 저 키보드 알려드릴까요?",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038773.1.11",
          "form": "네 못하고요 키보드도 알려주새요ㅎㅎ",
          "original_form": "네 못하고요 키보드도 알려주새요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038773.1.12",
          "form": "코시 베이직 실버 블루투스 키보드라고 하더라구요",
          "original_form": "코시 베이직 실버 블루투스 키보드라고 하더라구요",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038773.1.13",
          "form": "오 코시도 많이쓰죠ㅎㅎ",
          "original_form": "오 코시도 많이쓰죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038773.1.14",
          "form": "저도 코시살걸 그랬나ㅠ",
          "original_form": "저도 코시살걸 그랬나ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038773.1.15",
          "form": "많이 쓰는 브랜드인가요?",
          "original_form": "많이 쓰는 브랜드인가요?",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038773.1.16",
          "form": "저는 뭐 그냥 받아서 쓴거라ㅋㅋㅋ 찾아본적이 없어요",
          "original_form": "저는 뭐 그냥 받아서 쓴거라ㅋㅋㅋ 찾아본적이 없어요",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038773.1.17",
          "form": "받아 쓴 거면 모를 만도 하죠ㅎㅎ 전 진짜 샅샅히 뒤 졌 거든요ㅎㅎ",
          "original_form": "받아 쓴 거면 모를 만도 하죠ㅎㅎ 전 진짜 샅샅히 뒤 졌 거든요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038773.1.18",
          "form": "꼼꼼하게 분석후에 구매하셨군요!",
          "original_form": "꼼꼼하게 분석후에 구매하셨군요!",
          "speaker_id": "2",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100038773.1.19",
          "form": "제 키보드는 제가 엄청 쾅 쾅 눌러대서인지 맛이 살짝 간 거 같기도 하고ㅠ",
          "original_form": "제 키보드는 제가 엄청 쾅 쾅 눌러대서인지 맛이 살짝 간 거 같기도 하고ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100038773.1.20",
          "form": "ㅜㅜ 새로 장만해야하는건가요?\r\n오래 쓰셨어요?",
          "original_form": "ㅜㅜ 새로 장만해야하는건가요?\r\n오래 쓰셨어요?",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100038773.1.21",
          "form": "아니요ㅠ 제가 손가락 힘 하나는 넘쳐나더라고요",
          "original_form": "아니요ㅠ 제가 손가락 힘 하나는 넘쳐나더라고요",
          "speaker_id": "1",
          "time": "20210902 21:28"
        }
      ]
    }
