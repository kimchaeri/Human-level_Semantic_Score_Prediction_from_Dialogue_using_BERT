      "utterance": [
        {
          "id": "MDRW2100043341.1.1",
          "form": "사실 미대쪽 가고싶었는데",
          "original_form": "사실 미대쪽 가고싶었는데",
          "speaker_id": "1",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043341.1.2",
          "form": "집안 반대로 준비 못해서 못 갔었거든요 흑흑",
          "original_form": "집안 반대로 준비 못해서 못 갔었거든요 흑흑",
          "speaker_id": "1",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043341.1.3",
          "form": "헉 ㅠㅠㅠ 반대하시는 분들 많죠",
          "original_form": "헉 ㅠㅠㅠ 반대하시는 분들 많죠",
          "speaker_id": "2",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043341.1.4",
          "form": "대학 가서도 미련이 남아서 맛보기로 들은 수업인데",
          "original_form": "대학 가서도 미련이 남아서 맛보기로 들은 수업인데",
          "speaker_id": "1",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043341.1.5",
          "form": "저도 부모님이 처음엔 반대하셨는데",
          "original_form": "저도 부모님이 처음엔 반대하셨는데",
          "speaker_id": "2",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043341.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043341.1.7",
          "form": "학원도 미대 등록금도 쎄서 ㅜㅜ",
          "original_form": "학원도 미대 등록금도 쎄서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043341.1.8",
          "form": "그냥 교과로 갈 수있는곳으로 지원해서",
          "original_form": "그냥 교과로 갈 수있는곳으로 지원해서",
          "speaker_id": "2",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043341.1.9",
          "form": "홀랑 왔네용..",
          "original_form": "홀랑 왔네용..",
          "speaker_id": "2",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043341.1.10",
          "form": "멋지시다 전 홀랑 포기했는데",
          "original_form": "멋지시다 전 홀랑 포기했는데",
          "speaker_id": "1",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043341.1.11",
          "form": "ㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043341.1.12",
          "form": "와 그래도 미련있어서 이렇게 들으신거 대단해요",
          "original_form": "와 그래도 미련있어서 이렇게 들으신거 대단해요",
          "speaker_id": "2",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043341.1.13",
          "form": "끝까지 준비해서 진학하신 것도 너무 멋져요",
          "original_form": "끝까지 준비해서 진학하신 것도 너무 멋져요",
          "speaker_id": "1",
          "time": "20210907 18:00"
        },
        {
          "id": "MDRW2100043341.1.14",
          "form": "ㅋㅋㅋ감사해요으",
          "original_form": "ㅋㅋㅋ감사해요으",
          "speaker_id": "2",
          "time": "20210907 18:00"
        },
        {
          "id": "MDRW2100043341.1.15",
          "form": "어떤 수업 들으셨어요??",
          "original_form": "어떤 수업 들으셨어요??",
          "speaker_id": "2",
          "time": "20210907 18:00"
        },
        {
          "id": "MDRW2100043341.1.16",
          "form": "타이포그래피 수업이랑 하나 뭐더라(...)",
          "original_form": "타이포그래피 수업이랑 하나 뭐더라(...)",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043341.1.17",
          "form": "두 개 들었는데 하나는 기억이 안 나요 그악",
          "original_form": "두 개 들었는데 하나는 기억이 안 나요 그악",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043341.1.18",
          "form": "근데 타이포 배워두니까 유용하더라구요",
          "original_form": "근데 타이포 배워두니까 유용하더라구요",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043341.1.19",
          "form": "타이포 ! 타이포 생각보다 어렵더라구요",
          "original_form": "타이포 ! 타이포 생각보다 어렵더라구요",
          "speaker_id": "2",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043341.1.20",
          "form": "맞아요 유용하죠",
          "original_form": "맞아요 유용하죠",
          "speaker_id": "2",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043341.1.21",
          "form": "맞아요 처음에 글씨가 그게 그거지 싶었는데",
          "original_form": "맞아요 처음에 글씨가 그게 그거지 싶었는데",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043341.1.22",
          "form": "내는 과제마다 족족 다 빠꾸먹어서",
          "original_form": "내는 과제마다 족족 다 빠꾸먹어서",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043341.1.23",
          "form": "전 근데 어려워서 타이포쪽은 처다도 안봤던거같아요ㅋㅋㅋㅋ",
          "original_form": "전 근데 어려워서 타이포쪽은 처다도 안봤던거같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043341.1.24",
          "form": "내가 모르는 세계가 있나봐..! 하고 흥미를 가지기 시작했어요",
          "original_form": "내가 모르는 세계가 있나봐..! 하고 흥미를 가지기 시작했어요",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043341.1.25",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043341.1.26",
          "form": "그래도 타이포 배우면",
          "original_form": "그래도 타이포 배우면",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043341.1.27",
          "form": "헉 그럼 어떤거 들으셨나요",
          "original_form": "헉 그럼 어떤거 들으셨나요",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043341.1.28",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043341.1.29",
          "form": "나중에가선 어 이거 그폰트네! 하고 보이지 않나요??",
          "original_form": "나중에가선 어 이거 그폰트네! 하고 보이지 않나요??",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043341.1.30",
          "form": "그럴땐 재밌더라구요 ㅋㅋㅋ",
          "original_form": "그럴땐 재밌더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043341.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ아 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 맞아요",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043341.1.32",
          "form": "전 패키지디자인이나 브랜딩쪽으로 들어요!",
          "original_form": "전 패키지디자인이나 브랜딩쪽으로 들어요!",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043341.1.33",
          "form": "공공홍보물 같은 거 보면",
          "original_form": "공공홍보물 같은 거 보면",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043341.1.34",
          "form": "아 내가 써봤던 글씨체네 맞아 이거 좋제잉ㅋㅋ",
          "original_form": "아 내가 써봤던 글씨체네 맞아 이거 좋제잉ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043341.1.35",
          "form": "그럴때 아는거 보이면 되게 재밌죠 ㅋㅋㅋ",
          "original_form": "그럴때 아는거 보이면 되게 재밌죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043341.1.36",
          "form": "하고 혼자 내적 친밀감 상승해요",
          "original_form": "하고 혼자 내적 친밀감 상승해요",
          "speaker_id": "1",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043341.1.37",
          "form": "맞아요 저도 항상 그래요 ㅋㅋㅋ",
          "original_form": "맞아요 저도 항상 그래요 ㅋㅋㅋ",
