      "utterance": [
        {
          "id": "MDRW2100042087.1.1",
          "form": "로켓 그거 안했는데 해볼까봐요",
          "original_form": "로켓 그거 안했는데 해볼까봐요",
          "speaker_id": "2",
          "time": "20210906 19:34"
        },
        {
          "id": "MDRW2100042087.1.2",
          "form": "은근 볼게 있어요",
          "original_form": "은근 볼게 있어요",
          "speaker_id": "1",
          "time": "20210906 19:35"
        },
        {
          "id": "MDRW2100042087.1.3",
          "form": "오 머머 있는지요",
          "original_form": "오 머머 있는지요",
          "speaker_id": "2",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100042087.1.4",
          "form": "많이들 보시는 샹견니랑",
          "original_form": "많이들 보시는 샹견니랑",
          "speaker_id": "1",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042087.1.5",
          "form": "그게모죠?",
          "original_form": "그게모죠?",
          "speaker_id": "2",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042087.1.6",
          "form": "중국 드라마요",
          "original_form": "중국 드라마요",
          "speaker_id": "1",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100042087.1.7",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042087.1.8",
          "form": "저는 보다가 중단하긴 했는데",
          "original_form": "저는 보다가 중단하긴 했는데",
          "speaker_id": "1",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042087.1.9",
          "form": "재밌나보죠?",
          "original_form": "재밌나보죠?",
          "speaker_id": "2",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100042087.1.10",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100042087.1.11",
          "form": "아직 진도가 안나가는중 ㅜㅜㅋㅋ",
          "original_form": "아직 진도가 안나가는중 ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100042087.1.12",
          "form": "영화로도 개봉하나봐요",
          "original_form": "영화로도 개봉하나봐요",
          "speaker_id": "2",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100042087.1.13",
          "form": "그거는 뭔지 잘은 모르겠네요?",
          "original_form": "그거는 뭔지 잘은 모르겠네요?",
          "speaker_id": "1",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100042087.1.14",
          "form": "편수가 쫌 압박적이긴 해요",
          "original_form": "편수가 쫌 압박적이긴 해요",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042087.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100042087.1.16",
          "form": "전 짧은게 좋다는...",
