      "utterance": [
        {
          "id": "MDRW2100043060.1.1",
          "form": "헐 왜?",
          "original_form": "헐 왜?",
          "speaker_id": "2",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100043060.1.2",
          "form": "모르겠음 엠비씨 퇴사하고",
          "original_form": "모르겠음 엠비씨 퇴사하고",
          "speaker_id": "1",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100043060.1.3",
          "form": "독자노선간대",
          "original_form": "독자노선간대",
          "speaker_id": "1",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100043060.1.4",
          "form": "아무래도 월급쟁이보다는...낫겠지",
          "original_form": "아무래도 월급쟁이보다는...낫겠지",
          "speaker_id": "1",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100043060.1.5",
          "form": "아 김태호면 그정도는 가능하지",
          "original_form": "아 김태호면 그정도는 가능하지",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043060.1.6",
          "form": "그치 놀면뭐하니 피디바뀌려나",
          "original_form": "그치 놀면뭐하니 피디바뀌려나",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043060.1.7",
          "form": "야요즘 스우파 보냐",
          "original_form": "야요즘 스우파 보냐",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043060.1.8",
          "form": "ㄴㄴ 짤만 한두번봤어",
          "original_form": "ㄴㄴ 짤만 한두번봤어",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043060.1.9",
          "form": "그거 인기좋더라",
          "original_form": "그거 인기좋더라",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043060.1.10",
          "form": "나도 풀은 마니못봤는데 재밌어",
          "original_form": "나도 풀은 마니못봤는데 재밌어",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043060.1.11",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043060.1.12",
          "form": "잘춰 다들",
          "original_form": "잘춰 다들",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043060.1.13",
          "form": "관절없는듯..",
          "original_form": "관절없는듯..",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043060.1.14",
          "form": "그러다 관절나갈듯",
          "original_form": "그러다 관절나갈듯",
          "speaker_id": "2",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043060.1.15",
          "form": "그거 머지 007 마지막 시리즈 곧 개봉이래",
          "original_form": "그거 머지 007 마지막 시리즈 곧 개봉이래",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043060.1.16",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "2",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043060.1.17",
          "form": "ㅇㅇ남배 마지막이라하더라",
          "original_form": "ㅇㅇ남배 마지막이라하더라",
          "speaker_id": "1",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043060.1.18",
          "form": "왜그런데?",
          "original_form": "왜그런데?",
          "speaker_id": "2",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043060.1.19",
          "form": "글쎄...늙어서?",
          "original_form": "글쎄...늙어서?",
          "speaker_id": "1",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043060.1.20",
          "form": "나 넷플 오징어게임도 보고싶어",
          "original_form": "나 넷플 오징어게임도 보고싶어",
          "speaker_id": "1",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043060.1.21",
          "form": "아 그렇게ㅛ다 관정 괜찮너",
          "original_form": "아 그렇게ㅛ다 관정 괜찮너",
          "speaker_id": "2",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043060.1.22",
          "form": "그거잼대",
          "original_form": "그거잼대",
          "speaker_id": "2",
          "time": "20210907 13:49"
