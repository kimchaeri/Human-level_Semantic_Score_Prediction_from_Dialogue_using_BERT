      "utterance": [
        {
          "id": "MDRW2100015313.1.1",
          "form": "안녕하세요 저녁 드셨나요?",
          "original_form": "안녕하세요 저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015313.1.2",
          "form": "네 제육볶음 먹었답니다",
          "original_form": "네 제육볶음 먹었답니다",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015313.1.3",
          "form": "name1님은 뭐 드셨나요",
          "original_form": "&name1&님은 뭐 드셨나요",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015313.1.4",
          "form": "저는 김치찌개요!",
          "original_form": "저는 김치찌개요!",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015313.1.5",
          "form": "고기 참치 둘중 뭐가 찌개에 들어갔나요?",
          "original_form": "고기 참치 둘중 뭐가 찌개에 들어갔나요?",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015313.1.6",
          "form": "고기요! 역시 고기가 맛있죠 name2님은 어느쪽이 좋으세요?",
          "original_form": "고기요! 역시 고기가 맛있죠 &name2&님은 어느쪽이 좋으세요?",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015313.1.7",
          "form": "저도 고기파예요",
          "original_form": "저도 고기파예요",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015313.1.8",
          "form": "입맛이 통했네요",
          "original_form": "입맛이 통했네요",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015313.1.9",
          "form": "고기 넣은 국물의 기름진 맛도 좋고요ㅎ",
          "original_form": "고기 넣은 국물의 기름진 맛도 좋고요ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015313.1.10",
          "form": "그러게요ㅎ",
          "original_form": "그러게요ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015313.1.11",
          "form": "그쵸 약간 기름진게 더 맛있어요",
          "original_form": "그쵸 약간 기름진게 더 맛있어요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015313.1.12",
          "form": "김치찌개와 된장찌개 중에선 뭘 더 선호하세요?",
          "original_form": "김치찌개와 된장찌개 중에선 뭘 더 선호하세요?",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015313.1.13",
          "form": "요즘에는 된장이요 name2님은요?",
          "original_form": "요즘에는 된장이요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015313.1.14",
          "form": "저는 초지일관 된장이요",
          "original_form": "저는 초지일관 된장이요",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015313.1.15",
          "form": "입맛이 저랑 비슷하신가봐요 ㅎㅎ",
          "original_form": "입맛이 저랑 비슷하신가봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015313.1.16",
          "form": "둘다 좋아하지만 평생 하나만 먹으라면 된찌",
          "original_form": "둘다 좋아하지만 평생 하나만 먹으라면 된찌",
          "speaker_id": "2",
          "time": "20210825 22:41"
