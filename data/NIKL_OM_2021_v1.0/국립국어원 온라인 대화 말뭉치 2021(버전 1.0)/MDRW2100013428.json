      "utterance": [
        {
          "id": "MDRW2100013428.1.1",
          "form": "크루엘라는 처음 들었어요",
          "original_form": "크루엘라는 처음 들었어요",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013428.1.2",
          "form": "장르가 뭔가요",
          "original_form": "장르가 뭔가요",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013428.1.3",
          "form": "패션영화같은데 화려해보여서 끌리더라고요",
          "original_form": "패션영화같은데 화려해보여서 끌리더라고요",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013428.1.4",
          "form": "엠마스톤 나와요 ㅎㅎ",
          "original_form": "엠마스톤 나와요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013428.1.5",
          "form": "지금 크루엘라 찾아봤는데 포스터가 강렬하네요",
          "original_form": "지금 크루엘라 찾아봤는데 포스터가 강렬하네요",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013428.1.6",
          "form": "아니 이분이 엠마스톤이었군요ㅎㅎ",
          "original_form": "아니 이분이 엠마스톤이었군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013428.1.7",
          "form": "맞아요 뭐지 달마시안 애니 그게 원작일거예요",
          "original_form": "맞아요 뭐지 달마시안 애니 그게 원작일거예요",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013428.1.8",
          "form": "화장이 진해서 몰라볼뻔",
          "original_form": "화장이 진해서 몰라볼뻔",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013428.1.9",
          "form": "강아지들 많이 나오는 ㅋㅋ",
          "original_form": "강아지들 많이 나오는 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013428.1.10",
          "form": "오오 저 이거 왜 모르고 지나쳤죠",
          "original_form": "오오 저 이거 왜 모르고 지나쳤죠",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013428.1.11",
          "form": "앜ㅋㅋㅋ진짜 분장 제대로 한거같아요",
          "original_form": "앜ㅋㅋㅋ진짜 분장 제대로 한거같아요",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013428.1.12",
          "form": "저도 아직 안봐서 고민하고 있어요",
          "original_form": "저도 아직 안봐서 고민하고 있어요",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013428.1.13",
          "form": "후기평에 엠마들이 다해먹는 영화라네요",
          "original_form": "후기평에 엠마들이 다해먹는 영화라네요",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013428.1.14",
          "form": "와 ㅋㅋ 기대되네요 언제 한번 봐야겠어요",
          "original_form": "와 ㅋㅋ 기대되네요 언제 한번 봐야겠어요",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013428.1.15",
          "form": "101마리 달마시안이 어떻게 이렇게 변하지요ㅎ",
          "original_form": "101마리 달마시안이 어떻게 이렇게 변하지요ㅎ",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013428.1.16",
          "form": "name2 덕에 재미난 작품 알았네요",
          "original_form": "&name2& 덕에 재미난 작품 알았네요",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013428.1.17",
          "form": "코로나 때문에 영화관을 자주 못가니 이런 소식에도 어두워요ㅜ",
          "original_form": "코로나 때문에 영화관을 자주 못가니 이런 소식에도 어두워요ㅜ",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013428.1.18",
          "form": "맞아요 예전에는 진짜 자주갔는데 아쉬위요 ㅠㅠ",
          "original_form": "맞아요 예전에는 진짜 자주갔는데 아쉬위요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013428.1.19",
          "form": "그래도 블랙위도우는 영화관에서 봤어요! 마블영화라 스케일이 크더라고요 ㅋㅋ",
          "original_form": "그래도 블랙위도우는 영화관에서 봤어요! 마블영화라 스케일이 크더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013428.1.20",
          "form": "재밌어요",
          "original_form": "재밌어요",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013428.1.21",
          "form": "마블은 무조건 크게 봐야죠!",
          "original_form": "마블은 무조건 크게 봐야죠!",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013428.1.22",
          "form": "아이언맨은 이제 없지만ㅜ",
          "original_form": "아이언맨은 이제 없지만ㅜ",
          "speaker_id": "1",
          "time": "20210825 14:34"
        }
      ]
    }
