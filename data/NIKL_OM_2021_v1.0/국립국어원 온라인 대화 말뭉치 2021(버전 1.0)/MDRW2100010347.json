      "utterance": [
        {
          "id": "MDRW2100010347.1.1",
          "form": "엇 답을 뭐라고 보내셨죠? ㅋㅋㅋㅠ",
          "original_form": "엇 답을 뭐라고 보내셨죠? ㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210823 18:00"
        },
        {
          "id": "MDRW2100010347.1.2",
          "form": "다시시작하져....ㅎㅎ..",
          "original_form": "다시시작하져....ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100010347.1.3",
          "form": "최근에 영화 보신거 있으세요?",
          "original_form": "최근에 영화 보신거 있으세요?",
          "speaker_id": "1",
          "time": "20210824 21:28"
        },
        {
          "id": "MDRW2100010347.1.4",
          "form": "ㅋㅋㅋㅋㅋ 전 델마와 루이스 봤어요!",
          "original_form": "ㅋㅋㅋㅋㅋ 전 델마와 루이스 봤어요!",
          "speaker_id": "2",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100010347.1.5",
          "form": "오 저 대충 내용만 알아요!",
          "original_form": "오 저 대충 내용만 알아요!",
          "speaker_id": "1",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100010347.1.6",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "1",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100010347.1.7",
          "form": "네!!! 재밌어요 한번 봐보세요 ㅎㅎ",
          "original_form": "네!!! 재밌어요 한번 봐보세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100010347.1.8",
          "form": "나중에 봐야겠어요! 명작으로 언급되더라구요ㅋㅋㅋㅋ",
          "original_form": "나중에 봐야겠어요! 명작으로 언급되더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100010347.1.9",
          "form": "엔딩이 진짜 멋있어요 ㅋㅋㅋㅋ",
          "original_form": "엔딩이 진짜 멋있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100010347.1.10",
          "form": "오오 엔딩!! 궁금하지만 스포가 될 수도 있으니 안 물을게요ㅋㅋㅋ",
          "original_form": "오오 엔딩!! 궁금하지만 스포가 될 수도 있으니 안 물을게요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100010347.1.11",
          "form": "인생영화는 어떤게 있으세요?",
          "original_form": "인생영화는 어떤게 있으세요?",
          "speaker_id": "1",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100010347.1.12",
          "form": "아 그런 건 없어요 ㅋㅋㅋㅋ큐ㅠ",
          "original_form": "아 그런 건 없어요 ㅋㅋㅋㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210825 00:12"
        },
        {
          "id": "MDRW2100010347.1.13",
          "form": "name1님은 있으세요?",
          "original_form": "&name1&님은 있으세요?",
          "speaker_id": "2",
          "time": "20210825 00:12"
        },
        {
          "id": "MDRW2100010347.1.14",
          "form": "저는 타이타닉이요! 해리포터도 질리게 봤네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 타이타닉이요! 해리포터도 질리게 봤네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:15"
        },
        {
          "id": "MDRW2100010347.1.15",
          "form": "오 유명해서 알아요 ㅋㅋㅋㅋ",
          "original_form": "오 유명해서 알아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100010347.1.16",
          "form": "해리포터는 계속 보려다가 아직도 안봣네요",
          "original_form": "해리포터는 계속 보려다가 아직도 안봣네요",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100010347.1.17",
          "form": "헐!! 해리포터를 안 본 귀한눈이시군요",
          "original_form": "헐!! 해리포터를 안 본 귀한눈이시군요",
          "speaker_id": "1",
          "time": "20210825 00:20"
        },
        {
          "id": "MDRW2100010347.1.18",
          "form": "ㅋㅋㅋㅋㅋ 그쵸 나중에 친구랑 같이 보려고요",
          "original_form": "ㅋㅋㅋㅋㅋ 그쵸 나중에 친구랑 같이 보려고요",
