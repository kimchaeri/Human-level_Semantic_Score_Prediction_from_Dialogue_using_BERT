      "utterance": [
        {
          "id": "MDRW2100016260.1.1",
          "form": "ㅋㅋ안받는게아니라",
          "original_form": "ㅋㅋ안받는게아니라",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016260.1.2",
          "form": "비싼거받나요",
          "original_form": "비싼거받나요",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016260.1.3",
          "form": "이씨는 왜 선물안줘요",
          "original_form": "이씨는 왜 선물안줘요",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016260.1.4",
          "form": "1년동안",
          "original_form": "1년동안",
          "speaker_id": "1",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016260.1.5",
          "form": "저한테 잘 보이시면",
          "original_form": "저한테 잘 보이시면",
          "speaker_id": "1",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016260.1.6",
          "form": "그땐 콩고물좀 떨어지지않을까요?",
          "original_form": "그땐 콩고물좀 떨어지지않을까요?",
          "speaker_id": "1",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016260.1.7",
          "form": "박씨 입사한지 얼마안됐잖아요",
          "original_form": "박씨 입사한지 얼마안됐잖아요",
          "speaker_id": "1",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016260.1.8",
          "form": "1년ㄷㄷ",
          "original_form": "1년ㄷㄷ",
          "speaker_id": "2",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100016260.1.9",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100016260.1.10",
          "form": "기대할게여",
          "original_form": "기대할게여",
          "speaker_id": "2",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100016260.1.11",
          "form": "30만",
          "original_form": "30만",
          "speaker_id": "2",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100016260.1.12",
          "form": "ㄷ ㄷ",
          "original_form": "ㄷ ㄷ",
          "speaker_id": "1",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016260.1.13",
          "form": "그러다 3만원짜리도 못받아요",
          "original_form": "그러다 3만원짜리도 못받아요",
          "speaker_id": "1",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016260.1.14",
          "form": "저는 생일선물로",
          "original_form": "저는 생일선물로",
          "speaker_id": "1",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016260.1.15",
          "form": "사무실아래에 두는 발받침대 사주세요",
          "original_form": "사무실아래에 두는 발받침대 사주세요",
          "speaker_id": "1",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100016260.1.16",
          "form": "그건 공구죠",
          "original_form": "그건 공구죠",
          "speaker_id": "2",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016260.1.17",
          "form": "사실건가요???",
          "original_form": "사실건가요???",
          "speaker_id": "1",
          "time": "20210826 11:51"
        },
        {
          "id": "MDRW2100016260.1.18",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100016260.1.19",
          "form": "다리길어서",
          "original_form": "다리길어서",
          "speaker_id": "2",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100016260.1.20",
          "form": "안된다니까요",
          "original_form": "안된다니까요",
          "speaker_id": "2",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100016260.1.21",
          "form": "뭐야 그니까 저만 사주세요",
          "original_form": "뭐야 그니까 저만 사주세요",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100016260.1.22",
          "form": "나무로 된거 사주세요",
          "original_form": "나무로 된거 사주세요",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100016260.1.23",
          "form": "나무로 된거도잇나요",
          "original_form": "나무로 된거도잇나요",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016260.1.24",
          "form": "넹넹 좀 비싼건",
          "original_form": "넹넹 좀 비싼건",
          "speaker_id": "1",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016260.1.25",
          "form": "각도조절할수있어요",
          "original_form": "각도조절할수있어요",
          "speaker_id": "1",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016260.1.26",
          "form": "오호?",
          "original_form": "오호?",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016260.1.27",
          "form": "저희 회사 책상도 좀 좋은 책상으로 바꿨으면 좋겟네요",
          "original_form": "저희 회사 책상도 좀 좋은 책상으로 바꿨으면 좋겟네요",
          "speaker_id": "1",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016260.1.28",
          "form": "저는 슬리퍼사고픔",
          "original_form": "저는 슬리퍼사고픔",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016260.1.29",
          "form": "거북목 퇴치",
          "original_form": "거북목 퇴치",
          "speaker_id": "1",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016260.1.30",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016260.1.31",
          "form": "그건 여기서 얘기할게아닌데",
          "original_form": "그건 여기서 얘기할게아닌데",
          "speaker_id": "2",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016260.1.32",
          "form": "거북목교정기도ㅇㅇ",
          "original_form": "거북목교정기도ㅇㅇ",
          "speaker_id": "2",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016260.1.33",
          "form": "어디다 말해야하죠?",
          "original_form": "어디다 말해야하죠?",
          "speaker_id": "1",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016260.1.34",
          "form": "말한다고 되나요",
          "original_form": "말한다고 되나요",
          "speaker_id": "1",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016260.1.35",
          "form": "저번에 팀장님이 마트가셧을때",
          "original_form": "저번에 팀장님이 마트가셧을때",
          "speaker_id": "1",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016260.1.36",
          "form": "부탁해보시지 그랫어요 ㅋㅋ",
          "original_form": "부탁해보시지 그랫어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016260.1.37",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016260.1.38",
          "form": "그럴걸그랬네요",
          "original_form": "그럴걸그랬네요",
