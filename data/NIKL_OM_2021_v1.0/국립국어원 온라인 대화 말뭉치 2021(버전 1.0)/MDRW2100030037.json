      "utterance": [
        {
          "id": "MDRW2100030037.1.1",
          "form": "지금하는 거 말고 예전해 해봤던 운동 있나요?",
          "original_form": "지금하는 거 말고 예전해 해봤던 운동 있나요?",
          "speaker_id": "1",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100030037.1.2",
          "form": "헬스다녔었어요",
          "original_form": "헬스다녔었어요",
          "speaker_id": "2",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100030037.1.3",
          "form": "코로나와함께 안녕...",
          "original_form": "코로나와함께 안녕...",
          "speaker_id": "2",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100030037.1.4",
          "form": "저랑 비슷하시네요",
          "original_form": "저랑 비슷하시네요",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100030037.1.5",
          "form": "전 수영 다녔는데.. 수영장을 못가게 됐어요",
          "original_form": "전 수영 다녔는데.. 수영장을 못가게 됐어요",
          "speaker_id": "1",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100030037.1.6",
          "form": "수영장은 진짜 그렇네요",
          "original_form": "수영장은 진짜 그렇네요",
          "speaker_id": "2",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100030037.1.7",
          "form": "마스크하고 수영불가니...",
          "original_form": "마스크하고 수영불가니...",
          "speaker_id": "2",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100030037.1.8",
          "form": "헬스장도 다녔었는데..",
          "original_form": "헬스장도 다녔었는데..",
          "speaker_id": "1",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100030037.1.9",
          "form": "작년부터 다중이용시설은 멀리하고 있어서요",
          "original_form": "작년부터 다중이용시설은 멀리하고 있어서요",
          "speaker_id": "1",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100030037.1.10",
          "form": "위험하죠 아무래도",
          "original_form": "위험하죠 아무래도",
          "speaker_id": "2",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100030037.1.11",
          "form": "어딜가야하나요",
          "original_form": "어딜가야하나요",
          "speaker_id": "2",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100030037.1.12",
          "form": "운동도 제대로 못하고",
          "original_form": "운동도 제대로 못하고",
          "speaker_id": "2",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100030037.1.13",
          "form": "그래서 좀 답답해요.. 전 외향적이라 몸을 움직여줘야 하거든요",
          "original_form": "그래서 좀 답답해요.. 전 외향적이라 몸을 움직여줘야 하거든요",
          "speaker_id": "1",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100030037.1.14",
          "form": "아 외향적이세요?",
          "original_form": "아 외향적이세요?",
          "speaker_id": "2",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100030037.1.15",
          "form": "그럼 등산은 어떠세요?",
          "original_form": "그럼 등산은 어떠세요?",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100030037.1.16",
          "form": "등산 좋은데 한국은 등산하는 사람들도 많아서 사람 많은 곳 피하려니 등산도 잘 안하게 되네요.. 작년부터요..",
          "original_form": "등산 좋은데 한국은 등산하는 사람들도 많아서 사람 많은 곳 피하려니 등산도 잘 안하게 되네요.. 작년부터요..",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100030037.1.17",
          "form": "등산하는사람 많은가요 요즘?",
          "original_form": "등산하는사람 많은가요 요즘?",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100030037.1.18",
          "form": "한국은 등산가면 언제나 사람들이 바글했었죠",
          "original_form": "한국은 등산가면 언제나 사람들이 바글했었죠",
          "speaker_id": "1",
          "time": "20210904 05:11"
        },
        {
          "id": "MDRW2100030037.1.19",
          "form": "작년부터 안다녀서 요새는 어떤지 모르겠네요",
          "original_form": "작년부터 안다녀서 요새는 어떤지 모르겠네요",
          "speaker_id": "1",
          "time": "20210904 05:11"
        },
        {
          "id": "MDRW2100030037.1.20",
          "form": "요즘 더 많을걸요?",
          "original_form": "요즘 더 많을걸요?",
          "speaker_id": "2",
          "time": "20210904 08:11"
        },
        {
          "id": "MDRW2100030037.1.21",
          "form": "그런가요? 가을이라서 산행하는 사람들 더 많겠네요",
          "original_form": "그런가요? 가을이라서 산행하는 사람들 더 많겠네요",
          "speaker_id": "1",
          "time": "20210904 08:59"
        },
        {
          "id": "MDRW2100030037.1.22",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210904 09:16"
        },
        {
          "id": "MDRW2100030037.1.23",
          "form": "단풍놀이 가시는 어머니 아버지들 많으실듯",
          "original_form": "단풍놀이 가시는 어머니 아버지들 많으실듯",
          "speaker_id": "2",
          "time": "20210904 09:16"
        }
      ]
