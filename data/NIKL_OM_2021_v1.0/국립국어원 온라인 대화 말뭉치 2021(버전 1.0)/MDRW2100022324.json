      "utterance": [
        {
          "id": "MDRW2100022324.1.1",
          "form": "네 누나가 진짜 얄미운 짓 많이 하잖아요 ㅠ",
          "original_form": "네 누나가 진짜 얄미운 짓 많이 하잖아요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100022324.1.2",
          "form": "자기만 맛있는 거 골라 먹으려고 하는게 이기적인 것 같아요",
          "original_form": "자기만 맛있는 거 골라 먹으려고 하는게 이기적인 것 같아요",
          "speaker_id": "2",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100022324.1.3",
          "form": "원래 누나들이 좀 그런거같아요",
          "original_form": "원래 누나들이 좀 그런거같아요",
          "speaker_id": "1",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100022324.1.4",
          "form": "피자 좋아하세요",
          "original_form": "피자 좋아하세요",
          "speaker_id": "1",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100022324.1.5",
          "form": "밖에서도 친구들이랑 먹을 때 그럴까 걱정이에요",
          "original_form": "밖에서도 친구들이랑 먹을 때 그럴까 걱정이에요",
          "speaker_id": "2",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100022324.1.6",
          "form": "피자 저 오늘 피자 먹고싶어요",
          "original_form": "피자 저 오늘 피자 먹고싶어요",
          "speaker_id": "2",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100022324.1.7",
          "form": "근데 뽈찜 드신다고.ㅈ.",
          "original_form": "근데 뽈찜 드신다고.ㅈ.",
          "speaker_id": "1",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100022324.1.8",
          "form": "그치만 참치뽈찜을 처리해야되거든요",
          "original_form": "그치만 참치뽈찜을 처리해야되거든요",
          "speaker_id": "2",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100022324.1.9",
          "form": "ㅋㅋㅋㅋ 피자 무슨피자좋아하세요",
          "original_form": "ㅋㅋㅋㅋ 피자 무슨피자좋아하세요",
          "speaker_id": "1",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100022324.1.10",
          "form": "지금은 단종 됐는데",
          "original_form": "지금은 단종 됐는데",
          "speaker_id": "2",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100022324.1.11",
          "form": "피자헛 중에서 갈릭 마블스테이크 피자라고 있었어요",
          "original_form": "피자헛 중에서 갈릭 마블스테이크 피자라고 있었어요",
          "speaker_id": "2",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100022324.1.12",
          "form": "단종된걸 좋아하시다니..",
          "original_form": "단종된걸 좋아하시다니..",
          "speaker_id": "1",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100022324.1.13",
          "form": "왜 없어졌을까요 ㅠㅠㅠ",
          "original_form": "왜 없어졌을까요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100022324.1.14",
          "form": "맘아프시겠다",
          "original_form": "맘아프시겠다",
          "speaker_id": "1",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100022324.1.15",
          "form": "엣지라도 살려주시지....",
          "original_form": "엣지라도 살려주시지....",
          "speaker_id": "2",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100022324.1.16",
          "form": "이제 못드시다니..",
          "original_form": "이제 못드시다니..",
          "speaker_id": "1",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100022324.1.17",
          "form": "그거 엣지 진짜 맛잇었는데..",
          "original_form": "그거 엣지 진짜 맛잇었는데..",
          "speaker_id": "2",
          "time": "20210829 12:37"
        }
      ]
