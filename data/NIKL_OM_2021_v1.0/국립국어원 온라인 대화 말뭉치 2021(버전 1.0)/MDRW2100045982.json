      "utterance": [
        {
          "id": "MDRW2100045982.1.1",
          "form": "아 맞아요 달다보면 속이 좀 근질거릴 때가 있어요",
          "original_form": "아 맞아요 달다보면 속이 좀 근질거릴 때가 있어요",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045982.1.2",
          "form": "자꾸 생크림이 한가득 들어있는 생크림팥빵을 사오는데",
          "original_form": "자꾸 생크림이 한가득 들어있는 생크림팥빵을 사오는데",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045982.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045982.1.4",
          "form": "ㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045982.1.5",
          "form": "그래놓고 왜 다들 안먹냐하면 얼탱이가없어요",
          "original_form": "그래놓고 왜 다들 안먹냐하면 얼탱이가없어요",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045982.1.6",
          "form": "언니나먹엉! 소리가 절로 나오져",
          "original_form": "언니나먹엉! 소리가 절로 나오져",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045982.1.7",
          "form": "ㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋ언니 빼고 가족분들이 다",
          "original_form": "ㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋ언니 빼고 가족분들이 다",
          "speaker_id": "2",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045982.1.8",
          "form": "느끼한 걸 안 드세요?",
          "original_form": "느끼한 걸 안 드세요?",
          "speaker_id": "2",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045982.1.9",
          "form": "네네 별로 좋아하지않아요",
          "original_form": "네네 별로 좋아하지않아요",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045982.1.10",
          "form": "저 얼마전에 베이글 먹으로갔는데",
          "original_form": "저 얼마전에 베이글 먹으로갔는데",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045982.1.11",
          "form": "토마토베이컨크림치즈가 새로 나왔길래 시켜봤거든요",
          "original_form": "토마토베이컨크림치즈가 새로 나왔길래 시켜봤거든요",
          "speaker_id": "1",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045982.1.12",
          "form": "한입먹자마자 아메리카노 주문했어요",
          "original_form": "한입먹자마자 아메리카노 주문했어요",
          "speaker_id": "1",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045982.1.13",
          "form": "ㅋㅋㅋㅋㅋㅌㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅌㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045982.1.14",
          "form": "피자빵 치즈는 잘 드시면서!",
          "original_form": "피자빵 치즈는 잘 드시면서!",
          "speaker_id": "2",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045982.1.15",
          "form": "어째서죠",
          "original_form": "어째서죠",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045982.1.16",
          "form": "ㅎ....",
          "original_form": "ㅎ....",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045982.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045982.1.18",
          "form": "모순덩어리에요",
          "original_form": "모순덩어리에요",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045982.1.19",
          "form": "맛은 있었어요!!!!",
          "original_form": "맛은 있었어요!!!!",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045982.1.20",
          "form": "근데 아메리카노가 필요한 맛이었을뿐 ㅎ",
          "original_form": "근데 아메리카노가 필요한 맛이었을뿐 ㅎ",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045982.1.21",
          "form": "헉 맛없게 느껴지느 게 아니예요?",
          "original_form": "헉 맛없게 느껴지느 게 아니예요?",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045982.1.22",
          "form": "네네 맛있었어요",
          "original_form": "네네 맛있었어요",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045982.1.23",
          "form": "의외다 못 드신다길래",
          "original_form": "의외다 못 드신다길래",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045982.1.24",
          "form": "왜 그...",
          "original_form": "왜 그...",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045982.1.25",
          "form": "맛없어서 안 드시는 줄 알았어요",
          "original_form": "맛없어서 안 드시는 줄 알았어요",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045982.1.26",
          "form": "아메리카노랑 같이 먹어야하는 음식들 있잖아요",
          "original_form": "아메리카노랑 같이 먹어야하는 음식들 있잖아요",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045982.1.27",
          "form": "생크림은 잘 안먹는데",
          "original_form": "생크림은 잘 안먹는데",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045982.1.28",
          "form": "베이글에 발라먹었던 토마토베이컨크림치즈는 좋았어요",
          "original_form": "베이글에 발라먹었던 토마토베이컨크림치즈는 좋았어요",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045982.1.29",
          "form": "세개 다 좋아하거든요 토마토 베이컨 크림치즠ㅋㅋㅋㅋ",
          "original_form": "세개 다 좋아하거든요 토마토 베이컨 크림치즠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:25"
        }
      ]
