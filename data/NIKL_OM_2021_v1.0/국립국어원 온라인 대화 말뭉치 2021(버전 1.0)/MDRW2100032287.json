      "utterance": [
        {
          "id": "MDRW2100032287.1.1",
          "form": "네 비행기는 극히 일부의",
          "original_form": "네 비행기는 극히 일부의",
          "speaker_id": "1",
          "time": "20210905 14:53"
        },
        {
          "id": "MDRW2100032287.1.2",
          "form": "조종사만 탑니다 ㅋㅋ",
          "original_form": "조종사만 탑니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:53"
        },
        {
          "id": "MDRW2100032287.1.3",
          "form": "그 극히 일부엔 속하지 못하셨군요ㅎ",
          "original_form": "그 극히 일부엔 속하지 못하셨군요ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100032287.1.4",
          "form": "조종사는 장교에요ㅋㅋㅋ",
          "original_form": "조종사는 장교에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:00"
        },
        {
          "id": "MDRW2100032287.1.5",
          "form": "그렇군요 군대 무식자는 공군이면 다 비행기 타는 줄ㅋㅋㅋㅋ",
          "original_form": "그렇군요 군대 무식자는 공군이면 다 비행기 타는 줄ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:02"
        },
        {
          "id": "MDRW2100032287.1.6",
          "form": "병사는 땅을 지킵니다",
          "original_form": "병사는 땅을 지킵니다",
          "speaker_id": "1",
          "time": "20210905 20:06"
        },
        {
          "id": "MDRW2100032287.1.7",
          "form": "공군이면 비행기 안타도 땅에서 하늘을 지키는 일을 하는거죠?",
          "original_form": "공군이면 비행기 안타도 땅에서 하늘을 지키는 일을 하는거죠?",
          "speaker_id": "2",
          "time": "20210905 20:14"
        },
        {
          "id": "MDRW2100032287.1.8",
          "form": "비행기가 무사히 뜰 수",
          "original_form": "비행기가 무사히 뜰 수",
          "speaker_id": "1",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100032287.1.9",
          "form": "있도록 제반업무가",
          "original_form": "있도록 제반업무가",
          "speaker_id": "1",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100032287.1.10",
          "form": "저희의 임무입니다@!",
          "original_form": "저희의 임무입니다@!",
          "speaker_id": "1",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100032287.1.11",
          "form": "비행기를 타든 안타든 다 멋있는 일이군요ㅎ",
          "original_form": "비행기를 타든 안타든 다 멋있는 일이군요ㅎ",
          "speaker_id": "2",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100032287.1.12",
          "form": "사실 육군에 비해서는",
          "original_form": "사실 육군에 비해서는",
          "speaker_id": "1",
          "time": "20210906 08:38"
        },
        {
          "id": "MDRW2100032287.1.13",
          "form": "꿀빨았습니다",
          "original_form": "꿀빨았습니다",
          "speaker_id": "1",
          "time": "20210906 08:38"
        },
        {
          "id": "MDRW2100032287.1.14",
          "form": "군대 이야기를 해서 그른가...말투가 군인같으시네요ㅋㅋㅋㅋ",
          "original_form": "군대 이야기를 해서 그른가...말투가 군인같으시네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:53"
        },
        {
          "id": "MDRW2100032287.1.15",
          "form": "ㅋㅋㅋ 예?",
          "original_form": "ㅋㅋㅋ 예?",
