      "utterance": [
        {
          "id": "MDRW2100020249.1.1",
          "form": "오늘도",
          "original_form": "오늘도",
          "speaker_id": "2",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020249.1.2",
          "form": "배달해드십니까??",
          "original_form": "배달해드십니까??",
          "speaker_id": "2",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020249.1.3",
          "form": "아침에 무엇을 먹을건가요?",
          "original_form": "아침에 무엇을 먹을건가요?",
          "speaker_id": "1",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020249.1.4",
          "form": "아 배달 조금 지겨워서 집에 있는 걸로 요리해먹으려규요",
          "original_form": "아 배달 조금 지겨워서 집에 있는 걸로 요리해먹으려규요",
          "speaker_id": "1",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020249.1.5",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020249.1.6",
          "form": "저는일단아침을먹지않고",
          "original_form": "저는일단아침을먹지않고",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020249.1.7",
          "form": "필라테스를갑니당",
          "original_form": "필라테스를갑니당",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020249.1.8",
          "form": "무슨요리해먹어용??",
          "original_form": "무슨요리해먹어용??",
          "speaker_id": "2",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020249.1.9",
          "form": "음 고민중입니다~ 김치가 있으니",
          "original_form": "음 고민중입니다~ 김치가 있으니",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020249.1.10",
          "form": "김치볶음밥ㄹ 하지 않을까요..?",
          "original_form": "김치볶음밥ㄹ 하지 않을까요..?",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020249.1.11",
          "form": "오오오오옹",
          "original_form": "오오오오옹",
          "speaker_id": "2",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020249.1.12",
          "form": "김치볶음밥",
          "original_form": "김치볶음밥",
          "speaker_id": "2",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020249.1.13",
          "form": "좋져",
          "original_form": "좋져",
          "speaker_id": "2",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020249.1.14",
          "form": "케첩잇으신가요??",
          "original_form": "케첩잇으신가요??",
          "speaker_id": "2",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020249.1.15",
          "form": "그쵸! 김치볶음밥에 계란후라이 올려야하는데 계란이 없네요..",
          "original_form": "그쵸! 김치볶음밥에 계란후라이 올려야하는데 계란이 없네요..",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020249.1.16",
          "form": "케쳡 없죠",
          "original_form": "케쳡 없죠",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020249.1.17",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020249.1.18",
          "form": "계란도없고",
          "original_form": "계란도없고",
          "speaker_id": "2",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020249.1.19",
          "form": "있눈게 없는 상태로 김치볶움밥을 할겁니다",
          "original_form": "있눈게 없는 상태로 김치볶움밥을 할겁니다",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020249.1.20",
          "form": "케첩도없고",
          "original_form": "케첩도없고",
          "speaker_id": "2",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020249.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020249.1.22",
          "form": "가위도없고...",
          "original_form": "가위도없고...",
          "speaker_id": "2",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020249.1.23",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020249.1.24",
          "form": "김치+밥+참기름",
          "original_form": "김치+밥+참기름",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020249.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020249.1.26",
          "form": "으아아ㅏ아아아ㅏ아ㅏㅏㅏㅏ",
          "original_form": "으아아ㅏ아아아ㅏ아ㅏㅏㅏㅏ",
          "speaker_id": "2",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020249.1.27",
          "form": "아 가위를 또 안샀네",
          "original_form": "아 가위를 또 안샀네",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020249.1.28",
          "form": "이정도면",
          "original_form": "이정도면",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020249.1.29",
          "form": "칼로 요리하면 되죠..",
          "original_form": "칼로 요리하면 되죠..",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020249.1.30",
          "form": "가위를살생각이없으신데욬ㅋㅋㅋ",
          "original_form": "가위를살생각이없으신데욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020249.1.31",
          "form": "아그쵸",
          "original_form": "아그쵸",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020249.1.32",
          "form": "칼로하면되죵",
          "original_form": "칼로하면되죵",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020249.1.33",
          "form": "그럼요그럼요~ 칼로 다 가능합니다",
          "original_form": "그럼요그럼요~ 칼로 다 가능합니다",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020249.1.34",
          "form": "아그럼요그럼요",
