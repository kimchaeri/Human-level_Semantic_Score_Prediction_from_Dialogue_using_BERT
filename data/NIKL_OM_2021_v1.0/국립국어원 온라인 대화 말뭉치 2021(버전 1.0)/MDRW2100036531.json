      "utterance": [
        {
          "id": "MDRW2100036531.1.1",
          "form": "로이였나?",
          "original_form": "로이였나?",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036531.1.2",
          "form": "우리 앵무새는 골든체리 모란앵무야.",
          "original_form": "우리 앵무새는 골든체리 모란앵무야.",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036531.1.3",
          "form": "아냐. 로사도 아니고 로이도ㅠ아냐.",
          "original_form": "아냐. 로사도 아니고 로이도ㅠ아냐.",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036531.1.4",
          "form": "name3야!",
          "original_form": "&name3&야!",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036531.1.5",
          "form": "앵무새는 종류가 몇가지야??",
          "original_form": "앵무새는 종류가 몇가지야??",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036531.1.6",
          "form": "지금 옆에서 자고있어",
          "original_form": "지금 옆에서 자고있어",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036531.1.7",
          "form": "그건 나도 몰라",
          "original_form": "그건 나도 몰라",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036531.1.8",
          "form": "종류는 무궁무짆9",
          "original_form": "종류는 무궁무짆9",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036531.1.9",
          "form": "앵무새는 야행성이 아니야?",
          "original_form": "앵무새는 야행성이 아니야?",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036531.1.10",
          "form": "모란앵무 중에서도 색에 따라 모양에 따라 좀 나뉘어",
          "original_form": "모란앵무 중에서도 색에 따라 모양에 따라 좀 나뉘어",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036531.1.11",
          "form": "아냐 근데 새벽에 ㄲ9",
          "original_form": "아냐 근데 새벽에 ㄲ9",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036531.1.12",
          "form": "우와 신기하다",
          "original_form": "우와 신기하다",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036531.1.13",
          "form": "가끔 새벽에 깨서 지저귀는데",
          "original_form": "가끔 새벽에 깨서 지저귀는데",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036531.1.14",
          "form": "깨며는 너를 깨우니??",
          "original_form": "깨며는 너를 깨우니??",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036531.1.15",
          "form": "나도 같이 깨",
          "original_form": "나도 같이 깨",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036531.1.16",
          "form": "두번정도 무시하면 다시 자는 듯 해",
          "original_form": "두번정도 무시하면 다시 자는 듯 해",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036531.1.17",
          "form": "그럼 부모님은 화 안내셔?",
          "original_form": "그럼 부모님은 화 안내셔?",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036531.1.18",
          "form": "그럼 나도 잘 수ㅠ있어",
          "original_form": "그럼 나도 잘 수ㅠ있어",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036531.1.19",
          "form": "부모님은 모르는 척 ㅎ9",
          "original_form": "부모님은 모르는 척 ㅎ9",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036531.1.20",
          "form": "오늘은 내 방에서 재우고 있어",
          "original_form": "오늘은 내 방에서 재우고 있어",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036531.1.21",
          "form": "나는 다른집 새가 우는건 너무 힘들었어",
          "original_form": "나는 다른집 새가 우는건 너무 힘들었어",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036531.1.22",
          "form": "깨우고싶야",
          "original_form": "깨우고싶야",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036531.1.23",
          "form": "깨우면 짜증낼까?",
          "original_form": "깨우면 짜증낼까?",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036531.1.24",
          "form": "유기견들 참 안됐어",
          "original_form": "유기견들 참 안됐어",
          "speaker_id": "1",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036531.1.25",
          "form": "예쁜애들인데 정말 왜 애를 유기하는걸까",
          "original_form": "예쁜애들인데 정말 왜 애를 유기하는걸까",
          "speaker_id": "1",
          "time": "20210831 00:12"
        }
      ]
    }
