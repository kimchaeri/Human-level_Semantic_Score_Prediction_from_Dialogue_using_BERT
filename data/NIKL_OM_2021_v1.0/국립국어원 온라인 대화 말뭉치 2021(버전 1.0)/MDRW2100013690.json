        },
        {
          "id": "MDRW2100013690.1.2",
          "form": "탄산수떨어졌어요",
          "original_form": "탄산수떨어졌어요",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013690.1.3",
          "form": "이번에께 더 낫던가??",
          "original_form": "이번에께 더 낫던가??",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013690.1.4",
          "form": "아빠가 잘마시더라",
          "original_form": "아빠가 잘마시더라",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013690.1.5",
          "form": "탄산수",
          "original_form": "탄산수",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013690.1.6",
          "form": "그럼그거살까요",
          "original_form": "그럼그거살까요",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013690.1.7",
          "form": "그래그거좋더라",
          "original_form": "그래그거좋더라",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013690.1.8",
          "form": "요즘탄산음료를 탄산수로 바꿔서 살빠진듯",
          "original_form": "요즘탄산음료를 탄산수로 바꿔서 살빠진듯",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013690.1.9",
          "form": "그래너많이빠졌어",
          "original_form": "그래너많이빠졌어",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013690.1.10",
          "form": "뿌듯뿌듯",
          "original_form": "뿌듯뿌듯",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013690.1.11",
          "form": "근데 너무 많이 마시면",
          "original_form": "근데 너무 많이 마시면",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013690.1.12",
          "form": "목이아파요",
          "original_form": "목이아파요",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013690.1.13",
          "form": "그래위에도안좋단다",
          "original_form": "그래위에도안좋단다",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013690.1.14",
          "form": "위에안좋으면 엄마한테는진짜",
          "original_form": "위에안좋으면 엄마한테는진짜",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013690.1.15",
          "form": "그래안먹는다그래서",
          "original_form": "그래안먹는다그래서",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013690.1.16",
          "form": "고기먹을때 치킨먹을때만먹어요ㅎㅎㅎ",
          "original_form": "고기먹을때 치킨먹을때만먹어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013690.1.17",
          "form": "저도줄여야겠어요",
          "original_form": "저도줄여야겠어요",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013690.1.18",
          "form": "그래너도목이안좋다",
