      "utterance": [
        {
          "id": "MDRW2100041913.1.1",
          "form": "이제 어른들 같이 노는 일은 없을 거 같고",
          "original_form": "이제 어른들 같이 노는 일은 없을 거 같고",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041913.1.2",
          "form": "애들끼리만 놀겠지",
          "original_form": "애들끼리만 놀겠지",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041913.1.3",
          "form": "괜히 어른들이 끼어서",
          "original_form": "괜히 어른들이 끼어서",
          "speaker_id": "2",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041913.1.4",
          "form": "안 그럼 애들 이집트 집 다니면서 편하게 놀 텐데",
          "original_form": "안 그럼 애들 이집트 집 다니면서 편하게 놀 텐데",
          "speaker_id": "2",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041913.1.5",
          "form": "근데 애들 일에는 어쩔 수 없는 듯 요;;",
          "original_form": "근데 애들 일에는 어쩔 수 없는 듯 요;;",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041913.1.6",
          "form": "이집트 집 이라니 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이집트 집 이라니 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041913.1.7",
          "form": "그래 그냥 그러려니해야지 뭐",
          "original_form": "그래 그냥 그러려니해야지 뭐",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041913.1.8",
          "form": "다 자기애가 소중하지",
          "original_form": "다 자기애가 소중하지",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041913.1.9",
          "form": "맞아요 애 일은 다들 어렵다ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 애 일은 다들 어렵다ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041913.1.10",
          "form": "그래 우린 애들끼리 안봐서 좋네 ㅋㅋㄱㄱㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래 우린 애들끼리 안봐서 좋네 ㅋㅋㄱㄱㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041913.1.11",
          "form": "싸울 일이 없음ㅋㅋㅋㅋㅋ",
          "original_form": "싸울 일이 없음ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041913.1.12",
          "form": "근데 언니 오래가는 사이 보면",
          "original_form": "근데 언니 오래가는 사이 보면",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041913.1.13",
          "form": "어른들끼리만 놈ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어른들끼리만 놈ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041913.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그러이",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그러이",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041913.1.15",
          "form": "일단 애들 무시",
          "original_form": "일단 애들 무시",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041913.1.16",
          "form": "어른끼리 친해야지",
          "original_form": "어른끼리 친해야지",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041913.1.17",
          "form": "우리가 놀라고 만남ㅋㅋㅋㅋㅋㅋ",
          "original_form": "우리가 놀라고 만남ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100041913.1.18",
          "form": "애들은 뭐 싸울 수밖에 없으니ㅋㅋㅋㅋ",
          "original_form": "애들은 뭐 싸울 수밖에 없으니ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100041913.1.19",
          "form": "ㅋㅋㅋㅋㅋ 그러이",
          "original_form": "ㅋㅋㅋㅋㅋ 그러이",
          "speaker_id": "1",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100041913.1.20",
          "form": "애들 싸울거같으니",
          "original_form": "애들 싸울거같으니",
          "speaker_id": "1",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100041913.1.21",
          "form": "놔두고 우리끼리 만나잖아 ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "놔두고 우리끼리 만나잖아 ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100041913.1.22",
          "form": "애들은 따로 놀고",
          "original_form": "애들은 따로 놀고",
          "speaker_id": "1",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100041913.1.23",
          "form": "내가 즐거워야 해 염ㅋㅋㅋ",
          "original_form": "내가 즐거워야 해 염ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100041913.1.24",
          "form": "애들은 알아서 친구 만들어서 놀아라ㅋㅋㅋㅋㅋ",
          "original_form": "애들은 알아서 친구 만들어서 놀아라ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100041913.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그래 지친구 내친구 따로 놀아야지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그래 지친구 내친구 따로 놀아야지",
          "speaker_id": "1",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041913.1.26",
          "form": "같이 놀면 속시끄럽다",
          "original_form": "같이 놀면 속시끄럽다",
          "speaker_id": "1",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041913.1.27",
          "form": "그러니까요ㅋㅋㅋㅋㅋ",
          "original_form": "그러니까요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041913.1.28",
          "form": "우리도 놀이터에서 얼마나 많이 헤어졌는 가 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "우리도 놀이터에서 얼마나 많이 헤어졌는 가 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041913.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:15"
        },
        {
          "id": "MDRW2100041913.1.30",
          "form": "너의 찌그러진 얼굴이 떠오르네",
          "original_form": "너의 찌그러진 얼굴이 떠오르네",
          "speaker_id": "1",
          "time": "20210906 19:15"
        },
        {
          "id": "MDRW2100041913.1.31",
          "form": "우리힐데스첫째",
          "original_form": "우리힐데스첫째",
          "speaker_id": "1",
          "time": "20210906 19:15"
        },
        {
          "id": "MDRW2100041913.1.32",
          "form": "많이 컸다 정말",
          "original_form": "많이 컸다 정말",
          "speaker_id": "1",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100041913.1.33",
          "form": "둘째는 놀이터 근처도 안가잖아요ㅋㅋㅋㅋㅋ",
          "original_form": "둘째는 놀이터 근처도 안가잖아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100041913.1.34",
