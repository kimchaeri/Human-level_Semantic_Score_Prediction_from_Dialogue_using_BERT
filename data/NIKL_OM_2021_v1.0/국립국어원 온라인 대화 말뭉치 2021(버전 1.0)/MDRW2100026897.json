      "utterance": [
        {
          "id": "MDRW2100026897.1.1",
          "form": "혹시 반려동물 키우시나묘",
          "original_form": "혹시 반려동물 키우시나묘",
          "speaker_id": "1",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100026897.1.2",
          "form": "푸들 키웁니다",
          "original_form": "푸들 키웁니다",
          "speaker_id": "2",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100026897.1.3",
          "form": "오 무슨색인가요",
          "original_form": "오 무슨색인가요",
          "speaker_id": "1",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100026897.1.4",
          "form": "나이는 4살이고 까만색이에요",
          "original_form": "나이는 4살이고 까만색이에요",
          "speaker_id": "2",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100026897.1.5",
          "form": "유기견이었어요",
          "original_form": "유기견이었어요",
          "speaker_id": "2",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100026897.1.6",
          "form": "귀엽겠네요",
          "original_form": "귀엽겠네요",
          "speaker_id": "1",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100026897.1.7",
          "form": "유기견센터에서 입양하셨나요",
          "original_form": "유기견센터에서 입양하셨나요",
          "speaker_id": "1",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100026897.1.8",
          "form": "믹스라 영리합니다",
          "original_form": "믹스라 영리합니다",
          "speaker_id": "2",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100026897.1.9",
          "form": "좋은 가족을 만났네요",
          "original_form": "좋은 가족을 만났네요",
          "speaker_id": "1",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100026897.1.10",
          "form": "애견샵에 임시보호된 강아지였어요",
          "original_form": "애견샵에 임시보호된 강아지였어요",
          "speaker_id": "2",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100026897.1.11",
          "form": "반려동물은 사랑입니다",
          "original_form": "반려동물은 사랑입니다",
          "speaker_id": "2",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100026897.1.12",
          "form": "어떤 고기를 좋야아나요",
          "original_form": "어떤 고기를 좋야아나요",
          "speaker_id": "1",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100026897.1.13",
          "form": "좋아하나요",
          "original_form": "좋아하나요",
          "speaker_id": "1",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100026897.1.14",
          "form": "개고기 말씀인가요?",
          "original_form": "개고기 말씀인가요?",
          "speaker_id": "2",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100026897.1.15",
          "form": "푸들이 어떤고기를 좋아하나요",
          "original_form": "푸들이 어떤고기를 좋아하나요",
          "speaker_id": "1",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100026897.1.16",
          "form": "제친구가 키우는 개는 닭 알러지 있어서",
          "original_form": "제친구가 키우는 개는 닭 알러지 있어서",
          "speaker_id": "1",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100026897.1.17",
          "form": "잡식입니다",
          "original_form": "잡식입니다",
          "speaker_id": "2",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100026897.1.18",
          "form": "오리고기 먹더라구요",
          "original_form": "오리고기 먹더라구요",
          "speaker_id": "1",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100026897.1.19",
          "form": "건사료만 먹이시나요",
          "original_form": "건사료만 먹이시나요",
          "speaker_id": "1",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100026897.1.20",
          "form": "믹스가 영리하죠",
          "original_form": "믹스가 영리하죠",
          "speaker_id": "1",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100026897.1.21",
          "form": "우유껌을 좋아합니다",
          "original_form": "우유껌을 좋아합니다",
          "speaker_id": "2",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100026897.1.22",
          "form": "잡식이 건강하고",
          "original_form": "잡식이 건강하고",
          "speaker_id": "1",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100026897.1.23",
          "form": "고양이 키워 보셨나요",
          "original_form": "고양이 키워 보셨나요",
          "speaker_id": "2",
          "time": "20210904 13:29"
        }
      ]
