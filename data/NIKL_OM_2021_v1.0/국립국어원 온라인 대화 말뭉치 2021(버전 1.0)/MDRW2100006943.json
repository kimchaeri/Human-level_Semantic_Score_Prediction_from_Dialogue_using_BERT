      "utterance": [
        {
          "id": "MDRW2100006943.1.1",
          "form": "포실포실하니",
          "original_form": "포실포실하니",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006943.1.2",
          "form": "털찐 아이들",
          "original_form": "털찐 아이들",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006943.1.3",
          "form": "너무 귀여워요",
          "original_form": "너무 귀여워요",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006943.1.4",
          "form": "맞아요 그 털찐 상태",
          "original_form": "맞아요 그 털찐 상태",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006943.1.5",
          "form": "젖으면 홀쭉해지는게 반전포인트에요",
          "original_form": "젖으면 홀쭉해지는게 반전포인트에요",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006943.1.6",
          "form": "맞아요ㅋㅋㅋㅋ 넘나 귀욤",
          "original_form": "맞아요ㅋㅋㅋㅋ 넘나 귀욤",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006943.1.7",
          "form": "그러시면 강아지랑 고양이 중에 강아지파이신가요?",
          "original_form": "그러시면 강아지랑 고양이 중에 강아지파이신가요?",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006943.1.8",
          "form": "네ㅜㅜ 소형견이랑 고양이는... 약간 사나운 게 있어서",
          "original_form": "네ㅜㅜ 소형견이랑 고양이는... 약간 사나운 게 있어서",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006943.1.9",
          "form": "아 진짜요? 전 작은애들이 귀엽다고 생각했는데",
          "original_form": "아 진짜요? 전 작은애들이 귀엽다고 생각했는데",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006943.1.10",
          "form": "역시 작은애들이 성격이 있군요",
          "original_form": "역시 작은애들이 성격이 있군요",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006943.1.11",
          "form": "크기로 보면 작은 게 귀엽긴 한데",
          "original_form": "크기로 보면 작은 게 귀엽긴 한데",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006943.1.12",
          "form": "크기랑 성격이 반비례 같아요",
          "original_form": "크기랑 성격이 반비례 같아요",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006943.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006943.1.14",
          "form": "세상에나 ㅋㅋㅋㄱㅋ",
          "original_form": "세상에나 ㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006943.1.15",
          "form": "저도 직장동료가 키우는 작은 강아지 있는데 성격이 장난아니에요",
          "original_form": "저도 직장동료가 키우는 작은 강아지 있는데 성격이 장난아니에요",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006943.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006943.1.17",
          "form": "몸집이 작읈수록 경계태세에 놓여서 그런 것 같아요",
          "original_form": "몸집이 작읈수록 경계태세에 놓여서 그런 것 같아요",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006943.1.18",
          "form": "이름이 name3인데 그래서 맨날 name3님 name3님 이러면서 모시고 있어요",
          "original_form": "이름이 &name3&인데 그래서 맨날 &name3&님 &name3&님 이러면서 모시고 있어요",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006943.1.19",
          "form": "name3님ㅋㅋㅋㅋㅋ",
          "original_form": "&name3&님ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006943.1.20",
          "form": "언제나 인간은 집사가 되죠",
          "original_form": "언제나 인간은 집사가 되죠",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006943.1.21",
          "form": "그런데 진짜 귀여워요 푸들이랑 비숑 혼혈?",
          "original_form": "그런데 진짜 귀여워요 푸들이랑 비숑 혼혈?",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006943.1.22",
          "form": "오 둘 다 귀여운 종이에요",
          "original_form": "오 둘 다 귀여운 종이에요",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006943.1.23",
