      "utterance": [
        {
          "id": "MDRW2100029098.1.1",
          "form": "아니요ㅠㅠ 다쿠아즈 하나 먹었어요",
          "original_form": "아니요ㅠㅠ 다쿠아즈 하나 먹었어요",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029098.1.2",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029098.1.3",
          "form": "ㅜㅠ배고프시지 않나요?ㅠㅠ",
          "original_form": "ㅜㅠ배고프시지 않나요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029098.1.4",
          "form": "전 저녁 먹었어요ㅎㄹ",
          "original_form": "전 저녁 먹었어요ㅎㄹ",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029098.1.5",
          "form": "배고파요...ㅠㅠ",
          "original_form": "배고파요...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029098.1.6",
          "form": "뭐 드셨는지 자랑해주세요!!!",
          "original_form": "뭐 드셨는지 자랑해주세요!!!",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029098.1.7",
          "form": "전 굽네치킨 고 추 바사삭을 먹었어요ㅋㅋㅋ",
          "original_form": "전 굽네치킨 고 추 바사삭을 먹었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029098.1.8",
          "form": "와... 진짜 행벅하시겠어요,,,",
          "original_form": "와... 진짜 행벅하시겠어요,,,",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029098.1.9",
          "form": "굽네는 맛있는거 많은 것 같아요",
          "original_form": "굽네는 맛있는거 많은 것 같아요",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029098.1.10",
          "form": "저는 갈비천왕도 맛있더라구요! 무난갑",
          "original_form": "저는 갈비천왕도 맛있더라구요! 무난갑",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029098.1.11",
          "form": "그렇군요ㅎㅎ 갈비천왕 얘기 많이 들어봐서 담에 한번 먹어봐야겟네요",
          "original_form": "그렇군요ㅎㅎ 갈비천왕 얘기 많이 들어봐서 담에 한번 먹어봐야겟네요",
          "speaker_id": "1",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029098.1.12",
          "form": "전 볼케이노 자주 먹었었어요ㅋㅋ",
          "original_form": "전 볼케이노 자주 먹었었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029098.1.13",
          "form": "그것도 유명하잖아요 많이 매워요??",
          "original_form": "그것도 유명하잖아요 많이 매워요??",
          "speaker_id": "2",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029098.1.14",
          "form": "네 제가 불닭 기본도 잘 못먹는 편이라서 매워요ㅋㅋ 근데 맛은 있으니까 시켜 먹어요ㅋㅋ",
          "original_form": "네 제가 불닭 기본도 잘 못먹는 편이라서 매워요ㅋㅋ 근데 맛은 있으니까 시켜 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029098.1.15",
          "form": "소스도 따로 팔았던 걸로 기억하는데 너무 궁금해요!!",
          "original_form": "소스도 따로 팔았던 걸로 기억하는데 너무 궁금해요!!",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029098.1.16",
          "form": "생각해보면 왤케 다 먹겠다고 다짐만 하고 안먹었을까 싶네요ㅋㅋㅋㅠㅠ",
          "original_form": "생각해보면 왤케 다 먹겠다고 다짐만 하고 안먹었을까 싶네요ㅋㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 00:09"
        }
      ]
