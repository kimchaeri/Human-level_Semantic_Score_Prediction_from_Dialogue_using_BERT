      "utterance": [
        {
          "id": "MDRW2100038228.1.1",
          "form": "진짜자주하더라구요요즘..?",
          "original_form": "진짜자주하더라구요요즘..?",
          "speaker_id": "2",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100038228.1.2",
          "form": "안그래도 저 그래서 고민인게",
          "original_form": "안그래도 저 그래서 고민인게",
          "speaker_id": "1",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100038228.1.3",
          "form": "네네 ㅇㅁㅇ",
          "original_form": "네네 ㅇㅁㅇ",
          "speaker_id": "2",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100038228.1.4",
          "form": "요기요 슈퍼클럽 가입 안한게 제 마지노선같은 건데",
          "original_form": "요기요 슈퍼클럽 가입 안한게 제 마지노선같은 건데",
          "speaker_id": "1",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038228.1.5",
          "form": "엌..저도..",
          "original_form": "엌..저도..",
          "speaker_id": "2",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038228.1.6",
          "form": "어차피 열번은 먹는데 왜 가입안하고 있죶ㅋㅋ",
          "original_form": "어차피 열번은 먹는데 왜 가입안하고 있죶ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038228.1.7",
          "form": "네번만 써도 이득인데ㅋㅋ",
          "original_form": "네번만 써도 이득인데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038228.1.8",
          "form": "이럴거면가입하는게맞는거니 라고 저한테묻곤하죠",
          "original_form": "이럴거면가입하는게맞는거니 라고 저한테묻곤하죠",
          "speaker_id": "2",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038228.1.9",
          "form": "ㄱㅈㅋㅈㄱㅈㄱㅈㄱㅈㄱㄷㄱ",
          "original_form": "ㄱㅈㅋㅈㄱㅈㄱㅈㄱㅈㄱㄷㄱ",
          "speaker_id": "2",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038228.1.10",
          "form": "그러게요ㅋㄱㅋㄱㅋㅋ",
          "original_form": "그러게요ㅋㄱㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038228.1.11",
          "form": "뭔가 슈퍼클럽 가입 안한걱",
          "original_form": "뭔가 슈퍼클럽 가입 안한걱",
          "speaker_id": "1",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038228.1.12",
          "form": "내가 이렇게 자제하고 있다는 상징 그이상도 이하도 아님요ㅋㅋ",
          "original_form": "내가 이렇게 자제하고 있다는 상징 그이상도 이하도 아님요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038228.1.13",
          "form": "근데그걸사면\r\n이득보려고억지로사먹을까봐..?",
          "original_form": "근데그걸사면\r\n이득보려고억지로사먹을까봐..?",
          "speaker_id": "2",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038228.1.14",
          "form": "라는걱정을하며안하고있는듸ㅣ.ㅈ....",
          "original_form": "라는걱정을하며안하고있는듸ㅣ.ㅈ....",
          "speaker_id": "2",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038228.1.15",
          "form": "ㅋㅈㄱㅈㄱㅈㄱㅈㄱㄷㄱ",
          "original_form": "ㅋㅈㄱㅈㄱㅈㄱㅈㄱㄷㄱ",
          "speaker_id": "2",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038228.1.16",
          "form": "ㅎㅎㅎ하지만 그러면서 요기요 자체행사에 낚이고 있는 우리ㅋㅋ",
          "original_form": "ㅎㅎㅎ하지만 그러면서 요기요 자체행사에 낚이고 있는 우리ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038228.1.17",
          "form": "그러게요ㅋㅋㅋ가입햇으면벌써본전건졌을듯",
          "original_form": "그러게요ㅋㅋㅋ가입햇으면벌써본전건졌을듯",
          "speaker_id": "2",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038228.1.18",
          "form": "사실 오늘 치킨사면서도 계속 고민했지만",
          "original_form": "사실 오늘 치킨사면서도 계속 고민했지만",
          "speaker_id": "1",
          "time": "20210902 01:25"
        },
        {
          "id": "MDRW2100038228.1.19",
          "form": "우선 자제하는 나 콘셉트를 하루 더 유지하기로ㅎㅎ",
          "original_form": "우선 자제하는 나 콘셉트를 하루 더 유지하기로ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 01:25"
        },
        {
          "id": "MDRW2100038228.1.20",
