      "utterance": [
        {
          "id": "MDRW2100026056.1.1",
          "form": "교회 갈까?",
          "original_form": "교회 갈까?",
          "speaker_id": "1",
          "time": "20210901 19:25"
        },
        {
          "id": "MDRW2100026056.1.2",
          "form": "온라인예배만 하잖아",
          "original_form": "온라인예배만 하잖아",
          "speaker_id": "2",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100026056.1.3",
          "form": "못감 ㄴㄴ",
          "original_form": "못감 ㄴㄴ",
          "speaker_id": "2",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100026056.1.4",
          "form": "나는 잘모르겠어 믿음생활",
          "original_form": "나는 잘모르겠어 믿음생활",
          "speaker_id": "1",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100026056.1.5",
          "form": "교회 안간지 꽤 돴잖아",
          "original_form": "교회 안간지 꽤 돴잖아",
          "speaker_id": "2",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100026056.1.6",
          "form": "응 코로나로 망설여지더라",
          "original_form": "응 코로나로 망설여지더라",
          "speaker_id": "1",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100026056.1.7",
          "form": "교인들 태도가 좀",
          "original_form": "교인들 태도가 좀",
          "speaker_id": "1",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100026056.1.8",
          "form": "천주교 개종할까",
          "original_form": "천주교 개종할까",
          "speaker_id": "2",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100026056.1.9",
          "form": "그거나 이거나 비슷하지",
          "original_form": "그거나 이거나 비슷하지",
          "speaker_id": "1",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100026056.1.10",
          "form": "오히려 불교가나은듯. 호국불교",
          "original_form": "오히려 불교가나은듯. 호국불교",
          "speaker_id": "2",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100026056.1.11",
          "form": "이번에 빛을 발했지",
          "original_form": "이번에 빛을 발했지",
          "speaker_id": "2",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100026056.1.12",
          "form": "천주교가 교회보단 낫긴하지",
          "original_form": "천주교가 교회보단 낫긴하지",
          "speaker_id": "1",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100026056.1.13",
          "form": "근데 일요일마다 죄책감들긔",
          "original_form": "근데 일요일마다 죄책감들긔",
          "speaker_id": "1",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100026056.1.14",
          "form": "괜찮아 다 마음속에 있지",
          "original_form": "괜찮아 다 마음속에 있지",
          "speaker_id": "2",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100026056.1.15",
          "form": "응 쉽지않아 믿음도",
          "original_form": "응 쉽지않아 믿음도",
          "speaker_id": "1",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100026056.1.16",
          "form": "하나님이 계시긴한데",
          "original_form": "하나님이 계시긴한데",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026056.1.17",
          "form": "실천이 어려워",
          "original_form": "실천이 어려워",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026056.1.18",
          "form": "어리석은 믿음이니까",
          "original_form": "어리석은 믿음이니까",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026056.1.19",
          "form": "재고따지면 누가 믿어",
          "original_form": "재고따지면 누가 믿어",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026056.1.20",
          "form": "교회는 코로나끝나믄 가자",
          "original_form": "교회는 코로나끝나믄 가자",
          "speaker_id": "1",
          "time": "20210901 19:30"
        }
      ]
