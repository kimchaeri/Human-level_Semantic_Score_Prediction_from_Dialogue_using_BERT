      "utterance": [
        {
          "id": "MDRW2100024449.1.1",
          "form": "어... 안예은씨 노래요",
          "original_form": "어... 안예은씨 노래요",
          "speaker_id": "2",
          "time": "20210831 10:09"
        },
        {
          "id": "MDRW2100024449.1.2",
          "form": "창귀가 은근 중독적임.",
          "original_form": "창귀가 은근 중독적임.",
          "speaker_id": "2",
          "time": "20210831 10:09"
        },
        {
          "id": "MDRW2100024449.1.3",
          "form": "아! 그거 얼마전에 나오 ㄴ노래인가요?",
          "original_form": "아! 그거 얼마전에 나오 ㄴ노래인가요?",
          "speaker_id": "1",
          "time": "20210831 10:10"
        },
        {
          "id": "MDRW2100024449.1.4",
          "form": "안들어봤어요",
          "original_form": "안들어봤어요",
          "speaker_id": "1",
          "time": "20210831 10:10"
        },
        {
          "id": "MDRW2100024449.1.5",
          "form": "네 한달 좀 안됐을걸요?",
          "original_form": "네 한달 좀 안됐을걸요?",
          "speaker_id": "2",
          "time": "20210831 10:10"
        },
        {
          "id": "MDRW2100024449.1.6",
          "form": "저도 알수없는 알고리즘 땜에 어쩌다 들었는데",
          "original_form": "저도 알수없는 알고리즘 땜에 어쩌다 들었는데",
          "speaker_id": "2",
          "time": "20210831 10:11"
        },
        {
          "id": "MDRW2100024449.1.7",
          "form": "은근 취향이라 계속 듣게 됐네요",
          "original_form": "은근 취향이라 계속 듣게 됐네요",
          "speaker_id": "2",
          "time": "20210831 10:11"
        },
        {
          "id": "MDRW2100024449.1.8",
          "form": "오.. 저도 들어봐야겠어요",
          "original_form": "오.. 저도 들어봐야겠어요",
          "speaker_id": "1",
          "time": "20210831 10:12"
        },
        {
          "id": "MDRW2100024449.1.9",
          "form": "저는 안예은씨 노래 중에 홍연이랑",
          "original_form": "저는 안예은씨 노래 중에 홍연이랑",
          "speaker_id": "1",
          "time": "20210831 10:12"
        },
        {
          "id": "MDRW2100024449.1.10",
          "form": "문어의꿈 좋아해요!",
          "original_form": "문어의꿈 좋아해요!",
          "speaker_id": "1",
          "time": "20210831 10:12"
        },
        {
          "id": "MDRW2100024449.1.11",
          "form": "저는 홀로봄 좋아해요",
          "original_form": "저는 홀로봄 좋아해요",
          "speaker_id": "2",
          "time": "20210831 10:16"
        },
        {
          "id": "MDRW2100024449.1.12",
          "form": "가사가 왠지 내 얘기같고 그런달까... ㅎ",
          "original_form": "가사가 왠지 내 얘기같고 그런달까... ㅎ",
          "speaker_id": "2",
          "time": "20210831 10:17"
        },
        {
          "id": "MDRW2100024449.1.13",
          "form": "그것도 안예은님 노래예요?",
          "original_form": "그것도 안예은님 노래예요?",
          "speaker_id": "1",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024449.1.14",
          "form": "처음들어봐요 두곡다 들어봐야겠어요",
          "original_form": "처음들어봐요 두곡다 들어봐야겠어요",
          "speaker_id": "1",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024449.1.15",
          "form": "함 들어보세요. 좋아요~",
          "original_form": "함 들어보세요. 좋아요~",
          "speaker_id": "2",
          "time": "20210831 10:58"
        },
        {
          "id": "MDRW2100024449.1.16",
          "form": "안예은님 목소리가....오ㅏ",
          "original_form": "안예은님 목소리가....오ㅏ",
          "speaker_id": "1",
          "time": "20210901 06:20"
        },
        {
          "id": "MDRW2100024449.1.17",
          "form": "더 떴으면 좋겠는데ㅜㅜ",
          "original_form": "더 떴으면 좋겠는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 06:21"
        },
        {
          "id": "MDRW2100024449.1.18",
          "form": "막 활동하시기엔 수줍은 편이셔서....",
          "original_form": "막 활동하시기엔 수줍은 편이셔서....",
          "speaker_id": "2",
          "time": "20210901 09:22"
        },
        {
          "id": "MDRW2100024449.1.19",
          "form": "그래도 차츰 매니아층이 늘어가고 있는것 같아요",
          "original_form": "그래도 차츰 매니아층이 늘어가고 있는것 같아요",
          "speaker_id": "2",
          "time": "20210901 09:22"
        },
        {
          "id": "MDRW2100024449.1.20",
          "form": "그럼 드라마 역적 보셨어요?",
          "original_form": "그럼 드라마 역적 보셨어요?",
          "speaker_id": "1",
          "time": "20210901 10:50"
        },
        {
          "id": "MDRW2100024449.1.21",
          "form": "그게 홍연노래듣고 영감받아서 만든거래요",
          "original_form": "그게 홍연노래듣고 영감받아서 만든거래요",
          "speaker_id": "1",
          "time": "20210901 10:50"
        },
        {
          "id": "MDRW2100024449.1.22",
          "form": "저도 들었어요",
          "original_form": "저도 들었어요",
          "speaker_id": "2",
          "time": "20210901 10:55"
        },
        {
          "id": "MDRW2100024449.1.23",
          "form": "그래서 홍연 드라마버전이 따로 있잖아요!",
          "original_form": "그래서 홍연 드라마버전이 따로 있잖아요!",
          "speaker_id": "2",
          "time": "20210901 10:55"
        },
        {
          "id": "MDRW2100024449.1.24",
          "form": "그냥 드라마 장면보면 노래가 막",
          "original_form": "그냥 드라마 장면보면 노래가 막",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100024449.1.25",
          "form": "자동재생돼요ㅋㅋ",
          "original_form": "자동재생돼요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100024449.1.26",
          "form": "드라마 아니어도 노래들으면 서사 하나가 뚝딱!",
          "original_form": "드라마 아니어도 노래들으면 서사 하나가 뚝딱!",
          "speaker_id": "2",
          "time": "20210901 10:59"
        },
        {
          "id": "MDRW2100024449.1.27",
