      "utterance": [
        {
          "id": "MDRW2100002641.1.1",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002641.1.2",
          "form": "커알못이라 커피 아닌 음료들만 마셔용ㅋㅋ",
          "original_form": "커알못이라 커피 아닌 음료들만 마셔용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002641.1.3",
          "form": "커피 좋아하시나요?",
          "original_form": "커피 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002641.1.4",
          "form": "카페가서 그럼 무슨 음료 드세요??",
          "original_form": "카페가서 그럼 무슨 음료 드세요??",
          "speaker_id": "1",
          "time": "20210820 14:19"
        },
        {
          "id": "MDRW2100002641.1.5",
          "form": "저는 커피 좋아해요!",
          "original_form": "저는 커피 좋아해요!",
          "speaker_id": "1",
          "time": "20210820 14:19"
        },
        {
          "id": "MDRW2100002641.1.6",
          "form": "스벅가면 프라푸치노 먹어요:)",
          "original_form": "스벅가면 프라푸치노 먹어요:)",
          "speaker_id": "2",
          "time": "20210820 14:23"
        },
        {
          "id": "MDRW2100002641.1.7",
          "form": "어떤 커피 좋아하세요?",
          "original_form": "어떤 커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 14:23"
        },
        {
          "id": "MDRW2100002641.1.8",
          "form": "저는 바닐라라떼랑 도레라떼 좋아해요",
          "original_form": "저는 바닐라라떼랑 도레라떼 좋아해요",
          "speaker_id": "1",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002641.1.9",
          "form": "프라푸치노 맛있죠!",
          "original_form": "프라푸치노 맛있죠!",
          "speaker_id": "1",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002641.1.10",
          "form": "라떼 종류를 좋아하시나봐용",
          "original_form": "라떼 종류를 좋아하시나봐용",
          "speaker_id": "2",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002641.1.11",
          "form": "도레라떼는 처음 들어봐용",
          "original_form": "도레라떼는 처음 들어봐용",
          "speaker_id": "2",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002641.1.12",
          "form": "네 우유류를 좋아해서!",
          "original_form": "네 우유류를 좋아해서!",
          "speaker_id": "1",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002641.1.13",
          "form": "돌체라떼 오타났어요 ㅠㅠㅠㅠ",
          "original_form": "돌체라떼 오타났어요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002641.1.14",
          "form": "아항! 돌체라떼!!",
          "original_form": "아항! 돌체라떼!!",
          "speaker_id": "2",
          "time": "20210820 14:48"
        },
        {
          "id": "MDRW2100002641.1.15",
          "form": "저희 엄마도 좋아하세요ㅋㅋㅋ",
          "original_form": "저희 엄마도 좋아하세요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:48"
        },
        {
          "id": "MDRW2100002641.1.16",
          "form": "여성분들은 거진 좋아하는거 같아여",
          "original_form": "여성분들은 거진 좋아하는거 같아여",
          "speaker_id": "2",
          "time": "20210820 14:48"
        },
        {
          "id": "MDRW2100002641.1.17",
          "form": "ㅋㅋㅋㅋ 그쵸 달달하니 스트레스 풀리는 맛이라!!",
          "original_form": "ㅋㅋㅋㅋ 그쵸 달달하니 스트레스 풀리는 맛이라!!",
          "speaker_id": "1",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002641.1.18",
          "form": "저도 주변에 돌체라떼 극찬하는\r\n분들이 많아서 마셔보고 싶긴 하더라구영ㅋ",
          "original_form": "저도 주변에 돌체라떼 극찬하는\r\n분들이 많아서 마셔보고 싶긴 하더라구영ㅋ",
          "speaker_id": "2",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002641.1.19",
          "form": "스트레스 풀리는 맛이라니 더 땡기네여",
          "original_form": "스트레스 풀리는 맛이라니 더 땡기네여",
          "speaker_id": "2",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002641.1.20",
          "form": "카페인을 못드셔서 추천하지못해 아쉽네요",
          "original_form": "카페인을 못드셔서 추천하지못해 아쉽네요",
          "speaker_id": "1",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002641.1.21",
          "form": "오늘은 불금인데 맛있는거 드시나요",
          "original_form": "오늘은 불금인데 맛있는거 드시나요",
          "speaker_id": "1",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002641.1.22",
          "form": "불금에는 왜케 치키니가 땡길까요ㅋㅋ",
          "original_form": "불금에는 왜케 치키니가 땡길까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002641.1.23",
          "form": "치키니 시킬까 하는데 뭐 드세용?",
          "original_form": "치키니 시킬까 하는데 뭐 드세용?",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002641.1.24",
          "form": "ㅋㅋㅋㅋ치킨에 맥주 캬 좋죠",
          "original_form": "ㅋㅋㅋㅋ치킨에 맥주 캬 좋죠",
          "speaker_id": "1",
          "time": "20210820 19:32"
        },
        {
          "id": "MDRW2100002641.1.25",
          "form": "저는 오늘 카레먹엇어요!",
          "original_form": "저는 오늘 카레먹엇어요!",
          "speaker_id": "1",
          "time": "20210820 19:32"
        },
        {
          "id": "MDRW2100002641.1.26",
          "form": "벌써 주말의 끝이 보이네요",
          "original_form": "벌써 주말의 끝이 보이네요",
