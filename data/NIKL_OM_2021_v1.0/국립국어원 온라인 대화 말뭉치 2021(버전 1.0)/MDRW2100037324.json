      "utterance": [
        {
          "id": "MDRW2100037324.1.1",
          "form": "송지효랑 겁나 엮잖아여 !!",
          "original_form": "송지효랑 겁나 엮잖아여 !!",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037324.1.2",
          "form": "맞아요!!!! 김종국 유튜브 보셨어요?!",
          "original_form": "맞아요!!!! 김종국 유튜브 보셨어요?!",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100037324.1.3",
          "form": "거기에도 나왔잖아요!",
          "original_form": "거기에도 나왔잖아요!",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100037324.1.4",
          "form": "아 네너 !",
          "original_form": "아 네너 !",
          "speaker_id": "1",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100037324.1.5",
          "form": "김종국 집에서 한거!",
          "original_form": "김종국 집에서 한거!",
          "speaker_id": "1",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100037324.1.6",
          "form": "맞죠?",
          "original_form": "맞죠?",
          "speaker_id": "1",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100037324.1.7",
          "form": "맞아요!!!! 그거 너무 웃겨요 ㅠ.^ 송지효 전날 술 마셔서",
          "original_form": "맞아요!!!! 그거 너무 웃겨요 ㅠ.^ 송지효 전날 술 마셔서",
          "speaker_id": "2",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100037324.1.8",
          "form": "촬영에 늦어버린..",
          "original_form": "촬영에 늦어버린..",
          "speaker_id": "2",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100037324.1.9",
          "form": "아 맞아옄ㅋㅋㅋㅋ",
          "original_form": "아 맞아옄ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100037324.1.10",
          "form": "둘이 완전 케미 쩔어여 ㅋㅋㄱㅋ 그편에서 ㅋㅋ",
          "original_form": "둘이 완전 케미 쩔어여 ㅋㅋㄱㅋ 그편에서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100037324.1.11",
          "form": "전 그 전소민 송지효 케미도 너무 좋어요 ㅎㅎㅎ!!!!!!",
          "original_form": "전 그 전소민 송지효 케미도 너무 좋어요 ㅎㅎㅎ!!!!!!",
          "speaker_id": "2",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100037324.1.12",
          "form": "아 맞야오!!!",
          "original_form": "아 맞야오!!!",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
