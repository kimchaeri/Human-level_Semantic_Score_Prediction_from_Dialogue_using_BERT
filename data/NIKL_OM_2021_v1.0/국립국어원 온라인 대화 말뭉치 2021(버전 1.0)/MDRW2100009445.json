        {
          "id": "MDRW2100009445.1.1",
          "form": "집안일분담해요?",
          "original_form": "집안일분담해요?",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009445.1.2",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009445.1.3",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009445.1.4",
          "form": "전업이니 크게 분담 하는건 없는것 같은데",
          "original_form": "전업이니 크게 분담 하는건 없는것 같은데",
          "speaker_id": "2",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009445.1.5",
          "form": "분리수거 같은건 해줘요",
          "original_form": "분리수거 같은건 해줘요",
          "speaker_id": "2",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009445.1.6",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "1",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009445.1.7",
          "form": "좀 도와주시죠?",
          "original_form": "좀 도와주시죠?",
          "speaker_id": "2",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009445.1.8",
          "form": "전 설거지 화장실 전담이요",
          "original_form": "전 설거지 화장실 전담이요",
          "speaker_id": "1",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100009445.1.9",
          "form": "맞벌이신가봐요",
          "original_form": "맞벌이신가봐요",
          "speaker_id": "2",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100009445.1.10",
          "form": "외벌이요",
          "original_form": "외벌이요",
          "speaker_id": "1",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100009445.1.11",
          "form": "아~와",
          "original_form": "아~와",
          "speaker_id": "2",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100009445.1.12",
          "form": "외벌인데ㅋㅋ 그런거면 많이 도와주시네요",
          "original_form": "외벌인데ㅋㅋ 그런거면 많이 도와주시네요",
          "speaker_id": "2",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100009445.1.13",
          "form": "외벌이신데 그정도면 많이 도와주시는것 같네요~",
          "original_form": "외벌이신데 그정도면 많이 도와주시는것 같네요~",
          "speaker_id": "2",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009445.1.14",
          "form": "때론 퇴근해서 나는 집에서 밥 먹은적도 없는데 설거지한다고 투정부리면 내자식 먹은 거라고 해서 찍소리 안하고ㅜ해여",
          "original_form": "때론 퇴근해서 나는 집에서 밥 먹은적도 없는데 설거지한다고 투정부리면 내자식 먹은 거라고 해서 찍소리 안하고ㅜ해여",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009445.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ. 착하시다~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ. 착하시다~",
          "speaker_id": "2",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009445.1.16",
          "form": "지는게 편해요",
          "original_form": "지는게 편해요",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009445.1.17",
          "form": "삶의 지혜죠",
          "original_form": "삶의 지혜죠",
          "speaker_id": "1",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009445.1.18",
          "form": "저희신랑은 퇴근해서 오면 저밥차리는 동안 애봐주면 밥 먹고 나면 둘째 씻기기만 하면 거의 끝.",
          "original_form": "저희신랑은 퇴근해서 오면 저밥차리는 동안 애봐주면 밥 먹고 나면 둘째 씻기기만 하면 거의 끝.",
          "speaker_id": "2",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009445.1.19",
          "form": "그담 쓰.레.기 .분리수거는 매일 하는거 아니니 여튼 그래요ㅠ",
          "original_form": "그담 쓰.레.기 .분리수거는 매일 하는거 아니니 여튼 그래요ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100009445.1.20",
          "form": "삶의 지혜를 저희 신랑은 전혀ㅋㅋ터득하지 못했어요ㅋ",
          "original_form": "삶의 지혜를 저희 신랑은 전혀ㅋㅋ터득하지 못했어요ㅋ",
          "speaker_id": "2",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100009445.1.21",
          "form": "아  우리집은 분리수거 매일 버려요 와이프가 쌓여 있는거 못봐서",
          "original_form": "아  우리집은 분리수거 매일 버려요 와이프가 쌓여 있는거 못봐서",
          "speaker_id": "1",
          "time": "20210822 23:36"
        }
      ]
