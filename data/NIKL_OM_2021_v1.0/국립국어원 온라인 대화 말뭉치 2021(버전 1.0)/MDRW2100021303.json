      "utterance": [
        {
          "id": "MDRW2100021303.1.1",
          "form": "이번에 뮤지컬 하는것도 다 거리두기 좌석하려나?",
          "original_form": "이번에 뮤지컬 하는것도 다 거리두기 좌석하려나?",
          "speaker_id": "1",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021303.1.2",
          "form": "아마 그렇지 않을까? 4단계니까",
          "original_form": "아마 그렇지 않을까? 4단계니까",
          "speaker_id": "2",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021303.1.3",
          "form": "ㅠㅠ한자리씩만 띄워 앉으면 좋겠다",
          "original_form": "ㅠㅠ한자리씩만 띄워 앉으면 좋겠다",
          "speaker_id": "1",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100021303.1.4",
          "form": "맞아맞아 내 자리가 없어...띄어앉기하면...",
          "original_form": "맞아맞아 내 자리가 없어...띄어앉기하면...",
          "speaker_id": "2",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100021303.1.5",
          "form": "맞아...두자리 띄워앉기 할때 최악",
          "original_form": "맞아...두자리 띄워앉기 할때 최악",
          "speaker_id": "1",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021303.1.6",
          "form": "진짜진짜 최악이야...",
          "original_form": "진짜진짜 최악이야...",
          "speaker_id": "2",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021303.1.7",
          "form": "가뜩이나 티켓팅 못하는데..",
          "original_form": "가뜩이나 티켓팅 못하는데..",
          "speaker_id": "2",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021303.1.8",
          "form": "아 그니까..저러면 티켓팅 해도 문제인게",
          "original_form": "아 그니까..저러면 티켓팅 해도 문제인게",
          "speaker_id": "1",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021303.1.9",
          "form": "또 취소되고 그러면 어케",
          "original_form": "또 취소되고 그러면 어케",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021303.1.10",
          "form": "표가 취소된다고..",
          "original_form": "표가 취소된다고..",
          "speaker_id": "1",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021303.1.11",
          "form": "그니깐..",
          "original_form": "그니깐..",
          "speaker_id": "1",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021303.1.12",
          "form": "저번에도 그거때문에 스트레스 많이 받았잖아",
          "original_form": "저번에도 그거때문에 스트레스 많이 받았잖아",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021303.1.13",
          "form": "어 젠틀맨스가이드 때 무려 한자리 가지고 4번인가?",
          "original_form": "어 젠틀맨스가이드 때 무려 한자리 가지고 4번인가?",
          "speaker_id": "1",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021303.1.14",
          "form": "아 맞아맞아 그때 진짜 언니 엄청 짜증난다고 했었잖아",
          "original_form": "아 맞아맞아 그때 진짜 언니 엄청 짜증난다고 했었잖아",
          "speaker_id": "2",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100021303.1.15",
          "form": "그니까 당해보면 알걸 정말 최악이야..",
          "original_form": "그니까 당해보면 알걸 정말 최악이야..",
          "speaker_id": "1",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100021303.1.16",
          "form": "아니 근데 김준수는 티켓팅 진짜 빡세더라",
          "original_form": "아니 근데 김준수는 티켓팅 진짜 빡세더라",
          "speaker_id": "1",
          "time": "20210828 18:33"
        }
