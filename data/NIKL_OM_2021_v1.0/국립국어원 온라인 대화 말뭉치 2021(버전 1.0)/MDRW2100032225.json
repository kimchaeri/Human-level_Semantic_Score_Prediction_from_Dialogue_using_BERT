      "utterance": [
        {
          "id": "MDRW2100032225.1.1",
          "form": "여행은 어디 가셨었나요?",
          "original_form": "여행은 어디 가셨었나요?",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032225.1.2",
          "form": "국내 말씀하시는건가요?",
          "original_form": "국내 말씀하시는건가요?",
          "speaker_id": "2",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032225.1.3",
          "form": "예",
          "original_form": "예",
          "speaker_id": "1",
          "time": "20210905 14:56"
        },
        {
          "id": "MDRW2100032225.1.4",
          "form": "국내요",
          "original_form": "국내요",
          "speaker_id": "1",
          "time": "20210905 14:56"
        },
        {
          "id": "MDRW2100032225.1.5",
          "form": "경주랑....양양이랑....강릉이랑...안동이랑...부산이랑...",
          "original_form": "경주랑....양양이랑....강릉이랑...안동이랑...부산이랑...",
          "speaker_id": "2",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100032225.1.6",
          "form": "여기저기 다녔네요!!",
          "original_form": "여기저기 다녔네요!!",
          "speaker_id": "2",
          "time": "20210905 15:15"
        },
        {
          "id": "MDRW2100032225.1.7",
          "form": "어디 다녀오셨나요?",
          "original_form": "어디 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210905 15:15"
        },
        {
          "id": "MDRW2100032225.1.8",
          "form": "이야..!!!!많이 갔다오셨네요",
          "original_form": "이야..!!!!많이 갔다오셨네요",
          "speaker_id": "1",
          "time": "20210905 15:30"
        },
        {
          "id": "MDRW2100032225.1.9",
          "form": "저도 속초랑...강릉....뭐....",
          "original_form": "저도 속초랑...강릉....뭐....",
          "speaker_id": "1",
          "time": "20210905 15:31"
        },
        {
          "id": "MDRW2100032225.1.10",
          "form": "어디가 좋았나요??",
          "original_form": "어디가 좋았나요??",
          "speaker_id": "1",
          "time": "20210905 15:31"
        },
        {
          "id": "MDRW2100032225.1.11",
          "form": "저는 양양이 좋았습니다",
          "original_form": "저는 양양이 좋았습니다",
          "speaker_id": "2",
          "time": "20210905 19:18"
        },
        {
          "id": "MDRW2100032225.1.12",
          "form": "어디가 가장 좋으셨나요?",
          "original_form": "어디가 가장 좋으셨나요?",
          "speaker_id": "2",
          "time": "20210905 19:18"
        },
        {
          "id": "MDRW2100032225.1.13",
          "form": "저는.......부산도 좋았어요~~",
          "original_form": "저는.......부산도 좋았어요~~",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032225.1.14",
          "form": "양양 어디 가샸나요??",
          "original_form": "양양 어디 가샸나요??",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032225.1.15",
          "form": "낙산사 다녀왔는데 바다랑 다 너무 이뻐서 좋았습니다~~",
          "original_form": "낙산사 다녀왔는데 바다랑 다 너무 이뻐서 좋았습니다~~",
          "speaker_id": "2",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032225.1.16",
          "form": "부산 어디가 기억에 남으시나요?",
          "original_form": "부산 어디가 기억에 남으시나요?",
          "speaker_id": "2",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032225.1.17",
          "form": "음 어떤 절 갔던거 같은데",
          "original_form": "음 어떤 절 갔던거 같은데",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032225.1.18",
          "form": "황금부처가 기억에 남네요",
          "original_form": "황금부처가 기억에 남네요",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032225.1.19",
          "form": "어딘지 기억에 안나는데",
          "original_form": "어딘지 기억에 안나는데",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032225.1.20",
          "form": "좋은곳 다녀오셨네요!",
          "original_form": "좋은곳 다녀오셨네요!",
          "speaker_id": "2",
          "time": "20210906 12:29"
        },
        {
          "id": "MDRW2100032225.1.21",
          "form": "해외여행은 어디 다녀오셨나요?",
          "original_form": "해외여행은 어디 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210906 12:29"
        },
        {
          "id": "MDRW2100032225.1.22",
          "form": "해외는 헝가리랑 필리핀이요",
          "original_form": "해외는 헝가리랑 필리핀이요",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
