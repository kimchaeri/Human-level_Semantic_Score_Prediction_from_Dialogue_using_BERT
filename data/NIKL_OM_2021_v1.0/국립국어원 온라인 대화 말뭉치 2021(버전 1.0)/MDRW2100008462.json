      "utterance": [
        {
          "id": "MDRW2100008462.1.1",
          "form": "뭐 챙겨보사는 것 있나요?",
          "original_form": "뭐 챙겨보사는 것 있나요?",
          "speaker_id": "2",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008462.1.2",
          "form": "아니요 근데 아빠가 야구보셔서 틀어놓으면 같이 봐요",
          "original_form": "아니요 근데 아빠가 야구보셔서 틀어놓으면 같이 봐요",
          "speaker_id": "1",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008462.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008462.1.4",
          "form": "저는 얼마전에 올림픽",
          "original_form": "저는 얼마전에 올림픽",
          "speaker_id": "2",
          "time": "20210822 16:01"
        },
        {
          "id": "MDRW2100008462.1.5",
          "form": "배구 열심히 챙겨봤었어요!",
          "original_form": "배구 열심히 챙겨봤었어요!",
          "speaker_id": "2",
          "time": "20210822 16:01"
        },
        {
          "id": "MDRW2100008462.1.6",
          "form": "야구는 어때요?",
          "original_form": "야구는 어때요?",
          "speaker_id": "2",
          "time": "20210822 16:01"
        },
        {
          "id": "MDRW2100008462.1.7",
          "form": "어 저도 배구 챙겨봤어요",
          "original_form": "어 저도 배구 챙겨봤어요",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008462.1.8",
          "form": "전 야구는 있으면 보고 없으면 안봐서..",
          "original_form": "전 야구는 있으면 보고 없으면 안봐서..",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008462.1.9",
          "form": "배구 진짜 대박이지않았나요",
          "original_form": "배구 진짜 대박이지않았나요",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008462.1.10",
          "form": "진짜 대박이었죠",
          "original_form": "진짜 대박이었죠",
          "speaker_id": "2",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008462.1.11",
          "form": "저 심장 터지는 줄 알았어요",
          "original_form": "저 심장 터지는 줄 알았어요",
          "speaker_id": "2",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008462.1.12",
          "form": "특히 터키전 보셨나요?",
          "original_form": "특히 터키전 보셨나요?",
          "speaker_id": "2",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008462.1.13",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008462.1.14",
          "form": "진짜 포기하지않고 끝까지 싸워준거",
          "original_form": "진짜 포기하지않고 끝까지 싸워준거",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008462.1.15",
          "form": "너무 대단하고 고맙더라구요",
          "original_form": "너무 대단하고 고맙더라구요",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008462.1.16",
          "form": "맞아요 선수분들 다 너무 멋있었어요 ㅠㅠ",
          "original_form": "맞아요 선수분들 다 너무 멋있었어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008462.1.17",
          "form": "저도 막 배구가 하고싶어지고 그러더라고요",
          "original_form": "저도 막 배구가 하고싶어지고 그러더라고요",
          "speaker_id": "2",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008462.1.18",
          "form": "시즌전도 관심이 가고여",
          "original_form": "시즌전도 관심이 가고여",
          "speaker_id": "2",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008462.1.19",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008462.1.20",
          "form": "배구 너무 재밌었어요",
          "original_form": "배구 너무 재밌었어요",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008462.1.21",
          "form": "이렇게 재밌는걸 이제야 알게 된게",
          "original_form": "이렇게 재밌는걸 이제야 알게 된게",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008462.1.22",
          "form": "뭔가 좋기도 하면서 한편으론 아쉬웠어요.\r\n좀 더 빨리 알았으면 좋았을걸",
          "original_form": "뭔가 좋기도 하면서 한편으론 아쉬웠어요.\r\n좀 더 빨리 알았으면 좋았을걸",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008462.1.23",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008462.1.24",
          "form": "여자배구 지원이 너무 적어서",
          "original_form": "여자배구 지원이 너무 적어서",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008462.1.25",
          "form": "그건그래요",
          "original_form": "그건그래요",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008462.1.26",
          "form": "더 응원 열심히해야겠다 생각도 들고",
          "original_form": "더 응원 열심히해야겠다 생각도 들고",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008462.1.27",
          "form": "관심도가 높아지면 좀 더 개선이 되겠죠",
          "original_form": "관심도가 높아지면 좀 더 개선이 되겠죠",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008462.1.28",
          "form": "계속 목소리를 내야겠죠",
          "original_form": "계속 목소리를 내야겠죠",
          "speaker_id": "2",
          "time": "20210822 16:18"
        }
      ]
