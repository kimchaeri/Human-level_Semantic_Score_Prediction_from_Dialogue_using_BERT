      "utterance": [
        {
          "id": "MDRW2100044018.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044018.1.2",
          "form": "요즘 사고픈거 있으세요?",
          "original_form": "요즘 사고픈거 있으세요?",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044018.1.3",
          "form": "영양제요 떨어져서 사야되요",
          "original_form": "영양제요 떨어져서 사야되요",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044018.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044018.1.5",
          "form": "저두요!!",
          "original_form": "저두요!!",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044018.1.6",
          "form": "유산균 사려구요",
          "original_form": "유산균 사려구요",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044018.1.7",
          "form": "전 종합바타민 c d 오메가3  사려고요",
          "original_form": "전 종합바타민 c d 오메가3  사려고요",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044018.1.8",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044018.1.9",
          "form": "저는 에어팟도 고민중이에요",
          "original_form": "저는 에어팟도 고민중이에요",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044018.1.10",
          "form": "오 요즘 무선이어폰 많이 쓰죠",
          "original_form": "오 요즘 무선이어폰 많이 쓰죠",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044018.1.11",
          "form": "저 아직도 없어서 ㅋㅋ고민이에요",
          "original_form": "저 아직도 없어서 ㅋㅋ고민이에요",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044018.1.12",
          "form": "하나 사시면 좋죠 전 청력 때문에 고려도 못해요 ㅠㅋㅋ",
          "original_form": "하나 사시면 좋죠 전 청력 때문에 고려도 못해요 ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044018.1.13",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044018.1.14",
          "form": "하긴 이어폰이 귀에 엄청 안좋죠 ㅠ",
          "original_form": "하긴 이어폰이 귀에 엄청 안좋죠 ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044018.1.15",
          "form": "저음쪽이 안 좋은 적이 있어서요",
          "original_form": "저음쪽이 안 좋은 적이 있어서요",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044018.1.16",
          "form": "시끄러운 소음 있을 때 안 들으면 되죠 자주 사용 안하고요",
          "original_form": "시끄러운 소음 있을 때 안 들으면 되죠 자주 사용 안하고요",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044018.1.17",
          "form": "아하~~근데 요즘은 밖이 시끄러우니 ㅠ",
          "original_form": "아하~~근데 요즘은 밖이 시끄러우니 ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044018.1.18",
          "form": "흐 덜 쓰긴해야겠어요",
          "original_form": "흐 덜 쓰긴해야겠어요",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044018.1.19",
          "form": "그래도 에어팟 힙해요 어르신들도 잘 사용하시더라구요",
          "original_form": "그래도 에어팟 힙해요 어르신들도 잘 사용하시더라구요",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044018.1.20",
          "form": "ㅎㅎ가격이 흠이에요",
          "original_form": "ㅎㅎ가격이 흠이에요",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044018.1.21",
          "form": "맞아요 전자기기는 가격이 ㅠㅠ",
          "original_form": "맞아요 전자기기는 가격이 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044018.1.22",
          "form": "ㅋㅋㅋ이어폰이라 중고도 좀 그렇고",
          "original_form": "ㅋㅋㅋ이어폰이라 중고도 좀 그렇고",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044018.1.23",
          "form": "그냥 적당한거 살까싶어요",
          "original_form": "그냥 적당한거 살까싶어요",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044018.1.24",
          "form": "뭐 생각중이세요?",
          "original_form": "뭐 생각중이세요?",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044018.1.25",
          "form": "아직 모르겠어요",
          "original_form": "아직 모르겠어요",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044018.1.26",
          "form": "하긴 고민되죠 그래도 좋은 것 쓰는 게 좋더라구요",
          "original_form": "하긴 고민되죠 그래도 좋은 것 쓰는 게 좋더라구요",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044018.1.27",
          "form": "그런가요~ㅎㅎ",
          "original_form": "그런가요~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044018.1.28",
          "form": "잃어버리는것도 걱정돼요",
          "original_form": "잃어버리는것도 걱정돼요",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044018.1.29",
          "form": "네 좋은 것 하나가 주는 만족감이 크더라구요",
          "original_form": "네 좋은 것 하나가 주는 만족감이 크더라구요",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044018.1.30",
          "form": "ㅎㅎ맞아요",
          "original_form": "ㅎㅎ맞아요",
