      "utterance": [
        {
          "id": "MDRW2100043000.1.1",
          "form": "다시들어가서",
          "original_form": "다시들어가서",
          "speaker_id": "2",
          "time": "20210907 12:48"
        },
        {
          "id": "MDRW2100043000.1.2",
          "form": "교촌 사서 다 먹곸ㅋㅋㅋㅋㅋ",
          "original_form": "교촌 사서 다 먹곸ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:48"
        },
        {
          "id": "MDRW2100043000.1.3",
          "form": "다시 와앙사서먹고",
          "original_form": "다시 와앙사서먹고",
          "speaker_id": "2",
          "time": "20210907 12:48"
        },
        {
          "id": "MDRW2100043000.1.4",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:49"
        },
        {
          "id": "MDRW2100043000.1.5",
          "form": "걍 웃겼어요..",
          "original_form": "걍 웃겼어요..",
          "speaker_id": "2",
          "time": "20210907 12:49"
        },
        {
          "id": "MDRW2100043000.1.6",
          "form": "마라는 참지않아",
          "original_form": "마라는 참지않아",
          "speaker_id": "1",
          "time": "20210907 12:49"
        },
        {
          "id": "MDRW2100043000.1.7",
          "form": "그냥 사먹어",
          "original_form": "그냥 사먹어",
          "speaker_id": "1",
          "time": "20210907 12:49"
        },
        {
          "id": "MDRW2100043000.1.8",
          "form": "알바생이 웃으면서 저쪽에서",
          "original_form": "알바생이 웃으면서 저쪽에서",
          "speaker_id": "2",
          "time": "20210907 12:49"
        },
        {
          "id": "MDRW2100043000.1.9",
          "form": "드셔도된다고해서ㅎㅎ네ㅎㅅ;;",
          "original_form": "드셔도된다고해서ㅎㅎ네ㅎㅅ;;",
          "speaker_id": "2",
          "time": "20210907 12:49"
        },
        {
          "id": "MDRW2100043000.1.10",
          "form": "하고앉아서 먹고갓어요",
          "original_form": "하고앉아서 먹고갓어요",
          "speaker_id": "2",
          "time": "20210907 12:49"
        },
        {
          "id": "MDRW2100043000.1.11",
          "form": "name1님의 고로케평가",
          "original_form": "&name1&님의 고로케평가",
          "speaker_id": "2",
          "time": "20210907 12:49"
        },
        {
          "id": "MDRW2100043000.1.12",
          "form": "어땟나요",
          "original_form": "어땟나요",
          "speaker_id": "2",
          "time": "20210907 12:49"
        },
        {
          "id": "MDRW2100043000.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:49"
        },
        {
          "id": "MDRW2100043000.1.14",
          "form": "와 치킨맛을 이렇게 살려낸다고?",
          "original_form": "와 치킨맛을 이렇게 살려낸다고?",
          "speaker_id": "1",
          "time": "20210907 12:49"
        },
        {
          "id": "MDRW2100043000.1.15",
          "form": "진짜 연구 많이했다",
          "original_form": "진짜 연구 많이했다",
          "speaker_id": "1",
          "time": "20210907 12:49"
        },
        {
          "id": "MDRW2100043000.1.16",
          "form": "이 붙어있는 소보로같은거 바삭함을 살려줄 때 용이하다",
          "original_form": "이 붙어있는 소보로같은거 바삭함을 살려줄 때 용이하다",
          "speaker_id": "1",
          "time": "20210907 12:50"
        },
        {
          "id": "MDRW2100043000.1.17",
          "form": "ㅇㅈㅇㅈ진심",
          "original_form": "ㅇㅈㅇㅈ진심",
          "speaker_id": "2",
          "time": "20210907 12:50"
        },
        {
          "id": "MDRW2100043000.1.18",
          "form": "교촌 치킨은 교촌 치킨이지만 뚜쥬 힘냈다",
          "original_form": "교촌 치킨은 교촌 치킨이지만 뚜쥬 힘냈다",
          "speaker_id": "1",
          "time": "20210907 12:50"
        },
        {
          "id": "MDRW2100043000.1.19",
          "form": "이거 바로 나왔을 때 먹어야한다",
          "original_form": "이거 바로 나왔을 때 먹어야한다",
          "speaker_id": "1",
          "time": "20210907 12:50"
        },
        {
          "id": "MDRW2100043000.1.20",
          "form": "등등",
          "original_form": "등등",
          "speaker_id": "1",
          "time": "20210907 12:50"
        },
        {
          "id": "MDRW2100043000.1.21",
          "form": "아니 기대이상이지않나요 진짜",
          "original_form": "아니 기대이상이지않나요 진짜",
          "speaker_id": "1",
          "time": "20210907 12:50"
        },
        {
          "id": "MDRW2100043000.1.22",
          "form": "뚜쥬의 이런콜라보 ㄹㅇ감탄했어요",
          "original_form": "뚜쥬의 이런콜라보 ㄹㅇ감탄했어요",
          "speaker_id": "2",
          "time": "20210907 12:50"
        },
        {
          "id": "MDRW2100043000.1.23",
          "form": "그거 보고 파바가 파바닭인가 만들었는데",
          "original_form": "그거 보고 파바가 파바닭인가 만들었는데",
          "speaker_id": "1",
          "time": "20210907 12:50"
        },
        {
          "id": "MDRW2100043000.1.24",
