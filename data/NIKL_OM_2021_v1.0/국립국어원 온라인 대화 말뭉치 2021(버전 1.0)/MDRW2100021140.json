        },
        {
          "id": "MDRW2100021140.1.3",
          "form": "전 런닝맨",
          "original_form": "전 런닝맨",
          "speaker_id": "1",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100021140.1.4",
          "form": "전 드라마요",
          "original_form": "전 드라마요",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100021140.1.5",
          "form": "결혼작사이혼작곡 1편을 재방으로 보고있어요",
          "original_form": "결혼작사이혼작곡 1편을 재방으로 보고있어요",
          "speaker_id": "2",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100021140.1.6",
          "form": "잼나나요?",
          "original_form": "잼나나요?",
          "speaker_id": "1",
          "time": "20210902 16:55"
        },
        {
          "id": "MDRW2100021140.1.7",
          "form": "막장이죠^^",
          "original_form": "막장이죠^^",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100021140.1.8",
          "form": "슬의생은 최애 드라마죠",
          "original_form": "슬의생은 최애 드라마죠",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100021140.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100021140.1.10",
          "form": "유쾌한 드라마죠?  보셨나요?",
          "original_form": "유쾌한 드라마죠?  보셨나요?",
          "speaker_id": "2",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100021140.1.11",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100021140.1.12",
          "form": "목요일 저녁을 너무 기다려요",
          "original_form": "목요일 저녁을 너무 기다려요",
          "speaker_id": "2",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100021140.1.13",
          "form": "담에 기회도면 보세요",
          "original_form": "담에 기회도면 보세요",
          "speaker_id": "2",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100021140.1.14",
          "form": "그래야겠네요",
          "original_form": "그래야겠네요",
          "speaker_id": "1",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100021140.1.15",
          "form": "음악도 좋아요 제 세대에 맞는 음악이라 더 좋아지는것같아요",
          "original_form": "음악도 좋아요 제 세대에 맞는 음악이라 더 좋아지는것같아요",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100021140.1.16",
          "form": "요즘은 TV로 유튜브나 넷플렉스도 볼수있어서 좋아요",
          "original_form": "요즘은 TV로 유튜브나 넷플렉스도 볼수있어서 좋아요",
