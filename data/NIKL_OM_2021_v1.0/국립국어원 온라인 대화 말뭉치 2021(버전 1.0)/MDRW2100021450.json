      "utterance": [
        {
          "id": "MDRW2100021450.1.1",
          "form": "근데 여자제품이 없고 남자 사이즈라",
          "original_form": "근데 여자제품이 없고 남자 사이즈라",
          "speaker_id": "2",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100021450.1.2",
          "form": "저에게 클까 걱정입니다",
          "original_form": "저에게 클까 걱정입니다",
          "speaker_id": "2",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100021450.1.3",
          "form": "그렇다고 여자 벨트를 사자니",
          "original_form": "그렇다고 여자 벨트를 사자니",
          "speaker_id": "2",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100021450.1.4",
          "form": "품질이 염려가 되더라구요",
          "original_form": "품질이 염려가 되더라구요",
          "speaker_id": "2",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100021450.1.5",
          "form": "아시죠? 여성용이 남성용보다",
          "original_form": "아시죠? 여성용이 남성용보다",
          "speaker_id": "2",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100021450.1.6",
          "form": "전 예전에 남자벨트사서",
          "original_form": "전 예전에 남자벨트사서",
          "speaker_id": "1",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100021450.1.7",
          "form": "너무커가지고",
          "original_form": "너무커가지고",
          "speaker_id": "1",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100021450.1.8",
          "form": "만듦새가 떨어지잖아요",
          "original_form": "만듦새가 떨어지잖아요",
          "speaker_id": "2",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100021450.1.9",
          "form": "중간에 벨트구멍을",
          "original_form": "중간에 벨트구멍을",
          "speaker_id": "1",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100021450.1.10",
          "form": "송곳으로 뚫은 기억이나네요",
          "original_form": "송곳으로 뚫은 기억이나네요",
          "speaker_id": "1",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100021450.1.11",
          "form": "으으으음 고민되네요",
          "original_form": "으으으음 고민되네요",
          "speaker_id": "2",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100021450.1.12",
          "form": "저는 체구가 작아서",
          "original_form": "저는 체구가 작아서",
          "speaker_id": "2",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100021450.1.13",
          "form": "벨트에 잡아먹힐까봐 고민이 돼요",
          "original_form": "벨트에 잡아먹힐까봐 고민이 돼요",
          "speaker_id": "2",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100021450.1.14",
          "form": "제 손으로 물건을",
          "original_form": "제 손으로 물건을",
          "speaker_id": "2",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100021450.1.15",
          "form": "뚝딱뚝딱 만들고 싶은 마음이 굴뚝같아요 ㅋㅋㅋㅋ",
          "original_form": "뚝딱뚝딱 만들고 싶은 마음이 굴뚝같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100021450.1.16",
          "form": "짜증 잔뜩나죠",
          "original_form": "짜증 잔뜩나죠",
          "speaker_id": "1",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100021450.1.17",
          "form": "갖고싶은 물건인데",
          "original_form": "갖고싶은 물건인데",
          "speaker_id": "1",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100021450.1.18",
          "form": "뭔가 마음에 걸리면",
          "original_form": "뭔가 마음에 걸리면",
          "speaker_id": "1",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100021450.1.19",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100021450.1.20",
          "form": "name1도 가디건 막",
          "original_form": "&name1&도 가디건 막",
          "speaker_id": "2",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100021450.1.21",
          "form": "직접 만들고싶지않나요",
          "original_form": "직접 만들고싶지않나요",
          "speaker_id": "2",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021450.1.22",
          "form": "솔직히 기술과 재료만 있다면 저는 그럴 것 같아요",
          "original_form": "솔직히 기술과 재료만 있다면 저는 그럴 것 같아요",
          "speaker_id": "2",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021450.1.23",
          "form": "제 취향은 제가 제일 잘 아는데",
          "original_form": "제 취향은 제가 제일 잘 아는데",
          "speaker_id": "2",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021450.1.24",
          "form": "저도 요즘 가디건을 사고싶은데",
          "original_form": "저도 요즘 가디건을 사고싶은데",
          "speaker_id": "1",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021450.1.25",
          "form": "마음에 드는 제품이 없어서",
          "original_form": "마음에 드는 제품이 없어서",
          "speaker_id": "1",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021450.1.26",
          "form": "차라리 내손으로 만들겠다",
          "original_form": "차라리 내손으로 만들겠다",
          "speaker_id": "1",
          "time": "20210828 19:13"
        },
        {
          "id": "MDRW2100021450.1.27",
          "form": "생각한적있어요",
          "original_form": "생각한적있어요",
          "speaker_id": "1",
          "time": "20210828 19:13"
        },
        {
          "id": "MDRW2100021450.1.28",
          "form": "베이지색 가디건",
          "original_form": "베이지색 가디건",
          "speaker_id": "1",
          "time": "20210828 19:13"
        },
        {
          "id": "MDRW2100021450.1.29",
          "form": "보풀안생기는걸로",
          "original_form": "보풀안생기는걸로",
          "speaker_id": "1",
          "time": "20210828 19:14"
        },
        {
          "id": "MDRW2100021450.1.30",
          "form": "와 그런 옷 저도 갖고싶어요",
          "original_form": "와 그런 옷 저도 갖고싶어요",
          "speaker_id": "2",
          "time": "20210828 19:14"
        },
        {
          "id": "MDRW2100021450.1.31",
          "form": "보풀 안 생기는 거",
          "original_form": "보풀 안 생기는 거",
          "speaker_id": "2",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100021450.1.32",
          "form": "너무 간절해요",
          "original_form": "너무 간절해요",
          "speaker_id": "2",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100021450.1.33",
          "form": "상품은 질이 제일 중요해요",
          "original_form": "상품은 질이 제일 중요해요",
          "speaker_id": "1",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100021450.1.34",
          "form": "맞아요 아무리 예뻐도 세탁했을 때",
          "original_form": "맞아요 아무리 예뻐도 세탁했을 때",
          "speaker_id": "2",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100021450.1.35",
          "form": "겉이 다 피면 소용이 없더라구요",
          "original_form": "겉이 다 피면 소용이 없더라구요",
          "speaker_id": "2",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100021450.1.36",
          "form": "근데 요즘 옷들은 질이 가격에 못 미치는 게",
          "original_form": "근데 요즘 옷들은 질이 가격에 못 미치는 게",
          "speaker_id": "2",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100021450.1.37",
          "form": "많아보여서 쇼핑하기 더 힘들어요",
          "original_form": "많아보여서 쇼핑하기 더 힘들어요",
          "speaker_id": "2",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021450.1.38",
          "form": "저는 가격싸고 질이 허접한 옷이 싫더라고요",
          "original_form": "저는 가격싸고 질이 허접한 옷이 싫더라고요",
          "speaker_id": "1",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021450.1.39",
          "form": "짱나고 보면 내돈을 날린것같아요",
          "original_form": "짱나고 보면 내돈을 날린것같아요",
          "speaker_id": "1",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021450.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 맞아요",
          "speaker_id": "2",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100021450.1.41",
          "form": "거적떼기 돈 주고 사는 게",
          "original_form": "거적떼기 돈 주고 사는 게",
          "speaker_id": "2",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100021450.1.42",
          "form": "아까워요",
          "original_form": "아까워요",
          "speaker_id": "2",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021450.1.43",
          "form": "저 근데 혹시",
          "original_form": "저 근데 혹시",
          "speaker_id": "2",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021450.1.44",
          "form": "저는 중고도 엄청 관심많아요",
          "original_form": "저는 중고도 엄청 관심많아요",
          "speaker_id": "1",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021450.1.45",
          "form": "말 놔도 될까요",
          "original_form": "말 놔도 될까요",
          "speaker_id": "2",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021450.1.46",
          "form": "네 좋아요",
          "original_form": "네 좋아요",
          "speaker_id": "1",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021450.1.47",
          "form": "중고로 요즘 신발보고있는데",
          "original_form": "중고로 요즘 신발보고있는데",
          "speaker_id": "1",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021450.1.48",
          "form": "환경과도 밀접하게",
          "original_form": "환경과도 밀접하게",
          "speaker_id": "1",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021450.1.49",
          "form": "관련이 있으니까",
          "original_form": "관련이 있으니까",
          "speaker_id": "1",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021450.1.50",
          "form": "중고도 말끔하면",
          "original_form": "중고도 말끔하면",
          "speaker_id": "2",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100021450.1.51",
          "form": "나름 괜찮더라",
          "original_form": "나름 괜찮더라",
          "speaker_id": "2",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100021450.1.52",
          "form": "가끔 상태 사기치는 사람들이 있긴 한데",
          "original_form": "가끔 상태 사기치는 사람들이 있긴 한데",
          "speaker_id": "2",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100021450.1.53",
          "form": "잘만 피하면 좋은 값에 원하던 물건을",
          "original_form": "잘만 피하면 좋은 값에 원하던 물건을",
          "speaker_id": "2",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100021450.1.54",
          "form": "맞아 사기치는 놈만 없으면 할만하지",
          "original_form": "맞아 사기치는 놈만 없으면 할만하지",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021450.1.55",
          "form": "얻을 수 있으니까",
          "original_form": "얻을 수 있으니까",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021450.1.56",
          "form": "윈윈이라 좋더라구",
          "original_form": "윈윈이라 좋더라구",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021450.1.57",
          "form": "내가 팔고싶은것도 꽤 팔리고",
          "original_form": "내가 팔고싶은것도 꽤 팔리고",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021450.1.58",
          "form": "나 이번에 호카오네오네라는 브랜드 신발을",
          "original_form": "나 이번에 호카오네오네라는 브랜드 신발을",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021450.1.59",
          "form": "해외배송으로 샀거든?",
          "original_form": "해외배송으로 샀거든?",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021450.1.60",
          "form": "근데 진짜 오래기다려서 받았는데\r\n너무 불편한거야",
          "original_form": "근데 진짜 오래기다려서 받았는데\r\n너무 불편한거야",
