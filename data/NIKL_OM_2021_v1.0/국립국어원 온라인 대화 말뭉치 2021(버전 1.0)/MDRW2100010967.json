        {
          "id": "MDRW2100010967.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100010967.1.2",
          "form": "저는 안키워요~",
          "original_form": "저는 안키워요~",
          "speaker_id": "2",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010967.1.3",
          "form": "저도 여건이",
          "original_form": "저도 여건이",
          "speaker_id": "1",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010967.1.4",
          "form": "뭐 키우고 싶은 동물 있으세요?",
          "original_form": "뭐 키우고 싶은 동물 있으세요?",
          "speaker_id": "2",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100010967.1.5",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100010967.1.6",
          "form": "아하 강아지 좋아하시나봐요",
          "original_form": "아하 강아지 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100010967.1.7",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010967.1.8",
          "form": "저도 집만 넓으면 키우고싶더라구요 전 대형견이 귀여운것 같아요",
          "original_form": "저도 집만 넓으면 키우고싶더라구요 전 대형견이 귀여운것 같아요",
          "speaker_id": "2",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100010967.1.9",
          "form": "뛰어놀고싶다",
          "original_form": "뛰어놀고싶다",
          "speaker_id": "1",
          "time": "20210823 23:27"
        },
        {
          "id": "MDRW2100010967.1.10",
          "form": "아 어디서요?",
          "original_form": "아 어디서요?",
          "speaker_id": "2",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100010967.1.11",
          "form": "마당있는집에서",
          "original_form": "마당있는집에서",
          "speaker_id": "1",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100010967.1.12",
          "form": "아~넓어서 동물 키우기 좋겠네요",
          "original_form": "아~넓어서 동물 키우기 좋겠네요",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010967.1.13",
          "form": "집리 없음",
          "original_form": "집리 없음",
          "speaker_id": "1",
