        },
        {
          "id": "MDRW2100045010.1.2",
          "form": "외계인방앗간?",
          "original_form": "외계인방앗간?",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045010.1.3",
          "form": "밀가루대신 쌀가루를 넣었다고하더라구요!!",
          "original_form": "밀가루대신 쌀가루를 넣었다고하더라구요!!",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045010.1.4",
          "form": "엇 아니요 ㅋㅋㅋ저장하겠습니다",
          "original_form": "엇 아니요 ㅋㅋㅋ저장하겠습니다",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045010.1.5",
          "form": "제가 가본곳은 \"소소하게\" 에요!!!",
          "original_form": "제가 가본곳은 \"소소하게\" 에요!!!",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045010.1.6",
          "form": "소소하게 알아요!!!!!!",
          "original_form": "소소하게 알아요!!!!!!",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045010.1.7",
          "form": "거긴 진짜 디저트맛집인데",
          "original_form": "거긴 진짜 디저트맛집인데",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045010.1.8",
          "form": "가고싶네요",
          "original_form": "가고싶네요",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045010.1.9",
          "form": "ㅋㅋㅋ오오오!!!!!!!! 뭐뭐 드셔보셨어요?",
          "original_form": "ㅋㅋㅋ오오오!!!!!!!! 뭐뭐 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045010.1.10",
          "form": "아녀 리스트에만 있어요",
          "original_form": "아녀 리스트에만 있어요",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045010.1.11",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045010.1.12",
          "form": "으헝 ㅠㅠㅠㅠ 이번에 북촌에도 생겼더라구요!!",
          "original_form": "으헝 ㅠㅠㅠㅠ 이번에 북촌에도 생겼더라구요!!",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045010.1.13",
          "form": "북촌이라니!!!",
          "original_form": "북촌이라니!!!",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045010.1.14",
          "form": "원래 그 성수동에만 있었는데 주말에",
          "original_form": "원래 그 성수동에만 있었는데 주말에",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045010.1.15",
          "form": "거긴 갈 수있을거 같은데..!!",
          "original_form": "거긴 갈 수있을거 같은데..!!",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045010.1.16",
          "form": "북촌갔더니 있더라구요!!!",
          "original_form": "북촌갔더니 있더라구요!!!",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045010.1.17",
          "form": "1000명 아래로 내려가면 아침에라도 가보겠어요ㅋㅋㅋㅋ",
          "original_form": "1000명 아래로 내려가면 아침에라도 가보겠어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045010.1.18",
          "form": "태양커피라고 아시나요?",
          "original_form": "태양커피라고 아시나요?",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045010.1.19",
          "form": "흐음 아니요 거기도 맛집인가요?!",
          "original_form": "흐음 아니요 거기도 맛집인가요?!",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045010.1.20",
          "form": "네네 커피만 파는 작은 카페인데",
          "original_form": "네네 커피만 파는 작은 카페인데",
          "speaker_id": "2",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045010.1.21",
          "form": "아인슈페너가 유며해요",
          "original_form": "아인슈페너가 유며해요",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045010.1.22",
          "form": "우와우와 ㅋㅋㅋ 다음에 먹어볼래요 여기도 저장!!!",
