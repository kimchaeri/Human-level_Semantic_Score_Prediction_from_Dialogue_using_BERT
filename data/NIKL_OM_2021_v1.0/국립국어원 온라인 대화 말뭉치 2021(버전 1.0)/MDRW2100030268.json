      "utterance": [
        {
          "id": "MDRW2100030268.1.1",
          "form": "요즘 날씨 어떤가오?",
          "original_form": "요즘 날씨 어떤가오?",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030268.1.2",
          "form": "요즘은 날씨가 왔다갔다해요",
          "original_form": "요즘은 날씨가 왔다갔다해요",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030268.1.3",
          "form": "오늘은 완전가을날씨네요",
          "original_form": "오늘은 완전가을날씨네요",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030268.1.4",
          "form": "그쳐 바람이 많이부는ㅋㅋ",
          "original_form": "그쳐 바람이 많이부는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030268.1.5",
          "form": "name2님 집은 그 앞이 산이라 바람이 더 불어 같아오",
          "original_form": "&name2&님 집은 그 앞이 산이라 바람이 더 불어 같아오",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030268.1.6",
          "form": "그래서 바람은 많이 부는데",
          "original_form": "그래서 바람은 많이 부는데",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030268.1.7",
          "form": "산이 옆이라 습한 바람이에요",
          "original_form": "산이 옆이라 습한 바람이에요",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030268.1.8",
          "form": "님 집근처도 산이 가깝지 않으신가요??",
          "original_form": "님 집근처도 산이 가깝지 않으신가요??",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030268.1.9",
          "form": "저는 그 경륜장이오ㅋㅋㅋ나름 탁트여서 산이 좀 멀리 있어오",
          "original_form": "저는 그 경륜장이오ㅋㅋㅋ나름 탁트여서 산이 좀 멀리 있어오",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030268.1.10",
          "form": "바람이 많이 불어서 선풍기 틀면 요즘 좀 춥기도한..",
          "original_form": "바람이 많이 불어서 선풍기 틀면 요즘 좀 춥기도한..",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030268.1.11",
          "form": "탁트인곳이 좋죠",
          "original_form": "탁트인곳이 좋죠",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030268.1.12",
          "form": "바람이 많이 들어오면 시원하겠다~~~",
          "original_form": "바람이 많이 들어오면 시원하겠다~~~",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030268.1.13",
          "form": "탁트이면 계절이 바뀌면 다보이겠넹8",
          "original_form": "탁트이면 계절이 바뀌면 다보이겠넹{emoji:‥}8",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030268.1.14",
          "form": "님 동네도 언눙 습한 바람가고 시원한 바람 오기를!!!!!",
          "original_form": "님 동네도 언눙 습한 바람가고 시원한 바람 오기를!!!!!",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030268.1.15",
          "form": "지금 앞에 재개발 중이라 탁트임",
          "original_form": "지금 앞에 재개발 중이라 탁트임",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030268.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030268.1.17",
          "form": "근디 아파트 들어서도 괜춘할거 같기도 하고",
          "original_form": "근디 아파트 들어서도 괜춘할거 같기도 하고",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030268.1.18",
          "form": "요즘 아파트 공간이 널찍해서 괜찮을꺼예요",
          "original_form": "요즘 아파트 공간이 널찍해서 괜찮을꺼예요",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030268.1.19",
          "form": "그럼 다행임니댜",
          "original_form": "그럼 다행임니댜",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030268.1.20",
          "form": "가디건 챙기고 다니나오",
          "original_form": "가디건 챙기고 다니나오",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030268.1.21",
          "form": "날씨가 저녁에는 선선해서",
          "original_form": "날씨가 저녁에는 선선해서",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030268.1.22",
          "form": "곧 가을 올거 같아오",
          "original_form": "곧 가을 올거 같아오",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030268.1.23",
          "form": "지금이 가을인것같습니다",
          "original_form": "지금이 가을인것같습니다",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030268.1.24",
          "form": "낮에는 덥기도하곱",
          "original_form": "낮에는 덥기도하곱",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030268.1.25",
          "form": "밤에는 시원하고",
          "original_form": "밤에는 시원하고",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030268.1.26",
          "form": "가을입니다",
          "original_form": "가을입니다",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030268.1.27",
          "form": "넹 낮에는 더웡",
          "original_form": "넹 낮에는 더웡",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030268.1.28",
          "form": "오늘 낮에 걷다가 땀좀 흘림ㅠㅠㅠ",
          "original_form": "오늘 낮에 걷다가 땀좀 흘림ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030268.1.29",
          "form": "가을은 고기라 고기 우리 꾸워먹어야 하는디..",
          "original_form": "가을은 고기라 고기 우리 꾸워먹어야 하는디..",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030268.1.30",
          "form": "가을에 캠핑가서 고기 먹고싶어요",
          "original_form": "가을에 캠핑가서 고기 먹고싶어요",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030268.1.31",
          "form": "날씨좋은날 놀러가요",
          "original_form": "날씨좋은날 놀러가요",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030268.1.32",
          "form": "그쳐 캠핑가서 고기 묵으면 좋을거 같슴니댜",
          "original_form": "그쳐 캠핑가서 고기 묵으면 좋을거 같슴니댜",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030268.1.33",
          "form": "가을에는 캠핑이져",
          "original_form": "가을에는 캠핑이져",
          "speaker_id": "1",
