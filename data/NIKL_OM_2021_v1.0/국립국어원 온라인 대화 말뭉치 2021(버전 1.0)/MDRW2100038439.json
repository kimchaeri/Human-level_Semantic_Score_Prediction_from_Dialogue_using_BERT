      "utterance": [
        {
          "id": "MDRW2100038439.1.1",
          "form": "나 그거 보고 놀라ㅛ잖아",
          "original_form": "나 그거 보고 놀라ㅛ잖아",
          "speaker_id": "2",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100038439.1.2",
          "form": "뭐?",
          "original_form": "뭐?",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100038439.1.3",
          "form": "홍진경 동생이 부모도 안그러는데",
          "original_form": "홍진경 동생이 부모도 안그러는데",
          "speaker_id": "2",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100038439.1.4",
          "form": "ㅇㅇ엉ㅇ",
          "original_form": "ㅇㅇ엉ㅇ",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100038439.1.5",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100038439.1.6",
          "form": "지혼자만 계속 누나 어딨는지 뭐하는지 확인하고 보고 받으려고 했대",
          "original_form": "지혼자만 계속 누나 어딨는지 뭐하는지 확인하고 보고 받으려고 했대",
          "speaker_id": "2",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100038439.1.7",
          "form": "지가 뭔데 그 난리임 진짜",
          "original_form": "지가 뭔데 그 난리임 진짜",
          "speaker_id": "2",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100038439.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100038439.1.9",
          "form": "소름돋아",
          "original_form": "소름돋아",
          "speaker_id": "1",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100038439.1.10",
          "form": "너무싫어",
          "original_form": "너무싫어",
          "speaker_id": "1",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100038439.1.11",
          "form": "어디 동생이 누나한테 난리임",
          "original_form": "어디 동생이 누나한테 난리임",
          "speaker_id": "2",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100038439.1.12",
          "form": "자기보다낮게",
          "original_form": "자기보다낮게",
          "speaker_id": "1",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100038439.1.13",
          "form": "보려고",
          "original_form": "보려고",
          "speaker_id": "1",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100038439.1.14",
