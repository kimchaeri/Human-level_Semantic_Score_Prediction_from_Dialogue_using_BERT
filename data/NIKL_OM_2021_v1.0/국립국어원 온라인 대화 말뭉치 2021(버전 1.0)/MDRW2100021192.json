      "utterance": [
        {
          "id": "MDRW2100021192.1.1",
          "form": "탐탐 고구마프레즐도 괜찮아요",
          "original_form": "탐탐 고구마프레즐도 괜찮아요",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021192.1.2",
          "form": "아 저 고구마케이크도 좋아해요",
          "original_form": "아 저 고구마케이크도 좋아해요",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021192.1.3",
          "form": "고구마맛 너무 좋네요",
          "original_form": "고구마맛 너무 좋네요",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021192.1.4",
          "form": "음 그리고 피자도 판답니다",
          "original_form": "음 그리고 피자도 판답니다",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021192.1.5",
          "form": "제대로 된 피자를요",
          "original_form": "제대로 된 피자를요",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021192.1.6",
          "form": "탐탐 카페서 별거를 다 파네요",
          "original_form": "탐탐 카페서 별거를 다 파네요",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100021192.1.7",
          "form": "커피 아닌 피자 맛집에 가깝죠",
          "original_form": "커피 아닌 피자 맛집에 가깝죠",
          "speaker_id": "2",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100021192.1.8",
          "form": "저는 거기 끼니 떼우러 가요",
          "original_form": "저는 거기 끼니 떼우러 가요",
          "speaker_id": "2",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100021192.1.9",
          "form": "추천할 음료는 아메가 전부인가요",
          "original_form": "추천할 음료는 아메가 전부인가요",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100021192.1.10",
          "form": "아 거기 레몬에이드 맛있어요",
          "original_form": "아 거기 레몬에이드 맛있어요",
          "speaker_id": "2",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100021192.1.11",
          "form": "시큼과 새콤의 경계를 잘 잡았어요",
          "original_form": "시큼과 새콤의 경계를 잘 잡았어요",
          "speaker_id": "2",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100021192.1.12",
          "form": "여름에 더울때 먹음 딱이겠어요",
          "original_form": "여름에 더울때 먹음 딱이겠어요",
          "speaker_id": "1",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100021192.1.13",
          "form": "그쵸. 더우면 생각나요",
          "original_form": "그쵸. 더우면 생각나요",
          "speaker_id": "2",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100021192.1.14",
          "form": "근데 빵과 먹기엔 좀 별로라",
          "original_form": "근데 빵과 먹기엔 좀 별로라",
          "speaker_id": "2",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100021192.1.15",
          "form": "하긴 단품으로 마시는게 더 빛나는 음료죠",
          "original_form": "하긴 단품으로 마시는게 더 빛나는 음료죠",
          "speaker_id": "1",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100021192.1.16",
          "form": "아무튼 레몬에이드 추천!",
          "original_form": "아무튼 레몬에이드 추천!",
          "speaker_id": "2",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100021192.1.17",
          "form": "자몽에이드도 괜찮아요",
          "original_form": "자몽에이드도 괜찮아요",
          "speaker_id": "2",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100021192.1.18",
          "form": "명심할게요. 아직 날 더우니 사마셔봐야겠어요",
          "original_form": "명심할게요. 아직 날 더우니 사마셔봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100021192.1.19",
          "form": "ㅎㅎ 요샌 가을도 여름같아서요",
          "original_form": "ㅎㅎ 요샌 가을도 여름같아서요",
          "speaker_id": "2",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100021192.1.20",
          "form": "전 요새도 종종 사마셔요",
          "original_form": "전 요새도 종종 사마셔요",
          "speaker_id": "2",
          "time": "20210828 16:47"
        }
      ]
