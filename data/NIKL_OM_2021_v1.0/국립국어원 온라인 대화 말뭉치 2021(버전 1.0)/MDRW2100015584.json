      "utterance": [
        {
          "id": "MDRW2100015584.1.1",
          "form": "주제를 바꿔봐ㅛ어요ㅎㅎㅎ",
          "original_form": "주제를 바꿔봐ㅛ어요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015584.1.2",
          "form": "스포츠 뭐좋아하세요?",
          "original_form": "스포츠 뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015584.1.3",
          "form": "야구 좋아해요 ㅎㅎ",
          "original_form": "야구 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015584.1.4",
          "form": "ㅎㅎ 이번에 올림픽야구 엄청 실망했어요ㅍ",
          "original_form": "ㅎㅎ 이번에 올림픽야구 엄청 실망했어요ㅍ",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015584.1.5",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015584.1.6",
          "form": "드럽게 못해서...",
          "original_form": "드럽게 못해서...",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015584.1.7",
          "form": "맞아요ㅎㅎㅎ",
          "original_form": "맞아요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015584.1.8",
          "form": "더럽게 못함ㅎㅎ 이정후만 잘하고",
          "original_form": "더럽게 못함ㅎㅎ 이정후만 잘하고",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015584.1.9",
          "form": "메달을 못딴게 진짜....",
          "original_form": "메달을 못딴게 진짜....",
          "speaker_id": "2",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015584.1.10",
          "form": "배가 불렀죠",
          "original_form": "배가 불렀죠",
          "speaker_id": "2",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015584.1.11",
          "form": "맞아요ㅎ 야구망망이로 좀 맞아야해요",
          "original_form": "맞아요ㅎ 야구망망이로 좀 맞아야해요",
          "speaker_id": "1",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015584.1.12",
          "form": "이건뭐... 창피해서",
          "original_form": "이건뭐... 창피해서",
          "speaker_id": "1",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015584.1.13",
          "form": "ㅋㅋㅋㅋ 매가 약이긴한데 ㅋ",
          "original_form": "ㅋㅋㅋㅋ 매가 약이긴한데 ㅋ",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015584.1.14",
          "form": "나중에 정신차리겟죠ㄹㅎ",
          "original_form": "나중에 정신차리겟죠ㄹㅎ",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015584.1.15",
          "form": "전에 유튜브보는데",
          "original_form": "전에 유튜브보는데",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015584.1.16",
          "form": "동메달은 따겠지 했더니만",
          "original_form": "동메달은 따겠지 했더니만",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015584.1.17",
          "form": "야구선수들 원정경기가면",
          "original_form": "야구선수들 원정경기가면",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015584.1.18",
          "form": "장난아니래요",
          "original_form": "장난아니래요",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015584.1.19",
          "form": "정신 차려야할꺼에요ㅎ",
          "original_form": "정신 차려야할꺼에요ㅎ",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015584.1.20",
          "form": "그때 터진거요?",
          "original_form": "그때 터진거요?",
          "speaker_id": "2",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015584.1.21",
          "form": "몰랐는데 야구가 유럽은 안한다면서요?",
          "original_form": "몰랐는데 야구가 유럽은 안한다면서요?",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015584.1.22",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015584.1.23",
          "form": "네 안한대요",
          "original_form": "네 안한대요",
          "speaker_id": "2",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015584.1.24",
          "form": "아예 안한다고;;",
          "original_form": "아예 안한다고;;",
          "speaker_id": "2",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015584.1.25",
          "form": "그좋은것을 왜안하는지",
          "original_form": "그좋은것을 왜안하는지",
          "speaker_id": "1",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015584.1.26",
          "form": "근데 이해는가요 축구가 워낙에 인기가있으니ㅎ 거기선 야구가 비인기종목이겟죠ㅎ",
          "original_form": "근데 이해는가요 축구가 워낙에 인기가있으니ㅎ 거기선 야구가 비인기종목이겟죠ㅎ",
          "speaker_id": "1",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015584.1.27",
          "form": "소프트볼? 그런건 하는거 같던데",
          "original_form": "소프트볼? 그런건 하는거 같던데",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015584.1.28",
          "form": "알았여ㅎㅎㅎ",
          "original_form": "알았여ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015584.1.29",
          "form": "유럽은 그럴듯ㅎㅎ",
          "original_form": "유럽은 그럴듯ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015584.1.30",
          "form": "축구는좋아하세요?",
          "original_form": "축구는좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015584.1.31",
          "form": "축구는 그냥저냥",
          "original_form": "축구는 그냥저냥",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100015584.1.32",
          "form": "오오ㅎㅎ 그러시구나ㅎ",
          "original_form": "오오ㅎㅎ 그러시구나ㅎ",
          "speaker_id": "1",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100015584.1.33",
          "form": "그럼 농구?",
          "original_form": "그럼 농구?",
          "speaker_id": "1",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100015584.1.34",
          "form": "전 야구 배구 좋아해요 ㅋㅋㅋ",
          "original_form": "전 야구 배구 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100015584.1.35",
          "form": "오오ㅎ 좋죠ㅎㅎ",
          "original_form": "오오ㅎ 좋죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100015584.1.36",
          "form": "직관은하셨어요?",
          "original_form": "직관은하셨어요?",
          "speaker_id": "1",
          "time": "20210827 22:23"
        },
        {
          "id": "MDRW2100015584.1.37",
          "form": "네 둘다 직관했어요 ㅋㅋ",
          "original_form": "네 둘다 직관했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100015584.1.38",
          "form": "오오ㅎㅎ 관중들많을땨요?",
          "original_form": "오오ㅎㅎ 관중들많을땨요?",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100015584.1.39",
          "form": "뭐가 더 좋나요? 직관하기ㅡ?",
          "original_form": "뭐가 더 좋나요? 직관하기ㅡ?",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100015584.1.40",
          "form": "전 야구가 좋더라구요",
          "original_form": "전 야구가 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100015584.1.41",
          "form": "먹으면서 보기 좋아서",
          "original_form": "먹으면서 보기 좋아서",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100015584.1.42",
          "form": "치킨에 맥주죠ㅎㅎ",
          "original_form": "치킨에 맥주죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100015584.1.43",
          "form": "야구ㅎㅎ",
          "original_form": "야구ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:38"
        }
      ]
    }
