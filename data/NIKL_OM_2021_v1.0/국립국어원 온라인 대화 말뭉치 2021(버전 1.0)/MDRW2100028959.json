      "utterance": [
        {
          "id": "MDRW2100028959.1.1",
          "form": "정신없네요",
          "original_form": "정신없네요",
          "speaker_id": "2",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100028959.1.2",
          "form": "어떤게요 ㄹㄹ",
          "original_form": "어떤게요 ㄹㄹ",
          "speaker_id": "1",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028959.1.3",
          "form": "대화하고 맥주먹기ㅋ",
          "original_form": "대화하고 맥주먹기ㅋ",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028959.1.4",
          "form": "ㅎㅎㅎ 집주잉 좀 안될거 같어요",
          "original_form": "ㅎㅎㅎ 집주잉 좀 안될거 같어요",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028959.1.5",
          "form": "이것도 울리고\r\n오늘 바톡 진입함",
          "original_form": "이것도 울리고\r\n오늘 바톡 진입함",
          "speaker_id": "2",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028959.1.6",
          "form": "바톧은 무에요",
          "original_form": "바톧은 무에요",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100028959.1.7",
          "form": "바나나",
          "original_form": "바나나",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100028959.1.8",
          "form": "주식이요 ㅅㅅ",
          "original_form": "주식이요 ㅅㅅ",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100028959.1.9",
          "form": "오늘야식은 고기네요",
          "original_form": "오늘야식은 고기네요",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100028959.1.10",
          "form": "이미 왔지여 ㅋ",
          "original_form": "이미 왔지여 ㅋ",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100028959.1.11",
          "form": "오 고기 좋죠",
          "original_form": "오 고기 좋죠",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100028959.1.12",
          "form": "뒷고기",
          "original_form": "뒷고기",
          "speaker_id": "2",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100028959.1.13",
          "form": "뒷고기라 하심 울산이신가",
          "original_form": "뒷고기라 하심 울산이신가",
          "speaker_id": "1",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100028959.1.14",
          "form": "아 배송 택배ㅋ",
          "original_form": "아 배송 택배ㅋ",
          "speaker_id": "2",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100028959.1.15",
          "form": "거저요",
          "original_form": "거저요",
          "speaker_id": "2",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100028959.1.16",
          "form": "거제입니다",
          "original_form": "거제입니다",
          "speaker_id": "2",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100028959.1.17",
          "form": "아항  ㅎㅎㅎ 제가 예전에 울산2년정도 살앗어서 ㅋㅋ",
          "original_form": "아항  ㅎㅎㅎ 제가 예전에 울산2년정도 살앗어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100028959.1.18",
          "form": "거제는 여행으로 가봤네요 ㅋ",
          "original_form": "거제는 여행으로 가봤네요 ㅋ",
