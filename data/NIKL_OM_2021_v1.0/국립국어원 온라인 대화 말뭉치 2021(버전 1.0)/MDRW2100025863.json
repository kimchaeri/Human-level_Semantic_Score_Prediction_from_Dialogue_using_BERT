      "utterance": [
        {
          "id": "MDRW2100025863.1.1",
          "form": "여행 좋아하는 편이세요?",
          "original_form": "여행 좋아하는 편이세요?",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100025863.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025863.1.3",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025863.1.4",
          "form": "네 그런데 요즘은 자주 못가요..",
          "original_form": "네 그런데 요즘은 자주 못가요..",
          "speaker_id": "1",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025863.1.5",
          "form": "최근엔 어디 다녀오셨나요?",
          "original_form": "최근엔 어디 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025863.1.6",
          "form": "최근이라고해봐야 3년전이지만요 전주 갔었어요",
          "original_form": "최근이라고해봐야 3년전이지만요 전주 갔었어요",
          "speaker_id": "2",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025863.1.7",
          "form": "아 전주 ㅎㅎ 맛있는거 많이 드셨나여?",
          "original_form": "아 전주 ㅎㅎ 맛있는거 많이 드셨나여?",
          "speaker_id": "1",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100025863.1.8",
          "form": "저는 전주 다녀온지 5년은 넘은 거 같아요",
          "original_form": "저는 전주 다녀온지 5년은 넘은 거 같아요",
          "speaker_id": "1",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100025863.1.9",
          "form": "전주는 역시 맛집이죠",
          "original_form": "전주는 역시 맛집이죠",
          "speaker_id": "2",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100025863.1.10",
          "form": "국밥도 먹고 해산물도 먹고 이것저것 먹었어요",
          "original_form": "국밥도 먹고 해산물도 먹고 이것저것 먹었어요",
          "speaker_id": "2",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100025863.1.11",
          "form": "12월에 다시 갈 것 같은데 벌써 맛집 기대되네요 ㅎㅎ",
          "original_form": "12월에 다시 갈 것 같은데 벌써 맛집 기대되네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025863.1.12",
          "form": "그죠! 12월에 가시는구나",
          "original_form": "그죠! 12월에 가시는구나",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100025863.1.13",
          "form": "피순대국밥 꼭 먹어주세요",
          "original_form": "피순대국밥 꼭 먹어주세요",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100025863.1.14",
          "form": "넵 리스트업할게요 ㅋㅋ",
          "original_form": "넵 리스트업할게요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100025863.1.15",
          "form": "콩나물국밥도요...ㅠㅠ 시장쪽에서",
          "original_form": "콩나물국밥도요...ㅠㅠ 시장쪽에서",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100025863.1.16",
          "form": "너무맛있게먹어서 너무 그리워요",
          "original_form": "너무맛있게먹어서 너무 그리워요",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100025863.1.17",
          "form": "12월에는 코로나 잠잠해지면 좋겠네요",
          "original_form": "12월에는 코로나 잠잠해지면 좋겠네요",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100025863.1.18",
          "form": "그쵸 제발 조금이라도 줄길...",
          "original_form": "그쵸 제발 조금이라도 줄길...",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100025863.1.19",
          "form": "국내여행도 이렇게 힘든 날이 올 줄 몰랏어요ㅜ",
          "original_form": "국내여행도 이렇게 힘든 날이 올 줄 몰랏어요ㅜ",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100025863.1.20",
          "form": "그러니까요ㅠㅠ 서울로 와서 궁궐구경도 다니고싶은데",
          "original_form": "그러니까요ㅠㅠ 서울로 와서 궁궐구경도 다니고싶은데",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100025863.1.21",
          "form": "참 상황이 애매하네요",
          "original_form": "참 상황이 애매하네요",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100025863.1.22",
          "form": "그래서 차타고 멀리 가서 숲보고 바다보고 그런거만 해요...",
          "original_form": "그래서 차타고 멀리 가서 숲보고 바다보고 그런거만 해요...",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100025863.1.23",
          "form": "차있으면 그런게 좋네요",
          "original_form": "차있으면 그런게 좋네요",
          "speaker_id": "2",
          "time": "20210901 23:17"
        }
      ]
    }
  ]
