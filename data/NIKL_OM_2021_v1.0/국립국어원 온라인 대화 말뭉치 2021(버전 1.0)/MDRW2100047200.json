        },
        {
          "id": "MDRW2100047200.1.3",
          "form": "ㅇㄴ",
          "original_form": "ㅇㄴ",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100047200.1.4",
          "form": "방귀맛 소주 vs 똥맛 맥주",
          "original_form": "방귀맛 소주 vs 똥맛 맥주",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100047200.1.5",
          "form": "이런게 아니라요?",
          "original_form": "이런게 아니라요?",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100047200.1.6",
          "form": "하 고민되네 저거",
          "original_form": "하 고민되네 저거",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100047200.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100047200.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100047200.1.9",
          "form": "방귀맛....소주?",
          "original_form": "방귀맛....소주?",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100047200.1.10",
          "form": "입냄새 대신 입에서 방귀냄새 #가보자고",
          "original_form": "입냄새 대신 입에서 방귀냄새 #가보자고",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100047200.1.11",
          "form": "그쵸 똥맛보다는",
          "original_form": "그쵸 똥맛보다는",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100047200.1.12",
          "form": "내가 잘못했을 때",
          "original_form": "내가 잘못했을 때",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100047200.1.13",
          "form": "님 \r\n힙합에 미쳐서 합장하는 남친 VS 감성에 미쳐서 나만보면 시짓는 남친",
          "original_form": "님 \r\n힙합에 미쳐서 합장하는 남친 VS 감성에 미쳐서 나만보면 시짓는 남친",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100047200.1.14",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100047200.1.15",
          "form": "누가 더 악인가요",
          "original_form": "누가 더 악인가요",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100047200.1.16",
          "form": "뭘 잘못했는데? 꼬치꼬치 묻는 남친이지만 이틀만에 풀림 vs 혼자만의 시간이 필요하다해서 줬는데 일주일 후까지 삐져있는 남친",
          "original_form": "뭘 잘못했는데? 꼬치꼬치 묻는 남친이지만 이틀만에 풀림 vs 혼자만의 시간이 필요하다해서 줬는데 일주일 후까지 삐져있는 남친",
          "speaker_id": "1",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100047200.1.17",
          "form": "아 둘다싫네",
          "original_form": "아 둘다싫네",
          "speaker_id": "1",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100047200.1.18",
          "form": "시요",
          "original_form": "시요",
          "speaker_id": "1",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100047200.1.19",
          "form": "글은 숨길수라도 있어요",
          "original_form": "글은 숨길수라도 있어요",
          "speaker_id": "1",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100047200.1.20",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100047200.1.21",
          "form": "근데 페북에 올린다? 이거 심각해진다",
          "original_form": "근데 페북에 올린다? 이거 심각해진다",
          "speaker_id": "1",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100047200.1.22",
          "form": "갑자기 화나네",
          "original_form": "갑자기 화나네",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100047200.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100047200.1.24",
          "form": "님 시를 왜 짓는 줄 아세요?",
          "original_form": "님 시를 왜 짓는 줄 아세요?",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100047200.1.25",
          "form": "ㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100047200.1.26",
          "form": "인스타에 해시태그 걸려고요",
          "original_form": "인스타에 해시태그 걸려고요",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100047200.1.27",
          "form": "아나",
          "original_form": "아나",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100047200.1.28",
          "form": "난 그럼 비밀연애할래요",
          "original_form": "난 그럼 비밀연애할래요",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100047200.1.29",
          "form": "#여친위한선물 #여친눈물 #시인",
          "original_form": "#여친위한선물 #여친눈물 #시인",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100047200.1.30",
          "form": "아이돌 만나듯이요",
          "original_form": "아이돌 만나듯이요",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100047200.1.31",
          "form": "이런거 오백개 달아서 님 태그할건데요",
          "original_form": "이런거 오백개 달아서 님 태그할건데요",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100047200.1.32",
          "form": "계폭가능한가요",
          "original_form": "계폭가능한가요",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100047200.1.33",
          "form": "난 조용하게 연애하고샆오요",
          "original_form": "난 조용하게 연애하고샆오요",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100047200.1.34",
          "form": "그리고 전 꼬치꼬치가 나아요 하 ...",
          "original_form": "그리고 전 꼬치꼬치가 나아요 하 ...",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100047200.1.35",
          "form": "아뇨 님.",
          "original_form": "아뇨 님.",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100047200.1.36",
          "form": "불가능하죠",
          "original_form": "불가능하죠",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100047200.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100047200.1.38",
          "form": "풀어주다가 화날거같아 어케 일주일 삐져 사람이냐?!",
          "original_form": "풀어주다가 화날거같아 어케 일주일 삐져 사람이냐?!",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100047200.1.39",
          "form": "꼬치꼬치 말꼬리잡는데요?",
          "original_form": "꼬치꼬치 말꼬리잡는데요?",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100047200.1.40",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100047200.1.41",
          "form": "제발.....",
          "original_form": "제발.....",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100047200.1.42",
          "form": "같이잡아보자고",
          "original_form": "같이잡아보자고",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100047200.1.43",
          "form": "적반하장 #가보자고",
          "original_form": "적반하장 #가보자고",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100047200.1.44",
          "form": "내가 미안하다고..\r\nㄴ뭐가미안하냐고\r\n이거이거 잘못했다고\r\nㄴ잘못한걸 아는 사람이 그러냐",
          "original_form": "내가 미안하다고..\r\nㄴ뭐가미안하냐고\r\n이거이거 잘못했다고\r\nㄴ잘못한걸 아는 사람이 그러냐",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100047200.1.45",
          "form": "아 ...",
          "original_form": "아 ...",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100047200.1.46",
          "form": "냐아? 냐라고 했어 지금??",
          "original_form": "냐아? 냐라고 했어 지금??",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100047200.1.47",
          "form": "진짜 화난다 어떡하지",
          "original_form": "진짜 화난다 어떡하지",
          "speaker_id": "2",
          "time": "20210825 23:39"
