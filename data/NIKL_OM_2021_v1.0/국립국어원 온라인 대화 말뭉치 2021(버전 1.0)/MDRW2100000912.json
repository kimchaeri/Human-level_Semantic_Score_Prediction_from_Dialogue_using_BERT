      "utterance": [
        {
          "id": "MDRW2100000912.1.1",
          "form": "야 울집 오늘 엄마가 덥다고 에어컨 켬",
          "original_form": "야 울집 오늘 엄마가 덥다고 에어컨 켬",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000912.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ나는 지금도 틀려있음",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ나는 지금도 틀려있음",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000912.1.3",
          "form": "저녁밥하는데 더웠나봐",
          "original_form": "저녁밥하는데 더웠나봐",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000912.1.4",
          "form": "기숙사는 뭐 맨날 틀어야지.",
          "original_form": "기숙사는 뭐 맨날 틀어야지.",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000912.1.5",
          "form": "밖에 나가서 걸어보ㅏ",
          "original_form": "밖에 나가서 걸어보ㅏ",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000912.1.6",
          "form": "나는 아까 낮에 잠들었는데 춥더라ㅠ",
          "original_form": "나는 아까 낮에 잠들었는데 춥더라ㅠ",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000912.1.7",
          "form": "맨날 덥다",
          "original_form": "맨날 덥다",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000912.1.8",
          "form": "이불 두개나 두고 담요 두개덮고 잠",
          "original_form": "이불 두개나 두고 담요 두개덮고 잠",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000912.1.9",
          "form": "요새 근데 밤마다 비 계속 와",
          "original_form": "요새 근데 밤마다 비 계속 와",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000912.1.10",
          "form": "그 너무 과한거같은데",
          "original_form": "그 너무 과한거같은데",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000912.1.11",
          "form": "아까도 거의 스콜수준..",
          "original_form": "아까도 거의 스콜수준..",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000912.1.12",
          "form": "아 그니까 무슨 스콜처럼",
          "original_form": "아 그니까 무슨 스콜처럼",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000912.1.13",
          "form": "과하기는. 춥더라.",
          "original_form": "과하기는. 춥더라.",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000912.1.14",
          "form": "아까 번개는 봤나",
          "original_form": "아까 번개는 봤나",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000912.1.15",
          "form": "그래도 밤마다 비오니까 시원해",
          "original_form": "그래도 밤마다 비오니까 시원해",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000912.1.16",
          "form": "그리고 요즘 해 갑자기 엄청 짧아짐",
          "original_form": "그리고 요즘 해 갑자기 엄청 짧아짐",
          "speaker_id": "1",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000912.1.17",
          "form": "어니??번개도 쳤나",
          "original_form": "어니??번개도 쳤나",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000912.1.18",
          "form": "나 걸어오는데 븐쩍 하더라",
          "original_form": "나 걸어오는데 븐쩍 하더라",
          "speaker_id": "1",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000912.1.19",
          "form": "나 커튼내리고 이어폰껴서 안들려",
          "original_form": "나 커튼내리고 이어폰껴서 안들려",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000912.1.20",
          "form": "해 짧아지지마ㅠㅠ",
          "original_form": "해 짧아지지마ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:35"
        }
      ]
