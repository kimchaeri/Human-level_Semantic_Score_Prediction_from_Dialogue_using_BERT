      "utterance": [
        {
          "id": "MDRW2100006700.1.1",
          "form": "집에서 노는거 좋아하세요?",
          "original_form": "집에서 노는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100006700.1.2",
          "form": "넼ㅋㅋㅋ 집이 너무 좋아요!",
          "original_form": "넼ㅋㅋㅋ 집이 너무 좋아요!",
          "speaker_id": "2",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100006700.1.3",
          "form": "ㅎㅎ 보통 모하시나요",
          "original_form": "ㅎㅎ 보통 모하시나요",
          "speaker_id": "1",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100006700.1.4",
          "form": "핸드폰하거나 넷플릭스 아니면 유투브를 보는 편이에요",
          "original_form": "핸드폰하거나 넷플릭스 아니면 유투브를 보는 편이에요",
          "speaker_id": "2",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100006700.1.5",
          "form": "진짜 뻘짓으로는 바닥에 누워서 구르기도 하구요ㅋㅋㅋㅋ",
          "original_form": "진짜 뻘짓으로는 바닥에 누워서 구르기도 하구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100006700.1.6",
          "form": "ㅋㅋ 요즘엔 홈트도 많이 하던데ㅋ",
          "original_form": "ㅋㅋ 요즘엔 홈트도 많이 하던데ㅋ",
          "speaker_id": "1",
          "time": "20210908 07:14"
        },
        {
          "id": "MDRW2100006700.1.7",
          "form": "홈트는 해보려고 했는데 자세가 안 좋으면 안하는게 낫다고해서 초보가 괜히 나댔다가 병원갈까봐 못하겠더라구요",
          "original_form": "홈트는 해보려고 했는데 자세가 안 좋으면 안하는게 낫다고해서 초보가 괜히 나댔다가 병원갈까봐 못하겠더라구요",
          "speaker_id": "2",
          "time": "20210908 07:15"
        },
        {
          "id": "MDRW2100006700.1.8",
          "form": "귀찮은것도 았구요",
          "original_form": "귀찮은것도 았구요",
          "speaker_id": "2",
          "time": "20210908 07:15"
        },
        {
          "id": "MDRW2100006700.1.9",
          "form": "맞아요 너무 운동해도 해롭대요",
          "original_form": "맞아요 너무 운동해도 해롭대요",
          "speaker_id": "1",
          "time": "20210908 07:15"
        },
        {
          "id": "MDRW2100006700.1.10",
          "form": "역시 뭐든 적당히가 좋은 것 같아요",
          "original_form": "역시 뭐든 적당히가 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100006700.1.11",
          "form": "넷플은 티비로 보세요?",
          "original_form": "넷플은 티비로 보세요?",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100006700.1.12",
          "form": "네! 티비로 크게봐요!!",
          "original_form": "네! 티비로 크게봐요!!",
          "speaker_id": "2",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100006700.1.13",
          "form": "ㅎㅎ 집에선 또 뭘하고 놀 수 있을까요",
          "original_form": "ㅎㅎ 집에선 또 뭘하고 놀 수 있을까요",
          "speaker_id": "1",
          "time": "20210908 07:27"
        },
        {
          "id": "MDRW2100006700.1.14",
          "form": "어... 멍때리기나 컴퓨터 게임이 있지 않을까요?",
          "original_form": "어... 멍때리기나 컴퓨터 게임이 있지 않을까요?",
          "speaker_id": "2",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100006700.1.15",
          "form": "저는 게임은 잼병이라 침대위에서 뒹굴거리는게 다지만요 ㅎㅎ",
          "original_form": "저는 게임은 잼병이라 침대위에서 뒹굴거리는게 다지만요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100006700.1.16",
          "form": "그럼 침대와 쇼파가 좋으셔야겠어요ㅋ",
          "original_form": "그럼 침대와 쇼파가 좋으셔야겠어요ㅋ",
          "speaker_id": "1",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100006700.1.17",
          "form": "넼ㅋㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:31"
        },
        {
          "id": "MDRW2100006700.1.18",
          "form": "침대는 너무나 사랑이에요ㅋㅋㅋㅋ",
