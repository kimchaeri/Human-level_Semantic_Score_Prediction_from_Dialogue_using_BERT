      "utterance": [
        {
          "id": "MDRW2100001033.1.1",
          "form": "오늘 점심 뭐먹어??",
          "original_form": "오늘 점심 뭐먹어??",
          "speaker_id": "1",
          "time": "20210721 11:05"
        },
        {
          "id": "MDRW2100001033.1.2",
          "form": "오늘 치킨!",
          "original_form": "오늘 치킨!",
          "speaker_id": "2",
          "time": "20210721 11:07"
        },
        {
          "id": "MDRW2100001033.1.3",
          "form": "와 마싯겠다",
          "original_form": "와 마싯겠다",
          "speaker_id": "1",
          "time": "20210721 11:08"
        },
        {
          "id": "MDRW2100001033.1.4",
          "form": "뭐 시켰어?",
          "original_form": "뭐 시켰어?",
          "speaker_id": "1",
          "time": "20210721 11:08"
        },
        {
          "id": "MDRW2100001033.1.5",
          "form": "나 네네치킨",
          "original_form": "나 네네치킨",
          "speaker_id": "2",
          "time": "20210721 11:09"
        },
        {
          "id": "MDRW2100001033.1.6",
          "form": "내가 스트레스 받으면",
          "original_form": "내가 스트레스 받으면",
          "speaker_id": "2",
          "time": "20210721 11:09"
        },
        {
          "id": "MDRW2100001033.1.7",
          "form": "시켜먹어",
          "original_form": "시켜먹어",
          "speaker_id": "2",
          "time": "20210721 11:09"
        },
        {
          "id": "MDRW2100001033.1.8",
          "form": "쇼킹핫",
          "original_form": "쇼킹핫",
          "speaker_id": "2",
          "time": "20210721 11:09"
        },
        {
          "id": "MDRW2100001033.1.9",
          "form": "쇼킹핫 진짜 매운데",
          "original_form": "쇼킹핫 진짜 매운데",
          "speaker_id": "1",
          "time": "20210721 11:12"
        },
        {
          "id": "MDRW2100001033.1.10",
          "form": "스트레스 엄청 받아야 먹는거겠지?",
          "original_form": "스트레스 엄청 받아야 먹는거겠지?",
          "speaker_id": "1",
          "time": "20210721 11:12"
        },
        {
          "id": "MDRW2100001033.1.11",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210721 11:13"
        },
        {
          "id": "MDRW2100001033.1.12",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210721 11:13"
        },
        {
          "id": "MDRW2100001033.1.13",
          "form": "헤헤",
          "original_form": "헤헤",
          "speaker_id": "2",
          "time": "20210721 11:13"
        },
        {
          "id": "MDRW2100001033.1.14",
          "form": "너는 무슨 치킨 좋아해",
          "original_form": "너는 무슨 치킨 좋아해",
          "speaker_id": "2",
          "time": "20210721 11:13"
        },
        {
          "id": "MDRW2100001033.1.15",
          "form": "난 옛날치킨을 제일 좋아함",
          "original_form": "난 옛날치킨을 제일 좋아함",
          "speaker_id": "1",
          "time": "20210721 11:13"
        },
        {
          "id": "MDRW2100001033.1.16",
          "form": "소스찍어먹는게 마싯어",
          "original_form": "소스찍어먹는게 마싯어",
          "speaker_id": "1",
          "time": "20210721 11:13"
        },
        {
          "id": "MDRW2100001033.1.17",
          "form": "헉 나도 좋아해",
          "original_form": "헉 나도 좋아해",
          "speaker_id": "2",
          "time": "20210721 11:14"
        },
        {
          "id": "MDRW2100001033.1.18",
          "form": "우리집앞에 맛집있는데",
          "original_form": "우리집앞에 맛집있는데",
          "speaker_id": "2",
          "time": "20210721 11:14"
        },
        {
          "id": "MDRW2100001033.1.19",
          "form": "헐 담에 너네집 놀러갈테니까",
          "original_form": "헐 담에 너네집 놀러갈테니까",
          "speaker_id": "1",
          "time": "20210721 11:15"
        },
        {
          "id": "MDRW2100001033.1.20",
          "form": "그거먹자",
          "original_form": "그거먹자",
          "speaker_id": "1",
          "time": "20210721 11:15"
        },
        {
          "id": "MDRW2100001033.1.21",
          "form": "그러자~~",
          "original_form": "그러자~~",
          "speaker_id": "2",
          "time": "20210721 11:15"
        },
        {
          "id": "MDRW2100001033.1.22",
          "form": "거기 줄 서있어",
          "original_form": "거기 줄 서있어",
          "speaker_id": "2",
          "time": "20210721 11:15"
        },
        {
          "id": "MDRW2100001033.1.23",
          "form": "줄 서서 먹는데야??",
          "original_form": "줄 서서 먹는데야??",
          "speaker_id": "1",
          "time": "20210721 11:16"
        },
        {
          "id": "MDRW2100001033.1.24",
          "form": "헉스바뤼마싯겟다",
          "original_form": "헉스바뤼마싯겟다",
          "speaker_id": "1",
          "time": "20210721 11:16"
        },
        {
          "id": "MDRW2100001033.1.25",
          "form": "그치 완전 껍질 바삭",
          "original_form": "그치 완전 껍질 바삭",
          "speaker_id": "2",
          "time": "20210721 11:16"
        },
        {
          "id": "MDRW2100001033.1.26",
          "form": "냠냠",
          "original_form": "냠냠",
          "speaker_id": "2",
          "time": "20210721 11:16"
        },
        {
          "id": "MDRW2100001033.1.27",
          "form": "소스는 다양해??",
          "original_form": "소스는 다양해??",
