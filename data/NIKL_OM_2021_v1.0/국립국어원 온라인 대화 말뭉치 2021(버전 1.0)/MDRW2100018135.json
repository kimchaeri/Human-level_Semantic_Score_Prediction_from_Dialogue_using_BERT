      "utterance": [
        {
          "id": "MDRW2100018135.1.1",
          "form": "다이어트 하시나요?",
          "original_form": "다이어트 하시나요?",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018135.1.2",
          "form": "해야지 .. 생각만 하고 있어요",
          "original_form": "해야지 .. 생각만 하고 있어요",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018135.1.3",
          "form": "추천하실 다이어트 방법이 있나요?",
          "original_form": "추천하실 다이어트 방법이 있나요?",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018135.1.4",
          "form": "간헐적 단식요ㅋ",
          "original_form": "간헐적 단식요ㅋ",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018135.1.5",
          "form": "해보셨나요??? 너무 배고플거 같아요",
          "original_form": "해보셨나요??? 너무 배고플거 같아요",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018135.1.6",
          "form": "전 점심 저녁만 먹고 16시간 공복인데 효과 봤어요",
          "original_form": "전 점심 저녁만 먹고 16시간 공복인데 효과 봤어요",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018135.1.7",
          "form": "저는 한약다이어트만 성공해 봤어요",
          "original_form": "저는 한약다이어트만 성공해 봤어요",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018135.1.8",
          "form": "요요가 온다던데 괜찮았어요?",
          "original_form": "요요가 온다던데 괜찮았어요?",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018135.1.9",
          "form": "요요 와요ㅎㅎ 그냥 안먹게 하는거에요",
          "original_form": "요요 와요ㅎㅎ 그냥 안먹게 하는거에요",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018135.1.10",
          "form": "식욕 저하되게",
          "original_form": "식욕 저하되게",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018135.1.11",
          "form": "제 주위에도 하던데 안 먹어서 그런지 빠지긴 하드라구요ㅋ",
          "original_form": "제 주위에도 하던데 안 먹어서 그런지 빠지긴 하드라구요ㅋ",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018135.1.12",
          "form": "운동으로 빼야 한다는데",
          "original_form": "운동으로 빼야 한다는데",
          "speaker_id": "2",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018135.1.13",
          "form": "말이 쉽지 어렵더라구여",
          "original_form": "말이 쉽지 어렵더라구여",
          "speaker_id": "2",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018135.1.14",
          "form": "맞아요.  근데 식이와 같이 해야 빠져요",
          "original_form": "맞아요.  근데 식이와 같이 해야 빠져요",
          "speaker_id": "1",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018135.1.15",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018135.1.16",
          "form": "예전에 걷는거 좋아했는데 지금은 게으러서 아무것도 안해요ㅋ",
          "original_form": "예전에 걷는거 좋아했는데 지금은 게으러서 아무것도 안해요ㅋ",
          "speaker_id": "1",
          "time": "20210826 23:03"
        }
