      "utterance": [
        {
          "id": "MDRW2100025741.1.1",
          "form": "벤다이어그램식",
          "original_form": "벤다이어그램식",
          "speaker_id": "1",
          "time": "20210901 16:39"
        },
        {
          "id": "MDRW2100025741.1.2",
          "form": "겹치기 갈거 같아요 ㅋㅋ",
          "original_form": "겹치기 갈거 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:39"
        },
        {
          "id": "MDRW2100025741.1.3",
          "form": "대탈출은 보셨어요?",
          "original_form": "대탈출은 보셨어요?",
          "speaker_id": "2",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025741.1.4",
          "form": "네 ㅋㅋ 재밌죠",
          "original_form": "네 ㅋㅋ 재밌죠",
          "speaker_id": "1",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100025741.1.5",
          "form": "오ㅜ이번 시리즈 보셨어요? 완전 대박",
          "original_form": "오ㅜ이번 시리즈 보셨어요? 완전 대박",
          "speaker_id": "2",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100025741.1.6",
          "form": "네 진짜 그 레일로 이동하는 스케일이....",
          "original_form": "네 진짜 그 레일로 이동하는 스케일이....",
          "speaker_id": "1",
          "time": "20210901 16:43"
        },
        {
          "id": "MDRW2100025741.1.7",
          "form": "악마가 따라오고 있는 설정도요 ",
          "original_form": "악마가 따라오고 있는 설정도요 ",
          "speaker_id": "2",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100025741.1.8",
          "form": "아 문열릴때",
          "original_form": "아 문열릴때",
          "speaker_id": "1",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100025741.1.9",
          "form": "진짜 소리지를뻔ㅋㅋ",
          "original_form": "진짜 소리지를뻔ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025741.1.10",
          "form": "진짜 무서웠어요",
          "original_form": "진짜 무서웠어요",
          "speaker_id": "1",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025741.1.11",
          "form": "진짜ㅠㅋㅋ 그거보면서 너무 보드게임같지않나 시시하다햤는데",
          "original_form": "진짜ㅠㅋㅋ 그거보면서 너무 보드게임같지않나 시시하다햤는데",
          "speaker_id": "2",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025741.1.12",
          "form": "바로 악마나와서 ㅋㅋ",
          "original_form": "바로 악마나와서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025741.1.13",
          "form": "불끄고 보고있었는데 이불 뒤집어썼잖라욬",
          "original_form": "불끄고 보고있었는데 이불 뒤집어썼잖라욬",
          "speaker_id": "2",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025741.1.14",
          "form": "ㅋㅋ진짜 역대급 편이었어요",
          "original_form": "ㅋㅋ진짜 역대급 편이었어요",
          "speaker_id": "1",
          "time": "20210901 18:00"
        },
        {
          "id": "MDRW2100025741.1.15",
          "form": "잔잔하면서 역대급ㅋㅋㅋ",
          "original_form": "잔잔하면서 역대급ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:06"
        },
        {
          "id": "MDRW2100025741.1.16",
          "form": "ㅋㅋ진짜 타임머신에 이은 대작이었어요",
          "original_form": "ㅋㅋ진짜 타임머신에 이은 대작이었어요",
          "speaker_id": "1",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100025741.1.17",
          "form": "아 타임머신1도 대박이었는데 막판도 스케일컸죠!",
          "original_form": "아 타임머신1도 대박이었는데 막판도 스케일컸죠!",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100025741.1.18",
          "form": "암호가 산토끼였나",
          "original_form": "암호가 산토끼였나",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100025741.1.19",
          "form": "ㅋㅋ철방회 재미졌죠",
          "original_form": "ㅋㅋ철방회 재미졌죠",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100025741.1.20",
          "form": "아 철방회?",
          "original_form": "아 철방회?",
          "speaker_id": "2",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100025741.1.21",
          "form": "네 ㅋㅋ 아 산토끼 그 하석진 암호 그건가",
