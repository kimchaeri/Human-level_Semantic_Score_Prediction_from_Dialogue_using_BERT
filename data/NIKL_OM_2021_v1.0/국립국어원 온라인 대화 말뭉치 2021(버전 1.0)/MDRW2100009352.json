      "utterance": [
        {
          "id": "MDRW2100009352.1.1",
          "form": "무슨 계절 좋아하세요?",
          "original_form": "무슨 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100009352.1.2",
          "form": "저는 겨울이요! 요즘 시원해져서 너무 좋아요 ㅋㅋㅋ",
          "original_form": "저는 겨울이요! 요즘 시원해져서 너무 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009352.1.3",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009352.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009352.1.5",
          "form": "봄부터 겨울만 기다려요",
          "original_form": "봄부터 겨울만 기다려요",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009352.1.6",
          "form": "더위에 취약해서요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "더위에 취약해서요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009352.1.7",
          "form": "겨울 왜 좋아하세요?",
          "original_form": "겨울 왜 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009352.1.8",
          "form": "여름의 불쾌지수를 못 견디겠어요ㅋㅋㅋㅋ",
          "original_form": "여름의 불쾌지수를 못 견디겠어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009352.1.9",
          "form": "에어컨 없으면 절대 살 수 없는...",
          "original_form": "에어컨 없으면 절대 살 수 없는...",
          "speaker_id": "2",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009352.1.10",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009352.1.11",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009352.1.12",
          "form": "벌레도요",
          "original_form": "벌레도요",
          "speaker_id": "1",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009352.1.13",
          "form": "아 벌레 동감요\r\n이번 여름 너무 더워서 모기도 그렇고 벌레들 없었다는 게 너무 어이없어요 ㅋㅋ",
          "original_form": "아 벌레 동감요\r\n이번 여름 너무 더워서 모기도 그렇고 벌레들 없었다는 게 너무 어이없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009352.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009352.1.15",
          "form": "평소보다 적기는 했어요",
          "original_form": "평소보다 적기는 했어요",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009352.1.16",
          "form": "그래도 많이 보긴 했어요",
          "original_form": "그래도 많이 보긴 했어요",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009352.1.17",
          "form": "집이 2층이라서요",
          "original_form": "집이 2층이라서요",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009352.1.18",
          "form": "제가 경기 북부에서 평생 살다가 경남에 살게 됐는데 벌레 크기부터 다르더라고요",
          "original_form": "제가 경기 북부에서 평생 살다가 경남에 살게 됐는데 벌레 크기부터 다르더라고요",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009352.1.19",
          "form": "와 저층이시면 평소에 많이 보시겠네요",
          "original_form": "와 저층이시면 평소에 많이 보시겠네요",
          "speaker_id": "2",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009352.1.20",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009352.1.21",
          "form": "헉 경남은 크군요..",
          "original_form": "헉 경남은 크군요..",
          "speaker_id": "2",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009352.1.22",
          "form": "경기 북부에서는",
          "original_form": "경기 북부에서는",
          "speaker_id": "1",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009352.1.23",
          "form": "겨울이 추워서",
          "original_form": "겨울이 추워서",
          "speaker_id": "1",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009352.1.24",
          "form": "겨울마다 죽고 새로 태어났는데",
          "original_form": "겨울마다 죽고 새로 태어났는데",
          "speaker_id": "1",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009352.1.25",
          "form": "아하ㅋㅋㅋㅋ",
          "original_form": "아하ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009352.1.26",
          "form": "경남은 그런 거 없이 작년 본 그 애가 그 애더라고요",
          "original_form": "경남은 그런 거 없이 작년 본 그 애가 그 애더라고요",
          "speaker_id": "1",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009352.1.27",
          "form": "으악",
          "original_form": "으악",
          "speaker_id": "1",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009352.1.28",
          "form": "오마이갓 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오마이갓 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009352.1.29",
          "form": "중부지방 사세요?",
          "original_form": "중부지방 사세요?",
          "speaker_id": "1",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009352.1.30",
          "form": "저는 전남 거주중이에요 ㅋㅋ",
          "original_form": "저는 전남 거주중이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:58"
        }
      ]
    }
