      "utterance": [
        {
          "id": "MDRW2100002130.1.1",
          "form": "요즘 당근마켓에 공유기 키워드 해두거든",
          "original_form": "요즘 당근마켓에 공유기 키워드 해두거든",
          "speaker_id": "1",
          "time": "20210819 23:50"
        },
        {
          "id": "MDRW2100002130.1.2",
          "form": "아 맞다 공유기 필요하지",
          "original_form": "아 맞다 공유기 필요하지",
          "speaker_id": "2",
          "time": "20210819 23:50"
        },
        {
          "id": "MDRW2100002130.1.3",
          "form": "근데 공유기 파는 사람들 있더나",
          "original_form": "근데 공유기 파는 사람들 있더나",
          "speaker_id": "2",
          "time": "20210819 23:50"
        },
        {
          "id": "MDRW2100002130.1.4",
          "form": "응 근데 진짜 가격 비싸게 팔더라",
          "original_form": "응 근데 진짜 가격 비싸게 팔더라",
          "speaker_id": "1",
          "time": "20210819 23:50"
        },
        {
          "id": "MDRW2100002130.1.5",
          "form": "공유기 비싸더라ㅠ",
          "original_form": "공유기 비싸더라ㅠ",
          "speaker_id": "2",
          "time": "20210819 23:50"
        },
        {
          "id": "MDRW2100002130.1.6",
          "form": "인터넷에서 이만원인데 만오천원에 팔아",
          "original_form": "인터넷에서 이만원인데 만오천원에 팔아",
          "speaker_id": "1",
          "time": "20210819 23:50"
        },
        {
          "id": "MDRW2100002130.1.7",
          "form": "헝 그래도 괜찮은데??",
          "original_form": "헝 그래도 괜찮은데??",
          "speaker_id": "2",
          "time": "20210819 23:50"
        },
        {
          "id": "MDRW2100002130.1.8",
          "form": "나도 첫 자취할때 비싸게 주고 삿다..",
          "original_form": "나도 첫 자취할때 비싸게 주고 삿다..",
          "speaker_id": "2",
          "time": "20210819 23:50"
        },
        {
          "id": "MDRW2100002130.1.9",
          "form": "그리고 좀 가격 괜찮다 싶으면",
          "original_form": "그리고 좀 가격 괜찮다 싶으면",
          "speaker_id": "1",
          "time": "20210819 23:50"
        },
        {
          "id": "MDRW2100002130.1.10",
          "form": "멀리가야해",
          "original_form": "멀리가야해",
          "speaker_id": "1",
          "time": "20210819 23:51"
        },
        {
          "id": "MDRW2100002130.1.11",
          "form": "막 서구까지뜨더라",
          "original_form": "막 서구까지뜨더라",
          "speaker_id": "1",
          "time": "20210819 23:51"
        },
        {
          "id": "MDRW2100002130.1.12",
          "form": "그러면 그냥 집에거 가져가서 써봐",
          "original_form": "그러면 그냥 집에거 가져가서 써봐",
          "speaker_id": "2",
          "time": "20210819 23:51"
        },
        {
          "id": "MDRW2100002130.1.13",
          "form": "느리겠지만.. 잘안되겎지만",
          "original_form": "느리겠지만.. 잘안되겎지만",
          "speaker_id": "2",
          "time": "20210819 23:51"
        },
        {
          "id": "MDRW2100002130.1.14",
          "form": "예전에 쓰던거?",
          "original_form": "예전에 쓰던거?",
          "speaker_id": "1",
          "time": "20210819 23:51"
        },
        {
          "id": "MDRW2100002130.1.15",
          "form": "그래야겠다...근데 홧병날듯 ㅠ",
          "original_form": "그래야겠다...근데 홧병날듯 ㅠ",
          "speaker_id": "1",
          "time": "20210819 23:51"
        },
        {
          "id": "MDRW2100002130.1.16",
          "form": "공유기가격은 왜 안싸지지?",
          "original_form": "공유기가격은 왜 안싸지지?",
          "speaker_id": "2",
          "time": "20210819 23:51"
        },
        {
          "id": "MDRW2100002130.1.17",
          "form": "아 맞다ㅠ",
          "original_form": "아 맞다ㅠ",
          "speaker_id": "2",
          "time": "20210819 23:51"
        },
        {
          "id": "MDRW2100002130.1.18",
          "form": "아니 저번에 가격 괜찮아서",
          "original_form": "아니 저번에 가격 괜찮아서",
          "speaker_id": "1",
          "time": "20210819 23:51"
        },
        {
          "id": "MDRW2100002130.1.19",
          "form": "나랑같이 알뜰요금제 쓰자",
          "original_form": "나랑같이 알뜰요금제 쓰자",
          "speaker_id": "2",
          "time": "20210819 23:51"
        },
        {
          "id": "MDRW2100002130.1.20",
          "form": "데이터 맘ㅎ은걸로... 그래서 공유기 필요없이...",
          "original_form": "데이터 맘ㅎ은걸로... 그래서 공유기 필요없이...",
          "speaker_id": "2",
          "time": "20210819 23:51"
        },
        {
          "id": "MDRW2100002130.1.21",
          "form": "호다닥 대화걸고난다음에",
          "original_form": "호다닥 대화걸고난다음에",
          "speaker_id": "1",
          "time": "20210819 23:51"
        },
        {
          "id": "MDRW2100002130.1.22",
          "form": "와 그것도 괜찮네",
          "original_form": "와 그것도 괜찮네",
          "speaker_id": "1",
          "time": "20210819 23:51"
        },
        {
          "id": "MDRW2100002130.1.23",
          "form": "알뜰가자~~",
          "original_form": "알뜰가자~~",
          "speaker_id": "2",
          "time": "20210819 23:51"
        }
      ]
