      "utterance": [
        {
          "id": "MDRW2100041969.1.1",
          "form": "저녁 김치찌개 남은거 먹을지 마라샹궈 먹을디 고민",
          "original_form": "저녁 김치찌개 남은거 먹을지 마라샹궈 먹을디 고민",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041969.1.2",
          "form": "뭐머거요?",
          "original_form": "뭐머거요?",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041969.1.3",
          "form": "오마라샹궈\r\n저는 버거킹요!",
          "original_form": "오마라샹궈\r\n저는 버거킹요!",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041969.1.4",
          "form": "와 오늘 버거킹 먹나요 버거킹 뭐가 맛있어요?",
          "original_form": "와 오늘 버거킹 먹나요 버거킹 뭐가 맛있어요?",
          "speaker_id": "2",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041969.1.5",
          "form": "저는 와퍼밖에안먹어본듯해요!",
          "original_form": "저는 와퍼밖에안먹어본듯해요!",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041969.1.6",
          "form": "오늘요기요행사해서ㅎㅎ",
          "original_form": "오늘요기요행사해서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041969.1.7",
          "form": "우와 행사하는구나",
          "original_form": "우와 행사하는구나",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041969.1.8",
          "form": "저는 무슨 새우 와퍼인가 진짜 맛났던거같아요",
          "original_form": "저는 무슨 새우 와퍼인가 진짜 맛났던거같아요",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041969.1.9",
          "form": "오새우와퍼궁금해요",
          "original_form": "오새우와퍼궁금해요",
          "speaker_id": "1",
          "time": "20210906 18:22"
        },
        {
          "id": "MDRW2100041969.1.10",
          "form": "버거킹 조아요 뭐 골랐나요",
          "original_form": "버거킹 조아요 뭐 골랐나요",
          "speaker_id": "2",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100041969.1.11",
          "form": "그냥와퍼시켜쪄여!",
          "original_form": "그냥와퍼시켜쪄여!",
          "speaker_id": "1",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100041969.1.12",
          "form": "저녁정햇어요?",
          "original_form": "저녁정햇어요?",
          "speaker_id": "1",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100041969.1.13",
          "form": "저녁 아마도 남은 김치찌개 먹을것같아요",
          "original_form": "저녁 아마도 남은 김치찌개 먹을것같아요",
          "speaker_id": "2",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100041969.1.14",
          "form": "아하 마라샹궈를참앗군요",
          "original_form": "아하 마라샹궈를참앗군요",
          "speaker_id": "1",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100041969.1.15",
          "form": "네네 마라샹궈 줄여야해요ㅠ",
          "original_form": "네네 마라샹궈 줄여야해요ㅠ",
          "speaker_id": "2",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100041969.1.16",
          "form": "버거킹 맛나겠다",
          "original_form": "버거킹 맛나겠다",
          "speaker_id": "2",
          "time": "20210906 18:35"
        },
        {
          "id": "MDRW2100041969.1.17",
          "form": "맛납니다 또 2버거때렷어요",
          "original_form": "맛납니다 또 2버거때렷어요",
          "speaker_id": "1",
          "time": "20210906 18:35"
        },
        {
          "id": "MDRW2100041969.1.18",
          "form": "우아 잘했어요",
          "original_form": "우아 잘했어요",
          "speaker_id": "2",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100041969.1.19",
          "form": "콜라됴 드시나요",
          "original_form": "콜라됴 드시나요",
