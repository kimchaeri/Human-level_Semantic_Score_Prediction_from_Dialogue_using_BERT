      "utterance": [
        {
          "id": "MDRW2100021100.1.1",
          "form": "올림픽 보셨나요?",
          "original_form": "올림픽 보셨나요?",
          "speaker_id": "1",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100021100.1.2",
          "form": "네 ㅋㅋㅋㅋ 엄청 열심히 봣어요",
          "original_form": "네 ㅋㅋㅋㅋ 엄청 열심히 봣어요",
          "speaker_id": "2",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100021100.1.3",
          "form": "뭐보셨어요?",
          "original_form": "뭐보셨어요?",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100021100.1.4",
          "form": "거의 다봣어요 ㅋㅋㅋㅋ name1님은요?",
          "original_form": "거의 다봣어요 ㅋㅋㅋㅋ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100021100.1.5",
          "form": "저는 축구 펜싱 양궁 정도요 ㅋㅋㅋ",
          "original_form": "저는 축구 펜싱 양궁 정도요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100021100.1.6",
          "form": "배구도 화제엿는데 배구는 안보셧나요?",
          "original_form": "배구도 화제엿는데 배구는 안보셧나요?",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100021100.1.7",
          "form": "아 배구는 안보다가 막경기만 조금 봤어요",
          "original_form": "아 배구는 안보다가 막경기만 조금 봤어요",
          "speaker_id": "1",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100021100.1.8",
          "form": "보신 경기중에는 뭘 재밋게 보셧어요?",
          "original_form": "보신 경기중에는 뭘 재밋게 보셧어요?",
          "speaker_id": "2",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100021100.1.9",
          "form": "음 저는",
          "original_form": "음 저는",
          "speaker_id": "1",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100021100.1.10",
          "form": "축구를 원래 좋아해서 ㅎㅎ축구요",
          "original_form": "축구를 원래 좋아해서 ㅎㅎ축구요",
          "speaker_id": "1",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100021100.1.11",
          "form": "아 축구 좋아하시는구나",
          "original_form": "아 축구 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100021100.1.12",
          "form": "네 잘 모르는데",
          "original_form": "네 잘 모르는데",
          "speaker_id": "1",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100021100.1.13",
          "form": "재밌더라구요",
          "original_form": "재밌더라구요",
          "speaker_id": "1",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100021100.1.14",
          "form": "어디 응원하세요?",
          "original_form": "어디 응원하세요?",
          "speaker_id": "2",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100021100.1.15",
          "form": "전 팀? 그런거까진 모르고 그냥 월드컵 이런 큰 경기 우리나라거만 봐요 ㅌㅋㅋ",
          "original_form": "전 팀? 그런거까진 모르고 그냥 월드컵 이런 큰 경기 우리나라거만 봐요 ㅌㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100021100.1.16",
          "form": "국대 경기가 재밋죠 ㅋㅋ",
          "original_form": "국대 경기가 재밋죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100021100.1.17",
          "form": "러시아 월드컵 재밋게 보셧겟네요",
          "original_form": "러시아 월드컵 재밋게 보셧겟네요",
