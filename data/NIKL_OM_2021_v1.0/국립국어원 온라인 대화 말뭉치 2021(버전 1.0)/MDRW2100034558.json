      "utterance": [
        {
          "id": "MDRW2100034558.1.1",
          "form": "주로 영화볼때 뭐드세요?",
          "original_form": "주로 영화볼때 뭐드세요?",
          "speaker_id": "1",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100034558.1.2",
          "form": "집에서요 영화관에서요?",
          "original_form": "집에서요 영화관에서요?",
          "speaker_id": "2",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100034558.1.3",
          "form": "음 둘다요 ㅋㅋㅋ",
          "original_form": "음 둘다요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:33"
        },
        {
          "id": "MDRW2100034558.1.4",
          "form": "일단 집에서는 영화볼때는 주로 친구들이랑봐서 저녁먹으면서 보는데 주로 회 많이  먹는거같아요",
          "original_form": "일단 집에서는 영화볼때는 주로 친구들이랑봐서 저녁먹으면서 보는데 주로 회 많이  먹는거같아요",
          "speaker_id": "2",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100034558.1.5",
          "form": "오 회고 좋죠!",
          "original_form": "오 회고 좋죠!",
          "speaker_id": "1",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100034558.1.6",
          "form": "회는 생각못했네요",
          "original_form": "회는 생각못했네요",
          "speaker_id": "1",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100034558.1.7",
          "form": "영화관에서는 진부하지만 제가 팝콘을 좋아해서 팝콘 꼭 먹어요ㅋㅋㅋ",
          "original_form": "영화관에서는 진부하지만 제가 팝콘을 좋아해서 팝콘 꼭 먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100034558.1.8",
          "form": "노량진회 배달 돼서 노량진에서 자주 시켜먹어요!",
          "original_form": "노량진회 배달 돼서 노량진에서 자주 시켜먹어요!",
          "speaker_id": "2",
          "time": "20210828 15:41"
        },
        {
          "id": "MDRW2100034558.1.9",
          "form": "와 부러워요",
          "original_form": "와 부러워요",
          "speaker_id": "1",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100034558.1.10",
          "form": "노량진이면 믿고 먹겠어요!",
          "original_form": "노량진이면 믿고 먹겠어요!",
          "speaker_id": "1",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100034558.1.11",
          "form": "저는 초코팝콘 좋아해요! cgv요",
          "original_form": "저는 초코팝콘 좋아해요! cgv요",
          "speaker_id": "1",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100034558.1.12",
          "form": "노량진 맛있더라구요",
          "original_form": "노량진 맛있더라구요",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100034558.1.13",
          "form": "오 초코팝콘은 한번도 안먹어봤는데",
          "original_form": "오 초코팝콘은 한번도 안먹어봤는데",
          "speaker_id": "2",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100034558.1.14",
          "form": "초코 많이 덮혀있나요?",
          "original_form": "초코 많이 덮혀있나요?",
          "speaker_id": "2",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100034558.1.15",
          "form": "전 그거 먹으러 씨지브이 가요 ㅋㅋㅋ",
          "original_form": "전 그거 먹으러 씨지브이 가요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100034558.1.16",
          "form": "네 그건 기계가아니고",
          "original_form": "네 그건 기계가아니고",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100034558.1.17",
          "form": "사람이 팝콘 펼쳐놓고 두가지 소스를 뿌린대요!",
          "original_form": "사람이 팝콘 펼쳐놓고 두가지 소스를 뿌린대요!",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100034558.1.18",
          "form": "그래서 골고루 많아서 좋아요 ㅠㅠ",
          "original_form": "그래서 골고루 많아서 좋아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100034558.1.19",
          "form": "아ㅋㅋㅋ그정도면 초코팝콘 한번먹어봐야겠네요",
          "original_form": "아ㅋㅋㅋ그정도면 초코팝콘 한번먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100034558.1.20",
          "form": "강추합니다",
          "original_form": "강추합니다",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100034558.1.21",
          "form": "그럼 훨씬 비싸겠어요",
          "original_form": "그럼 훨씬 비싸겠어요",
          "speaker_id": "2",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100034558.1.22",
          "form": "인건비가 많이 드니까ㅋㅋ",
          "original_form": "인건비가 많이 드니까ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100034558.1.23",
          "form": "옛날이라 기억은 잘 안나는데",
          "original_form": "옛날이라 기억은 잘 안나는데",
          "speaker_id": "1",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100034558.1.24",
          "form": "다른 캬라멜 이런거보단 더비싸긴했어요 ㅠㅠ",
          "original_form": "다른 캬라멜 이런거보단 더비싸긴했어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100034558.1.25",
          "form": "양이 적어요 ㅋㅋㅋ",
          "original_form": "양이 적어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:45"
        }
      ]
    }
