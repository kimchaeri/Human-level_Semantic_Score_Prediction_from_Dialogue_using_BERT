      "utterance": [
        {
          "id": "MDRW2100010429.1.1",
          "form": "안녕하세요 요즘 구매하신거 있으세요?",
          "original_form": "안녕하세요 요즘 구매하신거 있으세요?",
          "speaker_id": "1",
          "time": "20210823 18:06"
        },
        {
          "id": "MDRW2100010429.1.2",
          "form": "안녕하세요!!",
          "original_form": "안녕하세요!!",
          "speaker_id": "2",
          "time": "20210823 18:06"
        },
        {
          "id": "MDRW2100010429.1.3",
          "form": "저는 요가매트 샀어요 최근에 ㅎㅎ",
          "original_form": "저는 요가매트 샀어요 최근에 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 18:07"
        },
        {
          "id": "MDRW2100010429.1.4",
          "form": "아 저도 하나있는데ㅎㅎ 저도 요가링샀어요ㅎㅎ",
          "original_form": "아 저도 하나있는데ㅎㅎ 저도 요가링샀어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 18:07"
        },
        {
          "id": "MDRW2100010429.1.5",
          "form": "오 요가링 어떤가요 아프다던데",
          "original_form": "오 요가링 어떤가요 아프다던데",
          "speaker_id": "2",
          "time": "20210823 18:07"
        },
        {
          "id": "MDRW2100010429.1.6",
          "form": "근데 전 살뿐이라 그런건지 안아파서 폼롤러를 사야겠어요ㅎㅎ",
          "original_form": "근데 전 살뿐이라 그런건지 안아파서 폼롤러를 사야겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 18:08"
        },
        {
          "id": "MDRW2100010429.1.7",
          "form": "아 정말요?? 저 폼롤러도 같이 샀어요 ㅋㅋ",
          "original_form": "아 정말요?? 저 폼롤러도 같이 샀어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:09"
        },
        {
          "id": "MDRW2100010429.1.8",
          "form": "근데 아직 안 해봤답니다 ㅎㅎ",
          "original_form": "근데 아직 안 해봤답니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 18:09"
        },
        {
          "id": "MDRW2100010429.1.9",
          "form": "아 그러시구나ㅎㅎ 전 어깨에 하다 죽을뻔ㅜㅜ아주 잘쓰고 있어요ㅋㅋ",
          "original_form": "아 그러시구나ㅎㅎ 전 어깨에 하다 죽을뻔ㅜㅜ아주 잘쓰고 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:09"
        },
        {
          "id": "MDRW2100010429.1.10",
          "form": "딱딱한 걸로 하시나요?",
          "original_form": "딱딱한 걸로 하시나요?",
          "speaker_id": "2",
          "time": "20210823 18:10"
        },
        {
          "id": "MDRW2100010429.1.11",
          "form": "아뇨 말랑한건데도 효과가 있더라구여",
          "original_form": "아뇨 말랑한건데도 효과가 있더라구여",
          "speaker_id": "1",
          "time": "20210823 18:10"
        },
        {
          "id": "MDRW2100010429.1.12",
          "form": "오 저는 딱딱한거 샀는데 마음의 준비를 해야겠네요 ㅎㅎ",
          "original_form": "오 저는 딱딱한거 샀는데 마음의 준비를 해야겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 18:11"
        },
        {
          "id": "MDRW2100010429.1.13",
          "form": "중고거래도 자주 하시나요?",
          "original_form": "중고거래도 자주 하시나요?",
          "speaker_id": "2",
          "time": "20210823 18:11"
        },
        {
          "id": "MDRW2100010429.1.14",
          "form": "아 당근에 파는것만 해봤어요ㅎㅎ",
          "original_form": "아 당근에 파는것만 해봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 18:12"
        },
        {
          "id": "MDRW2100010429.1.15",
          "form": "아 당근 맞아요 저도 좀 했었는데 후딱 팔아버리니까 편하더라고요 ㅋㅋ",
          "original_form": "아 당근 맞아요 저도 좀 했었는데 후딱 팔아버리니까 편하더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:13"
        },
        {
          "id": "MDRW2100010429.1.16",
          "form": "저도 그래요ㅋㅋ",
          "original_form": "저도 그래요ㅋㅋ",
