      "utterance": [
        {
          "id": "MDRW2100005264.1.1",
          "form": "살빼자",
          "original_form": "살빼자",
          "speaker_id": "1",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005264.1.2",
          "form": "내 자신의 의지로",
          "original_form": "내 자신의 의지로",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005264.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ개웃김",
          "original_form": "ㅋㅋㅋㅋㅋㅋ개웃김",
          "speaker_id": "1",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005264.1.4",
          "form": "운동뭐하니?",
          "original_form": "운동뭐하니?",
          "speaker_id": "1",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005264.1.5",
          "form": "안함 하하~",
          "original_form": "안함 하하~",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005264.1.6",
          "form": "클라이밍 해보고싶어",
          "original_form": "클라이밍 해보고싶어",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005264.1.7",
          "form": "헬스다니자",
          "original_form": "헬스다니자",
          "speaker_id": "1",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005264.1.8",
          "form": "클라이밍 살빼는데 좋나?",
          "original_form": "클라이밍 살빼는데 좋나?",
          "speaker_id": "1",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005264.1.9",
          "form": "근육엔 좋을거같은데 근력아닐까 유산소보단",
          "original_form": "근육엔 좋을거같은데 근력아닐까 유산소보단",
          "speaker_id": "2",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005264.1.10",
          "form": "근력이군",
          "original_form": "근력이군",
          "speaker_id": "1",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005264.1.11",
          "form": "출장가면 엄청 걸어다니니까 합리화해",
          "original_form": "출장가면 엄청 걸어다니니까 합리화해",
          "speaker_id": "2",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005264.1.12",
          "form": "그것도 운동이지",
          "original_form": "그것도 운동이지",
          "speaker_id": "1",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005264.1.13",
          "form": "맞아 걷기가 최고잖아",
          "original_form": "맞아 걷기가 최고잖아",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005264.1.14",
          "form": "하루 6000보 이상",
          "original_form": "하루 6000보 이상",
          "speaker_id": "1",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005264.1.15",
          "form": "맞아 삼성에서는 그걸권장하지 출근하면채워지지않아?",
          "original_form": "맞아 삼성에서는 그걸권장하지 출근하면채워지지않아?",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
