      "utterance": [
        {
          "id": "MDRW2100015540.1.1",
          "form": "여행가구싶다",
          "original_form": "여행가구싶다",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015540.1.2",
          "form": "기념품 사온 적 있어요?",
          "original_form": "기념품 사온 적 있어요?",
          "speaker_id": "1",
          "time": "20210826 07:28"
        },
        {
          "id": "MDRW2100015540.1.3",
          "form": "수학여행갔을때 주걱을 샀던기억이나요",
          "original_form": "수학여행갔을때 주걱을 샀던기억이나요",
          "speaker_id": "2",
          "time": "20210826 09:39"
        },
        {
          "id": "MDRW2100015540.1.4",
          "form": "그때가좋았는데..",
          "original_form": "그때가좋았는데..",
          "speaker_id": "2",
          "time": "20210826 09:40"
        },
        {
          "id": "MDRW2100015540.1.5",
          "form": "아 주걱 기념품 국룰이죠",
          "original_form": "아 주걱 기념품 국룰이죠",
          "speaker_id": "1",
          "time": "20210826 09:40"
        },
        {
          "id": "MDRW2100015540.1.6",
          "form": "엄마가 이런것좀 사오지말랫던 기억이 나네요",
          "original_form": "엄마가 이런것좀 사오지말랫던 기억이 나네요",
          "speaker_id": "1",
          "time": "20210826 09:40"
        },
        {
          "id": "MDRW2100015540.1.7",
          "form": "수학여행때 왜그렇게 주걱이 사고싶었는지",
          "original_form": "수학여행때 왜그렇게 주걱이 사고싶었는지",
          "speaker_id": "1",
          "time": "20210826 09:40"
        },
        {
          "id": "MDRW2100015540.1.8",
          "form": "ㄱㅋㅋㅋㅋㅋㅋ기쁘게받아주셧지만 기뻐보이지않앗어요",
          "original_form": "ㄱㅋㅋㅋㅋㅋㅋ기쁘게받아주셧지만 기뻐보이지않앗어요",
          "speaker_id": "2",
          "time": "20210826 09:40"
        },
        {
          "id": "MDRW2100015540.1.9",
          "form": "ㅋㄱㅋㅈㄱㅈㅋㅈㅋ",
          "original_form": "ㅋㄱㅋㅈㄱㅈㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210826 09:40"
        },
        {
          "id": "MDRW2100015540.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:40"
        },
        {
          "id": "MDRW2100015540.1.11",
          "form": "그나마거기서는주걱이뻣는데",
          "original_form": "그나마거기서는주걱이뻣는데",
          "speaker_id": "2",
          "time": "20210826 09:40"
        },
        {
          "id": "MDRW2100015540.1.12",
          "form": "수학여행 그립네요",
          "original_form": "수학여행 그립네요",
          "speaker_id": "1",
          "time": "20210826 09:41"
        },
        {
          "id": "MDRW2100015540.1.13",
          "form": "그러게요 그리워요 놀이기구도 타러가고싶어여어어",
          "original_form": "그러게요 그리워요 놀이기구도 타러가고싶어여어어",
          "speaker_id": "2",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100015540.1.14",
          "form": "사실 예전보다 잘탈자신은없지만",
          "original_form": "사실 예전보다 잘탈자신은없지만",
          "speaker_id": "2",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100015540.1.15",
          "form": "나도 겨우 범버카나 타는 쩌리입니다",
          "original_form": "나도 겨우 범버카나 타는 쩌리입니다",
          "speaker_id": "1",
          "time": "20210826 10:03"
        },
        {
          "id": "MDRW2100015540.1.16",
          "form": "재밌겠다범퍼카 \r\n근데이제타면삭신이쑤실거같아여",
          "original_form": "재밌겠다범퍼카 \r\n근데이제타면삭신이쑤실거같아여",
          "speaker_id": "2",
          "time": "20210826 10:05"
        },
        {
          "id": "MDRW2100015540.1.17",
          "form": "그럼 회전목마 타셔야겠군요",
          "original_form": "그럼 회전목마 타셔야겠군요",
          "speaker_id": "1",
          "time": "20210826 10:05"
        },
        {
          "id": "MDRW2100015540.1.18",
          "form": "개장하는 롯데월드 가서 조경만 보다올것 같은 사람 나야나",
          "original_form": "개장하는 롯데월드 가서 조경만 보다올것 같은 사람 나야나",
