      "utterance": [
        {
          "id": "MDRW2100003449.1.1",
          "form": "좋아하는 음식이 있나요?",
          "original_form": "좋아하는 음식이 있나요?",
          "speaker_id": "1",
          "time": "20210820 23:17"
        },
        {
          "id": "MDRW2100003449.1.2",
          "form": "저는 고기종류 좋아해요!",
          "original_form": "저는 고기종류 좋아해요!",
          "speaker_id": "2",
          "time": "20210820 23:22"
        },
        {
          "id": "MDRW2100003449.1.3",
          "form": "고기면 어떤거요? 돼지고기 소고기 닭고기?",
          "original_form": "고기면 어떤거요? 돼지고기 소고기 닭고기?",
          "speaker_id": "1",
          "time": "20210820 23:22"
        },
        {
          "id": "MDRW2100003449.1.4",
          "form": "고기라면 가리지 않고 다 좋아합니다",
          "original_form": "고기라면 가리지 않고 다 좋아합니다",
          "speaker_id": "2",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003449.1.5",
          "form": "하지만 돼지가 제일 좋기는해요",
          "original_form": "하지만 돼지가 제일 좋기는해요",
          "speaker_id": "2",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003449.1.6",
          "form": "저두요 삼겹살! 너무 맛있어요",
          "original_form": "저두요 삼겹살! 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003449.1.7",
          "form": "진짜 삼겹살은 매일먹어도 질리지가 않는 것 같아요ㅜㅜ",
          "original_form": "진짜 삼겹살은 매일먹어도 질리지가 않는 것 같아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 23:28"
        },
        {
          "id": "MDRW2100003449.1.8",
          "form": "삼겹살 먹을 때 같이 주로 뭐를 곁들여 드시나요?",
          "original_form": "삼겹살 먹을 때 같이 주로 뭐를 곁들여 드시나요?",
          "speaker_id": "1",
          "time": "20210820 23:29"
        },
        {
          "id": "MDRW2100003449.1.9",
          "form": "저는 마늘 파채 김치 등등 뭐랑 먹어도 너무 맛있더라구요",
          "original_form": "저는 마늘 파채 김치 등등 뭐랑 먹어도 너무 맛있더라구요",
          "speaker_id": "1",
          "time": "20210820 23:29"
        },
        {
          "id": "MDRW2100003449.1.10",
          "form": "일단 기본적으로 상추쌈을 먹어줘야해요ㅎㅎ",
          "original_form": "일단 기본적으로 상추쌈을 먹어줘야해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100003449.1.11",
          "form": "저는 굉장히 크게크게 쌈싸먹는 스타일이에요",
          "original_form": "저는 굉장히 크게크게 쌈싸먹는 스타일이에요",
          "speaker_id": "2",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003449.1.12",
          "form": "상추쌈 좋아요",
          "original_form": "상추쌈 좋아요",
          "speaker_id": "1",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003449.1.13",
          "form": "쌈장 넣어드시나요?",
          "original_form": "쌈장 넣어드시나요?",
          "speaker_id": "1",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003449.1.14",
          "form": "그럼요!  상추쌈에 쌈장은 필수에요",
          "original_form": "그럼요!  상추쌈에 쌈장은 필수에요",
          "speaker_id": "2",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100003449.1.15",
          "form": "그럼 마늘은 구운게 좋아요 생마늘이 좋아요? 전 구운거요",
          "original_form": "그럼 마늘은 구운게 좋아요 생마늘이 좋아요? 전 구운거요",
          "speaker_id": "1",
          "time": "20210820 23:38"
        },
        {
          "id": "MDRW2100003449.1.16",
          "form": "저는 사실 마늘은 생도 좋고 구운것도 좋은데 고기쌈에는 구운게 더 맛있는 거 같아요",
          "original_form": "저는 사실 마늘은 생도 좋고 구운것도 좋은데 고기쌈에는 구운게 더 맛있는 거 같아요",
          "speaker_id": "2",
          "time": "20210820 23:40"
        },
        {
          "id": "MDRW2100003449.1.17",
          "form": "맞아요 구운게 뭔가 좀 더 감칠맛이 난다랄까",
          "original_form": "맞아요 구운게 뭔가 좀 더 감칠맛이 난다랄까",
          "speaker_id": "1",
          "time": "20210820 23:41"
        },
        {
          "id": "MDRW2100003449.1.18",
          "form": "좀 더 맛있더라구요",
          "original_form": "좀 더 맛있더라구요",
          "speaker_id": "1",
          "time": "20210820 23:41"
        },
        {
          "id": "MDRW2100003449.1.19",
          "form": "삼겹살 말고 햄버거 좋아해요",
          "original_form": "삼겹살 말고 햄버거 좋아해요",
          "speaker_id": "1",
          "time": "20210820 23:42"
        }
