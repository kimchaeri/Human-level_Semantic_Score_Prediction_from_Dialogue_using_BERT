      "utterance": [
        {
          "id": "MDRW2100022088.1.1",
          "form": "아뇨 ㅋㅋㅋ 단모음키보드에요 이게 엄청 빨라요",
          "original_form": "아뇨 ㅋㅋㅋ 단모음키보드에요 이게 엄청 빨라요",
          "speaker_id": "2",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100022088.1.2",
          "form": "타이핑양이 엄청난데요",
          "original_form": "타이핑양이 엄청난데요",
          "speaker_id": "1",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100022088.1.3",
          "form": "그쳐 =ㅡ= 좀 많이 듣는 말.. ㅋㅋㅋ",
          "original_form": "그쳐 =ㅡ= 좀 많이 듣는 말.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100022088.1.4",
          "form": "쿼티 쓰세요 천지인쓰세요?",
          "original_form": "쿼티 쓰세요 천지인쓰세요?",
          "speaker_id": "2",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100022088.1.5",
          "form": "재미있는 성격이실것 같에요",
          "original_form": "재미있는 성격이실것 같에요",
          "speaker_id": "1",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100022088.1.6",
          "form": "천지인",
          "original_form": "천지인",
          "speaker_id": "1",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100022088.1.7",
          "form": "하핳....",
          "original_form": "하핳....",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100022088.1.8",
          "form": "음 커피는 어때요? 좋아하세요?",
          "original_form": "음 커피는 어때요? 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100022088.1.9",
          "form": "단모음키보드 써보세요 컴이랑 비슷해서 좋아요 ㅋㅋ",
          "original_form": "단모음키보드 써보세요 컴이랑 비슷해서 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100022088.1.10",
          "form": "아 커피 ㅠㅠ 장이 약해서 커피는 디카페인으로 먹거나 어쩌다 한잔씩만 해서 잘은 몰라요",
          "original_form": "아 커피 ㅠㅠ 장이 약해서 커피는 디카페인으로 먹거나 어쩌다 한잔씩만 해서 잘은 몰라요",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100022088.1.11",
          "form": "그저 모카커피가 맛있다 정도..ㅋㅋㅋㅋ",
          "original_form": "그저 모카커피가 맛있다 정도..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100022088.1.12",
          "form": "그럼 카퍼 잘 안가세요?",
          "original_form": "그럼 카퍼 잘 안가세요?",
          "speaker_id": "1",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100022088.1.13",
          "form": "카페",
          "original_form": "카페",
          "speaker_id": "1",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100022088.1.14",
          "form": "네 카페 가면 다른 음료 마셔요",
          "original_form": "네 카페 가면 다른 음료 마셔요",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100022088.1.15",
          "form": "주로 어떤거?",
          "original_form": "주로 어떤거?",
          "speaker_id": "1",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100022088.1.16",
          "form": "아 디카페인 커피 되면 가끔 아메리카노 먹긴 해요 +ㅅ+",
          "original_form": "아 디카페인 커피 되면 가끔 아메리카노 먹긴 해요 +ㅅ+",
          "speaker_id": "2",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100022088.1.17",
          "form": "어 자몽티라던가, 홍차류라던가",
          "original_form": "어 자몽티라던가, 홍차류라던가",
          "speaker_id": "2",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100022088.1.18",
          "form": "초코도 좋구여",
          "original_form": "초코도 좋구여",
          "speaker_id": "2",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100022088.1.19",
          "form": "아 그런것도 좋죠",
          "original_form": "아 그런것도 좋죠",
          "speaker_id": "1",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100022088.1.20",
          "form": "어릴땐 유자차 온리였어여 ㅋㅋ",
          "original_form": "어릴땐 유자차 온리였어여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100022088.1.21",
          "form": "유자차 짱맛있... 건더기도 다 건져먹고 ㅋㅋ",
          "original_form": "유자차 짱맛있... 건더기도 다 건져먹고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100022088.1.22",
          "form": "유자차 맛있죠",
          "original_form": "유자차 맛있죠",
          "speaker_id": "1",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100022088.1.23",
          "form": "유자청 듬뿍넣어서",
          "original_form": "유자청 듬뿍넣어서",
          "speaker_id": "1",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100022088.1.24",
          "form": "건더기 먹나요 안먹나여 =ㅡ=ㅋㅋ",
          "original_form": "건더기 먹나요 안먹나여 =ㅡ=ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100022088.1.25",
          "form": "달콤하고 새콤하고",
          "original_form": "달콤하고 새콤하고",
          "speaker_id": "1",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100022088.1.26",
          "form": "건더기도 다먹어요",
          "original_form": "건더기도 다먹어요",
          "speaker_id": "1",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100022088.1.27",
          "form": "오오오오오오",
          "original_form": "오오오오오오",
          "speaker_id": "2",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100022088.1.28",
          "form": "식감이 또 좋인서",
          "original_form": "식감이 또 좋인서",
          "speaker_id": "1",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100022088.1.29",
          "form": "안먹는 사람이 많아여",
          "original_form": "안먹는 사람이 많아여",
          "speaker_id": "2",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100022088.1.30",
          "form": "그쳐! 그리고 씹으면 그 껍질 향이 팍! 올라오잖아여",
          "original_form": "그쳐! 그리고 씹으면 그 껍질 향이 팍! 올라오잖아여",
          "speaker_id": "2",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100022088.1.31",
          "form": "근데 그거 왜 먹냐고 하는 애들 넘 많아서 ㅋㅋㅋ",
          "original_form": "근데 그거 왜 먹냐고 하는 애들 넘 많아서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100022088.1.32",
          "form": "유자차도 만들고 그랬는데 예전엔..=ㄱ=",
          "original_form": "유자차도 만들고 그랬는데 예전엔..=ㄱ=",
          "speaker_id": "2",
          "time": "20210829 02:43"
        },
        {
          "id": "MDRW2100022088.1.33",
          "form": "아근데 먹으면서 약간",
          "original_form": "아근데 먹으면서 약간",
          "speaker_id": "1",
          "time": "20210829 02:44"
        },
        {
          "id": "MDRW2100022088.1.34",
          "form": "이거 농약 깨끗이 씻었을까하는 이런 깨름찍함이",
          "original_form": "이거 농약 깨끗이 씻었을까하는 이런 깨름찍함이",
          "speaker_id": "1",
          "time": "20210829 02:44"
        },
        {
          "id": "MDRW2100022088.1.35",
          "form": "조금 있었어요",
          "original_form": "조금 있었어요",
          "speaker_id": "1",
          "time": "20210829 02:44"
        },
        {
          "id": "MDRW2100022088.1.36",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:44"
        },
        {
          "id": "MDRW2100022088.1.37",
          "form": "괜찮아요.. 안죽어요.. 응응..",
          "original_form": "괜찮아요.. 안죽어요.. 응응..",
