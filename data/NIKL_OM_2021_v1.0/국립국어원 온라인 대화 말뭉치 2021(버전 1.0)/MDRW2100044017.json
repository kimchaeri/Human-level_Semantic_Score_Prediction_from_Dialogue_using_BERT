      "utterance": [
        {
          "id": "MDRW2100044017.1.1",
          "form": "네네! 유투브요!ㅋㅋㅋ",
          "original_form": "네네! 유투브요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044017.1.2",
          "form": "아 몇개 뜨긴하는데",
          "original_form": "아 몇개 뜨긴하는데",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044017.1.3",
          "form": "제가 좋아하는 아이돌 아니면 잘 안봐요 ㅋㅋㅋㅋ",
          "original_form": "제가 좋아하는 아이돌 아니면 잘 안봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044017.1.4",
          "form": "누구거 본적은 잇는거같은데 기억이..",
          "original_form": "누구거 본적은 잇는거같은데 기억이..",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044017.1.5",
          "form": "ㅋㅋㅋㅋ오 어떤아이돌좋아하세여",
          "original_form": "ㅋㅋㅋㅋ오 어떤아이돌좋아하세여",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044017.1.6",
          "form": "어떤 남돌 자컨이 재밌다고 본거같던데",
          "original_form": "어떤 남돌 자컨이 재밌다고 본거같던데",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044017.1.7",
          "form": "저 스테이시도 좋아하구",
          "original_form": "저 스테이시도 좋아하구",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044017.1.8",
          "form": "여돌 걍 다좋아욬ㅋㅋㅋㅋ",
          "original_form": "여돌 걍 다좋아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044017.1.9",
          "form": "오 스테이시 어젠가 ㅇ앨범나왓던데",
          "original_form": "오 스테이시 어젠가 ㅇ앨범나왓던데",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044017.1.10",
          "form": "맞아요! 색안경",
          "original_form": "맞아요! 색안경",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044017.1.11",
          "form": "ㅋㅋㅋㅋㅋ사실 아직 무대는 안봣지만 ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ사실 아직 무대는 안봣지만 ㅎ",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044017.1.12",
          "form": "생각난김에 들어봐야겠네요 저도",
          "original_form": "생각난김에 들어봐야겠네요 저도",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044017.1.13",
          "form": "너무 귀여워요 친구들 ㅠㅍ",
          "original_form": "너무 귀여워요 친구들 ㅠㅍ",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044017.1.14",
          "form": "asap는 진짜잘들었거든요 ㅋㅋㅋㅋㅋ",
          "original_form": "asap는 진짜잘들었거든요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044017.1.15",
          "form": "~~",
          "original_form": "~~",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044017.1.16",
          "form": "에쎕~~",
          "original_form": "에쎕~~",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044017.1.17",
          "form": "저는 쏘배드 데뷔때부터 좋아랫어욬ㅋㅋㅋ",
          "original_form": "저는 쏘배드 데뷔때부터 좋아랫어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044017.1.18",
          "form": "애들이 벌써 완성형이야 어쩜..",
          "original_form": "애들이 벌써 완성형이야 어쩜..",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044017.1.19",
          "form": "대박ㅋㅋㅋ아니 어뜨케 데뷔땨부터 알아보셨나요",
          "original_form": "대박ㅋㅋㅋ아니 어뜨케 데뷔땨부터 알아보셨나요",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044017.1.20",
          "form": "아니 너무",
          "original_form": "아니 너무",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044017.1.21",
          "form": "완벽한 아이돌상 ㅠㅠ",
          "original_form": "완벽한 아이돌상 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044017.1.22",
          "form": "노래도 좋고",
          "original_form": "노래도 좋고",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044017.1.23",
          "form": "ㅋㅋㅋ색안경 듣는중인데 다들으면 쏘배드도 들어볼게요",
          "original_form": "ㅋㅋㅋ색안경 듣는중인데 다들으면 쏘배드도 들어볼게요",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044017.1.24",
          "form": "수록곡에 잔잔? 발라드?도 있나요?",
          "original_form": "수록곡에 잔잔? 발라드?도 있나요?",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044017.1.25",
          "form": "발라드...",
          "original_form": "발라드...",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044017.1.26",
          "form": "머드라 그",
          "original_form": "머드라 그",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044017.1.27",
          "form": "사랑은 원래 이렇게 아픈건가요였나",
          "original_form": "사랑은 원래 이렇게 아픈건가요였나",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044017.1.28",
          "form": "그거 찐 짝사랑하는 소녀느낌ㅇㅣ라 귀엽고 ㅋㅋㅋㅋ",
          "original_form": "그거 찐 짝사랑하는 소녀느낌ㅇㅣ라 귀엽고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044017.1.29",
          "form": "댄스느낌은 아닌데 노래좋아요 들어보세여 ㅌㅋㅋ",
