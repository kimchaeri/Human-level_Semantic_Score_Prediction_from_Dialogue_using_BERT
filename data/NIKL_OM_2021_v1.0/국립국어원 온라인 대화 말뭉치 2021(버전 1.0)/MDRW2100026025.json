      "utterance": [
        {
          "id": "MDRW2100026025.1.1",
          "form": "왜 내가 멀쩡한 회사를 때려쳤나!!! ㅋㅋ",
          "original_form": "왜 내가 멀쩡한 회사를 때려쳤나!!! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:56"
        },
        {
          "id": "MDRW2100026025.1.2",
          "form": "재택하시나요??",
          "original_form": "재택하시나요??",
          "speaker_id": "1",
          "time": "20210901 18:56"
        },
        {
          "id": "MDRW2100026025.1.3",
          "form": "캐나다 가시느냐고 퇴사하셨겠네요",
          "original_form": "캐나다 가시느냐고 퇴사하셨겠네요",
          "speaker_id": "2",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100026025.1.4",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100026025.1.5",
          "form": "네 겸사겸사 3년 넘게 다니던 회사라 권태기와서ㅎㅎ",
          "original_form": "네 겸사겸사 3년 넘게 다니던 회사라 권태기와서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100026025.1.6",
          "form": "오 딱 좋은 타이임이였네요",
          "original_form": "오 딱 좋은 타이임이였네요",
          "speaker_id": "2",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100026025.1.7",
          "form": "저 아는사람도 내년에 퇴사하고 영국워킹가요",
          "original_form": "저 아는사람도 내년에 퇴사하고 영국워킹가요",
          "speaker_id": "2",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100026025.1.8",
          "form": "오 다행히 비자가 나왔네요! 기회가 되는 경우 많은거같아요",
          "original_form": "오 다행히 비자가 나왔네요! 기회가 되는 경우 많은거같아요",
          "speaker_id": "1",
          "time": "20210901 19:14"
        },
        {
          "id": "MDRW2100026025.1.9",
          "form": "같이갔던 친구는 거기서 정착하고 살아요",
          "original_form": "같이갔던 친구는 거기서 정착하고 살아요",
          "speaker_id": "1",
          "time": "20210901 19:14"
        },
        {
          "id": "MDRW2100026025.1.10",
          "form": "그러니깐요",
          "original_form": "그러니깐요",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100026025.1.11",
          "form": "와대박",
          "original_form": "와대박",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100026025.1.12",
          "form": "제친구도 그 케이스에요",
          "original_form": "제친구도 그 케이스에요",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100026025.1.13",
          "form": "남친을 거기서 만나서ㅋㅋ 외로우니까 되더라고요ㅎㅎ",
          "original_form": "남친을 거기서 만나서ㅋㅋ 외로우니까 되더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026025.1.14",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026025.1.15",
          "form": "같은...",
          "original_form": "같은...",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026025.1.16",
          "form": "제친구도요",
          "original_form": "제친구도요",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026025.1.17",
          "form": "역시 인생사 뭐있나요",
          "original_form": "역시 인생사 뭐있나요",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026025.1.18",
          "form": "ㅋㅋ 아 진짜ㅋㅋ 다 비슷한 루트네요! ㅋㅋ 그래도 행복하게 잘사니까요ㅎㅎ",
          "original_form": "ㅋㅋ 아 진짜ㅋㅋ 다 비슷한 루트네요! ㅋㅋ 그래도 행복하게 잘사니까요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026025.1.19",
          "form": "맞아요 부럽네요",
          "original_form": "맞아요 부럽네요",
          "speaker_id": "2",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026025.1.20",
          "form": "둘이 가서 사니깐 좋은점도많은것같더라고요",
          "original_form": "둘이 가서 사니깐 좋은점도많은것같더라고요",
          "speaker_id": "2",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100026025.1.21",
          "form": "나중에 퇴사하고 갈곳없음 받아달라고ㅋㅋ 그랬네요ㅋㅋ",
          "original_form": "나중에 퇴사하고 갈곳없음 받아달라고ㅋㅋ 그랬네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026025.1.22",
          "form": "재택도 하시나요?",
          "original_form": "재택도 하시나요?",
          "speaker_id": "1",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026025.1.23",
          "form": "아니요. 저는 여지껏 재택근무한적이없어요",
          "original_form": "아니요. 저는 여지껏 재택근무한적이없어요",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100026025.1.24",
          "form": "재택해보셨어요?",
          "original_form": "재택해보셨어요?",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100026025.1.25",
          "form": "주1~2회정도 불규칙적으로 해요",
          "original_form": "주1~2회정도 불규칙적으로 해요",
          "speaker_id": "1",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100026025.1.26",
          "form": "출근하시나요?ㅜㅠ",
          "original_form": "출근하시나요?ㅜㅠ",
          "speaker_id": "1",
          "time": "20210907 07:42"
        }
      ]
    }
