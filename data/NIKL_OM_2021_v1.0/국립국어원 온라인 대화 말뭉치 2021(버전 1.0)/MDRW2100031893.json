        {
          "id": "MDRW2100031893.1.1",
          "form": "아침 드셨나요",
          "original_form": "아침 드셨나요",
          "speaker_id": "1",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100031893.1.2",
          "form": "네 먹었습니다",
          "original_form": "네 먹었습니다",
          "speaker_id": "2",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031893.1.3",
          "form": "뭐 드셨나요",
          "original_form": "뭐 드셨나요",
          "speaker_id": "1",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100031893.1.4",
          "form": "무난하게 햄 구워서 먹었어요",
          "original_form": "무난하게 햄 구워서 먹었어요",
          "speaker_id": "2",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031893.1.5",
          "form": "스팸이요?",
          "original_form": "스팸이요?",
          "speaker_id": "1",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100031893.1.6",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100031893.1.7",
          "form": "리챔이요...",
          "original_form": "리챔이요...",
          "speaker_id": "2",
          "time": "20210905 14:52"
        },
        {
          "id": "MDRW2100031893.1.8",
          "form": "그렇군요 스팸/리챔 맛이 다른가요? 자주 안먹어봐서...",
          "original_form": "그렇군요 스팸/리챔 맛이 다른가요? 자주 안먹어봐서...",
          "speaker_id": "1",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100031893.1.9",
          "form": "사실",
          "original_form": "사실",
          "speaker_id": "2",
          "time": "20210905 16:26"
        },
        {
          "id": "MDRW2100031893.1.10",
          "form": "크게 다르다고 느끼지는 않는데",
          "original_form": "크게 다르다고 느끼지는 않는데",
          "speaker_id": "2",
          "time": "20210905 16:26"
        },
        {
          "id": "MDRW2100031893.1.11",
          "form": "그렇군요 점심은 뭐드셨나요",
          "original_form": "그렇군요 점심은 뭐드셨나요",
          "speaker_id": "1",
          "time": "20210905 16:46"
        },
        {
          "id": "MDRW2100031893.1.12",
          "form": "초밥 먹었습니다",
          "original_form": "초밥 먹었습니다",
          "speaker_id": "2",
          "time": "20210905 17:52"
        },
        {
          "id": "MDRW2100031893.1.13",
          "form": "맛있는거 드셨네요 저도 초밥 좋아하는데.~",
          "original_form": "맛있는거 드셨네요 저도 초밥 좋아하는데.~",
          "speaker_id": "1",
          "time": "20210905 18:50"
        },
        {
          "id": "MDRW2100031893.1.14",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "2",
          "time": "20210906 08:29"
        },
        {
          "id": "MDRW2100031893.1.15",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "2",
          "time": "20210906 08:29"
        },
        {
          "id": "MDRW2100031893.1.16",
          "form": "오늘 아침은 드셨나요",
          "original_form": "오늘 아침은 드셨나요",
          "speaker_id": "2",
