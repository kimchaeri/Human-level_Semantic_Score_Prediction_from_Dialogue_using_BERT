      "utterance": [
        {
          "id": "MDRW2100034625.1.1",
          "form": "최근 쇼핑하셨나요?",
          "original_form": "최근 쇼핑하셨나요?",
          "speaker_id": "1",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100034625.1.2",
          "form": "네 인터넷쇼핑했어요",
          "original_form": "네 인터넷쇼핑했어요",
          "speaker_id": "2",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100034625.1.3",
          "form": "뭐사셨어요?",
          "original_form": "뭐사셨어요?",
          "speaker_id": "1",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100034625.1.4",
          "form": "먹거리랑 생필품들이요",
          "original_form": "먹거리랑 생필품들이요",
          "speaker_id": "2",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100034625.1.5",
          "form": "아 저두요",
          "original_form": "아 저두요",
          "speaker_id": "1",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100034625.1.6",
          "form": "생필품 뭐사셨나요?",
          "original_form": "생필품 뭐사셨나요?",
          "speaker_id": "1",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100034625.1.7",
          "form": "마스크랑 소독티슈랑 샴푸",
          "original_form": "마스크랑 소독티슈랑 샴푸",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100034625.1.8",
          "form": "베이킹소다",
          "original_form": "베이킹소다",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100034625.1.9",
          "form": "이것저것 사셨네요!",
          "original_form": "이것저것 사셨네요!",
          "speaker_id": "1",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100034625.1.10",
          "form": "넹ㅎㅎ",
          "original_form": "넹ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100034625.1.11",
          "form": "생필품은 쟁여두면 든든하죠",
          "original_form": "생필품은 쟁여두면 든든하죠",
          "speaker_id": "1",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100034625.1.12",
          "form": "맞아용",
          "original_form": "맞아용",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100034625.1.13",
          "form": "보통 어느몰에서 구입하세요?",
          "original_form": "보통 어느몰에서 구입하세요?",
          "speaker_id": "1",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100034625.1.14",
          "form": "그때그때 다양해요",
          "original_form": "그때그때 다양해요",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100034625.1.15",
          "form": "옥션 지마켓",
          "original_form": "옥션 지마켓",
          "speaker_id": "2",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100034625.1.16",
          "form": "이마트",
          "original_form": "이마트",
          "speaker_id": "2",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100034625.1.17",
          "form": "저두요 ㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100034625.1.18",
          "form": "이마트몰도 좋더라구요!",
          "original_form": "이마트몰도 좋더라구요!",
          "speaker_id": "1",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100034625.1.19",
          "form": "맞아요 쓱배송 편해요",
          "original_form": "맞아요 쓱배송 편해요",
          "speaker_id": "2",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100034625.1.20",
          "form": "신세계가 계속 커지는 이유가 있는 듯요",
          "original_form": "신세계가 계속 커지는 이유가 있는 듯요",
          "speaker_id": "2",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034625.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034625.1.22",
          "form": "정말 빨리오더라구요",
          "original_form": "정말 빨리오더라구요",
          "speaker_id": "1",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034625.1.23",
          "form": "요새 새벽배송도 엄청 빨라요",
          "original_form": "요새 새벽배송도 엄청 빨라요",
