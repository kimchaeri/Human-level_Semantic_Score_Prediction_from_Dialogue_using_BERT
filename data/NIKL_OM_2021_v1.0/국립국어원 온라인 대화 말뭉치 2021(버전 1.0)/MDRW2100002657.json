      "utterance": [
        {
          "id": "MDRW2100002657.1.1",
          "form": "최근에 책같은거 읽은거 있어?",
          "original_form": "최근에 책같은거 읽은거 있어?",
          "speaker_id": "1",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002657.1.2",
          "form": "최근에 책은 요근래에 잘 안읽고 있고",
          "original_form": "최근에 책은 요근래에 잘 안읽고 있고",
          "speaker_id": "2",
          "time": "20210820 14:26"
        },
        {
          "id": "MDRW2100002657.1.3",
          "form": "학생땐 엄청 읽었긴해",
          "original_form": "학생땐 엄청 읽었긴해",
          "speaker_id": "2",
          "time": "20210820 14:26"
        },
        {
          "id": "MDRW2100002657.1.4",
          "form": "다독상을 초~고 계속 탔으니",
          "original_form": "다독상을 초~고 계속 탔으니",
          "speaker_id": "2",
          "time": "20210820 14:26"
        },
        {
          "id": "MDRW2100002657.1.5",
          "form": "나도 고딩때까지는 책을 읽었는데 졸업하고 나서 안읽게 되더라",
          "original_form": "나도 고딩때까지는 책을 읽었는데 졸업하고 나서 안읽게 되더라",
          "speaker_id": "1",
          "time": "20210820 14:27"
        },
        {
          "id": "MDRW2100002657.1.6",
          "form": "맞아 나도",
          "original_form": "맞아 나도",
          "speaker_id": "2",
          "time": "20210820 14:27"
        },
        {
          "id": "MDRW2100002657.1.7",
          "form": "졸업하고나서 거의 안하게 되더라고",
          "original_form": "졸업하고나서 거의 안하게 되더라고",
          "speaker_id": "2",
          "time": "20210820 14:27"
        },
        {
          "id": "MDRW2100002657.1.8",
          "form": "그래서 최근에 메이즈러너 쭈욱 읽고 있어",
          "original_form": "그래서 최근에 메이즈러너 쭈욱 읽고 있어",
          "speaker_id": "1",
          "time": "20210820 14:27"
        },
        {
          "id": "MDRW2100002657.1.9",
          "form": "메이즈러너 엄청 재밌지!",
          "original_form": "메이즈러너 엄청 재밌지!",
          "speaker_id": "2",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002657.1.10",
          "form": "고등학교때 읽어서 기억난다",
          "original_form": "고등학교때 읽어서 기억난다",
          "speaker_id": "2",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002657.1.11",
          "form": "집에 해리포터도 있어서 다시 읽어보고 있는중이고",
          "original_form": "집에 해리포터도 있어서 다시 읽어보고 있는중이고",
          "speaker_id": "1",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002657.1.12",
          "form": "너도 전에 읽었던 책같은거 다시 읽는거 어때?",
          "original_form": "너도 전에 읽었던 책같은거 다시 읽는거 어때?",
          "speaker_id": "1",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002657.1.13",
          "form": "그래야겠어",
          "original_form": "그래야겠어",
          "speaker_id": "2",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002657.1.14",
          "form": "다른 분 소설도 사보는게 좋을거같기도 하고",
          "original_form": "다른 분 소설도 사보는게 좋을거같기도 하고",
          "speaker_id": "2",
          "time": "20210820 14:30"
        },
        {
          "id": "MDRW2100002657.1.15",
          "form": "혹시 재밌는거 발견하면 알려줘",
          "original_form": "혹시 재밌는거 발견하면 알려줘",
          "speaker_id": "1",
          "time": "20210820 14:30"
        },
        {
          "id": "MDRW2100002657.1.16",
          "form": "오케이 알았어~!",
          "original_form": "오케이 알았어~!",
          "speaker_id": "2",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002657.1.17",
          "form": "맞다 책을 사서 보기 힘들면 전자책도 있어!",
          "original_form": "맞다 책을 사서 보기 힘들면 전자책도 있어!",
          "speaker_id": "1",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002657.1.18",
          "form": "맞다 전자책",
          "original_form": "맞다 전자책",
          "speaker_id": "2",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002657.1.19",
          "form": "전자책도 있었지~",
          "original_form": "전자책도 있었지~",
          "speaker_id": "2",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002657.1.20",
          "form": "종이책은 무겁고 들고다니기도 좀 힘든데",
          "original_form": "종이책은 무겁고 들고다니기도 좀 힘든데",
          "speaker_id": "1",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002657.1.21",
          "form": "전자책이 있으니까 편하더라",
          "original_form": "전자책이 있으니까 편하더라",
          "speaker_id": "1",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002657.1.22",
          "form": "그래도 그나마 읽을수 있는편?",
          "original_form": "그래도 그나마 읽을수 있는편?",
          "speaker_id": "1",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002657.1.23",
          "form": "안그러면 진짜 안읽을것 같아",
          "original_form": "안그러면 진짜 안읽을것 같아",
          "speaker_id": "1",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002657.1.24",
          "form": "전자책은 뭐랄까",
          "original_form": "전자책은 뭐랄까",
          "speaker_id": "2",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002657.1.25",
          "form": "편하긴한데 눈이아픈?",
          "original_form": "편하긴한데 눈이아픈?",
          "speaker_id": "2",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002657.1.26",
          "form": "그래도 편한건 전자책이긴하지!",
          "original_form": "그래도 편한건 전자책이긴하지!",
          "speaker_id": "2",
          "time": "20210820 14:34"
        }
      ]
    }
  ]
}
