      "utterance": [
        {
          "id": "MDRW2100004546.1.1",
          "form": "운동",
          "original_form": "운동",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004546.1.2",
          "form": "운동하죠!",
          "original_form": "운동하죠!",
          "speaker_id": "2",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004546.1.3",
          "form": "짐볼로 운동했네요!",
          "original_form": "짐볼로 운동했네요!",
          "speaker_id": "2",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004546.1.4",
          "form": "힘들지 않을까요?",
          "original_form": "힘들지 않을까요?",
          "speaker_id": "1",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004546.1.5",
          "form": "균형이랑 밸런스 잡으면서 하다보니",
          "original_form": "균형이랑 밸런스 잡으면서 하다보니",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004546.1.6",
          "form": "조금은 힘들긴하죠",
          "original_form": "조금은 힘들긴하죠",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004546.1.7",
          "form": "그런데 운동 효과는 엄청나는거 같아요 ㅎㅎ",
          "original_form": "그런데 운동 효과는 엄청나는거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004546.1.8",
          "form": "전 홈트해요",
          "original_form": "전 홈트해요",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004546.1.9",
          "form": "홈트! 저도 홈트해요!",
          "original_form": "홈트! 저도 홈트해요!",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004546.1.10",
          "form": "플랭크해요",
          "original_form": "플랭크해요",
          "speaker_id": "1",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004546.1.11",
          "form": "플랭크 힘들지 않으세요?ㅠㅠ",
          "original_form": "플랭크 힘들지 않으세요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004546.1.12",
          "form": "저는 1분도 못버티던데..",
          "original_form": "저는 1분도 못버티던데..",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004546.1.13",
          "form": "최근에 그래도 많이해서 1분 조금은 버티는거같아요",
          "original_form": "최근에 그래도 많이해서 1분 조금은 버티는거같아요",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004546.1.14",
          "form": "네 저도 하니까 늘어요",
          "original_form": "네 저도 하니까 늘어요",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004546.1.15",
          "form": "처음엔 30초 밖에 못하다가 하면 할수록",
          "original_form": "처음엔 30초 밖에 못하다가 하면 할수록",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004546.1.16",
          "form": "버티는 시간이 늘어나는게 신기해요!",
          "original_form": "버티는 시간이 늘어나는게 신기해요!",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004546.1.17",
          "form": "몸이 적응하는 듯",
          "original_form": "몸이 적응하는 듯",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004546.1.18",
          "form": "맞아요! 적응한다는게 참 놀라워요!",
          "original_form": "맞아요! 적응한다는게 참 놀라워요!",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004546.1.19",
          "form": "요가는 해보실 생각 있으신가요,",
          "original_form": "요가는 해보실 생각 있으신가요,",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004546.1.20",
          "form": "몸이 안예뻐서 ㅎㅎ",
          "original_form": "몸이 안예뻐서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:12"
        }
      ]
