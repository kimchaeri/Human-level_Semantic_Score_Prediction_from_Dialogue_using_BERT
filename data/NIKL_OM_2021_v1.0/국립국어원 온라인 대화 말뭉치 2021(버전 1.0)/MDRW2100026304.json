      "utterance": [
        {
          "id": "MDRW2100026304.1.1",
          "form": "읽고 있는 책 있으세요?",
          "original_form": "읽고 있는 책 있으세요?",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026304.1.2",
          "form": "짬이 안나서 제대로 못 읽고 있어요",
          "original_form": "짬이 안나서 제대로 못 읽고 있어요",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026304.1.3",
          "form": "요즘 책 읽으시나요?",
          "original_form": "요즘 책 읽으시나요?",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026304.1.4",
          "form": "네 실용서 위주로 읽고 있어요 필요한 부분만 보면되서 좋아요",
          "original_form": "네 실용서 위주로 읽고 있어요 필요한 부분만 보면되서 좋아요",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026304.1.5",
          "form": "오! 어떤 테마의 실용서인가요??",
          "original_form": "오! 어떤 테마의 실용서인가요??",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026304.1.6",
          "form": "귀건강이랑 제로에이스트 그리고 미술치료요",
          "original_form": "귀건강이랑 제로에이스트 그리고 미술치료요",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026304.1.7",
          "form": "제로웨이스트 하시는군요!",
          "original_form": "제로웨이스트 하시는군요!",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026304.1.8",
          "form": "저도 환경책 읽고 제로웨이스트 실천중이에요 ㅎㅎ",
          "original_form": "저도 환경책 읽고 제로웨이스트 실천중이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026304.1.9",
          "form": "전 로우웨이스트 같아요 부족해서 이번에 책 대출했어요",
          "original_form": "전 로우웨이스트 같아요 부족해서 이번에 책 대출했어요",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026304.1.10",
          "form": "시작하신게 멋진걸요!",
          "original_form": "시작하신게 멋진걸요!",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026304.1.11",
          "form": "아 그럼 샐러드는 가게에서 담아 오시나요??",
          "original_form": "아 그럼 샐러드는 가게에서 담아 오시나요??",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026304.1.12",
          "form": "아뇨 비닐이랑 포크 티슈만 받지 않는 정도에요",
          "original_form": "아뇨 비닐이랑 포크 티슈만 받지 않는 정도에요",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026304.1.13",
          "form": "책 읽고 더 잘 해볼려고요",
          "original_form": "책 읽고 더 잘 해볼려고요",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026304.1.14",
          "form": "참 어떤 환경책 읽으셨나요?",
          "original_form": "참 어떤 환경책 읽으셨나요?",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026304.1.15",
          "form": "두 번째 지구는 없다",
          "original_form": "두 번째 지구는 없다",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026304.1.16",
          "form": "라는 책이었어요!",
          "original_form": "라는 책이었어요!",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026304.1.17",
          "form": "기회가 된다면 한번 읽어보시길 추천드립니다 ㅎㅎ",
          "original_form": "기회가 된다면 한번 읽어보시길 추천드립니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026304.1.18",
          "form": "오 들어본 책 같아요 찜해놔야 겠어요",
          "original_form": "오 들어본 책 같아요 찜해놔야 겠어요",
          "speaker_id": "1",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026304.1.19",
          "form": "이번 책 읽고 도서관 가서 대출해야겠어요",
          "original_form": "이번 책 읽고 도서관 가서 대출해야겠어요",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026304.1.20",
          "form": "읽기 편하고 괜찮은 책이었어요!",
          "original_form": "읽기 편하고 괜찮은 책이었어요!",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026304.1.21",
          "form": "오 더 기대되는 책이네요",
          "original_form": "오 더 기대되는 책이네요",
          "speaker_id": "1",
          "time": "20210901 22:03"
        }
      ]
