      "utterance": [
        {
          "id": "MDRW2100016045.1.1",
          "form": "안녕하세요\n아침에 식사는 하셨나요?",
          "original_form": "안녕하세요\n아침에 식사는 하셨나요?",
          "speaker_id": "1",
          "time": "20210826 08:45"
        },
        {
          "id": "MDRW2100016045.1.2",
          "form": "아뇨 ㅠㅠ 아침은 원래 안챙겨먹어요!",
          "original_form": "아뇨 ㅠㅠ 아침은 원래 안챙겨먹어요!",
          "speaker_id": "2",
          "time": "20210826 08:46"
        },
        {
          "id": "MDRW2100016045.1.3",
          "form": "여행이가고싶네요",
          "original_form": "여행이가고싶네요",
          "speaker_id": "1",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016045.1.4",
          "form": "어디 가고 싶은곳 있으세요?",
          "original_form": "어디 가고 싶은곳 있으세요?",
          "speaker_id": "1",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016045.1.5",
          "form": "전 코로나 끝나면 ㅠㅠ 온천여행가고싶어요",
          "original_form": "전 코로나 끝나면 ㅠㅠ 온천여행가고싶어요",
          "speaker_id": "2",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016045.1.6",
          "form": "아 온천이요 좋죠\n지금은 가기가 더힘든곳이네요",
          "original_form": "아 온천이요 좋죠\n지금은 가기가 더힘든곳이네요",
          "speaker_id": "1",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016045.1.7",
          "form": "지역은 어디로?",
          "original_form": "지역은 어디로?",
          "speaker_id": "1",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016045.1.8",
          "form": "국내도 좋고 해외도 가보고 싶어요 ㅠㅠ",
          "original_form": "국내도 좋고 해외도 가보고 싶어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 08:52"
        },
        {
          "id": "MDRW2100016045.1.9",
          "form": "해외로 갈거면 대만이랑 일본에 가보고 싶고...",
          "original_form": "해외로 갈거면 대만이랑 일본에 가보고 싶고...",
          "speaker_id": "2",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016045.1.10",
          "form": "국내는 온양온천쪽말고는 잘 모르겠네요 ㅜㅜ",
          "original_form": "국내는 온양온천쪽말고는 잘 모르겠네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016045.1.11",
          "form": "아..대만...가고싶네요",
          "original_form": "아..대만...가고싶네요",
          "speaker_id": "1",
          "time": "20210826 08:56"
        },
        {
          "id": "MDRW2100016045.1.12",
          "form": "최근에본 상견니 드라마 너무 잼났거든요",
          "original_form": "최근에본 상견니 드라마 너무 잼났거든요",
          "speaker_id": "1",
          "time": "20210826 08:56"
        },
        {
          "id": "MDRW2100016045.1.13",
          "form": "여행계획 짤때가 제일 행복한데..언제짤수있을지",
          "original_form": "여행계획 짤때가 제일 행복한데..언제짤수있을지",
          "speaker_id": "1",
          "time": "20210826 08:56"
        },
        {
          "id": "MDRW2100016045.1.14",
          "form": "ㅠㅠ 그러니까요",
          "original_form": "ㅠㅠ 그러니까요",
          "speaker_id": "2",
          "time": "20210826 08:57"
        },
        {
          "id": "MDRW2100016045.1.15",
          "form": "하와이.. 스페인.. 너무가고싶네요",
          "original_form": "하와이.. 스페인.. 너무가고싶네요",
          "speaker_id": "1",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100016045.1.16",
          "form": "스페인 박서준이 너무멋있었어요",
          "original_form": "스페인 박서준이 너무멋있었어요",
          "speaker_id": "1",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100016045.1.17",
          "form": "스페인에는 어떤 볼거리가 있나요??",
          "original_form": "스페인에는 어떤 볼거리가 있나요??",
          "speaker_id": "2",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100016045.1.18",
          "form": "동네가 너무이뻤어요 해변이랑..",
          "original_form": "동네가 너무이뻤어요 해변이랑..",
          "speaker_id": "1",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100016045.1.19",
          "form": "멋져보여서요",
          "original_form": "멋져보여서요",
          "speaker_id": "1",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100016045.1.20",
          "form": "오 ㅠㅠㅠ 그런거 좋죠",
          "original_form": "오 ㅠㅠㅠ 그런거 좋죠",
          "speaker_id": "2",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100016045.1.21",
          "form": "사진찍으면 멋있을 것 같은 곳들...",
          "original_form": "사진찍으면 멋있을 것 같은 곳들...",
          "speaker_id": "2",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100016045.1.22",
          "form": "네 한국에서는 볼수없는장면들이요",
          "original_form": "네 한국에서는 볼수없는장면들이요",
          "speaker_id": "1",
          "time": "20210826 12:07"
        }
      ]
    }
