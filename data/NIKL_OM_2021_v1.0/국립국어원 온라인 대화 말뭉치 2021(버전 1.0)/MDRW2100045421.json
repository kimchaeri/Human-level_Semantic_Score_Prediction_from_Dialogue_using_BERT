      "utterance": [
        {
          "id": "MDRW2100045421.1.1",
          "form": "눈이 낮은데 취향이???",
          "original_form": "눈이 낮은데 취향이???",
          "speaker_id": "2",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045421.1.2",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "1",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045421.1.3",
          "form": "아니 친구요",
          "original_form": "아니 친구요",
          "speaker_id": "2",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045421.1.4",
          "form": "굳이 자기 스펙에 안만나도 되는\r\n그런 남자들을 만나고 헤어지고",
          "original_form": "굳이 자기 스펙에 안만나도 되는\r\n그런 남자들을 만나고 헤어지고",
          "speaker_id": "2",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045421.1.5",
          "form": "아 친구분이요",
          "original_form": "아 친구분이요",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045421.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045421.1.7",
          "form": "으",
          "original_form": "으",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045421.1.8",
          "form": "한국에서 그러기 쉬운 것 같아요. 워낙 주변에서 무언의 압박이 있으니",
          "original_form": "한국에서 그러기 쉬운 것 같아요. 워낙 주변에서 무언의 압박이 있으니",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045421.1.9",
          "form": "작년 목표가 결혼",
          "original_form": "작년 목표가 결혼",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045421.1.10",
          "form": "남친없는데ㅋㅋ",
          "original_form": "남친없는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045421.1.11",
          "form": "그리고 수많은 소개팅",
          "original_form": "그리고 수많은 소개팅",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045421.1.12",
          "form": "사회적 정상성이라는 게 몹시 강력한 거라 ..",
          "original_form": "사회적 정상성이라는 게 몹시 강력한 거라 ..",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045421.1.13",
          "form": "아 ..",
          "original_form": "아 ..",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045421.1.14",
          "form": "제 친구는 파혼을 2번 했는데",
          "original_form": "제 친구는 파혼을 2번 했는데",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045421.1.15",
          "form": "....",
          "original_form": "....",
          "speaker_id": "2",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045421.1.16",
          "form": "다른 친구예요 ㅎㅎ",
          "original_form": "다른 친구예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045421.1.17",
          "form": "같은 남잔아니죠?",
          "original_form": "같은 남잔아니죠?",
          "speaker_id": "2",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045421.1.18",
          "form": "네. 다른 사람이예요",
          "original_form": "네. 다른 사람이예요",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045421.1.19",
          "form": "ㅋㅋ제친구는 법원까지...",
          "original_form": "ㅋㅋ제친구는 법원까지...",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045421.1.20",
          "form": "두 번 째 사람 만나기 전에 제가 그 친구를 알게 됐는데",
          "original_form": "두 번 째 사람 만나기 전에 제가 그 친구를 알게 됐는데",
          "speaker_id": "1",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045421.1.21",
          "form": "아오 빡치는 일이 많네요",
          "original_form": "아오 빡치는 일이 많네요",
          "speaker_id": "1",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045421.1.22",
          "form": "둘 다 바람 피운 게 드러나서 파혼 했어요.",
          "original_form": "둘 다 바람 피운 게 드러나서 파혼 했어요.",
          "speaker_id": "1",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045421.1.23",
          "form": "ㅜㅋㅋ진짜 이렇게 빡치는 세상에!!!",
          "original_form": "ㅜㅋㅋ진짜 이렇게 빡치는 세상에!!!",
          "speaker_id": "2",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045421.1.24",
          "form": "혼전 외도 이제 법적으로 보상 받을 수 있나보던데",
          "original_form": "혼전 외도 이제 법적으로 보상 받을 수 있나보던데",
          "speaker_id": "1",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045421.1.25",
          "form": "제 친구도 진짜 못남 만났는데 선배랑 바람났엉：",
          "original_form": "제 친구도 진짜 못남 만났는데 선배랑 바람났엉：",
          "speaker_id": "2",
          "time": "20210908 02:30"
        }
      ]
