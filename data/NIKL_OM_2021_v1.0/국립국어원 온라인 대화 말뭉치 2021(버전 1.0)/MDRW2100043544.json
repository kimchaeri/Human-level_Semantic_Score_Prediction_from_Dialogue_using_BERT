        {
          "id": "MDRW2100043544.1.1",
          "form": "다행히 주말엔 맑더라구요 ㅎㅎ",
          "original_form": "다행히 주말엔 맑더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043544.1.2",
          "form": "우왕 주말에약속있어요",
          "original_form": "우왕 주말에약속있어요",
          "speaker_id": "1",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043544.1.3",
          "form": "저는 데이트합니다",
          "original_form": "저는 데이트합니다",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043544.1.4",
          "form": "무려 월욜엔 연차썻어유",
          "original_form": "무려 월욜엔 연차썻어유",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043544.1.5",
          "form": "ㅎㅎㅎ저도요",
          "original_form": "ㅎㅎㅎ저도요",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043544.1.6",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043544.1.7",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043544.1.8",
          "form": "앗 통하셧군요",
          "original_form": "앗 통하셧군요",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043544.1.9",
          "form": "히히히",
          "original_form": "히히히",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043544.1.10",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043544.1.11",
          "form": "어디가세요",
          "original_form": "어디가세요",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043544.1.12",
          "form": "쉬고 영화보고하다가 부산 다녀올려구요 ㅎㅎ",
          "original_form": "쉬고 영화보고하다가 부산 다녀올려구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043544.1.13",
          "form": "계획세우셧나요",
          "original_form": "계획세우셧나요",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043544.1.14",
          "form": "ㅎㅎ저는일단만나서세우게요ㅎㅎ",
          "original_form": "ㅎㅎ저는일단만나서세우게요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043544.1.15",
          "form": "샹치 보셧나용",
          "original_form": "샹치 보셧나용",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043544.1.16",
          "form": "영화인가요",
          "original_form": "영화인가요",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043544.1.17",
          "form": "네 마블영화에여",
          "original_form": "네 마블영화에여",
