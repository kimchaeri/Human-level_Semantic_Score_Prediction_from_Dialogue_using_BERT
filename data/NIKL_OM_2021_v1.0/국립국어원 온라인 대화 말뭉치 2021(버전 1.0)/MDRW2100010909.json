      "utterance": [
        {
          "id": "MDRW2100010909.1.1",
          "form": "역시 아는자",
          "original_form": "역시 아는자",
          "speaker_id": "1",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010909.1.2",
          "form": "맞아요 ㅎㅎ참 옛날은 뭘해도 학교때 추억이죠",
          "original_form": "맞아요 ㅎㅎ참 옛날은 뭘해도 학교때 추억이죠",
          "speaker_id": "1",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100010909.1.3",
          "form": "홈트 동호회라는것도 있대요",
          "original_form": "홈트 동호회라는것도 있대요",
          "speaker_id": "2",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100010909.1.4",
          "form": "뭐 그런거라도 참여하면 운동을 하려나",
          "original_form": "뭐 그런거라도 참여하면 운동을 하려나",
          "speaker_id": "2",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100010909.1.5",
          "form": "있겠죠",
          "original_form": "있겠죠",
          "speaker_id": "1",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010909.1.6",
          "form": "유행이예요 홈트",
          "original_form": "유행이예요 홈트",
          "speaker_id": "1",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010909.1.7",
          "form": "요새 모이기 어려울듯요",
          "original_form": "요새 모이기 어려울듯요",
          "speaker_id": "1",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010909.1.8",
          "form": "근데 카페서도 홈트이야기하고",
          "original_form": "근데 카페서도 홈트이야기하고",
          "speaker_id": "1",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100010909.1.9",
          "form": "서로 얼마뭐했다 독려하고 전화해서 그러던데요",
          "original_form": "서로 얼마뭐했다 독려하고 전화해서 그러던데요",
          "speaker_id": "1",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100010909.1.10",
          "form": "그런게 모임이죠 뭐",
          "original_form": "그런게 모임이죠 뭐",
          "speaker_id": "1",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100010909.1.11",
          "form": "음 직접만나서 뭐하는것도 좋긴한데말이죠",
          "original_form": "음 직접만나서 뭐하는것도 좋긴한데말이죠",
          "speaker_id": "2",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100010909.1.12",
          "form": "독서 영화 동호회나",
          "original_form": "독서 영화 동호회나",
          "speaker_id": "2",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100010909.1.13",
          "form": "아 언어배우는것도",
          "original_form": "아 언어배우는것도",
          "speaker_id": "2",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100010909.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010909.1.15",
          "form": "직접 만나서 가끔 보겠죠",
          "original_form": "직접 만나서 가끔 보겠죠",
          "speaker_id": "1",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100010909.1.16",
          "form": "근데 카페서 모임은 서로 독려 파트너 구하더라구요",
          "original_form": "근데 카페서 모임은 서로 독려 파트너 구하더라구요",
          "speaker_id": "1",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100010909.1.17",
          "form": "카페가 음 카카오페이지? 네이버카페?",
          "original_form": "카페가 음 카카오페이지? 네이버카페?",
          "speaker_id": "2",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100010909.1.18",
          "form": "네이버요",
          "original_form": "네이버요",
          "speaker_id": "1",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010909.1.19",
          "form": "아 걷기 모임은 만나서 걷는데요",
          "original_form": "아 걷기 모임은 만나서 걷는데요",
          "speaker_id": "1",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010909.1.20",
          "form": "저도 하고 싶더라구요 모임신청 하고파요",
          "original_form": "저도 하고 싶더라구요 모임신청 하고파요",
          "speaker_id": "1",
          "time": "20210823 23:27"
        },
        {
          "id": "MDRW2100010909.1.21",
          "form": "그럼 덜심심하게 운동 하겠죠",
          "original_form": "그럼 덜심심하게 운동 하겠죠",
          "speaker_id": "1",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100010909.1.22",
          "form": "오 지자체에서 하는거도 있는거같더라고요",
          "original_form": "오 지자체에서 하는거도 있는거같더라고요",
          "speaker_id": "2",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100010909.1.23",
          "form": "지나가다 현수막 본거같아요",
          "original_form": "지나가다 현수막 본거같아요",
          "speaker_id": "2",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100010909.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100010909.1.25",
          "form": "운동모임",
          "original_form": "운동모임",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100010909.1.26",
          "form": "꼭 오프라인 아니면 얼마전에 긴급 헌혈단 가입하긴했어요",
          "original_form": "꼭 오프라인 아니면 얼마전에 긴급 헌혈단 가입하긴했어요",
          "speaker_id": "2",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100010909.1.27",
          "form": "오 그러셨어요 넘 대단하세요",
          "original_form": "오 그러셨어요 넘 대단하세요",
          "speaker_id": "1",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100010909.1.28",
          "form": "전무서워요",
          "original_form": "전무서워요",
          "speaker_id": "1",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100010909.1.29",
          "form": "제동생은 헌혈 자주해서 뭔증서랑 상장같은거 받았더라고요",
          "original_form": "제동생은 헌혈 자주해서 뭔증서랑 상장같은거 받았더라고요",
          "speaker_id": "1",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100010909.1.30",
          "form": "정말 좋은일 하시네요",
          "original_form": "정말 좋은일 하시네요",
          "speaker_id": "1",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100010909.1.31",
          "form": "코로나라서 혈소판이 모자라다고 하더라고요",
          "original_form": "코로나라서 혈소판이 모자라다고 하더라고요",
          "speaker_id": "2",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100010909.1.32",
          "form": "난리라고 하더라구요",
          "original_form": "난리라고 하더라구요",
          "speaker_id": "1",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100010909.1.33",
          "form": "좋은 모임하시네요",
          "original_form": "좋은 모임하시네요",
          "speaker_id": "1",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100010909.1.34",
          "form": "정말 칭찬합니다 ㅎㅎ",
          "original_form": "정말 칭찬합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100010909.1.35",
          "form": "혈소판 헌혈은 좀다르다던데",
          "original_form": "혈소판 헌혈은 좀다르다던데",
          "speaker_id": "1",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100010909.1.36",
          "form": "막힘든가요?",
          "original_form": "막힘든가요?",
          "speaker_id": "1",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100010909.1.37",
          "form": "여자는 좀 하기힘들어요",
          "original_form": "여자는 좀 하기힘들어요",
          "speaker_id": "2",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100010909.1.38",
          "form": "오래걸리고요",
          "original_form": "오래걸리고요",
          "speaker_id": "2",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100010909.1.39",
          "form": "한시간반",
          "original_form": "한시간반",
          "speaker_id": "2",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100010909.1.40",
          "form": "그럴것 같아요",
          "original_form": "그럴것 같아요",
          "speaker_id": "1",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100010909.1.41",
          "form": "진짜 큰맘 먹으셨네요",
          "original_form": "진짜 큰맘 먹으셨네요",
          "speaker_id": "1",
          "time": "20210823 23:59"
        },
        {
          "id": "MDRW2100010909.1.42",
          "form": "기계가 피 잘안나온다고 계속 삑삑거려요",
          "original_form": "기계가 피 잘안나온다고 계속 삑삑거려요",
          "speaker_id": "2",
          "time": "20210823 23:59"
        },
        {
          "id": "MDRW2100010909.1.43",
          "form": "간호사들도 사고날까봐 예민해지고",
          "original_form": "간호사들도 사고날까봐 예민해지고",
          "speaker_id": "2",
          "time": "20210823 23:59"
        },
        {
          "id": "MDRW2100010909.1.44",
          "form": "세상에 위험하네요",
          "original_form": "세상에 위험하네요",
          "speaker_id": "1",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100010909.1.45",
          "form": "전 못할듯해요",
          "original_form": "전 못할듯해요",
          "speaker_id": "1",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100010909.1.46",
          "form": "모임중레서 젤 위험하며 가장 필요하고 훌륭한 모임이네요",
          "original_form": "모임중레서 젤 위험하며 가장 필요하고 훌륭한 모임이네요",
          "speaker_id": "1",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100010909.1.47",
          "form": "저도 헌혈의집에서 별로 꺼려하시더라고요",
          "original_form": "저도 헌혈의집에서 별로 꺼려하시더라고요",
          "speaker_id": "2",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100010909.1.48",
          "form": "그냥 전혈이나 혈장하시라고",
          "original_form": "그냥 전혈이나 혈장하시라고",
          "speaker_id": "2",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100010909.1.49",
          "form": "여자들은 피도 잘안나오고 기계에서 피가 빨리안나온다고 삑삑거리고",
          "original_form": "여자들은 피도 잘안나오고 기계에서 피가 빨리안나온다고 삑삑거리고",
          "speaker_id": "2",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100010909.1.50",
          "form": "아 그럼 그렇게 하세요",
          "original_form": "아 그럼 그렇게 하세요",
          "speaker_id": "1",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100010909.1.51",
          "form": "무리마시고",
          "original_form": "무리마시고",
          "speaker_id": "1",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100010909.1.52",
          "form": "또 오래걸리니까 화장실 못가는거때문에 긴장하게되고",
          "original_form": "또 오래걸리니까 화장실 못가는거때문에 긴장하게되고",
          "speaker_id": "2",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100010909.1.53",
          "form": "안그래도 이젠 안할려고요",
          "original_form": "안그래도 이젠 안할려고요",
          "speaker_id": "2",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100010909.1.54",
          "form": "내 하지 마셔요 ㅜㅜ",
          "original_form": "내 하지 마셔요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100010909.1.55",
          "form": "봉사도 내몸 챙겨가며 하셔야해요",
          "original_form": "봉사도 내몸 챙겨가며 하셔야해요",
          "speaker_id": "1",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100010909.1.56",
          "form": "건강한 남자들있으면 좀 하라고 독려는하고싶어요",
          "original_form": "건강한 남자들있으면 좀 하라고 독려는하고싶어요",
          "speaker_id": "2",
          "time": "20210824 00:03"
        }
      ]
