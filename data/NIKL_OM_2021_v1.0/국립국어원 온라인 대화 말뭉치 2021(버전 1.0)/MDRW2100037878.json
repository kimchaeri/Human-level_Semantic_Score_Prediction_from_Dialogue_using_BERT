      "utterance": [
        {
          "id": "MDRW2100037878.1.1",
          "form": "울집 애들도 코숏이에요 ㅎㅎ",
          "original_form": "울집 애들도 코숏이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100037878.1.2",
          "form": "근데 엄마가 터앙믹스인지 터앙 장모처럼 생겼어요",
          "original_form": "근데 엄마가 터앙믹스인지 터앙 장모처럼 생겼어요",
          "speaker_id": "1",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100037878.1.3",
          "form": "뽀송뽀송하겠어요 ㅋㅋㅋㅋㅋ",
          "original_form": "뽀송뽀송하겠어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100037878.1.4",
          "form": "털이 장난이 아니네요... 형제냥인데 하나는 고등어라는 ㅋㅋㅋ 전혀 안닮았어요",
          "original_form": "털이 장난이 아니네요... 형제냥인데 하나는 고등어라는 ㅋㅋㅋ 전혀 안닮았어요",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037878.1.5",
          "form": "저희 애도 믹스인 것 같은데 알 수가 없네용 크기가 메인쿤보다 약간 작은 것 같아요!",
          "original_form": "저희 애도 믹스인 것 같은데 알 수가 없네용 크기가 메인쿤보다 약간 작은 것 같아요!",
          "speaker_id": "2",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037878.1.6",
          "form": "오 털색이 어때요",
          "original_form": "오 털색이 어때요",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037878.1.7",
          "form": "무늬가 되게 작고 자잘한 삼색이에요!",
          "original_form": "무늬가 되게 작고 자잘한 삼색이에요!",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100037878.1.8",
          "form": "오그럼 암컷이겠어요?!",
          "original_form": "오그럼 암컷이겠어요?!",
          "speaker_id": "1",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100037878.1.9",
          "form": "복덩이 수컷이에요 ㅎㅎ",
          "original_form": "복덩이 수컷이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100037878.1.10",
          "form": "10살 넘기는게 목표에요 ㅎㅎ",
          "original_form": "10살 넘기는게 목표에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100037878.1.11",
          "form": "헉 삼색이  수컷은 엄청 희귀냥 아닌가요?",
          "original_form": "헉 삼색이  수컷은 엄청 희귀냥 아닌가요?",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100037878.1.12",
          "form": "복덩이 맞네요",
          "original_form": "복덩이 맞네요",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100037878.1.13",
          "form": "지금 몇살이에요?",
          "original_form": "지금 몇살이에요?",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100037878.1.14",
          "form": "6살이에요!",
          "original_form": "6살이에요!",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037878.1.15",
          "form": "수컷은 수명이 짧다그래서 걱정입니다ㅠㅠ",
          "original_form": "수컷은 수명이 짧다그래서 걱정입니다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037878.1.16",
          "form": "오 형님이네요",
          "original_form": "오 형님이네요",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037878.1.17",
          "form": "애기들 몇 살이에요??",
          "original_form": "애기들 몇 살이에요??",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037878.1.18",
          "form": "우리애들은 4살이에요",
          "original_form": "우리애들은 4살이에요",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037878.1.19",
          "form": "유투브에 할배처럼",
          "original_form": "유투브에 할배처럼",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100037878.1.20",
          "form": "오래오래 살기를요 ㅠㅠ 저도 바라는중",
          "original_form": "오래오래 살기를요 ㅠㅠ 저도 바라는중",
          "speaker_id": "1",
          "time": "20210901 20:53"
        }
      ]
