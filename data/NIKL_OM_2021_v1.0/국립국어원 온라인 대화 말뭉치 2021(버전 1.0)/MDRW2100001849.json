      "utterance": [
        {
          "id": "MDRW2100001849.1.1",
          "form": "오늘도 뭔가 비가 내린 것 같아요ㅋㅋㅋ요새 비가 매일 내리네요",
          "original_form": "오늘도 뭔가 비가 내린 것 같아요ㅋㅋㅋ요새 비가 매일 내리네요",
          "speaker_id": "1",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100001849.1.2",
          "form": "늦장마라더니.. 비가 매일오네요ㅜ",
          "original_form": "늦장마라더니.. 비가 매일오네요ㅜ",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100001849.1.3",
          "form": "그러니까요ㅠㅜ",
          "original_form": "그러니까요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100001849.1.4",
          "form": "해 쨍한 가을을 어서 느끼고 싶어요",
          "original_form": "해 쨍한 가을을 어서 느끼고 싶어요",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100001849.1.5",
          "form": "맞아요 저 가을을 제일 좋아하거든요ㅋㅋㅋ",
          "original_form": "맞아요 저 가을을 제일 좋아하거든요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100001849.1.6",
          "form": "근데 금방 끝나서 아쉬워요ㅠㅜ",
          "original_form": "근데 금방 끝나서 아쉬워요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100001849.1.7",
          "form": "가을은 하늘만 보고 있어도 행복해요",
          "original_form": "가을은 하늘만 보고 있어도 행복해요",
          "speaker_id": "2",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100001849.1.8",
          "form": "그쵸ㅋㅋㅋㅋ진짜 왜 천고마비인지 알겠더라고요ㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋㅋ진짜 왜 천고마비인지 알겠더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100001849.1.9",
          "form": "가을 지나면 곧 겨울인데 이번 겨울은 좀 덜 추웠음 좋겠어요ㅠㅜ",
          "original_form": "가을 지나면 곧 겨울인데 이번 겨울은 좀 덜 추웠음 좋겠어요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100001849.1.10",
          "form": "겨울은 추워서 롱패딩과 한몸이예요ㅋ",
          "original_form": "겨울은 추워서 롱패딩과 한몸이예요ㅋ",
          "speaker_id": "2",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100001849.1.11",
          "form": "맞아요ㅋㅋㅋ근데 또 실내 들어가면 더워져서 안에는 좀 가볍게 입게 되더라고요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ근데 또 실내 들어가면 더워져서 안에는 좀 가볍게 입게 되더라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100001849.1.12",
          "form": "그래서 겉은 꼭 롱패딩입어요ㅋㅋ",
          "original_form": "그래서 겉은 꼭 롱패딩입어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100001849.1.13",
          "form": "맞아요ㅋㅋㅋ롱패딩 만든 사람한테 상줘야해요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ롱패딩 만든 사람한테 상줘야해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100001849.1.14",
          "form": "최고의 발명품이예요ㅎ",
          "original_form": "최고의 발명품이예요ㅎ",
          "speaker_id": "2",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100001849.1.15",
          "form": "헉 여기 지금 비 내리네요ㅋㅋㅋ",
          "original_form": "헉 여기 지금 비 내리네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100001849.1.16",
          "form": "진짜요?!",
          "original_form": "진짜요?!",
          "speaker_id": "2",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100001849.1.17",
          "form": "네ㅋㅋㅋㅋ근데 금방 그칠 것 같아요ㅋㅋㅋ",
