      "utterance": [
        {
          "id": "MDRW2100037627.1.1",
          "form": "혹시 운동하시는거 있으세요?",
          "original_form": "혹시 운동하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100037627.1.2",
          "form": "저 이번주말에 클라이밍하러가요!!",
          "original_form": "저 이번주말에 클라이밍하러가요!!",
          "speaker_id": "1",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100037627.1.3",
          "form": "해보셨는지,,",
          "original_form": "해보셨는지,,",
          "speaker_id": "1",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100037627.1.4",
          "form": "앗 해보진 않았고 봤어요",
          "original_form": "앗 해보진 않았고 봤어요",
          "speaker_id": "2",
          "time": "20210901 14:00"
        },
        {
          "id": "MDRW2100037627.1.5",
          "form": "그 인공벽에서하는그거 맞나요?",
          "original_form": "그 인공벽에서하는그거 맞나요?",
          "speaker_id": "2",
          "time": "20210901 14:00"
        },
        {
          "id": "MDRW2100037627.1.6",
          "form": "네! 인공벽이에요",
          "original_form": "네! 인공벽이에요",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100037627.1.7",
          "form": "이번에 올림픽 종목도 됏더라구요",
          "original_form": "이번에 올림픽 종목도 됏더라구요",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100037627.1.8",
          "form": "오 첫도전이신가요 아님  종종 가세요?",
          "original_form": "오 첫도전이신가요 아님  종종 가세요?",
          "speaker_id": "2",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100037627.1.9",
          "form": "저 코로나 이전엔 종종 갔는데 코로나 이후엔 처음이라 설레요!",
          "original_form": "저 코로나 이전엔 종종 갔는데 코로나 이후엔 처음이라 설레요!",
          "speaker_id": "1",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100037627.1.10",
          "form": "원래도 없던 근육 증발햇을 것 같아요",
          "original_form": "원래도 없던 근육 증발햇을 것 같아요",
          "speaker_id": "1",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100037627.1.11",
          "form": "그런건 1회에 얼마정도해요?",
          "original_form": "그런건 1회에 얼마정도해요?",
          "speaker_id": "2",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100037627.1.12",
          "form": "초보자도할수있나요? ㅎㅎ",
          "original_form": "초보자도할수있나요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100037627.1.13",
          "form": "2-30000원쯤 햇던거같아요!!",
          "original_form": "2-30000원쯤 햇던거같아요!!",
          "speaker_id": "1",
          "time": "20210901 17:42"
        },
        {
          "id": "MDRW2100037627.1.14",
          "form": "처음 가도 할 수 잇어요! 초보자 코스가 잇어요",
          "original_form": "처음 가도 할 수 잇어요! 초보자 코스가 잇어요",
          "speaker_id": "1",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100037627.1.15",
          "form": "가면 가르쳐주는 사람도 있나요? 초보일때는 ㅎㅎ 오 신기해라",
          "original_form": "가면 가르쳐주는 사람도 있나요? 초보일때는 ㅎㅎ 오 신기해라",
          "speaker_id": "2",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100037627.1.16",
          "form": "강습반도 잇고 원데이클래스도 잇어서 입문하긴 좋아보여요!",
          "original_form": "강습반도 잇고 원데이클래스도 잇어서 입문하긴 좋아보여요!",
          "speaker_id": "1",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100037627.1.17",
          "form": "근데 그런것도 꾸준히 하시는분들이계시는군요",
          "original_form": "근데 그런것도 꾸준히 하시는분들이계시는군요",
          "speaker_id": "2",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100037627.1.18",
          "form": "근력운동이려나 ㅋㅋ",
          "original_form": "근력운동이려나 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100037627.1.19",
          "form": "네 ㅋㅋㅋㅋㅋ 가면 고인물들이 턱걸이 무한개 하고잇어요",
          "original_form": "네 ㅋㅋㅋㅋㅋ 가면 고인물들이 턱걸이 무한개 하고잇어요",
          "speaker_id": "1",
          "time": "20210901 19:19"
        }
