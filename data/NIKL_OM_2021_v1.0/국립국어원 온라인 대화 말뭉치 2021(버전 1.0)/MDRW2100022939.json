      "utterance": [
        {
          "id": "MDRW2100022939.1.1",
          "form": "알아서 접혀나왓으면 싶져",
          "original_form": "알아서 접혀나왓으면 싶져",
          "speaker_id": "2",
          "time": "20210829 19:30"
        },
        {
          "id": "MDRW2100022939.1.2",
          "form": "웅 나와서 접고 자기집으로 들어갔으면",
          "original_form": "웅 나와서 접고 자기집으로 들어갔으면",
          "speaker_id": "1",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100022939.1.3",
          "form": "맞아 서랍장에 스스로",
          "original_form": "맞아 서랍장에 스스로",
          "speaker_id": "2",
          "time": "20210829 19:33"
        },
        {
          "id": "MDRW2100022939.1.4",
          "form": "빨래도 해야하는데",
          "original_form": "빨래도 해야하는데",
          "speaker_id": "2",
          "time": "20210829 19:33"
        },
        {
          "id": "MDRW2100022939.1.5",
          "form": "난 남편와이셔츠 다림질도",
          "original_form": "난 남편와이셔츠 다림질도",
          "speaker_id": "1",
          "time": "20210829 19:33"
        },
        {
          "id": "MDRW2100022939.1.6",
          "form": "이런 할일이태산",
          "original_form": "이런 할일이태산",
          "speaker_id": "2",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022939.1.7",
          "form": "시간은 점점 지나고있는데...",
          "original_form": "시간은 점점 지나고있는데...",
          "speaker_id": "1",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022939.1.8",
          "form": "ㅋㅋ전 아직도 친정",
          "original_form": "ㅋㅋ전 아직도 친정",
          "speaker_id": "2",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100022939.1.9",
          "form": "아직도 천국",
          "original_form": "아직도 천국",
          "speaker_id": "1",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100022939.1.10",
          "form": "ㅋㅋ현실로 돌아와 빨래돌림",
          "original_form": "ㅋㅋ현실로 돌아와 빨래돌림",
          "speaker_id": "2",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100022939.1.11",
          "form": "해도해도 끝이 안나는 집안일",
          "original_form": "해도해도 끝이 안나는 집안일",
          "speaker_id": "1",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100022939.1.12",
          "form": "숙제처럼하는데",
          "original_form": "숙제처럼하는데",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100022939.1.13",
          "form": "숙제는 끝이라도 있지",
          "original_form": "숙제는 끝이라도 있지",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100022939.1.14",
          "form": "ㅎㅎ 오늘 나의 비싼 그릇을 깨먹은 마이 허즈번드",
          "original_form": "ㅎㅎ 오늘 나의 비싼 그릇을 깨먹은 마이 허즈번드",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100022939.1.15",
          "form": "ㅋㅋ속상하게",
          "original_form": "ㅋㅋ속상하게",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100022939.1.16",
          "form": "세상 슬퍼 울어",
          "original_form": "세상 슬퍼 울어",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100022939.1.17",
          "form": "ㅋㅋ애슐리 밀키트도 먹어봣어요?",
          "original_form": "ㅋㅋ애슐리 밀키트도 먹어봣어요?",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100022939.1.18",
          "form": "아니 아직 도전을 못함",
          "original_form": "아니 아직 도전을 못함",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100022939.1.19",
          "form": "집안일이 산더미",
          "original_form": "집안일이 산더미",
          "speaker_id": "1",
          "time": "20210830 13:16"
        },
        {
          "id": "MDRW2100022939.1.20",
          "form": "저도 오늘도빨래3번",
          "original_form": "저도 오늘도빨래3번",
          "speaker_id": "2",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100022939.1.21",
          "form": "빨래의 날",
          "original_form": "빨래의 날",
          "speaker_id": "1",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100022939.1.22",
          "form": "ㅋㅋ청소기도 돌리고",
          "original_form": "ㅋㅋ청소기도 돌리고",
          "speaker_id": "2",
          "time": "20210830 13:41"
        },
