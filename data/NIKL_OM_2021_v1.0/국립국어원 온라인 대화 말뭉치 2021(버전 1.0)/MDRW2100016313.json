      "utterance": [
        {
          "id": "MDRW2100016313.1.1",
          "form": "안녕하세요 최근에 보신 영화가 있으신가요",
          "original_form": "안녕하세요 최근에 보신 영화가 있으신가요",
          "speaker_id": "1",
          "time": "20210826 11:53"
        },
        {
          "id": "MDRW2100016313.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210826 11:53"
        },
        {
          "id": "MDRW2100016313.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 11:53"
        },
        {
          "id": "MDRW2100016313.1.4",
          "form": "저는 캐치미이프유캔이요",
          "original_form": "저는 캐치미이프유캔이요",
          "speaker_id": "2",
          "time": "20210826 11:53"
        },
        {
          "id": "MDRW2100016313.1.5",
          "form": "별 다섯 개 중에 몇 점이에요?",
          "original_form": "별 다섯 개 중에 몇 점이에요?",
          "speaker_id": "1",
          "time": "20210826 11:54"
        },
        {
          "id": "MDRW2100016313.1.6",
          "form": "다섯개요 재밌더라구요",
          "original_form": "다섯개요 재밌더라구요",
          "speaker_id": "2",
          "time": "20210826 11:54"
        },
        {
          "id": "MDRW2100016313.1.7",
          "form": "뭐보셨어요?",
          "original_form": "뭐보셨어요?",
          "speaker_id": "2",
          "time": "20210826 11:54"
        },
        {
          "id": "MDRW2100016313.1.8",
          "form": "저는 영화 말고 드라마인데 킹덤 봤어요",
          "original_form": "저는 영화 말고 드라마인데 킹덤 봤어요",
          "speaker_id": "1",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016313.1.9",
          "form": "넷플릭스 보시나봐요",
          "original_form": "넷플릭스 보시나봐요",
          "speaker_id": "2",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016313.1.10",
          "form": "왓챠 넷플릭스 한 달씩 번갈아가면서 봐요 흐흐",
          "original_form": "왓챠 넷플릭스 한 달씩 번갈아가면서 봐요 흐흐",
          "speaker_id": "1",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016313.1.11",
          "form": "저 둘다봐요 ㅋㅋㅋ",
          "original_form": "저 둘다봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016313.1.12",
          "form": "넷플릭스는 드라마 왓챠는 영화가 좋은 거 같아요ㅎㅎ",
          "original_form": "넷플릭스는 드라마 왓챠는 영화가 좋은 거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016313.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016313.1.14",
          "form": "넷플릭스 드러마 뭐보세요?",
          "original_form": "넷플릭스 드러마 뭐보세요?",
          "speaker_id": "2",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016313.1.15",
          "form": "최근에 어둠 속으로 되게 재밌게 봤어요ㅎㅎ",
          "original_form": "최근에 어둠 속으로 되게 재밌게 봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100016313.1.16",
          "form": "처음들어보네요 어느나라건가요??",
          "original_form": "처음들어보네요 어느나라건가요??",
          "speaker_id": "2",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100016313.1.17",
          "form": "벨기에 드라마에요 저도 벨기에 드라마는 처음 봤는데 재밌더라고요!ㅋㅋ",
          "original_form": "벨기에 드라마에요 저도 벨기에 드라마는 처음 봤는데 재밌더라고요!ㅋㅋ",
