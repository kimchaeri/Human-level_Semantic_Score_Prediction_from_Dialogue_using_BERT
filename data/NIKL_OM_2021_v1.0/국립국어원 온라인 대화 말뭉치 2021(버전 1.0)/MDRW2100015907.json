      "utterance": [
        {
          "id": "MDRW2100015907.1.1",
          "form": "올해 어디 다녀오셨나요?",
          "original_form": "올해 어디 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210826 02:42"
        },
        {
          "id": "MDRW2100015907.1.2",
          "form": "음 올해에는 근교에 한군데만 다녀왔어요",
          "original_form": "음 올해에는 근교에 한군데만 다녀왔어요",
          "speaker_id": "2",
          "time": "20210826 02:43"
        },
        {
          "id": "MDRW2100015907.1.3",
          "form": "아 서울도 다녀왔네요",
          "original_form": "아 서울도 다녀왔네요",
          "speaker_id": "2",
          "time": "20210826 02:43"
        },
        {
          "id": "MDRW2100015907.1.4",
          "form": "어디요?",
          "original_form": "어디요?",
          "speaker_id": "1",
          "time": "20210826 02:43"
        },
        {
          "id": "MDRW2100015907.1.5",
          "form": "문경 갔다왔어요!",
          "original_form": "문경 갔다왔어요!",
          "speaker_id": "2",
          "time": "20210826 02:43"
        },
        {
          "id": "MDRW2100015907.1.6",
          "form": "전 안가봤는데좋나요?",
          "original_form": "전 안가봤는데좋나요?",
          "speaker_id": "1",
          "time": "20210826 02:45"
        },
        {
          "id": "MDRW2100015907.1.7",
          "form": "뭐가 유명해요?",
          "original_form": "뭐가 유명해요?",
          "speaker_id": "1",
          "time": "20210826 02:45"
        },
        {
          "id": "MDRW2100015907.1.8",
          "form": "음 문경새재...? 작고 한적한 도시에요 ㅋㅋㅋ",
          "original_form": "음 문경새재...? 작고 한적한 도시에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:48"
        },
        {
          "id": "MDRW2100015907.1.9",
          "form": "경상도 살아서 무난하게 가기 좋더라구요",
          "original_form": "경상도 살아서 무난하게 가기 좋더라구요",
          "speaker_id": "2",
          "time": "20210826 02:49"
        },
        {
          "id": "MDRW2100015907.1.10",
          "form": "교과서에서 나오는~  ㅎㅎ",
          "original_form": "교과서에서 나오는~  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 02:49"
        },
        {
          "id": "MDRW2100015907.1.11",
          "form": "다들 코로나라고해도 갈사람은 다가는거같기도해요",
          "original_form": "다들 코로나라고해도 갈사람은 다가는거같기도해요",
          "speaker_id": "1",
          "time": "20210826 02:49"
        },
        {
          "id": "MDRW2100015907.1.12",
          "form": "제주도가 숙소가 없을정도던데요",
          "original_form": "제주도가 숙소가 없을정도던데요",
          "speaker_id": "1",
          "time": "20210826 02:50"
        },
        {
          "id": "MDRW2100015907.1.13",
          "form": "아 맞아요 ㅋㅋㅋ주변에 봐도 제주도 많이 가더라구요",
          "original_form": "아 맞아요 ㅋㅋㅋ주변에 봐도 제주도 많이 가더라구요",
          "speaker_id": "2",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100015907.1.14",
          "form": "저도 가고싶긴한데 시간맞는 친구들이 잘 없네요",
          "original_form": "저도 가고싶긴한데 시간맞는 친구들이 잘 없네요",
          "speaker_id": "2",
          "time": "20210826 02:52"
        },
        {
          "id": "MDRW2100015907.1.15",
          "form": "저도 셋이서 가려다 날짜 안맞아서  못가고있네요",
          "original_form": "저도 셋이서 가려다 날짜 안맞아서  못가고있네요",
          "speaker_id": "1",
          "time": "20210826 02:52"
        },
        {
          "id": "MDRW2100015907.1.16",
          "form": "4단계 격상돼서 더  ㅠ",
          "original_form": "4단계 격상돼서 더  ㅠ",
          "speaker_id": "1",
          "time": "20210826 02:53"
        },
        {
          "id": "MDRW2100015907.1.17",
          "form": "맞아요 ㅠㅠ 몬가 나이들수록 점점 여행시간 맞추기 힘든거같아요",
          "original_form": "맞아요 ㅠㅠ 몬가 나이들수록 점점 여행시간 맞추기 힘든거같아요",
          "speaker_id": "2",
          "time": "20210826 02:54"
        },
        {
          "id": "MDRW2100015907.1.18",
          "form": "한명이 올해 결혼해서 그전에 갈랬는데",
          "original_form": "한명이 올해 결혼해서 그전에 갈랬는데",
          "speaker_id": "1",
          "time": "20210826 02:54"
        },
        {
          "id": "MDRW2100015907.1.19",
          "form": "결국 친구 셋이서는 한번도 못가보네요ㅎ",
          "original_form": "결국 친구 셋이서는 한번도 못가보네요ㅎ",
          "speaker_id": "1",
          "time": "20210826 02:55"
        },
        {
          "id": "MDRW2100015907.1.20",
          "form": "아쉽겠어요 ㅜㅜ 결혼하고나면 더 시간 맞추기 힘들거같은데",
          "original_form": "아쉽겠어요 ㅜㅜ 결혼하고나면 더 시간 맞추기 힘들거같은데",
          "speaker_id": "2",
          "time": "20210826 02:57"
        },
        {
          "id": "MDRW2100015907.1.21",
          "form": "그니까요 둘씩은 가봤는데 셋이서 못가보니 아쉽더라구요",
          "original_form": "그니까요 둘씩은 가봤는데 셋이서 못가보니 아쉽더라구요",
          "speaker_id": "1",
          "time": "20210826 02:57"
        }
      ]
