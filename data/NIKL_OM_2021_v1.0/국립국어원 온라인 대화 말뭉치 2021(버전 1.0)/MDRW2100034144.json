      "utterance": [
        {
          "id": "MDRW2100034144.1.1",
          "form": "알바마도 보셨어요?",
          "original_form": "알바마도 보셨어요?",
          "speaker_id": "2",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100034144.1.2",
          "form": "리제로도 좋아하는데 요새 뭔가 산으로 가는듯해서 ㅋㅋ",
          "original_form": "리제로도 좋아하는데 요새 뭔가 산으로 가는듯해서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100034144.1.3",
          "form": "알바마가 뭐죠..?",
          "original_form": "알바마가 뭐죠..?",
          "speaker_id": "1",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100034144.1.4",
          "form": "리제로는 만화책으로만 봤어요",
          "original_form": "리제로는 만화책으로만 봤어요",
          "speaker_id": "1",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100034144.1.5",
          "form": "알바뛰는마왕님요",
          "original_form": "알바뛰는마왕님요",
          "speaker_id": "2",
          "time": "20210827 18:32"
        },
        {
          "id": "MDRW2100034144.1.6",
          "form": "한번 찾아봐야겠어요",
          "original_form": "한번 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100034144.1.7",
          "form": "네 오래전꺼예요 넷플에서 없어졌다 생겼다 해요 ㅋㅋ",
          "original_form": "네 오래전꺼예요 넷플에서 없어졌다 생겼다 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100034144.1.8",
          "form": "아ㅠ 넷플은 순식간이더라구요",
          "original_form": "아ㅠ 넷플은 순식간이더라구요",
          "speaker_id": "1",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100034144.1.9",
          "form": "네 보려면 사라지고 ㅜㅜ",
          "original_form": "네 보려면 사라지고 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100034144.1.10",
          "form": "있을때 봐야해요",
          "original_form": "있을때 봐야해요",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100034144.1.11",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100034144.1.12",
          "form": "말해주는데 저만 모르는건지..",
          "original_form": "말해주는데 저만 모르는건지..",
          "speaker_id": "1",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100034144.1.13",
          "form": "저도 몰라요",
          "original_form": "저도 몰라요",
          "speaker_id": "2",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100034144.1.14",
          "form": "아 기묘한 이야기 다음도 기다리고 있어요",
          "original_form": "아 기묘한 이야기 다음도 기다리고 있어요",
          "speaker_id": "2",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100034144.1.15",
          "form": "넘 잼나게 본 시리즈 예요",
          "original_form": "넘 잼나게 본 시리즈 예요",
          "speaker_id": "2",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100034144.1.16",
          "form": "뭐 재밌게 보셨어요?",
          "original_form": "뭐 재밌게 보셨어요?",
          "speaker_id": "2",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100034144.1.17",
          "form": "디지몬이요ㅣㄱㅋㅋ",
          "original_form": "디지몬이요ㅣㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100034144.1.18",
          "form": "넷플은 디지몬이 많더라구요",
          "original_form": "넷플은 디지몬이 많더라구요",
          "speaker_id": "1",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100034144.1.19",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100034144.1.20",
          "form": "저 예전에 아들이랑 다봤었는게",
          "original_form": "저 예전에 아들이랑 다봤었는게",
          "speaker_id": "2",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100034144.1.21",
          "form": "티비에서도 했었거든요",
          "original_form": "티비에서도 했었거든요",
          "speaker_id": "2",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100034144.1.22",
          "form": "정말 오래됐어요",
          "original_form": "정말 오래됐어요",
          "speaker_id": "2",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100034144.1.23",
          "form": "장난감도 있었어요 ㅎㅎ",
          "original_form": "장난감도 있었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:07"
        },
        {
          "id": "MDRW2100034144.1.24",
          "form": "오오ㅋㅋ 저도요!",
          "original_form": "오오ㅋㅋ 저도요!",
          "speaker_id": "1",
          "time": "20210827 19:07"
        },
        {
          "id": "MDRW2100034144.1.25",
          "form": "인형도 샀었어요",
          "original_form": "인형도 샀었어요",
          "speaker_id": "1",
          "time": "20210827 19:07"
        },
        {
          "id": "MDRW2100034144.1.26",
          "form": "리키랑 나리 좋아했어요",
          "original_form": "리키랑 나리 좋아했어요",
          "speaker_id": "1",
          "time": "20210827 19:07"
        },
        {
          "id": "MDRW2100034144.1.27",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:08"
        },
        {
          "id": "MDRW2100034144.1.28",
          "form": "근데 어딨는지 ㅡㅡ모르겠네요",
          "original_form": "근데 어딨는지 ㅡㅡ모르겠네요",
          "speaker_id": "2",
          "time": "20210827 19:08"
        },
        {
          "id": "MDRW2100034144.1.29",
          "form": "버렸거나 팔았나봐요",
          "original_form": "버렸거나 팔았나봐요",
          "speaker_id": "2",
          "time": "20210827 19:08"
        },
        {
          "id": "MDRW2100034144.1.30",
          "form": "진짜 오래전인데 ㅋㅋ",
          "original_form": "진짜 오래전인데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:08"
        },
        {
          "id": "MDRW2100034144.1.31",
          "form": "세상에 디지몬 본분 참봐요 ㅎㅎ",
          "original_form": "세상에 디지몬 본분 참봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:09"
        }
      ]
