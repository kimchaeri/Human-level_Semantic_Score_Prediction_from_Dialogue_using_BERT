      "utterance": [
        {
          "id": "MDRW2100018590.1.1",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:53"
        },
        {
          "id": "MDRW2100018590.1.2",
          "form": "어딜 가도 빅데이터만....슬쩍",
          "original_form": "어딜 가도 빅데이터만....슬쩍",
          "speaker_id": "1",
          "time": "20210827 08:53"
        },
        {
          "id": "MDRW2100018590.1.3",
          "form": "요즘 페밀리?",
          "original_form": "요즘 페밀리?",
          "speaker_id": "1",
          "time": "20210827 08:53"
        },
        {
          "id": "MDRW2100018590.1.4",
          "form": "그런 어플도 추천받았어요",
          "original_form": "그런 어플도 추천받았어요",
          "speaker_id": "1",
          "time": "20210827 08:53"
        },
        {
          "id": "MDRW2100018590.1.5",
          "form": "저 고양이 키우는 거 어찌아시구",
          "original_form": "저 고양이 키우는 거 어찌아시구",
          "speaker_id": "1",
          "time": "20210827 08:54"
        },
        {
          "id": "MDRW2100018590.1.6",
          "form": "헉 고양이 키우시는구나!!!!",
          "original_form": "헉 고양이 키우시는구나!!!!",
          "speaker_id": "2",
          "time": "20210827 08:55"
        },
        {
          "id": "MDRW2100018590.1.7",
          "form": "고양이 이름이 뭔지 여쭤봐도 돼용?",
          "original_form": "고양이 이름이 뭔지 여쭤봐도 돼용?",
          "speaker_id": "2",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018590.1.8",
          "form": "name3이에요",
          "original_form": "&name3&이에요",
          "speaker_id": "1",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018590.1.9",
          "form": "귀엽죠",
          "original_form": "귀엽죠",
          "speaker_id": "1",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018590.1.10",
          "form": "어머 name3이 짖짜 기엽다",
          "original_form": "어머 &name3&이 짖짜 기엽다",
          "speaker_id": "2",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100018590.1.11",
          "form": "혹시 하얀고양이에요?",
          "original_form": "혹시 하얀고양이에요?",
          "speaker_id": "2",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100018590.1.12",
          "form": "하얀색 많이 섞인 치즈예요",
          "original_form": "하얀색 많이 섞인 치즈예요",
          "speaker_id": "1",
          "time": "20210827 09:04"
        },
        {
          "id": "MDRW2100018590.1.13",
          "form": "귀엽..",
          "original_form": "귀엽..",
          "speaker_id": "1",
          "time": "20210827 09:04"
        },
        {
          "id": "MDRW2100018590.1.14",
          "form": "저는 세상에 치즈냥이가 다 name3처럼 생긴 줄..",
          "original_form": "저는 세상에 치즈냥이가 다 &name3&처럼 생긴 줄..",
          "speaker_id": "1",
          "time": "20210827 09:05"
        },
        {
          "id": "MDRW2100018590.1.15",
          "form": "근데 강아지들은 똑같이생겼다! 라는 생각이 드는 애들이 많은데",
          "original_form": "근데 강아지들은 똑같이생겼다! 라는 생각이 드는 애들이 많은데",
          "speaker_id": "2",
          "time": "20210827 09:05"
        },
        {
          "id": "MDRW2100018590.1.16",
          "form": "고양이는 진짜 다 다르게 생긴거같아요",
          "original_form": "고양이는 진짜 다 다르게 생긴거같아요",
          "speaker_id": "2",
          "time": "20210827 09:05"
        },
        {
          "id": "MDRW2100018590.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요!!!!!!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요!!!!!!!",
          "speaker_id": "1",
          "time": "20210827 09:06"
        },
        {
          "id": "MDRW2100018590.1.18",
          "form": "사랑해 고양이들!",
          "original_form": "사랑해 고양이들!",
          "speaker_id": "1",
          "time": "20210827 09:06"
        },
        {
          "id": "MDRW2100018590.1.19",
          "form": "전 그거 해보고싶어요",
          "original_form": "전 그거 해보고싶어요",
          "speaker_id": "2",
          "time": "20210827 09:06"
        },
        {
          "id": "MDRW2100018590.1.20",
          "form": "고양이 쭈욱...",
          "original_form": "고양이 쭈욱...",
          "speaker_id": "2",
          "time": "20210827 09:06"
        },
        {
          "id": "MDRW2100018590.1.21",
          "form": "안을때 고양이 쭈욱..",
          "original_form": "안을때 고양이 쭈욱..",
          "speaker_id": "2",
          "time": "20210827 09:06"
        },
        {
          "id": "MDRW2100018590.1.22",
          "form": "완전 뭐냐 인절미에 콜고물안묻힌 것처럼",
          "original_form": "완전 뭐냐 인절미에 콜고물안묻힌 것처럼",
          "speaker_id": "1",
          "time": "20210827 09:07"
        },
        {
          "id": "MDRW2100018590.1.23",
          "form": "늘어나요",
          "original_form": "늘어나요",
          "speaker_id": "1",
          "time": "20210827 09:07"
        },
        {
          "id": "MDRW2100018590.1.24",
          "form": "아 ㅋ ㅋ ㅋ ㅋ ㅋ ㅋ ㅋ ㅋ ㅋ  ㅋㅋ  인절밐ㅋㅋㅋㅋㅋㅋ 이해가 팍돼요",
          "original_form": "아 ㅋ ㅋ ㅋ ㅋ ㅋ ㅋ ㅋ ㅋ ㅋ  ㅋㅋ  인절밐ㅋㅋㅋㅋㅋㅋ 이해가 팍돼요",
          "speaker_id": "2",
          "time": "20210827 09:07"
        },
        {
          "id": "MDRW2100018590.1.25",
          "form": "새벽에 출근할 일이 있었는데 어디서 애기가 자지러지게 우는거에요",
          "original_form": "새벽에 출근할 일이 있었는데 어디서 애기가 자지러지게 우는거에요",
