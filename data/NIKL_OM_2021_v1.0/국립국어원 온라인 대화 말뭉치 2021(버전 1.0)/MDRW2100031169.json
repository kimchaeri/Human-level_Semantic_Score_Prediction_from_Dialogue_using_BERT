      "utterance": [
        {
          "id": "MDRW2100031169.1.1",
          "form": "그게 되나요",
          "original_form": "그게 되나요",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031169.1.2",
          "form": "투자하는 노력과 시간이 많지 않으니 기대를 크게 하지 않는 거죠",
          "original_form": "투자하는 노력과 시간이 많지 않으니 기대를 크게 하지 않는 거죠",
          "speaker_id": "1",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031169.1.3",
          "form": "왠지 잘하시면서 대화해야 될 것 같아요",
          "original_form": "왠지 잘하시면서 대화해야 될 것 같아요",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031169.1.4",
          "form": "그렇진 않아요",
          "original_form": "그렇진 않아요",
          "speaker_id": "1",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031169.1.5",
          "form": "근데 돈을 벌면 돈 걱정이 사람이 되지 않나요",
          "original_form": "근데 돈을 벌면 돈 걱정이 사람이 되지 않나요",
          "speaker_id": "2",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031169.1.6",
          "form": "많이 벌면 걱정이 사라질 거라고 믿지만 그렇진 않아요",
          "original_form": "많이 벌면 걱정이 사라질 거라고 믿지만 그렇진 않아요",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031169.1.7",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "2",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031169.1.8",
          "form": "연봉이 크게 오르지 않거든요",
          "original_form": "연봉이 크게 오르지 않거든요",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031169.1.9",
          "form": "월급이 오르지 않는단 얘기죠",
          "original_form": "월급이 오르지 않는단 얘기죠",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031169.1.10",
          "form": "맞아요 우리 애들 월급이 줄었어요",
          "original_form": "맞아요 우리 애들 월급이 줄었어요",
          "speaker_id": "2",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031169.1.11",
          "form": "돈은 목적인가요 수단인가요",
          "original_form": "돈은 목적인가요 수단인가요",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031169.1.12",
          "form": "그게 무슨 말이에요",
          "original_form": "그게 무슨 말이에요",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031169.1.13",
          "form": "돈을 벌기 위해 일하는지 피로에서 돈을 버는 지",
          "original_form": "돈을 벌기 위해 일하는지 피로에서 돈을 버는 지",
          "speaker_id": "1",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031169.1.14",
          "form": "필요해서 보는 거죠",
          "original_form": "필요해서 보는 거죠",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031169.1.15",
          "form": "저는 살아가는 데 필요해서 돈을 법 니다 저에게 돈은 수단입니다",
          "original_form": "저는 살아가는 데 필요해서 돈을 법 니다 저에게 돈은 수단입니다",
          "speaker_id": "1",
