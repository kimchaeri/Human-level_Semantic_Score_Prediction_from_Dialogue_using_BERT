      "utterance": [
        {
          "id": "MDRW2100007775.1.1",
          "form": "편의점 가면 꼭 사는 물품이 있나요",
          "original_form": "편의점 가면 꼭 사는 물품이 있나요",
          "speaker_id": "1",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007775.1.2",
          "form": "저는 원플러스원 탄산음료사요",
          "original_form": "저는 원플러스원 탄산음료사요",
          "speaker_id": "2",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007775.1.3",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007775.1.4",
          "form": "요즘 음료값도 비싸서요",
          "original_form": "요즘 음료값도 비싸서요",
          "speaker_id": "2",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007775.1.5",
          "form": "콜라나환타삽니다",
          "original_form": "콜라나환타삽니다",
          "speaker_id": "2",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007775.1.6",
          "form": "요즘 원플원인게 뭐가있지",
          "original_form": "요즘 원플원인게 뭐가있지",
          "speaker_id": "1",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007775.1.7",
          "form": "너무좋다",
          "original_form": "너무좋다",
          "speaker_id": "1",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007775.1.8",
          "form": "환타 무슨 맛 좋아하세요",
          "original_form": "환타 무슨 맛 좋아하세요",
          "speaker_id": "1",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007775.1.9",
          "form": "매달 품목이바뀌는데 콜라나 사이다는 항상 있어요",
          "original_form": "매달 품목이바뀌는데 콜라나 사이다는 항상 있어요",
          "speaker_id": "2",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007775.1.10",
          "form": "저는 환타 포도 제일좋아해요",
          "original_form": "저는 환타 포도 제일좋아해요",
          "speaker_id": "2",
          "time": "20210822 10:30"
        },
        {
          "id": "MDRW2100007775.1.11",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210822 10:30"
        },
        {
          "id": "MDRW2100007775.1.12",
          "form": "어떤맛좋아하세요?",
          "original_form": "어떤맛좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 10:30"
        },
        {
          "id": "MDRW2100007775.1.13",
          "form": "포도 맛잘알",
          "original_form": "포도 맛잘알",
          "speaker_id": "1",
          "time": "20210822 10:30"
        },
        {
          "id": "MDRW2100007775.1.14",
          "form": "제일맛있어",
          "original_form": "제일맛있어",
          "speaker_id": "1",
          "time": "20210822 10:30"
        },
        {
          "id": "MDRW2100007775.1.15",
          "form": "음료 말고 다른건 뭐 사시나요",
          "original_form": "음료 말고 다른건 뭐 사시나요",
          "speaker_id": "1",
          "time": "20210822 10:30"
        },
        {
          "id": "MDRW2100007775.1.16",
          "form": "음 저는 주로 배고플 때 들어가서 삼김이나 샌드위치 사요",
          "original_form": "음 저는 주로 배고플 때 들어가서 삼김이나 샌드위치 사요",
          "speaker_id": "1",
          "time": "20210822 10:30"
        },
        {
          "id": "MDRW2100007775.1.17",
          "form": "저도 계란참치샌드위치 자주사먹어요!",
          "original_form": "저도 계란참치샌드위치 자주사먹어요!",
          "speaker_id": "2",
          "time": "20210822 10:31"
        },
        {
          "id": "MDRW2100007775.1.18",
          "form": "그거 완전 맛있잖아요",
          "original_form": "그거 완전 맛있잖아요",
          "speaker_id": "1",
          "time": "20210822 10:32"
        },
        {
          "id": "MDRW2100007775.1.19",
          "form": "맞아요 살살녹아요",
          "original_form": "맞아요 살살녹아요",
          "speaker_id": "2",
          "time": "20210822 10:32"
        },
        {
          "id": "MDRW2100007775.1.20",
          "form": "계란 약간 뭐라해야하지 으깨서 넣은거 너무 맛있어쇼",
          "original_form": "계란 약간 뭐라해야하지 으깨서 넣은거 너무 맛있어쇼",
          "speaker_id": "1",
          "time": "20210822 10:32"
        }
