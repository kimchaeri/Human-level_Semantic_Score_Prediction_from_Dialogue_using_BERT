        {
          "id": "MDRW2100033454.1.26",
          "form": "아고.. 답답하긋다",
          "original_form": "아고.. 답답하긋다",
          "speaker_id": "2",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100033454.1.27",
          "form": "그러니깐 머라하던데",
          "original_form": "그러니깐 머라하던데",
