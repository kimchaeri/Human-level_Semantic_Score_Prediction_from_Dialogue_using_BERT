      "utterance": [
        {
          "id": "MDRW2100045888.1.1",
          "form": "출근 해야지",
          "original_form": "출근 해야지",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045888.1.2",
          "form": "일단 아침을 먹는 중이야",
          "original_form": "일단 아침을 먹는 중이야",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045888.1.3",
          "form": "ㅇㅎ 아직 준비중이군",
          "original_form": "ㅇㅎ 아직 준비중이군",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045888.1.4",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045888.1.5",
          "form": "카톡 오ㅔ 업그레이드 중이냐",
          "original_form": "카톡 오ㅔ 업그레이드 중이냐",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045888.1.6",
          "form": "어쩐지 바쁜거같더라 ㅋㅋㅋ",
          "original_form": "어쩐지 바쁜거같더라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100045888.1.7",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100045888.1.8",
          "form": "아침은 늘 바쁘져..",
          "original_form": "아침은 늘 바쁘져..",
          "speaker_id": "2",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100045888.1.9",
          "form": "언니는 출근 하는 중이야?",
          "original_form": "언니는 출근 하는 중이야?",
          "speaker_id": "2",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100045888.1.10",
          "form": "ㅇㅇ 직장인에게 아침은 전쟁이지",
          "original_form": "ㅇㅇ 직장인에게 아침은 전쟁이지",
          "speaker_id": "1",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100045888.1.11",
          "form": "아님 재택인가 ㅇㅎ",
          "original_form": "아님 재택인가 ㅇㅎ",
          "speaker_id": "2",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100045888.1.12",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100045888.1.13",
          "form": "일어나는 것부터가 전쟁이야",
          "original_form": "일어나는 것부터가 전쟁이야",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100045888.1.14",
          "form": "나 어제 재택",
          "original_form": "나 어제 재택",
          "speaker_id": "1",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100045888.1.15",
          "form": "오늘은 출근함!",
          "original_form": "오늘은 출근함!",
          "speaker_id": "1",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100045888.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100045888.1.17",
          "form": "화이팅....",
          "original_form": "화이팅....",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100045888.1.18",
          "form": "너도 화이팅 ㅠㅠ",
          "original_form": "너도 화이팅 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100045888.1.19",
          "form": "아직 회사 도착도 안했는데",
          "original_form": "아직 회사 도착도 안했는데",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045888.1.20",
          "form": "퇴근하고싶다",
          "original_form": "퇴근하고싶다",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045888.1.21",
          "form": "ㅋㅋㅋㅋㅋ 출근도 안했는데",
          "original_form": "ㅋㅋㅋㅋㅋ 출근도 안했는데",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045888.1.22",
          "form": "퇴근하고싶다22",
          "original_form": "퇴근하고싶다22",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045888.1.23",
          "form": "오늘은 또 어떤 고객이 날 반길까 ㅎ",
          "original_form": "오늘은 또 어떤 고객이 날 반길까 ㅎ",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045888.1.24",
          "form": "코난 주제가를 틀어야",
          "original_form": "코난 주제가를 틀어야",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045888.1.25",
