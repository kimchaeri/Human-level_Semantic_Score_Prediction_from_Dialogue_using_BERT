      "utterance": [
        {
          "id": "MDRW2100013913.1.1",
          "form": "반려동물 키운적 있어요?",
          "original_form": "반려동물 키운적 있어요?",
          "speaker_id": "1",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100013913.1.2",
          "form": "예전에 금붕어 길렀었어요ㅎㅎ 기르시는 동물 있으세요?",
          "original_form": "예전에 금붕어 길렀었어요ㅎㅎ 기르시는 동물 있으세요?",
          "speaker_id": "2",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100013913.1.3",
          "form": "헛 진짜요",
          "original_form": "헛 진짜요",
          "speaker_id": "1",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100013913.1.4",
          "form": "금붕어 안키워봤어요",
          "original_form": "금붕어 안키워봤어요",
          "speaker_id": "1",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100013913.1.5",
          "form": "저는 햄스터요",
          "original_form": "저는 햄스터요",
          "speaker_id": "1",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100013913.1.6",
          "form": "오 귀여울거같아요ㅋㅋㅋ 안키워봐서..",
          "original_form": "오 귀여울거같아요ㅋㅋㅋ 안키워봐서..",
          "speaker_id": "2",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100013913.1.7",
          "form": "엄청 귀여웠어요",
          "original_form": "엄청 귀여웠어요",
          "speaker_id": "1",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100013913.1.8",
          "form": "쥐 무섭고 징그러운데",
          "original_form": "쥐 무섭고 징그러운데",
          "speaker_id": "1",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100013913.1.9",
          "form": "가족이 되니까 좋더라고요",
          "original_form": "가족이 되니까 좋더라고요",
          "speaker_id": "1",
          "time": "20210825 19:24"
        },
        {
          "id": "MDRW2100013913.1.10",
          "form": "많이들 그렇더라구요...진짜 소중해하는 친구 있었어요",
          "original_form": "많이들 그렇더라구요...진짜 소중해하는 친구 있었어요",
          "speaker_id": "2",
          "time": "20210825 19:25"
        },
        {
          "id": "MDRW2100013913.1.11",
          "form": "제가 그랬어요",
          "original_form": "제가 그랬어요",
          "speaker_id": "1",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100013913.1.12",
          "form": "죽으니까 너무 슬프더라고요",
          "original_form": "죽으니까 너무 슬프더라고요",
          "speaker_id": "1",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100013913.1.13",
          "form": "ㅠㅠ오래 살기 힘들다 하더라구요ㅠㅠㅠ마음 아프네요",
          "original_form": "ㅠㅠ오래 살기 힘들다 하더라구요ㅠㅠㅠ마음 아프네요",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100013913.1.14",
          "form": "네 죽고나서 죄책감이 들더라고요",
          "original_form": "네 죽고나서 죄책감이 들더라고요",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100013913.1.15",
          "form": "죽었다는 자체에",
          "original_form": "죽었다는 자체에",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100013913.1.16",
          "form": "ㅠㅠㅠㅠ최선을 다하셨을텐데요ㅠㅠ마음을 알거에요ㅜㅜ",
          "original_form": "ㅠㅠㅠㅠ최선을 다하셨을텐데요ㅠㅠ마음을 알거에요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100013913.1.17",
          "form": "ㅜ 미안해요",
          "original_form": "ㅜ 미안해요",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100013913.1.18",
          "form": "ㅠㅠ저도 마음이 아프네요",
          "original_form": "ㅠㅠ저도 마음이 아프네요",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100013913.1.19",
          "form": "다시는",
          "original_form": "다시는",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100013913.1.20",
          "form": "못키울거같아요",
          "original_form": "못키울거같아요",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100013913.1.21",
          "form": "수명 길어도 헤어질때 힘들어서요",
          "original_form": "수명 길어도 헤어질때 힘들어서요",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100013913.1.22",
          "form": "ㅠㅠ맞아요 그렇더라구요..",
          "original_form": "ㅠㅠ맞아요 그렇더라구요..",
          "speaker_id": "2",
          "time": "20210825 20:05"
        }
      ]
