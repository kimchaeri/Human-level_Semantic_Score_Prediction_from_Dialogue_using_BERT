      "utterance": [
        {
          "id": "MDRW2100030258.1.1",
          "form": "진짜... 솔직히 제가 살면서 일본배우덕질을..하게될줄을몰랐는데...",
          "original_form": "진짜... 솔직히 제가 살면서 일본배우덕질을..하게될줄을몰랐는데...",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030258.1.2",
          "form": "이게..증말 ㅠㅠ",
          "original_form": "이게..증말 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030258.1.3",
          "form": "일본어배워야하나...2222",
          "original_form": "일본어배워야하나...2222",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030258.1.4",
          "form": "아 그니까요 -.- 이번에 시마 자이언티랑",
          "original_form": "아 그니까요 -.- 이번에 시마 자이언티랑",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030258.1.5",
          "form": "앨범 작업 한다면서요?",
          "original_form": "앨범 작업 한다면서요?",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030258.1.6",
          "form": "ㅇㅔ",
          "original_form": "ㅇㅔ",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030258.1.7",
          "form": "예?",
          "original_form": "예?",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030258.1.8",
          "form": "호시노겐이요????",
          "original_form": "호시노겐이요????",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030258.1.9",
          "form": "진짜요???????",
          "original_form": "진짜요???????",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030258.1.10",
          "form": "ㄴㅇㄱ",
          "original_form": "ㄴㅇㄱ",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030258.1.11",
          "form": "어억 저.. 그런 걸로 알고 잇었는데",
          "original_form": "어억 저.. 그런 걸로 알고 잇었는데",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030258.1.12",
          "form": "아니면... 어저지",
          "original_form": "아니면... 어저지",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030258.1.13",
          "form": "ㅎ.ㅎ..  맞을..걸요?",
          "original_form": "ㅎ.ㅎ..  맞을..걸요?",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030258.1.14",
          "form": "제가 잠깐 딴 덕질하는사이에무슨일이",
          "original_form": "제가 잠깐 딴 덕질하는사이에무슨일이",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030258.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030258.1.16",
          "form": "헐 마블에",
          "original_form": "헐 마블에",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030258.1.17",
          "form": "맞네요",
          "original_form": "맞네요",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030258.1.18",
          "form": "맞어요!!! 마블 어쩌고 노래란대요",
          "original_form": "맞어요!!! 마블 어쩌고 노래란대요",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030258.1.19",
          "form": "헛 맞아요 마블!!",
          "original_form": "헛 맞아요 마블!!",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030258.1.20",
          "form": "아 샹칭0",
          "original_form": "아 샹칭0",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030258.1.21",
          "form": "샹치에...세상에....",
          "original_form": "샹치에...세상에....",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030258.1.22",
          "form": "와......이게...정말",
          "original_form": "와......이게...정말",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030258.1.23",
          "form": "오늘 낮에 저 깜짝 놀랐었잔ㄹ아요!!",
          "original_form": "오늘 낮에 저 깜짝 놀랐었잔ㄹ아요!!",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030258.1.24",
          "form": "자이언티 부럽다...",
          "original_form": "자이언티 부럽다...",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030258.1.25",
          "form": "진짜.....자이언티부럽다............",
          "original_form": "진짜.....자이언티부럽다............",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030258.1.26",
          "form": "ㅜㅠㅠㅠㅠㅠ",
          "original_form": "ㅜㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030258.1.27",
          "form": "호시노겐...노래. 좋아요....ㅠㅠ",
          "original_form": "호시노겐...노래. 좋아요....ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030258.1.28",
          "form": "겐쨩 천년만년 음악해줘...",
          "original_form": "겐쨩 천년만년 음악해줘...",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030258.1.29",
          "form": "아 저 아직 시마 본업은 덕질을 못해서..!",
          "original_form": "아 저 아직 시마 본업은 덕질을 못해서..!",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030258.1.30",
          "form": "추천곡 있으신가요 ㅎ.ㅎ",
          "original_form": "추천곡 있으신가요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030258.1.31",
          "form": "일본노래는 레몬이랑 감전바껭 몰라요",
          "original_form": "일본노래는 레몬이랑 감전바껭 몰라요",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030258.1.32",
          "form": "요네즈 켄시 노래 좋죠 ㅋㅋㅋㅋ!!",
          "original_form": "요네즈 켄시 노래 좋죠 ㅋㅋㅋㅋ!!",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030258.1.33",
          "form": "추천곡 많은데..일단 코이! 그 시마 본체 나온 드라마 도망치는건 부끄럽지만 도움이 된다 이거 ost에용!",
          "original_form": "추천곡 많은데..일단 코이! 그 시마 본체 나온 드라마 도망치는건 부끄럽지만 도움이 된다 이거 ost에용!",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030258.1.34",
          "form": "글구 초ㅣ근에 나온 不思議(후시기) 도 좋아요!",
          "original_form": "글구 초ㅣ근에 나온 不思議(후시기) 도 좋아요!",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030258.1.35",
          "form": "후시기도 드라마 ost인데 제목이 꾸미는 사랑에는 이유가 있다 구용! 요 드라마두 재밋어욤!!",
          "original_form": "후시기도 드라마 ost인데 제목이 꾸미는 사랑에는 이유가 있다 구용! 요 드라마두 재밋어욤!!",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030258.1.36",
          "form": "헐헐 지금 캡처했습니다! 당장 들어볼게요 ㅎ.ㅎ !!",
          "original_form": "헐헐 지금 캡처했습니다! 당장 들어볼게요 ㅎ.ㅎ !!",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030258.1.37",
          "form": "그 부끄럽지만에서 만난 배우랑 결혼한 건가요?!",
          "original_form": "그 부끄럽지만에서 만난 배우랑 결혼한 건가요?!",
