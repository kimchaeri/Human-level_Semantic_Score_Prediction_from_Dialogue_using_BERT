      "utterance": [
        {
          "id": "MDRW2100019742.1.1",
          "form": "저도 김희진 선수 ㅠㅠ 넘 좋아요 ㅋㅋㅋ",
          "original_form": "저도 김희진 선수 ㅠㅠ 넘 좋아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019742.1.2",
          "form": "귀여워욬ㅋㅋ",
          "original_form": "귀여워욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019742.1.3",
          "form": "그쵸 왕 큰 곰돌이 같아요..",
          "original_form": "그쵸 왕 큰 곰돌이 같아요..",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019742.1.4",
          "form": "아 맞아요 ㅋㅋㅋ 곰돌이 !",
          "original_form": "아 맞아요 ㅋㅋㅋ 곰돌이 !",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019742.1.5",
          "form": "겉모습은 완전 언닌데",
          "original_form": "겉모습은 완전 언닌데",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019742.1.6",
          "form": "성격은 귀여운 바보 같아요 ㅋㅋ",
          "original_form": "성격은 귀여운 바보 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019742.1.7",
          "form": "네ㅋㅋㅋ그게 매력이에요ㅠㅠ",
          "original_form": "네ㅋㅋㅋ그게 매력이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019742.1.8",
          "form": "막 직관 그런것도 관심 생기셨겠네요!",
          "original_form": "막 직관 그런것도 관심 생기셨겠네요!",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019742.1.9",
          "form": "네..! 코로나 끝나면 직관 갈 거예요ㅠㅠ",
          "original_form": "네..! 코로나 끝나면 직관 갈 거예요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019742.1.10",
          "form": "그날만 기다리고 있답니다",
          "original_form": "그날만 기다리고 있답니다",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019742.1.11",
          "form": "그때쯤 되면 김희진선수도 회복 잘하셨을꺼에요 !",
          "original_form": "그때쯤 되면 김희진선수도 회복 잘하셨을꺼에요 !",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019742.1.12",
          "form": "재활도 제대로 못하셨잖아요 ㅜㅜ",
          "original_form": "재활도 제대로 못하셨잖아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019742.1.13",
          "form": "그러니까요.. 무릎 안 좋은데ㅠㅠㅠㅠ",
          "original_form": "그러니까요.. 무릎 안 좋은데ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 22:18"
        },
        {
          "id": "MDRW2100019742.1.14",
          "form": "경기 시간 맞춰서 못 본 날에는 짤 같은 걸로 보는데",
          "original_form": "경기 시간 맞춰서 못 본 날에는 짤 같은 걸로 보는데",
          "speaker_id": "2",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019742.1.15",
          "form": "마음으로 울면서 보고 있어요..",
          "original_form": "마음으로 울면서 보고 있어요..",
          "speaker_id": "2",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019742.1.16",
          "form": "ㅜㅜ 요즘은 리그에서 어때요?",
          "original_form": "ㅜㅜ 요즘은 리그에서 어때요?",
          "speaker_id": "1",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100019742.1.17",
          "form": "잘하시구 계신가요!",
          "original_form": "잘하시구 계신가요!",
          "speaker_id": "1",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100019742.1.18",
          "form": "네!! 무릎 안 좋은데 활약 대단하세요!",
          "original_form": "네!! 무릎 안 좋은데 활약 대단하세요!",
          "speaker_id": "2",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019742.1.19",
          "form": "멋있는데 무릎 아픈 거 보면 속상하기도 해요 ㅎㅎㅠ",
          "original_form": "멋있는데 무릎 아픈 거 보면 속상하기도 해요 ㅎㅎㅠ",
          "speaker_id": "2",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019742.1.20",
          "form": "와 헐 대박이네요 ㅠㅠ",
          "original_form": "와 헐 대박이네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019742.1.21",
          "form": "무릎 안아프면 훨씬 날라다닐텐데 ㅠㅠ",
          "original_form": "무릎 안아프면 훨씬 날라다닐텐데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019742.1.22",
          "form": "리그 언제 끝나요 ??",
          "original_form": "리그 언제 끝나요 ??",
          "speaker_id": "1",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019742.1.23",
          "form": "29일이요!",
          "original_form": "29일이요!",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019742.1.24",
          "form": "진짜..코로나 아니었으면 이미 직관 갔을 거예요..",
          "original_form": "진짜..코로나 아니었으면 이미 직관 갔을 거예요..",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019742.1.25",
          "form": "헐 곧 끝나네요 ㅠㅠ",
          "original_form": "헐 곧 끝나네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019742.1.26",
          "form": "맞아요 배구 직관이 진짜 재밌다던데 ㅠㅠ",
          "original_form": "맞아요 배구 직관이 진짜 재밌다던데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019742.1.27",
          "form": "제 친구도 종종 가더라구요!",
          "original_form": "제 친구도 종종 가더라구요!",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019742.1.28",
          "form": "그렇군요ㅠㅠㅠ저도 꼭 가봐야겠어요",
          "original_form": "그렇군요ㅠㅠㅠ저도 꼭 가봐야겠어요",
          "speaker_id": "2",
          "time": "20210827 22:39"
        },
        {
          "id": "MDRW2100019742.1.29",
          "form": "이번 올림픽으로 여름 재밌게 보냈어요..",
          "original_form": "이번 올림픽으로 여름 재밌게 보냈어요..",
          "speaker_id": "2",
          "time": "20210827 22:39"
        }
      ]
