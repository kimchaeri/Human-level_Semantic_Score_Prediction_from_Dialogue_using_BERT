      "utterance": [
        {
          "id": "MDRW2100040976.1.1",
          "form": "음 아니요 못본거같아요!",
          "original_form": "음 아니요 못본거같아요!",
          "speaker_id": "2",
          "time": "20210905 14:19"
        },
        {
          "id": "MDRW2100040976.1.2",
          "form": "유명한 영화있나요?",
          "original_form": "유명한 영화있나요?",
          "speaker_id": "2",
          "time": "20210905 14:19"
        },
        {
          "id": "MDRW2100040976.1.3",
          "form": "아는여자 김씨표류기 재밌어요!!",
          "original_form": "아는여자 김씨표류기 재밌어요!!",
          "speaker_id": "1",
          "time": "20210905 14:22"
        },
        {
          "id": "MDRW2100040976.1.4",
          "form": "아 둘다 이름은 들어봤어요!!",
          "original_form": "아 둘다 이름은 들어봤어요!!",
          "speaker_id": "2",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100040976.1.5",
          "form": "다음에 봐볼게요!",
          "original_form": "다음에 봐볼게요!",
          "speaker_id": "2",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100040976.1.6",
          "form": "둘다 진짜 볼만한 영화에요!",
          "original_form": "둘다 진짜 볼만한 영화에요!",
          "speaker_id": "1",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100040976.1.7",
          "form": "넷플릭스는 이용하세요?",
          "original_form": "넷플릭스는 이용하세요?",
          "speaker_id": "2",
          "time": "20210905 15:23"
        },
        {
          "id": "MDRW2100040976.1.8",
          "form": "네네 넷플 친구랑 이용중이에요 ㅋㅋ 넷플이랑 왓챠 나눠쓰고 네이버멤버십으로 티빙써요 ㅋㅋ",
          "original_form": "네네 넷플 친구랑 이용중이에요 ㅋㅋ 넷플이랑 왓챠 나눠쓰고 네이버멤버십으로 티빙써요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:25"
        },
        {
          "id": "MDRW2100040976.1.9",
          "form": "ㅋㅋㅋㅋㅋ저두 넷플이랑 네이버플러스로 티빙써요",
          "original_form": "ㅋㅋㅋㅋㅋ저두 넷플이랑 네이버플러스로 티빙써요",
          "speaker_id": "2",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100040976.1.10",
          "form": "디피 보셧나요!",
          "original_form": "디피 보셧나요!",
          "speaker_id": "2",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100040976.1.11",
          "form": "안그래도 다들 재밌다길래 보려고 하고있어요 ㅋㅋㅋ보셨어요?",
          "original_form": "안그래도 다들 재밌다길래 보려고 하고있어요 ㅋㅋㅋ보셨어요?",
          "speaker_id": "1",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100040976.1.12",
          "form": "네네 재밌더라구요 ㅋㅋㅋㅋ별로 소재가 안끌렸는데",
          "original_form": "네네 재밌더라구요 ㅋㅋㅋㅋ별로 소재가 안끌렸는데",
          "speaker_id": "2",
          "time": "20210905 20:15"
        },
        {
          "id": "MDRW2100040976.1.13",
          "form": "1화보니까 몰입감장난아니구 계속보게되요 ㅋㅋㅋㅋ",
          "original_form": "1화보니까 몰입감장난아니구 계속보게되요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:15"
        },
        {
          "id": "MDRW2100040976.1.14",
          "form": "저는 잔인한거일까봐 좀 그랬는데 좀 코미디도있다면서요?",
          "original_form": "저는 잔인한거일까봐 좀 그랬는데 좀 코미디도있다면서요?",
          "speaker_id": "1",
          "time": "20210905 20:16"
        },
        {
          "id": "MDRW2100040976.1.15",
          "form": "아 네 ㅋㅋㅋㅋ거기 나오는 구교환배우분 웃겨요",
          "original_form": "아 네 ㅋㅋㅋㅋ거기 나오는 구교환배우분 웃겨요",
          "speaker_id": "2",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100040976.1.16",
          "form": "잔인하기보다는 좀 폭력적인 느낌이 있어요 ㅠㅠ",
          "original_form": "잔인하기보다는 좀 폭력적인 느낌이 있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100040976.1.17",
          "form": "보면서 좀 너무하다 싶은 정도까진 아닌거죠?",
          "original_form": "보면서 좀 너무하다 싶은 정도까진 아닌거죠?",
          "speaker_id": "1",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100040976.1.18",
          "form": "옛날에 군대 괴롭힘 문제 현실고증 느낌이라 좀 불편하긴해요 ㅠㅠ",
          "original_form": "옛날에 군대 괴롭힘 문제 현실고증 느낌이라 좀 불편하긴해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100040976.1.19",
          "form": "보면서 주의 해야겠군요 ㅜ",
          "original_form": "보면서 주의 해야겠군요 ㅜ",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100040976.1.20",
          "form": "디피랑 미치지않고서야 꼭 볼거에요! ㅋㅋ 또 뭐 재밌나요",
          "original_form": "디피랑 미치지않고서야 꼭 볼거에요! ㅋㅋ 또 뭐 재밌나요",
          "speaker_id": "1",
          "time": "20210905 20:25"
        }
      ]
    }
  ]
