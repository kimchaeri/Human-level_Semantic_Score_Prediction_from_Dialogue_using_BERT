      "utterance": [
        {
          "id": "MDRW2100040244.1.1",
          "form": "ㅋㅋ그건 저녁쯤?? 아점 먹고 나왔어요",
          "original_form": "ㅋㅋ그건 저녁쯤?? 아점 먹고 나왔어요",
          "speaker_id": "2",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100040244.1.2",
          "form": "북촌맛집 가셨나요?",
          "original_form": "북촌맛집 가셨나요?",
          "speaker_id": "2",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100040244.1.3",
          "form": "음..인사동갓는데 별루엿어요",
          "original_form": "음..인사동갓는데 별루엿어요",
          "speaker_id": "1",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100040244.1.4",
          "form": "비싸요",
          "original_form": "비싸요",
          "speaker_id": "1",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100040244.1.5",
          "form": "솥밥",
          "original_form": "솥밥",
          "speaker_id": "1",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100040244.1.6",
          "form": "아...거기 물가 어마무시하다고..ㅜㅠ",
          "original_form": "아...거기 물가 어마무시하다고..ㅜㅠ",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100040244.1.7",
          "form": "실망햇네요",
          "original_form": "실망햇네요",
          "speaker_id": "1",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100040244.1.8",
          "form": "라떼먹늠중",
          "original_form": "라떼먹늠중",
          "speaker_id": "1",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100040244.1.9",
          "form": "ㅋㅋ 아인슈패너 먹고싶네요",
          "original_form": "ㅋㅋ 아인슈패너 먹고싶네요",
          "speaker_id": "2",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100040244.1.10",
          "form": "ㅋㅋㅋ크림마싯죵",
          "original_form": "ㅋㅋㅋ크림마싯죵",
          "speaker_id": "1",
          "time": "20210904 15:01"
        },
        {
          "id": "MDRW2100040244.1.11",
          "form": "맞아요 컴포즈 아인슈패너 요즘 맛있다던데 조만간 먹어보려고요",
          "original_form": "맞아요 컴포즈 아인슈패너 요즘 맛있다던데 조만간 먹어보려고요",
          "speaker_id": "2",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040244.1.12",
          "form": "태양커피 아세요?",
          "original_form": "태양커피 아세요?",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040244.1.13",
          "form": "오 그런가요 저도 보이면 도전 ㅎ",
          "original_form": "오 그런가요 저도 보이면 도전 ㅎ",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040244.1.14",
          "form": "몰라요",
          "original_form": "몰라요",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040244.1.15",
          "form": "방배에 있는 카펜데 맛있는데 양이ㅜㅠ",
          "original_form": "방배에 있는 카펜데 맛있는데 양이ㅜㅠ",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040244.1.16",
          "form": "아인슈패너맛집이에요",
          "original_form": "아인슈패너맛집이에요",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040244.1.17",
          "form": "오호? 가바야겟네요",
          "original_form": "오호? 가바야겟네요",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040244.1.18",
          "form": "넵 사람이 많은데 오픈때 갔어요 대부분 투고하더라고요",
          "original_form": "넵 사람이 많은데 오픈때 갔어요 대부분 투고하더라고요",
          "speaker_id": "2",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040244.1.19",
          "form": "아 찐맛집",
          "original_form": "아 찐맛집",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040244.1.20",
          "form": "근데 다른 카페도 아인슈패너는 다 평균이상같아요",
          "original_form": "근데 다른 카페도 아인슈패너는 다 평균이상같아요",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040244.1.21",
          "form": "이디야제외",
          "original_form": "이디야제외",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040244.1.22",
          "form": "ㅋㅋㅋ 이디야...맛없더라구요",
          "original_form": "ㅋㅋㅋ 이디야...맛없더라구요",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040244.1.23",
          "form": "네...진짜 처참했어요ㅜㅠ",
          "original_form": "네...진짜 처참했어요ㅜㅠ",
          "speaker_id": "2",
