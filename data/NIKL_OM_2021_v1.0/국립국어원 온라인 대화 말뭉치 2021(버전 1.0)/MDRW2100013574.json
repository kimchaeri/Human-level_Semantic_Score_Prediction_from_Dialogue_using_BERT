      "utterance": [
        {
          "id": "MDRW2100013574.1.1",
          "form": "미용실 언제 가보셨어요?",
          "original_form": "미용실 언제 가보셨어요?",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013574.1.2",
          "form": "몇년전에 가본게 다에요 ㅋㅋ",
          "original_form": "몇년전에 가본게 다에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013574.1.3",
          "form": "학교에있던 미용실에서 몇천원주고 금방잘라주셧엇던 기억이 나네요.",
          "original_form": "학교에있던 미용실에서 몇천원주고 금방잘라주셧엇던 기억이 나네요.",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013574.1.4",
          "form": "비싼가위여서 뭐가 다르긴하더라고요",
          "original_form": "비싼가위여서 뭐가 다르긴하더라고요",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013574.1.5",
          "form": "이제 안가요?",
          "original_form": "이제 안가요?",
          "speaker_id": "2",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013574.1.6",
          "form": "머리 엄청 길겠네요",
          "original_form": "머리 엄청 길겠네요",
          "speaker_id": "2",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013574.1.7",
          "form": "저는 전문 미용사가 있답니다",
          "original_form": "저는 전문 미용사가 있답니다",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013574.1.8",
          "form": "저만 잘라줘요",
          "original_form": "저만 잘라줘요",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013574.1.9",
          "form": "오 그렇군요 일년에 몇번자르시는데요?",
          "original_form": "오 그렇군요 일년에 몇번자르시는데요?",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013574.1.10",
          "form": "1번요",
          "original_form": "1번요",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013574.1.11",
          "form": "그럼 지금 좀 길으시겟네요",
          "original_form": "그럼 지금 좀 길으시겟네요",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013574.1.12",
          "form": "원래 2년에 한번 자르고싶었어요",
          "original_form": "원래 2년에 한번 자르고싶었어요",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013574.1.13",
          "form": "별로 안길어요",
          "original_form": "별로 안길어요",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013574.1.14",
          "form": "그러면 목에 부담많이갈것같은데요",
          "original_form": "그러면 목에 부담많이갈것같은데요",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013574.1.15",
          "form": "샴푸도 많이써야하고요",
          "original_form": "샴푸도 많이써야하고요",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013574.1.16",
          "form": "어깨좀 넘는정도",
          "original_form": "어깨좀 넘는정도",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013574.1.17",
          "form": "거짓말마시길",
          "original_form": "거짓말마시길",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013574.1.18",
          "form": "전 카디비나 킴카다시안 머리길이가 좋네요",
          "original_form": "전 카디비나 킴카다시안 머리길이가 좋네요",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013574.1.19",
          "form": "저는 머리묶일정도로의 단발머리를 선호해요",
          "original_form": "저는 머리묶일정도로의 단발머리를 선호해요",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013574.1.20",
          "form": "거짓말아님요",
          "original_form": "거짓말아님요",
          "speaker_id": "2",
          "time": "20210825 15:28"
        }
      ]
    }
