      "utterance": [
        {
          "id": "MDRW2100004799.1.1",
          "form": "옹",
          "original_form": "옹",
          "speaker_id": "2",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004799.1.2",
          "form": "올림픽 보셨나요",
          "original_form": "올림픽 보셨나요",
          "speaker_id": "1",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004799.1.3",
          "form": "네!! 완전 과몰입해서 봤어요",
          "original_form": "네!! 완전 과몰입해서 봤어요",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004799.1.4",
          "form": "어떤 종목 주로 보셧어요? 전 여자배구",
          "original_form": "어떤 종목 주로 보셧어요? 전 여자배구",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004799.1.5",
          "form": "양궁도 봣믄데 양궁은 국내 리그가 없대요",
          "original_form": "양궁도 봣믄데 양궁은 국내 리그가 없대요",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004799.1.6",
          "form": "여자배구랑 양궁, 탁구, 태권도 등등등 시간 되면 다 봤던 거 같아요!",
          "original_form": "여자배구랑 양궁, 탁구, 태권도 등등등 시간 되면 다 봤던 거 같아요!",
          "speaker_id": "2",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004799.1.7",
          "form": "양궁 국내리그가 없어요!? 그럼 전국체전(?)만 있는 건가요??",
          "original_form": "양궁 국내리그가 없어요!? 그럼 전국체전(?)만 있는 건가요??",
          "speaker_id": "2",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004799.1.8",
          "form": "보면 리그도 계속 보고싶고 관심이 생기도라구요",
          "original_form": "보면 리그도 계속 보고싶고 관심이 생기도라구요",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004799.1.9",
          "form": "배구처럼 그런게 없고 체전만 잇나보더라구요",
          "original_form": "배구처럼 그런게 없고 체전만 잇나보더라구요",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004799.1.10",
          "form": "맞아요 그래서 23일부터 여자배구 리그한다해서 보려구요!",
          "original_form": "맞아요 그래서 23일부터 여자배구 리그한다해서 보려구요!",
          "speaker_id": "2",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004799.1.11",
          "form": "아까 찾아봣다 없대서 놀랏엉ㅅ",
          "original_form": "아까 찾아봣다 없대서 놀랏엉ㅅ",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004799.1.12",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004799.1.13",
          "form": "아쉽네요 ㅠㅠ",
          "original_form": "아쉽네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004799.1.14",
          "form": "코보컵 기대돼요",
          "original_form": "코보컵 기대돼요",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004799.1.15",
          "form": "저도요! 응원하는 팀은 따로 있으세요?? 저는 아직 못 정했어요",
          "original_form": "저도요! 응원하는 팀은 따로 있으세요?? 저는 아직 못 정했어요",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004799.1.16",
          "form": "저는 인삼공사요!",
          "original_form": "저는 인삼공사요!",
          "speaker_id": "1",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004799.1.17",
          "form": "소영선배따라서 인삼으로 정했어요",
          "original_form": "소영선배따라서 인삼으로 정했어요",
          "speaker_id": "1",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004799.1.18",
          "form": "인삼공사 직관가면 인삼액(?) 준대요!",
          "original_form": "인삼공사 직관가면 인삼액(?) 준대요!",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004799.1.19",
          "form": "ㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004799.1.20",
          "form": "브이리그 직관가고 싶어요",
          "original_form": "브이리그 직관가고 싶어요",
          "speaker_id": "1",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004799.1.21",
          "form": "저두요ㅜㅜ 서울 살면 직관 예매했을텐데 지방이라 아쉬워요 ㅠㅠ",
          "original_form": "저두요ㅜㅜ 서울 살면 직관 예매했을텐데 지방이라 아쉬워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004799.1.22",
          "form": "ㅜㅜ 서울이면 지에스..?!",
          "original_form": "ㅜㅜ 서울이면 지에스..?!",
          "speaker_id": "1",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004799.1.23",
          "form": "그럴걸요!?",
          "original_form": "그럴걸요!?",
          "speaker_id": "2",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004799.1.24",
          "form": "졔스에 오지영선수랑 안혜진선수 잇자나요!",
          "original_form": "졔스에 오지영선수랑 안혜진선수 잇자나요!",
          "speaker_id": "1",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004799.1.25",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "2",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004799.1.26",
          "form": "제발 유관중이엇으면 좋겟어요",
          "original_form": "제발 유관중이엇으면 좋겟어요",
          "speaker_id": "1",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004799.1.27",
          "form": "선수들 직접 보고싶어요",
          "original_form": "선수들 직접 보고싶어요",
          "speaker_id": "1",
          "time": "20210821 18:53"
        }
      ]
    }
