      "utterance": [
        {
          "id": "MDRW2100002208.1.1",
          "form": "응 손목이 고생했지.",
          "original_form": "응 손목이 고생했지.",
          "speaker_id": "2",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002208.1.2",
          "form": "그 아이스크림은 맛있지만 아무래도 예전 일터라서 가기가 쉽지 않네.",
          "original_form": "그 아이스크림은 맛있지만 아무래도 예전 일터라서 가기가 쉽지 않네.",
          "speaker_id": "2",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002208.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그립지는않아?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그립지는않아?",
          "speaker_id": "1",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002208.1.4",
          "form": "전혀 그립지 않아.",
          "original_form": "전혀 그립지 않아.",
          "speaker_id": "2",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002208.1.5",
          "form": "나는 그런 서비스직을 즐기면서 하는 사람들이 신기하네.",
          "original_form": "나는 그런 서비스직을 즐기면서 하는 사람들이 신기하네.",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002208.1.6",
          "form": "그때 어떤게 제일 힘들었어?",
          "original_form": "그때 어떤게 제일 힘들었어?",
          "speaker_id": "1",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002208.1.7",
          "form": "게임 같다는 사람들도 있더라",
          "original_form": "게임 같다는 사람들도 있더라",
          "speaker_id": "1",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002208.1.8",
          "form": "계속 서있으니 허리가 아프고",
          "original_form": "계속 서있으니 허리가 아프고",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002208.1.9",
          "form": "아이스크림을 푸면 손목이 아파.",
          "original_form": "아이스크림을 푸면 손목이 아파.",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002208.1.10",
          "form": "그리고 혼자 일하는데 손님은 많으니까 손님들이 재촉해.",
          "original_form": "그리고 혼자 일하는데 손님은 많으니까 손님들이 재촉해.",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002208.1.11",
          "form": "아아 앉아있을 수 없는 알바였지",
          "original_form": "아아 앉아있을 수 없는 알바였지",
          "speaker_id": "1",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002208.1.12",
          "form": "가끔은 진상 할아버지 아저씨들도 온단다.",
          "original_form": "가끔은 진상 할아버지 아저씨들도 온단다.",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002208.1.13",
          "form": "손님들은 성격이 너무 급해",
          "original_form": "손님들은 성격이 너무 급해",
          "speaker_id": "1",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002208.1.14",
          "form": "밥 먹을 시간도 없었어.",
          "original_form": "밥 먹을 시간도 없었어.",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002208.1.15",
          "form": "그렇게 바쁘면 전날오든가",
          "original_form": "그렇게 바쁘면 전날오든가",
          "speaker_id": "1",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002208.1.16",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002208.1.17",
          "form": "제발. 전날 와.",
          "original_form": "제발. 전날 와.",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002208.1.18",
          "form": "ㅠㅠ밥먹으면 보통 뭐먹었어",
          "original_form": "ㅠㅠ밥먹으면 보통 뭐먹었어",
          "speaker_id": "1",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002208.1.19",
          "form": "편의점에 가서 김밥을 사온단다.",
          "original_form": "편의점에 가서 김밥을 사온단다.",
          "speaker_id": "2",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002208.1.20",
          "form": "가끔은 점장님이 사주시곤했지.",
          "original_form": "가끔은 점장님이 사주시곤했지.",
          "speaker_id": "2",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002208.1.21",
          "form": "김밥 냄새나는건 괜찮아?",
          "original_form": "김밥 냄새나는건 괜찮아?",
          "speaker_id": "1",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002208.1.22",
          "form": "카드를 주셨지만 양심이 있어서 김밥만 사왔어.",
          "original_form": "카드를 주셨지만 양심이 있어서 김밥만 사왔어.",
          "speaker_id": "2",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002208.1.23",
          "form": "문이 없고 병원 안이라 괜찮아.",
          "original_form": "문이 없고 병원 안이라 괜찮아.",
          "speaker_id": "2",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002208.1.24",
          "form": "가게에 문이 없었거든",
          "original_form": "가게에 문이 없었거든",
          "speaker_id": "2",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002208.1.25",
          "form": "ㅠㅠ다시 돌아간다면 똑같이 김밥살꺼야..?",
          "original_form": "ㅠㅠ다시 돌아간다면 똑같이 김밥살꺼야..?",
          "speaker_id": "1",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002208.1.26",
          "form": "ㅌㅋㅋㅋㅋㅋ그게 무슨말이야ㅠ",
          "original_form": "ㅌㅋㅋㅋㅋㅋ그게 무슨말이야ㅠ",
          "speaker_id": "2",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002208.1.27",
          "form": "다시 돌아간다면 더 비싼걸 먹자",
          "original_form": "다시 돌아간다면 더 비싼걸 먹자",
          "speaker_id": "1",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002208.1.28",
          "form": "그래. 어차피 다시 돌아갈 일 없는 곳이니",
          "original_form": "그래. 어차피 다시 돌아갈 일 없는 곳이니",
          "speaker_id": "2",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002208.1.29",
          "form": "다시 돌아간다면 앞뒤안보고 비싼 퀴즈너스를 먹겠어.",
          "original_form": "다시 돌아간다면 앞뒤안보고 비싼 퀴즈너스를 먹겠어.",
          "speaker_id": "2",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002208.1.30",
          "form": "단호하네 엄청 힘들었나봐",
          "original_form": "단호하네 엄청 힘들었나봐",
          "speaker_id": "1",
          "time": "20210820 00:34"
        }
      ]
