      "utterance": [
        {
          "id": "MDRW2100024931.1.1",
          "form": "여행 좋아하세요",
          "original_form": "여행 좋아하세요",
          "speaker_id": "1",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100024931.1.2",
          "form": "큰여행은 안하고 맛집좋아해요",
          "original_form": "큰여행은 안하고 맛집좋아해요",
          "speaker_id": "2",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100024931.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100024931.1.4",
          "form": "전 둘다요.",
          "original_form": "전 둘다요.",
          "speaker_id": "1",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100024931.1.5",
          "form": "최근에는 제주도 다녀왔어요",
          "original_form": "최근에는 제주도 다녀왔어요",
          "speaker_id": "1",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100024931.1.6",
          "form": "우와 최근 제주도비싸던데 그래도 제주ㄷ가서 바람쬐면",
          "original_form": "우와 최근 제주도비싸던데 그래도 제주ㄷ가서 바람쬐면",
          "speaker_id": "2",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100024931.1.7",
          "form": "좋다더라구요 부럽네요 저도 가고 싶어요",
          "original_form": "좋다더라구요 부럽네요 저도 가고 싶어요",
          "speaker_id": "2",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100024931.1.8",
          "form": "맛집은 최근에 어디 다녀오셨나요",
          "original_form": "맛집은 최근에 어디 다녀오셨나요",
          "speaker_id": "1",
          "time": "20210831 21:04"
        },
        {
          "id": "MDRW2100024931.1.9",
          "form": "전 을지로 가게 몇군데요",
          "original_form": "전 을지로 가게 몇군데요",
          "speaker_id": "2",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100024931.1.10",
          "form": "촙촙 줄리아 등,,",
          "original_form": "촙촙 줄리아 등,,",
          "speaker_id": "2",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100024931.1.11",
          "form": "가보셨나요?",
          "original_form": "가보셨나요?",
          "speaker_id": "2",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100024931.1.12",
          "form": "을지로는 가깝긴한데 잘안가봤어요",
          "original_form": "을지로는 가깝긴한데 잘안가봤어요",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024931.1.13",
          "form": "촙촙에선 뭘 파나요",
          "original_form": "촙촙에선 뭘 파나요",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024931.1.14",
          "form": "쌀국수인데 칠리쌀국수가 특이메뉴에요",
          "original_form": "쌀국수인데 칠리쌀국수가 특이메뉴에요",
          "speaker_id": "2",
          "time": "20210831 21:10"
        },
        {
          "id": "MDRW2100024931.1.15",
          "form": "오 줄리아는요?",
          "original_form": "오 줄리아는요?",
          "speaker_id": "1",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100024931.1.16",
          "form": "이탈리안 레스토랑 느낌이네요 ㅋㅋ",
          "original_form": "이탈리안 레스토랑 느낌이네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100024931.1.17",
          "form": "퓨전 중국집이요 ㅋㅋ",
          "original_form": "퓨전 중국집이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100024931.1.18",
          "form": "딱 퓨전중국집,,",
          "original_form": "딱 퓨전중국집,,",
          "speaker_id": "2",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100024931.1.19",
          "form": "렉이 걸렸나 서버오류인가 지금 계속 말이 가다말다 하네요ㅜ",
          "original_form": "렉이 걸렸나 서버오류인가 지금 계속 말이 가다말다 하네요ㅜ",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100024931.1.20",
          "form": "엥 그렇군요",
          "original_form": "엥 그렇군요",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100024931.1.21",
          "form": "줄리아 추천메뉴는 뭔가요",
          "original_form": "줄리아 추천메뉴는 뭔가요",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100024931.1.22",
          "form": "안주로는 삼겹살 튀김이요",
          "original_form": "안주로는 삼겹살 튀김이요",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100024931.1.23",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100024931.1.24",
          "form": "꼭 기억해두겠습니닿ㅎ",
          "original_form": "꼭 기억해두겠습니닿ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:06"
        }
      ]
    }
