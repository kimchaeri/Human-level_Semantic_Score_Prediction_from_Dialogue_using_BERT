      "utterance": [
        {
          "id": "MDRW2100031998.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031998.1.2",
          "form": "네 안녕하세요~",
          "original_form": "네 안녕하세요~",
          "speaker_id": "2",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100031998.1.3",
          "form": "여행지 기억에 남는곳 있으세요?",
          "original_form": "여행지 기억에 남는곳 있으세요?",
          "speaker_id": "1",
          "time": "20210905 10:45"
        },
        {
          "id": "MDRW2100031998.1.4",
          "form": "몇년전에 친구들이랑 도쿄 다녀온게 기억에 남네요",
          "original_form": "몇년전에 친구들이랑 도쿄 다녀온게 기억에 남네요",
          "speaker_id": "2",
          "time": "20210905 11:04"
        },
        {
          "id": "MDRW2100031998.1.5",
          "form": "도쿄 어떤점이 좋으셨어요?",
          "original_form": "도쿄 어떤점이 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210905 11:06"
        },
        {
          "id": "MDRW2100031998.1.6",
          "form": "우선 친구들이랑 간 첫 해외여행이어서 뭘 해도 재밌었던것 같아요 ㅋㅋ",
          "original_form": "우선 친구들이랑 간 첫 해외여행이어서 뭘 해도 재밌었던것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:35"
        },
        {
          "id": "MDRW2100031998.1.7",
          "form": "ㅎㅎ 그러셨겠어요",
          "original_form": "ㅎㅎ 그러셨겠어요",
          "speaker_id": "1",
          "time": "20210905 11:35"
        },
        {
          "id": "MDRW2100031998.1.8",
          "form": "여름이어서 되게 습하고 더웠는데도 재밌게 돌아다녔던 기억이 있네요 ㅎㅎ",
          "original_form": "여름이어서 되게 습하고 더웠는데도 재밌게 돌아다녔던 기억이 있네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 11:35"
        },
        {
          "id": "MDRW2100031998.1.9",
          "form": "음식도 잘 맞으셨어요?",
          "original_form": "음식도 잘 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210905 11:36"
        },
        {
          "id": "MDRW2100031998.1.10",
          "form": "네 전 괜찮았어요",
          "original_form": "네 전 괜찮았어요",
          "speaker_id": "2",
          "time": "20210905 11:37"
        },
        {
          "id": "MDRW2100031998.1.11",
          "form": "라멘이랑 샤브샤브 카츠 이렇게 먹었던 기억이 있는데 대체로 무난하게 잘 먹은 것 같네요 ㅎㅎ",
          "original_form": "라멘이랑 샤브샤브 카츠 이렇게 먹었던 기억이 있는데 대체로 무난하게 잘 먹은 것 같네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 11:40"
        },
        {
          "id": "MDRW2100031998.1.12",
          "form": "ㅎㅎ그렇군요",
          "original_form": "ㅎㅎ그렇군요",
          "speaker_id": "1",
          "time": "20210905 11:48"
        },
        {
          "id": "MDRW2100031998.1.13",
          "form": "디저트도 맛있다더라구요",
          "original_form": "디저트도 맛있다더라구요",
          "speaker_id": "1",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100031998.1.14",
          "form": "맞아요 저희도 그때 파르페 먹어보자 하고 인기있다는 카페 갔는데 웨이팅까지 있더라구요",
          "original_form": "맞아요 저희도 그때 파르페 먹어보자 하고 인기있다는 카페 갔는데 웨이팅까지 있더라구요",
          "speaker_id": "2",
          "time": "20210905 12:31"
        },
        {
          "id": "MDRW2100031998.1.15",
          "form": "기다려서 먹어봤는데 웨이팅 있을만하다 싶었죠 ㅎㅎ",
          "original_form": "기다려서 먹어봤는데 웨이팅 있을만하다 싶었죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 12:31"
        },
        {
          "id": "MDRW2100031998.1.16",
          "form": "프랑스 디저트도 잘하는군요!",
          "original_form": "프랑스 디저트도 잘하는군요!",
          "speaker_id": "1",
          "time": "20210905 14:09"
        },
        {
          "id": "MDRW2100031998.1.17",
          "form": "일본은 안가봐서 ㅎㅎ 몰랐어요",
          "original_form": "일본은 안가봐서 ㅎㅎ 몰랐어요",
          "speaker_id": "1",
          "time": "20210905 14:09"
        },
        {
          "id": "MDRW2100031998.1.18",
          "form": "맞아요 일본이 디저트가 많이 발달한 편인것 같더라구요",
          "original_form": "맞아요 일본이 디저트가 많이 발달한 편인것 같더라구요",
          "speaker_id": "2",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100031998.1.19",
          "form": "푸딩도 그렇고 편의점에서 바움쿠헨을 팔아서 먹어봤는데 생각보다 괜찮아서 놀랐어요 ㅋㅋㅋ",
          "original_form": "푸딩도 그렇고 편의점에서 바움쿠헨을 팔아서 먹어봤는데 생각보다 괜찮아서 놀랐어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:16"
        },
        {
          "id": "MDRW2100031998.1.20",
          "form": "바움쿠헨 먹고싶어요 ㅋㅋ",
          "original_form": "바움쿠헨 먹고싶어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100031998.1.21",
          "form": "나중에 혹시 일본 가볼 일 있으시면 편의점 바움쿠헨 한번 드셔보세요!ㅋㅋㅋ",
          "original_form": "나중에 혹시 일본 가볼 일 있으시면 편의점 바움쿠헨 한번 드셔보세요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:25"
        },
        {
          "id": "MDRW2100031998.1.22",
          "form": "ㅋㅋㅋ그럴게요~독일에선 진짜 맛났는데",
          "original_form": "ㅋㅋㅋ그럴게요~독일에선 진짜 맛났는데",
          "speaker_id": "1",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100031998.1.23",
          "form": "앗 독일에서 드셔보셨군요 ㅎㅎ",
          "original_form": "앗 독일에서 드셔보셨군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100031998.1.24",
          "form": "아무래도 그나라에서 직접 먹는건 또 다를것 같아요",
          "original_form": "아무래도 그나라에서 직접 먹는건 또 다를것 같아요",
          "speaker_id": "2",
          "time": "20210905 21:01"
        }
      ]
