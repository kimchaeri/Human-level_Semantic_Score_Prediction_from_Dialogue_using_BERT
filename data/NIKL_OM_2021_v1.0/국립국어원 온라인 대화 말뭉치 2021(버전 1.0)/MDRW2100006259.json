      "utterance": [
        {
          "id": "MDRW2100006259.1.1",
          "form": "무슨과목좋아하시나요?",
          "original_form": "무슨과목좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100006259.1.2",
          "form": "저는 역사요",
          "original_form": "저는 역사요",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100006259.1.3",
          "form": "역사는 알다보면 재밌고",
          "original_form": "역사는 알다보면 재밌고",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006259.1.4",
          "form": "어디 여행가도 다 쓸모가 있더라구요",
          "original_form": "어디 여행가도 다 쓸모가 있더라구요",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006259.1.5",
          "form": "오..전역사가어려웠네요",
          "original_form": "오..전역사가어려웠네요",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006259.1.6",
          "form": "정말요?",
          "original_form": "정말요?",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006259.1.7",
          "form": "세계사도 어렵고..",
          "original_form": "세계사도 어렵고..",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006259.1.8",
          "form": "요즘 애들 책보니까 세계사를 배우던데",
          "original_form": "요즘 애들 책보니까 세계사를 배우던데",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006259.1.9",
          "form": "지금은 재미있네요",
          "original_form": "지금은 재미있네요",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006259.1.10",
          "form": "맞아요 세계사....",
          "original_form": "맞아요 세계사....",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006259.1.11",
          "form": "딸래미가 가르쳐주는데..",
          "original_form": "딸래미가 가르쳐주는데..",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006259.1.12",
          "form": "맞아요 요즘 세계사관련",
          "original_form": "맞아요 요즘 세계사관련",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006259.1.13",
          "form": "넘넘잼나요",
          "original_form": "넘넘잼나요",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006259.1.14",
          "form": "요즘은",
          "original_form": "요즘은",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006259.1.15",
          "form": "프로 많이 하잖아요",
          "original_form": "프로 많이 하잖아요",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006259.1.16",
          "form": "티비봐도재미있고",
          "original_form": "티비봐도재미있고",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006259.1.17",
          "form": "그거도 재밌더라구요",
          "original_form": "그거도 재밌더라구요",
          "speaker_id": "1",
          "time": "20210907 22:19"
