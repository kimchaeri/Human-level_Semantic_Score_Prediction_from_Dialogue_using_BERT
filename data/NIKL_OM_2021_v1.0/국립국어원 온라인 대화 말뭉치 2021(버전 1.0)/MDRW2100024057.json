      "utterance": [
        {
          "id": "MDRW2100024057.1.1",
          "form": "오늘 저녁은 뭘로 드셨나요?",
          "original_form": "오늘 저녁은 뭘로 드셨나요?",
          "speaker_id": "1",
          "time": "20210831 21:02"
        },
        {
          "id": "MDRW2100024057.1.2",
          "form": "전 오늘 바빠서 그냥 밥이요",
          "original_form": "전 오늘 바빠서 그냥 밥이요",
          "speaker_id": "2",
          "time": "20210831 21:02"
        },
        {
          "id": "MDRW2100024057.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210831 21:02"
        },
        {
          "id": "MDRW2100024057.1.4",
          "form": "저는 빵 먹었어요ㅎㅎ",
          "original_form": "저는 빵 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100024057.1.5",
          "form": "라떼랑 같이 먹으니 맛있네여ㅎㅎ",
          "original_form": "라떼랑 같이 먹으니 맛있네여ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100024057.1.6",
          "form": "무슨빵인가요 식빵??",
          "original_form": "무슨빵인가요 식빵??",
          "speaker_id": "2",
          "time": "20210831 21:04"
        },
        {
          "id": "MDRW2100024057.1.7",
          "form": "파바에서 파는 건데요",
          "original_form": "파바에서 파는 건데요",
          "speaker_id": "1",
          "time": "20210831 21:04"
        },
        {
          "id": "MDRW2100024057.1.8",
          "form": "식빵?에 연유크림 들어있는 거예요.",
          "original_form": "식빵?에 연유크림 들어있는 거예요.",
          "speaker_id": "1",
          "time": "20210831 21:04"
        },
        {
          "id": "MDRW2100024057.1.9",
          "form": "이름이 있는데 까먹었습니다...",
          "original_form": "이름이 있는데 까먹었습니다...",
          "speaker_id": "1",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100024057.1.10",
          "form": "연유크림빵 아닌가요?",
          "original_form": "연유크림빵 아닌가요?",
          "speaker_id": "2",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100024057.1.11",
          "form": "후레시크림샌드 빵이라고 나오네요",
          "original_form": "후레시크림샌드 빵이라고 나오네요",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024057.1.12",
          "form": "연유크림이 아닌가바여ㅋㅋㅋㅋ",
          "original_form": "연유크림이 아닌가바여ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024057.1.13",
          "form": "흰 크림인데 생크림은 아닙니다.",
          "original_form": "흰 크림인데 생크림은 아닙니다.",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024057.1.14",
          "form": "연유 식빵 같은건 연유랑 설탕탄 하얀 슈가코팅같은거 올려요",
          "original_form": "연유 식빵 같은건 연유랑 설탕탄 하얀 슈가코팅같은거 올려요",
          "speaker_id": "2",
          "time": "20210831 21:10"
        },
        {
          "id": "MDRW2100024057.1.15",
          "form": "앗 뭔지 알아요",
          "original_form": "앗 뭔지 알아요",
          "speaker_id": "1",
          "time": "20210831 21:10"
        },
        {
          "id": "MDRW2100024057.1.16",
          "form": "파바에선 연유브레드라고 하더라구요",
          "original_form": "파바에선 연유브레드라고 하더라구요",
          "speaker_id": "1",
          "time": "20210831 21:10"
        },
        {
          "id": "MDRW2100024057.1.17",
          "form": "저는 코팅으론 단맛이 좀 아쉬워서 연유브레드는 잘 안 먹지만...",
          "original_form": "저는 코팅으론 단맛이 좀 아쉬워서 연유브레드는 잘 안 먹지만...",
          "speaker_id": "1",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100024057.1.18",
          "form": "연유브레드랑 또 딴 빵인가요?",
          "original_form": "연유브레드랑 또 딴 빵인가요?",
          "speaker_id": "2",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100024057.1.19",
          "form": "넹 딴 거예요",
          "original_form": "넹 딴 거예요",
          "speaker_id": "1",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024057.1.20",
          "form": "후레시크림샌드?는 단팥빵처럼 빵 안에 크림이 들어있어요",
          "original_form": "후레시크림샌드?는 단팥빵처럼 빵 안에 크림이 들어있어요",
          "speaker_id": "1",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024057.1.21",
          "form": "많이 달아요ㅋㅋㅋ",
          "original_form": "많이 달아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024057.1.22",
          "form": "달콤한 빵을 좋아하시는 구나/ 식사빵은 안사드시나요?",
          "original_form": "달콤한 빵을 좋아하시는 구나/ 식사빵은 안사드시나요?",
          "speaker_id": "2",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024057.1.23",
          "form": "식사빵은 주로 베이글 먹는데",
          "original_form": "식사빵은 주로 베이글 먹는데",
          "speaker_id": "1",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024057.1.24",
          "form": "플레인 베이글은 맛이 없어서ㅋㅋㅋ",
          "original_form": "플레인 베이글은 맛이 없어서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024057.1.25",
          "form": "크림치즈 발려 있는 베이글 먹어요",
          "original_form": "크림치즈 발려 있는 베이글 먹어요",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024057.1.26",
          "form": "우와 저도 크림치즈를 좋아해서 최근에 1키로 샀어요",
          "original_form": "우와 저도 크림치즈를 좋아해서 최근에 1키로 샀어요",
          "speaker_id": "2",
          "time": "20210831 21:17"
        }
      ]
