      "utterance": [
        {
          "id": "MDRW2100021355.1.1",
          "form": "저도 뭔가 추천해 드리고 싶은데",
          "original_form": "저도 뭔가 추천해 드리고 싶은데",
          "speaker_id": "2",
          "time": "20210828 17:59"
        },
        {
          "id": "MDRW2100021355.1.2",
          "form": "어디가 좋을까요...ㅠㅠㅠ",
          "original_form": "어디가 좋을까요...ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 18:00"
        },
        {
          "id": "MDRW2100021355.1.3",
          "form": "흠...",
          "original_form": "흠...",
          "speaker_id": "2",
          "time": "20210828 18:00"
        },
        {
          "id": "MDRW2100021355.1.4",
          "form": "추천 안해주셔도 괜찮아요! 저는 사시는곳 주변도 좋고",
          "original_form": "추천 안해주셔도 괜찮아요! 저는 사시는곳 주변도 좋고",
          "speaker_id": "1",
          "time": "20210828 18:00"
        },
        {
          "id": "MDRW2100021355.1.5",
          "form": "혹시 해주신다면 다 잘 받아적겠습니다",
          "original_form": "혹시 해주신다면 다 잘 받아적겠습니다",
          "speaker_id": "1",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100021355.1.6",
          "form": "저는 마저 추천드리자면",
          "original_form": "저는 마저 추천드리자면",
          "speaker_id": "1",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100021355.1.7",
          "form": "부산역쪽에는 곱도리탕 맛있는 그집 이라는곳이 있구요",
          "original_form": "부산역쪽에는 곱도리탕 맛있는 그집 이라는곳이 있구요",
          "speaker_id": "1",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100021355.1.8",
          "form": "서면에는 향미공간이라는 디저트 커피 잘하는 카페가 있어요",
          "original_form": "서면에는 향미공간이라는 디저트 커피 잘하는 카페가 있어요",
          "speaker_id": "1",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100021355.1.9",
          "form": "으어 제가 사는 동네는 좋은 곳이 별로 없어요ㅠㅠㅠ",
          "original_form": "으어 제가 사는 동네는 좋은 곳이 별로 없어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100021355.1.10",
          "form": "제가 그저 관광도시에 사는거라",
          "original_form": "제가 그저 관광도시에 사는거라",
          "speaker_id": "1",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100021355.1.11",
          "form": "괜찮아요!",
          "original_form": "괜찮아요!",
          "speaker_id": "1",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100021355.1.12",
          "form": "아 가을 쯤에 중앙공원에서 꽃무릇이 예쁘긴해요ㅋㅋㅋㅋ",
          "original_form": "아 가을 쯤에 중앙공원에서 꽃무릇이 예쁘긴해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100021355.1.13",
          "form": "그곳이 어디예요?",
          "original_form": "그곳이 어디예요?",
          "speaker_id": "1",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100021355.1.14",
          "form": "경기 서현이요!!",
          "original_form": "경기 서현이요!!",
          "speaker_id": "2",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100021355.1.15",
          "form": "모르실거 같은데ㅠㅠ",
          "original_form": "모르실거 같은데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100021355.1.16",
          "form": "관광지가 아니라 안 유명해요ㅠㅠ",
          "original_form": "관광지가 아니라 안 유명해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100021355.1.17",
          "form": "저 서울에 자취한지 일년밖에 안되어서 경기도정보 귀해요!",
          "original_form": "저 서울에 자취한지 일년밖에 안되어서 경기도정보 귀해요!",
          "speaker_id": "1",
          "time": "20210828 18:15"
        },
        {
          "id": "MDRW2100021355.1.18",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "1",
          "time": "20210828 18:15"
        },
        {
          "id": "MDRW2100021355.1.19",
          "form": "아 서울 사시는구나",
          "original_form": "아 서울 사시는구나",
          "speaker_id": "2",
          "time": "20210828 18:18"
        },
        {
          "id": "MDRW2100021355.1.20",
          "form": "서울 사시면 전시회 같은 거 재밌는 거 많아요!!",
          "original_form": "서울 사시면 전시회 같은 거 재밌는 거 많아요!!",
          "speaker_id": "2",
          "time": "20210828 18:18"
        },
        {
          "id": "MDRW2100021355.1.21",
          "form": "그리고경기권은 양평이나 가평이",
          "original_form": "그리고경기권은 양평이나 가평이",
          "speaker_id": "2",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100021355.1.22",
          "form": "힐링하기 좋은 여행지가 많구요",
          "original_form": "힐링하기 좋은 여행지가 많구요",
          "speaker_id": "2",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100021355.1.23",
          "form": "포천에 허브아일랜드도 좋아요!!",
          "original_form": "포천에 허브아일랜드도 좋아요!!",
          "speaker_id": "2",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100021355.1.24",
          "form": "우와 허브아일랜드 저 허브엄청좋아하는데",
          "original_form": "우와 허브아일랜드 저 허브엄청좋아하는데",
          "speaker_id": "1",
          "time": "20210828 18:53"
        },
        {
          "id": "MDRW2100021355.1.25",
          "form": "꼭 가봐야겠어요",
          "original_form": "꼭 가봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 18:53"
        },
        {
          "id": "MDRW2100021355.1.26",
          "form": "허브 좋아하시면 진짜 추천이요!!",
          "original_form": "허브 좋아하시면 진짜 추천이요!!",
          "speaker_id": "2",
          "time": "20210828 19:00"
        },
        {
          "id": "MDRW2100021355.1.27",
          "form": "가면 공방 체험 같은 거도 할 수 있었는데",
          "original_form": "가면 공방 체험 같은 거도 할 수 있었는데",
          "speaker_id": "2",
          "time": "20210828 19:00"
        },
        {
          "id": "MDRW2100021355.1.28",
          "form": "지금도 가능할지는 모르겟네요ㅠㅠ",
          "original_form": "지금도 가능할지는 모르겟네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 19:00"
        },
        {
          "id": "MDRW2100021355.1.29",
          "form": "우와 체험할수있는곳도 있군요",
          "original_form": "우와 체험할수있는곳도 있군요",
          "speaker_id": "1",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100021355.1.30",
          "form": "어떤거하나요?",
          "original_form": "어떤거하나요?",
          "speaker_id": "1",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100021355.1.31",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100021355.1.32",
          "form": "향초도 있고",
          "original_form": "향초도 있고",
          "speaker_id": "2",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100021355.1.33",
          "form": "고체향수 만드는 것도 있어요",
          "original_form": "고체향수 만드는 것도 있어요",
          "speaker_id": "2",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100021355.1.34",
          "form": "헉 알려주셔서 감사해요",
          "original_form": "헉 알려주셔서 감사해요",
          "speaker_id": "1",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021355.1.35",
          "form": "너무 재밌겠어요",
          "original_form": "너무 재밌겠어요",
          "speaker_id": "1",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100021355.1.36",
          "form": "여름철에 가시면",
          "original_form": "여름철에 가시면",
          "speaker_id": "2",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100021355.1.37",
          "form": "라벤더 아이스크림도 있어요!!",
          "original_form": "라벤더 아이스크림도 있어요!!",
          "speaker_id": "2",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100021355.1.38",
          "form": "헉 저는 이게더 혹해요",
          "original_form": "헉 저는 이게더 혹해요",
          "speaker_id": "1",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100021355.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100021355.1.40",
          "form": "여름에 꼭...!",
          "original_form": "여름에 꼭...!",
          "speaker_id": "1",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100021355.1.41",
          "form": "금강산도 식후경이죠~",
          "original_form": "금강산도 식후경이죠~",
          "speaker_id": "2",
          "time": "20210828 19:09"
        }
      ]
    }
