      "utterance": [
        {
          "id": "MDRW2100029343.1.1",
          "form": "어디어디 찾아놨는데",
          "original_form": "어디어디 찾아놨는데",
          "speaker_id": "2",
          "time": "20210903 02:01"
        },
        {
          "id": "MDRW2100029343.1.2",
          "form": "중식당이랑 일식 오마카세랑 인도음식점",
          "original_form": "중식당이랑 일식 오마카세랑 인도음식점",
          "speaker_id": "1",
          "time": "20210903 02:02"
        },
        {
          "id": "MDRW2100029343.1.3",
          "form": "일식은 별로",
          "original_form": "일식은 별로",
          "speaker_id": "2",
          "time": "20210903 02:04"
        },
        {
          "id": "MDRW2100029343.1.4",
          "form": "중식당이랑 인도음식점이 좋아",
          "original_form": "중식당이랑 인도음식점이 좋아",
          "speaker_id": "2",
          "time": "20210903 02:04"
        },
        {
          "id": "MDRW2100029343.1.5",
          "form": "일식은 왜 별로야 좋아했자나",
          "original_form": "일식은 왜 별로야 좋아했자나",
          "speaker_id": "1",
          "time": "20210903 02:05"
        },
        {
          "id": "MDRW2100029343.1.6",
          "form": "엄마가 좋아한건아냐",
          "original_form": "엄마가 좋아한건아냐",
          "speaker_id": "2",
          "time": "20210903 02:05"
        },
        {
          "id": "MDRW2100029343.1.7",
          "form": "아빠가 좋아하니까 따라 다닌거지",
          "original_form": "아빠가 좋아하니까 따라 다닌거지",
          "speaker_id": "2",
          "time": "20210903 02:05"
        },
        {
          "id": "MDRW2100029343.1.8",
          "form": "엄마는 면이 좋아",
          "original_form": "엄마는 면이 좋아",
          "speaker_id": "2",
          "time": "20210903 02:05"
        },
        {
          "id": "MDRW2100029343.1.9",
          "form": "나두 면이 좋아",
          "original_form": "나두 면이 좋아",
          "speaker_id": "1",
          "time": "20210903 02:05"
        },
        {
          "id": "MDRW2100029343.1.10",
          "form": "그럼 중식당 가야겠네",
          "original_form": "그럼 중식당 가야겠네",
          "speaker_id": "1",
          "time": "20210903 02:05"
        },
        {
          "id": "MDRW2100029343.1.11",
          "form": "아님 여의도에 콩국수 잘 하는집 있어",
          "original_form": "아님 여의도에 콩국수 잘 하는집 있어",
          "speaker_id": "1",
          "time": "20210903 02:05"
        },
        {
          "id": "MDRW2100029343.1.12",
          "form": "엄청 유명해서 오래 기다려야된대",
          "original_form": "엄청 유명해서 오래 기다려야된대",
          "speaker_id": "1",
          "time": "20210903 02:06"
        },
        {
          "id": "MDRW2100029343.1.13",
          "form": "기다리는건 싫은데",
          "original_form": "기다리는건 싫은데",
          "speaker_id": "2",
          "time": "20210903 02:06"
        },
        {
          "id": "MDRW2100029343.1.14",
          "form": "포장은 안된대???",
          "original_form": "포장은 안된대???",
          "speaker_id": "2",
          "time": "20210903 02:06"
        },
        {
          "id": "MDRW2100029343.1.15",
          "form": "포장하면 뭐랬더라 면이 다르댔나",
          "original_form": "포장하면 뭐랬더라 면이 다르댔나",
          "speaker_id": "1",
          "time": "20210903 02:07"
        },
        {
          "id": "MDRW2100029343.1.16",
          "form": "포장이랑 매장에서 먹는거랑 뭐가 다르댔는데",
          "original_form": "포장이랑 매장에서 먹는거랑 뭐가 다르댔는데",
          "speaker_id": "1",
          "time": "20210903 02:07"
        },
        {
          "id": "MDRW2100029343.1.17",
          "form": "콩국수는 면도 중요한데",
          "original_form": "콩국수는 면도 중요한데",
          "speaker_id": "2",
          "time": "20210903 02:07"
        },
        {
          "id": "MDRW2100029343.1.18",
          "form": "가서 먹어야겠네",
          "original_form": "가서 먹어야겠네",
          "speaker_id": "2",
          "time": "20210903 02:07"
        },
        {
          "id": "MDRW2100029343.1.19",
          "form": "아예 오전에 가면 사람이 적대",
          "original_form": "아예 오전에 가면 사람이 적대",
          "speaker_id": "1",
          "time": "20210903 02:08"
        },
        {
          "id": "MDRW2100029343.1.20",
          "form": "그럼 아침도 안먹고 가야겠네",
          "original_form": "그럼 아침도 안먹고 가야겠네",
          "speaker_id": "2",
          "time": "20210903 02:08"
        },
        {
          "id": "MDRW2100029343.1.21",
          "form": "그 정도로 맛있대??",
          "original_form": "그 정도로 맛있대??",
          "speaker_id": "2",
          "time": "20210903 02:08"
        },
        {
          "id": "MDRW2100029343.1.22",
          "form": "맛있다고 하는데 솔직히 우리집 콩국수가 더 맛있을거같아",
          "original_form": "맛있다고 하는데 솔직히 우리집 콩국수가 더 맛있을거같아",
          "speaker_id": "1",
          "time": "20210903 02:08"
        },
        {
          "id": "MDRW2100029343.1.23",
          "form": "우리집은 직접 농사한 콩이자나",
          "original_form": "우리집은 직접 농사한 콩이자나",
          "speaker_id": "1",
          "time": "20210903 02:09"
        },
        {
          "id": "MDRW2100029343.1.24",
          "form": "그렇지 아빠가 주말마다 고생해서 가져온거니까",
          "original_form": "그렇지 아빠가 주말마다 고생해서 가져온거니까",
          "speaker_id": "2",
          "time": "20210903 02:09"
        },
        {
          "id": "MDRW2100029343.1.25",
          "form": "집에 맛있는거 없나여",
          "original_form": "집에 맛있는거 없나여",
          "speaker_id": "1",
          "time": "20210905 17:36"
        }
      ]
    }
  ]
