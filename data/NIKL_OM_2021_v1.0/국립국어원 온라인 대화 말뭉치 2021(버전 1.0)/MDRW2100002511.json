      "utterance": [
        {
          "id": "MDRW2100002511.1.1",
          "form": "챙겨보거나 즐겨보는 프로그램 있나여?",
          "original_form": "챙겨보거나 즐겨보는 프로그램 있나여?",
          "speaker_id": "1",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002511.1.2",
          "form": "저",
          "original_form": "저",
          "speaker_id": "2",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002511.1.3",
          "form": "이번에 스토브리그",
          "original_form": "이번에 스토브리그",
          "speaker_id": "2",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002511.1.4",
          "form": "완주했습니당",
          "original_form": "완주했습니당",
          "speaker_id": "2",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002511.1.5",
          "form": "잼써염 ㅎ.ㅎ",
          "original_form": "잼써염 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002511.1.6",
          "form": "스토브리그가 뭐더라요",
          "original_form": "스토브리그가 뭐더라요",
          "speaker_id": "1",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002511.1.7",
          "form": "그 남궁민 나오던 야구?",
          "original_form": "그 남궁민 나오던 야구?",
          "speaker_id": "1",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002511.1.8",
          "form": "오 아시네염",
          "original_form": "오 아시네염",
          "speaker_id": "2",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002511.1.9",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002511.1.10",
          "form": "그거 약간",
          "original_form": "그거 약간",
          "speaker_id": "2",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002511.1.11",
          "form": "야구 관리자들의 얘기인뎅",
          "original_form": "야구 관리자들의 얘기인뎅",
          "speaker_id": "2",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002511.1.12",
          "form": "잼드라구요 ..",
          "original_form": "잼드라구요 ..",
          "speaker_id": "2",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002511.1.13",
          "form": "남궁민 연기 잘해서",
          "original_form": "남궁민 연기 잘해서",
          "speaker_id": "1",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002511.1.14",
          "form": "그거 시작하는 날만 기다렸던거 같은데",
          "original_form": "그거 시작하는 날만 기다렸던거 같은데",
          "speaker_id": "1",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002511.1.15",
          "form": "놓치고나니 한번에 쫙 봐야지하고\r\n아예 안봤던걸로 기억해요ㅋㅋ",
          "original_form": "놓치고나니 한번에 쫙 봐야지하고\r\n아예 안봤던걸로 기억해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002511.1.16",
          "form": "아...연기 진짜 잘하드라구여...",
          "original_form": "아...연기 진짜 잘하드라구여...",
          "speaker_id": "2",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002511.1.17",
          "form": "예전에 악역도 한거봤는데",
          "original_form": "예전에 악역도 한거봤는데",
          "speaker_id": "2",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002511.1.18",
          "form": "그것도 잘하공...",
          "original_form": "그것도 잘하공...",
          "speaker_id": "2",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002511.1.19",
          "form": "그리고 남궁민이 약간 작품보는 능력이 있는거같으염 ㅋㅋㅋ",
          "original_form": "그리고 남궁민이 약간 작품보는 능력이 있는거같으염 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002511.1.20",
          "form": "맞아:) 될놈될인듯 남궁민도",
          "original_form": "맞아:) 될놈될인듯 남궁민도{emoji:♡}",
          "speaker_id": "1",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002511.1.21",
          "form": "남궁민 나오면 거의 다  성공하는듓 ㅋㅋㅋ",
          "original_form": "남궁민 나오면 거의 다  성공하는듓 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002511.1.22",
          "form": "암튼 잼쓔염 .ㅎ.ㅎ.ㅎ",
          "original_form": "암튼 잼쓔염 .ㅎ.ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210820 14:19"
        },
        {
          "id": "MDRW2100002511.1.23",
          "form": "결말도 나름 굿",
          "original_form": "결말도 나름 굿",
          "speaker_id": "2",
          "time": "20210820 14:19"
        },
        {
          "id": "MDRW2100002511.1.24",
          "form": "저도 지금이라도 봐야겠네요ㅋㅋ",
          "original_form": "저도 지금이라도 봐야겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002511.1.25",
          "form": "알고있지만도 이번주가 막회라\r\n볼거 없었는데 ㅋㅋ",
          "original_form": "알고있지만도 이번주가 막회라\r\n볼거 없었는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002511.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ막회입니까?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ막회입니까?",
          "speaker_id": "2",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002511.1.27",
          "form": "아쉽겠군뇨",
          "original_form": "아쉽겠군뇨",
          "speaker_id": "2",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002511.1.28",
          "form": "네ㅜㅜ 벌써 막회네요",
          "original_form": "네ㅜㅜ 벌써 막회네요",
          "speaker_id": "1",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002511.1.29",
          "form": "결말이 어떨런지ㅋㅋ",
          "original_form": "결말이 어떨런지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002511.1.30",
          "form": "저도 시간나면 함 봐야되는뎅",
          "original_form": "저도 시간나면 함 봐야되는뎅",
          "speaker_id": "2",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002511.1.31",
          "form": "송강씨보러 ㅋㅋㅋㅋ",
          "original_form": "송강씨보러 ㅋㅋㅋㅋ",
