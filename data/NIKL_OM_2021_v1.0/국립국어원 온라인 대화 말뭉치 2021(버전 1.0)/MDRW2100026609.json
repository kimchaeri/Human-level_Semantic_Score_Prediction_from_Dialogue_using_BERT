      "utterance": [
        {
          "id": "MDRW2100026609.1.1",
          "form": "오늘 아침에 어떤거 드실건가요?",
          "original_form": "오늘 아침에 어떤거 드실건가요?",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026609.1.2",
          "form": "토스트에 설탕 뿌려먹을거에요",
          "original_form": "토스트에 설탕 뿌려먹을거에요",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026609.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026609.1.4",
          "form": "전 보통아침을 안먹어요",
          "original_form": "전 보통아침을 안먹어요",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026609.1.5",
          "form": "커피마셔요!!!",
          "original_form": "커피마셔요!!!",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026609.1.6",
          "form": "아아요!!",
          "original_form": "아아요!!",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026609.1.7",
          "form": "와우 공복에 커피 잘 받으시나요?",
          "original_form": "와우 공복에 커피 잘 받으시나요?",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026609.1.8",
          "form": "점심까지 배고프지 않으세요?",
          "original_form": "점심까지 배고프지 않으세요?",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026609.1.9",
          "form": "커피는 잘 받아요!!",
          "original_form": "커피는 잘 받아요!!",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026609.1.10",
          "form": "배고파서 밥이 더 잘들어가는 느낌이에요ㅋㅋㅋ",
          "original_form": "배고파서 밥이 더 잘들어가는 느낌이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026609.1.11",
          "form": "ㅋㅋㅋ맞아요 배고프면 다 맛있어요",
          "original_form": "ㅋㅋㅋ맞아요 배고프면 다 맛있어요",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026609.1.12",
          "form": "전 공복에 커피마시고 토한적이 한두번이 아니라 포기했어요.. 아아 맛있는데ㅠ",
          "original_form": "전 공복에 커피마시고 토한적이 한두번이 아니라 포기했어요.. 아아 맛있는데ㅠ",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026609.1.13",
          "form": "저도 사려야겠다는 생각은 가끔하는데",
          "original_form": "저도 사려야겠다는 생각은 가끔하는데",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026609.1.14",
          "form": "정말 공복커피가 안좋은가봐요...저도 좀 줄여가야겠어요",
          "original_form": "정말 공복커피가 안좋은가봐요...저도 좀 줄여가야겠어요",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026609.1.15",
          "form": "ㅎㅎ 아침 드셨나요",
          "original_form": "ㅎㅎ 아침 드셨나요",
          "speaker_id": "1",
          "time": "20210902 08:24"
        },
        {
          "id": "MDRW2100026609.1.16",
          "form": "네 오늘은",
          "original_form": "네 오늘은",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100026609.1.17",
          "form": "편의점에서 유부초밥사먹었어요!!!",
          "original_form": "편의점에서 유부초밥사먹었어요!!!",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100026609.1.18",
          "form": "오 그거 저두 자주먹었는데 ㅋㅋㅋ 크래미같은거 올려져있는거",
          "original_form": "오 그거 저두 자주먹었는데 ㅋㅋㅋ 크래미같은거 올려져있는거",
          "speaker_id": "1",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100026609.1.19",
          "form": "헐 그거먹었어요 댑ㄱㆍㄱ",
          "original_form": "헐 그거먹었어요 댑ㄱㆍㄱ",
          "speaker_id": "2",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100026609.1.20",
          "form": "우왕ㅋㅋㅋ 그거 넘 맛있져 편의점 음식치고",
          "original_form": "우왕ㅋㅋㅋ 그거 넘 맛있져 편의점 음식치고",
          "speaker_id": "1",
          "time": "20210902 15:31"
        },
        {
          "id": "MDRW2100026609.1.21",
          "form": "저도 저녁에 사먹어야겠어요",
          "original_form": "저도 저녁에 사먹어야겠어요",
          "speaker_id": "1",
          "time": "20210902 15:31"
        },
        {
          "id": "MDRW2100026609.1.22",
          "form": "와사비도있던데",
          "original_form": "와사비도있던데",
          "speaker_id": "2",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100026609.1.23",
          "form": "적어서 살짝 아쉬웠어요",
          "original_form": "적어서 살짝 아쉬웠어요",
