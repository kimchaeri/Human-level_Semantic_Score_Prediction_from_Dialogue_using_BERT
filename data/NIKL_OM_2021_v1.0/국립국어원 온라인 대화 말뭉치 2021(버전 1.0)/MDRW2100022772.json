      "utterance": [
        {
          "id": "MDRW2100022772.1.1",
          "form": "지금 그러면 정시준비하시는건가요",
          "original_form": "지금 그러면 정시준비하시는건가요",
          "speaker_id": "1",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022772.1.2",
          "form": "수시 넣긴 하는데 큰 기대는 없어서",
          "original_form": "수시 넣긴 하는데 큰 기대는 없어서",
          "speaker_id": "2",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022772.1.3",
          "form": "곧 원서 쓰는 기간이라 여러모로 알아보고 있는데",
          "original_form": "곧 원서 쓰는 기간이라 여러모로 알아보고 있는데",
          "speaker_id": "2",
          "time": "20210829 17:50"
        },
        {
          "id": "MDRW2100022772.1.4",
          "form": "과는 정하셨어요?",
          "original_form": "과는 정하셨어요?",
          "speaker_id": "1",
          "time": "20210829 17:50"
        },
        {
          "id": "MDRW2100022772.1.5",
          "form": "가고 싶은 데가 너무 넘사벽이라",
          "original_form": "가고 싶은 데가 너무 넘사벽이라",
          "speaker_id": "2",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022772.1.6",
          "form": "우주 상향으로 일단 써보려구요",
          "original_form": "우주 상향으로 일단 써보려구요",
          "speaker_id": "2",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022772.1.7",
          "form": "적정이랑 하향도 하나 끼워넣고...",
          "original_form": "적정이랑 하향도 하나 끼워넣고...",
          "speaker_id": "2",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022772.1.8",
          "form": "수시로 갈거면 사실 지금부턴",
          "original_form": "수시로 갈거면 사실 지금부턴",
          "speaker_id": "1",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022772.1.9",
          "form": "놀아도 될거같아요 ㅋㅋ",
          "original_form": "놀아도 될거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022772.1.10",
          "form": "저는 수시로 입학했었어서",
          "original_form": "저는 수시로 입학했었어서",
          "speaker_id": "1",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022772.1.11",
          "form": "이맘때쯤에 거의 놀기만했었던거같아요",
          "original_form": "이맘때쯤에 거의 놀기만했었던거같아요",
          "speaker_id": "1",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022772.1.12",
          "form": "저희반은 노는 애가 별로 없던데...ㅎㅎ",
          "original_form": "저희반은 노는 애가 별로 없던데...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022772.1.13",
          "form": "제가 수학을 진짜진짜진짜 못하거든요",
          "original_form": "제가 수학을 진짜진짜진짜 못하거든요",
          "speaker_id": "2",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022772.1.14",
          "form": "수학 저도 진짜 싫어했는데 ㅋㅋ",
          "original_form": "수학 저도 진짜 싫어했는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022772.1.15",
          "form": "학과가 이과만 아니면",
          "original_form": "학과가 이과만 아니면",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022772.1.16",
          "form": "사실 대학오면 수학할일이 없긴해요",
          "original_form": "사실 대학오면 수학할일이 없긴해요",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022772.1.17",
          "form": "제가 아까 가고 싶다 말한 게 전문대 간호학과인데",
          "original_form": "제가 아까 가고 싶다 말한 게 전문대 간호학과인데",
          "speaker_id": "2",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100022772.1.18",
          "form": "정시로도 넣어보려고 하는데ㅡ.",
          "original_form": "정시로도 넣어보려고 하는데ㅡ.",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022772.1.19",
          "form": "그러려면 수학을 공부해야 되는데...",
          "original_form": "그러려면 수학을 공부해야 되는데...",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022772.1.20",
          "form": "음.. 정시로 가려면",
          "original_form": "음.. 정시로 가려면",
          "speaker_id": "1",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022772.1.21",
          "form": "지금부터 공부하긴해야하죠 ㅋㅋ",
          "original_form": "지금부터 공부하긴해야하죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022772.1.22",
          "form": "국어 영어는 어느 정도 잡아놔서",
          "original_form": "국어 영어는 어느 정도 잡아놔서",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022772.1.23",
          "form": "수학이 너무너무너무 걱정되네요",
          "original_form": "수학이 너무너무너무 걱정되네요",
          "speaker_id": "2",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022772.1.24",
          "form": "전문대 간호학과도",
          "original_form": "전문대 간호학과도",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022772.1.25",
          "form": "제가 지금 3점 문제도 제대로 못풀거든요...^^",
          "original_form": "제가 지금 3점 문제도 제대로 못풀거든요...^^",
          "speaker_id": "2",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022772.1.26",
          "form": "최소등급?",
          "original_form": "최소등급?",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022772.1.27",
          "form": "아직 있나여",
          "original_form": "아직 있나여",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022772.1.28",
          "form": "기억이 가물가물해서",
          "original_form": "기억이 가물가물해서",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022772.1.29",
          "form": "국영수 합 10 이내 이런거",
          "original_form": "국영수 합 10 이내 이런거",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022772.1.30",
          "form": "음 그런 건 잘 모르겠네요...?",
          "original_form": "음 그런 건 잘 모르겠네요...?",
          "speaker_id": "2",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100022772.1.31",
          "form": "정시도 최소 등급이 있어요!?!?",
          "original_form": "정시도 최소 등급이 있어요!?!?",
          "speaker_id": "2",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100022772.1.32",
          "form": "특정학과는 있었던거같아서용",
          "original_form": "특정학과는 있었던거같아서용",
