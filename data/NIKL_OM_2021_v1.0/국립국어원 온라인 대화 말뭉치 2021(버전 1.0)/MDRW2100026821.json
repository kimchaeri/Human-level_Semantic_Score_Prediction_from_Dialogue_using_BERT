      "utterance": [
        {
          "id": "MDRW2100026821.1.1",
          "form": "비교는 안하죠ㅎㅎ 나름 아내도 노력하고있으니깐ㅎ",
          "original_form": "비교는 안하죠ㅎㅎ 나름 아내도 노력하고있으니깐ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100026821.1.2",
          "form": "아내이야기가 나와서 말인데ㅎ",
          "original_form": "아내이야기가 나와서 말인데ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100026821.1.3",
          "form": "주말에 차를 보러간다네요ㅎ 어짜피 이혼할꺼니깐 물어봤어요",
          "original_form": "주말에 차를 보러간다네요ㅎ 어짜피 이혼할꺼니깐 물어봤어요",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100026821.1.4",
          "form": "음 네넹",
          "original_form": "음 네넹",
          "speaker_id": "1",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026821.1.5",
          "form": "차는 무슨돈으로 사냐고?ㅎ 올할부로 하려나봐요... 아니면 렌트로 ....",
          "original_form": "차는 무슨돈으로 사냐고?ㅎ 올할부로 하려나봐요... 아니면 렌트로 ....",
          "speaker_id": "2",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026821.1.6",
          "form": "음.. 아내가 모아둔돈이없늗데...",
          "original_form": "음.. 아내가 모아둔돈이없늗데...",
          "speaker_id": "2",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026821.1.7",
          "form": "음 복직하면  여유가 되시려나요",
          "original_form": "음 복직하면  여유가 되시려나요",
          "speaker_id": "1",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026821.1.8",
          "form": "어떻게 살껀지 모르겟네요ㅠ",
          "original_form": "어떻게 살껀지 모르겟네요ㅠ",
          "speaker_id": "2",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026821.1.9",
          "form": "여유가 안되죠",
          "original_form": "여유가 안되죠",
          "speaker_id": "2",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026821.1.10",
          "form": "그전에 월급이 230ㅡ?",
          "original_form": "그전에 월급이 230ㅡ?",
          "speaker_id": "2",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026821.1.11",
          "form": "할부면  엄청 스트레스 실텐데",
          "original_form": "할부면  엄청 스트레스 실텐데",
          "speaker_id": "1",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026821.1.12",
          "form": "세금제외하고 했고",
          "original_form": "세금제외하고 했고",
          "speaker_id": "2",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026821.1.13",
          "form": "제가 양육비줘도",
          "original_form": "제가 양육비줘도",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026821.1.14",
          "form": "330이나되겟네여ㅎ",
          "original_form": "330이나되겟네여ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026821.1.15",
          "form": "차를 산다고하니ㅎ 참... 좀그래요ㅎㅎ",
          "original_form": "차를 산다고하니ㅎ 참... 좀그래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026821.1.16",
          "form": "음  친정쪽이 여유잇는거 아님 움",
          "original_form": "음  친정쪽이 여유잇는거 아님 움",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026821.1.17",
          "form": "여유는있어요ㅎ",
          "original_form": "여유는있어요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026821.1.18",
          "form": "어머님이 집한채 가지고계시고 뭐 현금도 조금있으시구요",
          "original_form": "어머님이 집한채 가지고계시고 뭐 현금도 조금있으시구요",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100026821.1.19",
          "form": "그럼 그나마 다행이네요",
          "original_form": "그럼 그나마 다행이네요",
          "speaker_id": "1",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100026821.1.20",
          "form": "형부가 사업을 하셔서 아마 그렇게 여유없는 집은 아니에요",
          "original_form": "형부가 사업을 하셔서 아마 그렇게 여유없는 집은 아니에요",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100026821.1.21",
          "form": "근데 생각하는게 조금 아니라 생각서... 전.",
          "original_form": "근데 생각하는게 조금 아니라 생각서... 전.",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100026821.1.22",
          "form": "만약에 나라면 어떻게 했을까 하는 생각이 들엇어요ㅎ",
          "original_form": "만약에 나라면 어떻게 했을까 하는 생각이 들엇어요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100026821.1.23",
          "form": "형부돈은 형부거죠 ㅎㅎ",
          "original_form": "형부돈은 형부거죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100026821.1.24",
          "form": "근데 온가족이뭐...",
          "original_form": "근데 온가족이뭐...",
          "speaker_id": "2",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100026821.1.25",
          "form": "어머님,생활비도 형님이 내시고",
          "original_form": "어머님,생활비도 형님이 내시고",
          "speaker_id": "2",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100026821.1.26",
          "form": "원래는 저희아내가 냈었는데 저희가 결혼하면서 형님이 내시다가",
          "original_form": "원래는 저희아내가 냈었는데 저희가 결혼하면서 형님이 내시다가",
          "speaker_id": "2",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100026821.1.27",
          "form": "아마 아내가 다시 돌아가면 분명 생활비 낼꺼에요",
          "original_form": "아마 아내가 다시 돌아가면 분명 생활비 낼꺼에요",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026821.1.28",
          "form": "조금 독특하긴 하네요",
          "original_form": "조금 독특하긴 하네요",
          "speaker_id": "1",
          "time": "20210902 00:30"
        }
      ]
    }
