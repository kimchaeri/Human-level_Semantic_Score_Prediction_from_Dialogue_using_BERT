      "utterance": [
        {
          "id": "MDRW2100038717.1.1",
          "form": "저희 둘째는 착해서 배방구도 좋아한다는 ㄷㄷ",
          "original_form": "저희 둘째는 착해서 배방구도 좋아한다는 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100038717.1.2",
          "form": "배방구는 진짜 기분 좋을 때만 받아주던데",
          "original_form": "배방구는 진짜 기분 좋을 때만 받아주던데",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100038717.1.3",
          "form": "너무 귀엽네요 ㅎㅎ",
          "original_form": "너무 귀엽네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100038717.1.4",
          "form": "그냥 사람처럼 대짜로 뻗어있어요 ㅋㅋ",
          "original_form": "그냥 사람처럼 대짜로 뻗어있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100038717.1.5",
          "form": "둘째라 순한가바요",
          "original_form": "둘째라 순한가바요",
          "speaker_id": "1",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100038717.1.6",
          "form": "첫째는 평생 그렇게  안누워자는데 ㅋㅋ",
          "original_form": "첫째는 평생 그렇게  안누워자는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100038717.1.7",
          "form": "저희 애는 삐지면 그렇게 누워 있어요 ㅋㅋㅋㅋ",
          "original_form": "저희 애는 삐지면 그렇게 누워 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100038717.1.8",
          "form": "보통 어디 구석에 박히던데 시위하듯이 뻗어요 ㅋㅋㅋ",
          "original_form": "보통 어디 구석에 박히던데 시위하듯이 뻗어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100038717.1.9",
          "form": "그때 만지면 뜯기나요?! ㅋㅋㅋ",
          "original_form": "그때 만지면 뜯기나요?! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100038717.1.10",
          "form": "잔소리해요 ㅋㅋㅋㅋㅋ",
          "original_form": "잔소리해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100038717.1.11",
          "form": "거기도 혹시 수다쟁이 인가요",
          "original_form": "거기도 혹시 수다쟁이 인가요",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100038717.1.12",
          "form": "엄청 수다쟁이에요 ㅎㅎ",
          "original_form": "엄청 수다쟁이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100038717.1.13",
          "form": "재택 때 줌회의 하면 지가 와서 회의하고ㅠ ㅋㅋㅋㅋㅋ",
          "original_form": "재택 때 줌회의 하면 지가 와서 회의하고ㅠ ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100038717.1.14",
          "form": "우리 첫째도요 ㅋㅋㅋㅋ",
          "original_form": "우리 첫째도요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100038717.1.15",
          "form": "헐 사람들이 귀엽다 하실듯 ㅋㅋㅋㅋ",
          "original_form": "헐 사람들이 귀엽다 하실듯 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100038717.1.16",
          "form": "근데 불만도 많아서 맨날 울어요 ㅜㅜ",
          "original_form": "근데 불만도 많아서 맨날 울어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100038717.1.17",
          "form": "귀여워해주셔서 다행인데 계속 이러니까 눈치 보여요 ㅎㅎ",
          "original_form": "귀여워해주셔서 다행인데 계속 이러니까 눈치 보여요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100038717.1.18",
          "form": "문 닫으면 거기 앞에 앉아서 열라고 울고 ㅋㅋㅋㅋㅋ",
          "original_form": "문 닫으면 거기 앞에 앉아서 열라고 울고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100038717.1.19",
          "form": "오 마자요",
          "original_form": "오 마자요",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100038717.1.20",
          "form": "씻을때도 열고 씻는다는...",
          "original_form": "씻을때도 열고 씻는다는...",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100038717.1.21",
          "form": "샤워기 물 맨날 1분씩 흡입하고 가십니다...",
          "original_form": "샤워기 물 맨날 1분씩 흡입하고 가십니다...",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100038717.1.22",
          "form": "맞아요 화장실 문을 못 닫아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 화장실 문을 못 닫아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100038717.1.23",
          "form": "지 화장실 갔을 때는 못 오라 하면서 ㅋㅋㅋ",
          "original_form": "지 화장실 갔을 때는 못 오라 하면서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100038717.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ 저는 똥쌀때 구경가요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저는 똥쌀때 구경가요",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100038717.1.25",
          "form": "힘주는거 너무 우껴요",
          "original_form": "힘주는거 너무 우껴요",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100038717.1.26",
          "form": "얼굴 오므리는거 너무 귀엽고 웃기죠 ㅋㅋㅋㅋㅋ",
          "original_form": "얼굴 오므리는거 너무 귀엽고 웃기죠 ㅋㅋㅋㅋㅋ",
