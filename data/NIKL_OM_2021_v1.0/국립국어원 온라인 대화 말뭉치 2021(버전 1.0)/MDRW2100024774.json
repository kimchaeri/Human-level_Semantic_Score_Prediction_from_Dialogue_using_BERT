      "utterance": [
        {
          "id": "MDRW2100024774.1.1",
          "form": "ㄹㅇ...",
          "original_form": "ㄹㅇ...",
          "speaker_id": "2",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100024774.1.2",
          "form": "마비노기 모바일도 나왔으면 좋겠다",
          "original_form": "마비노기 모바일도 나왔으면 좋겠다",
          "speaker_id": "1",
          "time": "20210831 18:22"
        },
        {
          "id": "MDRW2100024774.1.3",
          "form": "고인물 없게 다 똑같이 뉴비부터 시작하게 해달라",
          "original_form": "고인물 없게 다 똑같이 뉴비부터 시작하게 해달라",
          "speaker_id": "1",
          "time": "20210831 18:22"
        },
        {
          "id": "MDRW2100024774.1.4",
          "form": "나왔다 말아먹지 않았나?",
          "original_form": "나왔다 말아먹지 않았나?",
          "speaker_id": "2",
          "time": "20210831 18:22"
        },
        {
          "id": "MDRW2100024774.1.5",
          "form": "아닌가 그건 메이플인가",
          "original_form": "아닌가 그건 메이플인가",
          "speaker_id": "2",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100024774.1.6",
          "form": "근데 마비노기보다 원신이 용량 더 클것같아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 마비노기보다 원신이 용량 더 클것같아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100024774.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100024774.1.8",
          "form": "그럴거같긴 해",
          "original_form": "그럴거같긴 해",
          "speaker_id": "1",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100024774.1.9",
          "form": "마비 나와도 검은사막 류의 로딩방식일듯",
          "original_form": "마비 나와도 검은사막 류의 로딩방식일듯",
          "speaker_id": "1",
          "time": "20210831 18:30"
        },
        {
          "id": "MDRW2100024774.1.10",
          "form": "마비노기는.....손가락도 없잖아",
          "original_form": "마비노기는.....손가락도 없잖아",
          "speaker_id": "2",
          "time": "20210831 18:30"
        },
        {
          "id": "MDRW2100024774.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:39"
        },
        {
          "id": "MDRW2100024774.1.12",
          "form": "거긴 그래픽 개선 안한대...?",
          "original_form": "거긴 그래픽 개선 안한대...?",
          "speaker_id": "1",
          "time": "20210831 18:39"
        },
        {
          "id": "MDRW2100024774.1.13",
          "form": "키트에서 나온 머리카락이나 얼굴이나 옷 그래픽은",
          "original_form": "키트에서 나온 머리카락이나 얼굴이나 옷 그래픽은",
          "speaker_id": "2",
          "time": "20210831 18:39"
        },
        {
          "id": "MDRW2100024774.1.14",
          "form": "많이 나아졌는데",
          "original_form": "많이 나아졌는데",
          "speaker_id": "2",
          "time": "20210831 18:40"
        },
        {
          "id": "MDRW2100024774.1.15",
          "form": "손가락 키트같은건",
          "original_form": "손가락 키트같은건",
          "speaker_id": "2",
          "time": "20210831 18:40"
        },
        {
          "id": "MDRW2100024774.1.16",
          "form": "내놓지 않았기 때문에....",
          "original_form": "내놓지 않았기 때문에....",
          "speaker_id": "2",
          "time": "20210831 18:40"
        },
        {
          "id": "MDRW2100024774.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024774.1.18",
          "form": "아니 대체 왜 손가락 구현을 안하는거야...",
          "original_form": "아니 대체 왜 손가락 구현을 안하는거야...",
          "speaker_id": "1",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024774.1.19",
          "form": "이해할 수가 없다",
          "original_form": "이해할 수가 없다",
          "speaker_id": "1",
          "time": "20210831 18:54"
        },
        {
          "id": "MDRW2100024774.1.20",
          "form": "그래도 손은 잘 안보여서 옷이랑 가발만 잘 입히면",
          "original_form": "그래도 손은 잘 안보여서 옷이랑 가발만 잘 입히면",
          "speaker_id": "2",
          "time": "20210831 18:54"
        },
        {
          "id": "MDRW2100024774.1.21",
          "form": "나름 괜찮아.......?",
          "original_form": "나름 괜찮아.......?",
          "speaker_id": "2",
          "time": "20210831 18:54"
        },
        {
          "id": "MDRW2100024774.1.22",
          "form": "물론 유료지만",
          "original_form": "물론 유료지만",
          "speaker_id": "2",
          "time": "20210831 18:54"
        },
        {
          "id": "MDRW2100024774.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100024774.1.24",
          "form": "나름 원조(?)겜인데",
          "original_form": "나름 원조(?)겜인데",
          "speaker_id": "1",
          "time": "20210831 19:06"
        },
        {
          "id": "MDRW2100024774.1.25",
          "form": "이해할 수 없는 그래픽 업데이트...",
          "original_form": "이해할 수 없는 그래픽 업데이트...",
          "speaker_id": "1",
          "time": "20210831 19:06"
        },
        {
          "id": "MDRW2100024774.1.26",
          "form": "그거 어 떠도는 말이긴 한데",
          "original_form": "그거 어 떠도는 말이긴 한데",
          "speaker_id": "2",
          "time": "20210831 19:08"
        },
        {
          "id": "MDRW2100024774.1.27",
          "form": "가설이 있긴 해",
          "original_form": "가설이 있긴 해",
          "speaker_id": "2",
          "time": "20210831 19:08"
        },
        {
          "id": "MDRW2100024774.1.28",
          "form": "가설? 어떤거?",
          "original_form": "가설? 어떤거?",
          "speaker_id": "1",
          "time": "20210831 19:08"
        },
        {
          "id": "MDRW2100024774.1.29",
          "form": "그거 초반 개발진이 스토리며 그래픽이며 다 했는데",
          "original_form": "그거 초반 개발진이 스토리며 그래픽이며 다 했는데",
          "speaker_id": "2",
          "time": "20210831 19:08"
        },
        {
          "id": "MDRW2100024774.1.30",
          "form": "중간에 싹 바뀐적이 있거든",
          "original_form": "중간에 싹 바뀐적이 있거든",
          "speaker_id": "2",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100024774.1.31",
          "form": "그때 인수인계가 안되서",
          "original_form": "그때 인수인계가 안되서",
          "speaker_id": "2",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100024774.1.32",
          "form": "헐 그게 가능해??",
          "original_form": "헐 그게 가능해??",
          "speaker_id": "1",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100024774.1.33",
          "form": "스토리 그때부터 말아먹었고",
          "original_form": "스토리 그때부터 말아먹었고",
          "speaker_id": "2",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100024774.1.34",
          "form": "코드는 건드릴수가없어서",
          "original_form": "코드는 건드릴수가없어서",
          "speaker_id": "2",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100024774.1.35",
          "form": "크게 개편을ㅇ",
          "original_form": "크게 개편을ㅇ",
          "speaker_id": "2",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100024774.1.36",
          "form": "못한다는거임",
          "original_form": "못한다는거임",
          "speaker_id": "2",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100024774.1.37",
          "form": "인수인계는 잘 모르겠는데",
          "original_form": "인수인계는 잘 모르겠는데",
          "speaker_id": "2",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024774.1.38",
          "form": "헐 ㅋㅋㅋㅋㅋㅋㅋ 인디겜도 아니고 마비노기가...",
          "original_form": "헐 ㅋㅋㅋㅋㅋㅋㅋ 인디겜도 아니고 마비노기가...",
          "speaker_id": "1",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024774.1.39",
          "form": "개발진 중간에 물갈이된거 맞고",
          "original_form": "개발진 중간에 물갈이된거 맞고",
          "speaker_id": "2",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024774.1.40",
          "form": "스토리 말아먹은것도 맞긴 해",
          "original_form": "스토리 말아먹은것도 맞긴 해",
          "speaker_id": "2",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024774.1.41",
          "form": "럽프듀도",
          "original_form": "럽프듀도",
          "speaker_id": "1",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024774.1.42",
          "form": "10챕까지는 초기 스작이 하고",
          "original_form": "10챕까지는 초기 스작이 하고",
          "speaker_id": "1",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024774.1.43",
          "form": "이후에 타 회사에서 돈 어마어마하게 주고 그 스작 데려갔다는 찌라시 돌던데",
          "original_form": "이후에 타 회사에서 돈 어마어마하게 주고 그 스작 데려갔다는 찌라시 돌던데",
          "speaker_id": "1",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024774.1.44",
          "form": "스토리 망한거 보면 맞을듯...",
          "original_form": "스토리 망한거 보면 맞을듯...",
          "speaker_id": "1",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024774.1.45",
          "form": "이거는 걍 유저사이에서 도는 얘기라",
          "original_form": "이거는 걍 유저사이에서 도는 얘기라",
          "speaker_id": "2",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024774.1.46",
          "form": "정확도는 신뢰할순없긴 해",
          "original_form": "정확도는 신뢰할순없긴 해",
          "speaker_id": "2",
          "time": "20210831 19:11"
        }
      ]
    }
