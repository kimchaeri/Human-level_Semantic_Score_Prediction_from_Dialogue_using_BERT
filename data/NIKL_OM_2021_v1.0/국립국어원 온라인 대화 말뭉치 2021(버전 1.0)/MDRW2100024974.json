      "utterance": [
        {
          "id": "MDRW2100024974.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100024974.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100024974.1.3",
          "form": "저느 지금 커피랑 복숭아를 저녁 뒤 후식으로 먹고 있어요 ㅋㅋ name2님은",
          "original_form": "저느 지금 커피랑 복숭아를 저녁 뒤 후식으로 먹고 있어요 ㅋㅋ &name2&님은",
          "speaker_id": "1",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100024974.1.4",
          "form": "오늘 후식으로 뭘 드셨나요?",
          "original_form": "오늘 후식으로 뭘 드셨나요?",
          "speaker_id": "1",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100024974.1.5",
          "form": "우왕 최고네요",
          "original_form": "우왕 최고네요",
          "speaker_id": "2",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100024974.1.6",
          "form": "커피와 복숭아",
          "original_form": "커피와 복숭아",
          "speaker_id": "2",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100024974.1.7",
          "form": "저는ㅜ ㅜ 속이안좋아서 죽먹었어요",
          "original_form": "저는ㅜ ㅜ 속이안좋아서 죽먹었어요",
          "speaker_id": "2",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100024974.1.8",
          "form": "복숭아 먹고싶은데 꾹 참고 있다죠",
          "original_form": "복숭아 먹고싶은데 꾹 참고 있다죠",
          "speaker_id": "2",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100024974.1.9",
          "form": "아이구,, 여름이 끝나갈때지만 그래도 요즘 좀 더워서",
          "original_form": "아이구,, 여름이 끝나갈때지만 그래도 요즘 좀 더워서",
          "speaker_id": "1",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024974.1.10",
          "form": "음식 조심하셔요 ㅠㅠ",
          "original_form": "음식 조심하셔요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024974.1.11",
          "form": "전 아침에 꺼내놓고 나간 빵을 밤에 와서 먹으니 크림이 쉬었더라고요,",
          "original_form": "전 아침에 꺼내놓고 나간 빵을 밤에 와서 먹으니 크림이 쉬었더라고요,",
          "speaker_id": "1",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100024974.1.12",
          "form": "헉 조심하셔요 ㅠㅠ",
          "original_form": "헉 조심하셔요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100024974.1.13",
          "form": "저는 소화불량 장염 달고살아서",
          "original_form": "저는 소화불량 장염 달고살아서",
          "speaker_id": "2",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100024974.1.14",
          "form": "익숙하네요 ㄷㄷ",
          "original_form": "익숙하네요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100024974.1.15",
          "form": "ㅠㅠㅠ 아픔이 어떻게익숙해져요 ㅠㅠ",
          "original_form": "ㅠㅠㅠ 아픔이 어떻게익숙해져요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024974.1.16",
          "form": "방심했다가 음 조금의 응가 급행열차가 있었던 며칠전이었습니다 ㅋㅋ",
          "original_form": "방심했다가 음 조금의 응가 급행열차가 있었던 며칠전이었습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024974.1.17",
          "form": "그건그래요 아플때마다 지긋지긋하다가도 또 시작이구나 싶어요",
          "original_form": "그건그래요 아플때마다 지긋지긋하다가도 또 시작이구나 싶어요",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100024974.1.18",
          "form": "ㅋㅋㅋㅋ 그래도 조금의 응가라서 다행입니다",
          "original_form": "ㅋㅋㅋㅋ 그래도 조금의 응가라서 다행입니다",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100024974.1.19",
          "form": "앤님과 모르는 사이라 피해양을 좀 줄여서 말해ㅐㅆ습니다 ㅎㅋㅋㅋㅋ",
          "original_form": "앤님과 모르는 사이라 피해양을 좀 줄여서 말해ㅐㅆ습니다 ㅎㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100024974.1.20",
          "form": "ㅋㅋ악ㅋㅋㅋㅋ 귀여우셔라ㅋㅋㅋ",
          "original_form": "ㅋㅋ악ㅋㅋㅋㅋ 귀여우셔라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024974.1.21",
          "form": "아무튼 배가 아프면 그 꾸룩한 느낌때매",
          "original_form": "아무튼 배가 아프면 그 꾸룩한 느낌때매",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024974.1.22",
          "form": "아직 내외하고 싶은 마음 호호",
          "original_form": "아직 내외하고 싶은 마음 호호",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024974.1.23",
          "form": "맞아요ㅋㅋ 식은땀나는 그런",
          "original_form": "맞아요ㅋㅋ 식은땀나는 그런",
          "speaker_id": "2",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024974.1.24",
          "form": "그죠그죠 배가 차가와지면서",
          "original_form": "그죠그죠 배가 차가와지면서",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024974.1.25",
          "form": "그래서 모든 음식을 아직도 냉장고에 다 넣고 있어요",
          "original_form": "그래서 모든 음식을 아직도 냉장고에 다 넣고 있어요",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024974.1.26",
          "form": "오 맞아요 배 차가워지며서",
          "original_form": "오 맞아요 배 차가워지며서",
          "speaker_id": "2",
          "time": "20210831 21:53"
        }
      ]
    }
