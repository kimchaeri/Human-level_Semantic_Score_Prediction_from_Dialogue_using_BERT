      "utterance": [
        {
          "id": "MDRW2100037004.1.1",
          "form": "어후 먹으니 졸려요ㅠ",
          "original_form": "어후 먹으니 졸려요ㅠ",
          "speaker_id": "2",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100037004.1.2",
          "form": "이정도면 병 아닌가 싶어요ㅋㅋ",
          "original_form": "이정도면 병 아닌가 싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100037004.1.3",
          "form": "ㅋㅋㅋㅋ저도 식곤증땜시 뭘하지를못해여",
          "original_form": "ㅋㅋㅋㅋ저도 식곤증땜시 뭘하지를못해여",
          "speaker_id": "1",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100037004.1.4",
          "form": "이게 체력때문인건지",
          "original_form": "이게 체력때문인건지",
          "speaker_id": "1",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100037004.1.5",
          "form": "아우 그런걸까요ㅋㅋㅠ",
          "original_form": "아우 그런걸까요ㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100037004.1.6",
          "form": "맛있는걸 먹으면 젓가락 내려놓기가 너무 힘드네요",
          "original_form": "맛있는걸 먹으면 젓가락 내려놓기가 너무 힘드네요",
          "speaker_id": "2",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100037004.1.7",
          "form": "전 당뇨도 조만간걸릴것같아요",
          "original_form": "전 당뇨도 조만간걸릴것같아요",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100037004.1.8",
          "form": "당뇨증세가 식곤증엄청심해진대요",
          "original_form": "당뇨증세가 식곤증엄청심해진대요",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100037004.1.9",
          "form": "저도요ㅜㅜ내양만큼적당히먹기가 안돼요",
          "original_form": "저도요ㅜㅜ내양만큼적당히먹기가 안돼요",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100037004.1.10",
          "form": "헐.. 저도인 것 같네요..",
          "original_form": "헐.. 저도인 것 같네요..",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100037004.1.11",
          "form": "음료수도 많이줄여야되는데ㅜ",
          "original_form": "음료수도 많이줄여야되는데ㅜ",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100037004.1.12",
          "form": "제가설탕 당을엄청좋아하거든여ㅜㅜ",
          "original_form": "제가설탕 당을엄청좋아하거든여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100037004.1.13",
          "form": "맞아요ㅠ 세상에 먹을게 저렇게 많은데 먼저 내려놓을 수 없단말예요",
          "original_form": "맞아요ㅠ 세상에 먹을게 저렇게 많은데 먼저 내려놓을 수 없단말예요",
          "speaker_id": "2",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100037004.1.14",
          "form": "저두요 밀가루도요!",
          "original_form": "저두요 밀가루도요!",
          "speaker_id": "2",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100037004.1.15",
          "form": "네 저도 완전밀가루 사랑합니다ㅜ",
          "original_form": "네 저도 완전밀가루 사랑합니다ㅜ",
          "speaker_id": "1",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100037004.1.16",
          "form": "면 빵 미쳐요",
          "original_form": "면 빵 미쳐요",
          "speaker_id": "1",
          "time": "20210831 16:55"
        },
        {
          "id": "MDRW2100037004.1.17",
          "form": "저두요ㅠㅜ",
          "original_form": "저두요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210831 16:55"
        },
        {
          "id": "MDRW2100037004.1.18",
          "form": "진짜 그 두개는 없어서 못먹죠",
          "original_form": "진짜 그 두개는 없어서 못먹죠",
          "speaker_id": "2",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100037004.1.19",
          "form": "여름엔 진짜 면만먹어서",
          "original_form": "여름엔 진짜 면만먹어서",
          "speaker_id": "1",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100037004.1.20",
          "form": "식도염달고살았어요",
          "original_form": "식도염달고살았어요",
          "speaker_id": "1",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100037004.1.21",
          "form": "헉 고생하셨겠어요",
          "original_form": "헉 고생하셨겠어요",
          "speaker_id": "2",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100037004.1.22",
          "form": "저는 위염 흑ㅜ",
          "original_form": "저는 위염 흑ㅜ",
          "speaker_id": "2",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100037004.1.23",
          "form": "네 위 장이 시원찮은데 좋아하는음식들이저런거라",
          "original_form": "네 위 장이 시원찮은데 좋아하는음식들이저런거라",
          "speaker_id": "1",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100037004.1.24",
          "form": "안먹는거 너무 힘들어요",
          "original_form": "안먹는거 너무 힘들어요",
          "speaker_id": "2",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100037004.1.25",
          "form": "흑흑 비슷하시네여",
          "original_form": "흑흑 비슷하시네여",
          "speaker_id": "1",
          "time": "20210831 16:57"
        }
      ]
    }
  ]
