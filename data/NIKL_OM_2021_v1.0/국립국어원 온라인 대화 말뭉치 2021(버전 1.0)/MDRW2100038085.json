      "utterance": [
        {
          "id": "MDRW2100038085.1.1",
          "form": "너 잠옷 따로 있어?",
          "original_form": "너 잠옷 따로 있어?",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038085.1.2",
          "form": "그냥 일반 옷 말고 잠옷으로 나온 잠옷말이야.",
          "original_form": "그냥 일반 옷 말고 잠옷으로 나온 잠옷말이야.",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038085.1.3",
          "form": "아니 나는 그냥 옷들을 주워입어",
          "original_form": "아니 나는 그냥 옷들을 주워입어",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038085.1.4",
          "form": "나는 폭닥한 재질밖에 어1ㅅ어",
          "original_form": "나는 폭닥한 재질밖에 어1ㅅ어",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038085.1.5",
          "form": "고등학교때 반티도 아직입는단다",
          "original_form": "고등학교때 반티도 아직입는단다",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038085.1.6",
          "form": "어 나도 주워입어",
          "original_form": "어 나도 주워입어",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038085.1.7",
          "form": "나도 하의가 지금 고등학교체육복이야",
          "original_form": "나도 하의가 지금 고등학교체육복이야",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038085.1.8",
          "form": "잠옷을 따로 구비하면 기분이 좋니??",
          "original_form": "잠옷을 따로 구비하면 기분이 좋니??",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038085.1.9",
          "form": "옷이 참 헤지지않고 튼튼해",
          "original_form": "옷이 참 헤지지않고 튼튼해",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038085.1.10",
          "form": "옷을 돌려입기에 지쳤어",
          "original_form": "옷을 돌려입기에 지쳤어",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038085.1.11",
          "form": "너무 챱 달라붙는 옷은 민망하잖아",
          "original_form": "너무 챱 달라붙는 옷은 민망하잖아",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038085.1.12",
          "form": "그래서 상의를 잘 골라야해",
          "original_form": "그래서 상의를 잘 골라야해",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038085.1.13",
          "form": "맞아 나는 그리고 요즘 자주 나가야해서",
          "original_form": "맞아 나는 그리고 요즘 자주 나가야해서",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038085.1.14",
          "form": "그래서 고를필요없이 잠옷울 사려고 했어",
          "original_form": "그래서 고를필요없이 잠옷울 사려고 했어",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038085.1.15",
          "form": "입고 나갈 수 있는 옷을 잠옷으로 애용해",
          "original_form": "입고 나갈 수 있는 옷을 잠옷으로 애용해",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038085.1.16",
          "form": "근데 잠옷이 챱 달라붙는 재질이라면",
          "original_form": "근데 잠옷이 챱 달라붙는 재질이라면",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038085.1.17",
          "form": "믿기니?",
          "original_form": "믿기니?",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038085.1.18",
          "form": "세상에 그럼 실크인거야?",
          "original_form": "세상에 그럼 실크인거야?",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038085.1.19",
          "form": "그러면 코시국에 적절하지않아",
          "original_form": "그러면 코시국에 적절하지않아",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038085.1.20",
          "form": "나는 잠옷은 잠옷이런더 요즘.",
          "original_form": "나는 잠옷은 잠옷이런더 요즘.",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038085.1.21",
          "form": "아니ㅠ 챱 달라붙는",
          "original_form": "아니ㅠ 챱 달라붙는",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038085.1.22",
          "form": "쫀쫀재질이야",
          "original_form": "쫀쫀재질이야",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038085.1.23",
          "form": "너 다이어리 꾸미기라고 아니?",
          "original_form": "너 다이어리 꾸미기라고 아니?",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038085.1.24",
          "form": "나는 요즘 그걸보고 스티커를 사고싶더라",
          "original_form": "나는 요즘 그걸보고 스티커를 사고싶더라",
          "speaker_id": "1",
          "time": "20210901 23:04"
        }
      ]
