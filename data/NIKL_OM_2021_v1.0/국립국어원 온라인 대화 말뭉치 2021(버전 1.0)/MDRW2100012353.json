      "utterance": [
        {
          "id": "MDRW2100012353.1.1",
          "form": "안녕하세요. 오늘 무엇을 드셨나요?",
          "original_form": "안녕하세요. 오늘 무엇을 드셨나요?",
          "speaker_id": "1",
          "time": "20210824 21:28"
        },
        {
          "id": "MDRW2100012353.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210824 21:31"
        },
        {
          "id": "MDRW2100012353.1.3",
          "form": "오늘은 김치찌개 먹었어요",
          "original_form": "오늘은 김치찌개 먹었어요",
          "speaker_id": "2",
          "time": "20210824 21:31"
        },
        {
          "id": "MDRW2100012353.1.4",
          "form": "김치찌개 좋아하시나요?",
          "original_form": "김치찌개 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210824 21:31"
        },
        {
          "id": "MDRW2100012353.1.5",
          "form": "네! 저는 꽁치김치찌개를 좋아합니다",
          "original_form": "네! 저는 꽁치김치찌개를 좋아합니다",
          "speaker_id": "1",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012353.1.6",
          "form": "꽁치김치찌개 맛있죠",
          "original_form": "꽁치김치찌개 맛있죠",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012353.1.7",
          "form": "자주는 못먹는데 밥이랑 김이랑 같이 먹으면",
          "original_form": "자주는 못먹는데 밥이랑 김이랑 같이 먹으면",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012353.1.8",
          "form": "든든해요",
          "original_form": "든든해요",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012353.1.9",
          "form": "그쵸그쵸! 김이 밥도둑이에요",
          "original_form": "그쵸그쵸! 김이 밥도둑이에요",
          "speaker_id": "1",
          "time": "20210824 21:34"
        },
        {
          "id": "MDRW2100012353.1.10",
          "form": "갑자기 드는 생각인데",
          "original_form": "갑자기 드는 생각인데",
          "speaker_id": "2",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100012353.1.11",
          "form": "김 정말 김치만큼이나",
          "original_form": "김 정말 김치만큼이나",
          "speaker_id": "2",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100012353.1.12",
          "form": "소중한 반찬이라는 걸 깨달았어요",
          "original_form": "소중한 반찬이라는 걸 깨달았어요",
          "speaker_id": "2",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100012353.1.13",
          "form": "맞아요.... 진짜",
          "original_form": "맞아요.... 진짜",
          "speaker_id": "1",
          "time": "20210824 21:38"
        },
        {
          "id": "MDRW2100012353.1.14",
          "form": "어느곳이나 잘어울리는 김",
          "original_form": "어느곳이나 잘어울리는 김",
          "speaker_id": "2",
          "time": "20210824 21:38"
        },
        {
          "id": "MDRW2100012353.1.15",
          "form": "밥만 있으면 한끼 뚝딱이죠",
          "original_form": "밥만 있으면 한끼 뚝딱이죠",
          "speaker_id": "1",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100012353.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100012353.1.17",
          "form": "현대까지 살아남은 이유가 있어요",
          "original_form": "현대까지 살아남은 이유가 있어요",
          "speaker_id": "2",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100012353.1.18",
          "form": "혹시 민초 드시나요?",
          "original_form": "혹시 민초 드시나요?",
          "speaker_id": "2",
          "time": "20210824 21:41"
        },
        {
          "id": "MDRW2100012353.1.19",
          "form": "어쩌다 가끔 먹는거 같아요!",
          "original_form": "어쩌다 가끔 먹는거 같아요!",
          "speaker_id": "1",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100012353.1.20",
          "form": "민초 드시는군요",
          "original_form": "민초 드시는군요",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012353.1.21",
          "form": "전 민초파입니다",
          "original_form": "전 민초파입니다",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012353.1.22",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012353.1.23",
          "form": "민초 극혐하는 사람들은 엄청 심해서",
          "original_form": "민초 극혐하는 사람들은 엄청 심해서",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012353.1.24",
          "form": "같이는 잘 못먹지만",
          "original_form": "같이는 잘 못먹지만",
          "speaker_id": "2",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012353.1.25",
          "form": "그래도 좋아해요",
          "original_form": "그래도 좋아해요",
          "speaker_id": "2",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012353.1.26",
          "form": "취향차이니까 서로 존중해야죠~! 저는 파인애플 피자 좋아해요",
          "original_form": "취향차이니까 서로 존중해야죠~! 저는 파인애플 피자 좋아해요",
          "speaker_id": "1",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012353.1.27",
          "form": "아 저도 파인애플 피자 좋아해요",
          "original_form": "아 저도 파인애플 피자 좋아해요",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012353.1.28",
          "form": "이것도 호불호가 꽤 갈리더라구요",
          "original_form": "이것도 호불호가 꽤 갈리더라구요",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012353.1.29",
          "form": "저는 따뜻한 과일을 좋아해요. 풍미가 살아나는 느낌이 있어요",
          "original_form": "저는 따뜻한 과일을 좋아해요. 풍미가 살아나는 느낌이 있어요",
          "speaker_id": "1",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100012353.1.30",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012353.1.31",
          "form": "단 맛이 더 잘 느껴지죠",
          "original_form": "단 맛이 더 잘 느껴지죠",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012353.1.32",
          "form": "그쵸그쵸!!!!",
          "original_form": "그쵸그쵸!!!!",
          "speaker_id": "1",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012353.1.33",
          "form": "제주에선 귤도 불에 구워먹는다더라구요",
          "original_form": "제주에선 귤도 불에 구워먹는다더라구요",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012353.1.34",
          "form": "이색적이긴한데 맛있을것같아요",
          "original_form": "이색적이긴한데 맛있을것같아요",
          "speaker_id": "2",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100012353.1.35",
          "form": "오...! 처음듣는 정보네요. 맛있겠어요",
          "original_form": "오...! 처음듣는 정보네요. 맛있겠어요",
          "speaker_id": "1",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100012353.1.36",
          "form": "약간 귤 잼 같은 느낌?",
          "original_form": "약간 귤 잼 같은 느낌?",
          "speaker_id": "2",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100012353.1.37",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "2",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012353.1.38",
          "form": "디저트에 올라간 과일들도",
          "original_form": "디저트에 올라간 과일들도",
          "speaker_id": "2",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012353.1.39",
          "form": "구워져서 올라간 경우가 많네요",
          "original_form": "구워져서 올라간 경우가 많네요",
          "speaker_id": "2",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012353.1.40",
          "form": "사실상 다들 따뜻한 과일에 익숙한거죠",
          "original_form": "사실상 다들 따뜻한 과일에 익숙한거죠",
          "speaker_id": "2",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012353.1.41",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012353.1.42",
          "form": "맞아요! 고로 파인애플 피자도 숭상해야죠",
          "original_form": "맞아요! 고로 파인애플 피자도 숭상해야죠",
          "speaker_id": "1",
          "time": "20210824 22:02"
        }
      ]
    }
