      "utterance": [
        {
          "id": "MDRW2100044039.1.1",
          "form": "백신은 다 맞으셨어요?",
          "original_form": "백신은 다 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044039.1.2",
          "form": "네 저 딱 오늘 2차완료했어요",
          "original_form": "네 저 딱 오늘 2차완료했어요",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044039.1.3",
          "form": "헉 오늘 맞으셨구나!",
          "original_form": "헉 오늘 맞으셨구나!",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044039.1.4",
          "form": "화이자 맞으셨어요? 괜찮으세요?",
          "original_form": "화이자 맞으셨어요? 괜찮으세요?",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044039.1.5",
          "form": "네 오른팔이 좀욱식거랴요",
          "original_form": "네 오른팔이 좀욱식거랴요",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044039.1.6",
          "form": "모더나여",
          "original_form": "모더나여",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044039.1.7",
          "form": "헉 모더나요?? 모더나 맞은 분들 잘 없던데 신기해요!",
          "original_form": "헉 모더나요?? 모더나 맞은 분들 잘 없던데 신기해요!",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044039.1.8",
          "form": "팔이 좀 욱신거리시는구나ㅠㅠ",
          "original_form": "팔이 좀 욱신거리시는구나ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044039.1.9",
          "form": "맞고나서 하루이틀은 좀 아프다고 하더라구요ㅠㅠㅠ",
          "original_form": "맞고나서 하루이틀은 좀 아프다고 하더라구요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044039.1.10",
          "form": "네 전 사전예약으로 잡았었는데 그때모더나더라구여",
          "original_form": "네 전 사전예약으로 잡았었는데 그때모더나더라구여",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044039.1.11",
          "form": "오오ㅎㅎㅎ",
          "original_form": "오오ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044039.1.12",
          "form": "다 비슷비슷하겠지만",
          "original_form": "다 비슷비슷하겠지만",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044039.1.13",
          "form": "모더나가 젤 좋다고하던데",
          "original_form": "모더나가 젤 좋다고하던데",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044039.1.14",
          "form": "좋은 백신 맞으셨네요!",
          "original_form": "좋은 백신 맞으셨네요!",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044039.1.15",
          "form": "아 그래여??",
          "original_form": "아 그래여??",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044039.1.16",
          "form": "근데 쿡쿡쑤싱는증상이 1차때있었어서 ㅎㅎ....ㅜ",
          "original_form": "근데 쿡쿡쑤싱는증상이 1차때있었어서 ㅎㅎ....ㅜ",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044039.1.17",
          "form": "조금무섭긴해요",
          "original_form": "조금무섭긴해요",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044039.1.18",
          "form": "아하ㅜㅜㅜㅜ",
          "original_form": "아하ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044039.1.19",
          "form": "근데 심장만 안 아프면 되는 것 같더라구요!",
          "original_form": "근데 심장만 안 아프면 되는 것 같더라구요!",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044039.1.20",
          "form": "초큼 무서운",
          "original_form": "초큼 무서운",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044039.1.21",
          "form": "심장이 아프진 않으시죠??",
          "original_form": "심장이 아프진 않으시죠??",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044039.1.22",
          "form": "네 그 심장이 콕콕쑤시는 ㅋㅋㅋㅋ",
          "original_form": "네 그 심장이 콕콕쑤시는 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044039.1.23",
          "form": "소염제먹으면 괜찮아지더라구여",
          "original_form": "소염제먹으면 괜찮아지더라구여",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044039.1.24",
          "form": "아이고ㅠㅠ 심장이 쑤시신다니... 놀라셨겠어요",
          "original_form": "아이고ㅠㅠ 심장이 쑤시신다니... 놀라셨겠어요",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044039.1.25",
          "form": "다행이네요~~ 하루이틀만 버티면 괜찮아지실거에요!!",
