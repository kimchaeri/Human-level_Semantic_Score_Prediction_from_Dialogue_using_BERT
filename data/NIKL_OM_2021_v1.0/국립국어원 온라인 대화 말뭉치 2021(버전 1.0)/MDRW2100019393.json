      "utterance": [
        {
          "id": "MDRW2100019393.1.1",
          "form": "name2님 저녁은 드셨나용",
          "original_form": "&name2&님 저녁은 드셨나용",
          "speaker_id": "1",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019393.1.2",
          "form": "저 엄마가",
          "original_form": "저 엄마가",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019393.1.3",
          "form": "치킨 순살넣어야하는데 뼈넣어서",
          "original_form": "치킨 순살넣어야하는데 뼈넣어서",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019393.1.4",
          "form": "그거먹었어욜",
          "original_form": "그거먹었어욜",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019393.1.5",
          "form": "갈릭막왕",
          "original_form": "갈릭막왕",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019393.1.6",
          "form": "갈릭마왕 맛잇어요? 다음에 먹어볼만할까유",
          "original_form": "갈릭마왕 맛잇어요? 다음에 먹어볼만할까유",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100019393.1.7",
          "form": "아니 근데 다른사람히고 얘기하다들은건데 그거 평이 최악이래요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니 근데 다른사람히고 얘기하다들은건데 그거 평이 최악이래요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100019393.1.8",
          "form": "나중에 울집와서 먹어봐유",
          "original_form": "나중에 울집와서 먹어봐유",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100019393.1.9",
          "form": "헐 진짜요? 왜 최악이지ㅠㅠ 힝",
          "original_form": "헐 진짜요? 왜 최악이지ㅠㅠ 힝",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100019393.1.10",
          "form": "그니깐여 저는 맛있든데 ㅎㅎ",
          "original_form": "그니깐여 저는 맛있든데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100019393.1.11",
          "form": "뭔가 마늘이랑 구운치킨생각하면 맛있을수밖에 없는데!!!",
          "original_form": "뭔가 마늘이랑 구운치킨생각하면 맛있을수밖에 없는데!!!",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100019393.1.12",
          "form": "근데 저는 소스를 반만 넣어먹거든요?! 그럼딱좋더라규요",
          "original_form": "근데 저는 소스를 반만 넣어먹거든요?! 그럼딱좋더라규요",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100019393.1.13",
          "form": "아 소스가 마늘소스에요? 사람들이 마늘이넘진해서 싫어하나?",
          "original_form": "아 소스가 마늘소스에요? 사람들이 마늘이넘진해서 싫어하나?",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100019393.1.14",
          "form": "한국인이 그럴리가없는뎅ㅋㅋㄴ",
          "original_form": "한국인이 그럴리가없는뎅ㅋㅋㄴ",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100019393.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저두 잘모르겠어요 담에와서 평가해주세요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저두 잘모르겠어요 담에와서 평가해주세요",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100019393.1.16",
          "form": "넹 좋아요 ㅋㅋㅋ히히 아 늦은시간인데 갑자기 맛있는 케이크 먹구싶어요ㅠ",
          "original_form": "넹 좋아요 ㅋㅋㅋ히히 아 늦은시간인데 갑자기 맛있는 케이크 먹구싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100019393.1.17",
          "form": "근데 은근 요즘 진짜 맛있는 케익집이없는것같아서 슬퍼요ㅠ",
          "original_form": "근데 은근 요즘 진짜 맛있는 케익집이없는것같아서 슬퍼요ㅠ",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100019393.1.18",
          "form": "아니!! 공릉쪽에 맛있는데 맛잖아여!!!",
          "original_form": "아니!! 공릉쪽에 맛있는데 맛잖아여!!!",
          "speaker_id": "2",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100019393.1.19",
          "form": "저 내일 복숭아케이크사러 나갈거에요",
          "original_form": "저 내일 복숭아케이크사러 나갈거에요",
          "speaker_id": "2",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100019393.1.20",
          "form": "ㅋㅋ우왕 어디로갈거에요? 유명한곳있어요?",
          "original_form": "ㅋㅋ우왕 어디로갈거에요? 유명한곳있어요?",
          "speaker_id": "1",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100019393.1.21",
          "form": "맛있는곳있어요? 복숭아케이크ㅠㅠㅠㅠ",
          "original_form": "맛있는곳있어요? 복숭아케이크ㅠㅠㅠㅠ",
