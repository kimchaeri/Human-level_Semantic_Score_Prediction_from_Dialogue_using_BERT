        },
        {
          "id": "MDRW2100008254.1.2",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "2",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100008254.1.3",
          "form": "헉 이 주제 신선하네요",
          "original_form": "헉 이 주제 신선하네요",
          "speaker_id": "2",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100008254.1.4",
          "form": "이상형이 어케되시나요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이상형이 어케되시나요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008254.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008254.1.6",
          "form": "저도 이 주제는 신선해요",
          "original_form": "저도 이 주제는 신선해요",
          "speaker_id": "1",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008254.1.7",
          "form": "이상형.. 귀여운사람?",
          "original_form": "이상형.. 귀여운사람?",
          "speaker_id": "1",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008254.1.8",
          "form": "서브남주 스타일 좋아해요ㅋㅋ",
          "original_form": "서브남주 스타일 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008254.1.9",
          "form": "헐 다정남 좋아하시는구나",
          "original_form": "헐 다정남 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008254.1.10",
          "form": "약간 갈발에 갱얼쥐 상",
          "original_form": "약간 갈발에 갱얼쥐 상",
          "speaker_id": "1",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008254.1.11",
          "form": "혹시 저에 맞춰 투디로 대답해주시는거냐구욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "혹시 저에 맞춰 투디로 대답해주시는거냐구욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008254.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아 그치만",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아 그치만",
          "speaker_id": "1",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008254.1.13",
          "form": "닉보고 ㅎㅎ 웹소웹툰얘기해야지",
          "original_form": "닉보고 ㅎㅎ 웹소웹툰얘기해야지",
          "speaker_id": "2",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100008254.1.14",
          "form": "하고 왔는걸요",
          "original_form": "하고 왔는걸요",
          "speaker_id": "1",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100008254.1.15",
          "form": "맞아요 옅은 머리색이 너무 좋아요",
          "original_form": "맞아요 옅은 머리색이 너무 좋아요",
          "speaker_id": "2",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100008254.1.16",
          "form": "강아지상 최고",
          "original_form": "강아지상 최고",
          "speaker_id": "1",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100008254.1.17",
          "form": "저는 대놓고 잘해주는 사람이 좋아서 ㅎ",
          "original_form": "저는 대놓고 잘해주는 사람이 좋아서 ㅎ",
          "speaker_id": "1",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100008254.1.18",
          "form": "예전엔 약간 차가운 남주가 유행이었는데 요즘은",
          "original_form": "예전엔 약간 차가운 남주가 유행이었는데 요즘은",
          "speaker_id": "2",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008254.1.19",
          "form": "섭남같은 남주가 로맨스에 잘 나오는 것같지만",
          "original_form": "섭남같은 남주가 로맨스에 잘 나오는 것같지만",
          "speaker_id": "2",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008254.1.20",
          "form": "그가 섭남일 때 매력이 있는 것(?)같아요",
          "original_form": "그가 섭남일 때 매력이 있는 것(?)같아요",
          "speaker_id": "2",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008254.1.21",
          "form": "저는 북부대공스타일이 이상형이긴하지만",
          "original_form": "저는 북부대공스타일이 이상형이긴하지만",
          "speaker_id": "1",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008254.1.22",
          "form": "다들 맘에 섭남 하나는 품고살잖어요",
          "original_form": "다들 맘에 섭남 하나는 품고살잖어요",
          "speaker_id": "2",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008254.1.23",
          "form": "ㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008254.1.24",
          "form": "섭남은 섭남일때 가장",
          "original_form": "섭남은 섭남일때 가장",
          "speaker_id": "1",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008254.1.25",
          "form": "매력적인거 같아요",
          "original_form": "매력적인거 같아요",
          "speaker_id": "1",
          "time": "20210822 14:39"
        }
      ]
    }
