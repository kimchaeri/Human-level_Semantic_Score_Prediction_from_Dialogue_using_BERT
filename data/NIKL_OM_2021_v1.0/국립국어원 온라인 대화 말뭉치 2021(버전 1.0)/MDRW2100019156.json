      "utterance": [
        {
          "id": "MDRW2100019156.1.1",
          "form": "로제떡볶이좋아하세요?",
          "original_form": "로제떡볶이좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100019156.1.2",
          "form": "네 좋아해요 드셔보셨어요?",
          "original_form": "네 좋아해요 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019156.1.3",
          "form": "네 저 배떡 먹어봤어요 ㅋㅋㅋ",
          "original_form": "네 저 배떡 먹어봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019156.1.4",
          "form": "진짜 맛있던데..",
          "original_form": "진짜 맛있던데..",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019156.1.5",
          "form": "맞아요 ㅠㅠ 근데 떡군이네 표절이라고 해서",
          "original_form": "맞아요 ㅠㅠ 근데 떡군이네 표절이라고 해서",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019156.1.6",
          "form": "전 신전으로 갈아탔어요 ㅠ",
          "original_form": "전 신전으로 갈아탔어요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019156.1.7",
          "form": "헐?",
          "original_form": "헐?",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019156.1.8",
          "form": "떡군이네가 그럼 원조인건가요?",
          "original_form": "떡군이네가 그럼 원조인건가요?",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019156.1.9",
          "form": "네 ㅠㅠ 거기일하던 사람이레시피 들고 튀어서 만든게",
          "original_form": "네 ㅠㅠ 거기일하던 사람이레시피 들고 튀어서 만든게",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019156.1.10",
          "form": "배떡이래요",
          "original_form": "배떡이래요",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019156.1.11",
          "form": "진짜 몰랐어요",
          "original_form": "진짜 몰랐어요",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019156.1.12",
          "form": "나쁜사람들이네..",
          "original_form": "나쁜사람들이네..",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019156.1.13",
          "form": "신전도 로제떡볶이 생겼나요?",
          "original_form": "신전도 로제떡볶이 생겼나요?",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019156.1.14",
          "form": "그러니까요 ㅠ 너무하죠",
          "original_form": "그러니까요 ㅠ 너무하죠",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019156.1.15",
          "form": "안먹은지 꽤됐는데",
          "original_form": "안먹은지 꽤됐는데",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019156.1.16",
          "form": "네 신전이 전 배떡이랑 제일 비슷하다 느꼈어요",
          "original_form": "네 신전이 전 배떡이랑 제일 비슷하다 느꼈어요",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019156.1.17",
          "form": "그땐 치즈떡볶이밖에 없었던..",
          "original_form": "그땐 치즈떡볶이밖에 없었던..",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019156.1.18",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019156.1.19",
          "form": "그 특유의..",
          "original_form": "그 특유의..",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019156.1.20",
          "form": "양도 적당해서 혼자먹디 좋더라구요",
          "original_form": "양도 적당해서 혼자먹디 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019156.1.21",
          "form": "카레가루맛? 같은거",
          "original_form": "카레가루맛? 같은거",
          "speaker_id": "1",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019156.1.22",
          "form": "꾸덕꾸덕!",
          "original_form": "꾸덕꾸덕!",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019156.1.23",
          "form": "때문인가봐요",
          "original_form": "때문인가봐요",
          "speaker_id": "1",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019156.1.24",
          "form": "오맞아요",
          "original_form": "오맞아요",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019156.1.25",
          "form": "저도 배떡 먹었을때",
          "original_form": "저도 배떡 먹었을때",
          "speaker_id": "1",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019156.1.26",
          "form": "배떡도 좀 카레맛 나지 않나요?",
          "original_form": "배떡도 좀 카레맛 나지 않나요?",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019156.1.27",
          "form": "신전이랑 맛 비슷하다고",
          "original_form": "신전이랑 맛 비슷하다고",
          "speaker_id": "1",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019156.1.28",
          "form": "생각했는데",
          "original_form": "생각했는데",
          "speaker_id": "1",
          "time": "20210827 15:48"
        }
      ]
    }
  ]
