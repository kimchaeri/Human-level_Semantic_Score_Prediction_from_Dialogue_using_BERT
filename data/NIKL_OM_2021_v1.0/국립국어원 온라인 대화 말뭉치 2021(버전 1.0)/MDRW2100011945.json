      "utterance": [
        {
          "id": "MDRW2100011945.1.1",
          "form": "국내말고 해외는",
          "original_form": "국내말고 해외는",
          "speaker_id": "2",
          "time": "20210824 17:39"
        },
        {
          "id": "MDRW2100011945.1.2",
          "form": "가고싶은데 있어?",
          "original_form": "가고싶은데 있어?",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011945.1.3",
          "form": "나 유럽!",
          "original_form": "나 유럽!",
          "speaker_id": "1",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011945.1.4",
          "form": "유럽 환상도 깨지기는 했는데",
          "original_form": "유럽 환상도 깨지기는 했는데",
          "speaker_id": "1",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011945.1.5",
          "form": "영어공부 열심히 하고 간다음에 가서 영어 써보고 싶어",
          "original_form": "영어공부 열심히 하고 간다음에 가서 영어 써보고 싶어",
          "speaker_id": "1",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011945.1.6",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011945.1.7",
          "form": "나도 유럽...ㅋㅋㅋㅋㅋㅋ나도 환상 진짜많이 깨졌는데",
          "original_form": "나도 유럽...ㅋㅋㅋㅋㅋㅋ나도 환상 진짜많이 깨졌는데",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011945.1.8",
          "form": "그래도 예쁜건물이나 풍경 이런거 구경하고 힐링하고싶음",
          "original_form": "그래도 예쁜건물이나 풍경 이런거 구경하고 힐링하고싶음",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011945.1.9",
          "form": "근데 사람많은 시기말고",
          "original_form": "근데 사람많은 시기말고",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011945.1.10",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011945.1.11",
          "form": "돈많이벌어서 무조건 사람없는시기에 한적하게 여행해야함",
          "original_form": "돈많이벌어서 무조건 사람없는시기에 한적하게 여행해야함",
          "speaker_id": "2",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011945.1.12",
          "form": "사람 없는 시기가 있을까...?",
          "original_form": "사람 없는 시기가 있을까...?",
          "speaker_id": "1",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011945.1.13",
          "form": "난 무조건 겨울",
          "original_form": "난 무조건 겨울",
          "speaker_id": "1",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011945.1.14",
          "form": "겨울 좋지",
          "original_form": "겨울 좋지",
          "speaker_id": "2",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011945.1.15",
          "form": "영국의 해리포터 겨울느낌을",
          "original_form": "영국의 해리포터 겨울느낌을",
          "speaker_id": "1",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011945.1.16",
          "form": "원한다..ㅡ",
          "original_form": "원한다..ㅡ",
          "speaker_id": "1",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011945.1.17",
          "form": "와....겨울에 꼭가자",
          "original_form": "와....겨울에 꼭가자",
          "speaker_id": "2",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011945.1.18",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "1",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011945.1.19",
          "form": "가서 그 9 3/4승강장가서",
          "original_form": "가서 그 9 3/4승강장가서",
          "speaker_id": "2",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011945.1.20",
          "form": "머리박고오자",
          "original_form": "머리박고오자",
          "speaker_id": "2",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011945.1.21",
          "form": "친구끼리 다니면",
          "original_form": "친구끼리 다니면",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011945.1.22",
          "form": "싸운다니까",
          "original_form": "싸운다니까",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011945.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011945.1.24",
          "form": "따로 여행하다가",
          "original_form": "따로 여행하다가",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011945.1.25",
          "form": "만나서 또 여행하고",
          "original_form": "만나서 또 여행하고",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011945.1.26",
          "form": "각자 가고 싶은곳 같다가",
          "original_form": "각자 가고 싶은곳 같다가",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011945.1.27",
          "form": "같이가고 그러자",
          "original_form": "같이가고 그러자",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011945.1.28",
          "form": "보더로운 생활 그 언니도 그렇게 했더라",
          "original_form": "보더로운 생활 그 언니도 그렇게 했더라",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011945.1.29",
          "form": "근데 피곤해서 싸울일이 생길 수 밖에 없을것 같아",
          "original_form": "근데 피곤해서 싸울일이 생길 수 밖에 없을것 같아",
          "speaker_id": "1",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011945.1.30",
          "form": "오 그거도 좋겠다",
          "original_form": "오 그거도 좋겠다",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011945.1.31",
          "form": "확실히 우리는 체력이나 에너지가 쓰..레..기니까ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "확실히 우리는 체력이나 에너지가 쓰..레..기니까ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011945.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011945.1.33",
          "form": "아니야",
          "original_form": "아니야",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011945.1.34",
          "form": "가고싶은데 갔다와서 숙소에서 같이 얘기하고 그러면",
          "original_form": "가고싶은데 갔다와서 숙소에서 같이 얘기하고 그러면",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011945.1.35",
          "form": "진짜좋을듯",
          "original_form": "진짜좋을듯",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011945.1.36",
          "form": "건강한 몸 만들어서",
          "original_form": "건강한 몸 만들어서",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011945.1.37",
          "form": "구석구석 돌아다녀야지",
          "original_form": "구석구석 돌아다녀야지",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011945.1.38",
          "form": "난 영국 갔디가 아이스란드도 가고 싶어",
          "original_form": "난 영국 갔디가 아이스란드도 가고 싶어",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011945.1.39",
          "form": "아이스란드에서 오로라 보고 싶어",
          "original_form": "아이스란드에서 오로라 보고 싶어",
          "speaker_id": "1",
          "time": "20210824 17:46"
        }
      ]
    }
  ]
