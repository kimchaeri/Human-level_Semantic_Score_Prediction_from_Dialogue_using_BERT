      "utterance": [
        {
          "id": "MDRW2100033960.1.1",
          "form": "아 이어서 말하자",
          "original_form": "아 이어서 말하자",
          "speaker_id": "2",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100033960.1.2",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100033960.1.3",
          "form": "추리게임 미해결사건부 하는중",
          "original_form": "추리게임 미해결사건부 하는중",
          "speaker_id": "2",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100033960.1.4",
          "form": "아 나도 광고하는 거 봤어",
          "original_form": "아 나도 광고하는 거 봤어",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100033960.1.5",
          "form": "어때?",
          "original_form": "어때?",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100033960.1.6",
          "form": "스토리도 꽤 흥미진진하고 재밌어",
          "original_form": "스토리도 꽤 흥미진진하고 재밌어",
          "speaker_id": "2",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100033960.1.7",
          "form": "스토리 진행만 해?",
          "original_form": "스토리 진행만 해?",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100033960.1.8",
          "form": "서브 컨텐츠도 많아",
          "original_form": "서브 컨텐츠도 많아",
          "speaker_id": "2",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100033960.1.9",
          "form": "오 할 거 많은 편?",
          "original_form": "오 할 거 많은 편?",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100033960.1.10",
          "form": "응 꽤 많아",
          "original_form": "응 꽤 많아",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100033960.1.11",
          "form": "별로 지루하지 않아서 괜찮아",
          "original_form": "별로 지루하지 않아서 괜찮아",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100033960.1.12",
          "form": "재밌겠네",
          "original_form": "재밌겠네",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100033960.1.13",
          "form": "응 이것저것 되게 많아",
          "original_form": "응 이것저것 되게 많아",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100033960.1.14",
          "form": "어떤 내용이야?",
          "original_form": "어떤 내용이야?",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100033960.1.15",
          "form": "주인공이 변호사인데 변호하려고 조사하고 재판도 진행해",
          "original_form": "주인공이 변호사인데 변호하려고 조사하고 재판도 진행해",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100033960.1.16",
          "form": "어렵지 않아서 재밌게 하는중",
          "original_form": "어렵지 않아서 재밌게 하는중",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100033960.1.17",
          "form": "어렵지 않은 건 좋네",
          "original_form": "어렵지 않은 건 좋네",
          "speaker_id": "1",
          "time": "20210825 16:55"
        }
      ]
