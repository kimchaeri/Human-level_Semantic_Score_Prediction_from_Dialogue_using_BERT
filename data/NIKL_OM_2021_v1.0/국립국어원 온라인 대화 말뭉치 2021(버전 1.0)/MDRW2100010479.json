        {
          "id": "MDRW2100010479.1.1",
          "form": "온라인 쇼핑 하시나요",
          "original_form": "온라인 쇼핑 하시나요",
          "speaker_id": "1",
          "time": "20210823 18:42"
        },
        {
          "id": "MDRW2100010479.1.2",
          "form": "네 자주해요",
          "original_form": "네 자주해요",
          "speaker_id": "2",
          "time": "20210823 18:58"
        },
        {
          "id": "MDRW2100010479.1.3",
          "form": "저도 자주 이용 해요",
          "original_form": "저도 자주 이용 해요",
          "speaker_id": "1",
          "time": "20210824 06:37"
        },
        {
          "id": "MDRW2100010479.1.4",
          "form": "최근에 무얼 사셨는지요",
          "original_form": "최근에 무얼 사셨는지요",
          "speaker_id": "1",
          "time": "20210824 06:37"
        },
        {
          "id": "MDRW2100010479.1.5",
          "form": "변기 청소 용품이요ㅋㅋ",
          "original_form": "변기 청소 용품이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100010479.1.6",
          "form": "혹 홈쇼핌 방송에 나오는 것 인가요",
          "original_form": "혹 홈쇼핌 방송에 나오는 것 인가요",
          "speaker_id": "1",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100010479.1.7",
          "form": "그건 모르겠는데ㅋㅋ 그 파란거로 닦는",
          "original_form": "그건 모르겠는데ㅋㅋ 그 파란거로 닦는",
          "speaker_id": "2",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100010479.1.8",
          "form": "네 맞아요 닦 고 버리는 청소용품 ....\r\n잘되려나 모르겠네요",
          "original_form": "네 맞아요 닦 고 버리는 청소용품 ....\r\n잘되려나 모르겠네요",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100010479.1.9",
          "form": "은근 만족스럽던데요!",
          "original_form": "은근 만족스럽던데요!",
          "speaker_id": "2",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100010479.1.10",
          "form": "전 살까 말까 고민중 이었답니다",
          "original_form": "전 살까 말까 고민중 이었답니다",
