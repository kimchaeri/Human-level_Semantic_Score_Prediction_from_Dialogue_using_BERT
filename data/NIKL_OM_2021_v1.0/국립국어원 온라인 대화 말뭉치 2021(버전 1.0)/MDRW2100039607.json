      "utterance": [
        {
          "id": "MDRW2100039607.1.1",
          "form": "저 수요일에 화이자 2차 맞았어요",
          "original_form": "저 수요일에 화이자 2차 맞았어요",
          "speaker_id": "1",
          "time": "20210903 19:35"
        },
        {
          "id": "MDRW2100039607.1.2",
          "form": "오 저는 잔여백신 초기에 맞아서 7월쯤 2차 맞았어요",
          "original_form": "오 저는 잔여백신 초기에 맞아서 7월쯤 2차 맞았어요",
          "speaker_id": "2",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039607.1.3",
          "form": "아제랑 섞인 화이자라는 ㅋㅋ",
          "original_form": "아제랑 섞인 화이자라는 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039607.1.4",
          "form": "우와! 일찍 맞으셨넹ㅅ",
          "original_form": "우와! 일찍 맞으셨넹ㅅ",
          "speaker_id": "1",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039607.1.5",
          "form": "1차 아제 2차 화이자 맞으셨군요",
          "original_form": "1차 아제 2차 화이자 맞으셨군요",
          "speaker_id": "1",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039607.1.6",
          "form": "화이자는 근육통만 있더라구요",
          "original_form": "화이자는 근육통만 있더라구요",
          "speaker_id": "2",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039607.1.7",
          "form": "괜찬으셨나요?",
          "original_form": "괜찬으셨나요?",
          "speaker_id": "1",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039607.1.8",
          "form": "열은 안나시고요?",
          "original_form": "열은 안나시고요?",
          "speaker_id": "1",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039607.1.9",
          "form": "아제는 죽음이더라구요",
          "original_form": "아제는 죽음이더라구요",
          "speaker_id": "2",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039607.1.10",
          "form": "2일내내 약먹어도 38도",
          "original_form": "2일내내 약먹어도 38도",
          "speaker_id": "2",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039607.1.11",
          "form": "아제는어땠는데요?",
          "original_form": "아제는어땠는데요?",
          "speaker_id": "1",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039607.1.12",
          "form": "해열제를 먹어도요?",
          "original_form": "해열제를 먹어도요?",
          "speaker_id": "1",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039607.1.13",
          "form": "독감 증상이랑 같더라구요",
          "original_form": "독감 증상이랑 같더라구요",
          "speaker_id": "2",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039607.1.14",
          "form": "먹으면 37",
          "original_form": "먹으면 37",
          "speaker_id": "2",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039607.1.15",
          "form": "헐... 아제는 1차가 힘들다던데요",
          "original_form": "헐... 아제는 1차가 힘들다던데요",
          "speaker_id": "1",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039607.1.16",
          "form": "그러게요  1차를 아제맞앗어요 ㅜㅜ",
          "original_form": "그러게요  1차를 아제맞앗어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039607.1.17",
          "form": "고생하셨네요 ㅠ",
          "original_form": "고생하셨네요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039607.1.18",
          "form": "님은 어땟어요",
          "original_form": "님은 어땟어요",
          "speaker_id": "2",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039607.1.19",
          "form": "그래도 2차 수월하게 지나가셔서 다행이세요",
          "original_form": "그래도 2차 수월하게 지나가셔서 다행이세요",
          "speaker_id": "1",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039607.1.20",
          "form": "전 겁을 넘  먹어서 1차 맞자마자 타이레놀 먹었거든요",
          "original_form": "전 겁을 넘  먹어서 1차 맞자마자 타이레놀 먹었거든요",
          "speaker_id": "1",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039607.1.21",
          "form": "저는 부작용은 거의 희박한 확률이니 백신 맞능거 안무섭드라구요",
          "original_form": "저는 부작용은 거의 희박한 확률이니 백신 맞능거 안무섭드라구요",
          "speaker_id": "2",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039607.1.22",
          "form": "그래서 그런가 아픈거 모르고 넘어갔어요 4시간 간격으로 24시간 꼬박 먹었어요",
          "original_form": "그래서 그런가 아픈거 모르고 넘어갔어요 4시간 간격으로 24시간 꼬박 먹었어요",
          "speaker_id": "1",
          "time": "20210903 19:39"
        },
        {
          "id": "MDRW2100039607.1.23",
          "form": "헉 ㅋㅋㅋ",
          "original_form": "헉 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:39"
        },
        {
          "id": "MDRW2100039607.1.24",
          "form": "희박한 확률인데 그게 저일수도 있잖아요 ㅠ",
          "original_form": "희박한 확률인데 그게 저일수도 있잖아요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 19:39"
        },
        {
          "id": "MDRW2100039607.1.25",
          "form": "아 안아픈데 드셨군요",
          "original_form": "아 안아픈데 드셨군요",
          "speaker_id": "2",
          "time": "20210903 19:39"
        },
        {
          "id": "MDRW2100039607.1.26",
          "form": "전 화이자 2차는 각오 하셔야 한다고 해서 2차도 엄청 겁 먹었었어요",
          "original_form": "전 화이자 2차는 각오 하셔야 한다고 해서 2차도 엄청 겁 먹었었어요",
          "speaker_id": "1",
          "time": "20210903 19:39"
        }
      ]
    }
