      "utterance": [
        {
          "id": "MDRW2100005646.1.1",
          "form": "밤이요",
          "original_form": "밤이요",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005646.1.2",
          "form": "저는.퍽퍽한걸 좋아해요",
          "original_form": "저는.퍽퍽한걸 좋아해요",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005646.1.3",
          "form": "호박은 너무 달고..",
          "original_form": "호박은 너무 달고..",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005646.1.4",
          "form": "저두 밤고구마 좋아해요",
          "original_form": "저두 밤고구마 좋아해요",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005646.1.5",
          "form": "퍽퍽하고 밤맛나는 게 좋더라구요",
          "original_form": "퍽퍽하고 밤맛나는 게 좋더라구요",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005646.1.6",
          "form": "고구마 요리 좋아하는 거 있으신가오",
          "original_form": "고구마 요리 좋아하는 거 있으신가오",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005646.1.7",
          "form": "저는 고구마는 쪄먹는것만 좋아하고... 고구마전이나 마탕은 종종 먹음 좋아해요",
          "original_form": "저는 고구마는 쪄먹는것만 좋아하고... 고구마전이나 마탕은 종종 먹음 좋아해요",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100005646.1.8",
          "form": "고구마 활용한 요리는 정말 맛있는거같아요",
          "original_form": "고구마 활용한 요리는 정말 맛있는거같아요",
          "speaker_id": "1",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100005646.1.9",
          "form": "약간 양념을 따로 하지않아도 고유의 달콤함이 있어서 좋더라구요",
          "original_form": "약간 양념을 따로 하지않아도 고유의 달콤함이 있어서 좋더라구요",
          "speaker_id": "1",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100005646.1.10",
          "form": "고구마 줄기무침도 먹을만 해요",
          "original_form": "고구마 줄기무침도 먹을만 해요",
          "speaker_id": "2",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100005646.1.11",
          "form": "저도 그거좋아해요",
          "original_form": "저도 그거좋아해요",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100005646.1.12",
          "form": "그냥 먹는것도 좋고 비빔밥에 넣어먹어도 좋아요",
          "original_form": "그냥 먹는것도 좋고 비빔밥에 넣어먹어도 좋아요",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100005646.1.13",
          "form": "사실 전 있으면 먹고 없으면 두는편...ㅋㅋ",
          "original_form": "사실 전 있으면 먹고 없으면 두는편...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100005646.1.14",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100005646.1.15",
          "form": "약간 고구마의 모든 것은 잘 드시는 거 같아요",
          "original_form": "약간 고구마의 모든 것은 잘 드시는 거 같아요",
          "speaker_id": "1",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100005646.1.16",
          "form": "좋아하는데 사실",
          "original_form": "좋아하는데 사실",
          "speaker_id": "2",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100005646.1.17",
          "form": "요즘은 잘 안 찾게 되네요",
          "original_form": "요즘은 잘 안 찾게 되네요",
          "speaker_id": "2",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100005646.1.18",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100005646.1.19",
          "form": "입맛이 없나요?",
          "original_form": "입맛이 없나요?",
          "speaker_id": "1",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100005646.1.20",
          "form": "그래도 오랜만에 먹고싶어요 ㅋㅋㅋ",
          "original_form": "그래도 오랜만에 먹고싶어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100005646.1.21",
          "form": "어...그냥..",
          "original_form": "어...그냥..",
          "speaker_id": "2",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100005646.1.22",
          "form": "별 이유는없어요 안찾게돼요",
          "original_form": "별 이유는없어요 안찾게돼요",
          "speaker_id": "2",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100005646.1.23",
          "form": "앗.. 그럴 수 있어요! ㅎㅎ",
          "original_form": "앗.. 그럴 수 있어요! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:17"
        },
        {
          "id": "MDRW2100005646.1.24",
          "form": "먹고싶을 때 먹으면 되는거죠",
          "original_form": "먹고싶을 때 먹으면 되는거죠",
          "speaker_id": "1",
          "time": "20210822 15:17"
        },
        {
          "id": "MDRW2100005646.1.25",
          "form": "그렇죠?",
          "original_form": "그렇죠?",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100005646.1.26",
          "form": "맞아여 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아여 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:50"
        }
      ]
    }
