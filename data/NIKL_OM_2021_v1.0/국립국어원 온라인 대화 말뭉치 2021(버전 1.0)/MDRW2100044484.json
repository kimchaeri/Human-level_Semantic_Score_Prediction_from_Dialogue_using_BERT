      "utterance": [
        {
          "id": "MDRW2100044484.1.1",
          "form": "최근에 어떤 물건 사셨어요?",
          "original_form": "최근에 어떤 물건 사셨어요?",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044484.1.2",
          "form": "얼마 전에",
          "original_form": "얼마 전에",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044484.1.3",
          "form": "성유리가 광고하는 유리들 앰플 스틱 샀어요",
          "original_form": "성유리가 광고하는 유리들 앰플 스틱 샀어요",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044484.1.4",
          "form": "오~? 좋은가요?",
          "original_form": "오~? 좋은가요?",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044484.1.5",
          "form": "내 촉촉한 게",
          "original_form": "내 촉촉한 게",
          "speaker_id": "2",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100044484.1.6",
          "form": "좋은거같아요 ^^",
          "original_form": "좋은거같아요 ^^",
          "speaker_id": "2",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100044484.1.7",
          "form": "앰플스틱 사니 \n마스프팩 10개 주더라구요",
          "original_form": "앰플스틱 사니 \n마스프팩 10개 주더라구요",
          "speaker_id": "2",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100044484.1.8",
          "form": "오 마스크팩 10개!\n사은품도 좋네요~",
          "original_form": "오 마스크팩 10개!\n사은품도 좋네요~",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100044484.1.9",
          "form": "사은품이 탐났답니다 ㅎㅎ",
          "original_form": "사은품이 탐났답니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100044484.1.10",
          "form": "어제는",
          "original_form": "어제는",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100044484.1.11",
          "form": "아이 색연필 싸인펜도 샀구요",
          "original_form": "아이 색연필 싸인펜도 샀구요",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100044484.1.12",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100044484.1.13",
          "form": "전 아이브로우 브러쉬 사고 싶어요ㅋㅋ\n너무 오래되서 바꿔야 할것 같아요",
          "original_form": "전 아이브로우 브러쉬 사고 싶어요ㅋㅋ\n너무 오래되서 바꿔야 할것 같아요",
          "speaker_id": "1",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100044484.1.14",
          "form": "브러쉬가\n은근 비싸죠",
          "original_form": "브러쉬가\n은근 비싸죠",
          "speaker_id": "2",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100044484.1.15",
          "form": "저는 화장을 잘안해서 화장품이 별로 없는데",
          "original_form": "저는 화장을 잘안해서 화장품이 별로 없는데",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100044484.1.16",
          "form": "전에 한 번 브러쉬 종류를 사려고 보니까",
          "original_form": "전에 한 번 브러쉬 종류를 사려고 보니까",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100044484.1.17",
          "form": "꽤나 비싸더라구요",
          "original_form": "꽤나 비싸더라구요",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100044484.1.18",
          "form": "저도 화장 안 했는데 이제 일 시작해서 화장해야하더라고여ㅎㅎㅎ",
          "original_form": "저도 화장 안 했는데 이제 일 시작해서 화장해야하더라고여ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100044484.1.19",
          "form": "ㅎㅎㅎ 화장품 이것저것 사는",
          "original_form": "ㅎㅎㅎ 화장품 이것저것 사는",
          "speaker_id": "2",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100044484.1.20",
          "form": "재미가 쏠쏠하겠어요",
          "original_form": "재미가 쏠쏠하겠어요",
          "speaker_id": "2",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100044484.1.21",
          "form": "ㅋㅋㅋ너무 비싸서 몇 개 사니 금액 초과",
          "original_form": "ㅋㅋㅋ너무 비싸서 몇 개 사니 금액 초과",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100044484.1.22",
          "form": "ㅎㅎㅎㅎㅎ 그렇죠",
          "original_form": "ㅎㅎㅎㅎㅎ 그렇죠",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100044484.1.23",
          "form": "화장품 너무 비싸요",
          "original_form": "화장품 너무 비싸요",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100044484.1.24",
          "form": "맞아요 ㅠ 안 사다 사려니까 더 아까운..",
          "original_form": "맞아요 ㅠ 안 사다 사려니까 더 아까운..",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100044484.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100044484.1.26",
          "form": "저도 화장을 자주 안 하니까",
          "original_form": "저도 화장을 자주 안 하니까",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100044484.1.27",
          "form": "늘 조금 쓰다가 버리거든요",
          "original_form": "늘 조금 쓰다가 버리거든요",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100044484.1.28",
          "form": "유통기한이 지나서",
          "original_form": "유통기한이 지나서",
          "speaker_id": "2",
