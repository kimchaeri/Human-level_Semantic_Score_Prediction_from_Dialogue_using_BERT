      "utterance": [
        {
          "id": "MDRW2100040946.1.1",
          "form": "못먹는과일 있으신가요?!",
          "original_form": "못먹는과일 있으신가요?!",
          "speaker_id": "2",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100040946.1.2",
          "form": "하나도 없어요!",
          "original_form": "하나도 없어요!",
          "speaker_id": "1",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100040946.1.3",
          "form": "전 멜론못먹어요...ㅜㅜ",
          "original_form": "전 멜론못먹어요...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100040946.1.4",
          "form": "안 먹어본 과일은 있을지언정 못먹는건 아직 없었어요",
          "original_form": "안 먹어본 과일은 있을지언정 못먹는건 아직 없었어요",
          "speaker_id": "1",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100040946.1.5",
          "form": "아 ㅠ 알러지??",
          "original_form": "아 ㅠ 알러지??",
          "speaker_id": "1",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100040946.1.6",
          "form": "아니면",
          "original_form": "아니면",
          "speaker_id": "1",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100040946.1.7",
          "form": "나쁜 사건때문에 그런가요?",
          "original_form": "나쁜 사건때문에 그런가요?",
          "speaker_id": "1",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100040946.1.8",
          "form": "알러지요 ㅜㅜ",
          "original_form": "알러지요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100040946.1.9",
          "form": "심할때는 바나나에도 반응합니다 ㅋㅋㅋ",
          "original_form": "심할때는 바나나에도 반응합니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100040946.1.10",
          "form": "으악...ㅠ 알러지때문에 못 먹는게 제일 억울해요 ㅠ",
          "original_form": "으악...ㅠ 알러지때문에 못 먹는게 제일 억울해요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100040946.1.11",
          "form": "제가 꽃가루 알러지가 있거든요?",
          "original_form": "제가 꽃가루 알러지가 있거든요?",
          "speaker_id": "2",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100040946.1.12",
          "form": "이게 과일과 교차반응을 하네요?",
          "original_form": "이게 과일과 교차반응을 하네요?",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100040946.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100040946.1.14",
          "form": "아? ㅠㅠㅠㅠ 으아 ㅠㅠㅠ",
          "original_form": "아? ㅠㅠㅠㅠ 으아 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100040946.1.15",
          "form": "알러지 있으면 너무 고통스럽잖아요 ㅠㅠ",
          "original_form": "알러지 있으면 너무 고통스럽잖아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100040946.1.16",
          "form": "목이 간질간질 목감기심하게 온것처럼 그래요 ㅜㅜ 멜론맛있는데 ㅎㅎㅎ",
          "original_form": "목이 간질간질 목감기심하게 온것처럼 그래요 ㅜㅜ 멜론맛있는데 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100040946.1.17",
          "form": "그러면 이번 여름에 혹시 과일 빙수 드셨나요?",
          "original_form": "그러면 이번 여름에 혹시 과일 빙수 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100040946.1.18",
          "form": "이번 설빙에서 메론 빙수가 유행이라 먹고 싶었지만",
          "original_form": "이번 설빙에서 메론 빙수가 유행이라 먹고 싶었지만",
          "speaker_id": "1",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100040946.1.19",
          "form": "저는 딸기 빙수 밖에 못 먹었거든요",
          "original_form": "저는 딸기 빙수 밖에 못 먹었거든요",
          "speaker_id": "1",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100040946.1.20",
          "form": "저 진짜 올해빙수 하나도못먹었어요....",
          "original_form": "저 진짜 올해빙수 하나도못먹었어요....",
          "speaker_id": "2",
          "time": "20210905 13:38"
        },
        {
          "id": "MDRW2100040946.1.21",
          "form": "유당불내증때문에 보통 포장해서 집에서 먹는데 제가 사먹는 빙수집까지 갈 일이 없었네요....",
          "original_form": "유당불내증때문에 보통 포장해서 집에서 먹는데 제가 사먹는 빙수집까지 갈 일이 없었네요....",
          "speaker_id": "2",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100040946.1.22",
          "form": "아 ㅠㅠㅠ",
          "original_form": "아 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100040946.1.23",
          "form": "우유빙수가 워낙 많아서 ㅠㅠㅠ",
          "original_form": "우유빙수가 워낙 많아서 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100040946.1.24",
          "form": "많이 더웠는데 어떻게 여름 보내셨어요 ㅠㅠ",
          "original_form": "많이 더웠는데 어떻게 여름 보내셨어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100040946.1.25",
          "form": "이번여름에 바 아이스크림 엄청먹었네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이번여름에 바 아이스크림 엄청먹었네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100040946.1.26",
          "form": "바 아이스크림!  어떤거 좋아하세요?",
          "original_form": "바 아이스크림!  어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100040946.1.27",
          "form": "저이번여름에 레몬맛종류는 다먹은거같아요 ㅋㅋㅋ",
          "original_form": "저이번여름에 레몬맛종류는 다먹은거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:44"
        }
      ]
