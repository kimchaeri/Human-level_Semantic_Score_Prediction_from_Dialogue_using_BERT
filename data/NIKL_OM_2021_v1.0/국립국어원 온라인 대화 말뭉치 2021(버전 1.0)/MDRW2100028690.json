      "utterance": [
        {
          "id": "MDRW2100028690.1.1",
          "form": "뭐 사고싶은 거 있으세요?",
          "original_form": "뭐 사고싶은 거 있으세요?",
          "speaker_id": "2",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028690.1.2",
          "form": "음 언더베드 리빙박스요ㅋㅋㅋ",
          "original_form": "음 언더베드 리빙박스요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028690.1.3",
          "form": "정리가 안되네요",
          "original_form": "정리가 안되네요",
          "speaker_id": "1",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028690.1.4",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100028690.1.5",
          "form": "리빙박스 사야해여 ㅠㅠ",
          "original_form": "리빙박스 사야해여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100028690.1.6",
          "form": "ㅠㅠ 리빙박스진짜 필수품이죠....하...",
          "original_form": "ㅠㅠ 리빙박스진짜 필수품이죠....하...",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100028690.1.7",
          "form": "로또돼서 집 얼른 사고싶어요 ㅋㅋㅋ",
          "original_form": "로또돼서 집 얼른 사고싶어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100028690.1.8",
          "form": "오 ㅎ 저두여",
          "original_form": "오 ㅎ 저두여",
          "speaker_id": "2",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100028690.1.9",
          "form": "로또 언제되는건지..",
          "original_form": "로또 언제되는건지..",
          "speaker_id": "2",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100028690.1.10",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100028690.1.11",
          "form": "5천원이라도 매주됐으면..ㅎ..",
          "original_form": "5천원이라도 매주됐으면..ㅎ..",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100028690.1.12",
          "form": "최근에 사신거 있으세요?",
          "original_form": "최근에 사신거 있으세요?",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100028690.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100028690.1.14",
          "form": "전 최근에 상게 없어요 ㅠㅠㅠㅠ",
          "original_form": "전 최근에 상게 없어요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100028690.1.15",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100028690.1.16",
          "form": "아하아하",
          "original_form": "아하아하",
          "speaker_id": "1",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100028690.1.17",
          "form": "저는 코로나때문에 쇼핑 중독이에요ㅠ완전 ㅋㅋ",
          "original_form": "저는 코로나때문에 쇼핑 중독이에요ㅠ완전 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100028690.1.18",
          "form": "아ㅋㅋ 전 물욕이 줄었어요 ㅎ",
          "original_form": "아ㅋㅋ 전 물욕이 줄었어요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100028690.1.19",
          "form": "옷 뭐 샀나요?,",
          "original_form": "옷 뭐 샀나요?,",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100028690.1.20",
          "form": "옷은 안샀다고 할랬는데 생각해보니 스포츠 브라랑 그런걸 샀네요 ㅋㅋㅋㅋ 그외엔 다 식단용 닭가슴살이랑 샐러드만 샀어요",
