      "utterance": [
        {
          "id": "MDRW2100014373.1.1",
          "form": "저녁드셨어요?",
          "original_form": "저녁드셨어요?",
          "speaker_id": "1",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014373.1.2",
          "form": "아직이요 그쪽은 드심?",
          "original_form": "아직이요 그쪽은 드심?",
          "speaker_id": "2",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014373.1.3",
          "form": "네 감바스요",
          "original_form": "네 감바스요",
          "speaker_id": "1",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014373.1.4",
          "form": "말키트로다가",
          "original_form": "말키트로다가",
          "speaker_id": "1",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014373.1.5",
          "form": "점심은 뭐드셧어요",
          "original_form": "점심은 뭐드셧어요",
          "speaker_id": "1",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014373.1.6",
          "form": "올 감바스~~ 저도 점심은 파스타 해먹었어유 제일 만만합니다용",
          "original_form": "올 감바스~~ 저도 점심은 파스타 해먹었어유 제일 만만합니다용",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014373.1.7",
          "form": "저녁은 뭐먹징..",
          "original_form": "저녁은 뭐먹징..",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014373.1.8",
          "form": "오 출근 안하셧구나 무슨파스타?",
          "original_form": "오 출근 안하셧구나 무슨파스타?",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014373.1.9",
          "form": "걍 오일파스타",
          "original_form": "걍 오일파스타",
          "speaker_id": "2",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014373.1.10",
          "form": "내일은 출근예정",
          "original_form": "내일은 출근예정",
          "speaker_id": "2",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014373.1.11",
          "form": "뭐넣어서요",
          "original_form": "뭐넣어서요",
          "speaker_id": "1",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014373.1.12",
          "form": "마늘 ? 새우??",
          "original_form": "마늘 ? 새우??",
          "speaker_id": "1",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014373.1.13",
          "form": "스파게티면 ?",
          "original_form": "스파게티면 ?",
          "speaker_id": "1",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014373.1.14",
          "form": "걍 면이랑 소스만!! 소스에 마늘도 다들어가있더라구욥~!",
          "original_form": "걍 면이랑 소스만!! 소스에 마늘도 다들어가있더라구욥~!",
          "speaker_id": "2",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014373.1.15",
          "form": "아소스사셧구나 추천좀요",
          "original_form": "아소스사셧구나 추천좀요",
          "speaker_id": "1",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014373.1.16",
          "form": "소스추천~!",
          "original_form": "소스추천~!",
          "speaker_id": "1",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014373.1.17",
          "form": "저게 어디거였더라 그 다니엘헤니가 선전햇던",
          "original_form": "저게 어디거였더라 그 다니엘헤니가 선전햇던",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014373.1.18",
          "form": "아 폰타나",
          "original_form": "아 폰타나",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014373.1.19",
          "form": "아하 폰타나",
          "original_form": "아하 폰타나",
          "speaker_id": "1",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014373.1.20",
          "form": "근데 막 추천할정도는 아닙니당",
          "original_form": "근데 막 추천할정도는 아닙니당",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014373.1.21",
          "form": "글쿤요 정보감사요",
          "original_form": "글쿤요 정보감사요",
          "speaker_id": "1",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100014373.1.22",
          "form": "폰타나 크림도 괜찮다던데",
          "original_form": "폰타나 크림도 괜찮다던데",
          "speaker_id": "1",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100014373.1.23",
          "form": "젤 추천하는 파스타소스 있나요?",
          "original_form": "젤 추천하는 파스타소스 있나요?",
          "speaker_id": "1",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100014373.1.24",
          "form": "폰타나크림도 먹어봤어",
          "original_form": "폰타나크림도 먹어봤어",
          "speaker_id": "2",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100014373.1.25",
          "form": "괜춘해요",
          "original_form": "괜춘해요",
          "speaker_id": "2",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100014373.1.26",
          "form": "오호 저녁 뭐드셧어요",
          "original_form": "오호 저녁 뭐드셧어요",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014373.1.27",
          "form": "된장국하고 고구마순김치 먹을예정",
          "original_form": "된장국하고 고구마순김치 먹을예정",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014373.1.28",
          "form": "나 고구마순김치짱좋아함",
          "original_form": "나 고구마순김치짱좋아함",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014373.1.29",
          "form": "알아둬",
          "original_form": "알아둬",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014373.1.30",
          "form": "앗...그게뭐람 안머거봑우요",
          "original_form": "앗...그게뭐람 안머거봑우요",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014373.1.31",
          "form": "어머니작품 ?",
          "original_form": "어머니작품 ?",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014373.1.32",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014373.1.33",
          "form": "맛나",
          "original_form": "맛나",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014373.1.34",
          "form": "사진보내주고싶네",
          "original_form": "사진보내주고싶네",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014373.1.35",
          "form": "대박....나두먹구싶다",
          "original_form": "대박....나두먹구싶다",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014373.1.36",
          "form": "고구마순 머위 이런거넘좋앙 ㅋㅋㅋㅋㅋ키키",
          "original_form": "고구마순 머위 이런거넘좋앙 ㅋㅋㅋㅋㅋ키키",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100014373.1.37",
          "form": "머위도 첨듣는당",
          "original_form": "머위도 첨듣는당",
          "speaker_id": "1",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100014373.1.38",
          "form": "엥 머위는 알걸?!?! 보면 알걸?!?!",
          "original_form": "엥 머위는 알걸?!?! 보면 알걸?!?!",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100014373.1.39",
          "form": "머위대 들깨볶음 보세요",
          "original_form": "머위대 들깨볶음 보세요",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100014373.1.40",
          "form": "그론가... 고구마순김치 파김치랑 비슥하게생겻어",
          "original_form": "그론가... 고구마순김치 파김치랑 비슥하게생겻어",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100014373.1.41",
          "form": "아저거알지!  나조아하지! ㅋㅋ",
          "original_form": "아저거알지!  나조아하지! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100014373.1.42",
          "form": "마졐ㅋㅋㅋㅋㅋㅋ근데 고구마순만의 느낌이있어 파랑은 다르지",
          "original_form": "마졐ㅋㅋㅋㅋㅋㅋ근데 고구마순만의 느낌이있어 파랑은 다르지",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100014373.1.43",
          "form": "그지!!",
          "original_form": "그지!!",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100014373.1.44",
          "form": "고구마순도 들깨에 저렇게 볶으면 맛난데...아 땡기네~",
          "original_form": "고구마순도 들깨에 저렇게 볶으면 맛난데...아 땡기네~",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100014373.1.45",
          "form": "궁금타...",
          "original_form": "궁금타...",
          "speaker_id": "1",
          "time": "20210825 22:09"
        }
      ]
    }
