      "utterance": [
        {
          "id": "MDRW2100018142.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018142.1.2",
          "form": "봉사활동을 해보신적 있으신가요",
          "original_form": "봉사활동을 해보신적 있으신가요",
          "speaker_id": "1",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018142.1.3",
          "form": "유기견보호소에서 봉사활동 해본 적 있어요",
          "original_form": "유기견보호소에서 봉사활동 해본 적 있어요",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018142.1.4",
          "form": "오 유기견보호소에서는 어떤 일을 하셨나요?",
          "original_form": "오 유기견보호소에서는 어떤 일을 하셨나요?",
          "speaker_id": "1",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100018142.1.5",
          "form": "어린 강아지들 모아놓은 방이 있는데 거기 청소하고 밥 세팅하는 일을 했어요",
          "original_form": "어린 강아지들 모아놓은 방이 있는데 거기 청소하고 밥 세팅하는 일을 했어요",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018142.1.6",
          "form": "유기견보호소를 직접 찾아가신건가요",
          "original_form": "유기견보호소를 직접 찾아가신건가요",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018142.1.7",
          "form": "네 인터넷검색해서 찾아갔어요",
          "original_form": "네 인터넷검색해서 찾아갔어요",
          "speaker_id": "2",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018142.1.8",
          "form": "유기견들이 많이 있던가요?",
          "original_form": "유기견들이 많이 있던가요?",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018142.1.9",
          "form": "최근 동네에서는 유기견들을 본 적이 없어서",
          "original_form": "최근 동네에서는 유기견들을 본 적이 없어서",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018142.1.10",
          "form": "네 안타까운 일이지만 정말 많더라구요",
          "original_form": "네 안타까운 일이지만 정말 많더라구요",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018142.1.11",
          "form": "어린 강아지부터 커다란 품종의 개까지 많더라구요",
          "original_form": "어린 강아지부터 커다란 품종의 개까지 많더라구요",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018142.1.12",
          "form": "안타까운 일이네요",
          "original_form": "안타까운 일이네요",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018142.1.13",
          "form": "그런 품종개들은",
          "original_form": "그런 품종개들은",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018142.1.14",
          "form": "다들 사람들이 버리고 가거나",
          "original_form": "다들 사람들이 버리고 가거나",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018142.1.15",
          "form": "펫샵에서 버려진 견들이라 생각하니",
          "original_form": "펫샵에서 버려진 견들이라 생각하니",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018142.1.16",
          "form": "화가나네요",
          "original_form": "화가나네요",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018142.1.17",
          "form": "그러니깐요 ㅜ",
          "original_form": "그러니깐요 ㅜ",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018142.1.18",
          "form": "산책봉사도 있어서",
          "original_form": "산책봉사도 있어서",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018142.1.19",
          "form": "웰시코기랑 같이 산책했었는데",
          "original_form": "웰시코기랑 같이 산책했었는데",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018142.1.20",
          "form": "사람을 정말 잘따라요",
          "original_form": "사람을 정말 잘따라요",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018142.1.21",
          "form": "웰시코기 정말 귀엽겠네요",
          "original_form": "웰시코기 정말 귀엽겠네요",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018142.1.22",
          "form": "봉사하시면서 유기견들을 보고",
          "original_form": "봉사하시면서 유기견들을 보고",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018142.1.23",
          "form": "네 맞아요 정말 귀여웠어요",
          "original_form": "네 맞아요 정말 귀여웠어요",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018142.1.24",
          "form": "귀여워서 데려가려는 봉사자들도 있었겠네요",
          "original_form": "귀여워서 데려가려는 봉사자들도 있었겠네요",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018142.1.25",
          "form": "제가 봉사하러 갔을땐 봉사자가 저랑 제 친구밖에 없었는데",
          "original_form": "제가 봉사하러 갔을땐 봉사자가 저랑 제 친구밖에 없었는데",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018142.1.26",
          "form": "대신 보호소 애들을 입양하려고 알아보는 사람들이 생각보다 많더라구요",
          "original_form": "대신 보호소 애들을 입양하려고 알아보는 사람들이 생각보다 많더라구요",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018142.1.27",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018142.1.28",
          "form": "주로 봉사자들로 이루어져있는 환경이라",
          "original_form": "주로 봉사자들로 이루어져있는 환경이라",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018142.1.29",
          "form": "유기견보호소 운영하기에도",
          "original_form": "유기견보호소 운영하기에도",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018142.1.30",
          "form": "솔직히 엄청 힘들것같은데",
          "original_form": "솔직히 엄청 힘들것같은데",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018142.1.31",
          "form": "센터장? 같은 개념으로 운영하는건가요",
          "original_form": "센터장? 같은 개념으로 운영하는건가요",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018142.1.32",
          "form": "글쎄요;;; 저도 그것까진 잘 모르겠어요",
          "original_form": "글쎄요;;; 저도 그것까진 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018142.1.33",
          "form": "또 다른 봉사활동을 하신적은 있으신가요",
          "original_form": "또 다른 봉사활동을 하신적은 있으신가요",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018142.1.34",
          "form": "음... 다른 봉사는 너무 오래돼서 잘 기억이 안나네요",
          "original_form": "음... 다른 봉사는 너무 오래돼서 잘 기억이 안나네요",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018142.1.35",
          "form": "저는 봉사활동을",
          "original_form": "저는 봉사활동을",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018142.1.36",
          "form": "오래전에 해봤는데",
          "original_form": "오래전에 해봤는데",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018142.1.37",
          "form": "요양원에서 해봤어요",
          "original_form": "요양원에서 해봤어요",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018142.1.38",
          "form": "오 요양원에서 해보셨군요",
          "original_form": "오 요양원에서 해보셨군요",
          "speaker_id": "2",
          "time": "20210826 23:16"
        }
