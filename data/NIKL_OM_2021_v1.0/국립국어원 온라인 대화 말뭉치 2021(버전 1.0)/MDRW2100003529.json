      "utterance": [
        {
          "id": "MDRW2100003529.1.1",
          "form": "읭? 무슨 택배?",
          "original_form": "읭? 무슨 택배?",
          "speaker_id": "2",
          "time": "20210821 00:30"
        },
        {
          "id": "MDRW2100003529.1.2",
          "form": "집 앞에 택배 와있는데?",
          "original_form": "집 앞에 택배 와있는데?",
          "speaker_id": "1",
          "time": "20210821 00:30"
        },
        {
          "id": "MDRW2100003529.1.3",
          "form": "뭐 시켰어?",
          "original_form": "뭐 시켰어?",
          "speaker_id": "1",
          "time": "20210821 00:30"
        },
        {
          "id": "MDRW2100003529.1.4",
          "form": "읭? 시킨 거 없는데..ㅎ",
          "original_form": "읭? 시킨 거 없는데..ㅎ",
          "speaker_id": "2",
          "time": "20210821 00:31"
        },
        {
          "id": "MDRW2100003529.1.5",
          "form": "누구한테서 온 거에요?",
          "original_form": "누구한테서 온 거에요?",
          "speaker_id": "2",
          "time": "20210821 00:31"
        },
        {
          "id": "MDRW2100003529.1.6",
          "form": "name3라고 적혀있어",
          "original_form": "&name3&라고 적혀있어",
          "speaker_id": "1",
          "time": "20210821 00:31"
        },
        {
          "id": "MDRW2100003529.1.7",
          "form": "아!!! 그거",
          "original_form": "아!!! 그거",
          "speaker_id": "2",
          "time": "20210821 00:31"
        },
        {
          "id": "MDRW2100003529.1.8",
          "form": "보험 설계 담당자 분이 선물 보내주신다고 햇었는데",
          "original_form": "보험 설계 담당자 분이 선물 보내주신다고 햇었는데",
          "speaker_id": "2",
          "time": "20210821 00:31"
        },
        {
          "id": "MDRW2100003529.1.9",
          "form": "그건가보네요~~",
          "original_form": "그건가보네요~~",
          "speaker_id": "2",
          "time": "20210821 00:31"
        },
        {
          "id": "MDRW2100003529.1.10",
          "form": "아~ 그래?",
          "original_form": "아~ 그래?",
          "speaker_id": "1",
          "time": "20210821 00:31"
        },
        {
          "id": "MDRW2100003529.1.11",
          "form": "이게 뭔데?",
          "original_form": "이게 뭔데?",
          "speaker_id": "1",
          "time": "20210821 00:31"
        },
        {
          "id": "MDRW2100003529.1.12",
          "form": "저도 뭔지는 잘 몰라요~",
          "original_form": "저도 뭔지는 잘 몰라요~",
          "speaker_id": "2",
          "time": "20210821 00:32"
        },
        {
          "id": "MDRW2100003529.1.13",
          "form": "뜯어봐야 알 수 있을듯ㅎㅎ",
          "original_form": "뜯어봐야 알 수 있을듯ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 00:32"
        },
        {
          "id": "MDRW2100003529.1.14",
          "form": "뜯어볼까?",
          "original_form": "뜯어볼까?",
          "speaker_id": "1",
          "time": "20210821 00:32"
        },
        {
          "id": "MDRW2100003529.1.15",
          "form": "그래요~",
          "original_form": "그래요~",
          "speaker_id": "2",
          "time": "20210821 00:32"
        },
        {
          "id": "MDRW2100003529.1.16",
          "form": "물 담아먹는 건데",
          "original_form": "물 담아먹는 건데",
          "speaker_id": "1",
          "time": "20210821 00:32"
        },
        {
          "id": "MDRW2100003529.1.17",
          "form": "아~~ 텀블러인가 보네요~",
          "original_form": "아~~ 텀블러인가 보네요~",
          "speaker_id": "2",
          "time": "20210821 00:33"
        },
        {
          "id": "MDRW2100003529.1.18",
          "form": "어 그런거 같아",
          "original_form": "어 그런거 같아",
          "speaker_id": "1",
          "time": "20210821 00:33"
        },
        {
          "id": "MDRW2100003529.1.19",
          "form": "커요 작아요?",
          "original_form": "커요 작아요?",
          "speaker_id": "2",
          "time": "20210821 00:33"
        },
        {
          "id": "MDRW2100003529.1.20",
