      "utterance": [
        {
          "id": "MDRW2100033354.1.1",
          "form": "앞으로는 코로나랑 같이 이어간다해서",
          "original_form": "앞으로는 코로나랑 같이 이어간다해서",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100033354.1.2",
          "form": "위드코로나라고 뷰르나봐요",
          "original_form": "위드코로나라고 뷰르나봐요",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100033354.1.3",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100033354.1.4",
          "form": "name1님은 혹시 백신 맞으셨나요?",
          "original_form": "&name1&님은 혹시 백신 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100033354.1.5",
          "form": "네! 저 1차 맞았습니당",
          "original_form": "네! 저 1차 맞았습니당",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100033354.1.6",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100033354.1.7",
          "form": "저도 1차 8월말에 맞았어요 ㅎㅎ",
          "original_form": "저도 1차 8월말에 맞았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100033354.1.8",
          "form": "화이자 맞았었는데 첫날 팔통증 빼면 열 살짝 올랐다 내리고 끝이더라구요 ㅎㅎ",
          "original_form": "화이자 맞았었는데 첫날 팔통증 빼면 열 살짝 올랐다 내리고 끝이더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100033354.1.9",
          "form": "둘째날부터는 팔통증도 엄청 완화돼서 별거 아니네~~ 싶었어요 ㅋㅋㅋ",
          "original_form": "둘째날부터는 팔통증도 엄청 완화돼서 별거 아니네~~ 싶었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100033354.1.10",
          "form": "저도 팔 좀 뻐근한거 빼고는 아무렇지도 않았어요 ㅋㅋㅋ",
          "original_form": "저도 팔 좀 뻐근한거 빼고는 아무렇지도 않았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033354.1.11",
          "form": "2차가 아프다고 그래서 지금 겁 먹은 상태...ㅎㅎ허허",
          "original_form": "2차가 아프다고 그래서 지금 겁 먹은 상태...ㅎㅎ허허",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033354.1.12",
          "form": "name2닌 2차 언제세요?",
          "original_form": "&name2&닌 2차 언제세요?",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033354.1.13",
          "form": "맞아요 2차가 많이 아프다고 하더라구요 ㅎ..",
          "original_form": "맞아요 2차가 많이 아프다고 하더라구요 ㅎ..",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100033354.1.14",
          "form": "저는 2차 10월 7일이에요",
          "original_form": "저는 2차 10월 7일이에요",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100033354.1.15",
          "form": "저 5일이에요!",
          "original_form": "저 5일이에요!",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033354.1.16",
          "form": "제가 먼저 고통을 맛보겠내요 ㅎㅎ",
          "original_form": "제가 먼저 고통을 맛보겠내요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033354.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100033354.1.18",
          "form": "근데 마지노선이 6주라면서요? 몰랐는데",
          "original_form": "근데 마지노선이 6주라면서요? 몰랐는데",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100033354.1.19",
          "form": "그래서 병원에 전화해서 혹시 땡길수 있는지 물어볼까 생각중이에요",
          "original_form": "그래서 병원에 전화해서 혹시 땡길수 있는지 물어볼까 생각중이에요",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100033354.1.20",
          "form": "병원바이병원인데 해주는 곳도 있다고 하더라구요",
          "original_form": "병원바이병원인데 해주는 곳도 있다고 하더라구요",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100033354.1.21",
          "form": "그래요? 저는 그냥 그날이라고 문자오길래 그렇구나 하고 있었어요ㅋㅋㅋㅋ",
          "original_form": "그래요? 저는 그냥 그날이라고 문자오길래 그렇구나 하고 있었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:18"
        },
        {
          "id": "MDRW2100033354.1.22",
          "form": "2차는 진짜 좀 무서워요",
          "original_form": "2차는 진짜 좀 무서워요",
          "speaker_id": "1",
          "time": "20210906 23:19"
        },
        {
          "id": "MDRW2100033354.1.23",
          "form": "아프면 어떡해요",
          "original_form": "아프면 어떡해요",
          "speaker_id": "1",
          "time": "20210906 23:19"
        },
        {
          "id": "MDRW2100033354.1.24",
          "form": "저는 좀.. 무던한 편이라",
          "original_form": "저는 좀.. 무던한 편이라",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100033354.1.25",
          "form": "아프겠지..? 그래도 뭐 맞아야지.. 싶어요",
          "original_form": "아프겠지..? 그래도 뭐 맞아야지.. 싶어요",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100033354.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100033354.1.27",
          "form": "아직 안 맞아서 안 와닿는 걸수도 있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아직 안 맞아서 안 와닿는 걸수도 있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100033354.1.28",
          "form": "백신얘긴 그만해야겠어요",
          "original_form": "백신얘긴 그만해야겠어요",
          "speaker_id": "1",
          "time": "20210906 23:28"
        },
        {
          "id": "MDRW2100033354.1.29",
          "form": "위드코로나니까 앞으로 독감처럼 매년 백신접종을 하게될까요?",
          "original_form": "위드코로나니까 앞으로 독감처럼 매년 백신접종을 하게될까요?",
          "speaker_id": "1",
          "time": "20210906 23:42"
        },
        {
          "id": "MDRW2100033354.1.30",
          "form": "위드코로나니까 앞으로 매년 독감초롬 코로나 백신을 접종하는걸까요?",
          "original_form": "위드코로나니까 앞으로 매년 독감초롬 코로나 백신을 접종하는걸까요?",
