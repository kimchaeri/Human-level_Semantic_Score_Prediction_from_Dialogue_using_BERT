      "utterance": [
        {
          "id": "MDRW2100034327.1.1",
          "form": "알바 뭐해보셨어요??",
          "original_form": "알바 뭐해보셨어요??",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034327.1.2",
          "form": "저는 애슐리랑 편의점 해봤어요",
          "original_form": "저는 애슐리랑 편의점 해봤어요",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100034327.1.3",
          "form": "와 애슐리 힘들지않나요?",
          "original_form": "와 애슐리 힘들지않나요?",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100034327.1.4",
          "form": "애슐리 친구랑 같이 했었는데 처음에 텃세가 심해가지구",
          "original_form": "애슐리 친구랑 같이 했었는데 처음에 텃세가 심해가지구",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034327.1.5",
          "form": "아 ㅜㅜ",
          "original_form": "아 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034327.1.6",
          "form": "그것도 힘들었구요\n일도 힘들죠ㅠ",
          "original_form": "그것도 힘들었구요\n일도 힘들죠ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034327.1.7",
          "form": "알바텃세 진짜 같잖아요 ㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "알바텃세 진짜 같잖아요 ㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034327.1.8",
          "form": "주방에서 했는데",
          "original_form": "주방에서 했는데",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034327.1.9",
          "form": "뷔페가 힘들다더라구요",
          "original_form": "뷔페가 힘들다더라구요",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034327.1.10",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034327.1.11",
          "form": "요리하셨어요??",
          "original_form": "요리하셨어요??",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034327.1.12",
          "form": "손목 엄청 아프더라구요ㅠ",
          "original_form": "손목 엄청 아프더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034327.1.13",
          "form": "온몸이 다아파요 진짜ㅠ",
          "original_form": "온몸이 다아파요 진짜ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034327.1.14",
          "form": "아 ㅜㅜ",
          "original_form": "아 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100034327.1.15",
          "form": "손목아픈게 진짜 힘들어요",
          "original_form": "손목아픈게 진짜 힘들어요",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100034327.1.16",
          "form": "그래도 나중에 다 친해지니까 괜찮더라구요",
          "original_form": "그래도 나중에 다 친해지니까 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100034327.1.17",
          "form": "그니까요ㅠ",
          "original_form": "그니까요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100034327.1.18",
          "form": "오 그래도 텃새부리다가 친해지긴 햇네요 다행히",
