      "utterance": [
        {
          "id": "MDRW2100018355.1.1",
          "form": "안녕하세요 가고싶은 여행지 있나요?",
          "original_form": "안녕하세요 가고싶은 여행지 있나요?",
          "speaker_id": "1",
          "time": "20210827 00:16"
        },
        {
          "id": "MDRW2100018355.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100018355.1.3",
          "form": "저는 울릉도 가고싶어요",
          "original_form": "저는 울릉도 가고싶어요",
          "speaker_id": "2",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100018355.1.4",
          "form": "헐 너무 좋다더라고요 울릉도 저는 경주가고 싶네요!ㅎㅎ",
          "original_form": "헐 너무 좋다더라고요 울릉도 저는 경주가고 싶네요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 01:30"
        },
        {
          "id": "MDRW2100018355.1.5",
          "form": "경주 진짜 좋아요",
          "original_form": "경주 진짜 좋아요",
          "speaker_id": "2",
          "time": "20210827 08:17"
        },
        {
          "id": "MDRW2100018355.1.6",
          "form": "저도 옛날에 좋아서 두 번이나 갔어요",
          "original_form": "저도 옛날에 좋아서 두 번이나 갔어요",
          "speaker_id": "2",
          "time": "20210827 08:17"
        },
        {
          "id": "MDRW2100018355.1.7",
          "form": "하 그쵸... 저 아무것도 모르는 시절에 가서 그냥 대충보고 온게 성인 되고 보니 너무 아쉬운거예요ㅠ",
          "original_form": "하 그쵸... 저 아무것도 모르는 시절에 가서 그냥 대충보고 온게 성인 되고 보니 너무 아쉬운거예요ㅠ",
          "speaker_id": "1",
          "time": "20210827 11:20"
        },
        {
          "id": "MDRW2100018355.1.8",
          "form": "그래서 경주 가고 싶어요 하....",
          "original_form": "그래서 경주 가고 싶어요 하....",
          "speaker_id": "1",
          "time": "20210827 11:20"
        },
        {
          "id": "MDRW2100018355.1.9",
          "form": "갔다오셨던 여행지중에 여긴 다신 안간다 하는 곳 있나요?",
          "original_form": "갔다오셨던 여행지중에 여긴 다신 안간다 하는 곳 있나요?",
          "speaker_id": "1",
          "time": "20210827 11:20"
        },
        {
          "id": "MDRW2100018355.1.10",
          "form": "한번 다녀오세요",
          "original_form": "한번 다녀오세요",
          "speaker_id": "2",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018355.1.11",
          "form": "다신 안가고싶었던 곳은 전주였는데",
          "original_form": "다신 안가고싶었던 곳은 전주였는데",
          "speaker_id": "2",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018355.1.12",
          "form": "어떻게 하다보니 다시 가게 됐거든요? 근데 너무 좋았어요",
          "original_form": "어떻게 하다보니 다시 가게 됐거든요? 근데 너무 좋았어요",
          "speaker_id": "2",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018355.1.13",
          "form": "처음 갈 땐 너무 더운 날 가서 잘 몰랐었나봐요",
          "original_form": "처음 갈 땐 너무 더운 날 가서 잘 몰랐었나봐요",
          "speaker_id": "2",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018355.1.14",
          "form": "하 헐 그래요?",
          "original_form": "하 헐 그래요?",
          "speaker_id": "1",
          "time": "20210827 12:54"
        },
        {
          "id": "MDRW2100018355.1.15",
          "form": "저도 전주 갔었는데 야시장이 너무 좋은거예요...",
          "original_form": "저도 전주 갔었는데 야시장이 너무 좋은거예요...",
          "speaker_id": "1",
          "time": "20210827 12:54"
        },
        {
          "id": "MDRW2100018355.1.16",
          "form": "더운 날만 아니면 진짜 여행이 즐거워지는 거 같아요",
          "original_form": "더운 날만 아니면 진짜 여행이 즐거워지는 거 같아요",
          "speaker_id": "1",
          "time": "20210827 12:54"
        },
        {
          "id": "MDRW2100018355.1.17",
          "form": "ㅋㅋㅋㅋㅋㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:54"
        },
        {
          "id": "MDRW2100018355.1.18",
          "form": "와 야시장이 있었는지는 몰랐어요",
          "original_form": "와 야시장이 있었는지는 몰랐어요",
          "speaker_id": "2",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018355.1.19",
          "form": "또 가야할 이유가 생겼네요 ㅋㅋㅋ",
          "original_form": "또 가야할 이유가 생겼네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018355.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ거기 야시장 엄청 유명하더라고요 전주!!!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ거기 야시장 엄청 유명하더라고요 전주!!!!",
          "speaker_id": "1",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100018355.1.21",
          "form": "저는 코로나한참 전에 갔는데 돌아다니며 먹을 수 있다는 거에 행복했답니다",
          "original_form": "저는 코로나한참 전에 갔는데 돌아다니며 먹을 수 있다는 거에 행복했답니다",
          "speaker_id": "1",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100018355.1.22",
          "form": "와... 대박 ㅠㅠ 코로나 전에 ... 전 기억도 안나네여",
          "original_form": "와... 대박 ㅠㅠ 코로나 전에 ... 전 기억도 안나네여",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100018355.1.23",
          "form": "아 저 코로나 직전에 유럽갔었어오",
          "original_form": "아 저 코로나 직전에 유럽갔었어오",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100018355.1.24",
          "form": "우와 좋겠다ㅠㅠㅠㅠㅠㅠ 유럽 어디다녀오셨나요",
          "original_form": "우와 좋겠다ㅠㅠㅠㅠㅠㅠ 유럽 어디다녀오셨나요",
          "speaker_id": "1",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100018355.1.25",
          "form": "전 부다페스트랑 프라하여",
          "original_form": "전 부다페스트랑 프라하여",
          "speaker_id": "2",
          "time": "20210827 19:15"
        },
        {
          "id": "MDRW2100018355.1.26",
          "form": "그때 진짜 고생했는데...",
          "original_form": "그때 진짜 고생했는데...",
          "speaker_id": "2",
          "time": "20210827 19:15"
        },
        {
          "id": "MDRW2100018355.1.27",
          "form": "간거 후회했는데...",
          "original_form": "간거 후회했는데...",
          "speaker_id": "2",
          "time": "20210827 19:16"
        },
        {
          "id": "MDRW2100018355.1.28",
          "form": "코로나 오니까.. 갔다온 게 다행인 거였어요",
          "original_form": "코로나 오니까.. 갔다온 게 다행인 거였어요",
          "speaker_id": "2",
          "time": "20210827 19:16"
        },
        {
          "id": "MDRW2100018355.1.29",
          "form": "헐 헐 헐 헐",
          "original_form": "헐 헐 헐 헐",
          "speaker_id": "1",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100018355.1.30",
          "form": "너무 좋았을 거 같은데 후회하셨나요? ㅠㅠ",
          "original_form": "너무 좋았을 거 같은데 후회하셨나요? ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100018355.1.31",
          "form": "그때 좀 사건이 있어서 힘들었거든오...",
          "original_form": "그때 좀 사건이 있어서 힘들었거든오...",
          "speaker_id": "2",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100018355.1.32",
          "form": "그래서 좀 후회했어요",
          "original_form": "그래서 좀 후회했어요",
          "speaker_id": "2",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100018355.1.33",
          "form": "근데 지금와서보면 너무 잘한일이져",
          "original_form": "근데 지금와서보면 너무 잘한일이져",
          "speaker_id": "2",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100018355.1.34",
          "form": "아이고 ㅠㅠㅠㅠ",
          "original_form": "아이고 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100018355.1.35",
          "form": "그렇죠! 지금 생각하면 한 곳이라도 더 갔다온 게 잘한 일이죠 ㅋㅋㅋㅎㅎ",
          "original_form": "그렇죠! 지금 생각하면 한 곳이라도 더 갔다온 게 잘한 일이죠 ㅋㅋㅋㅎㅎ",
