      "utterance": [
        {
          "id": "MDRW2100044551.1.1",
          "form": "기태영랑 주현도 나왔어",
          "original_form": "기태영랑 주현도 나왔어",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044551.1.2",
          "form": "주말드라마였죠?",
          "original_form": "주말드라마였죠?",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044551.1.3",
          "form": "엉 주말드라마",
          "original_form": "엉 주말드라마",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044551.1.4",
          "form": "기억이 나는듯 해요",
          "original_form": "기억이 나는듯 해요",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044551.1.5",
          "form": "하나뿐인 내편 후속작인데 막장드라마였나봐",
          "original_form": "하나뿐인 내편 후속작인데 막장드라마였나봐",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044551.1.6",
          "form": "그랬던거 같아요ㅋ 김소연 악역이었던거 같은데",
          "original_form": "그랬던거 같아요ㅋ 김소연 악역이었던거 같은데",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044551.1.7",
          "form": "막장이라서 시청률이 높았는듯 ㅋㅋ",
          "original_form": "막장이라서 시청률이 높았는듯 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044551.1.8",
          "form": "막장은 이상하게 시청률 좋아요",
          "original_form": "막장은 이상하게 시청률 좋아요",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044551.1.9",
          "form": "작년에도 최명길 일일드라마했네",
          "original_form": "작년에도 최명길 일일드라마했네",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044551.1.10",
          "form": "진짜 열일 하네요",
          "original_form": "진짜 열일 하네요",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044551.1.11",
          "form": "욕하면서 보는 재미가 있나봐 ㅋㅋ",
          "original_form": "욕하면서 보는 재미가 있나봐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044551.1.12",
          "form": "펜트하우스처럼ㅋ",
          "original_form": "펜트하우스처럼ㅋ",
