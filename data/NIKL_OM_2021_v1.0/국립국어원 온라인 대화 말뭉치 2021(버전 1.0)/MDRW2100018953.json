      "utterance": [
        {
          "id": "MDRW2100018953.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018953.1.2",
          "form": "네 안녕하세요  요즘 즐겨보시는 것 있나요?",
          "original_form": "네 안녕하세요  요즘 즐겨보시는 것 있나요?",
          "speaker_id": "2",
          "time": "20210827 13:56"
        },
        {
          "id": "MDRW2100018953.1.3",
          "form": "저는 런닝맨이랑 유튜브 매탈남 채널 즐겨봐요!",
          "original_form": "저는 런닝맨이랑 유튜브 매탈남 채널 즐겨봐요!",
          "speaker_id": "1",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100018953.1.4",
          "form": "좋아하는 프로 있나요?",
          "original_form": "좋아하는 프로 있나요?",
          "speaker_id": "1",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100018953.1.5",
          "form": "딱히 없구요 그냥 해외 영화나 드라마 좋아해요",
          "original_form": "딱히 없구요 그냥 해외 영화나 드라마 좋아해요",
          "speaker_id": "2",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100018953.1.6",
          "form": "영화나 드라마 즐겨보세요?",
          "original_form": "영화나 드라마 즐겨보세요?",
          "speaker_id": "2",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100018953.1.7",
          "form": "영화 드라마 좋아해요!",
          "original_form": "영화 드라마 좋아해요!",
          "speaker_id": "1",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100018953.1.8",
          "form": "미드는 한때 ncis라는 드라마에 진짜 빠져 살았었어요 ㅎㅎ",
          "original_form": "미드는 한때 ncis라는 드라마에 진짜 빠져 살았었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100018953.1.9",
          "form": "추천해주실만한 작품이 있나요?",
          "original_form": "추천해주실만한 작품이 있나요?",
          "speaker_id": "1",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100018953.1.10",
          "form": "좋아하는 장르가 어떻게 되시나요? 전 판타지나 sf쪽 좋아해서요.",
          "original_form": "좋아하는 장르가 어떻게 되시나요? 전 판타지나 sf쪽 좋아해서요.",
          "speaker_id": "2",
          "time": "20210827 17:42"
        },
        {
          "id": "MDRW2100018953.1.11",
          "form": "고어물 말고는 가리는 것 없이 잘봐요!\r\n편하게 추천해주세요 ㅎㅎ",
          "original_form": "고어물 말고는 가리는 것 없이 잘봐요!\r\n편하게 추천해주세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:49"
        },
        {
          "id": "MDRW2100018953.1.12",
          "form": "미드 체르노빌이요 혹시 보셨나요?",
          "original_form": "미드 체르노빌이요 혹시 보셨나요?",
          "speaker_id": "2",
          "time": "20210827 17:54"
        },
        {
          "id": "MDRW2100018953.1.13",
          "form": "들어본 것 같아요!\r\n넷플릭스에서 꼭 봐야할 리스트 그런거에서 꼭 있었던 거 같은데 맞나요??",
          "original_form": "들어본 것 같아요!\r\n넷플릭스에서 꼭 봐야할 리스트 그런거에서 꼭 있었던 거 같은데 맞나요??",
          "speaker_id": "1",
          "time": "20210827 18:08"
        },
        {
          "id": "MDRW2100018953.1.14",
          "form": "네 워낙에 수작이라서 아마 있을 거예요ㅎ",
          "original_form": "네 워낙에 수작이라서 아마 있을 거예요ㅎ",
          "speaker_id": "2",
          "time": "20210827 18:58"
        },
        {
          "id": "MDRW2100018953.1.15",
          "form": "이번 추석연휴 때 꼭 볼게요 ㅎㅎ\r\n기대돼요!",
          "original_form": "이번 추석연휴 때 꼭 볼게요 ㅎㅎ\r\n기대돼요!",
          "speaker_id": "1",
          "time": "20210827 19:01"
        },
        {
          "id": "MDRW2100018953.1.16",
          "form": "네~ 아마 느끼시는 게 많을 거예요 저도 그랬거든요",
          "original_form": "네~ 아마 느끼시는 게 많을 거예요 저도 그랬거든요",
          "speaker_id": "2",
          "time": "20210827 19:03"
        },
        {
          "id": "MDRW2100018953.1.17",
          "form": "전 추석연휴에 좋아하는 배우 작품 보려고요",
          "original_form": "전 추석연휴에 좋아하는 배우 작품 보려고요",
          "speaker_id": "2",
          "time": "20210827 19:04"
        },
        {
          "id": "MDRW2100018953.1.18",
          "form": "아 당장 오늘부터 볼까봐요 ㅎㅎ",
          "original_form": "아 당장 오늘부터 볼까봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:04"
        },
        {
          "id": "MDRW2100018953.1.19",
          "form": "궁금해서 명절까지 못 기다리겠네요 ㅎㅎ",
