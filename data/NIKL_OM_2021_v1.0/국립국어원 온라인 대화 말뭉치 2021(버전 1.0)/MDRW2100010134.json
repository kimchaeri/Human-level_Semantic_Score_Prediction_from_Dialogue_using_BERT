      "utterance": [
        {
          "id": "MDRW2100010134.1.1",
          "form": "어디를 제일 좋아하나요",
          "original_form": "어디를 제일 좋아하나요",
          "speaker_id": "2",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100010134.1.2",
          "form": "일본여행을 좋아해서 일년에 네번씩은 갔었답니다",
          "original_form": "일본여행을 좋아해서 일년에 네번씩은 갔었답니다",
          "speaker_id": "1",
          "time": "20210823 14:42"
        },
        {
          "id": "MDRW2100010134.1.3",
          "form": "name2님은 어디를 가보셨나요?",
          "original_form": "&name2&님은 어디를 가보셨나요?",
          "speaker_id": "1",
          "time": "20210823 14:42"
        },
        {
          "id": "MDRW2100010134.1.4",
          "form": "저도 일본 좋아합니다",
          "original_form": "저도 일본 좋아합니다",
          "speaker_id": "2",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100010134.1.5",
          "form": "대만, 일본, 유럽에 가봤습니다",
          "original_form": "대만, 일본, 유럽에 가봤습니다",
          "speaker_id": "2",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100010134.1.6",
          "form": "유럽에서 가장 기억에 남는 곳은 어디야?",
          "original_form": "유럽에서 가장 기억에 남는 곳은 어디야?",
          "speaker_id": "1",
          "time": "20210823 14:44"
        },
        {
          "id": "MDRW2100010134.1.7",
          "form": "피렌체다",
          "original_form": "피렌체다",
          "speaker_id": "2",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010134.1.8",
          "form": "음식이 맛있다",
          "original_form": "음식이 맛있다",
          "speaker_id": "2",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010134.1.9",
          "form": "나는 니가 사다준 홍차 기념품을 아주 소중히 하고 있어.",
          "original_form": "나는 니가 사다준 홍차 기념품을 아주 소중히 하고 있어.",
          "speaker_id": "1",
          "time": "20210823 15:55"
        },
        {
          "id": "MDRW2100010134.1.10",
          "form": "여행이 가능해지면 어디를 먼저 가고싶니?",
          "original_form": "여행이 가능해지면 어디를 먼저 가고싶니?",
          "speaker_id": "1",
          "time": "20210823 15:55"
        },
        {
          "id": "MDRW2100010134.1.11",
          "form": "홍차기념품이 모.죠",
          "original_form": "홍차기념품이 모.죠",
          "speaker_id": "2",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100010134.1.12",
          "form": "홍차인가요",
          "original_form": "홍차인가요",
          "speaker_id": "2",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100010134.1.13",
          "form": "곰팡이 낫겠다",
          "original_form": "곰팡이 낫겠다",
          "speaker_id": "2",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100010134.1.14",
          "form": "그냥 가까운 일본에 가고싶다",
          "original_form": "그냥 가까운 일본에 가고싶다",
          "speaker_id": "2",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100010134.1.15",
          "form": "너는 어떠니",
          "original_form": "너는 어떠니",
          "speaker_id": "2",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100010134.1.16",
          "form": "나도 일본가고싶어",
          "original_form": "나도 일본가고싶어",
          "speaker_id": "1",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100010134.1.17",
          "form": "교토를 너무너무 가고싶어",
          "original_form": "교토를 너무너무 가고싶어",
          "speaker_id": "1",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100010134.1.18",
          "form": "같이 갈래?",
          "original_form": "같이 갈래?",
          "speaker_id": "1",
          "time": "20210823 15:57"
        },
        {
          "id": "MDRW2100010134.1.19",
          "form": "언젠가..언젠가...",
          "original_form": "언젠가..언젠가...",
          "speaker_id": "1",
          "time": "20210823 15:57"
        },
        {
          "id": "MDRW2100010134.1.20",
          "form": "절연하지 않을 수 있을까?",
          "original_form": "절연하지 않을 수 있을까?",
          "speaker_id": "2",
          "time": "20210823 15:57"
        },
        {
          "id": "MDRW2100010134.1.21",
          "form": "재미는 있겠다",
          "original_form": "재미는 있겠다",
          "speaker_id": "2",
          "time": "20210823 15:57"
        },
        {
          "id": "MDRW2100010134.1.22",
          "form": "내가 잘할게",
          "original_form": "내가 잘할게",
          "speaker_id": "1",
          "time": "20210823 15:57"
        },
        {
          "id": "MDRW2100010134.1.23",
          "form": "교토 너무 좋다 아라시야마 다시 가고 싶다",
          "original_form": "교토 너무 좋다 아라시야마 다시 가고 싶다",
          "speaker_id": "1",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100010134.1.24",
          "form": "란덴 타고싶다",
          "original_form": "란덴 타고싶다",
          "speaker_id": "1",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100010134.1.25",
          "form": "난 이치란 먹고싶다",
          "original_form": "난 이치란 먹고싶다",
          "speaker_id": "2",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100010134.1.26",
          "form": "이치란 맛있었어 생각보다",
          "original_form": "이치란 맛있었어 생각보다",
          "speaker_id": "1",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100010134.1.27",
          "form": "난 매일 먹을 수 있어",
          "original_form": "난 매일 먹을 수 있어",
          "speaker_id": "2",
          "time": "20210823 15:58"
        }
      ]
    }
  ]
