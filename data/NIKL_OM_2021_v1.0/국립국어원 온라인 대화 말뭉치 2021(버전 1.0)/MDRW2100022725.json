      "utterance": [
        {
          "id": "MDRW2100022725.1.1",
          "form": "저녁식사할때라니..시간이 빨라요",
          "original_form": "저녁식사할때라니..시간이 빨라요",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022725.1.2",
          "form": "오늘 정하셧나요?",
          "original_form": "오늘 정하셧나요?",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022725.1.3",
          "form": "아직이요ㅜ 정하셨어요?",
          "original_form": "아직이요ㅜ 정하셨어요?",
          "speaker_id": "1",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022725.1.4",
          "form": "저는 샐러드로.. ㅋㅋㅋㅋ",
          "original_form": "저는 샐러드로.. ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022725.1.5",
          "form": "앗.. 샐러드 드시는군욥",
          "original_form": "앗.. 샐러드 드시는군욥",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022725.1.6",
          "form": "어떤 샐러드 드셔요?",
          "original_form": "어떤 샐러드 드셔요?",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022725.1.7",
          "form": "양배추랑 당근 브로콜리 파프리카랑 드레싱이요..",
          "original_form": "양배추랑 당근 브로콜리 파프리카랑 드레싱이요..",
          "speaker_id": "2",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100022725.1.8",
          "form": "저녁은 귀찮아서.. 보통 샐러드랑",
          "original_form": "저녁은 귀찮아서.. 보통 샐러드랑",
          "speaker_id": "2",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100022725.1.9",
          "form": "빵이나 과자 먹어요.. ㅋㅋㅋ",
          "original_form": "빵이나 과자 먹어요.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100022725.1.10",
          "form": "앗 그렇구나ㅋㅋㅋ",
          "original_form": "앗 그렇구나ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100022725.1.11",
          "form": "확실히 챙겨먹기 귀찮아요ㅠ",
          "original_form": "확실히 챙겨먹기 귀찮아요ㅠ",
          "speaker_id": "1",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100022725.1.12",
          "form": "글쵸.. 혼자라 이렇게 먹는데.. \r\n애기들 있음 매번 힘들거같아요",
          "original_form": "글쵸.. 혼자라 이렇게 먹는데.. \r\n애기들 있음 매번 힘들거같아요",
          "speaker_id": "2",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100022725.1.13",
          "form": "아 그러게요ㅠ",
          "original_form": "아 그러게요ㅠ",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022725.1.14",
          "form": "저는 가족끼리 맞추는게 힘드네요ㅋㅋ",
          "original_form": "저는 가족끼리 맞추는게 힘드네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022725.1.15",
          "form": "가족끼리는 결국 식성 다 비슷해지지 않나여?",
          "original_form": "가족끼리는 결국 식성 다 비슷해지지 않나여?",
          "speaker_id": "2",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100022725.1.16",
          "form": "움.. 저흰 다 달라서..ㅋㅋㅋㅋ",
          "original_form": "움.. 저흰 다 달라서..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100022725.1.17",
