      "utterance": [
        {
          "id": "MDRW2100007526.1.1",
          "form": "최근에 올림픽 보셨나요",
          "original_form": "최근에 올림픽 보셨나요",
          "speaker_id": "1",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007526.1.2",
          "form": "헉 좋아하는 스포츠",
          "original_form": "헉 좋아하는 스포츠",
          "speaker_id": "2",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007526.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007526.1.4",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007526.1.5",
          "form": "어떤거 인상적이었어요?",
          "original_form": "어떤거 인상적이었어요?",
          "speaker_id": "1",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007526.1.6",
          "form": "안.산.",
          "original_form": "안.산.",
          "speaker_id": "2",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007526.1.7",
          "form": "사.랑.해",
          "original_form": "사.랑.해",
          "speaker_id": "2",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007526.1.8",
          "form": "근데 그 높이뛰기기",
          "original_form": "근데 그 높이뛰기기",
          "speaker_id": "2",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007526.1.9",
          "form": "안.산.국룰이죠",
          "original_form": "안.산.국룰이죠",
          "speaker_id": "1",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007526.1.10",
          "form": "높이뛰기가 제일 인상적이긴 했어요",
          "original_form": "높이뛰기가 제일 인상적이긴 했어요",
          "speaker_id": "2",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007526.1.11",
          "form": "우상혁 선수요?",
          "original_form": "우상혁 선수요?",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007526.1.12",
          "form": "사랑과는 별개로(?)",
          "original_form": "사랑과는 별개로(?)",
          "speaker_id": "2",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007526.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007526.1.14",
          "form": "그분 좀",
          "original_form": "그분 좀",
          "speaker_id": "2",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007526.1.15",
          "form": "80년대 느낌이더라고요",
          "original_form": "80년대 느낌이더라고요",
          "speaker_id": "2",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007526.1.16",
          "form": "아무래도 거기서 에너지를 얻으신 분들이 많더라구요",
          "original_form": "아무래도 거기서 에너지를 얻으신 분들이 많더라구요",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007526.1.17",
          "form": "뭔가 건강한 에너지",
          "original_form": "뭔가 건강한 에너지",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007526.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007526.1.19",
          "form": "좋았어요",
          "original_form": "좋았어요",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007526.1.20",
          "form": "어케 그렇게 건치이신지",
          "original_form": "어케 그렇게 건치이신지",
          "speaker_id": "2",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007526.1.21",
          "form": "그리고ㅋㅋ 그분도 그분인데",
          "original_form": "그리고ㅋㅋ 그분도 그분인데",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007526.1.22",
          "form": "거기 나온 선수들이ㅋㅋㅋ",
          "original_form": "거기 나온 선수들이ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007526.1.23",
          "form": "다 너무 개성이 강해서ㅋㅋ",
          "original_form": "다 너무 개성이 강해서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007526.1.24",
          "form": "혹시 보셨나요",
          "original_form": "혹시 보셨나요",
          "speaker_id": "1",
          "time": "20210822 02:55"
        },
        {
          "id": "MDRW2100007526.1.25",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:56"
        },
        {
          "id": "MDRW2100007526.1.26",
          "form": "그 인싸들이요?!ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그 인싸들이요?!ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:56"
        },
        {
          "id": "MDRW2100007526.1.27",
          "form": "높E뛰기ㅋㅋㅋ",
