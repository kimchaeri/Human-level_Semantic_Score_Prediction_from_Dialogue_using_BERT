      "utterance": [
        {
          "id": "MDRW2100035802.1.1",
          "form": "내일 나랑 국립 중앙 미술관 갈래",
          "original_form": "내일 나랑 국립 중앙 미술관 갈래",
          "speaker_id": "2",
          "time": "20210830 03:02"
        },
        {
          "id": "MDRW2100035802.1.2",
          "form": "지금 무슨 전시 있지",
          "original_form": "지금 무슨 전시 있지",
          "speaker_id": "1",
          "time": "20210830 03:02"
        },
        {
          "id": "MDRW2100035802.1.3",
          "form": "지금 이건희 전시가 핫 하잖아",
          "original_form": "지금 이건희 전시가 핫 하잖아",
          "speaker_id": "2",
          "time": "20210830 03:02"
        },
        {
          "id": "MDRW2100035802.1.4",
          "form": "근데 그거는 미리 예약하지 않으면 안 된대",
          "original_form": "근데 그거는 미리 예약하지 않으면 안 된대",
          "speaker_id": "1",
          "time": "20210830 03:02"
        },
        {
          "id": "MDRW2100035802.1.5",
          "form": "내가 그래서 미리 예약해놨지",
          "original_form": "내가 그래서 미리 예약해놨지",
          "speaker_id": "2",
          "time": "20210830 03:02"
        },
        {
          "id": "MDRW2100035802.1.6",
          "form": "정말 나도 가고 싶어",
          "original_form": "정말 나도 가고 싶어",
          "speaker_id": "1",
          "time": "20210830 03:02"
        },
        {
          "id": "MDRW2100035802.1.7",
          "form": "내일 미술관 전시 보러 가자",
          "original_form": "내일 미술관 전시 보러 가자",
          "speaker_id": "2",
          "time": "20210830 03:03"
        },
        {
          "id": "MDRW2100035802.1.8",
          "form": "인왕제색도 가 있다던데",
          "original_form": "인왕제색도 가 있다던데",
          "speaker_id": "1",
          "time": "20210830 03:03"
        },
        {
          "id": "MDRW2100035802.1.9",
          "form": "나도 그 작품 보고 싶어 빨리",
          "original_form": "나도 그 작품 보고 싶어 빨리",
          "speaker_id": "2",
          "time": "20210830 03:03"
        },
        {
          "id": "MDRW2100035802.1.10",
          "form": "얼마나 멋있을까",
          "original_form": "얼마나 멋있을까",
          "speaker_id": "1",
          "time": "20210830 03:03"
        },
        {
          "id": "MDRW2100035802.1.11",
          "form": "간 김에 다른 전시도 다 보고 오자",
          "original_form": "간 김에 다른 전시도 다 보고 오자",
          "speaker_id": "2",
          "time": "20210830 03:03"
        },
        {
          "id": "MDRW2100035802.1.12",
          "form": "전시회 가는 건 언제나 좋아",
          "original_form": "전시회 가는 건 언제나 좋아",
          "speaker_id": "1",
          "time": "20210830 03:03"
        },
        {
          "id": "MDRW2100035802.1.13",
          "form": "기분이 편안해지지",
          "original_form": "기분이 편안해지지",
          "speaker_id": "2",
          "time": "20210830 03:03"
        }
