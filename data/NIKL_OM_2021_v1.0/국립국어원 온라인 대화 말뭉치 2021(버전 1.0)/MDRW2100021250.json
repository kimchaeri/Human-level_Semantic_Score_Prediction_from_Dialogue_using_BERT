      "utterance": [
        {
          "id": "MDRW2100021250.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021250.1.2",
          "form": "어머!",
          "original_form": "어머!",
          "speaker_id": "1",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100021250.1.3",
          "form": "저 여행 좋아해요~~",
          "original_form": "저 여행 좋아해요~~",
          "speaker_id": "1",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100021250.1.4",
          "form": "제주도 여행 또 가보고 싶어요",
          "original_form": "제주도 여행 또 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100021250.1.5",
          "form": "제주도",
          "original_form": "제주도",
          "speaker_id": "2",
          "time": "20210829 11:00"
        },
        {
          "id": "MDRW2100021250.1.6",
          "form": "저도 제주도 좋아해요!",
          "original_form": "저도 제주도 좋아해요!",
          "speaker_id": "2",
          "time": "20210829 11:00"
        },
        {
          "id": "MDRW2100021250.1.7",
          "form": "어디어디 가보셨어요?",
          "original_form": "어디어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210829 11:00"
        },
        {
          "id": "MDRW2100021250.1.8",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100021250.1.9",
          "form": "제주도 안에서요?",
          "original_form": "제주도 안에서요?",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100021250.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100021250.1.11",
          "form": "전 성산일출봉 좋았어요",
          "original_form": "전 성산일출봉 좋았어요",
          "speaker_id": "2",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100021250.1.12",
          "form": "정말 뻥 뚫리는 풍경이었는데",
          "original_form": "정말 뻥 뚫리는 풍경이었는데",
          "speaker_id": "2",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100021250.1.13",
          "form": "내려올때 가파르고 힘 다 빠져서",
          "original_form": "내려올때 가파르고 힘 다 빠져서",
          "speaker_id": "2",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100021250.1.14",
          "form": "후들거리면서 내려왔어요",
          "original_form": "후들거리면서 내려왔어요",
          "speaker_id": "2",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100021250.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100021250.1.16",
          "form": "어머 어떡해요 ㅋㅋ 넘어지지는 않으셨어요?",
          "original_form": "어머 어떡해요 ㅋㅋ 넘어지지는 않으셨어요?",
          "speaker_id": "1",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100021250.1.17",
          "form": "저는 무슨 테마파크가 젤 기억에 남아요 기차타고 돌아다녔어요",
          "original_form": "저는 무슨 테마파크가 젤 기억에 남아요 기차타고 돌아다녔어요",
          "speaker_id": "1",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100021250.1.18",
          "form": "다행히 옆에 친구 붙잡고 내려와서 내려와서 넘어지진않았어요",
          "original_form": "다행히 옆에 친구 붙잡고 내려와서 내려와서 넘어지진않았어요",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100021250.1.19",
          "form": "오 기차타고",
          "original_form": "오 기차타고",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100021250.1.20",
          "form": "거긴안가봤는데 인상적이네요 기차라니",
          "original_form": "거긴안가봤는데 인상적이네요 기차라니",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100021250.1.21",
          "form": "옆에 풍경도 예뻤을것같아요",
          "original_form": "옆에 풍경도 예뻤을것같아요",
          "speaker_id": "2",
          "time": "20210829 11:17"
        },
        {
          "id": "MDRW2100021250.1.22",
          "form": "맞아요 ㅜ 그래서 정류장이 세갠가 네개 있어서",
          "original_form": "맞아요 ㅜ 그래서 정류장이 세갠가 네개 있어서",
          "speaker_id": "1",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100021250.1.23",
          "form": "정류장별로 테마가 다르고 그랬어요!!",
          "original_form": "정류장별로 테마가 다르고 그랬어요!!",
          "speaker_id": "1",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100021250.1.24",
          "form": "제가 해리포터 된 기분이였어요 마법학교 가는 기분",
          "original_form": "제가 해리포터 된 기분이였어요 마법학교 가는 기분",
          "speaker_id": "1",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100021250.1.25",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210829 13:34"
        },
        {
          "id": "MDRW2100021250.1.26",
          "form": "신비롭네요",
          "original_form": "신비롭네요",
          "speaker_id": "2",
          "time": "20210829 13:34"
        },
        {
          "id": "MDRW2100021250.1.27",
          "form": "혹시 에코랜드 인가요?",
          "original_form": "혹시 에코랜드 인가요?",
          "speaker_id": "2",
          "time": "20210829 13:35"
        },
        {
          "id": "MDRW2100021250.1.28",
          "form": "봄에 가면 엄청 예쁠것깉아요",
          "original_form": "봄에 가면 엄청 예쁠것깉아요",
          "speaker_id": "2",
          "time": "20210829 13:35"
        },
        {
          "id": "MDRW2100021250.1.29",
          "form": "어 ㅋ ㅋ에코랜드 맞는 것 같아요!",
          "original_form": "어 ㅋ ㅋ에코랜드 맞는 것 같아요!",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100021250.1.30",
          "form": "어떻게 아셨어요?",
          "original_form": "어떻게 아셨어요?",
          "speaker_id": "1",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100021250.1.31",
          "form": "인터넷에 한번 처봤어요",
          "original_form": "인터넷에 한번 처봤어요",
          "speaker_id": "2",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100021250.1.32",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100021250.1.33",
          "form": "ㅋㅋㅋ 되게 흥미롭지 않아요?",
          "original_form": "ㅋㅋㅋ 되게 흥미롭지 않아요?",
          "speaker_id": "1",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100021250.1.34",
          "form": "이런곳 있는지도 몰랐네요",
          "original_form": "이런곳 있는지도 몰랐네요",
          "speaker_id": "2",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100021250.1.35",
          "form": "저 거기서 사진 찍었는데 다들 유럽여행 다녀왔냐고 ㅋㅋ",
          "original_form": "저 거기서 사진 찍었는데 다들 유럽여행 다녀왔냐고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100021250.1.36",
          "form": "하루 갔다오기 딱 좋은것같아요",
          "original_form": "하루 갔다오기 딱 좋은것같아요",
          "speaker_id": "2",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100021250.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100021250.1.38",
          "form": "스위스 알프스 산맥인 줄 알았다면서 ㅋㅋㅋ",
          "original_form": "스위스 알프스 산맥인 줄 알았다면서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100021250.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100021250.1.40",
          "form": "제가봐도 한국 같지 않았어요",
          "original_form": "제가봐도 한국 같지 않았어요",
          "speaker_id": "1",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100021250.1.41",
          "form": "제주도 진짜 여행갈 곳 많네요",
          "original_form": "제주도 진짜 여행갈 곳 많네요",
          "speaker_id": "2",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100021250.1.42",
          "form": "그때 무슨 공간 공사도 하던데 지금 다 지어졌을걸요",
          "original_form": "그때 무슨 공간 공사도 하던데 지금 다 지어졌을걸요",
          "speaker_id": "1",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100021250.1.43",
          "form": "궁금해요 사진으론 조금밖에 안보여서",
          "original_form": "궁금해요 사진으론 조금밖에 안보여서",
          "speaker_id": "2",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100021250.1.44",
          "form": "코로나 끝나면 다녀와여겠어요",
          "original_form": "코로나 끝나면 다녀와여겠어요",
          "speaker_id": "2",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100021250.1.45",
          "form": "막 엄청 화려하거나 그러진 않은데",
          "original_form": "막 엄청 화려하거나 그러진 않은데",
          "speaker_id": "1",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100021250.1.46",
          "form": "그냥 기차 타고 돌아다니는 거 자체가 힐링이였어요",
          "original_form": "그냥 기차 타고 돌아다니는 거 자체가 힐링이였어요",
          "speaker_id": "1",
