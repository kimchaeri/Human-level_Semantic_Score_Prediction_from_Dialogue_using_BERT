      "utterance": [
        {
          "id": "MDRW2100001071.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210723 00:16"
        },
        {
          "id": "MDRW2100001071.1.2",
          "form": "네 고양이 키우고 있어요",
          "original_form": "네 고양이 키우고 있어요",
          "speaker_id": "2",
          "time": "20210723 01:22"
        },
        {
          "id": "MDRW2100001071.1.3",
          "form": "우와아!!!",
          "original_form": "우와아!!!",
          "speaker_id": "1",
          "time": "20210723 11:20"
        },
        {
          "id": "MDRW2100001071.1.4",
          "form": "저는 물고기 키우고 있거든요ㅋㅋㅋ",
          "original_form": "저는 물고기 키우고 있거든요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210723 11:21"
        },
        {
          "id": "MDRW2100001071.1.5",
          "form": "우오 그래요?",
          "original_form": "우오 그래요?",
          "speaker_id": "2",
          "time": "20210723 11:21"
        },
        {
          "id": "MDRW2100001071.1.6",
          "form": "구피 키우시는거에요?",
          "original_form": "구피 키우시는거에요?",
          "speaker_id": "2",
          "time": "20210723 11:21"
        },
        {
          "id": "MDRW2100001071.1.7",
          "form": "시골 강에서 주어 온 다슬기랑 같이ㅋㅋㅋ",
          "original_form": "시골 강에서 주어 온 다슬기랑 같이ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210723 11:21"
        },
        {
          "id": "MDRW2100001071.1.8",
          "form": "네!! 맞아요!!!",
          "original_form": "네!! 맞아요!!!",
          "speaker_id": "1",
          "time": "20210723 11:21"
        },
        {
          "id": "MDRW2100001071.1.9",
          "form": "나도 고양이 키우고 싶다아아ㅋㅋㅋㅋㅋ",
          "original_form": "나도 고양이 키우고 싶다아아ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210723 11:21"
        },
        {
          "id": "MDRW2100001071.1.10",
          "form": "고양이 귀엽죠ㅎㅎㅎㅎ",
          "original_form": "고양이 귀엽죠ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210723 11:22"
        },
        {
          "id": "MDRW2100001071.1.11",
          "form": "저희집도 구피 키웠었는데 고양이가 너무 관심을 많이 가져서 애들이 스트레스로 다 죽어버렸어요..",
          "original_form": "저희집도 구피 키웠었는데 고양이가 너무 관심을 많이 가져서 애들이 스트레스로 다 죽어버렸어요..",
          "speaker_id": "2",
          "time": "20210723 11:22"
        },
        {
          "id": "MDRW2100001071.1.12",
          "form": "히이이익?!!",
          "original_form": "히이이익?!!",
          "speaker_id": "1",
          "time": "20210723 11:23"
        },
        {
          "id": "MDRW2100001071.1.13",
          "form": "그래도 먹이가 되진 않았네요ㅋㅋㅋ",
          "original_form": "그래도 먹이가 되진 않았네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210723 11:23"
        },
        {
          "id": "MDRW2100001071.1.14",
          "form": "반려동물은 정말.. 키우면 엄청 좋긴 한데 신경이 너무 많이 쓰여요ㅋㅋㅋ",
          "original_form": "반려동물은 정말.. 키우면 엄청 좋긴 한데 신경이 너무 많이 쓰여요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210723 11:23"
        },
        {
          "id": "MDRW2100001071.1.15",
          "form": "ㅋㅋㅎㅎㅋㅋ 그러게요ㅎㅎ 먹이가 됐으면 큰일날뻔 했어요",
          "original_form": "ㅋㅋㅎㅎㅋㅋ 그러게요ㅎㅎ 먹이가 됐으면 큰일날뻔 했어요",
          "speaker_id": "2",
          "time": "20210723 11:23"
        },
        {
          "id": "MDRW2100001071.1.16",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210723 11:23"
        },
        {
          "id": "MDRW2100001071.1.17",
          "form": "또 병원비도 장난아니게 나와서ㅠ",
          "original_form": "또 병원비도 장난아니게 나와서ㅠ",
          "speaker_id": "2",
          "time": "20210723 11:24"
        },
        {
          "id": "MDRW2100001071.1.18",
          "form": "한번 아프면 주기적으로 가야하니까 병원비가 좀 골치 아프더라고요",
          "original_form": "한번 아프면 주기적으로 가야하니까 병원비가 좀 골치 아프더라고요",
          "speaker_id": "2",
          "time": "20210723 11:24"
        },
        {
          "id": "MDRW2100001071.1.19",
          "form": "특히 고양이는 병원비가 좀 세다고 하던디..",
          "original_form": "특히 고양이는 병원비가 좀 세다고 하던디..",
          "speaker_id": "1",
          "time": "20210723 11:25"
        },
        {
          "id": "MDRW2100001071.1.20",
          "form": "저도 키우고 싶은 마음은 큰데 애들 아플 때 책임을 못져줄 거 같아서 못키우겠어요ㅠㅠ",
          "original_form": "저도 키우고 싶은 마음은 큰데 애들 아플 때 책임을 못져줄 거 같아서 못키우겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210723 11:26"
        },
        {
          "id": "MDRW2100001071.1.21",
          "form": "아하ㅠㅠㅠㅠ",
          "original_form": "아하ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210723 11:26"
        },
        {
          "id": "MDRW2100001071.1.22",
          "form": "그래도 동물 좋아하니까 좋아하는 만큼 책임감이 따라줄 것 같아요",
          "original_form": "그래도 동물 좋아하니까 좋아하는 만큼 책임감이 따라줄 것 같아요",
          "speaker_id": "2",
          "time": "20210723 11:26"
        },
        {
          "id": "MDRW2100001071.1.23",
          "form": "그러면 좋겠어요 진짜ㅠㅠ",
          "original_form": "그러면 좋겠어요 진짜ㅠㅠ",
          "speaker_id": "1",
          "time": "20210723 11:52"
        },
        {
          "id": "MDRW2100001071.1.24",
          "form": "나중에 고정적인 월급 받기 시작하면 키워볼까 생각 중이에요ㅋㅋ",
          "original_form": "나중에 고정적인 월급 받기 시작하면 키워볼까 생각 중이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210723 11:53"
        },
        {
          "id": "MDRW2100001071.1.25",
          "form": "음음 좋아요ㅎㅎㅎ 특히 자취할 때 반려동물 있으면 정말 안정감이 많이 느껴진대요",
          "original_form": "음음 좋아요ㅎㅎㅎ 특히 자취할 때 반려동물 있으면 정말 안정감이 많이 느껴진대요",
          "speaker_id": "2",
          "time": "20210723 11:59"
        },
        {
          "id": "MDRW2100001071.1.26",
          "form": "자취하면 혼자라 외로운데..",
          "original_form": "자취하면 혼자라 외로운데..",
          "speaker_id": "1",
          "time": "20210723 17:55"
        },
        {
          "id": "MDRW2100001071.1.27",
          "form": "반려동물 있으면 사람처럼 대화는 못하더라도",
          "original_form": "반려동물 있으면 사람처럼 대화는 못하더라도",
          "speaker_id": "1",
          "time": "20210723 17:55"
        },
        {
          "id": "MDRW2100001071.1.28",
          "form": "엄청 좋다 하더라구요!!",
          "original_form": "엄청 좋다 하더라구요!!",
          "speaker_id": "1",
          "time": "20210723 17:55"
        },
        {
          "id": "MDRW2100001071.1.29",
          "form": "맞아요ㅠㅠㅠ 그래도 같이 한다는 안정감이 있으니까",
          "original_form": "맞아요ㅠㅠㅠ 그래도 같이 한다는 안정감이 있으니까",
          "speaker_id": "2",
          "time": "20210723 17:58"
        },
        {
          "id": "MDRW2100001071.1.30",
          "form": "없는 것 보단 낫죠",
          "original_form": "없는 것 보단 낫죠",
          "speaker_id": "2",
          "time": "20210723 17:58"
        },
        {
          "id": "MDRW2100001071.1.31",
          "form": "유튜브 보면 주인이 울고 있을 때 다가와서 위로해준다는거나!",
          "original_form": "유튜브 보면 주인이 울고 있을 때 다가와서 위로해준다는거나!",
          "speaker_id": "1",
          "time": "20210723 17:58"
        },
        {
          "id": "MDRW2100001071.1.32",
          "form": "힘들 때 힘이 되어주는 거 보면",
          "original_form": "힘들 때 힘이 되어주는 거 보면",
          "speaker_id": "1",
          "time": "20210723 17:59"
        },
        {
          "id": "MDRW2100001071.1.33",
          "form": "진짜... 너무 좋아요ㅎㅎ",
          "original_form": "진짜... 너무 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210723 17:59"
        },
        {
          "id": "MDRW2100001071.1.34",
          "form": "맞아요ㅠㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210723 18:02"
        },
        {
          "id": "MDRW2100001071.1.35",
          "form": "가끔씩 애교부리는 것도 너무 귀엽고ㅎㅎㅎ",
          "original_form": "가끔씩 애교부리는 것도 너무 귀엽고ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210724 10:29"
        },
        {
          "id": "MDRW2100001071.1.36",
          "form": "요즘은 반려동물들 장난감이나 간식이 너무 잘 나와요!!ㅋㅋ",
          "original_form": "요즘은 반려동물들 장난감이나 간식이 너무 잘 나와요!!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210724 10:29"
        },
        {
          "id": "MDRW2100001071.1.37",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210724 11:59"
        },
        {
          "id": "MDRW2100001071.1.38",
          "form": "값도 싸게 잘 나오더라고요 ㅎㅎ",
          "original_form": "값도 싸게 잘 나오더라고요 ㅎㅎ",
