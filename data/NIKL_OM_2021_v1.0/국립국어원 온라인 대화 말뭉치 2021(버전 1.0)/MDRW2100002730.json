      "utterance": [
        {
          "id": "MDRW2100002730.1.1",
          "form": "오늘 저녁메뉴는 뭘로 생각중이신가요?",
          "original_form": "오늘 저녁메뉴는 뭘로 생각중이신가요?",
          "speaker_id": "1",
          "time": "20210820 15:23"
        },
        {
          "id": "MDRW2100002730.1.2",
          "form": "저는 음 삼겹살생각중이예요!",
          "original_form": "저는 음 삼겹살생각중이예요!",
          "speaker_id": "2",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002730.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002730.1.4",
          "form": "삼겹살 으허ㅠ맛있겠어요ㅠ 저는... 그냥 있는거?ㅋㅋㅋ 대충 ..ㅋㅋㅋ",
          "original_form": "삼겹살 으허ㅠ맛있겠어요ㅠ 저는... 그냥 있는거?ㅋㅋㅋ 대충 ..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002730.1.5",
          "form": "요즘 삼겹살도 되게 비싸더라구요ㅠ흑",
          "original_form": "요즘 삼겹살도 되게 비싸더라구요ㅠ흑",
          "speaker_id": "1",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002730.1.6",
          "form": "맞아요ㄷㄷ목살이 더비쌌던거같은데",
          "original_form": "맞아요ㄷㄷ목살이 더비쌌던거같은데",
          "speaker_id": "2",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002730.1.7",
          "form": "어느순간 삼겹이",
          "original_form": "어느순간 삼겹이",
          "speaker_id": "2",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002730.1.8",
          "form": "훨더비싸졌더라고요",
          "original_form": "훨더비싸졌더라고요",
          "speaker_id": "2",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002730.1.9",
          "form": "삼겹살 먹고싶네요 쩝쩝. 저도 조만간 사와서 먹어야겠어요ㅋ",
          "original_form": "삼겹살 먹고싶네요 쩝쩝. 저도 조만간 사와서 먹어야겠어요ㅋ",
          "speaker_id": "1",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002730.1.10",
          "form": "맛있겠네요ㅋㅋㅋ",
          "original_form": "맛있겠네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002730.1.11",
          "form": "미나리삼겹살 드셔보셨나요?",
          "original_form": "미나리삼겹살 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002730.1.12",
          "form": "아 대박 상상했어요ㅠㅠㅋㅋ",
          "original_form": "아 대박 상상했어요ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002730.1.13",
          "form": "너무맛있죠ㅠ",
          "original_form": "너무맛있죠ㅠ",
          "speaker_id": "1",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002730.1.14",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100002730.1.15",
          "form": "매운건잘드세요?",
          "original_form": "매운건잘드세요?",
          "speaker_id": "2",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100002730.1.16",
          "form": "매운거는 엽떡2단계정도요~",
          "original_form": "매운거는 엽떡2단계정도요~",
          "speaker_id": "1",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100002730.1.17",
          "form": "저는 초보맛먹는답니다ㅎㅎㅎ",
          "original_form": "저는 초보맛먹는답니다ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 18:13"
        },
        {
          "id": "MDRW2100002730.1.18",
          "form": "불닭은 드시나요?",
          "original_form": "불닭은 드시나요?",
          "speaker_id": "2",
          "time": "20210822 18:13"
        },
        {
          "id": "MDRW2100002730.1.19",
          "form": "저도초보맛ㅋㅋ그게2단계같아요",
          "original_form": "저도초보맛ㅋㅋ그게2단계같아요",
          "speaker_id": "1",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100002730.1.20",
          "form": "불닭?볶음면?아니면 닭발?",
          "original_form": "불닭?볶음면?아니면 닭발?",
          "speaker_id": "1",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100002730.1.21",
          "form": "불닭볶음면이요ㅋㅋㅋ",
          "original_form": "불닭볶음면이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:54"
        },
        {
          "id": "MDRW2100002730.1.22",
          "form": "그건잘드시나요?",
          "original_form": "그건잘드시나요?",
          "speaker_id": "2",
          "time": "20210823 15:54"
        },
        {
          "id": "MDRW2100002730.1.23",
          "form": "컵라면은 맛있어서 가끔 먹어요",
