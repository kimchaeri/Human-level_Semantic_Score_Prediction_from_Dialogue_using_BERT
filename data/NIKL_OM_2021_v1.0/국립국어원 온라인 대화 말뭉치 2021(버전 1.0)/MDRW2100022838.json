      "utterance": [
        {
          "id": "MDRW2100022838.1.1",
          "form": "요즘 그런 연옌 많지않아요 ㅋㅋ ?",
          "original_form": "요즘 그런 연옌 많지않아요 ㅋㅋ ?",
          "speaker_id": "2",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100022838.1.2",
          "form": "남녀가리지않고 ㅋㅋ",
          "original_form": "남녀가리지않고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100022838.1.3",
          "form": "연예인 중에서 패션 워너비 누구에요?",
          "original_form": "연예인 중에서 패션 워너비 누구에요?",
          "speaker_id": "1",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022838.1.4",
          "form": "저런 스탈 멋있다.. 하는거..",
          "original_form": "저런 스탈 멋있다.. 하는거..",
          "speaker_id": "1",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022838.1.5",
          "form": "음 패션 저 슬기요 ㅋㅋㅋ",
          "original_form": "음 패션 저 슬기요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:29"
        },
        {
          "id": "MDRW2100022838.1.6",
          "form": "레벨 슬기 ㅋㅋ",
          "original_form": "레벨 슬기 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:29"
        },
        {
          "id": "MDRW2100022838.1.7",
          "form": "슬기도 은근 스트릿룩 사진이 많네요..",
          "original_form": "슬기도 은근 스트릿룩 사진이 많네요..",
          "speaker_id": "1",
          "time": "20210829 18:30"
        },
        {
          "id": "MDRW2100022838.1.8",
          "form": "맞죠 ㅋㅋ 전 너무 투머치로 꾸미는걸",
          "original_form": "맞죠 ㅋㅋ 전 너무 투머치로 꾸미는걸",
          "speaker_id": "2",
          "time": "20210829 18:32"
        },
        {
          "id": "MDRW2100022838.1.9",
          "form": "안좋아해서요 ㅎㅎ",
          "original_form": "안좋아해서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:32"
        },
        {
          "id": "MDRW2100022838.1.10",
          "form": "맞아용... 이쁘고 잘생겨야.. 투머치도 용서 받는거 같아요..",
          "original_form": "맞아용... 이쁘고 잘생겨야.. 투머치도 용서 받는거 같아요..",
          "speaker_id": "1",
          "time": "20210829 18:33"
        },
        {
          "id": "MDRW2100022838.1.11",
          "form": "앞으로ㅜ제이름은 투머치입니다.. ㅋㅋㅋ",
          "original_form": "앞으로ㅜ제이름은 투머치입니다.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:33"
        },
        {
          "id": "MDRW2100022838.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ아 많이 꾸미는거 좋아하시는군요 !",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 많이 꾸미는거 좋아하시는군요 !",
          "speaker_id": "2",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022838.1.13",
          "form": "연예인 중 어떤 스탈로 입으셔요?",
          "original_form": "연예인 중 어떤 스탈로 입으셔요?",
          "speaker_id": "2",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022838.1.14",
          "form": "음.. 전 저 닮은 공유요..",
          "original_form": "음.. 전 저 닮은 공유요..",
          "speaker_id": "1",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100022838.1.15",
          "form": "ㅎㅎ 닮은건 확실한가요?",
          "original_form": "ㅎㅎ 닮은건 확실한가요?",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022838.1.16",
          "form": "일할땐 평범하게 슬랙스에 로퍼 많이 잊고.. \r\n집에선 벙거지 자주 써요",
          "original_form": "일할땐 평범하게 슬랙스에 로퍼 많이 잊고.. \r\n집에선 벙거지 자주 써요",
          "speaker_id": "1",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022838.1.17",
          "form": "오 ㅎㅎ 슬랙스 로퍼면 투머치는 아닌걸요?",
          "original_form": "오 ㅎㅎ 슬랙스 로퍼면 투머치는 아닌걸요?",
          "speaker_id": "2",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100022838.1.18",
          "form": "눈 두개.. 코하나.. 입하나.. \r\n뭐 다를게 있나여? ㅋㅋ 사람다 거기서 고기죠.. ㅋㅋ",
          "original_form": "눈 두개.. 코하나.. 입하나.. \r\n뭐 다를게 있나여? ㅋㅋ 사람다 거기서 고기죠.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100022838.1.19",
          "form": "튀지않게 평범하게 입어요..",
          "original_form": "튀지않게 평범하게 입어요..",
          "speaker_id": "1",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100022838.1.20",
          "form": "ㅋㄱㄲㅋㅋ그럼 저도 전지현할래요 ㅋㅋ",
          "original_form": "ㅋㄱㄲㅋㅋ그럼 저도 전지현할래요 ㅋㅋ",
