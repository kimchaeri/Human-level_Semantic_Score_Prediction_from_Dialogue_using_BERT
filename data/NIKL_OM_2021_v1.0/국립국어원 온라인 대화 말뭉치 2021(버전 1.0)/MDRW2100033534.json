      "utterance": [
        {
          "id": "MDRW2100033534.1.1",
          "form": "최근에 사신거있나요?",
          "original_form": "최근에 사신거있나요?",
          "speaker_id": "1",
          "time": "20210824 11:53"
        },
        {
          "id": "MDRW2100033534.1.2",
          "form": "저 최근에 치킨",
          "original_form": "저 최근에 치킨",
          "speaker_id": "2",
          "time": "20210824 12:07"
        },
        {
          "id": "MDRW2100033534.1.3",
          "form": "구매한 거 뿐이네요ㅋㅋㅋㅋㅋ",
          "original_form": "구매한 거 뿐이네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:08"
        },
        {
          "id": "MDRW2100033534.1.4",
          "form": "뭐 사셨어요?",
          "original_form": "뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210824 12:08"
        },
        {
          "id": "MDRW2100033534.1.5",
          "form": "ㅋㅋㅋ어떤치킨인가요",
          "original_form": "ㅋㅋㅋ어떤치킨인가요",
          "speaker_id": "1",
          "time": "20210824 12:21"
        },
        {
          "id": "MDRW2100033534.1.6",
          "form": "저는 운동화요",
          "original_form": "저는 운동화요",
          "speaker_id": "1",
          "time": "20210824 12:21"
        },
        {
          "id": "MDRW2100033534.1.7",
          "form": "저 후참인데",
          "original_form": "저 후참인데",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100033534.1.8",
          "form": "맛이.. 좀..",
          "original_form": "맛이.. 좀..",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100033534.1.9",
          "form": "너무 아쉬웠엉6",
          "original_form": "너무 아쉬웠엉6",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100033534.1.10",
          "form": "신발 뭐 사셨어요??",
          "original_form": "신발 뭐 사셨어요??",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100033534.1.11",
          "form": "저 나이키요ㅋㅋㅋ",
          "original_form": "저 나이키요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:48"
        },
        {
          "id": "MDRW2100033534.1.12",
          "form": "오즈위고",
          "original_form": "오즈위고",
          "speaker_id": "1",
          "time": "20210824 12:48"
        },
        {
          "id": "MDRW2100033534.1.13",
          "form": "아시나요?",
          "original_form": "아시나요?",
