          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100033534.1.15",
          "form": "유명하잖아여ㅋㅋㅋㅋ",
          "original_form": "유명하잖아여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100033534.1.16",
          "form": "ㅋㅋㅋㅋ 다른거사고싶으신거",
          "original_form": "ㅋㅋㅋㅋ 다른거사고싶으신거",
          "speaker_id": "1",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100033534.1.17",
          "form": "있나요?",
