        {
          "id": "MDRW2100000435.1.1",
          "form": "어떤책 좋아해?",
          "original_form": "어떤책 좋아해?",
          "speaker_id": "1",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000435.1.2",
          "form": "보통 철학책 좋아해 소설도 가끔 읽어",
          "original_form": "보통 철학책 좋아해 소설도 가끔 읽어",
          "speaker_id": "2",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000435.1.3",
          "form": "철학 너무 어렵다",
          "original_form": "철학 너무 어렵다",
          "speaker_id": "1",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000435.1.4",
          "form": "철학과니?",
          "original_form": "철학과니?",
          "speaker_id": "1",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000435.1.5",
          "form": "그런가 철학은 교양으로 듣고 전공은 컴퓨터쪽이야",
          "original_form": "그런가 철학은 교양으로 듣고 전공은 컴퓨터쪽이야",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000435.1.6",
          "form": "컴공!!",
          "original_form": "컴공!!",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000435.1.7",
          "form": "취업 잘하겠넵!!",
          "original_form": "취업 잘하겠넵!!",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000435.1.8",
          "form": "맞앜ㅋ너는 전공 뭐야?",
          "original_form": "맞앜ㅋ너는 전공 뭐야?",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000435.1.9",
          "form": "전기!",
          "original_form": "전기!",
          "speaker_id": "1",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000435.1.10",
          "form": "씨언어,조금해봤는데 넘 어렵드라",
          "original_form": "씨언어,조금해봤는데 넘 어렵드라",
          "speaker_id": "1",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000435.1.11",
          "form": "오 전기면 비슷하넹 취업잘되겠다",
          "original_form": "오 전기면 비슷하넹 취업잘되겠다",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000435.1.12",
          "form": "그래도 힘들드라 ㅠㅠ",
          "original_form": "그래도 힘들드라 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000435.1.13",
          "form": "취업?ㅜ",
          "original_form": "취업?ㅜ",
          "speaker_id": "2",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000435.1.14",
