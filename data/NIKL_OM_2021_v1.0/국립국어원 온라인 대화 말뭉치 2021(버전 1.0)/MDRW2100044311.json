      "utterance": [
        {
          "id": "MDRW2100044311.1.1",
          "form": "병원 자주 다니세요 ?",
          "original_form": "병원 자주 다니세요 ?",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044311.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044311.1.3",
          "form": "네 저 잘 싸요",
          "original_form": "네 저 잘 싸요",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044311.1.4",
          "form": "구라치ㅣㅈ마셍6",
          "original_form": "구라치ㅣㅈ마셍6",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044311.1.5",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044311.1.6",
          "form": "절 아세요 ?",
          "original_form": "절 아세요 ?",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044311.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044311.1.8",
          "form": "님은 변비같아요",
          "original_form": "님은 변비같아요",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044311.1.9",
          "form": "아 관상볼줄 아시나봐요",
          "original_form": "아 관상볼줄 아시나봐요",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044311.1.10",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044311.1.11",
          "form": "아니지 얼굴이 아닌데",
          "original_form": "아니지 얼굴이 아닌데",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044311.1.12",
          "form": "초능력",
          "original_form": "초능력",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044311.1.13",
          "form": "가지고 게ㅐ시네요",
          "original_form": "가지고 게ㅐ시네요",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044311.1.14",
          "form": "정말 쓸데없군요",
          "original_form": "정말 쓸데없군요",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044311.1.15",
          "form": "텍스트를 보고 변비를 구별해내는 능력이라니",
          "original_form": "텍스트를 보고 변비를 구별해내는 능력이라니",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044311.1.16",
          "form": "알쓸이에요",
          "original_form": "알쓸이에요",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044311.1.17",
          "form": "알고보면 쓸모있는 name1?",
          "original_form": "알고보면 쓸모있는 &name1&?",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044311.1.18",
          "form": "전 대충 봐도 쓸모있죠",
          "original_form": "전 대충 봐도 쓸모있죠",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044311.1.19",
          "form": "당신 똥 잘 못 싸시죠?",
          "original_form": "당신 똥 잘 못 싸시죠?",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044311.1.20",
          "form": "name2씨",
          "original_form": "&name2&씨",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044311.1.21",
          "form": "알수록 쓸모없는",
          "original_form": "알수록 쓸모없는",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044311.1.22",
          "form": "아 모욕적이에요",
          "original_form": "아 모욕적이에요",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044311.1.23",
          "form": "진짜 얼굴도 모르는 사람한테",
          "original_form": "진짜 얼굴도 모르는 사람한테",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044311.1.24",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044311.1.25",
          "form": "똥 잘 싸시라는 의미로",
          "original_form": "똥 잘 싸시라는 의미로",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044311.1.26",
          "form": "모욕감을 받았어요",
          "original_form": "모욕감을 받았어요",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044311.1.27",
          "form": "말씀드린겁니다",
          "original_form": "말씀드린겁니다",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044311.1.28",
          "form": "어의없내요.",
          "original_form": "어의없내요.",
          "speaker_id": "2",
          "time": "20210907 22:18"
        }
      ]
