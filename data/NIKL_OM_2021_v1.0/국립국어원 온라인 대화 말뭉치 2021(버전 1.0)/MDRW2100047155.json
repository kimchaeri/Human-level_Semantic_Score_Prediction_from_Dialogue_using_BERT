      "utterance": [
        {
          "id": "MDRW2100047155.1.1",
          "form": "코로나 시국이라 여행 잘 못가는데.. 끝나면 꼭 가고 싶다 하는 곳 있으세요?",
          "original_form": "코로나 시국이라 여행 잘 못가는데.. 끝나면 꼭 가고 싶다 하는 곳 있으세요?",
          "speaker_id": "1",
          "time": "20210901 15:03"
        },
        {
          "id": "MDRW2100047155.1.2",
          "form": "저는 일본가고싶어요",
          "original_form": "저는 일본가고싶어요",
          "speaker_id": "2",
          "time": "20210901 15:03"
        },
        {
          "id": "MDRW2100047155.1.3",
          "form": "일본 어디 가고 싶으세요?",
          "original_form": "일본 어디 가고 싶으세요?",
          "speaker_id": "1",
          "time": "20210901 15:04"
        },
        {
          "id": "MDRW2100047155.1.4",
          "form": "어디든 좋을거같아요 좀 안가본곳 가고싶기도해요",
          "original_form": "어디든 좋을거같아요 좀 안가본곳 가고싶기도해요",
          "speaker_id": "2",
          "time": "20210901 15:04"
        },
        {
          "id": "MDRW2100047155.1.5",
          "form": "어디어디 가보셨어요?",
          "original_form": "어디어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210901 15:05"
        },
        {
          "id": "MDRW2100047155.1.6",
          "form": "홋카이도 말고는 큰도시는 가본거같아요",
          "original_form": "홋카이도 말고는 큰도시는 가본거같아요",
          "speaker_id": "2",
          "time": "20210901 15:05"
        },
        {
          "id": "MDRW2100047155.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210901 15:05"
        },
        {
          "id": "MDRW2100047155.1.8",
          "form": "오홍",
          "original_form": "오홍",
          "speaker_id": "1",
          "time": "20210901 15:05"
        },
        {
          "id": "MDRW2100047155.1.9",
          "form": "저는 하동 가고싶어요",
          "original_form": "저는 하동 가고싶어요",
          "speaker_id": "1",
          "time": "20210901 15:05"
        },
        {
          "id": "MDRW2100047155.1.10",
          "form": "국내에 있는 차 다원 가고 싶어요",
          "original_form": "국내에 있는 차 다원 가고 싶어요",
          "speaker_id": "1",
          "time": "20210901 15:05"
        },
        {
          "id": "MDRW2100047155.1.11",
          "form": "일본은 뭐가 젤 매력적인가요?",
          "original_form": "일본은 뭐가 젤 매력적인가요?",
          "speaker_id": "1",
          "time": "20210901 15:06"
        },
        {
          "id": "MDRW2100047155.1.12",
          "form": "음 그냥.. 동네같은데",
          "original_form": "음 그냥.. 동네같은데",
          "speaker_id": "2",
          "time": "20210901 15:07"
        },
        {
          "id": "MDRW2100047155.1.13",
          "form": "못가니까 가고싶은거같아요",
          "original_form": "못가니까 가고싶은거같아요",
          "speaker_id": "2",
          "time": "20210901 15:07"
        },
        {
          "id": "MDRW2100047155.1.14",
          "form": "아... 백신맞으셨어요?",
          "original_form": "아... 백신맞으셨어요?",
          "speaker_id": "1",
          "time": "20210901 15:08"
        },
        {
          "id": "MDRW2100047155.1.15",
          "form": "정기적으로 방문해야할거같아요",
          "original_form": "정기적으로 방문해야할거같아요",
          "speaker_id": "2",
          "time": "20210901 15:08"
        },
        {
          "id": "MDRW2100047155.1.16",
          "form": "백신맞았으면 지금이라도...",
          "original_form": "백신맞았으면 지금이라도...",
          "speaker_id": "1",
          "time": "20210901 15:08"
        },
        {
          "id": "MDRW2100047155.1.17",
          "form": "백신아직이에요",
          "original_form": "백신아직이에요",
          "speaker_id": "2",
          "time": "20210901 15:08"
        },
        {
          "id": "MDRW2100047155.1.18",
          "form": "정기적 방문 ㅎㅎ",
          "original_form": "정기적 방문 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 15:08"
        },
        {
          "id": "MDRW2100047155.1.19",
          "form": "일본을 사랑하시네요",
          "original_form": "일본을 사랑하시네요",
