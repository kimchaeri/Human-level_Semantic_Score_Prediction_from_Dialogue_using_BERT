      "utterance": [
        {
          "id": "MDRW2100045302.1.1",
          "form": "진짜이건뭐지...했잖니",
          "original_form": "진짜이건뭐지...했잖니",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045302.1.2",
          "form": "응 나중에 등장하는 애인데",
          "original_form": "응 나중에 등장하는 애인데",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045302.1.3",
          "form": "시오한테 첫눈에반함",
          "original_form": "시오한테 첫눈에반함",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045302.1.4",
          "form": "kizul",
          "original_form": "kizul",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045302.1.5",
          "form": "그 성적 유린 당한 친구?",
          "original_form": "그 성적 유린 당한 친구?",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045302.1.6",
          "form": "응....",
          "original_form": "응....",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045302.1.7",
          "form": "그래서 사토 좋아했는데",
          "original_form": "그래서 사토 좋아했는데",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045302.1.8",
          "form": "시오로 갈아탄 애?",
          "original_form": "시오로 갈아탄 애?",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045302.1.9",
          "form": "............응",
          "original_form": "............응",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045302.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045302.1.11",
          "form": "진짜 웃겨...",
          "original_form": "진짜 웃겨...",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045302.1.12",
          "form": "들으면 들을수록 정말 어이없네",
          "original_form": "들으면 들을수록 정말 어이없네",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045302.1.13",
          "form": "진짜 보면서 걘 또 뭐야 됨",
          "original_form": "진짜 보면서 걘 또 뭐야 됨",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045302.1.14",
          "form": "얘는 왜ㅠ여기와서 이러고 있나요",
          "original_form": "얘는 왜ㅠ여기와서 이러고 있나요",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045302.1.15",
          "form": "내말이....네가왜거기서나와",
          "original_form": "내말이....네가왜거기서나와",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045302.1.16",
          "form": "저기 정신과부터 가는게 어때...",
          "original_form": "저기 정신과부터 가는게 어때...",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045302.1.17",
          "form": "하다못해 경찰신고해",
          "original_form": "하다못해 경찰신고해",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045302.1.18",
          "form": "그거사랑아냐....왜아무도정신과갈생각을안하는거야",
          "original_form": "그거사랑아냐....왜아무도정신과갈생각을안하는거야",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045302.1.19",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045302.1.20",
          "form": "진짜로...........하지만 그럴일은 ㅇ없음 신고할 정신 있었음 이미 본인이 자수하고 철장들어감",
          "original_form": "진짜로...........하지만 그럴일은 ㅇ없음 신고할 정신 있었음 이미 본인이 자수하고 철장들어감",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045302.1.21",
          "form": "다들 자기 망한 사랑에서 자기 자신에 대한 연민을 느끼고 있음이",
          "original_form": "다들 자기 망한 사랑에서 자기 자신에 대한 연민을 느끼고 있음이",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045302.1.22",
          "form": "틀림없음",
          "original_form": "틀림없음",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045302.1.23",
          "form": "맞아.............. 그리고 여기서 아동인권 따지는 것도 의미없지만 어린아이 진짜 밑도끝도 대상화하는거 너무 어이없었음",
          "original_form": "맞아.............. 그리고 여기서 아동인권 따지는 것도 의미없지만 어린아이 진짜 밑도끝도 대상화하는거 너무 어이없었음",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045302.1.24",
          "form": "난 이렇게 망가졌지만 시오를 사랑해 시오를 지키고 싶어~",
          "original_form": "난 이렇게 망가졌지만 시오를 사랑해 시오를 지키고 싶어~",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045302.1.25",
          "form": "진심으로 유딩친구랑 그러면 뭐 어쩌라는건지",
          "original_form": "진심으로 유딩친구랑 그러면 뭐 어쩌라는건지",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045302.1.26",
          "form": "어린아이의 무해함...순수함...편견없이사람을받아들이는....<정말끝없이뽑아먹기",
          "original_form": "어린아이의 무해함...순수함...편견없이사람을받아들이는....<정말끝없이뽑아먹기",
          "speaker_id": "2",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045302.1.27",
          "form": "잘 모르겠음",
          "original_form": "잘 모르겠음",
          "speaker_id": "1",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045302.1.28",
          "form": "내말이......................... 그냥 병원이나 가지",
          "original_form": "내말이......................... 그냥 병원이나 가지",
          "speaker_id": "2",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045302.1.29",
          "form": "걍 다 광기에 차있어서",
          "original_form": "걍 다 광기에 차있어서",
