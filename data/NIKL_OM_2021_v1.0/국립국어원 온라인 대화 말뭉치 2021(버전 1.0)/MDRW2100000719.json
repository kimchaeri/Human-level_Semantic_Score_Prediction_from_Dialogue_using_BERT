        },
        {
          "id": "MDRW2100000719.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 18:43"
        },
        {
          "id": "MDRW2100000719.1.3",
          "form": "최근 싸이월드부활에 대해 들어보셨나요",
          "original_form": "최근 싸이월드부활에 대해 들어보셨나요",
          "speaker_id": "1",
          "time": "20210819 18:44"
        },
        {
          "id": "MDRW2100000719.1.4",
          "form": "네 싸이월드 다시한다더라구요 들어보기만했는데 지금 되나요?",
          "original_form": "네 싸이월드 다시한다더라구요 들어보기만했는데 지금 되나요?",
          "speaker_id": "2",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100000719.1.5",
          "form": "아직 부활한다는 소리만 듣고",
          "original_form": "아직 부활한다는 소리만 듣고",
          "speaker_id": "1",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100000719.1.6",
          "form": "해보진 못했는대 진짜 추억돋는거같아요",
          "original_form": "해보진 못했는대 진짜 추억돋는거같아요",
          "speaker_id": "1",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000719.1.7",
          "form": "친구들이랑 싸이월드 한참 재미잇게 햇는대",
          "original_form": "친구들이랑 싸이월드 한참 재미잇게 햇는대",
          "speaker_id": "1",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000719.1.8",
          "form": "맞아요 저도 싸이월드세대라서 그때당시 열심히했었는데",
          "original_form": "맞아요 저도 싸이월드세대라서 그때당시 열심히했었는데",
          "speaker_id": "2",
          "time": "20210819 19:03"
        },
        {
          "id": "MDRW2100000719.1.9",
          "form": "부활한다고하니까 추억이떠오르네요 ㅋㅋㅋ",
          "original_form": "부활한다고하니까 추억이떠오르네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:03"
        },
        {
          "id": "MDRW2100000719.1.10",
          "form": "그때 꾸미기 방명록",
          "original_form": "그때 꾸미기 방명록",
          "speaker_id": "1",
          "time": "20210819 19:55"
        },
        {
          "id": "MDRW2100000719.1.11",
          "form": "참 좋았는대 부활한다고하니 기대되요",
          "original_form": "참 좋았는대 부활한다고하니 기대되요",
          "speaker_id": "1",
          "time": "20210819 19:55"
        },
        {
          "id": "MDRW2100000719.1.12",
          "form": "그때 방명록 많이하셨나요",
          "original_form": "그때 방명록 많이하셨나요",
          "speaker_id": "1",
          "time": "20210819 19:55"
        },
        {
          "id": "MDRW2100000719.1.13",
          "form": "아뇨 저 아싸라 친한애들이랑만 해서 엄청 소소하게 굴러갔어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 저 아싸라 친한애들이랑만 해서 엄청 소소하게 굴러갔어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:55"
        },
        {
          "id": "MDRW2100000719.1.14",
          "form": "그 도토리로 미니홈피꾸미고",
          "original_form": "그 도토리로 미니홈피꾸미고",
          "speaker_id": "1",
          "time": "20210819 19:56"
        },
        {
          "id": "MDRW2100000719.1.15",
          "form": "진짜 그 시절 싸이월드 다시 인기 있어져서 또 다시 친구들이랑 하고싶네요",
          "original_form": "진짜 그 시절 싸이월드 다시 인기 있어져서 또 다시 친구들이랑 하고싶네요",
          "speaker_id": "1",
          "time": "20210819 19:56"
        },
        {
          "id": "MDRW2100000719.1.16",
          "form": "진짜 추억돋고 옛날 기억 돌아오게 해줘서 고마운거같아요",
          "original_form": "진짜 추억돋고 옛날 기억 돌아오게 해줘서 고마운거같아요",
          "speaker_id": "1",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000719.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ근데 또 오픈하거싶지않은 마음도들어요 그때의추억 끄집어내고싶지않은...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ근데 또 오픈하거싶지않은 마음도들어요 그때의추억 끄집어내고싶지않은...",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000719.1.18",
          "form": "사진있나 한번 보러가야될거같아여 흑역사들도 같이묻혀있어서 ㅋㅋㅋㅋㅋ",
          "original_form": "사진있나 한번 보러가야될거같아여 흑역사들도 같이묻혀있어서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000719.1.19",
          "form": "그럴수도 잇을거같아요 개인마다 또 틀리니까",
          "original_form": "그럴수도 잇을거같아요 개인마다 또 틀리니까",
          "speaker_id": "1",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000719.1.20",
          "form": "저도 잇을거같은 추억 돋는?",
          "original_form": "저도 잇을거같은 추억 돋는?",
          "speaker_id": "1",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000719.1.21",
          "form": "도토리 그것도 부활하면 좋겠다",
          "original_form": "도토리 그것도 부활하면 좋겠다",
          "speaker_id": "1",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100000719.1.22",
          "form": "ㅋㅋㅋㅋ도토리로 삐지사고 틀어놓고요",
          "original_form": "ㅋㅋㅋㅋ도토리로 삐지사고 틀어놓고요",
          "speaker_id": "2",
          "time": "20210819 20:02"
        },
        {
          "id": "MDRW2100000719.1.23",
          "form": "요즘 그런감성이 또 없으니까 추억이긴해요",
          "original_form": "요즘 그런감성이 또 없으니까 추억이긴해요",
          "speaker_id": "2",
          "time": "20210819 20:02"
        },
        {
          "id": "MDRW2100000719.1.24",
          "form": "맞아요 도토리 충전하는재미에",
          "original_form": "맞아요 도토리 충전하는재미에",
          "speaker_id": "1",
          "time": "20210819 20:02"
        },
        {
          "id": "MDRW2100000719.1.25",
          "form": "친구들이랑 같이 도토리 주고받고",
          "original_form": "친구들이랑 같이 도토리 주고받고",
          "speaker_id": "1",
          "time": "20210819 20:02"
        },
        {
          "id": "MDRW2100000719.1.26",
          "form": "싸이월드만큼 추억은 없죠",
          "original_form": "싸이월드만큼 추억은 없죠",
          "speaker_id": "1",
          "time": "20210819 20:03"
        },
        {
          "id": "MDRW2100000719.1.27",
          "form": "맞아요 그때 정말 일상이었어서 그시절하면 빼놓을수없어요",
          "original_form": "맞아요 그때 정말 일상이었어서 그시절하면 빼놓을수없어요",
          "speaker_id": "2",
          "time": "20210819 20:17"
        }
      ]
