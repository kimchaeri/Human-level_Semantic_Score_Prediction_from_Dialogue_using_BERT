      "utterance": [
        {
          "id": "MDRW2100035885.1.1",
          "form": "뷔페에서 먹는 샐러드가",
          "original_form": "뷔페에서 먹는 샐러드가",
          "speaker_id": "1",
          "time": "20210830 10:26"
        },
        {
          "id": "MDRW2100035885.1.2",
          "form": "얼마나 맛있는데....ㅋㅋㅋㅋ",
          "original_form": "얼마나 맛있는데....ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:26"
        },
        {
          "id": "MDRW2100035885.1.3",
          "form": "근데 뷔페 다 고르고 먹다보면 최애메뉴 몇번 못먹는게 아깝잖아요 아 그러니까요!ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 뷔페 다 고르고 먹다보면 최애메뉴 몇번 못먹는게 아깝잖아요 아 그러니까요!ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 10:26"
        },
        {
          "id": "MDRW2100035885.1.4",
          "form": "저 뷔페 김밥...상당히...좋아합니다... 있는곳 거의 못보긴하지만...",
          "original_form": "저 뷔페 김밥...상당히...좋아합니다... 있는곳 거의 못보긴하지만...",
          "speaker_id": "2",
          "time": "20210830 10:26"
        },
        {
          "id": "MDRW2100035885.1.5",
          "form": "아 맞아요ㅋㅋㅋ 오 이거 맛있네",
          "original_form": "아 맞아요ㅋㅋㅋ 오 이거 맛있네",
          "speaker_id": "1",
          "time": "20210830 10:26"
        },
        {
          "id": "MDRW2100035885.1.6",
          "form": "이거 더 먹어야겠다 하는데",
          "original_form": "이거 더 먹어야겠다 하는데",
          "speaker_id": "1",
          "time": "20210830 10:26"
        },
        {
          "id": "MDRW2100035885.1.7",
          "form": "배불러서 못먹고ㅋㅋㅋ",
          "original_form": "배불러서 못먹고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:26"
        },
        {
          "id": "MDRW2100035885.1.8",
          "form": "김밥 뷔페 김밥 넘 맛있어요ㅋㅋㅋ",
          "original_form": "김밥 뷔페 김밥 넘 맛있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:27"
        },
        {
          "id": "MDRW2100035885.1.9",
          "form": "예전에는 애슐리도 자주갔었는데 요즘은 통 안보이는것같아용!",
          "original_form": "예전에는 애슐리도 자주갔었는데 요즘은 통 안보이는것같아용!",
          "speaker_id": "1",
          "time": "20210830 10:27"
        },
        {
          "id": "MDRW2100035885.1.10",
          "form": "아 맞아요!ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 김밥..진짜 맛있잖아요... 헐 맞아요ㅠㅠㅠㅠ",
          "original_form": "아 맞아요!ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 김밥..진짜 맛있잖아요... 헐 맞아요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 10:27"
        },
        {
          "id": "MDRW2100035885.1.11",
          "form": "코로나이후에 뷔페를 못가서 아쉽네요",
          "original_form": "코로나이후에 뷔페를 못가서 아쉽네요",
          "speaker_id": "2",
          "time": "20210830 10:27"
        },
        {
          "id": "MDRW2100035885.1.12",
          "form": "저 자연별곡 엄청 좋아했는데...",
          "original_form": "저 자연별곡 엄청 좋아했는데...",
          "speaker_id": "2",
          "time": "20210830 10:27"
        },
        {
          "id": "MDRW2100035885.1.13",
          "form": "여기 시즌 바뀔때마다 일면에 서너번씩 갔었는데",
          "original_form": "여기 시즌 바뀔때마다 일면에 서너번씩 갔었는데",
          "speaker_id": "1",
          "time": "20210830 10:27"
        },
        {
          "id": "MDRW2100035885.1.14",
          "form": "제 생활권에선 하나둘씩 사라지더니",
          "original_form": "제 생활권에선 하나둘씩 사라지더니",
          "speaker_id": "1",
          "time": "20210830 10:27"
        },
        {
          "id": "MDRW2100035885.1.15",
          "form": "이젠 아예 없어용ㅠㅠ",
          "original_form": "이젠 아예 없어용ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 10:27"
        },
        {
          "id": "MDRW2100035885.1.16",
          "form": "이랜드에서 만든 종합 뷔페 가보셨어용??",
          "original_form": "이랜드에서 만든 종합 뷔페 가보셨어용??",
          "speaker_id": "1",
          "time": "20210830 10:28"
        },
        {
          "id": "MDRW2100035885.1.17",
          "form": "아 맞아요! 제가 사는 곳 근처도 없어졌어요ㅠㅠㅠ 헐 ...",
          "original_form": "아 맞아요! 제가 사는 곳 근처도 없어졌어요ㅠㅠㅠ 헐 ...",
          "speaker_id": "2",
          "time": "20210830 10:28"
        },
        {
          "id": "MDRW2100035885.1.18",
          "form": "그런곳이...있나요?",
          "original_form": "그런곳이...있나요?",
          "speaker_id": "2",
          "time": "20210830 10:28"
        },
        {
          "id": "MDRW2100035885.1.19",
          "form": "자연별곡이랑 애슐리랑 피자몰이랑",
          "original_form": "자연별곡이랑 애슐리랑 피자몰이랑",
          "speaker_id": "1",
          "time": "20210830 10:29"
        },
        {
          "id": "MDRW2100035885.1.20",
          "form": "여기있는 메뉴들을 한데 보아서",
          "original_form": "여기있는 메뉴들을 한데 보아서",
          "speaker_id": "1",
          "time": "20210830 10:29"
        },
        {
          "id": "MDRW2100035885.1.21",
          "form": "또 뷔페로 만든곳인데",
          "original_form": "또 뷔페로 만든곳인데",
          "speaker_id": "1",
          "time": "20210830 10:29"
        },
        {
          "id": "MDRW2100035885.1.22",
          "form": "페어링6라고 압구정에 있었는데 작년에 코로나때문인지 폐업한것같더라구요ㅠㅠㅠㅠ",
          "original_form": "페어링6라고 압구정에 있었는데 작년에 코로나때문인지 폐업한것같더라구요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 10:29"
        },
        {
          "id": "MDRW2100035885.1.23",
          "form": "와...말도 안돼...",
          "original_form": "와...말도 안돼...",
          "speaker_id": "2",
          "time": "20210830 10:29"
        },
        {
          "id": "MDRW2100035885.1.24",
          "form": "말도...안돼...폐업했다고요...?",
          "original_form": "말도...안돼...폐업했다고요...?",
          "speaker_id": "2",
          "time": "20210830 10:29"
        },
        {
          "id": "MDRW2100035885.1.25",
          "form": "저지금 검색하면서 침 흘렸는데...?",
          "original_form": "저지금 검색하면서 침 흘렸는데...?",
          "speaker_id": "2",
          "time": "20210830 10:29"
        },
        {
          "id": "MDRW2100035885.1.26",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 10:29"
        },
        {
          "id": "MDRW2100035885.1.27",
          "form": "아 로운 샤브샤브도 있어서",
          "original_form": "아 로운 샤브샤브도 있어서",
          "speaker_id": "1",
          "time": "20210830 10:30"
        },
        {
          "id": "MDRW2100035885.1.28",
          "form": "피자몰 진짜 맛있는데...진짜로.....",
          "original_form": "피자몰 진짜 맛있는데...진짜로.....",
          "speaker_id": "2",
          "time": "20210830 10:30"
        },
        {
          "id": "MDRW2100035885.1.29",
          "form": "샤브샤브도 무한리필이었어여",
          "original_form": "샤브샤브도 무한리필이었어여",
          "speaker_id": "1",
          "time": "20210830 10:30"
        },
        {
          "id": "MDRW2100035885.1.30",
          "form": "넨넹ㅠㅠ 종류별로 진짜 다양하게 있어서",
          "original_form": "넨넹ㅠㅠ 종류별로 진짜 다양하게 있어서",
          "speaker_id": "1",
          "time": "20210830 10:30"
        },
        {
          "id": "MDRW2100035885.1.31",
          "form": "와 ..",
          "original_form": "와 ..",
          "speaker_id": "2",
          "time": "20210830 10:30"
        },
        {
          "id": "MDRW2100035885.1.32",
          "form": "좀 비싸도 종종 갔었거든요ㅠㅠ",
          "original_form": "좀 비싸도 종종 갔었거든요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 10:30"
        },
        {
          "id": "MDRW2100035885.1.33",
          "form": "여기 빠네스프도 있네요? ㅠㅠㅠㅠㅠㅠㅠ 아놔진짜맛있겠어요",
          "original_form": "여기 빠네스프도 있네요? ㅠㅠㅠㅠㅠㅠㅠ 아놔진짜맛있겠어요",
          "speaker_id": "2",
          "time": "20210830 10:30"
        }
      ]
    }
