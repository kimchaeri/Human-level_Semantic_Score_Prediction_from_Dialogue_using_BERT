      "utterance": [
        {
          "id": "MDRW2100000502.1.1",
          "form": "우리 근데 경주가?",
          "original_form": "우리 근데 경주가?",
          "speaker_id": "1",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000502.1.2",
          "form": "우리 경주가?ㅋㅋㅋ",
          "original_form": "우리 경주가?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000502.1.3",
          "form": "접때 말했었잖아ㅋㅋㅋ 근데 우리끼리 돈모으는족족써서 가능할지는 모르겠다ㅋㅋ",
          "original_form": "접때 말했었잖아ㅋㅋㅋ 근데 우리끼리 돈모으는족족써서 가능할지는 모르겠다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000502.1.4",
          "form": "아니 코로나땜에 안가는걸로 된줄알았지ㅋㅋ",
          "original_form": "아니 코로나땜에 안가는걸로 된줄알았지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:39"
        },
        {
          "id": "MDRW2100000502.1.5",
          "form": "마자 진짜 돈 따로 모아야될거같아ㅋㅋ\r\n맨날 방탈출하느라 다 써서ㅋㅋ",
          "original_form": "마자 진짜 돈 따로 모아야될거같아ㅋㅋ\r\n맨날 방탈출하느라 다 써서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:39"
        },
        {
          "id": "MDRW2100000502.1.6",
          "form": "일단은 이번에 방탈출놀고! 백신 맞은담에 생각하자ㅋㅋㅋ",
          "original_form": "일단은 이번에 방탈출놀고! 백신 맞은담에 생각하자ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000502.1.7",
          "form": "그래ㅋㅋ백신맞으면 그래도 좀 더 안전하게 여행갈수있겠지ㅠ",
          "original_form": "그래ㅋㅋ백신맞으면 그래도 좀 더 안전하게 여행갈수있겠지ㅠ",
          "speaker_id": "2",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000502.1.8",
          "form": "맞아 당장 해외는 무리더라고 국내 좀 돌아다니고 싶어~",
          "original_form": "맞아 당장 해외는 무리더라고 국내 좀 돌아다니고 싶어~",
          "speaker_id": "1",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000502.1.9",
          "form": "마자ㅠ",
          "original_form": "마자ㅠ",
          "speaker_id": "2",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000502.1.10",
          "form": "패러글라이딩도 또 하고싶다",
          "original_form": "패러글라이딩도 또 하고싶다",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000502.1.11",
          "form": "짚",
          "original_form": "짚",
          "speaker_id": "1",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000502.1.12",
          "form": "짚 라인",
          "original_form": "짚 라인",
          "speaker_id": "1",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000502.1.13",
          "form": "이게 왜 나쁜말이여..",
          "original_form": "이게 왜 나쁜말이여..",
          "speaker_id": "1",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000502.1.14",
          "form": "너 짚 라인 탔었지?",
          "original_form": "너 짚 라인 탔었지?",
          "speaker_id": "1",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000502.1.15",
          "form": "ㅋㅋㅋㅋ나 짚라인 탔었지",
          "original_form": "ㅋㅋㅋㅋ나 짚라인 탔었지",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000502.1.16",
          "form": "근데 난 짚라인이 더 무서워",
          "original_form": "근데 난 짚라인이 더 무서워",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000502.1.17",
          "form": "아진짜? 신기하네ㅋㅋ 높이로 따지면 패러가 더 높자넝",
          "original_form": "아진짜? 신기하네ㅋㅋ 높이로 따지면 패러가 더 높자넝",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000502.1.18",
          "form": "그치 근데 짚라인은 도착할때 어디 부딪힐거같아서 무서워",
          "original_form": "그치 근데 짚라인은 도착할때 어디 부딪힐거같아서 무서워",
          "speaker_id": "2",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000502.1.19",
          "form": "아그래? 그걸로 무서워할줄은 생각도 못했당\r\n엄청 흔들려서?",
          "original_form": "아그래? 그걸로 무서워할줄은 생각도 못했당\r\n엄청 흔들려서?",
          "speaker_id": "1",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000502.1.20",
          "form": "아니 다와가는데 속도가 안줄어드니까",
          "original_form": "아니 다와가는데 속도가 안줄어드니까",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000502.1.21",
          "form": "부딪힐까봐ㅋㅋ",
          "original_form": "부딪힐까봐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000502.1.22",
          "form": "ㅋㅋㅋ 어차피 위에 막아주는거 있자너! 못믿는거니?ㅋㅋ",
          "original_form": "ㅋㅋㅋ 어차피 위에 막아주는거 있자너! 못믿는거니?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000502.1.23",
          "form": "마자 못믿어서 그런거같아ㅋㅋ",
          "original_form": "마자 못믿어서 그런거같아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000502.1.24",
          "form": "ㅋㅋㅋ 패러그라이딩은 믿고? 사람이라서?ㅋㅋ",
          "original_form": "ㅋㅋㅋ 패러그라이딩은 믿고? 사람이라서?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000502.1.25",
          "form": "그러네ㅋㅋㅋ",
          "original_form": "그러네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000502.1.26",
          "form": "몰라 그냥 뒤에 사람있으니까 안정적이야\r\n안심돼ㅋㅋ",
          "original_form": "몰라 그냥 뒤에 사람있으니까 안정적이야\r\n안심돼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000502.1.27",
          "form": "ㅋㅋㅋ 난 그때 자세가 너무 불편했어서 힘들었던 기억이야",
          "original_form": "ㅋㅋㅋ 난 그때 자세가 너무 불편했어서 힘들었던 기억이야",
          "speaker_id": "1",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000502.1.28",
          "form": "아그래?  쑥 앉으면 포근하지않니?ㅋㅋ",
          "original_form": "아그래?  쑥 앉으면 포근하지않니?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:06"
        },
        {
          "id": "MDRW2100000502.1.29",
          "form": "너가 제대로 못앉은거아냐?",
          "original_form": "너가 제대로 못앉은거아냐?",
          "speaker_id": "2",
          "time": "20210819 18:08"
        },
        {
          "id": "MDRW2100000502.1.30",
          "form": "그치 내가 제대로 못앉은거같아ㅠㅠㅋㅋ",
          "original_form": "그치 내가 제대로 못앉은거같아ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:09"
        },
        {
          "id": "MDRW2100000502.1.31",
          "form": "허리아파 죽는줄..",
          "original_form": "허리아파 죽는줄..",
          "speaker_id": "1",
          "time": "20210819 18:09"
        },
        {
          "id": "MDRW2100000502.1.32",
          "form": "내가 처음에 그렇게 앉았었는데 뒤에 분이 다시 앉혀주더라",
          "original_form": "내가 처음에 그렇게 앉았었는데 뒤에 분이 다시 앉혀주더라",
          "speaker_id": "2",
          "time": "20210819 18:11"
        },
        {
          "id": "MDRW2100000502.1.33",
          "form": "나 처음에는 이렇게 계속 서서가는건가 했어ㅋㅋ",
          "original_form": "나 처음에는 이렇게 계속 서서가는건가 했어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:11"
        },
        {
          "id": "MDRW2100000502.1.34",
          "form": "난 왜 안해주셨지..흑흑",
          "original_form": "난 왜 안해주셨지..흑흑",
          "speaker_id": "1",
          "time": "20210819 18:11"
        },
        {
          "id": "MDRW2100000502.1.35",
          "form": "허리",
          "original_form": "허리",
          "speaker_id": "1",
          "time": "20210819 18:11"
        },
        {
          "id": "MDRW2100000502.1.36",
          "form": "너 그럼 그렇게 탔을때 안무서웠어?",
          "original_form": "너 그럼 그렇게 탔을때 안무서웠어?",
          "speaker_id": "2",
          "time": "20210819 18:12"
        },
        {
          "id": "MDRW2100000502.1.37",
          "form": "나는 처음에 바다에 떨어질거같아가지구",
          "original_form": "나는 처음에 바다에 떨어질거같아가지구",
          "speaker_id": "2",
          "time": "20210819 18:12"
        },
        {
          "id": "MDRW2100000502.1.38",
          "form": "너무 무섭던데",
          "original_form": "너무 무섭던데",
          "speaker_id": "2",
          "time": "20210819 18:12"
        },
        {
          "id": "MDRW2100000502.1.39",
          "form": "무섭다기보단 너무 힘들었어 허리가 꺾인 상태라",
          "original_form": "무섭다기보단 너무 힘들었어 허리가 꺾인 상태라",
          "speaker_id": "1",
          "time": "20210819 18:13"
        },
        {
          "id": "MDRW2100000502.1.40",
          "form": "그래서 별로 기억이 안좋게 남았구나ㅠ",
          "original_form": "그래서 별로 기억이 안좋게 남았구나ㅠ",
          "speaker_id": "2",
          "time": "20210819 18:16"
        },
        {
          "id": "MDRW2100000502.1.41",
          "form": "나도 제대로 안앉혀줬으면 다신 안탔을듯ㅋㅋ",
          "original_form": "나도 제대로 안앉혀줬으면 다신 안탔을듯ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:17"
        }
      ]
