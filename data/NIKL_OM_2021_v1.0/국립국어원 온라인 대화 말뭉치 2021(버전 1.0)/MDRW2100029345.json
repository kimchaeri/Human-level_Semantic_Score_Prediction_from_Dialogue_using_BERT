      "utterance": [
        {
          "id": "MDRW2100029345.1.1",
          "form": "구직중이세요??",
          "original_form": "구직중이세요??",
          "speaker_id": "1",
          "time": "20210903 02:04"
        },
        {
          "id": "MDRW2100029345.1.2",
          "form": "어이쿠야.. 아뇨..ㅎ",
          "original_form": "어이쿠야.. 아뇨..ㅎ",
          "speaker_id": "2",
          "time": "20210903 02:05"
        },
        {
          "id": "MDRW2100029345.1.3",
          "form": "왜용??",
          "original_form": "왜용??",
          "speaker_id": "1",
          "time": "20210903 02:05"
        },
        {
          "id": "MDRW2100029345.1.4",
          "form": "개인적인 사정때문에 마음을 다잡는데 오래걸렸어요..",
          "original_form": "개인적인 사정때문에 마음을 다잡는데 오래걸렸어요..",
          "speaker_id": "2",
          "time": "20210903 02:06"
        },
        {
          "id": "MDRW2100029345.1.5",
          "form": "이제 고민해보려구요.. 구차하지만",
          "original_form": "이제 고민해보려구요.. 구차하지만",
          "speaker_id": "2",
          "time": "20210903 02:06"
        },
        {
          "id": "MDRW2100029345.1.6",
          "form": "어떤사정이요?",
          "original_form": "어떤사정이요?",
          "speaker_id": "1",
          "time": "20210903 02:07"
        },
        {
          "id": "MDRW2100029345.1.7",
          "form": "그것까진 말씀드리기가 힘들어요..ㅎ",
          "original_form": "그것까진 말씀드리기가 힘들어요..ㅎ",
          "speaker_id": "2",
          "time": "20210903 02:07"
        },
        {
          "id": "MDRW2100029345.1.8",
          "form": "그래도 굳게 다짐을 했습니다",
          "original_form": "그래도 굳게 다짐을 했습니다",
          "speaker_id": "2",
          "time": "20210903 02:08"
        },
        {
          "id": "MDRW2100029345.1.9",
          "form": "왜용..익명인뎅",
          "original_form": "왜용..익명인뎅",
          "speaker_id": "1",
          "time": "20210903 02:08"
        },
        {
          "id": "MDRW2100029345.1.10",
          "form": "내년을 목표로 취직하려해요",
          "original_form": "내년을 목표로 취직하려해요",
          "speaker_id": "2",
          "time": "20210903 02:08"
        },
        {
          "id": "MDRW2100029345.1.11",
          "form": "알지만 꺼내는 것조차 힘드네요..",
          "original_form": "알지만 꺼내는 것조차 힘드네요..",
          "speaker_id": "2",
          "time": "20210903 02:09"
        },
        {
          "id": "MDRW2100029345.1.12",
          "form": "흠ㅜㅡㅜ ...뭐지 심적으로힘드셧나요",
          "original_form": "흠ㅜㅡㅜ ...뭐지 심적으로힘드셧나요",
          "speaker_id": "1",
          "time": "20210903 02:10"
        },
        {
          "id": "MDRW2100029345.1.13",
          "form": "네..ㅎ 걱정해주셔서 감사합니다",
          "original_form": "네..ㅎ 걱정해주셔서 감사합니다",
          "speaker_id": "2",
          "time": "20210903 02:10"
        },
        {
          "id": "MDRW2100029345.1.14",
          "form": "하지만 계속 주저앉아있을 수는 없으니까 일어나려구요",
          "original_form": "하지만 계속 주저앉아있을 수는 없으니까 일어나려구요",
          "speaker_id": "2",
          "time": "20210903 02:11"
        },
        {
          "id": "MDRW2100029345.1.15",
          "form": "혹시 실연의 슬픔으로?",
          "original_form": "혹시 실연의 슬픔으로?",
          "speaker_id": "1",
          "time": "20210903 02:11"
        },
        {
          "id": "MDRW2100029345.1.16",
          "form": "고백도 못해봤는데 실연할틈이 있나요ㅎㅎ",
          "original_form": "고백도 못해봤는데 실연할틈이 있나요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 02:12"
        },
        {
          "id": "MDRW2100029345.1.17",
          "form": "아항... ㅜ",
          "original_form": "아항... ㅜ",
          "speaker_id": "1",
          "time": "20210903 02:13"
        },
        {
          "id": "MDRW2100029345.1.18",
          "form": "취업...성공하세요..",
          "original_form": "취업...성공하세요..",
