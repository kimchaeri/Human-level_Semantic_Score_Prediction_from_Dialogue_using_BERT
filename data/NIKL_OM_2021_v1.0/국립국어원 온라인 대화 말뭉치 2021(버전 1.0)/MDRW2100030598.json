      "utterance": [
        {
          "id": "MDRW2100030598.1.1",
          "form": "무슨일하시나요",
          "original_form": "무슨일하시나요",
          "speaker_id": "1",
          "time": "20210904 09:59"
        },
        {
          "id": "MDRW2100030598.1.2",
          "form": "평범한 사무직입니다",
          "original_form": "평범한 사무직입니다",
          "speaker_id": "2",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030598.1.3",
          "form": "무슨 일 하세요?",
          "original_form": "무슨 일 하세요?",
          "speaker_id": "2",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030598.1.4",
          "form": "온라인 쇼핑몰에서 일해요",
          "original_form": "온라인 쇼핑몰에서 일해요",
          "speaker_id": "1",
          "time": "20210904 11:09"
        },
        {
          "id": "MDRW2100030598.1.5",
          "form": "엠디!",
          "original_form": "엠디!",
          "speaker_id": "1",
          "time": "20210904 11:09"
        },
        {
          "id": "MDRW2100030598.1.6",
          "form": "와 패션트렌드에 빠삭하시겟네요",
          "original_form": "와 패션트렌드에 빠삭하시겟네요",
          "speaker_id": "2",
          "time": "20210904 11:39"
        },
        {
          "id": "MDRW2100030598.1.7",
          "form": "핸드폰 악세사리 쪽이라..ㅜㅜ",
          "original_form": "핸드폰 악세사리 쪽이라..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 11:40"
        },
        {
          "id": "MDRW2100030598.1.8",
          "form": "원래 짜장면집 사람은 짜장면 안먹는다잟아요",
          "original_form": "원래 짜장면집 사람은 짜장면 안먹는다잟아요",
          "speaker_id": "1",
          "time": "20210904 11:41"
        },
        {
          "id": "MDRW2100030598.1.9",
          "form": "앗 의류쪽인줄 알고 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 의류쪽인줄 알고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100030598.1.10",
          "form": "폰 악세사리 궁금하네요",
          "original_form": "폰 악세사리 궁금하네요",
          "speaker_id": "2",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100030598.1.11",
          "form": "폰케이스 보호필름 이런거 취급해여",
          "original_form": "폰케이스 보호필름 이런거 취급해여",
          "speaker_id": "1",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100030598.1.12",
          "form": "근데 코로나땜시 아휴",
          "original_form": "근데 코로나땜시 아휴",
          "speaker_id": "1",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100030598.1.13",
          "form": "코로나가 정말 많은사람들을 힘들게 하죠 ㅠㅠ",
          "original_form": "코로나가 정말 많은사람들을 힘들게 하죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030598.1.14",
          "form": "직장은 타격없으신가요",
          "original_form": "직장은 타격없으신가요",
          "speaker_id": "1",
          "time": "20210904 12:50"
        },
        {
          "id": "MDRW2100030598.1.15",
          "form": "많은게변햇죠",
          "original_form": "많은게변햇죠",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100030598.1.16",
          "form": "얼른 코로나가 종식되엇으면 좋겟어요 정말",
          "original_form": "얼른 코로나가 종식되엇으면 좋겟어요 정말",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100030598.1.17",
          "form": "이거 안끝나요",
          "original_form": "이거 안끝나요",
          "speaker_id": "1",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100030598.1.18",
          "form": "평생 같이갈거에요",
          "original_form": "평생 같이갈거에요",
          "speaker_id": "1",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100030598.1.19",
          "form": "이런 방식으로 계속 가지는 않을 것 같은데 ㅠㅠ 예전의 스페인독감처럼 몇년에 걸쳐서 전염력은 강해지고 치명률은 낮아지는 형태로 흘러가지 않을까 라고 생각하고 잇어요 저는",
          "original_form": "이런 방식으로 계속 가지는 않을 것 같은데 ㅠㅠ 예전의 스페인독감처럼 몇년에 걸쳐서 전염력은 강해지고 치명률은 낮아지는 형태로 흘러가지 않을까 라고 생각하고 잇어요 저는",
          "speaker_id": "2",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100030598.1.20",
          "form": "네 지금 얘기나온 델타변이가 그런꼴이에요",
          "original_form": "네 지금 얘기나온 델타변이가 그런꼴이에요",
          "speaker_id": "1",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100030598.1.21",
          "form": "그래서 영국이 위드코로나 실시한걸로 알아요ㅋ",
          "original_form": "그래서 영국이 위드코로나 실시한걸로 알아요ㅋ",
          "speaker_id": "1",
          "time": "20210904 16:31"
        }
      ]
