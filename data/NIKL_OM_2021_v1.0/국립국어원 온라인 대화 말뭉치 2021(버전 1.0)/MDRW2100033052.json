      "utterance": [
        {
          "id": "MDRW2100033052.1.1",
          "form": "그런주제에 국무총리될려고했나요",
          "original_form": "그런주제에 국무총리될려고했나요",
          "speaker_id": "1",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100033052.1.2",
          "form": "양심이없네",
          "original_form": "양심이없네",
          "speaker_id": "1",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100033052.1.3",
          "form": "그니까요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100033052.1.4",
          "form": "솔직히 정유라 때보다 훨씬 더 심하죠",
          "original_form": "솔직히 정유라 때보다 훨씬 더 심하죠",
          "speaker_id": "2",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100033052.1.5",
          "form": "아 국제영화제,,",
          "original_form": "아 국제영화제,,",
          "speaker_id": "2",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100033052.1.6",
          "form": "정유란 인정이라도했지",
          "original_form": "정유란 인정이라도했지",
          "speaker_id": "1",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100033052.1.7",
          "form": "그러니까요 ㅋㅋㅋㅋ",
          "original_form": "그러니까요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100033052.1.8",
          "form": "어 지금열리는중이네요",
          "original_form": "어 지금열리는중이네요",
          "speaker_id": "1",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100033052.1.9",
          "form": "우와 코로나인데",
          "original_form": "우와 코로나인데",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033052.1.10",
          "form": "정상적으로 개막하려나...",
          "original_form": "정상적으로 개막하려나...",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033052.1.11",
          "form": "김진아감독 진출했다는데",
          "original_form": "김진아감독 진출했다는데",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033052.1.12",
          "form": "배우들 보면 마스크도 잘 안쓰던데",
          "original_form": "배우들 보면 마스크도 잘 안쓰던데",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033052.1.13",
          "form": "오오?!",
          "original_form": "오오?!",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033052.1.14",
          "form": "첨듣네요",
          "original_form": "첨듣네요",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033052.1.15",
          "form": "이름 들어봤어요!!",
          "original_form": "이름 들어봤어요!!",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033052.1.16",
          "form": "엥 진짜요?",
          "original_form": "엥 진짜요?",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033052.1.17",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033052.1.18",
          "form": "소요산 이라는데",
          "original_form": "소요산 이라는데",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033052.1.19",
          "form": "작품들어보셨어요?",
          "original_form": "작품들어보셨어요?",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033052.1.20",
          "form": "앗 아니네요",
          "original_form": "앗 아니네요",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033052.1.21",
          "form": "진아라는 이름이 워낙 많아서",
          "original_form": "진아라는 이름이 워낙 많아서",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033052.1.22",
          "form": "헷갈렸어요 ㅠㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "헷갈렸어요 ㅠㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033052.1.23",
          "form": "그나저나 정말",
          "original_form": "그나저나 정말",
          "speaker_id": "2",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100033052.1.24",
          "form": "뛰어난 인재시네요",
          "original_form": "뛰어난 인재시네요",
          "speaker_id": "2",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100033052.1.25",
          "form": "글고 크리스틴 스튜어트나온다네요",
          "original_form": "글고 크리스틴 스튜어트나온다네요",
          "speaker_id": "1",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100033052.1.26",
          "form": "봉준호도갔나봐요",
          "original_form": "봉준호도갔나봐요",
          "speaker_id": "1",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100033052.1.27",
          "form": "슈스네요",
          "original_form": "슈스네요",
          "speaker_id": "1",
          "time": "20210906 14:46"
        }
      ]
