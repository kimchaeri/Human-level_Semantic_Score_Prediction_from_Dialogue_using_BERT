      "utterance": [
        {
          "id": "MDRW2100005581.1.1",
          "form": "안녕하세요 좋아하는 책 장르 있으신가요?",
          "original_form": "안녕하세요 좋아하는 책 장르 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005581.1.2",
          "form": "장르는 딱히 가리지않는 편이에요",
          "original_form": "장르는 딱히 가리지않는 편이에요",
          "speaker_id": "2",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005581.1.3",
          "form": "책을 자주 읽으시나요",
          "original_form": "책을 자주 읽으시나요",
          "speaker_id": "2",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005581.1.4",
          "form": "저는 사실 자주는 못읽어요ㅋㅋ",
          "original_form": "저는 사실 자주는 못읽어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005581.1.5",
          "form": "예전엔 도서관 자주갔었는데 요즘은 못가고있네요",
          "original_form": "예전엔 도서관 자주갔었는데 요즘은 못가고있네요",
          "speaker_id": "1",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005581.1.6",
          "form": "아 코로나 때문에쇼?",
          "original_form": "아 코로나 때문에쇼?",
          "speaker_id": "2",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005581.1.7",
          "form": "넵ㅜㅜ 게다가 가까운 도서관이 공사를 해서요",
          "original_form": "넵ㅜㅜ 게다가 가까운 도서관이 공사를 해서요",
          "speaker_id": "1",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005581.1.8",
          "form": "너무 아쉽겠어요ㅠㅠ",
          "original_form": "너무 아쉽겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005581.1.9",
          "form": "웹툰이나 웹소설은 보시나요?",
          "original_form": "웹툰이나 웹소설은 보시나요?",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005581.1.10",
          "form": "넵!! 웹소설은 안보는데 웹툰은 많이 보는거같아요",
          "original_form": "넵!! 웹소설은 안보는데 웹툰은 많이 보는거같아요",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005581.1.11",
          "form": "근데 요즘은 또 볼게 없더라구요ㅋㅋ",
          "original_form": "근데 요즘은 또 볼게 없더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005581.1.12",
          "form": "재밌는거 있나요?",
          "original_form": "재밌는거 있나요?",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005581.1.13",
          "form": "저는 네이버쪽 자주봐요",
          "original_form": "저는 네이버쪽 자주봐요",
          "speaker_id": "2",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005581.1.14",
          "form": "어떤 장르를 좋아하시나요",
          "original_form": "어떤 장르를 좋아하시나요",
          "speaker_id": "2",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005581.1.15",
          "form": "저는 주로 일상이나 가볍게 읽을수있는거 좋아하는거같아요",
          "original_form": "저는 주로 일상이나 가볍게 읽을수있는거 좋아하는거같아요",
          "speaker_id": "1",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005581.1.16",
          "form": "약간 묘죠일지나 대학일기 이런 종류요?",
          "original_form": "약간 묘죠일지나 대학일기 이런 종류요?",
          "speaker_id": "2",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005581.1.17",
          "form": "아 네네ㅋㅋ 모죠 너무 좋아해요",
          "original_form": "아 네네ㅋㅋ 모죠 너무 좋아해요",
          "speaker_id": "1",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005581.1.18",
          "form": "예전에 선천적얼간이도 재밌게 봣어요",
          "original_form": "예전에 선천적얼간이도 재밌게 봣어요",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005581.1.19",
          "form": "가스파드!",
          "original_form": "가스파드!",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005581.1.20",
          "form": "저도 그거 재밌게 봤어요",
          "original_form": "저도 그거 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005581.1.21",
          "form": "맞아요!!ㅋㅋ 전자오락수호대도 재밌긴했는데",
          "original_form": "맞아요!!ㅋㅋ 전자오락수호대도 재밌긴했는데",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005581.1.22",
          "form": "전 선천적얼간이가 더 재밋더라구요ㅋㅋ",
          "original_form": "전 선천적얼간이가 더 재밋더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:15"
        }
      ]
