      "utterance": [
        {
          "id": "MDRW2100021439.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021439.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100021439.1.3",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100021439.1.4",
          "form": "아직이요! 드셨나요",
          "original_form": "아직이요! 드셨나요",
          "speaker_id": "1",
          "time": "20210828 19:05"
        },
        {
          "id": "MDRW2100021439.1.5",
          "form": "네 복숭아랑 현미식빵에 블루베리잼으로 먹었어요",
          "original_form": "네 복숭아랑 현미식빵에 블루베리잼으로 먹었어요",
          "speaker_id": "2",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100021439.1.6",
          "form": "뭐 드실 예정인가요?",
          "original_form": "뭐 드실 예정인가요?",
          "speaker_id": "2",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100021439.1.7",
          "form": "맛있겠어요 ㅜㅜ",
          "original_form": "맛있겠어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100021439.1.8",
          "form": "저는 간단히 집밥이요",
          "original_form": "저는 간단히 집밥이요",
          "speaker_id": "1",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100021439.1.9",
          "form": "집밥이 건강에 좋죠 전 빵순이라서요ㅎㅎ",
          "original_form": "집밥이 건강에 좋죠 전 빵순이라서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100021439.1.10",
          "form": "헉 저두요!",
          "original_form": "헉 저두요!",
          "speaker_id": "1",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021439.1.11",
          "form": "빵 좋아하세요?",
          "original_form": "빵 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021439.1.12",
          "form": "동지를 만났네요",
          "original_form": "동지를 만났네요",
          "speaker_id": "1",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021439.1.13",
          "form": "뭐좋아하세요?!",
          "original_form": "뭐좋아하세요?!",
          "speaker_id": "1",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021439.1.14",
          "form": "거의 다 좋아해요 특히 케이크 좋아해요",
          "original_form": "거의 다 좋아해요 특히 케이크 좋아해요",
          "speaker_id": "2",
          "time": "20210828 19:14"
        },
        {
          "id": "MDRW2100021439.1.15",
          "form": "무슨 케익좋아하세요??",
          "original_form": "무슨 케익좋아하세요??",
          "speaker_id": "1",
          "time": "20210828 19:14"
        },
        {
          "id": "MDRW2100021439.1.16",
          "form": "생크림 고구마 블루베리 단호박 망고 아 쓰고 보니    달달한 것만 먹네요",
          "original_form": "생크림 고구마 블루베리 단호박 망고 아 쓰고 보니    달달한 것만 먹네요",
          "speaker_id": "2",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021439.1.17",
          "form": "다양하게 드시네요",
          "original_form": "다양하게 드시네요",
          "speaker_id": "1",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021439.1.18",
          "form": "케익이 다 달콤하죠 뭐 ㅋㅋㅋㅋ",
          "original_form": "케익이 다 달콤하죠 뭐 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:17"
