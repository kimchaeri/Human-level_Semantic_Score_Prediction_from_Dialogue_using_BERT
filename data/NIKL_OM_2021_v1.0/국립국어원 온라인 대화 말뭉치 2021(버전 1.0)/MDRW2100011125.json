      "utterance": [
        {
          "id": "MDRW2100011125.1.1",
          "form": "진짜 비가 너무 많이 온다ㅠㅠㅠ",
          "original_form": "진짜 비가 너무 많이 온다ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011125.1.2",
          "form": "이거 태풍이야?",
          "original_form": "이거 태풍이야?",
          "speaker_id": "1",
          "time": "20210824 01:22"
        },
        {
          "id": "MDRW2100011125.1.3",
          "form": "웅웅 오늘 새벽에 부산 지나간대ㅠㅠ",
          "original_form": "웅웅 오늘 새벽에 부산 지나간대ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 01:22"
        },
        {
          "id": "MDRW2100011125.1.4",
          "form": "비소리 들려?",
          "original_form": "비소리 들려?",
          "speaker_id": "2",
          "time": "20210824 01:22"
        },
        {
          "id": "MDRW2100011125.1.5",
          "form": "지금은 조용하네",
          "original_form": "지금은 조용하네",
          "speaker_id": "1",
          "time": "20210824 01:23"
        },
        {
          "id": "MDRW2100011125.1.6",
          "form": "아까는 대박이었는데",
          "original_form": "아까는 대박이었는데",
          "speaker_id": "1",
          "time": "20210824 01:23"
        },
        {
          "id": "MDRW2100011125.1.7",
          "form": "아까 천둥 완전 무서웠어 들었지?",
          "original_form": "아까 천둥 완전 무서웠어 들었지?",
          "speaker_id": "2",
          "time": "20210824 01:23"
        },
        {
          "id": "MDRW2100011125.1.8",
          "form": "응 들었어",
          "original_form": "응 들었어",
          "speaker_id": "1",
          "time": "20210824 01:23"
        },
        {
          "id": "MDRW2100011125.1.9",
          "form": "심장이 바운스바운스",
          "original_form": "심장이 바운스바운스",
          "speaker_id": "1",
          "time": "20210824 01:23"
        },
        {
          "id": "MDRW2100011125.1.10",
          "form": "아까 세수하다 엄청 놀랐어!",
          "original_form": "아까 세수하다 엄청 놀랐어!",
          "speaker_id": "2",
          "time": "20210824 01:24"
        },
        {
          "id": "MDRW2100011125.1.11",
          "form": "내일도 오겠지?",
          "original_form": "내일도 오겠지?",
          "speaker_id": "2",
          "time": "20210824 01:24"
        },
        {
          "id": "MDRW2100011125.1.12",
          "form": "응 올거같아",
          "original_form": "응 올거같아",
          "speaker_id": "1",
          "time": "20210824 01:24"
        },
        {
          "id": "MDRW2100011125.1.13",
          "form": "밖에 나가면 젖겠다",
          "original_form": "밖에 나가면 젖겠다",
          "speaker_id": "1",
          "time": "20210824 01:25"
        },
        {
          "id": "MDRW2100011125.1.14",
          "form": "맞아 내일도 우산 꼭 챙겨가~",
          "original_form": "맞아 내일도 우산 꼭 챙겨가~",
          "speaker_id": "2",
          "time": "20210824 01:25"
        },
        {
          "id": "MDRW2100011125.1.15",
          "form": "아 참 네 우산 돌려줘야하는데",
          "original_form": "아 참 네 우산 돌려줘야하는데",
          "speaker_id": "1",
          "time": "20210824 01:25"
        },
        {
          "id": "MDRW2100011125.1.16",
          "form": "너 내일 뭐해?",
          "original_form": "너 내일 뭐해?",
          "speaker_id": "1",
          "time": "20210824 01:26"
        },
        {
          "id": "MDRW2100011125.1.17",
          "form": "앜ㅋㅋㅋ 나 내일 친구가 옷사는거 도와달래서",
          "original_form": "앜ㅋㅋㅋ 나 내일 친구가 옷사는거 도와달래서",
          "speaker_id": "2",
          "time": "20210824 01:26"
        },
        {
          "id": "MDRW2100011125.1.18",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210824 01:26"
        },
        {
          "id": "MDRW2100011125.1.19",
          "form": "같이 센텀 백화점 가기로 했어~",
          "original_form": "같이 센텀 백화점 가기로 했어~",
          "speaker_id": "2",
          "time": "20210824 01:26"
        },
        {
          "id": "MDRW2100011125.1.20",
          "form": "옷 보러 가는구나",
          "original_form": "옷 보러 가는구나",
          "speaker_id": "1",
          "time": "20210824 01:26"
        }
      ]
