      "utterance": [
        {
          "id": "MDRW2100023860.1.1",
          "form": "ㅋㅋㅋㅋ재미없어요",
          "original_form": "ㅋㅋㅋㅋ재미없어요",
          "speaker_id": "2",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100023860.1.2",
          "form": "구냥 남친이 군대 얘기하면서 뭔가 반짝거리며 말하니깐",
          "original_form": "구냥 남친이 군대 얘기하면서 뭔가 반짝거리며 말하니깐",
          "speaker_id": "2",
          "time": "20210830 19:14"
        },
        {
          "id": "MDRW2100023860.1.3",
          "form": "그게 귀여워서 들어주는거지...",
          "original_form": "그게 귀여워서 들어주는거지...",
          "speaker_id": "2",
          "time": "20210830 19:14"
        },
        {
          "id": "MDRW2100023860.1.4",
          "form": "그죠 저도 연애할때나 들어봤네요",
          "original_form": "그죠 저도 연애할때나 들어봤네요",
          "speaker_id": "1",
          "time": "20210830 19:15"
        },
        {
          "id": "MDRW2100023860.1.5",
          "form": "ㅋㅋㅋㅋ결혼후엔 잘 안하나요",
          "original_form": "ㅋㅋㅋㅋ결혼후엔 잘 안하나요",
          "speaker_id": "2",
          "time": "20210830 19:16"
        },
        {
          "id": "MDRW2100023860.1.6",
          "form": "네 안물..안궁..^^..ㅜㅜㅋㅋ",
          "original_form": "네 안물..안궁..^^..ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:16"
        },
        {
          "id": "MDRW2100023860.1.7",
          "form": "ㅋㅋㅋㅋㅋ이제 안 말할때도 됐죠ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ이제 안 말할때도 됐죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:16"
        },
        {
          "id": "MDRW2100023860.1.8",
          "form": "민방위나 괴로워하고",
          "original_form": "민방위나 괴로워하고",
          "speaker_id": "2",
          "time": "20210830 19:16"
        },
        {
          "id": "MDRW2100023860.1.9",
          "form": "남친분은 일반 육군? 이었나요?",
          "original_form": "남친분은 일반 육군? 이었나요?",
          "speaker_id": "1",
          "time": "20210830 19:25"
        },
        {
          "id": "MDRW2100023860.1.10",
          "form": "의경이었대요",
          "original_form": "의경이었대요",
          "speaker_id": "2",
          "time": "20210830 19:27"
        },
        {
          "id": "MDRW2100023860.1.11",
          "form": "오 그랬군요 남편은 의무소방이요~! 뭔가 비슷한가요",
          "original_form": "오 그랬군요 남편은 의무소방이요~! 뭔가 비슷한가요",
          "speaker_id": "1",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100023860.1.12",
          "form": "오오 소방도 있군여!! 첨알았어요",
          "original_form": "오오 소방도 있군여!! 첨알았어요",
          "speaker_id": "2",
          "time": "20210830 19:31"
        },
        {
          "id": "MDRW2100023860.1.13",
          "form": "네 저도 남편만나고 처음 알았네요 ㅋ",
          "original_form": "네 저도 남편만나고 처음 알았네요 ㅋ",
          "speaker_id": "1",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100023860.1.14",
          "form": "고생많으셨겠어요 ㅋㅋㅋ",
          "original_form": "고생많으셨겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100023860.1.15",
          "form": "음 그래도 일반복무보단 편해보였어요 의경은 어떤가요",
          "original_form": "음 그래도 일반복무보단 편해보였어요 의경은 어떤가요",
          "speaker_id": "1",
          "time": "20210830 19:37"
        },
        {
          "id": "MDRW2100023860.1.16",
          "form": "사실 남친 일주일 되서 별 얘기 못들엇어요",
          "original_form": "사실 남친 일주일 되서 별 얘기 못들엇어요",
