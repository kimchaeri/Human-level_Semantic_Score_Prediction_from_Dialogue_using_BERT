      "utterance": [
        {
          "id": "MDRW2100011459.1.1",
          "form": "name1님은 어디 여행이 제일좋으셨나요",
          "original_form": "&name1&님은 어디 여행이 제일좋으셨나요",
          "speaker_id": "2",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100011459.1.2",
          "form": "저는 일본여행이요!",
          "original_form": "저는 일본여행이요!",
          "speaker_id": "1",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011459.1.3",
          "form": "일본좋죠",
          "original_form": "일본좋죠",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011459.1.4",
          "form": "저는 북해도갔었어요",
          "original_form": "저는 북해도갔었어요",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011459.1.5",
          "form": "오 저는 오사카 후쿠오카 가봤어요",
          "original_form": "오 저는 오사카 후쿠오카 가봤어요",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011459.1.6",
          "form": "대마도도 가봤네요",
          "original_form": "대마도도 가봤네요",
          "speaker_id": "2",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011459.1.7",
          "form": "머가 맛있나요",
          "original_form": "머가 맛있나요",
          "speaker_id": "2",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011459.1.8",
          "form": "후쿠오카 함바그 맛있었어요! 우와 대마도",
          "original_form": "후쿠오카 함바그 맛있었어요! 우와 대마도",
          "speaker_id": "1",
          "time": "20210824 18:00"
        },
        {
          "id": "MDRW2100011459.1.9",
          "form": "함바그저도 맛있게 먹었어요 후쿠오카가 우유가 질좋더라구요 초코도 맛있구",
          "original_form": "함바그저도 맛있게 먹었어요 후쿠오카가 우유가 질좋더라구요 초코도 맛있구",
          "speaker_id": "2",
          "time": "20210824 18:05"
        },
        {
          "id": "MDRW2100011459.1.10",
          "form": "대마도는 어떠셨나요 부산에서 배타죠?",
          "original_form": "대마도는 어떠셨나요 부산에서 배타죠?",
          "speaker_id": "1",
          "time": "20210824 18:07"
        },
        {
          "id": "MDRW2100011459.1.11",
          "form": "네 등산하러 많이 가는동네더라구요 전 산안탔지만 ㅋㅋ",
          "original_form": "네 등산하러 많이 가는동네더라구요 전 산안탔지만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011459.1.12",
          "form": "대마도도 일본 특유의 분위기있나요ㅎㅎ",
          "original_form": "대마도도 일본 특유의 분위기있나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011459.1.13",
          "form": "요즘 그립네요",
          "original_form": "요즘 그립네요",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011459.1.14",
          "form": "오밀조밀 시골 사람들 사는동네같아요",
          "original_form": "오밀조밀 시골 사람들 사는동네같아요",
          "speaker_id": "2",
          "time": "20210824 18:11"
        },
        {
          "id": "MDRW2100011459.1.15",
          "form": "가구며 집도작고 깨끗한동네더라구요",
          "original_form": "가구며 집도작고 깨끗한동네더라구요",
          "speaker_id": "2",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100011459.1.16",
          "form": "코로나가 잠잠해져야 가볼텐데ㅠ",
          "original_form": "코로나가 잠잠해져야 가볼텐데ㅠ",
          "speaker_id": "1",
          "time": "20210824 18:13"
        },
        {
          "id": "MDRW2100011459.1.17",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 18:15"
        }
      ]
