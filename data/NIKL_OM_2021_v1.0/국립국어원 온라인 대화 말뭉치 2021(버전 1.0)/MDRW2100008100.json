        },
        {
          "id": "MDRW2100008100.1.2",
          "form": "아뇨 안했어요",
          "original_form": "아뇨 안했어요",
          "speaker_id": "2",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008100.1.3",
          "form": "언제쯤?",
          "original_form": "언제쯤?",
          "speaker_id": "1",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008100.1.4",
          "form": "언제쯤 할지는 모르겠네요 여자친구도 없어서 ㅎㅎ",
          "original_form": "언제쯤 할지는 모르겠네요 여자친구도 없어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:53"
        },
        {
          "id": "MDRW2100008100.1.5",
          "form": "인연은 갑자기와요",
          "original_form": "인연은 갑자기와요",
          "speaker_id": "1",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100008100.1.6",
          "form": "갑자기 오죠",
          "original_form": "갑자기 오죠",
          "speaker_id": "2",
          "time": "20210822 14:10"
        },
        {
          "id": "MDRW2100008100.1.7",
          "form": "언제올련지 ㅎㅎ",
          "original_form": "언제올련지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:10"
        },
        {
          "id": "MDRW2100008100.1.8",
          "form": "기다리면 와요",
          "original_form": "기다리면 와요",
          "speaker_id": "1",
          "time": "20210822 14:13"
        },
        {
          "id": "MDRW2100008100.1.9",
          "form": "코로나때문에",
          "original_form": "코로나때문에",
          "speaker_id": "2",
          "time": "20210822 14:28"
        },
        {
          "id": "MDRW2100008100.1.10",
          "form": "기다려도 올련지",
          "original_form": "기다려도 올련지",
          "speaker_id": "2",
          "time": "20210822 14:28"
        },
        {
          "id": "MDRW2100008100.1.11",
          "form": "모르겠어요 ㅠ.ㅠ",
          "original_form": "모르겠어요 ㅠ.ㅠ",
          "speaker_id": "2",
          "time": "20210822 14:28"
        },
        {
          "id": "MDRW2100008100.1.12",
          "form": "반드시 오죠",
          "original_form": "반드시 오죠",
          "speaker_id": "1",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008100.1.13",
          "form": "그렇겠죠?",
          "original_form": "그렇겠죠?",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008100.1.14",
          "form": "왔으면 좋겠네요!",
          "original_form": "왔으면 좋겠네요!",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008100.1.15",
          "form": "만들어봐요",
          "original_form": "만들어봐요",
          "speaker_id": "1",
