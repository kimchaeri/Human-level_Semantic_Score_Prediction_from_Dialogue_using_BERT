      "utterance": [
        {
          "id": "MDRW2100045480.1.1",
          "form": "울을 거 같은데요",
          "original_form": "울을 거 같은데요",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045480.1.2",
          "form": "그럴거",
          "original_form": "그럴거",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045480.1.3",
          "form": "저도 찾아봐야겠어요",
          "original_form": "저도 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045480.1.4",
          "form": "푹 빠지셨네",
          "original_form": "푹 빠지셨네",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045480.1.5",
          "form": "인기 게임은 이유가 있다는 요",
          "original_form": "인기 게임은 이유가 있다는 요",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045480.1.6",
          "form": "재미있죠",
          "original_form": "재미있죠",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045480.1.7",
          "form": "농사 빨리 보고 싶은데",
          "original_form": "농사 빨리 보고 싶은데",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045480.1.8",
          "form": "저는 음식 터는 거",
          "original_form": "저는 음식 터는 거",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045480.1.9",
          "form": "계속 운전 중이 심",
          "original_form": "계속 운전 중이 심",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045480.1.10",
          "form": "아직 목적지가 없으신가 봐요",
          "original_form": "아직 목적지가 없으신가 봐요",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045480.1.11",
          "form": "좀비라도 나왔으면",
          "original_form": "좀비라도 나왔으면",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045480.1.12",
          "form": "대구 맵도 있어요",
          "original_form": "대구 맵도 있어요",
          "speaker_id": "2",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045480.1.13",
          "form": "지금은 아닌 거겠죠",
          "original_form": "지금은 아닌 거겠죠",
          "speaker_id": "1",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045480.1.14",
          "form": "아마도 요 어렵다 더라고요",
          "original_form": "아마도 요 어렵다 더라고요",
          "speaker_id": "2",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045480.1.15",
          "form": "멀티 된다네요",
          "original_form": "멀티 된다네요",
          "speaker_id": "1",
          "time": "20210908 02:54"
        }
