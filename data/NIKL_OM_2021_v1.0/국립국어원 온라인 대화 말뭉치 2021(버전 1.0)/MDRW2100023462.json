      "utterance": [
        {
          "id": "MDRW2100023462.1.1",
          "form": "여행하는데 혼밥하기 그래서 외식 잘 안하게.되네요",
          "original_form": "여행하는데 혼밥하기 그래서 외식 잘 안하게.되네요",
          "speaker_id": "2",
          "time": "20210830 04:26"
        },
        {
          "id": "MDRW2100023462.1.2",
          "form": "아 그럼 해드시는거에요?",
          "original_form": "아 그럼 해드시는거에요?",
          "speaker_id": "1",
          "time": "20210830 04:26"
        },
        {
          "id": "MDRW2100023462.1.3",
          "form": "그러고보니 요즘 지자체에서 숙박비용 지원해주는 어디어디 살이 프로그램이 많다고 들었어요",
          "original_form": "그러고보니 요즘 지자체에서 숙박비용 지원해주는 어디어디 살이 프로그램이 많다고 들었어요",
          "speaker_id": "1",
          "time": "20210830 04:27"
        },
        {
          "id": "MDRW2100023462.1.4",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "2",
          "time": "20210830 04:28"
        },
        {
          "id": "MDRW2100023462.1.5",
          "form": "전라도쪽 귀농귀촌 살이 프로그램도 있던데요",
          "original_form": "전라도쪽 귀농귀촌 살이 프로그램도 있던데요",
          "speaker_id": "2",
          "time": "20210830 04:28"
        },
        {
          "id": "MDRW2100023462.1.6",
          "form": "네 있을거에요",
          "original_form": "네 있을거에요",
          "speaker_id": "1",
          "time": "20210830 04:28"
        },
        {
          "id": "MDRW2100023462.1.7",
          "form": "제가 창원사는데요 창원이랑 김해됴",
          "original_form": "제가 창원사는데요 창원이랑 김해됴",
          "speaker_id": "1",
          "time": "20210830 04:29"
        },
        {
          "id": "MDRW2100023462.1.8",
          "form": "숙박비 주고 여행하고 블로그에 후기남기는거 하더라구요",
          "original_form": "숙박비 주고 여행하고 블로그에 후기남기는거 하더라구요",
          "speaker_id": "1",
          "time": "20210830 04:29"
        },
        {
          "id": "MDRW2100023462.1.9",
          "form": "엇",
          "original_form": "엇",
          "speaker_id": "2",
          "time": "20210830 04:31"
        },
        {
          "id": "MDRW2100023462.1.10",
          "form": "창원사람!!",
          "original_form": "창원사람!!",
          "speaker_id": "2",
          "time": "20210830 04:31"
        },
        {
          "id": "MDRW2100023462.1.11",
          "form": "저도 고향 창원여",
          "original_form": "저도 고향 창원여",
          "speaker_id": "2",
          "time": "20210830 04:31"
        },
        {
          "id": "MDRW2100023462.1.12",
          "form": "오오 그러시군요 반가워요!!",
          "original_form": "오오 그러시군요 반가워요!!",
          "speaker_id": "1",
          "time": "20210830 04:31"
        },
        {
          "id": "MDRW2100023462.1.13",
          "form": "방가~~",
          "original_form": "방가~~",
          "speaker_id": "2",
          "time": "20210830 04:31"
        },
        {
          "id": "MDRW2100023462.1.14",
          "form": "창원 여행 흠",
          "original_form": "창원 여행 흠",
          "speaker_id": "2",
          "time": "20210830 04:32"
        },
        {
          "id": "MDRW2100023462.1.15",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 04:32"
        },
        {
          "id": "MDRW2100023462.1.16",
          "form": "김해쪽이 나을 것 같네요",
          "original_form": "김해쪽이 나을 것 같네요",
          "speaker_id": "2",
          "time": "20210830 04:32"
        },
        {
          "id": "MDRW2100023462.1.17",
          "form": "네이버에 창원 여행 후기 이런걸로 해보세요",
          "original_form": "네이버에 창원 여행 후기 이런걸로 해보세요",
          "speaker_id": "1",
          "time": "20210830 04:32"
        },
        {
          "id": "MDRW2100023462.1.18",
          "form": "최근에 하더라구요 맛집검색하다 우연히봤어요",
          "original_form": "최근에 하더라구요 맛집검색하다 우연히봤어요",
          "speaker_id": "1",
          "time": "20210830 04:32"
        },
        {
          "id": "MDRW2100023462.1.19",
          "form": "오 블로그도 하시나여",
          "original_form": "오 블로그도 하시나여",
          "speaker_id": "2",
          "time": "20210830 04:34"
        },
        {
          "id": "MDRW2100023462.1.20",
          "form": "네 저도 하긴 하는데",
          "original_form": "네 저도 하긴 하는데",
          "speaker_id": "1",
          "time": "20210830 04:35"
        },
        {
          "id": "MDRW2100023462.1.21",
          "form": "제 블로그 얘기가 아니구 갖다온사람이 후기남겨야 하더라구요 그 조건으로 돈주는거래요",
          "original_form": "제 블로그 얘기가 아니구 갖다온사람이 후기남겨야 하더라구요 그 조건으로 돈주는거래요",
          "speaker_id": "1",
          "time": "20210830 04:35"
        },
        {
          "id": "MDRW2100023462.1.22",
          "form": "여행하고 돈 받고",
          "original_form": "여행하고 돈 받고",
          "speaker_id": "2",
          "time": "20210830 04:36"
        },
        {
          "id": "MDRW2100023462.1.23",
          "form": "좋은 걸까요",
          "original_form": "좋은 걸까요",
          "speaker_id": "2",
          "time": "20210830 04:36"
        },
        {
          "id": "MDRW2100023462.1.24",
          "form": "여행이 일이.되면 싫을 것 같은데\n.",
          "original_form": "여행이 일이.되면 싫을 것 같은데\n.",
          "speaker_id": "2",
          "time": "20210830 04:36"
        },
        {
          "id": "MDRW2100023462.1.25",
          "form": "근데 딱히 제한없이 올리는거라",
          "original_form": "근데 딱히 제한없이 올리는거라",
          "speaker_id": "1",
          "time": "20210830 04:36"
        },
        {
          "id": "MDRW2100023462.1.26",
          "form": "괜찮아보였어요 솔직히 그거보고 관광올거 같지 않더라구요",
          "original_form": "괜찮아보였어요 솔직히 그거보고 관광올거 같지 않더라구요",
          "speaker_id": "1",
          "time": "20210830 04:37"
        },
        {
          "id": "MDRW2100023462.1.27",
          "form": "그럼 그 돈 받는 사람이 이익인듯요 (세금 낭비)",
          "original_form": "그럼 그 돈 받는 사람이 이익인듯요 (세금 낭비)",
          "speaker_id": "1",
          "time": "20210830 04:37"
        }
      ]
