        {
          "id": "MDRW2100008297.1.1",
          "form": "좋아하는 아이돌 있으세요?",
          "original_form": "좋아하는 아이돌 있으세요?",
          "speaker_id": "1",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100008297.1.2",
          "form": "음 아니요~~~!!",
          "original_form": "음 아니요~~~!!",
          "speaker_id": "2",
          "time": "20210822 17:00"
        },
        {
          "id": "MDRW2100008297.1.3",
          "form": "즐겨듣는 가수는 혹시 있으세요?",
          "original_form": "즐겨듣는 가수는 혹시 있으세요?",
          "speaker_id": "1",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008297.1.4",
          "form": "저 옛날 노래를 ㅎㅎ 아이돌 좋아하시나봐요",
          "original_form": "저 옛날 노래를 ㅎㅎ 아이돌 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008297.1.5",
          "form": "네 아이돌 덕질하면서 살고 있어요ㅎㅎ",
          "original_form": "네 아이돌 덕질하면서 살고 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008297.1.6",
          "form": "누구요?ㅎㅎ",
          "original_form": "누구요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:04"
        },
        {
          "id": "MDRW2100008297.1.7",
          "form": "투모로우바이투게더랑 방탄 좋아해요",
          "original_form": "투모로우바이투게더랑 방탄 좋아해요",
          "speaker_id": "1",
          "time": "20210822 17:04"
        },
        {
          "id": "MDRW2100008297.1.8",
          "form": "오 유명하죠",
          "original_form": "오 유명하죠",
          "speaker_id": "2",
          "time": "20210822 17:59"
        },
        {
          "id": "MDRW2100008297.1.9",
          "form": "노래 엄청 많이 들어요",
          "original_form": "노래 엄청 많이 들어요",
          "speaker_id": "1",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008297.1.10",
          "form": "ㅋㅋ음반도 사시고요?",
          "original_form": "ㅋㅋ음반도 사시고요?",
          "speaker_id": "2",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008297.1.11",
          "form": "음반은 매번 사고 있어요ㅋㅋ 용돈의 유일한 사용처입니다",
          "original_form": "음반은 매번 사고 있어요ㅋㅋ 용돈의 유일한 사용처입니다",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008297.1.12",
          "form": "얼마정도 해요?",
          "original_form": "얼마정도 해요?",
          "speaker_id": "2",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008297.1.13",
          "form": "한 장당 1만4천에서 1만7천 정도 해요",
          "original_form": "한 장당 1만4천에서 1만7천 정도 해요",
          "speaker_id": "1",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008297.1.14",
          "form": "음 비싸지는 않군요",
          "original_form": "음 비싸지는 않군요",
          "speaker_id": "2",
          "time": "20210822 18:12"
        },
        {
          "id": "MDRW2100008297.1.15",
          "form": "요즘엔 내용물이 두둑해서 오히려 싸진 느낌이에요",
          "original_form": "요즘엔 내용물이 두둑해서 오히려 싸진 느낌이에요",
