      "utterance": [
        {
          "id": "MDRW2100013582.1.1",
          "form": "요즘은 당류 줄이는중",
          "original_form": "요즘은 당류 줄이는중",
          "speaker_id": "1",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013582.1.2",
          "form": "그래 살빼야지~~",
          "original_form": "그래 살빼야지~~",
          "speaker_id": "2",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013582.1.3",
          "form": "설탕이랑 시럽 안먹고 대체당 먹는중~",
          "original_form": "설탕이랑 시럽 안먹고 대체당 먹는중~",
          "speaker_id": "1",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013582.1.4",
          "form": "그것도 몸에 안 좋을것 같은데",
          "original_form": "그것도 몸에 안 좋을것 같은데",
          "speaker_id": "2",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100013582.1.5",
          "form": "아닌데 다이어트 하는 사람들 다 먹는데",
          "original_form": "아닌데 다이어트 하는 사람들 다 먹는데",
          "speaker_id": "1",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100013582.1.6",
          "form": "대체당이라 해도 달던데",
          "original_form": "대체당이라 해도 달던데",
          "speaker_id": "2",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100013582.1.7",
          "form": "달아도 혈당 안 높이는거야~",
          "original_form": "달아도 혈당 안 높이는거야~",
          "speaker_id": "1",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100013582.1.8",
          "form": "무슨 연구결과가 있는거가",
          "original_form": "무슨 연구결과가 있는거가",
          "speaker_id": "2",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100013582.1.9",
          "form": "전문가들도 제로콜라 잘만 먹던데 모~",
          "original_form": "전문가들도 제로콜라 잘만 먹던데 모~",
          "speaker_id": "1",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100013582.1.10",
          "form": "걱정이 되서 그렇지",
          "original_form": "걱정이 되서 그렇지",
          "speaker_id": "2",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100013582.1.11",
          "form": "어쨌든 당류 줄이고 이제 치킨만 줄이면 되는뎀",
          "original_form": "어쨌든 당류 줄이고 이제 치킨만 줄이면 되는뎀",
          "speaker_id": "1",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100013582.1.12",
          "form": "치킨은 너무 자주 먹잖아",
          "original_form": "치킨은 너무 자주 먹잖아",
          "speaker_id": "2",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100013582.1.13",
          "form": "줄여나가야지 이제부터~",
          "original_form": "줄여나가야지 이제부터~",
          "speaker_id": "1",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100013582.1.14",
          "form": "그래 줄여야지 그것때메 살찐다",
          "original_form": "그래 줄여야지 그것때메 살찐다",
          "speaker_id": "2",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013582.1.15",
          "form": "건강하고 맛있는건 없나ㅠㅠ",
          "original_form": "건강하고 맛있는건 없나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013582.1.16",
          "form": "다이어트 닭 먹어~",
          "original_form": "다이어트 닭 먹어~",
          "speaker_id": "2",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100013582.1.17",
