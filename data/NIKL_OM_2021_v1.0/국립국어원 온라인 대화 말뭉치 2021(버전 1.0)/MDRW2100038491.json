      "utterance": [
        {
          "id": "MDRW2100038491.1.1",
          "form": "재택 언제까지 해",
          "original_form": "재택 언제까지 해",
          "speaker_id": "2",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100038491.1.2",
          "form": "아마도 이번주나 다음주?",
          "original_form": "아마도 이번주나 다음주?",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100038491.1.3",
          "form": "아직 코로나 확진자 많은데",
          "original_form": "아직 코로나 확진자 많은데",
          "speaker_id": "2",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100038491.1.4",
          "form": "그니까 ㅜㅜ 계속 재택하고싶다",
          "original_form": "그니까 ㅜㅜ 계속 재택하고싶다",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100038491.1.5",
          "form": "한다고 해라",
          "original_form": "한다고 해라",
          "speaker_id": "2",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100038491.1.6",
          "form": "웅웅 근데 회사에서 해야하는 일이 있어서 ㅜ",
          "original_form": "웅웅 근데 회사에서 해야하는 일이 있어서 ㅜ",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100038491.1.7",
          "form": "아쉽네",
          "original_form": "아쉽네",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100038491.1.8",
          "form": "컴퓨터를 들고오던지 하면 될듯",
          "original_form": "컴퓨터를 들고오던지 하면 될듯",
          "speaker_id": "1",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100038491.1.9",
          "form": "들고 와라",
          "original_form": "들고 와라",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100038491.1.10",
          "form": "웅 무겁고 귀찮긴하지만..",
          "original_form": "웅 무겁고 귀찮긴하지만..",
          "speaker_id": "1",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100038491.1.11",
          "form": "그래도 재택 근무 하는게 낫지",
          "original_form": "그래도 재택 근무 하는게 낫지",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100038491.1.12",
          "form": "웅웅 훨씬 낫지",
          "original_form": "웅웅 훨씬 낫지",
          "speaker_id": "1",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100038491.1.13",
          "form": "말해 봐라",
          "original_form": "말해 봐라",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100038491.1.14",
