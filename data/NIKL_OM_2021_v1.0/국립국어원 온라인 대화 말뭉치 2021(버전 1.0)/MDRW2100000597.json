      "utterance": [
        {
          "id": "MDRW2100000597.1.1",
          "form": "싸이월드가 부활한다네 ㅋㅋ",
          "original_form": "싸이월드가 부활한다네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000597.1.2",
          "form": "웅웅~ㅋㅋ 메타버스 형식으로 부활한다던데~",
          "original_form": "웅웅~ㅋㅋ 메타버스 형식으로 부활한다던데~",
          "speaker_id": "2",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000597.1.3",
          "form": "막 홀로그램처럼 미니홈피 꾸밀 수 있곸ㅋㅋㅋ",
          "original_form": "막 홀로그램처럼 미니홈피 꾸밀 수 있곸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000597.1.4",
          "form": "오 진짜?!ㅋㅋㅋ",
          "original_form": "오 진짜?!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000597.1.5",
          "form": "신기하구만 제대로 못봤어 ㅋㅋ",
          "original_form": "신기하구만 제대로 못봤어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000597.1.6",
          "form": "웅웅~ 저번에 한번 실패하고 나서",
          "original_form": "웅웅~ 저번에 한번 실패하고 나서",
          "speaker_id": "2",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000597.1.7",
          "form": "단단히 벼르고 나오는 거 같은데, 이번엔 좀 잘되길~ㅋㅋ",
          "original_form": "단단히 벼르고 나오는 거 같은데, 이번엔 좀 잘되길~ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000597.1.8",
          "form": "웅..!ㅋㅋ 몬가 내가 하기엔 귀찮긴 하지만 ㅋㅋ",
          "original_form": "웅..!ㅋㅋ 몬가 내가 하기엔 귀찮긴 하지만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000597.1.9",
          "form": "웅웅~ 그냥 처음에 구경?정도만 할 거 같은 기분ㅋㅋㅋ",
          "original_form": "웅웅~ 그냥 처음에 구경?정도만 할 거 같은 기분ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000597.1.10",
          "form": "웅웅~구경좀 해봐야겠어~",
          "original_form": "웅웅~구경좀 해봐야겠어~",
          "speaker_id": "1",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000597.1.11",
          "form": "아직은 메타버스가 넘나 멀게 느껴져섴ㅋ",
          "original_form": "아직은 메타버스가 넘나 멀게 느껴져섴ㅋ",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000597.1.12",
          "form": "싸이월드는 딱 예전 그 감성이 좋은듯~",
          "original_form": "싸이월드는 딱 예전 그 감성이 좋은듯~",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000597.1.13",
          "form": "마자.. 잘몰겠어 아직도",
          "original_form": "마자.. 잘몰겠어 아직도",
          "speaker_id": "1",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000597.1.14",
          "form": "그러게~ 넘 급변하는거 아닌지 ㅋ",
          "original_form": "그러게~ 넘 급변하는거 아닌지 ㅋ",
          "speaker_id": "1",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000597.1.15",
          "form": "우리 중고딩때인가?ㅋㅋㅋ",
          "original_form": "우리 중고딩때인가?ㅋㅋㅋ",
