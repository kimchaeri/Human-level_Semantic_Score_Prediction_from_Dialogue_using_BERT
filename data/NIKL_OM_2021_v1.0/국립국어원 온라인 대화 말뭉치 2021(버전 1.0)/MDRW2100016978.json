      "utterance": [
        {
          "id": "MDRW2100016978.1.1",
          "form": "지금 name3이 자취방 가는 길?",
          "original_form": "지금 &name3&이 자취방 가는 길?",
          "speaker_id": "1",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100016978.1.2",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100016978.1.3",
          "form": "얘 백퍼 밥도 안먹고 낑낑거리고 있을걸",
          "original_form": "얘 백퍼 밥도 안먹고 낑낑거리고 있을걸",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100016978.1.4",
          "form": "그냥 아기고양이야",
          "original_form": "그냥 아기고양이야",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100016978.1.5",
          "form": "걔도 밥 거르는거 좀 심하잖아",
          "original_form": "걔도 밥 거르는거 좀 심하잖아",
          "speaker_id": "1",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100016978.1.6",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100016978.1.7",
          "form": "사실 우리중에 이name4하고 이name5하고 박name6만 먹는거에 진심이긴 해",
          "original_form": "사실 우리중에 이&name4&하고 이&name5&하고 박&name6&만 먹는거에 진심이긴 해",
          "speaker_id": "1",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100016978.1.8",
          "form": "맨날 나랑 밥 안먹으면 그냥 안먹던데",
          "original_form": "맨날 나랑 밥 안먹으면 그냥 안먹던데",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100016978.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100016978.1.10",
          "form": "그래서 동아리 오라해서 빕먹이려 했는ㄷㅔ",
          "original_form": "그래서 동아리 오라해서 빕먹이려 했는ㄷㅔ",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100016978.1.11",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100016978.1.12",
          "form": "이name5이 너무 강하개 꼬셔서 역효과 났어",
          "original_form": "이&name5&이 너무 강하개 꼬셔서 역효과 났어",
          "speaker_id": "2",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100016978.1.13",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100016978.1.14",
          "form": "언제나 name5이가 문제구만",
          "original_form": "언제나 &name5&이가 문제구만",
          "speaker_id": "1",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100016978.1.15",
          "form": "너는 애들 만났어?",
          "original_form": "너는 애들 만났어?",
          "speaker_id": "2",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100016978.1.16",
          "form": "엉 지금만나서 식당 가는 길",
          "original_form": "엉 지금만나서 식당 가는 길",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100016978.1.17",
          "form": "하 나도 고기...",
          "original_form": "하 나도 고기...",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100016978.1.18",
          "form": "개강전 학교 앞이라 그런지 한가해",
          "original_form": "개강전 학교 앞이라 그런지 한가해",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100016978.1.19",
          "form": "하긴 지금 방학이지",
          "original_form": "하긴 지금 방학이지",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100016978.1.20",
          "form": "엉 근데",
          "original_form": "엉 근데",
          "speaker_id": "1",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100016978.1.21",
          "form": "학교 앞이 싸고 맛있고 익숙하잖아 다들 근처 살고",
          "original_form": "학교 앞이 싸고 맛있고 익숙하잖아 다들 근처 살고",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100016978.1.22",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100016978.1.23",
          "form": "난 지금 죽사서 가는중",
          "original_form": "난 지금 죽사서 가는중",
          "speaker_id": "2",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100016978.1.24",
          "form": "머... 같은 고기긴 하네",
