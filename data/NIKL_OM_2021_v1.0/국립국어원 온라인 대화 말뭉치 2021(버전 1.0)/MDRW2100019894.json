          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019894.1.4",
          "form": "피자치킨그런",
          "original_form": "피자치킨그런",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019894.1.5",
          "form": "아 나도 그거 생각해봄",
          "original_form": "아 나도 그거 생각해봄",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019894.1.6",
          "form": "뭔?",
          "original_form": "뭔?",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019894.1.7",
          "form": "우라빠 닭강정",
          "original_form": "우라빠 닭강정",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019894.1.8",
          "form": "근데 배송비가3천이나한다",
          "original_form": "근데 배송비가3천이나한다",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019894.1.9",
          "form": "우라빠 배송비 많이나가긴함ㄱ",
          "original_form": "우라빠 배송비 많이나가긴함ㄱ",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019894.1.10",
          "form": "3천원이나 하더라",
          "original_form": "3천원이나 하더라",
          "speaker_id": "1",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019894.1.11",
          "form": "비싼듯",
          "original_form": "비싼듯",
          "speaker_id": "1",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019894.1.12",
          "form": "배송비 없는거중에 고르든동",
          "original_form": "배송비 없는거중에 고르든동",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019894.1.13",
          "form": "3천원?",
          "original_form": "3천원?",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019894.1.14",
          "form": "없는걸로 찾아보자",
          "original_form": "없는걸로 찾아보자",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019894.1.15",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019894.1.16",
          "form": "맞음 3천은 좀 비싸지",
          "original_form": "맞음 3천은 좀 비싸지",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019894.1.17",
          "form": "배송비없는데 없나",
          "original_form": "배송비없는데 없나",
          "speaker_id": "1",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019894.1.18",
          "form": "배송비없는거 알아서 찾아바라",
          "original_form": "배송비없는거 알아서 찾아바라",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019894.1.19",
          "form": "그래야겠다",
          "original_form": "그래야겠다",
          "speaker_id": "1",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019894.1.20",
          "form": "요새 배송비 없는",
          "original_form": "요새 배송비 없는",
