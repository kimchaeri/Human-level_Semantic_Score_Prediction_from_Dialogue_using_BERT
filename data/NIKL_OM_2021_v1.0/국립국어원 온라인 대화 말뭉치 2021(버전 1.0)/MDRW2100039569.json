      "utterance": [
        {
          "id": "MDRW2100039569.1.1",
          "form": "근데 여기 진짜 웃긴다",
          "original_form": "근데 여기 진짜 웃긴다",
          "speaker_id": "1",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039569.1.2",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039569.1.3",
          "form": "방금 식사권 10만원에 판다고 올라왔는데",
          "original_form": "방금 식사권 10만원에 판다고 올라왔는데",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039569.1.4",
          "form": "이거 살까...?",
          "original_form": "이거 살까...?",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039569.1.5",
          "form": "어.. 정말일까..?(못 믿음",
          "original_form": "어.. 정말일까..?(못 믿음",
          "speaker_id": "2",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039569.1.6",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039569.1.7",
          "form": "진짜면 돈 아끼고 좋겠지만...",
          "original_form": "진짜면 돈 아끼고 좋겠지만...",
          "speaker_id": "2",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039569.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ 당연 직거래해야지",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 당연 직거래해야지",
          "speaker_id": "1",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039569.1.9",
          "form": "근데 한장만이야...",
          "original_form": "근데 한장만이야...",
          "speaker_id": "1",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039569.1.10",
          "form": "10만원 이하의 가격을 제시하는 사람이 나타날까..?",
          "original_form": "10만원 이하의 가격을 제시하는 사람이 나타날까..?",
          "speaker_id": "1",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039569.1.11",
          "form": "9월 24일 전에...",
          "original_form": "9월 24일 전에...",
          "speaker_id": "1",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039569.1.12",
          "form": "ㅋㅋㅋㅋ 글쎄.. 10만...이하는 없을거 같기도 하고",
          "original_form": "ㅋㅋㅋㅋ 글쎄.. 10만...이하는 없을거 같기도 하고",
          "speaker_id": "2",
          "time": "20210903 19:47"
        },
        {
          "id": "MDRW2100039569.1.13",
          "form": "글고 타이밍 완전 대박인거",
          "original_form": "글고 타이밍 완전 대박인거",
          "speaker_id": "1",
          "time": "20210903 20:06"
        },
        {
          "id": "MDRW2100039569.1.14",
          "form": "원래 뷔페시간 100분 단축운영중이었는대",
          "original_form": "원래 뷔페시간 100분 단축운영중이었는대",
          "speaker_id": "1",
          "time": "20210903 20:06"
        },
        {
          "id": "MDRW2100039569.1.15",
          "form": "담주부터 거리두기 완화돼서",
          "original_form": "담주부터 거리두기 완화돼서",
          "speaker_id": "1",
          "time": "20210903 20:06"
        },
        {
          "id": "MDRW2100039569.1.16",
          "form": "120분으로 늘어났대",
          "original_form": "120분으로 늘어났대",
          "speaker_id": "1",
          "time": "20210903 20:06"
        },
        {
          "id": "MDRW2100039569.1.17",
          "form": "헉 완전 좋아",
          "original_form": "헉 완전 좋아",
          "speaker_id": "2",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039569.1.18",
          "form": "그런데 추석 지나고 다시 줄어들진 않겠지..?",
          "original_form": "그런데 추석 지나고 다시 줄어들진 않겠지..?",
          "speaker_id": "2",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100039569.1.19",
          "form": "설마...",
          "original_form": "설마...",
          "speaker_id": "1",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039569.1.20",
          "form": "줄어들더라도",
          "original_form": "줄어들더라도",
          "speaker_id": "1",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039569.1.21",
          "form": "우리는 추석 있는 주니까",
          "original_form": "우리는 추석 있는 주니까",
          "speaker_id": "1",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039569.1.22",
          "form": "그 다음주부터 시행일거야 ㅋㅋㅋ",
          "original_form": "그 다음주부터 시행일거야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039569.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그랭 그럼 우린 뷔페에서 쓸어담으면 되겠네 크으",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그랭 그럼 우린 뷔페에서 쓸어담으면 되겠네 크으",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039569.1.24",
          "form": "그러하다...",
          "original_form": "그러하다...",
          "speaker_id": "1",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039569.1.25",
          "form": "내가 일부러 창가자리에 배정해달라고 전화햇는데",
          "original_form": "내가 일부러 창가자리에 배정해달라고 전화햇는데",
          "speaker_id": "1",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039569.1.26",
          "form": "예약이 거의 다 찬 상태라 입구쪽일거래 ㅠ",
          "original_form": "예약이 거의 다 찬 상태라 입구쪽일거래 ㅠ",
          "speaker_id": "1",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039569.1.27",
          "form": "악",
          "original_form": "악",
          "speaker_id": "1",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100039569.1.28",
          "form": "아까 고민하던 식사권",
          "original_form": "아까 고민하던 식사권",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100039569.1.29",
          "form": "그사람이 맘이 급했는지",
          "original_form": "그사람이 맘이 급했는지",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100039569.1.30",
          "form": "9만원으로 내리고 칼거래됨 ㅠ",
          "original_form": "9만원으로 내리고 칼거래됨 ㅠ",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100039569.1.31",
          "form": "ㅎ ㅏ.. 네고 걸어볼걸 ㅠㅠㅠ",
          "original_form": "ㅎ ㅏ.. 네고 걸어볼걸 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100039569.1.32",
          "form": "헉 ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "헉 ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100039569.1.33",
          "form": "입구쪽이라도 머... 음식에 더 가깝자고 생각하자",
          "original_form": "입구쪽이라도 머... 음식에 더 가깝자고 생각하자",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100039569.1.34",
          "form": "그래도 여행가기 전에 계획 짜는게 젤 두근거리는듯 ㅋㅋㅋ",
          "original_form": "그래도 여행가기 전에 계획 짜는게 젤 두근거리는듯 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100039569.1.35",
          "form": "그치 ㅋㅋㅋㅋ 원래 여행 가기 전에가 제일 신나 이것도하고 저것도하고 ㅋㅋㅋㅋ",
          "original_form": "그치 ㅋㅋㅋㅋ 원래 여행 가기 전에가 제일 신나 이것도하고 저것도하고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:19"
        },
        {
          "id": "MDRW2100039569.1.36",
          "form": "ㅇㅇ 호텔 식사권 아직 찾아보는중..",
          "original_form": "ㅇㅇ 호텔 식사권 아직 찾아보는중..",
