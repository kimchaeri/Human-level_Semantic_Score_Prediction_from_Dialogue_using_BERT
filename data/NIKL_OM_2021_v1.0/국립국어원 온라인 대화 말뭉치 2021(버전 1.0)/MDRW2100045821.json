      "utterance": [
        {
          "id": "MDRW2100045821.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100045821.1.2",
          "form": "매운거 좋아해요",
          "original_form": "매운거 좋아해요",
          "speaker_id": "2",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100045821.1.3",
          "form": "면도 좋아하고요",
          "original_form": "면도 좋아하고요",
          "speaker_id": "2",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100045821.1.4",
          "form": "오..짬뽕 같은거 좋아하시겠네요.",
          "original_form": "오..짬뽕 같은거 좋아하시겠네요.",
          "speaker_id": "1",
          "time": "20210908 07:47"
        },
        {
          "id": "MDRW2100045821.1.5",
          "form": "네 너무 맛잇져\r\n모 좋아하세요?",
          "original_form": "네 너무 맛잇져\r\n모 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 07:48"
        },
        {
          "id": "MDRW2100045821.1.6",
          "form": "저는 찜종류 좋아해요",
          "original_form": "저는 찜종류 좋아해요",
          "speaker_id": "1",
          "time": "20210908 07:48"
        },
        {
          "id": "MDRW2100045821.1.7",
          "form": "짜장면 짬뽕 중에서는 뭐가 더 좋으세요?",
          "original_form": "짜장면 짬뽕 중에서는 뭐가 더 좋으세요?",
          "speaker_id": "1",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100045821.1.8",
          "form": "고르기 힘드네요ㅋ\r\n둘다너무 맛잇어서",
          "original_form": "고르기 힘드네요ㅋ\r\n둘다너무 맛잇어서",
          "speaker_id": "2",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045821.1.9",
          "form": "ㅋㅋㅋ저는 짜장면은 잘 안 고르는거 같아요. 항상 짬뽕.^^",
          "original_form": "ㅋㅋㅋ저는 짜장면은 잘 안 고르는거 같아요. 항상 짬뽕.^^",
          "speaker_id": "1",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100045821.1.10",
          "form": "전 두개 나눠먹어요ㅋㅋ",
          "original_form": "전 두개 나눠먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100045821.1.11",
          "form": "찜은 어떤 종류요?",
          "original_form": "찜은 어떤 종류요?",
          "speaker_id": "2",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100045821.1.12",
          "form": "미더덕찜이나.해물찜이요",
          "original_form": "미더덕찜이나.해물찜이요",
          "speaker_id": "1",
          "time": "20210908 08:00"
        },
        {
          "id": "MDRW2100045821.1.13",
          "form": "와 해물찜맛있져",
          "original_form": "와 해물찜맛있져",
          "speaker_id": "2",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100045821.1.14",
          "form": "아구찜도 맛나요",
          "original_form": "아구찜도 맛나요",
          "speaker_id": "2",
          "time": "20210908 08:08"
        },
        {
          "id": "MDRW2100045821.1.15",
          "form": "아구찜 미더덕찜 대구찜 다 맛있어요",
          "original_form": "아구찜 미더덕찜 대구찜 다 맛있어요",
          "speaker_id": "1",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045821.1.16",
          "form": "밥볶으면 꿀맛이죠",
          "original_form": "밥볶으면 꿀맛이죠",
          "speaker_id": "2",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045821.1.17",
          "form": "밥 볶아 먹어 보진 않았는데",
          "original_form": "밥 볶아 먹어 보진 않았는데",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045821.1.18",
          "form": "김가루 뿌리면 맛나요ㅋㅋ",
          "original_form": "김가루 뿌리면 맛나요ㅋㅋ",
          "speaker_id": "2",
