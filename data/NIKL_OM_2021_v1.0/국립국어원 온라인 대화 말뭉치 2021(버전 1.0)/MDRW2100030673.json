      "utterance": [
        {
          "id": "MDRW2100030673.1.1",
          "form": "아마스빈 가고 싶어요",
          "original_form": "아마스빈 가고 싶어요",
          "speaker_id": "2",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100030673.1.2",
          "form": "베트남 음식이 그립네요",
          "original_form": "베트남 음식이 그립네요",
          "speaker_id": "1",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100030673.1.3",
          "form": "타피오카 펄이 맛있는데 말이죠",
          "original_form": "타피오카 펄이 맛있는데 말이죠",
          "speaker_id": "2",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100030673.1.4",
          "form": "아마스빈 근처에 있잖아요",
          "original_form": "아마스빈 근처에 있잖아요",
          "speaker_id": "1",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100030673.1.5",
          "form": "지금 당장 고고",
          "original_form": "지금 당장 고고",
          "speaker_id": "1",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100030673.1.6",
          "form": "걸어가려면 10분 정도 걸려요",
          "original_form": "걸어가려면 10분 정도 걸려요",
          "speaker_id": "2",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100030673.1.7",
          "form": "너무 귀찮아요",
          "original_form": "너무 귀찮아요",
          "speaker_id": "2",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100030673.1.8",
          "form": "음료 마시기 좋은 시간이네요",
          "original_form": "음료 마시기 좋은 시간이네요",
          "speaker_id": "1",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100030673.1.9",
          "form": "어제 산 자두 놓잖아 마실래요",
          "original_form": "어제 산 자두 놓잖아 마실래요",
          "speaker_id": "2",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100030673.1.10",
          "form": "귀찮으면 저녁에 산책 삼아 나가세요",
          "original_form": "귀찮으면 저녁에 산책 삼아 나가세요",
          "speaker_id": "1",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100030673.1.11",
          "form": "나도 녹차",
          "original_form": "나도 녹차",
          "speaker_id": "2",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100030673.1.12",
          "form": "자두 녹차",
          "original_form": "자두 녹차",
          "speaker_id": "1",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100030673.1.13",
          "form": "발음 제대로 해 보세요",
          "original_form": "발음 제대로 해 보세요",
          "speaker_id": "1",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100030673.1.14",
          "form": "저녁에 나가는 건 더 귀찮아요",
          "original_form": "저녁에 나가는 건 더 귀찮아요",
          "speaker_id": "2",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100030673.1.15",
          "form": "자두 녹차",
          "original_form": "자두 녹차",
          "speaker_id": "2",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100030673.1.16",
          "form": "귀차니즘이 엄청나군요",
          "original_form": "귀차니즘이 엄청나군요",
          "speaker_id": "1",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100030673.1.17",
          "form": "제 발음이 이상 했나 봐요",
          "original_form": "제 발음이 이상 했나 봐요",
          "speaker_id": "2",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100030673.1.18",
          "form": "부지런한 자 먹을 수 있다",
          "original_form": "부지런한 자 먹을 수 있다",
          "speaker_id": "1",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100030673.1.19",
          "form": "제 귀차니즘은 세계 최고죠",
          "original_form": "제 귀차니즘은 세계 최고죠",
          "speaker_id": "2",
          "time": "20210904 11:17"
        }
