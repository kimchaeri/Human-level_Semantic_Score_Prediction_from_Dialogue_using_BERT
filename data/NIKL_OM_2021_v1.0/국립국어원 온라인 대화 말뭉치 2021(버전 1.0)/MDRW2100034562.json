      "utterance": [
        {
          "id": "MDRW2100034562.1.1",
          "form": "미용이나 건강을 위한 습관 있나요",
          "original_form": "미용이나 건강을 위한 습관 있나요",
          "speaker_id": "1",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100034562.1.2",
          "form": "음.. 습관은 딱히 없어요ㅎㅎ",
          "original_form": "음.. 습관은 딱히 없어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100034562.1.3",
          "form": "그렇군요 다이어트 해보셨나요",
          "original_form": "그렇군요 다이어트 해보셨나요",
          "speaker_id": "1",
          "time": "20210828 16:00"
        },
        {
          "id": "MDRW2100034562.1.4",
          "form": "평생햇죠ㅎㅅ",
          "original_form": "평생햇죠ㅎㅅ",
          "speaker_id": "2",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100034562.1.5",
          "form": "앞으로도..",
          "original_form": "앞으로도..",
          "speaker_id": "2",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100034562.1.6",
          "form": "추천해주실만한 팁 이",
          "original_form": "추천해주실만한 팁 이",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100034562.1.7",
          "form": "있나요",
          "original_form": "있나요",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100034562.1.8",
          "form": "그냥 적게 먹고 운동이죠ㅎㅎ",
          "original_form": "그냥 적게 먹고 운동이죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034562.1.9",
          "form": "야채많이요",
          "original_form": "야채많이요",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034562.1.10",
          "form": "운동은 뭐하시나요?",
          "original_form": "운동은 뭐하시나요?",
          "speaker_id": "1",
          "time": "20210828 17:13"
        },
        {
          "id": "MDRW2100034562.1.11",
          "form": "전 헬스햇어요",
          "original_form": "전 헬스햇어요",
          "speaker_id": "2",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100034562.1.12",
          "form": "아 헬스 얼마나 하셨나요",
          "original_form": "아 헬스 얼마나 하셨나요",
          "speaker_id": "1",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100034562.1.13",
          "form": "3~4년쯤 한거같아요ㅎ",
          "original_form": "3~4년쯤 한거같아요ㅎ",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034562.1.14",
          "form": "와 그럼 이제 웬만한 기구들을 다 사용하시겠어요 피티없이 혼자하셨나요",
          "original_form": "와 그럼 이제 웬만한 기구들을 다 사용하시겠어요 피티없이 혼자하셨나요",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034562.1.15",
          "form": "첨에는 피티로 배우고요ㅎ",
          "original_form": "첨에는 피티로 배우고요ㅎ",
          "speaker_id": "2",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100034562.1.16",
          "form": "아 그렇군요 ㅠ 저는 피티 받는중인데 혼자하려면 하나도 기억이 안나요 ㅠ",
          "original_form": "아 그렇군요 ㅠ 저는 피티 받는중인데 혼자하려면 하나도 기억이 안나요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100034562.1.17",
          "form": "피티받고 혼자서 계속 해보셔야해요ㅎ",
          "original_form": "피티받고 혼자서 계속 해보셔야해요ㅎ",
          "speaker_id": "2",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100034562.1.18",
          "form": "피티때만 운동가시지말고 텀두고 피티를 잡고",
          "original_form": "피티때만 운동가시지말고 텀두고 피티를 잡고",
          "speaker_id": "2",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100034562.1.19",
          "form": "피티받고서는 익숙해지게 혼자 계속해보세요~ 트레이너 근처를 맴돌며ㅋㅋ그럼 관리중인 회원이라 자세 한두번 더 봐줘요",
          "original_form": "피티받고서는 익숙해지게 혼자 계속해보세요~ 트레이너 근처를 맴돌며ㅋㅋ그럼 관리중인 회원이라 자세 한두번 더 봐줘요",
          "speaker_id": "2",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100034562.1.20",
          "form": "와 꿀팁이네요 피티 몇 회나 받으셨나요",
          "original_form": "와 꿀팁이네요 피티 몇 회나 받으셨나요",
          "speaker_id": "1",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100034562.1.21",
          "form": "전 20번쯤?",
          "original_form": "전 20번쯤?",
          "speaker_id": "2",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100034562.1.22",
          "form": "자세 배우고 익히느라 천천히 받앗어요ㅎ",
          "original_form": "자세 배우고 익히느라 천천히 받앗어요ㅎ",
          "speaker_id": "2",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100034562.1.23",
          "form": "천천히면 일주일에 두번정도인가요?",
          "original_form": "천천히면 일주일에 두번정도인가요?",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100034562.1.24",
          "form": "넵~",
          "original_form": "넵~",
          "speaker_id": "2",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100034562.1.25",
          "form": "이주에 세번정도도ㅎㅎ",
          "original_form": "이주에 세번정도도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100034562.1.26",
          "form": "그냥 무작정 피티만 받으먼 나중에는 피티없으면 혼자못하게되서요ㅎ",
          "original_form": "그냥 무작정 피티만 받으먼 나중에는 피티없으면 혼자못하게되서요ㅎ",
          "speaker_id": "2",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100034562.1.27",
          "form": "오ㅓ 진짜 텀을 길게 가지셨네요",
          "original_form": "오ㅓ 진짜 텀을 길게 가지셨네요",
          "speaker_id": "1",
          "time": "20210828 17:36"
        }
      ]
    }
