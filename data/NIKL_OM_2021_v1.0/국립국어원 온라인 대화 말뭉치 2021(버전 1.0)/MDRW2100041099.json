      "utterance": [
        {
          "id": "MDRW2100041099.1.1",
          "form": "게임하시는거 있으세용?",
          "original_form": "게임하시는거 있으세용?",
          "speaker_id": "1",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100041099.1.2",
          "form": "아뇨 하면 중독이라 손에안댐....",
          "original_form": "아뇨 하면 중독이라 손에안댐....",
          "speaker_id": "2",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100041099.1.3",
          "form": "헉 뭐 오래 하셨나봐여",
          "original_form": "헉 뭐 오래 하셨나봐여",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100041099.1.4",
          "form": "ㅋㅋㅋㅋ 오래는 아니더라도 하루종일....",
          "original_form": "ㅋㅋㅋㅋ 오래는 아니더라도 하루종일....",
          "speaker_id": "2",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100041099.1.5",
          "form": "최근은 메이플",
          "original_form": "최근은 메이플",
          "speaker_id": "2",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100041099.1.6",
          "form": "메이플! 저 메이플은 안해보고 예전에 큐링이라고",
          "original_form": "메이플! 저 메이플은 안해보고 예전에 큐링이라고",
          "speaker_id": "1",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100041099.1.7",
          "form": "메이플 짝퉁 했었어요",
          "original_form": "메이플 짝퉁 했었어요",
          "speaker_id": "1",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100041099.1.8",
          "form": "어 ㅋㅋㅋ 뭐지첨들어요",
          "original_form": "어 ㅋㅋㅋ 뭐지첨들어요",
          "speaker_id": "2",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100041099.1.9",
          "form": "요즘은요?",
          "original_form": "요즘은요?",
          "speaker_id": "2",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100041099.1.10",
          "form": "그쵸 ㅋㅋㅋㅋ 망겜이에요 그거",
          "original_form": "그쵸 ㅋㅋㅋㅋ 망겜이에요 그거",
          "speaker_id": "1",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100041099.1.11",
          "form": "섭종된 ㅠ",
          "original_form": "섭종된 ㅠ",
          "speaker_id": "1",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100041099.1.12",
          "form": "ㅋㅋㅋㅋㅋ어디서만들엇죠",
          "original_form": "ㅋㅋㅋㅋㅋ어디서만들엇죠",
          "speaker_id": "2",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100041099.1.13",
          "form": "요즘은 그냥 고양이자리 이런거 해요",
          "original_form": "요즘은 그냥 고양이자리 이런거 해요",
          "speaker_id": "1",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100041099.1.14",
          "form": "거기 아마 망햇을 걸요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "거기 아마 망햇을 걸요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100041099.1.15",
          "form": "고양이자리는 또머에요",
          "original_form": "고양이자리는 또머에요",
          "speaker_id": "2",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100041099.1.16",
          "form": "그것도 망겜이긴 한데",
          "original_form": "그것도 망겜이긴 한데",
          "speaker_id": "1",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100041099.1.17",
          "form": "고양이 키우는 겜이에요!!!",
          "original_form": "고양이 키우는 겜이에요!!!",
          "speaker_id": "1",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100041099.1.18",
          "form": "앗귀엽당",
          "original_form": "앗귀엽당",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100041099.1.19",
          "form": "폰겜요?",
          "original_form": "폰겜요?",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100041099.1.20",
          "form": "넹 맞아요",
          "original_form": "넹 맞아요",
          "speaker_id": "1",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100041099.1.21",
          "form": "넘 기여워요 ㅋㅋㅋㅋ",
          "original_form": "넘 기여워요 ㅋㅋㅋㅋ",
