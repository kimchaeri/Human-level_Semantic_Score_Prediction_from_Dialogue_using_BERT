      "utterance": [
        {
          "id": "MDRW2100026396.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026396.1.2",
          "form": "반갑습니다",
          "original_form": "반갑습니다",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026396.1.3",
          "form": "집에오시면 멀 젤먼저하시나요",
          "original_form": "집에오시면 멀 젤먼저하시나요",
          "speaker_id": "1",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026396.1.4",
          "form": "상황에 따라 다르긴한데,대체로 청소부터 해요.",
          "original_form": "상황에 따라 다르긴한데,대체로 청소부터 해요.",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026396.1.5",
          "form": "와 저는 씻기부터해요 손씻는겸해서",
          "original_form": "와 저는 씻기부터해요 손씻는겸해서",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026396.1.6",
          "form": "철저하시네요",
          "original_form": "철저하시네요",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026396.1.7",
          "form": "철저까지는 아니고,자기가 제일 못 견디는것부터 하게 되는것같아요.",
          "original_form": "철저까지는 아니고,자기가 제일 못 견디는것부터 하게 되는것같아요.",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026396.1.8",
          "form": "보이는것부터",
          "original_form": "보이는것부터",
          "speaker_id": "1",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100026396.1.9",
          "form": "차근히하시는군요",
          "original_form": "차근히하시는군요",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026396.1.10",
          "form": "멋집니다",
          "original_form": "멋집니다",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026396.1.11",
          "form": "무슨 그런 해석이..과찬이십니다.ㅎㅎ",
          "original_form": "무슨 그런 해석이..과찬이십니다.ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026396.1.12",
          "form": "저는 청소안하고 씻고 폰하고 뒹굴거든요ㅎㅎ",
          "original_form": "저는 청소안하고 씻고 폰하고 뒹굴거든요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026396.1.13",
          "form": "음..그런거면 ..틀린말이 아니겠네요.",
          "original_form": "음..그런거면 ..틀린말이 아니겠네요.",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026396.1.14",
          "form": "어제 방에 비새가지구ㅜㅜ방닦아놔서 뒹굴만해요ㅜㅜㅜ",
          "original_form": "어제 방에 비새가지구ㅜㅜ방닦아놔서 뒹굴만해요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026396.1.15",
          "form": "혼자 사시나봐요.아님,누군가 희생(^^)하시는 가족분이 계시던가..",
          "original_form": "혼자 사시나봐요.아님,누군가 희생(^^)하시는 가족분이 계시던가..",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026396.1.16",
          "form": "아..저런~",
          "original_form": "아..저런~",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026396.1.17",
          "form": "혼자 살아요",
          "original_form": "혼자 살아요",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026396.1.18",
          "form": "이번주에 비가 계속 올듯 하니 얼른 조치를 취하셔야될듯 하네요",
          "original_form": "이번주에 비가 계속 올듯 하니 얼른 조치를 취하셔야될듯 하네요",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026396.1.19",
          "form": "비가 자꾸오네요ㅋㅋ 비새는데",
          "original_form": "비가 자꾸오네요ㅋㅋ 비새는데",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026396.1.20",
          "form": "고쳤는데ㅜ잘안됐나봐용",
          "original_form": "고쳤는데ㅜ잘안됐나봐용",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026396.1.21",
          "form": "그렇다면 큰일이네요.이사 가세요~",
          "original_form": "그렇다면 큰일이네요.이사 가세요~",
          "speaker_id": "2",
          "time": "20210901 23:19"
        }
      ]
