      "utterance": [
        {
          "id": "MDRW2100045664.1.1",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "2",
          "time": "20210908 04:59"
        },
        {
          "id": "MDRW2100045664.1.2",
          "form": "로제 떡볶이 드셔보셨어요?",
          "original_form": "로제 떡볶이 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210908 04:59"
        },
        {
          "id": "MDRW2100045664.1.3",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210908 05:00"
        },
        {
          "id": "MDRW2100045664.1.4",
          "form": "칼로리 아시나요",
          "original_form": "칼로리 아시나요",
          "speaker_id": "1",
          "time": "20210908 05:01"
        },
        {
          "id": "MDRW2100045664.1.5",
          "form": "ㅎㅁㅎ",
          "original_form": "ㅎㅁㅎ",
          "speaker_id": "1",
          "time": "20210908 05:01"
        },
        {
          "id": "MDRW2100045664.1.6",
          "form": "아앗...그것은...ㅠㅠㅠ",
          "original_form": "아앗...그것은...ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:01"
        },
        {
          "id": "MDRW2100045664.1.7",
          "form": "금기어로 지정해요 칼로리!",
          "original_form": "금기어로 지정해요 칼로리!",
          "speaker_id": "2",
          "time": "20210908 05:01"
        },
        {
          "id": "MDRW2100045664.1.8",
          "form": "즉떡 먹고 치즈밥도 너무 맛있어요",
          "original_form": "즉떡 먹고 치즈밥도 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210908 05:01"
        },
        {
          "id": "MDRW2100045664.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100045664.1.10",
          "form": "치즈밥은 뭐에요?",
          "original_form": "치즈밥은 뭐에요?",
          "speaker_id": "1",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100045664.1.11",
          "form": "즉석떡볶이 먹고나서 남은 양념에 치즈랑 밥이랑해서 볶아주거든요, 그게 존맛이에요",
          "original_form": "즉석떡볶이 먹고나서 남은 양념에 치즈랑 밥이랑해서 볶아주거든요, 그게 존맛이에요",
          "speaker_id": "2",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100045664.1.12",
          "form": "오!! 볶음밥 아니에요?",
          "original_form": "오!! 볶음밥 아니에요?",
          "speaker_id": "1",
          "time": "20210908 05:03"
        },
        {
          "id": "MDRW2100045664.1.13",
          "form": "네 맞아요ㅠㅠ 치즈볶음밥...그냥 볶음밥도 맛있는데 치즈가 올라가서 더 맛있어요",
          "original_form": "네 맞아요ㅠㅠ 치즈볶음밥...그냥 볶음밥도 맛있는데 치즈가 올라가서 더 맛있어요",
          "speaker_id": "2",
          "time": "20210908 05:04"
        },
        {
          "id": "MDRW2100045664.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ name2님 침 흐르는거아니에요?!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ &name2&님 침 흐르는거아니에요?!",
          "speaker_id": "1",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045664.1.15",
          "form": "완전 배고파졌어요ㅠㅠㅠㅠ",
          "original_form": "완전 배고파졌어요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045664.1.16",
          "form": "저두여... 오늘 뭐드실거에요",
          "original_form": "저두여... 오늘 뭐드실거에요",
          "speaker_id": "1",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045664.1.17",
          "form": "아침은 귀찮으니까 시리얼 먹구...점심은 고민되네요...name1님은요?!",
          "original_form": "아침은 귀찮으니까 시리얼 먹구...점심은 고민되네요...&name1&님은요?!",
          "speaker_id": "2",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045664.1.18",
          "form": "저는 점심은 돈까스 먹을거에요!!",
          "original_form": "저는 점심은 돈까스 먹을거에요!!",
          "speaker_id": "1",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045664.1.19",
          "form": "바삭한 돈가스",
          "original_form": "바삭한 돈가스",
          "speaker_id": "1",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045664.1.20",
          "form": "헉 완전 맛있겠다ㅠㅠ",
          "original_form": "헉 완전 맛있겠다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:10"
        },
        {
          "id": "MDRW2100045664.1.21",
          "form": "요즘 비와서 그런가 바삭한 게 땡기네요",
          "original_form": "요즘 비와서 그런가 바삭한 게 땡기네요",
          "speaker_id": "2",
          "time": "20210908 05:10"
        },
        {
          "id": "MDRW2100045664.1.22",
          "form": "뜨끈한것두요!",
          "original_form": "뜨끈한것두요!",
          "speaker_id": "1",
