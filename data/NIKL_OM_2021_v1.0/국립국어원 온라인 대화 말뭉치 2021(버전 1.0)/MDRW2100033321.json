        },
        {
          "id": "MDRW2100033321.1.7",
          "form": "이디야 가서 플랫치노",
          "original_form": "이디야 가서 플랫치노",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100033321.1.8",
          "form": "왜 눈물 나냐",
          "original_form": "왜 눈물 나냐",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100033321.1.9",
          "form": "아 그건 존맛",
          "original_form": "아 그건 존맛",
          "speaker_id": "1",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100033321.1.10",
          "form": "마시려다 그냥 초코유유 마심",
          "original_form": "마시려다 그냥 초코유유 마심",
