      "utterance": [
        {
          "id": "MDRW2100045974.1.1",
          "form": "요새 뭐 맛있는거 잇나요?",
          "original_form": "요새 뭐 맛있는거 잇나요?",
          "speaker_id": "2",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045974.1.2",
          "form": "그래서 시험기간에는 박카스같은거 나눠마셔요 ㅋㅋㅋ",
          "original_form": "그래서 시험기간에는 박카스같은거 나눠마셔요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100045974.1.3",
          "form": "음 저는 ...! 피자 파스타 좋아해요!",
          "original_form": "음 저는 ...! 피자 파스타 좋아해요!",
          "speaker_id": "1",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100045974.1.4",
          "form": "양식파군요!",
          "original_form": "양식파군요!",
          "speaker_id": "2",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100045974.1.5",
          "form": "요즘은 두유파스타 해먹고있아요",
          "original_form": "요즘은 두유파스타 해먹고있아요",
          "speaker_id": "1",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100045974.1.6",
          "form": "나쁘지 않아요 ㅋㅋㅋㅋ",
          "original_form": "나쁘지 않아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100045974.1.7",
          "form": "와~~",
          "original_form": "와~~",
          "speaker_id": "2",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100045974.1.8",
          "form": "전 안먹아봤어요!",
          "original_form": "전 안먹아봤어요!",
          "speaker_id": "2",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100045974.1.9",
          "form": "정말 간단하게 할 수 있는데",
          "original_form": "정말 간단하게 할 수 있는데",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100045974.1.10",
          "form": "맛있게 하려면 이것저것 필요하긴해요 ㅋㅋㅋㅋ",
          "original_form": "맛있게 하려면 이것저것 필요하긴해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100045974.1.11",
          "form": "그래도 저는 귀찮아서 두유만 넣어요..",
          "original_form": "그래도 저는 귀찮아서 두유만 넣어요..",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100045974.1.12",
          "form": "전  면은 다 좋아해요!!",
          "original_form": "전  면은 다 좋아해요!!",
          "speaker_id": "2",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100045974.1.13",
          "form": "그럼 한번 시도해보세요! ㅎㅎㅎ",
          "original_form": "그럼 한번 시도해보세요! ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045974.1.14",
          "form": "두유만 있으면 돼요... 소금이랑...",
          "original_form": "두유만 있으면 돼요... 소금이랑...",
          "speaker_id": "1",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045974.1.15",
          "form": "버섯도 넣으면 좋고...",
          "original_form": "버섯도 넣으면 좋고...",
          "speaker_id": "1",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045974.1.16",
          "form": "상상이안가요~",
          "original_form": "상상이안가요~",
          "speaker_id": "2",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045974.1.17",
          "form": "정말 두유맛인데...ㅋㅋㅋㅋㅋ",
          "original_form": "정말 두유맛인데...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045974.1.18",
          "form": "어떻게 설명해야할까요",
          "original_form": "어떻게 설명해야할까요",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045974.1.19",
          "form": "콩맛 고소함의 크림스파게티??",
          "original_form": "콩맛 고소함의 크림스파게티??",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045974.1.20",
          "form": "담에",
          "original_form": "담에",
          "speaker_id": "2",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045974.1.21",
          "form": "한번해볼께요 ㅎㅎ!",
          "original_form": "한번해볼께요 ㅎㅎ!",
          "speaker_id": "2",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045974.1.22",
          "form": "다른 분이 한것도 궁금하네요!",
          "original_form": "다른 분이 한것도 궁금하네요!",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045974.1.23",
          "form": "사실 식당가서는 한번도 안먹어보ㅓㅆ거는요 ㅋㅋㅋ",
          "original_form": "사실 식당가서는 한번도 안먹어보ㅓㅆ거는요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045974.1.24",
          "form": "훨씬 맛있겠죠...!?",
          "original_form": "훨씬 맛있겠죠...!?",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045974.1.25",
          "form": "두유도 다양햐서",
          "original_form": "두유도 다양햐서",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045974.1.26",
          "form": "제일 기본을 사야겟죠!?",
          "original_form": "제일 기본을 사야겟죠!?",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045974.1.27",
          "form": "남은해준건 다맛잇어요ㅠㅠ ㅋㅋ",
          "original_form": "남은해준건 다맛잇어요ㅠㅠ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:21"
        }
      ]
