      "utterance": [
        {
          "id": "MDRW2100047649.1.1",
          "form": "꼬깔콘 무슨맛 좋아하세요?",
          "original_form": "꼬깔콘 무슨맛 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100047649.1.2",
          "form": "저는 고소한맛",
          "original_form": "저는 고소한맛{emoji:☆}",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100047649.1.3",
          "form": "빨간포장이 제일 무난한것 같아요!",
          "original_form": "빨간포장이 제일 무난한것 같아요!",
          "speaker_id": "1",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100047649.1.4",
          "form": "오 제일 기본 좋아하시는구나",
          "original_form": "오 제일 기본 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100047649.1.5",
          "form": "네네~~ 꼬깔콘 사서 드세오?",
          "original_form": "네네~~ 꼬깔콘 사서 드세오?",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100047649.1.6",
          "form": "네 꼬깔콘 좋아해요",
          "original_form": "네 꼬깔콘 좋아해요",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100047649.1.7",
          "form": "군옥수수맛!!",
          "original_form": "군옥수수맛!!",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100047649.1.8",
          "form": "갈색이죠 그건!!!",
          "original_form": "갈색이죠 그건!!!",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100047649.1.9",
          "form": "네 갈색!!",
          "original_form": "네 갈색!!",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100047649.1.10",
          "form": "오~ 그건 군옥수수맛이 진짜 나나요?",
          "original_form": "오~ 그건 군옥수수맛이 진짜 나나요?",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100047649.1.11",
          "form": "그럼 그것도 고소할거같은데 맛이 큰차이가 있어서 나눠놓은건가...?",
          "original_form": "그럼 그것도 고소할거같은데 맛이 큰차이가 있어서 나눠놓은건가...?",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100047649.1.12",
          "form": "좀더 짭짤해요",
          "original_form": "좀더 짭짤해요",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100047649.1.13",
          "form": "아~~ 차이가 있구나",
          "original_form": "아~~ 차이가 있구나",
          "speaker_id": "1",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100047649.1.14",
          "form": "저는 고소한맛이 너무 맛있더라구요",
          "original_form": "저는 고소한맛이 너무 맛있더라구요",
          "speaker_id": "1",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100047649.1.15",
          "form": "저도 고소한맛도 좋아해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 고소한맛도 좋아해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100047649.1.16",
          "form": "요즘 네다섯번 사먹은거 같아요 ㅋㅋㅋ",
          "original_form": "요즘 네다섯번 사먹은거 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100047649.1.17",
          "form": "아아 찐으로 좋아하시는구나 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아아 찐으로 좋아하시는구나 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100047649.1.18",
          "form": "새로나온맛도 잇던데 새우마요맛",
          "original_form": "새로나온맛도 잇던데 새우마요맛",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100047649.1.19",
          "form": "아직 안먹어봣어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아직 안먹어봣어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100047649.1.20",
          "form": "전 name1님이 쌀국수에 진심인만큼 과자에 진심인 것 같아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 &name1&님이 쌀국수에 진심인만큼 과자에 진심인 것 같아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100047649.1.21",
          "form": "과자 진짜 좋아해욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "과자 진짜 좋아해욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100047649.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ오 꼬깔콘 맛이 새로나와썽요??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ오 꼬깔콘 맛이 새로나와썽요??",
          "speaker_id": "1",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100047649.1.23",
          "form": "새우마요!!!!! 오오",
          "original_form": "새우마요!!!!! 오오",
          "speaker_id": "1",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100047649.1.24",
          "form": "포장지색깔이 궁금해요 ㅋㅋㅋ",
          "original_form": "포장지색깔이 궁금해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100047649.1.25",
          "form": "흰색인가봐요 ㅋㅋㅋㅋㅋ",
          "original_form": "흰색인가봐요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:08"
        },
        {
          "id": "MDRW2100047649.1.26",
          "form": "저도 안먹어봐서 맛은 모름!",
          "original_form": "저도 안먹어봐서 맛은 모름!",
          "speaker_id": "2",
          "time": "20210827 00:09"
        },
        {
          "id": "MDRW2100047649.1.27",
          "form": "ㅋㅋㅋㅋㅋ저는 항상 두가지만 진열되어 있길래 두개가 끝인줄 알았어요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ저는 항상 두가지만 진열되어 있길래 두개가 끝인줄 알았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:10"
        },
        {
          "id": "MDRW2100047649.1.28",
          "form": "다른 맛도 더 있을걸요? 그 찰옥수수맛인가...",
          "original_form": "다른 맛도 더 있을걸요? 그 찰옥수수맛인가...",
          "speaker_id": "2",
