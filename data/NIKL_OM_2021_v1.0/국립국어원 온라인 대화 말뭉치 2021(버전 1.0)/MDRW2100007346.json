      "utterance": [
        {
          "id": "MDRW2100007346.1.1",
          "form": "혹시 어떤 음식 선호하시나요?",
          "original_form": "혹시 어떤 음식 선호하시나요?",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007346.1.2",
          "form": "저는 간장베이스의 맛을 좋아해요~",
          "original_form": "저는 간장베이스의 맛을 좋아해요~",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007346.1.3",
          "form": "볶음 우동... 교촌치킨 등등",
          "original_form": "볶음 우동... 교촌치킨 등등",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007346.1.4",
          "form": "오 우동",
          "original_form": "오 우동",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007346.1.5",
          "form": "저 우동 진짜 좋아해요",
          "original_form": "저 우동 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007346.1.6",
          "form": "하루한끼님 볶음 우동에 마요네즈 뿌려드셔보세요!!",
          "original_form": "하루한끼님 볶음 우동에 마요네즈 뿌려드셔보세요!!",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007346.1.7",
          "form": "제 야식 최애메뉴 넘 맛나요",
          "original_form": "제 야식 최애메뉴 넘 맛나요",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007346.1.8",
          "form": "우와 진짜 맛있겠어요",
          "original_form": "우와 진짜 맛있겠어요",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007346.1.9",
          "form": "볶음우동은 댁에서 만들어드시나요?",
          "original_form": "볶음우동은 댁에서 만들어드시나요?",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007346.1.10",
          "form": "아니면 반조리로 사와서 조리만?",
          "original_form": "아니면 반조리로 사와서 조리만?",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007346.1.11",
          "form": "사누끼 냉동 우동 면을 사두고 가끔 먹고 싶을 때마다 조리해먹어요",
          "original_form": "사누끼 냉동 우동 면을 사두고 가끔 먹고 싶을 때마다 조리해먹어요",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007346.1.12",
          "form": "아 그러시구나~",
          "original_form": "아 그러시구나~",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007346.1.13",
          "form": "냉장 우동면은 삶으면 너무 불어버려서 식감이 안살더라구요ㅜㅜ",
          "original_form": "냉장 우동면은 삶으면 너무 불어버려서 식감이 안살더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007346.1.14",
          "form": "야식으로 딱이네요 진짜",
          "original_form": "야식으로 딱이네요 진짜",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007346.1.15",
          "form": "맞아요 그 타이밍 진짜 중요하죠",
          "original_form": "맞아요 그 타이밍 진짜 중요하죠",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007346.1.16",
          "form": "name1님은 냉동 우동 면 드세요",
          "original_form": "&name1&님은 냉동 우동 면 드세요",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007346.1.17",
          "form": "훨 맛나요! 저 자취 경력 2년! 믿고 드세요우!",
          "original_form": "훨 맛나요! 저 자취 경력 2년! 믿고 드세요우!",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007346.1.18",
          "form": "오 자취2년차 분 말씀이면 진짜 믿먹!",
          "original_form": "오 자취2년차 분 말씀이면 진짜 믿먹!",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007346.1.19",
          "form": "꼭 해먹을게요",
          "original_form": "꼭 해먹을게요",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007346.1.20",
          "form": "마요네즈는 그냥 오뚜기꺼 사용하시나옹?",
          "original_form": "마요네즈는 그냥 오뚜기꺼 사용하시나옹?",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007346.1.21",
          "form": "네! 오뚜기 마요가 칼로리 높은만큼 왕 고소해요",
          "original_form": "네! 오뚜기 마요가 칼로리 높은만큼 왕 고소해요",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007346.1.22",
          "form": "ㅎㅎㅎㅎ 칼로리 높은게 나름 맛의 지표가 되니까요 ㅋㅋㅋ",
          "original_form": "ㅎㅎㅎㅎ 칼로리 높은게 나름 맛의 지표가 되니까요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007346.1.23",
          "form": "맛있는걸 먹으려면 고칼로리로!",
          "original_form": "맛있는걸 먹으려면 고칼로리로!",
          "speaker_id": "1",
          "time": "20210822 01:44"
        }
      ]
    }
