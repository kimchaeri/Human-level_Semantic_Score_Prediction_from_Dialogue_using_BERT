      "utterance": [
        {
          "id": "MDRW2100037569.1.1",
          "form": "챙겨드시는 영양제는 있나요?",
          "original_form": "챙겨드시는 영양제는 있나요?",
          "speaker_id": "1",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100037569.1.2",
          "form": "오메가3 정도요?",
          "original_form": "오메가3 정도요?",
          "speaker_id": "2",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100037569.1.3",
          "form": "근데 자주 깜박해요",
          "original_form": "근데 자주 깜박해요",
          "speaker_id": "2",
          "time": "20210901 12:04"
        },
        {
          "id": "MDRW2100037569.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210901 12:04"
        },
        {
          "id": "MDRW2100037569.1.5",
          "form": "저는 요새 비타민씨를 먹어요",
          "original_form": "저는 요새 비타민씨를 먹어요",
          "speaker_id": "1",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100037569.1.6",
          "form": "원래 안먹다가 코로나 이후로 면역에 신경썯야 할 거 같아서요",
          "original_form": "원래 안먹다가 코로나 이후로 면역에 신경썯야 할 거 같아서요",
          "speaker_id": "1",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100037569.1.7",
          "form": "아 코로나 이후로 좀 경각심이 들죶ㅎ",
          "original_form": "아 코로나 이후로 좀 경각심이 들죶ㅎ",
          "speaker_id": "2",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100037569.1.8",
          "form": "백신은 맞으셨나요?",
          "original_form": "백신은 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100037569.1.9",
          "form": "저 다담주에 맞아요",
          "original_form": "저 다담주에 맞아요",
          "speaker_id": "1",
          "time": "20210901 12:08"
        },
        {
          "id": "MDRW2100037569.1.10",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210901 12:08"
        },
        {
          "id": "MDRW2100037569.1.11",
          "form": "전 내일 맞아요! 화이자요",
          "original_form": "전 내일 맞아요! 화이자요",
          "speaker_id": "2",
          "time": "20210901 12:08"
        },
        {
          "id": "MDRW2100037569.1.12",
          "form": "첫백신이라 두근두근하네요",
          "original_form": "첫백신이라 두근두근하네요",
          "speaker_id": "2",
          "time": "20210901 12:08"
        },
        {
          "id": "MDRW2100037569.1.13",
          "form": "헐.. 내일요? 와 오늘 고기 잘 드셔야겠어요.. 든든하게",
          "original_form": "헐.. 내일요? 와 오늘 고기 잘 드셔야겠어요.. 든든하게",
          "speaker_id": "1",
          "time": "20210901 12:10"
        },
        {
          "id": "MDRW2100037569.1.14",
          "form": "저는 아직 안정해졌지만 화이자나 모더나라는데.. 아마도 화이자일듯 해요",
          "original_form": "저는 아직 안정해졌지만 화이자나 모더나라는데.. 아마도 화이자일듯 해요",
          "speaker_id": "1",
          "time": "20210901 12:10"
        },
        {
          "id": "MDRW2100037569.1.15",
          "form": "대부분 화이자더라고요",
          "original_form": "대부분 화이자더라고요",
          "speaker_id": "2",
          "time": "20210901 12:10"
        },
        {
          "id": "MDRW2100037569.1.16",
          "form": "모더나가 델타변이에 더 강하대서 살짝 기대했는데",
          "original_form": "모더나가 델타변이에 더 강하대서 살짝 기대했는데",
          "speaker_id": "2",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100037569.1.17",
          "form": "네. 물량때문에 저도 화이자 같아요",
          "original_form": "네. 물량때문에 저도 화이자 같아요",
          "speaker_id": "1",
          "time": "20210901 12:13"
        },
        {
          "id": "MDRW2100037569.1.18",
          "form": "근데 화이자가 대기업이라 좀 더 안전할 거 같아요",
          "original_form": "근데 화이자가 대기업이라 좀 더 안전할 거 같아요",
          "speaker_id": "1",
          "time": "20210901 12:13"
        },
        {
          "id": "MDRW2100037569.1.19",
          "form": "하긴 좀더 검증되기는 했죠",
          "original_form": "하긴 좀더 검증되기는 했죠",
          "speaker_id": "2",
          "time": "20210901 12:15"
        },
        {
          "id": "MDRW2100037569.1.20",
          "form": "더 많은 사람이 맞아서ㅎ",
          "original_form": "더 많은 사람이 맞아서ㅎ",
          "speaker_id": "2",
          "time": "20210901 12:15"
        },
        {
          "id": "MDRW2100037569.1.21",
          "form": "일장일단이 있네요",
          "original_form": "일장일단이 있네요",
          "speaker_id": "2",
          "time": "20210901 12:15"
        },
        {
          "id": "MDRW2100037569.1.22",
          "form": "네.. 표본이 많대요",
          "original_form": "네.. 표본이 많대요",
          "speaker_id": "1",
          "time": "20210901 12:17"
        },
        {
          "id": "MDRW2100037569.1.23",
          "form": "그리고 어쩜 부스터샷도 맞을 수 있는데 그때도 교차접종이 아니라 같은 화이자 맞으면 좋으니까요",
          "original_form": "그리고 어쩜 부스터샷도 맞을 수 있는데 그때도 교차접종이 아니라 같은 화이자 맞으면 좋으니까요",
          "speaker_id": "1",
          "time": "20210901 12:17"
        },
        {
          "id": "MDRW2100037569.1.24",
          "form": "부스터샷까지.. 사태가 정말 오래가네요ㅜ",
          "original_form": "부스터샷까지.. 사태가 정말 오래가네요ㅜ",
          "speaker_id": "2",
          "time": "20210901 12:18"
        },
        {
          "id": "MDRW2100037569.1.25",
          "form": "얼른 끝났음 좋겠어요",
          "original_form": "얼른 끝났음 좋겠어요",
          "speaker_id": "2",
          "time": "20210901 12:19"
        },
        {
          "id": "MDRW2100037569.1.26",
          "form": "계속 몸사리게 돼서ㅜ 감기로도 흠칫하고",
          "original_form": "계속 몸사리게 돼서ㅜ 감기로도 흠칫하고",
          "speaker_id": "2",
          "time": "20210901 12:19"
        },
        {
          "id": "MDRW2100037569.1.27",
          "form": "이시국엔 감기 절리면 안돼죠.. 눈치보이고..",
          "original_form": "이시국엔 감기 절리면 안돼죠.. 눈치보이고..",
          "speaker_id": "1",
          "time": "20210901 12:21"
        }
      ]
