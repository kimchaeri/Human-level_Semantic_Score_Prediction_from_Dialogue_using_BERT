      "utterance": [
        {
          "id": "MDRW2100006850.1.1",
          "form": "밥이랑 국이 일반적인데",
          "original_form": "밥이랑 국이 일반적인데",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006850.1.2",
          "form": "속이 편한 음식이 좋겠어요",
          "original_form": "속이 편한 음식이 좋겠어요",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006850.1.3",
          "form": "계란후라이 어때요",
          "original_form": "계란후라이 어때요",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006850.1.4",
          "form": "좋죠 간장계란밥 아침으로 좋아요",
          "original_form": "좋죠 간장계란밥 아침으로 좋아요",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006850.1.5",
          "form": "김에 밥 싸먹어도 좋구요",
          "original_form": "김에 밥 싸먹어도 좋구요",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006850.1.6",
          "form": "간장이랑 김 있어도 한공기 뚝딱이지요",
          "original_form": "간장이랑 김 있어도 한공기 뚝딱이지요",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006850.1.7",
          "form": "저도요 김말이 좋아해요",
          "original_form": "저도요 김말이 좋아해요",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006850.1.8",
          "form": "국은 어떤것이 좋을까요?",
          "original_form": "국은 어떤것이 좋을까요?",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006850.1.9",
          "form": "음.   미역국?",
          "original_form": "음.   미역국?",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006850.1.10",
          "form": "된장국도 좋고 계란국도 좋겠어요",
          "original_form": "된장국도 좋고 계란국도 좋겠어요",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006850.1.11",
          "form": "저는 고기도 좋아요",
          "original_form": "저는 고기도 좋아요",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100006850.1.12",
          "form": "네 삼겹살도 아침으로 좋아요",
          "original_form": "네 삼겹살도 아침으로 좋아요",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100006850.1.13",
          "form": "고소한 고기 맛있지요",
          "original_form": "고소한 고기 맛있지요",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100006850.1.14",
          "form": "네 재첩국도 속 편해요",
          "original_form": "네 재첩국도 속 편해요",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100006850.1.15",
          "form": "아침으로 샌드위치도 좋아요",
          "original_form": "아침으로 샌드위치도 좋아요",
          "speaker_id": "1",
          "time": "20210908 10:47"
        }
