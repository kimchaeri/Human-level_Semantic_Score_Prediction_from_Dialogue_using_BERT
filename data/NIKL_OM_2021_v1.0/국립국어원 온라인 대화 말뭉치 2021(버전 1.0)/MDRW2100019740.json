      "utterance": [
        {
          "id": "MDRW2100019740.1.1",
          "form": "최근 구매한 물건이 있으십니까?",
          "original_form": "최근 구매한 물건이 있으십니까?",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019740.1.2",
          "form": "네 모자 몇개 샀어요",
          "original_form": "네 모자 몇개 샀어요",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019740.1.3",
          "form": "캡이랑 비니요",
          "original_form": "캡이랑 비니요",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019740.1.4",
          "form": "저는 요거트를 구매했습니다",
          "original_form": "저는 요거트를 구매했습니다",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019740.1.5",
          "form": "무슨 요거트요?",
          "original_form": "무슨 요거트요?",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019740.1.6",
          "form": "초코과자랑 섞어먹는 것이였는데 1+1 이라서 구매해버렸습니다",
          "original_form": "초코과자랑 섞어먹는 것이였는데 1+1 이라서 구매해버렸습니다",
          "speaker_id": "1",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019740.1.7",
          "form": "ㅋㅋㅋ 혹시 비요뜨 말씀하시는건가요?",
          "original_form": "ㅋㅋㅋ 혹시 비요뜨 말씀하시는건가요?",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019740.1.8",
          "form": "정답입니다",
          "original_form": "정답입니다",
          "speaker_id": "1",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100019740.1.9",
          "form": "저도 비요뜨 좋아해요",
          "original_form": "저도 비요뜨 좋아해요",
          "speaker_id": "2",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100019740.1.10",
          "form": "비요트랑 비슷한데 토핑도 맛있어요",
          "original_form": "비요트랑 비슷한데 토핑도 맛있어요",
          "speaker_id": "2",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019740.1.11",
          "form": "요거트 종류는 다 맛있더라고요",
          "original_form": "요거트 종류는 다 맛있더라고요",
          "speaker_id": "1",
          "time": "20210827 21:49"
        },
        {
          "id": "MDRW2100019740.1.12",
          "form": "그렇죠 전 최근에 그릭요거트를 만들어봤어요",
          "original_form": "그렇죠 전 최근에 그릭요거트를 만들어봤어요",
          "speaker_id": "2",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100019740.1.13",
          "form": "재료들 사가지고",
          "original_form": "재료들 사가지고",
          "speaker_id": "2",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100019740.1.14",
          "form": "맛은 어떠셨나요",
          "original_form": "맛은 어떠셨나요",
          "speaker_id": "1",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100019740.1.15",
          "form": "맛잇었죠 근데 이제 귀찮아서 못하겠어요 ㅎㅎ",
          "original_form": "맛잇었죠 근데 이제 귀찮아서 못하겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019740.1.16",
          "form": "맞아요...사먹는게 편하더라고요 맛도좋고",
          "original_form": "맞아요...사먹는게 편하더라고요 맛도좋고",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019740.1.17",
          "form": "다른거 사고 싶은건 없으세요?? 장볼때",
          "original_form": "다른거 사고 싶은건 없으세요?? 장볼때",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019740.1.18",
          "form": "과자종류를...주로 사네요",
          "original_form": "과자종류를...주로 사네요",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019740.1.19",
          "form": "백종원씨처럼 잘 만들어보고싶은데",
          "original_form": "백종원씨처럼 잘 만들어보고싶은데",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019740.1.20",
          "form": "음식하면 항상 망해서요",
          "original_form": "음식하면 항상 망해서요",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019740.1.21",
          "form": "무슨 과자 좋아하세요?",
          "original_form": "무슨 과자 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019740.1.22",
          "form": "저도 요리 유튜브 항상 구독만 하고 잘 하진 않는답니다 ㅎㅎ",
          "original_form": "저도 요리 유튜브 항상 구독만 하고 잘 하진 않는답니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019740.1.23",
          "form": "초코렛종류를 좋아해서 그걸 주로사요",
