      "utterance": [
        {
          "id": "MDRW2100016675.1.1",
          "form": "종교잇으신가요?",
          "original_form": "종교잇으신가요?",
          "speaker_id": "2",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016675.1.2",
          "form": "아뇨! name2님은 종교 있으신가요?",
          "original_form": "아뇨! &name2&님은 종교 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016675.1.3",
          "form": "무교입니당",
          "original_form": "무교입니당",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016675.1.4",
          "form": "가끔 놀러가서 절 가는건 좋아해요",
          "original_form": "가끔 놀러가서 절 가는건 좋아해요",
          "speaker_id": "2",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016675.1.5",
          "form": "저두요! 할머니따라 절에 가보고했어요 ㅎㅎ",
          "original_form": "저두요! 할머니따라 절에 가보고했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016675.1.6",
          "form": "제가 갔던 절에는 흔들다리가 있어서 무척 즐겼답니다",
          "original_form": "제가 갔던 절에는 흔들다리가 있어서 무척 즐겼답니다",
          "speaker_id": "1",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100016675.1.7",
          "form": "오?",
          "original_form": "오?",
          "speaker_id": "2",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100016675.1.8",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "2",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100016675.1.9",
          "form": "절에 흔들다리라니",
          "original_form": "절에 흔들다리라니",
          "speaker_id": "2",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100016675.1.10",
          "form": "그쵸? 산속에 있는 절이어서 그랬나봐요!",
          "original_form": "그쵸? 산속에 있는 절이어서 그랬나봐요!",
          "speaker_id": "1",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100016675.1.11",
          "form": "다른종교보다 불교는 편안한느낌이라 거부감이 없어요",
          "original_form": "다른종교보다 불교는 편안한느낌이라 거부감이 없어요",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100016675.1.12",
          "form": "맞아요! 포교활동이 적어서 부담스럽지 않아요!",
          "original_form": "맞아요! 포교활동이 적어서 부담스럽지 않아요!",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016675.1.13",
          "form": "제가 사회초년생때 다닌 회사 대표가 억지로 교회데려가서 정말 곤혹스러웠어요",
          "original_form": "제가 사회초년생때 다닌 회사 대표가 억지로 교회데려가서 정말 곤혹스러웠어요",
          "speaker_id": "1",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016675.1.14",
          "form": "하............",
          "original_form": "하............",
          "speaker_id": "2",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016675.1.15",
          "form": "저는 대표는아니지만",
          "original_form": "저는 대표는아니지만",
          "speaker_id": "2",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016675.1.16",
          "form": "초년생때 동료가.........",
          "original_form": "초년생때 동료가.........",
          "speaker_id": "2",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016675.1.17",
          "form": "앗 ㅠㅠㅠ 진짜 좋게 둘러말하면 못알아듣더라구요",
          "original_form": "앗 ㅠㅠㅠ 진짜 좋게 둘러말하면 못알아듣더라구요",
          "speaker_id": "1",
          "time": "20210826 15:30"
        },
        {
          "id": "MDRW2100016675.1.18",
          "form": "대표라서 진짜 곤혹ㅜ",
          "original_form": "대표라서 진짜 곤혹ㅜ",
          "speaker_id": "2",
          "time": "20210826 15:30"
        },
        {
          "id": "MDRW2100016675.1.19",
          "form": "그래서 한 번 따라갔었답니다 ....ㅎ",
          "original_form": "그래서 한 번 따라갔었답니다 ....ㅎ",
          "speaker_id": "1",
          "time": "20210826 15:30"
        },
        {
          "id": "MDRW2100016675.1.20",
          "form": "저는 동료한테 솔직히 말햇어요ㅜㅜ",
          "original_form": "저는 동료한테 솔직히 말햇어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 15:30"
        },
        {
          "id": "MDRW2100016675.1.21",
          "form": "제 소중한 일요일을 하루 날렸죠 ㅠㅠ",
          "original_form": "제 소중한 일요일을 하루 날렸죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100016675.1.22",
          "form": "잘빠져나와서 다행이에요!",
          "original_form": "잘빠져나와서 다행이에요!",
          "speaker_id": "1",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100016675.1.23",
          "form": "정말 기독교는....절레절레....",
          "original_form": "정말 기독교는....절레절레....",
          "speaker_id": "2",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100016675.1.24",
          "form": "저랑 안맞아요...",
          "original_form": "저랑 안맞아요...",
          "speaker_id": "2",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100016675.1.25",
          "form": "ㅎㅎㅎ저두요.... 남한테 강요하지 않는다면 존중하고싶은데",
          "original_form": "ㅎㅎㅎ저두요.... 남한테 강요하지 않는다면 존중하고싶은데",
          "speaker_id": "1",
