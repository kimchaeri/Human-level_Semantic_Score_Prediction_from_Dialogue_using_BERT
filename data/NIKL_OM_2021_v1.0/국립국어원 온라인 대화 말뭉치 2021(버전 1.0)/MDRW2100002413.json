      "utterance": [
        {
          "id": "MDRW2100002413.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210820 11:59"
        },
        {
          "id": "MDRW2100002413.1.2",
          "form": "안녕하세요! 게임 좋아하시나요?",
          "original_form": "안녕하세요! 게임 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100002413.1.3",
          "form": "저는 핸드폰게임정도만 주로합니다ㅎㅎ",
          "original_form": "저는 핸드폰게임정도만 주로합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100002413.1.4",
          "form": "게임좋아하시나요?",
          "original_form": "게임좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100002413.1.5",
          "form": "게임 엄청 좋아했었죠! 과거에ㅠㅠ",
          "original_form": "게임 엄청 좋아했었죠! 과거에ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002413.1.6",
          "form": "저는 롤 많이했어요 ㅋㅋ 핸드폰게임도 잠깐잠깐하고 그래요 ㅎㅎ",
          "original_form": "저는 롤 많이했어요 ㅋㅋ 핸드폰게임도 잠깐잠깐하고 그래요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002413.1.7",
          "form": "무슨게임하세요??",
          "original_form": "무슨게임하세요??",
          "speaker_id": "2",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002413.1.8",
          "form": "아 롤ㅋㅋ 좋아하시군요",
          "original_form": "아 롤ㅋㅋ 좋아하시군요",
          "speaker_id": "1",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002413.1.9",
          "form": "저는 카트라이더 잠깐합니다",
          "original_form": "저는 카트라이더 잠깐합니다",
          "speaker_id": "1",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002413.1.10",
          "form": "카트아시나요?",
          "original_form": "카트아시나요?",
          "speaker_id": "1",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002413.1.11",
          "form": "모바일카트 했었어요 ㅋㅋ 무지개장갑이었나그럴걸요",
          "original_form": "모바일카트 했었어요 ㅋㅋ 무지개장갑이었나그럴걸요",
          "speaker_id": "2",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002413.1.12",
          "form": "친구들이랑 같이하면 더 재밌더라구요 ㅋㅋ",
          "original_form": "친구들이랑 같이하면 더 재밌더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002413.1.13",
          "form": "잘하시나요??",
          "original_form": "잘하시나요??",
          "speaker_id": "2",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002413.1.14",
          "form": "아 저는 요즘가끔한답니다ㅋㅋㅋ",
          "original_form": "아 저는 요즘가끔한답니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002413.1.15",
          "form": "제기준잘하는거같아요ㅋㅋㅋ",
          "original_form": "제기준잘하는거같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002413.1.16",
          "form": "스피드전하시나요 템전하시나요?",
          "original_form": "스피드전하시나요 템전하시나요?",
          "speaker_id": "1",
          "time": "20210820 12:53"
        },
        {
          "id": "MDRW2100002413.1.17",
          "form": "오호 좋은 자신감이네요 ㅋㅋ 저도 한게임합니다ㅋ",
          "original_form": "오호 좋은 자신감이네요 ㅋㅋ 저도 한게임합니다ㅋ",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002413.1.18",
          "form": "피씨로 할때는 스피드전 좋아했는데",
          "original_form": "피씨로 할때는 스피드전 좋아했는데",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002413.1.19",
          "form": "모바일은 템전이 졸잼이더라고요 ㅋㅋ",
          "original_form": "모바일은 템전이 졸잼이더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002413.1.20",
          "form": "ㅋㅋㅋ저는 스피드전주로하는데 완전고수까진아니고 중수정도인듯요",
          "original_form": "ㅋㅋㅋ저는 스피드전주로하는데 완전고수까진아니고 중수정도인듯요",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002413.1.21",
          "form": "맞아요 템으로잡는게재밌죠ㅋㅋㅋ",
          "original_form": "맞아요 템으로잡는게재밌죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002413.1.22",
          "form": "오 스피드전 좋아하시는거보면 잘하실듯해요",
          "original_form": "오 스피드전 좋아하시는거보면 잘하실듯해요",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002413.1.23",
          "form": "레벨이어떻게되시나요 ㅋㅋ",
          "original_form": "레벨이어떻게되시나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002413.1.24",
          "form": "ㅋㅋㅋ레벨은기억이안나지만",
          "original_form": "ㅋㅋㅋ레벨은기억이안나지만",
          "speaker_id": "1",
          "time": "20210820 13:12"
        },
        {
          "id": "MDRW2100002413.1.25",
          "form": "스피드전생각나는맵",
          "original_form": "스피드전생각나는맵",
          "speaker_id": "1",
          "time": "20210820 13:12"
        },
        {
          "id": "MDRW2100002413.1.26",
          "form": "있으신가요?",
          "original_form": "있으신가요?",
          "speaker_id": "1",
          "time": "20210820 13:12"
        },
        {
          "id": "MDRW2100002413.1.27",
          "form": "어렸을때 피씨로 했을때보다 새로운 맵들이 많이나와서 어려운데 빌리지 손가락은 기억나요 ㅋㅋ",
          "original_form": "어렸을때 피씨로 했을때보다 새로운 맵들이 많이나와서 어려운데 빌리지 손가락은 기억나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:29"
        },
        {
          "id": "MDRW2100002413.1.28",
          "form": "어떤맵을 주로하시나요",
          "original_form": "어떤맵을 주로하시나요",
          "speaker_id": "2",
          "time": "20210820 13:29"
        },
        {
          "id": "MDRW2100002413.1.29",
          "form": "ㅋㅋㅋ저는",
          "original_form": "ㅋㅋㅋ저는",
          "speaker_id": "1",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002413.1.30",
          "form": "이어달리기주로해요ㅋㅋㅋㅋ",
          "original_form": "이어달리기주로해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002413.1.31",
          "form": "스피드전으로",
          "original_form": "스피드전으로",
          "speaker_id": "1",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002413.1.32",
          "form": "아 이어달리기라는것도 생겼나보네요 ㅎㅎ",
          "original_form": "아 이어달리기라는것도 생겼나보네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 13:38"
        }
      ]
    }
