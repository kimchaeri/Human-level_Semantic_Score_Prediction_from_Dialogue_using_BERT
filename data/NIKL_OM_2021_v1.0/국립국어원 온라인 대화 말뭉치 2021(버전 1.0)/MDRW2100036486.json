      "utterance": [
        {
          "id": "MDRW2100036486.1.1",
          "form": "음 자극적인건 좋아해",
          "original_form": "음 자극적인건 좋아해",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036486.1.2",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036486.1.3",
          "form": "나두 맵게 해먹는거 좋아해",
          "original_form": "나두 맵게 해먹는거 좋아해",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.4",
          "form": "순두부찌개 잘만드는거야? 그래서 물어본거야?",
          "original_form": "순두부찌개 잘만드는거야? 그래서 물어본거야?",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.5",
          "form": "아니아니",
          "original_form": "아니아니",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.6",
          "form": "그 안에 계란있어도 맛있어",
          "original_form": "그 안에 계란있어도 맛있어",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.7",
          "form": "말하니까 좀 배고프내",
          "original_form": "말하니까 좀 배고프내",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.8",
          "form": "순두부열라면이 요즘 유행하더라구",
          "original_form": "순두부열라면이 요즘 유행하더라구",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.9",
          "form": "그러게 나도 국물이 땡긴다",
          "original_form": "그러게 나도 국물이 땡긴다",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.10",
          "form": "우리학교 학식에 순두부찌개가 참 맛있었어",
          "original_form": "우리학교 학식에 순두부찌개가 참 맛있었어",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.11",
          "form": "그거 해먹어본적있어?",
          "original_form": "그거 해먹어본적있어?",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.12",
          "form": "지금 좀 배가고파",
          "original_form": "지금 좀 배가고파",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.13",
          "form": "먹는 얘길 왜 꺼낸거야?",
          "original_form": "먹는 얘길 왜 꺼낸거야?",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.14",
          "form": "흠 다음에 나도 데라고가서 먹여쥬ㅓ",
          "original_form": "흠 다음에 나도 데라고가서 먹여쥬ㅓ",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.15",
          "form": "아니 난 안먹어봤어",
          "original_form": "아니 난 안먹어봤어",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.16",
          "form": "그래 날 잡아야겠다.",
          "original_form": "그래 날 잡아야겠다.",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.17",
          "form": "커뮤니티에서 핫한 음식이라",
          "original_form": "커뮤니티에서 핫한 음식이라",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.18",
          "form": "너의 반응도 궁금했어",
          "original_form": "너의 반응도 궁금했어",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.19",
          "form": "순두부열라면 생각만해도 맛있네",
          "original_form": "순두부열라면 생각만해도 맛있네",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036486.1.20",
          "form": "먹어보고싶어",
          "original_form": "먹어보고싶어",
          "speaker_id": "2",
          "time": "20210830 23:45"
        }
      ]
    }
