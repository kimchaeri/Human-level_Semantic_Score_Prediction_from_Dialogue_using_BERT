      "utterance": [
        {
          "id": "MDRW2100044384.1.1",
          "form": "추위타세요?",
          "original_form": "추위타세요?",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044384.1.2",
          "form": "저는 추위보다 더위에 약한 편인 것 같아요",
          "original_form": "저는 추위보다 더위에 약한 편인 것 같아요",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044384.1.3",
          "form": "name1님은 추운 거랑 더운 거 중에 뭐가 더 나으세요?",
          "original_form": "&name1&님은 추운 거랑 더운 거 중에 뭐가 더 나으세요?",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044384.1.4",
          "form": "박빙인데",
          "original_form": "박빙인데",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044384.1.5",
          "form": "전더운거요",
          "original_form": "전더운거요",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044384.1.6",
          "form": "오 그러시구나 저랑 반대네요 저는 차라리 추우면 옷이라도 더 껴입으면 되니깐 추운 게 낫거든요",
          "original_form": "오 그러시구나 저랑 반대네요 저는 차라리 추우면 옷이라도 더 껴입으면 되니깐 추운 게 낫거든요",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044384.1.7",
          "form": "저는수족냉증이있어서요",
          "original_form": "저는수족냉증이있어서요",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044384.1.8",
          "form": "아 저도 약하게 있는 편이에요",
          "original_form": "아 저도 약하게 있는 편이에요",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044384.1.9",
          "form": "손은 어찌저찌 주무르면 괜찮은데",
          "original_form": "손은 어찌저찌 주무르면 괜찮은데",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044384.1.10",
          "form": "진짜 발은 양말을 두꺼운 거 신어도 시리죠 ㅜ",
          "original_form": "진짜 발은 양말을 두꺼운 거 신어도 시리죠 ㅜ",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044384.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044384.1.12",
          "form": "운동해도그뿐이더라고요",
          "original_form": "운동해도그뿐이더라고요",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044384.1.13",
          "form": "그니깐요 혈액순환이 원인이라곤 하던데",
          "original_form": "그니깐요 혈액순환이 원인이라곤 하던데",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044384.1.14",
          "form": "ㅜㅜ힝",
          "original_form": "ㅜㅜ힝",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044384.1.15",
          "form": "지압같은 것도 하시나요?",
          "original_form": "지압같은 것도 하시나요?",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044384.1.16",
          "form": "오 어디요?",
          "original_form": "오 어디요?",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044384.1.17",
          "form": "발이요 ㅋㅋㅋ 아치있는 부분 주물러 주면 좋다고 들었거든요",
          "original_form": "발이요 ㅋㅋㅋ 아치있는 부분 주물러 주면 좋다고 들었거든요",
