      "utterance": [
        {
          "id": "MDRW2100008590.1.1",
          "form": "요즘 어디맛집이 핫한가요",
          "original_form": "요즘 어디맛집이 핫한가요",
          "speaker_id": "1",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008590.1.2",
          "form": "블루보틀 카페 유명하더라고요",
          "original_form": "블루보틀 카페 유명하더라고요",
          "speaker_id": "2",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008590.1.3",
          "form": "오 맞아요. 줄도 길다고하던데 전 아직 한번도 못가봤어요",
          "original_form": "오 맞아요. 줄도 길다고하던데 전 아직 한번도 못가봤어요",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008590.1.4",
          "form": "저도 못가보고 얘기만 들었는데",
          "original_form": "저도 못가보고 얘기만 들었는데",
          "speaker_id": "2",
          "time": "20210822 17:01"
        },
        {
          "id": "MDRW2100008590.1.5",
          "form": "커피가 진짜 맛있다더라고요",
          "original_form": "커피가 진짜 맛있다더라고요",
          "speaker_id": "2",
          "time": "20210822 17:01"
        },
        {
          "id": "MDRW2100008590.1.6",
          "form": "맛난라떼 마시고싶네요.",
          "original_form": "맛난라떼 마시고싶네요.",
          "speaker_id": "1",
          "time": "20210822 17:01"
        },
        {
          "id": "MDRW2100008590.1.7",
          "form": "라떼 좋아하세요?",
          "original_form": "라떼 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008590.1.8",
          "form": "저는 아메리카노 파 ㅎ",
          "original_form": "저는 아메리카노 파 ㅎ",
          "speaker_id": "2",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100008590.1.9",
          "form": "집안일 하느라 답장이.늦었어요.ㅠ\r\n저도 얼죽아였는데 나이드니..... ㅋㅋ",
          "original_form": "집안일 하느라 답장이.늦었어요.ㅠ\r\n저도 얼죽아였는데 나이드니..... ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100008590.1.10",
          "form": "고생많으셨어요 ㅠㅠ",
          "original_form": "고생많으셨어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 07:01"
        },
        {
          "id": "MDRW2100008590.1.11",
          "form": "집안일 너무 힘들죠.. 이따 요 얘기도 해봐요 ㅎㅎ",
          "original_form": "집안일 너무 힘들죠.. 이따 요 얘기도 해봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 07:02"
        },
        {
          "id": "MDRW2100008590.1.12",
          "form": "요즘은 따아가 좋으신가요",
          "original_form": "요즘은 따아가 좋으신가요",
          "speaker_id": "2",
          "time": "20210823 07:02"
        },
        {
          "id": "MDRW2100008590.1.13",
          "form": "ㅎㅎ일찍일어나시나봐요!! \r\n저는 요즘 임신중이라 커피를 안마시고있아요 ㅜㅜ",
          "original_form": "ㅎㅎ일찍일어나시나봐요!! \r\n저는 요즘 임신중이라 커피를 안마시고있아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 19:57"
        },
        {
          "id": "MDRW2100008590.1.14",
          "form": "아아 글쿤뇽",
          "original_form": "아아 글쿤뇽",
          "speaker_id": "2",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100008590.1.15",
          "form": "축복이네요! 고생이 많으시겠어요",
          "original_form": "축복이네요! 고생이 많으시겠어요",
          "speaker_id": "2",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100008590.1.16",
          "form": "막땅기는 음식 없으새요?",
          "original_form": "막땅기는 음식 없으새요?",
          "speaker_id": "2",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100008590.1.17",
          "form": "땡기는게 그때 그때 달라요",
          "original_form": "땡기는게 그때 그때 달라요",
          "speaker_id": "1",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100008590.1.18",
          "form": "신기하더라구요.ㅎㅎ",
          "original_form": "신기하더라구요.ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100008590.1.19",
          "form": "신기하내요 ㅎㅎ",
          "original_form": "신기하내요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:51"
        },
        {
          "id": "MDRW2100008590.1.20",
          "form": "오늘은 뭐드셨나요",
          "original_form": "오늘은 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210824 20:51"
        },
        {
          "id": "MDRW2100008590.1.21",
          "form": "떡볶이요!",
          "original_form": "떡볶이요!",
          "speaker_id": "1",
          "time": "20210824 20:51"
        }
      ]
