      "utterance": [
        {
          "id": "MDRW2100020914.1.1",
          "form": "스포츠 관람 시 지켜야할 예절은 뭐가 있을까요?",
          "original_form": "스포츠 관람 시 지켜야할 예절은 뭐가 있을까요?",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020914.1.2",
          "form": "선수들보고 막말하는 사람들도 별로에요",
          "original_form": "선수들보고 막말하는 사람들도 별로에요",
          "speaker_id": "1",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100020914.1.3",
          "form": "본인이 해보든지 라고 말하고싶어요",
          "original_form": "본인이 해보든지 라고 말하고싶어요",
          "speaker_id": "1",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100020914.1.4",
          "form": "어 맞아요 ㅋㅋ 훈계질 하는거 ㅎㅎ",
          "original_form": "어 맞아요 ㅋㅋ 훈계질 하는거 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020914.1.5",
          "form": "아나",
          "original_form": "아나",
          "speaker_id": "2",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020914.1.6",
          "form": "그리고 음식 먹었으면",
          "original_form": "그리고 음식 먹었으면",
          "speaker_id": "2",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020914.1.7",
          "form": "좀 치웠으면 좋겠어요",
          "original_form": "좀 치웠으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020914.1.8",
          "form": "으..본인이 먹은건 본인이 치워야져",
          "original_form": "으..본인이 먹은건 본인이 치워야져",
          "speaker_id": "1",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020914.1.9",
          "form": "기본예절인걸",
          "original_form": "기본예절인걸",
          "speaker_id": "1",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020914.1.10",
          "form": "ㅠㅠ 진짜 짜증나요",
          "original_form": "ㅠㅠ 진짜 짜증나요",
          "speaker_id": "2",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100020914.1.11",
          "form": "아무리 청소부 있다지만 기본적인 건 좀 치웠으면...",
          "original_form": "아무리 청소부 있다지만 기본적인 건 좀 치웠으면...",
          "speaker_id": "2",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100020914.1.12",
          "form": "저는 관람 와놓고 핸드폰만 보는 것도 이해 안돼요",
          "original_form": "저는 관람 와놓고 핸드폰만 보는 것도 이해 안돼요",
          "speaker_id": "2",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100020914.1.13",
          "form": "ㅋㅋ끌려오셨나...",
          "original_form": "ㅋㅋ끌려오셨나...",
          "speaker_id": "1",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100020914.1.14",
          "form": "근데 관람하는입장에선 보기좀그래요",
          "original_form": "근데 관람하는입장에선 보기좀그래요",
          "speaker_id": "1",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100020914.1.15",
          "form": "그니깐요 ㅎㅎㅎ",
          "original_form": "그니깐요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100020914.1.16",
          "form": "저 예전에 야구장에서 공 받겠다고 저 멀리서 뛰어온 아저씨도 본 적 있네요",
          "original_form": "저 예전에 야구장에서 공 받겠다고 저 멀리서 뛰어온 아저씨도 본 적 있네요",
          "speaker_id": "2",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100020914.1.17",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100020914.1.18",
          "form": "그런사람들 자주있잖아요",
          "original_form": "그런사람들 자주있잖아요",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100020914.1.19",
          "form": "그정도는 봐줄만해요",
          "original_form": "그정도는 봐줄만해요",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100020914.1.20",
          "form": "애들 줄려고 그러는 거면 이해 가능 ㅎㅎ",
          "original_form": "애들 줄려고 그러는 거면 이해 가능 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100020914.1.21",
          "form": "다음주부터 월드컵 시작이라는 데 알고 계셨어요?",
          "original_form": "다음주부터 월드컵 시작이라는 데 알고 계셨어요?",
          "speaker_id": "2",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100020914.1.22",
          "form": "그래요??",
          "original_form": "그래요??",
          "speaker_id": "1",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100020914.1.23",
          "form": "축구는 관심없어서 몰랐네요",
          "original_form": "축구는 관심없어서 몰랐네요",
          "speaker_id": "1",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100020914.1.24",
          "form": "그건 어디서 해요?",
          "original_form": "그건 어디서 해요?",
          "speaker_id": "1",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100020914.1.25",
          "form": "아마 올림픽 때처럼 똑같이 방송사에서 중계하지 않을까요?",
          "original_form": "아마 올림픽 때처럼 똑같이 방송사에서 중계하지 않을까요?",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100020914.1.26",
          "form": "카타르 월드컵이라고 하더라구요",
          "original_form": "카타르 월드컵이라고 하더라구요",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100020914.1.27",
          "form": "아 카타르",
          "original_form": "아 카타르",
          "speaker_id": "1",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100020914.1.28",
          "form": "네네 카타르가 그 중동국가 맞죠?",
          "original_form": "네네 카타르가 그 중동국가 맞죠?",
          "speaker_id": "2",
          "time": "20210828 15:49"
        },
        {
          "id": "MDRW2100020914.1.29",
          "form": "선수들 거기 가서 또 더워하겠네요 ㅠ",
          "original_form": "선수들 거기 가서 또 더워하겠네요 ㅠ",
          "speaker_id": "2",
