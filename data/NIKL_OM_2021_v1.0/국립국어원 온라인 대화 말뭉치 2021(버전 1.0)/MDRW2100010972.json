      "utterance": [
        {
          "id": "MDRW2100010972.1.1",
          "form": "음 고추장라면?",
          "original_form": "음 고추장라면?",
          "speaker_id": "2",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100010972.1.2",
          "form": "음. 아니야",
          "original_form": "음. 아니야",
          "speaker_id": "1",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100010972.1.3",
          "form": "사실은 맞아.",
          "original_form": "사실은 맞아.",
          "speaker_id": "1",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100010972.1.4",
          "form": "뭐야 지금 날 농락하니?",
          "original_form": "뭐야 지금 날 농락하니?",
          "speaker_id": "2",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100010972.1.5",
          "form": "너구리라면을 먹었는데 고추장맛이 났어",
          "original_form": "너구리라면을 먹었는데 고추장맛이 났어",
          "speaker_id": "1",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010972.1.6",
          "form": "너무 쉽게 맞혀서 재미가 없었어",
          "original_form": "너무 쉽게 맞혀서 재미가 없었어",
          "speaker_id": "1",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010972.1.7",
          "form": "라면에 고추장은 정말 처음 들어봐",
          "original_form": "라면에 고추장은 정말 처음 들어봐",
          "speaker_id": "2",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010972.1.8",
          "form": "맛이 어땠어 좀 더 설명해줘",
          "original_form": "맛이 어땠어 좀 더 설명해줘",
          "speaker_id": "2",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010972.1.9",
          "form": "좀 신기한 맛이었어",
          "original_form": "좀 신기한 맛이었어",
          "speaker_id": "1",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010972.1.10",
          "form": "매운탕의 느낌도 있고..",
          "original_form": "매운탕의 느낌도 있고..",
          "speaker_id": "1",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010972.1.11",
          "form": "고추장맛이 많이나니?",
          "original_form": "고추장맛이 많이나니?",
          "speaker_id": "2",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010972.1.12",
          "form": "내가 좋아하던 그 너구리는 아니었어",
          "original_form": "내가 좋아하던 그 너구리는 아니었어",
          "speaker_id": "1",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010972.1.13",
          "form": "응 나는 원래 그런 재료를 잘 못멎히는데",
          "original_form": "응 나는 원래 그런 재료를 잘 못멎히는데",
          "speaker_id": "1",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010972.1.14",
          "form": "한번에 맞혔어",
          "original_form": "한번에 맞혔어",
          "speaker_id": "1",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010972.1.15",
          "form": "어느정도인지 알겠지?",
          "original_form": "어느정도인지 알겠지?",
          "speaker_id": "1",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010972.1.16",
          "form": "과연 미식가구나",
          "original_form": "과연 미식가구나",
          "speaker_id": "2",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010972.1.17",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋ고마워 ㅎ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋ고마워 ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100010972.1.18",
          "form": "나중에 된장도 한번 넣어봐",
          "original_form": "나중에 된장도 한번 넣어봐",
          "speaker_id": "2",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100010972.1.19",
          "form": "그러면 라면이 엄청 맛있어진대",
          "original_form": "그러면 라면이 엄청 맛있어진대",
          "speaker_id": "2",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100010972.1.20",
          "form": "식초도!",
          "original_form": "식초도!",
          "speaker_id": "2",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100010972.1.21",
          "form": "그래 쌈장을 넣어볼게",
          "original_form": "그래 쌈장을 넣어볼게",
          "speaker_id": "1",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100010972.1.22",
          "form": "너희집에서 넣어보자.",
          "original_form": "너희집에서 넣어보자.",
          "speaker_id": "1",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100010972.1.23",
          "form": "식초를 넣으면 꼬들해진다",
          "original_form": "식초를 넣으면 꼬들해진다",
          "speaker_id": "2",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100010972.1.24",
          "form": "아마 없을 것 같아",
          "original_form": "아마 없을 것 같아",
          "speaker_id": "2",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100010972.1.25",
          "form": "너 엽떡 먹어봤어?",
          "original_form": "너 엽떡 먹어봤어?",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100010972.1.26",
          "form": "엽기떡볶이라는거야.",
          "original_form": "엽기떡볶이라는거야.",
          "speaker_id": "1",
          "time": "20210823 23:37"
        }
      ]
    }
  ]
