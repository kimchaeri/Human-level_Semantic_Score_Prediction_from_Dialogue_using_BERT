        },
        {
          "id": "MDRW2100025321.1.13",
          "form": "????",
          "original_form": "????",
          "speaker_id": "2",
          "time": "20210901 09:24"
        },
        {
          "id": "MDRW2100025321.1.14",
          "form": "다 입양보내졌음",
          "original_form": "다 입양보내졌음",
          "speaker_id": "1",
          "time": "20210901 09:24"
        },
        {
          "id": "MDRW2100025321.1.15",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210901 09:24"
        },
        {
          "id": "MDRW2100025321.1.16",
          "form": "지금은 막내랑 둘이사는중",
          "original_form": "지금은 막내랑 둘이사는중",
          "speaker_id": "1",
          "time": "20210901 09:24"
        },
        {
          "id": "MDRW2100025321.1.17",
          "form": "내 세금..",
          "original_form": "내 세금..",
          "speaker_id": "2",
          "time": "20210901 09:25"
        },
        {
          "id": "MDRW2100025321.1.18",
          "form": "저런 사람한테 가는구나",
          "original_form": "저런 사람한테 가는구나",
          "speaker_id": "2",
          "time": "20210901 09:25"
        },
        {
          "id": "MDRW2100025321.1.19",
          "form": "돌겠네요 진짜",
          "original_form": "돌겠네요 진짜",
          "speaker_id": "2",
          "time": "20210901 09:25"
        },
        {
          "id": "MDRW2100025321.1.20",
          "form": "답도 없네",
          "original_form": "답도 없네",
          "speaker_id": "2",
          "time": "20210901 09:25"
        },
        {
          "id": "MDRW2100025321.1.21",
          "form": "막내가 초3이면",
          "original_form": "막내가 초3이면",
          "speaker_id": "2",
          "time": "20210901 09:25"
        },
        {
          "id": "MDRW2100025321.1.22",
          "form": "어휴",
          "original_form": "어휴",
          "speaker_id": "2",
          "time": "20210901 09:25"
        },
        {
          "id": "MDRW2100025321.1.23",
          "form": "근데 더 얼척없는거 알려줄까요? ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 더 얼척없는거 알려줄까요? ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 09:26"
        },
        {
          "id": "MDRW2100025321.1.24",
          "form": "난진짜 기초수급자 제대로파악좀했음좋겠음",
          "original_form": "난진짜 기초수급자 제대로파악좀했음좋겠음",
          "speaker_id": "1",
          "time": "20210901 09:26"
        },
        {
          "id": "MDRW2100025321.1.25",
          "form": "충분히 일할능력되고 그러면 자활이라고 구청에서 일자리 꽂아주는데",
          "original_form": "충분히 일할능력되고 그러면 자활이라고 구청에서 일자리 꽂아주는데",
          "speaker_id": "1",
          "time": "20210901 09:27"
        },
        {
          "id": "MDRW2100025321.1.26",
          "form": "자활안하고 수급비만 받게되면 한달에 80정도임",
          "original_form": "자활안하고 수급비만 받게되면 한달에 80정도임",
          "speaker_id": "1",
          "time": "20210901 09:28"
        },
        {
          "id": "MDRW2100025321.1.27",
          "form": "꽤 주네요",
          "original_form": "꽤 주네요",
          "speaker_id": "2",
          "time": "20210901 09:28"
        },
        {
          "id": "MDRW2100025321.1.28",
          "form": "자활하면 최대 80까지벌수있음",
          "original_form": "자활하면 최대 80까지벌수있음",
          "speaker_id": "1",
          "time": "20210901 09:28"
        },
        {
          "id": "MDRW2100025321.1.29",
          "form": "그럼한달에 160",
          "original_form": "그럼한달에 160",
          "speaker_id": "1",
          "time": "20210901 09:28"
        },
        {
          "id": "MDRW2100025321.1.30",
          "form": "근데 얘네엄마 일하기싫어서 귀찮아서임 ㅋㅋㅋㅋ",
          "original_form": "근데 얘네엄마 일하기싫어서 귀찮아서임 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025321.1.31",
          "form": "괜찮네 쉬운 일일테고",
          "original_form": "괜찮네 쉬운 일일테고",
          "speaker_id": "2",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025321.1.32",
          "form": "그냥집에서 놀고먹는데",
          "original_form": "그냥집에서 놀고먹는데",
          "speaker_id": "1",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025321.1.33",
          "form": "애는 맨날 라면만먹이고",
          "original_form": "애는 맨날 라면만먹이고",
          "speaker_id": "1",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025321.1.34",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025321.1.35",
          "form": "자기는 수급비 받으면 일주일안에 다씀",
          "original_form": "자기는 수급비 받으면 일주일안에 다씀",
          "speaker_id": "1",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025321.1.36",
          "form": "사람새.끼가 아닌데",
          "original_form": "사람새.끼가 아닌데",
          "speaker_id": "2",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025321.1.37",
          "form": "재작년인가 노트8이 새로나왓엇는데",
          "original_form": "재작년인가 노트8이 새로나왓엇는데",
          "speaker_id": "1",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025321.1.38",
          "form": "핸드폰가게가서 현금으로 노트8사옴",
          "original_form": "핸드폰가게가서 현금으로 노트8사옴",
          "speaker_id": "1",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025321.1.39",
          "form": "그날 수급비다썻다고 애엄마한테 돈빌려달라고전화오고",
          "original_form": "그날 수급비다썻다고 애엄마한테 돈빌려달라고전화오고",
          "speaker_id": "1",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025321.1.40",
          "form": "골이 비었네",
          "original_form": "골이 비었네",
          "speaker_id": "2",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025321.1.41",
          "form": "무슨 생각으로 사냐",
          "original_form": "무슨 생각으로 사냐",
          "speaker_id": "2",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025321.1.42",
          "form": "참...",
          "original_form": "참...",
          "speaker_id": "2",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025321.1.43",
          "form": "최근에도 집근처 술,집에서 혼자 술먹다가",
          "original_form": "최근에도 집근처 술,집에서 혼자 술먹다가",
          "speaker_id": "1",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025321.1.44",
          "form": "외상이 20만원인가",
          "original_form": "외상이 20만원인가",
          "speaker_id": "1",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025321.1.45",
          "form": "?ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "?ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:31"
        },
        {
          "id": "MDRW2100025321.1.46",
          "form": "자기는 혼자 수급비로 술,ㅊ먹고",
          "original_form": "자기는 혼자 수급비로 술,ㅊ먹고",
          "speaker_id": "1",
          "time": "20210901 09:31"
        },
        {
          "id": "MDRW2100025321.1.47",
          "form": "애가 울드라구요 엄마만 맨날 맛잇는거사먹고",
          "original_form": "애가 울드라구요 엄마만 맨날 맛잇는거사먹고",
          "speaker_id": "1",
          "time": "20210901 09:31"
        },
        {
          "id": "MDRW2100025321.1.48",
          "form": "언변은 좋나봐요 외상도 하고",
          "original_form": "언변은 좋나봐요 외상도 하고",
          "speaker_id": "2",
          "time": "20210901 09:31"
        },
        {
          "id": "MDRW2100025321.1.49",
          "form": "자기는 라면만준다고",
          "original_form": "자기는 라면만준다고",
          "speaker_id": "1",
          "time": "20210901 09:31"
        },
