      "utterance": [
        {
          "id": "MDRW2100021691.1.1",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021691.1.2",
          "form": "캡쳐도 해뒀습니당ㅎㅎ",
          "original_form": "캡쳐도 해뒀습니당ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021691.1.3",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021691.1.4",
          "form": "넷플로 보시나요?",
          "original_form": "넷플로 보시나요?",
          "speaker_id": "2",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021691.1.5",
          "form": "저는 네이버시리즈온이나",
          "original_form": "저는 네이버시리즈온이나",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021691.1.6",
          "form": "티비에서 영화 구매해서 봐요!",
          "original_form": "티비에서 영화 구매해서 봐요!",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021691.1.7",
          "form": "네이버 시리즈온도 보기 괜찮나요?",
          "original_form": "네이버 시리즈온도 보기 괜찮나요?",
          "speaker_id": "2",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100021691.1.8",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100021691.1.9",
          "form": "근데 없는것도 많아요ㅠ",
          "original_form": "근데 없는것도 많아요ㅠ",
          "speaker_id": "1",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100021691.1.10",
          "form": "아ㅠ 역시 하나로는 안되나봐요",
          "original_form": "아ㅠ 역시 하나로는 안되나봐요",
          "speaker_id": "2",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100021691.1.11",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021691.1.12",
          "form": "넷플이 근데 진짜 많지않아요?",
          "original_form": "넷플이 근데 진짜 많지않아요?",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021691.1.13",
          "form": "맞아요!ㄱ",
          "original_form": "맞아요!ㄱ",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021691.1.14",
          "form": "근데 왜 보려면 없는거 같을까요?",
          "original_form": "근데 왜 보려면 없는거 같을까요?",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021691.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021691.1.16",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021691.1.17",
          "form": "이상해요ㅋㅋㅋ",
          "original_form": "이상해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021691.1.18",
          "form": "그렇게 웨이브 슬쩍.. 티빙 슬쩍..ㅋㅋ",
          "original_form": "그렇게 웨이브 슬쩍.. 티빙 슬쩍..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021691.1.19",
          "form": "근데 매달 스트리밍 비용도 부담되지않아요?ㅠㅠ",
          "original_form": "근데 매달 스트리밍 비용도 부담되지않아요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021691.1.20",
          "form": "맞아요!ㅠ",
          "original_form": "맞아요!ㅠ",
          "speaker_id": "2",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100021691.1.21",
          "form": "결국 친구에게 빌려서 보고있어요ㅠ",
          "original_form": "결국 친구에게 빌려서 보고있어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100021691.1.22",
          "form": "그것도 방법이죠!!",
          "original_form": "그것도 방법이죠!!",
          "speaker_id": "1",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100021691.1.23",
          "form": "다들 나눠서들 보고 그로도라고요",
          "original_form": "다들 나눠서들 보고 그로도라고요",
          "speaker_id": "1",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100021691.1.24",
          "form": "가격 생각하면 그렇게 되는거같아요",
          "original_form": "가격 생각하면 그렇게 되는거같아요",
