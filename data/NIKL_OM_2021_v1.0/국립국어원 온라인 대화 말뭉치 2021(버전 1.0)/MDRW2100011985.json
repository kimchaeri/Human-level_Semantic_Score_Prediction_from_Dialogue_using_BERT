      "utterance": [
        {
          "id": "MDRW2100011985.1.1",
          "form": "상견니 극장상영 보고싶다",
          "original_form": "상견니 극장상영 보고싶다",
          "speaker_id": "1",
          "time": "20210824 18:06"
        },
        {
          "id": "MDRW2100011985.1.2",
          "form": "편집해서 하겠네",
          "original_form": "편집해서 하겠네",
          "speaker_id": "2",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011985.1.3",
          "form": "어떻게 상영할지 궁금~",
          "original_form": "어떻게 상영할지 궁금~",
          "speaker_id": "1",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100011985.1.4",
          "form": "그러게 사람들 많이 갈까도 싶고..",
          "original_form": "그러게 사람들 많이 갈까도 싶고..",
          "speaker_id": "2",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011985.1.5",
          "form": "다른 영화도 개봉하는데",
          "original_form": "다른 영화도 개봉하는데",
          "speaker_id": "1",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100011985.1.6",
          "form": "그러게 상견니.. 안본 사람도많던데",
          "original_form": "그러게 상견니.. 안본 사람도많던데",
          "speaker_id": "2",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100011985.1.7",
          "form": "인기가 많긴 한가보네",
          "original_form": "인기가 많긴 한가보네",
          "speaker_id": "2",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100011985.1.8",
          "form": "왜 안 봤을까? 난 극장가서도 보고픈데",
          "original_form": "왜 안 봤을까? 난 극장가서도 보고픈데",
          "speaker_id": "1",
          "time": "20210824 19:00"
        },
        {
          "id": "MDRW2100011985.1.9",
          "form": "근데 상견니가 첨에 좀지루해 뭔말인지 이해가",
          "original_form": "근데 상견니가 첨에 좀지루해 뭔말인지 이해가",
          "speaker_id": "2",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100011985.1.10",
          "form": "난 안 지루하던데ㅜ영화로 보면 더 이해 잘될듯",
          "original_form": "난 안 지루하던데ㅜ영화로 보면 더 이해 잘될듯",
          "speaker_id": "1",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100011985.1.11",
          "form": "아 첨엔 재미없음",
          "original_form": "아 첨엔 재미없음",
          "speaker_id": "1",
          "time": "20210824 19:03"
        },
        {
          "id": "MDRW2100011985.1.12",
          "form": "어 그래서 그걸 넘겨야된다",
          "original_form": "어 그래서 그걸 넘겨야된다",
          "speaker_id": "2",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100011985.1.13",
          "form": "그래 그걸 넘겨야된다",
          "original_form": "그래 그걸 넘겨야된다",
          "speaker_id": "1",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100011985.1.14",
          "form": "안그럼 다못봐\r\n어려워서 ㅋ",
          "original_form": "안그럼 다못봐\r\n어려워서 ㅋ",
          "speaker_id": "2",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100011985.1.15",
          "form": "영화관에선 어떻게 나올지 궁금",
          "original_form": "영화관에선 어떻게 나올지 궁금",
          "speaker_id": "1",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100011985.1.16",
          "form": "상상은간다",
          "original_form": "상상은간다",
          "speaker_id": "2",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100011985.1.17",
          "form": "잘어울릴듯 극장도",
          "original_form": "잘어울릴듯 극장도",
          "speaker_id": "2",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100011985.1.18",
          "form": "난 콘서트가는거 넘 좋던데 진짜 힐링됨",
