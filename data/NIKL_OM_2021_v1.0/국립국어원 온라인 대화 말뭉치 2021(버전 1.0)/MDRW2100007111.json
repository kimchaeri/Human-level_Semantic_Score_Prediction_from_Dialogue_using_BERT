      "utterance": [
        {
          "id": "MDRW2100007111.1.1",
          "form": "name2님은\r\n연애중이세요?",
          "original_form": "&name2&님은\r\n연애중이세요?",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007111.1.2",
          "form": "앗 아니요 솔로예요 ㅠㅠ",
          "original_form": "앗 아니요 솔로예요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007111.1.3",
          "form": "연애중이세요?",
          "original_form": "연애중이세요?",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007111.1.4",
          "form": "연애하고싶은 유부입니다ㅋㅋ",
          "original_form": "연애하고싶은 유부입니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007111.1.5",
          "form": "그럼 평소 자만추에요?",
          "original_form": "그럼 평소 자만추에요?",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007111.1.6",
          "form": "앗 그르시구나 ㅋㅋㅋ",
          "original_form": "앗 그르시구나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007111.1.7",
          "form": "아님 소개팅으로 만나세요?",
          "original_form": "아님 소개팅으로 만나세요?",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007111.1.8",
          "form": "네 맞아요 자만추가 좋은거 같아요 ㅋㅋㅋ",
          "original_form": "네 맞아요 자만추가 좋은거 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007111.1.9",
          "form": "name1님은 배우자분 어떻게 만나셨나용?",
          "original_form": "&name1&님은 배우자분 어떻게 만나셨나용?",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007111.1.10",
          "form": "저는 대학동기에요:)",
          "original_form": "저는 대학동기에요:)",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007111.1.11",
          "form": "와 그럼 오래 만나셨겠어요",
          "original_form": "와 그럼 오래 만나셨겠어요",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007111.1.12",
          "form": "썸타다 군대가고 \r\n이후에 연락하고 지내다\r\n저 먼저 졸업하고ㅋㅋㅋ\r\n각자 다른 사람 만나다\r\n어떻게 결혼은 했네요",
          "original_form": "썸타다 군대가고 \r\n이후에 연락하고 지내다\r\n저 먼저 졸업하고ㅋㅋㅋ\r\n각자 다른 사람 만나다\r\n어떻게 결혼은 했네요",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007111.1.13",
          "form": "알고지낸 시간이 길지\r\n연애한 시간은 1년도 안되요",
          "original_form": "알고지낸 시간이 길지\r\n연애한 시간은 1년도 안되요",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007111.1.14",
          "form": "우와 돌고돌아 그렇게 만난 거네요",
          "original_form": "우와 돌고돌아 그렇게 만난 거네요",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007111.1.15",
          "form": "헛 신기해요",
          "original_form": "헛 신기해요",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007111.1.16",
          "form": "네ㅋㅋ대학 동기들은\r\n너네 그럴줄 알았다고 하지만\r\n정작 저희 둘은 몰랐어요ㅋㅋ",
          "original_form": "네ㅋㅋ대학 동기들은\r\n너네 그럴줄 알았다고 하지만\r\n정작 저희 둘은 몰랐어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007111.1.17",
          "form": "이상형은 어떻게 되세요?",
          "original_form": "이상형은 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007111.1.18",
          "form": "저는 일단 웃음코드가 맞고 좀 감정기복이 덜한 사람이요 ㅋㅋㅋ",
          "original_form": "저는 일단 웃음코드가 맞고 좀 감정기복이 덜한 사람이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007111.1.19",
          "form": "ㅋㅋ웃음코드 궁금하네요ㅋㅋ",
          "original_form": "ㅋㅋ웃음코드 궁금하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007111.1.20",
          "form": "제가 충청도 사람이라 약간 개그코드가 있어요 ㅋㅋㅋㅋㅋ 마이너한",
          "original_form": "제가 충청도 사람이라 약간 개그코드가 있어요 ㅋㅋㅋㅋㅋ 마이너한",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007111.1.21",
          "form": "근데 뭐 이상형은 이상형일뿐이죠 ㅋㅋㅋㅋ",
          "original_form": "근데 뭐 이상형은 이상형일뿐이죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007111.1.22",
          "form": "ㅋㅋ맞아요 이상형은 이상형일뿐",
          "original_form": "ㅋㅋ맞아요 이상형은 이상형일뿐",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007111.1.23",
          "form": "연애도 결혼도 이상형과는\r\n전혀 다른 사람과 한 거 같아요ㅋ",
          "original_form": "연애도 결혼도 이상형과는\r\n전혀 다른 사람과 한 거 같아요ㅋ",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007111.1.24",
          "form": "배우자님이 일치하나요? ㅋㅋㅋ",
          "original_form": "배우자님이 일치하나요? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007111.1.25",
          "form": "앗 ㅋㅋㅋㅋ",
          "original_form": "앗 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007111.1.26",
          "form": "아 일치하는게 있긴 있어요",
          "original_form": "아 일치하는게 있긴 있어요",
          "speaker_id": "1",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007111.1.27",
          "form": "남성.... 이라는거...?",
          "original_form": "남성.... 이라는거...?",
          "speaker_id": "1",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007111.1.28",
          "form": "앗 이런 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗 이런 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007111.1.29",
          "form": "저한테는 너무\r\n로또같은 남자에요ㅋㅋㅋ",
          "original_form": "저한테는 너무\r\n로또같은 남자에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007111.1.30",
          "form": "엇 왜요? 궁금",
          "original_form": "엇 왜요? 궁금",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007111.1.31",
          "form": "안맞아도\r\n너무 안맞아서요ㅠ",
          "original_form": "안맞아도\r\n너무 안맞아서요ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007111.1.32",
          "form": "살수록 더 로또구나 싶어요",
          "original_form": "살수록 더 로또구나 싶어요",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007111.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007111.1.34",
          "form": "아니 다른 의미의 로또였다니",
          "original_form": "아니 다른 의미의 로또였다니",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007111.1.35",
          "form": "ㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007111.1.36",
          "form": "그럼 처음 만날때 어떤 부분에서 매력을 느끼셨나요",
          "original_form": "그럼 처음 만날때 어떤 부분에서 매력을 느끼셨나요",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007111.1.37",
          "form": "서울남자요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "서울남자요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007111.1.38",
          "form": "그리고 제가 귀여운 남자를 좋아하는데",
          "original_form": "그리고 제가 귀여운 남자를 좋아하는데",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007111.1.39",
          "form": "귀여웠었어요ㅋㅋ",
          "original_form": "귀여웠었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007111.1.40",
          "form": "아 혹시 고향이 경상도신가요 ㅋㅋㅋ 그런 환상이 있더라고요 ㅋㅋㅋ",
          "original_form": "아 혹시 고향이 경상도신가요 ㅋㅋㅋ 그런 환상이 있더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007111.1.41",
          "form": "ㅋㅋㅋ아뇨 저는 인천이요",
          "original_form": "ㅋㅋㅋ아뇨 저는 인천이요",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007111.1.42",
          "form": "아니 같은 수도권이시면서 ㅋㅋㅋㅋ",
          "original_form": "아니 같은 수도권이시면서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007111.1.43",
          "form": "ㅋㅋ너무 어렸어여ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ너무 어렸어여ㅋㅋㅋㅋ",
