      "utterance": [
        {
          "id": "MDRW2100012071.1.1",
          "form": "털 때문에 키우긴 좀 힘들테지만 키운다면 고양이가 좋아요",
          "original_form": "털 때문에 키우긴 좀 힘들테지만 키운다면 고양이가 좋아요",
          "speaker_id": "2",
          "time": "20210825 08:26"
        },
        {
          "id": "MDRW2100012071.1.2",
          "form": "고양이 좋져 강아지도 키워보고 싶긴 해여",
          "original_form": "고양이 좋져 강아지도 키워보고 싶긴 해여",
          "speaker_id": "1",
          "time": "20210825 08:31"
        },
        {
          "id": "MDRW2100012071.1.3",
          "form": "동물 좋아하나요?",
          "original_form": "동물 좋아하나요?",
          "speaker_id": "1",
          "time": "20210825 08:34"
        },
        {
          "id": "MDRW2100012071.1.4",
          "form": "당연히 좋아해요! 특히 포유류요! 귀엽잖아요",
          "original_form": "당연히 좋아해요! 특히 포유류요! 귀엽잖아요",
          "speaker_id": "2",
          "time": "20210825 09:38"
        },
        {
          "id": "MDRW2100012071.1.5",
          "form": "털 동물 귀엽죠",
          "original_form": "털 동물 귀엽죠",
          "speaker_id": "1",
          "time": "20210825 09:40"
        },
        {
          "id": "MDRW2100012071.1.6",
          "form": "특별히 좋아하는 동물 있나요?",
          "original_form": "특별히 좋아하는 동물 있나요?",
          "speaker_id": "1",
          "time": "20210825 09:41"
        },
        {
          "id": "MDRW2100012071.1.7",
          "form": "고양이요! 머리부터 꼬리끝까지 너무 귀여워요",
          "original_form": "고양이요! 머리부터 꼬리끝까지 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210825 12:43"
        },
        {
          "id": "MDRW2100012071.1.8",
          "form": "웬만한 품종은 다 귀여워요..!",
          "original_form": "웬만한 품종은 다 귀여워요..!",
          "speaker_id": "2",
          "time": "20210825 12:43"
        },
        {
          "id": "MDRW2100012071.1.9",
          "form": "ㅋㅋ토끼나 햄스터도 귀엽더라구요",
          "original_form": "ㅋㅋ토끼나 햄스터도 귀엽더라구요",
          "speaker_id": "1",
          "time": "20210825 12:48"
        },
        {
          "id": "MDRW2100012071.1.10",
          "form": "토끼 쓰다듬어봤나요?",
          "original_form": "토끼 쓰다듬어봤나요?",
          "speaker_id": "1",
          "time": "20210825 12:48"
        },
        {
          "id": "MDRW2100012071.1.11",
          "form": "아니요 전 아직도 휠렛버거네 토끼는 초면이에요 햄스터는 만져봤지만!",
          "original_form": "아니요 전 아직도 휠렛버거네 토끼는 초면이에요 햄스터는 만져봤지만!",
          "speaker_id": "2",
          "time": "20210825 12:51"
        },
        {
          "id": "MDRW2100012071.1.12",
          "form": "아 저두 토끼를 본적이 없어요",
          "original_form": "아 저두 토끼를 본적이 없어요",
          "speaker_id": "1",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100012071.1.13",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "1",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100012071.1.14",
          "form": "토끼가 그러케 사납다는데.,  버거네도 가끔 얘기들어보면 사나운것 같더라구요",
          "original_form": "토끼가 그러케 사납다는데.,  버거네도 가끔 얘기들어보면 사나운것 같더라구요",
          "speaker_id": "2",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100012071.1.15",
          "form": "그런거 같아요",
          "original_form": "그런거 같아요",
          "speaker_id": "1",
          "time": "20210825 13:00"
        },
        {
          "id": "MDRW2100012071.1.16",
          "form": "털 알러지 같은건 없나봐요?",
          "original_form": "털 알러지 같은건 없나봐요?",
          "speaker_id": "1",
          "time": "20210825 13:00"
        },
        {
          "id": "MDRW2100012071.1.17",
          "form": "아뇨 있어요ㅋㅋㅋ 몰랐는데 있더라구요",
          "original_form": "아뇨 있어요ㅋㅋㅋ 몰랐는데 있더라구요",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100012071.1.18",
          "form": "친구 강아지 대신 맡아줬다가 알게 됐어요",
          "original_form": "친구 강아지 대신 맡아줬다가 알게 됐어요",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100012071.1.19",
          "form": "ㅋㅋ키우려면 각오가 필요하겠네요",
          "original_form": "ㅋㅋ키우려면 각오가 필요하겠네요",
