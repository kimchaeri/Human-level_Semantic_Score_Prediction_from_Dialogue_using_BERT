      "utterance": [
        {
          "id": "MDRW2100013717.1.1",
          "form": "갤럭시로 비방울도 잘 찍힘?",
          "original_form": "갤럭시로 비방울도 잘 찍힘?",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013717.1.2",
          "form": "웅 선명하게 잘 나와 요즘은 많이 발전했더라",
          "original_form": "웅 선명하게 잘 나와 요즘은 많이 발전했더라",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013717.1.3",
          "form": "이번주 내내 비온다던데",
          "original_form": "이번주 내내 비온다던데",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013717.1.4",
          "form": "그래서인지 좀 습하네ㅜㅜ",
          "original_form": "그래서인지 좀 습하네ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013717.1.5",
          "form": "저번주부터니까... 장마수준이야",
          "original_form": "저번주부터니까... 장마수준이야",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013717.1.6",
          "form": "계속 방안습도 80넘어가.. 켁",
          "original_form": "계속 방안습도 80넘어가.. 켁",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013717.1.7",
          "form": "빨래도 안마르고",
          "original_form": "빨래도 안마르고",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013717.1.8",
          "form": "그러게 날은 좀 선선해진 거 같은데 습기때문에 자꾸 에어컨 켜게되네",
          "original_form": "그러게 날은 좀 선선해진 거 같은데 습기때문에 자꾸 에어컨 켜게되네",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013717.1.9",
          "form": "나도.. 에어컨 수납했다가 다시 꺼냄..",
          "original_form": "나도.. 에어컨 수납했다가 다시 꺼냄..",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013717.1.10",
          "form": "이불이 축축해",
          "original_form": "이불이 축축해",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013717.1.11",
          "form": "그러게 빨래만 널어도 꿉꿉해지더라 습기많아서",
          "original_form": "그러게 빨래만 널어도 꿉꿉해지더라 습기많아서",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013717.1.12",
          "form": "아열대기후야ㅠㅠ",
          "original_form": "아열대기후야ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013717.1.13",
          "form": "가을장마도오고...",
          "original_form": "가을장마도오고...",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013717.1.14",
          "form": "얼렁 진짜 가을와라",
          "original_form": "얼렁 진짜 가을와라",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013717.1.15",
          "form": "그래도 장마지나면 이제 가을날씨일 거 같아",
          "original_form": "그래도 장마지나면 이제 가을날씨일 거 같아",
          "speaker_id": "2",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013717.1.16",
          "form": "요즘은 봄가을이 진짜 후딱 지나간다",
          "original_form": "요즘은 봄가을이 진짜 후딱 지나간다",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013717.1.17",
          "form": "그러게.. 가을이 젤 좋은데 넘 짧지..",
          "original_form": "그러게.. 가을이 젤 좋은데 넘 짧지..",
          "speaker_id": "1",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013717.1.18",
          "form": "여름 좋아해?",
          "original_form": "여름 좋아해?",
          "speaker_id": "1",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013717.1.19",
          "form": "아니 더위타서 여름 너무 힘들어...",
          "original_form": "아니 더위타서 여름 너무 힘들어...",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013717.1.20",
          "form": "차라리 겨울이 낫다",
          "original_form": "차라리 겨울이 낫다",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013717.1.21",
          "form": "넌 어때",
          "original_form": "넌 어때",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013717.1.22",
          "form": "나도.. 땀쟁이라... \r\n가을 겨울이 좋앙..",
          "original_form": "나도.. 땀쟁이라... \r\n가을 겨울이 좋앙..",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013717.1.23",
          "form": "여른엔 슈퍼만 갔다와도 샤워해야하니까",
          "original_form": "여른엔 슈퍼만 갔다와도 샤워해야하니까",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013717.1.24",
          "form": "그치 가만히 있어도 땀난다",
          "original_form": "그치 가만히 있어도 땀난다",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013717.1.25",
          "form": "에어컨은 신의 축복 ㅋㅋ",
          "original_form": "에어컨은 신의 축복 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013717.1.26",
          "form": "제습기 켜면 너무 뜨거워서 집에 있을 때는 에어컨 켜는게 젤 좋두르 ㅋㅋ",
          "original_form": "제습기 켜면 너무 뜨거워서 집에 있을 때는 에어컨 켜는게 젤 좋두르 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013717.1.27",
          "form": "좋더라 ㅋㅋ",
          "original_form": "좋더라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:54"
        }
      ]
