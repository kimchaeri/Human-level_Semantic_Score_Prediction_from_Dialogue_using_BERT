      "utterance": [
        {
          "id": "MDRW2100024890.1.1",
          "form": "네에.... 아직 뭐가 뭔지 모르겠어요 ㅋㅋㅋ",
          "original_form": "네에.... 아직 뭐가 뭔지 모르겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024890.1.2",
          "form": "야근도 많고 ㅜㅜㅜ",
          "original_form": "야근도 많고 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024890.1.3",
          "form": "저도 처음들어갔을때",
          "original_form": "저도 처음들어갔을때",
          "speaker_id": "1",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024890.1.4",
          "form": "어리버리했어요",
          "original_form": "어리버리했어요",
          "speaker_id": "1",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024890.1.5",
          "form": "언제쯤 괜찮아지셨어요?",
          "original_form": "언제쯤 괜찮아지셨어요?",
          "speaker_id": "2",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024890.1.6",
          "form": "한 육개월 지나고나서요",
          "original_form": "한 육개월 지나고나서요",
          "speaker_id": "1",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100024890.1.7",
          "form": "오호! 육개월 버텨 보겠어요",
          "original_form": "오호! 육개월 버텨 보겠어요",
          "speaker_id": "2",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100024890.1.8",
          "form": "상사가 많이 혼내요?",
          "original_form": "상사가 많이 혼내요?",
          "speaker_id": "1",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100024890.1.9",
          "form": "혼나기도 혼나고 제가 너무 못해서 그게 스트레스더라구요",
          "original_form": "혼나기도 혼나고 제가 너무 못해서 그게 스트레스더라구요",
          "speaker_id": "2",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100024890.1.10",
          "form": "ㅜㅠㅠ 실력은 점점 나아지실거예요",
          "original_form": "ㅜㅠㅠ 실력은 점점 나아지실거예요",
          "speaker_id": "1",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100024890.1.11",
          "form": "아직 초보니까 괜찮아요",
          "original_form": "아직 초보니까 괜찮아요",
          "speaker_id": "1",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100024890.1.12",
          "form": "ㅠㅠㅠㅠㅠ 증말 감사해요",
          "original_form": "ㅠㅠㅠㅠㅠ 증말 감사해요",
          "speaker_id": "2",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100024890.1.13",
          "form": "저도 많이 혼났어요ㅜ",
          "original_form": "저도 많이 혼났어요ㅜ",
          "speaker_id": "1",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100024890.1.14",
          "form": "알면서 주늑드는건 어쩔 수 없나봐여 ㅋㅋㅋㅋㅋ",
          "original_form": "알면서 주늑드는건 어쩔 수 없나봐여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:22"
        },
        {
          "id": "MDRW2100024890.1.15",
          "form": "6년동안 하시면서 무슨 생각이 제일 많이 드세요?",
          "original_form": "6년동안 하시면서 무슨 생각이 제일 많이 드세요?",
          "speaker_id": "2",
          "time": "20210831 20:22"
        },
        {
          "id": "MDRW2100024890.1.16",
          "form": "이제는 그냥 무념무상이랄까요 ㅋㅋ",
          "original_form": "이제는 그냥 무념무상이랄까요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:22"
        },
        {
          "id": "MDRW2100024890.1.17",
          "form": "ㅋㅋㅋ 숨쉬듯이 출근하고 퇴근하고 그러시나요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 숨쉬듯이 출근하고 퇴근하고 그러시나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:23"
        },
        {
          "id": "MDRW2100024890.1.18",
          "form": "네 그런것 같아요",
          "original_form": "네 그런것 같아요",
          "speaker_id": "1",
          "time": "20210831 20:23"
        },
        {
          "id": "MDRW2100024890.1.19",
          "form": "저 궁금한게 있는데 이 기회에!! 한 번 여쭈어도 될까여?",
          "original_form": "저 궁금한게 있는데 이 기회에!! 한 번 여쭈어도 될까여?",
          "speaker_id": "2",
