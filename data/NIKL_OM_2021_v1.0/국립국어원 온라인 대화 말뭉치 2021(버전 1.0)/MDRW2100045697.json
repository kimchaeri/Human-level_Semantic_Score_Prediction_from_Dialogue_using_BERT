      "utterance": [
        {
          "id": "MDRW2100045697.1.1",
          "form": "혹시 민초도 좋아하시나요?",
          "original_form": "혹시 민초도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 05:37"
        },
        {
          "id": "MDRW2100045697.1.2",
          "form": "앗 아니요 ㅋㅋㅋ 안 좋아해요",
          "original_form": "앗 아니요 ㅋㅋㅋ 안 좋아해요",
          "speaker_id": "2",
          "time": "20210908 05:37"
        },
        {
          "id": "MDRW2100045697.1.3",
          "form": "name1님은 민초단이신가요",
          "original_form": "&name1&님은 민초단이신가요",
          "speaker_id": "2",
          "time": "20210908 05:37"
        },
        {
          "id": "MDRW2100045697.1.4",
          "form": "네! 민초의 아름다움을 모르시다니ㅜㅜ",
          "original_form": "네! 민초의 아름다움을 모르시다니ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 05:37"
        },
        {
          "id": "MDRW2100045697.1.5",
          "form": "아앗 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아앗 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:38"
        },
        {
          "id": "MDRW2100045697.1.6",
          "form": "name1님이 제 몫까지 알아주시면 될 것 같아요...",
          "original_form": "&name1&님이 제 몫까지 알아주시면 될 것 같아요...",
          "speaker_id": "2",
          "time": "20210908 05:39"
        },
        {
          "id": "MDRW2100045697.1.7",
          "form": "앗ㅜㅜ 제가 name2님 몫까지 열심히 먹겠습니다!",
          "original_form": "앗ㅜㅜ 제가 &name2&님 몫까지 열심히 먹겠습니다!",
          "speaker_id": "1",
          "time": "20210908 05:39"
        },
        {
          "id": "MDRW2100045697.1.8",
          "form": "근데 비가 오니까 파전두 땡기네요",
          "original_form": "근데 비가 오니까 파전두 땡기네요",
          "speaker_id": "1",
          "time": "20210908 05:39"
        },
        {
          "id": "MDRW2100045697.1.9",
          "form": "하 파전하니까 녹두전 생각나뇨",
          "original_form": "하 파전하니까 녹두전 생각나뇨",
          "speaker_id": "2",
          "time": "20210908 05:40"
        },
        {
          "id": "MDRW2100045697.1.10",
          "form": "생각나요",
          "original_form": "생각나요",
          "speaker_id": "2",
          "time": "20210908 05:40"
        },
        {
          "id": "MDRW2100045697.1.11",
          "form": "헐 녹두전ㅠㅠㅠㅠ",
          "original_form": "헐 녹두전ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:51"
        },
        {
          "id": "MDRW2100045697.1.12",
          "form": "녹두전도 넘 맛나오",
          "original_form": "녹두전도 넘 맛나오",
          "speaker_id": "1",
          "time": "20210908 05:51"
        },
        {
          "id": "MDRW2100045697.1.13",
          "form": "그쵸.. 하",
          "original_form": "그쵸.. 하",
          "speaker_id": "2",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045697.1.14",
          "form": "육전도 땡기네요",
          "original_form": "육전도 땡기네요",
          "speaker_id": "1",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045697.1.15",
          "form": "기승전 고기ㅋㅋ",
          "original_form": "기승전 고기ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045697.1.16",
          "form": "아 안양 중앙시장에",
          "original_form": "아 안양 중앙시장에",
          "speaker_id": "2",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045697.1.17",
          "form": "안쪽으로",
          "original_form": "안쪽으로",
          "speaker_id": "2",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045697.1.18",
          "form": "쭉 들 어 가면",
          "original_form": "쭉 들 어 가면",
          "speaker_id": "2",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045697.1.19",
          "form": "녹두전 파는 집도 있어요 ㅋㅋ",
          "original_form": "녹두전 파는 집도 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045697.1.20",
          "form": "크기는 좀 작긴 한데 한장에 사천원에 팔았던것같아요",
          "original_form": "크기는 좀 작긴 한데 한장에 사천원에 팔았던것같아요",
          "speaker_id": "2",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045697.1.21",
          "form": "육전 저는 아직 한번도 안 먹어봐서 맛이 궁금해요 ㅋㅋㅋ",
          "original_form": "육전 저는 아직 한번도 안 먹어봐서 맛이 궁금해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045697.1.22",
          "form": "어 헐 저 군포살아요",
          "original_form": "어 헐 저 군포살아요",
          "speaker_id": "1",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045697.1.23",
          "form": "안양 가까운데! 사먹어봐야겠어요",
          "original_form": "안양 가까운데! 사먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045697.1.24",
          "form": "저도 육전은 집에서 한 것만 먹어봤는데 파는 것도 궁금하네요",
          "original_form": "저도 육전은 집에서 한 것만 먹어봤는데 파는 것도 궁금하네요",
          "speaker_id": "1",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045697.1.25",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045697.1.26",
          "form": "아까 다른분이랑 안양 얘기했었는데ㅋㅋ name1님인줄 알았어요",
          "original_form": "아까 다른분이랑 안양 얘기했었는데ㅋㅋ &name1&님인줄 알았어요",
          "speaker_id": "2",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045697.1.27",
          "form": "하핫",
          "original_form": "하핫",
          "speaker_id": "2",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045697.1.28",
          "form": "근데 name1님도 가까이에 사시네요 ㅋㅋㅋㅋ",
          "original_form": "근데 &name1&님도 가까이에 사시네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045697.1.29",
          "form": "저는 안양살아요 ㅎㅎ",
          "original_form": "저는 안양살아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 06:27"
        },
        {
          "id": "MDRW2100045697.1.30",
          "form": "안양 주민들이 은근 근처에 많더라구요ㅋㅋㅋ",
          "original_form": "안양 주민들이 은근 근처에 많더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:27"
        },
        {
          "id": "MDRW2100045697.1.31",
          "form": "ㅋㅋㅋ 반갑네요",
          "original_form": "ㅋㅋㅋ 반갑네요",
