      "utterance": [
        {
          "id": "MDRW2100035118.1.1",
          "form": "그사이에 여권도 만료되었는데 만들지도않앗네요ㅠㅠ",
          "original_form": "그사이에 여권도 만료되었는데 만들지도않앗네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:12"
        },
        {
          "id": "MDRW2100035118.1.2",
          "form": "여행 가실때 패키지로 가세용 자유로 가세용?",
          "original_form": "여행 가실때 패키지로 가세용 자유로 가세용?",
          "speaker_id": "1",
          "time": "20210829 03:12"
        },
        {
          "id": "MDRW2100035118.1.3",
          "form": "엇 저두 거의 만료되어가요ㅋㅋㅋ",
          "original_form": "엇 저두 거의 만료되어가요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:12"
        },
        {
          "id": "MDRW2100035118.1.4",
          "form": "전 다 자유였어요!",
          "original_form": "전 다 자유였어요!",
          "speaker_id": "2",
          "time": "20210829 03:12"
        },
        {
          "id": "MDRW2100035118.1.5",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210829 03:12"
        },
        {
          "id": "MDRW2100035118.1.6",
          "form": "전 작년이였더라구요 ㅋㅋ20년 ㅋㅋㅋ",
          "original_form": "전 작년이였더라구요 ㅋㅋ20년 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:12"
        },
        {
          "id": "MDRW2100035118.1.7",
          "form": "그럼 계획도 직접 짜셨어요?",
          "original_form": "그럼 계획도 직접 짜셨어요?",
          "speaker_id": "1",
          "time": "20210829 03:12"
        },
        {
          "id": "MDRW2100035118.1.8",
          "form": "막 잘짜진못해요ㅠㅠ",
          "original_form": "막 잘짜진못해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:12"
        },
        {
          "id": "MDRW2100035118.1.9",
          "form": "저는 계획짜는거 싫어해서 돈주고 패키지로 맡겼어요ㅋㅋㅋㅋ",
          "original_form": "저는 계획짜는거 싫어해서 돈주고 패키지로 맡겼어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:12"
        },
        {
          "id": "MDRW2100035118.1.10",
          "form": "그냥 대충짜고 ㅋㅋㅋ",
          "original_form": "그냥 대충짜고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:13"
        },
        {
          "id": "MDRW2100035118.1.11",
          "form": "좀 틀어지더라도 그게 자유여행의 묘민데",
          "original_form": "좀 틀어지더라도 그게 자유여행의 묘민데",
          "speaker_id": "1",
          "time": "20210829 03:13"
        },
        {
          "id": "MDRW2100035118.1.12",
          "form": "대충갔다와서 아쉬움이많아요",
          "original_form": "대충갔다와서 아쉬움이많아요",
          "speaker_id": "2",
          "time": "20210829 03:13"
        },
        {
          "id": "MDRW2100035118.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:13"
        },
        {
          "id": "MDRW2100035118.1.14",
          "form": "그땐 넘 어려서 알지못했네요ㅋㅋㅋ",
          "original_form": "그땐 넘 어려서 알지못했네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:13"
        },
        {
          "id": "MDRW2100035118.1.15",
          "form": "아니에용",
          "original_form": "아니에용",
          "speaker_id": "1",
          "time": "20210829 03:13"
        },
        {
          "id": "MDRW2100035118.1.16",
          "form": "패키지는 진짜 기빨려요",
          "original_form": "패키지는 진짜 기빨려요",
          "speaker_id": "1",
          "time": "20210829 03:13"
        },
        {
          "id": "MDRW2100035118.1.17",
          "form": "첨에 태국은 패키지였는데 ㅋㅋㅋ",
          "original_form": "첨에 태국은 패키지였는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:13"
        },
        {
          "id": "MDRW2100035118.1.18",
          "form": "저 일본 패키지갔다가ㅋㅋㅋㅋㅋㅋ 마지막날에 관광버스안에서 사람들 다 조용히 잠만잤어욬ㅋㅋㅋㅋ",
          "original_form": "저 일본 패키지갔다가ㅋㅋㅋㅋㅋㅋ 마지막날에 관광버스안에서 사람들 다 조용히 잠만잤어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:13"
        },
        {
          "id": "MDRW2100035118.1.19",
          "form": "패키지가 취소되서 그냥그돈쓸꺼니 가자 하고갓었어요 ㅋㅋㅋ",
          "original_form": "패키지가 취소되서 그냥그돈쓸꺼니 가자 하고갓었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:14"
        },
        {
          "id": "MDRW2100035118.1.20",
          "form": "빡씨군요 ㅋㅋㅋㅋ",
          "original_form": "빡씨군요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:14"
        },
        {
          "id": "MDRW2100035118.1.21",
          "form": "앗 취소되기도 하더라구요ㅠ",
          "original_form": "앗 취소되기도 하더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210829 03:14"
        }
