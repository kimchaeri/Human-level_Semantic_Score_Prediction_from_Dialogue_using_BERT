      "utterance": [
        {
          "id": "MDRW2100043306.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210907 17:19"
        },
        {
          "id": "MDRW2100043306.1.2",
          "form": "아니요ㅠㅠ 하지만 고양이를 꼭 반려하고 싶어요!!",
          "original_form": "아니요ㅠㅠ 하지만 고양이를 꼭 반려하고 싶어요!!",
          "speaker_id": "2",
          "time": "20210907 17:35"
        },
        {
          "id": "MDRW2100043306.1.3",
          "form": "키우시나요???",
          "original_form": "키우시나요???",
          "speaker_id": "2",
          "time": "20210907 17:35"
        },
        {
          "id": "MDRW2100043306.1.4",
          "form": "아니요 저도 준비가 된후 데려오고싶어서",
          "original_form": "아니요 저도 준비가 된후 데려오고싶어서",
          "speaker_id": "1",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100043306.1.5",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "1",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100043306.1.6",
          "form": "저도 그래요",
          "original_form": "저도 그래요",
          "speaker_id": "2",
          "time": "20210907 18:09"
        },
        {
          "id": "MDRW2100043306.1.7",
          "form": "어떤 동물 반려하고 싶으세요???",
          "original_form": "어떤 동물 반려하고 싶으세요???",
          "speaker_id": "2",
          "time": "20210907 18:09"
        },
        {
          "id": "MDRW2100043306.1.8",
          "form": "전 강아지도 좋아요!!",
          "original_form": "전 강아지도 좋아요!!",
          "speaker_id": "2",
          "time": "20210907 18:09"
        },
        {
          "id": "MDRW2100043306.1.9",
          "form": "저는 고양이요!",
          "original_form": "저는 고양이요!",
          "speaker_id": "1",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100043306.1.10",
          "form": "길냥이 데려 오고 싶네요",
          "original_form": "길냥이 데려 오고 싶네요",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043306.1.11",
          "form": "저도요!! 보호소에서 도움의 손길 필요한 아이로 데려오고 싶어요",
          "original_form": "저도요!! 보호소에서 도움의 손길 필요한 아이로 데려오고 싶어요",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043306.1.12",
          "form": "주변에 임보하시다가 입양하시는 언니보면",
          "original_form": "주변에 임보하시다가 입양하시는 언니보면",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043306.1.13",
          "form": "랜선집사 경력은 긴 데 실제로 반려한 경험은 없어서 성묘 입양하는게 로망이에요",
          "original_form": "랜선집사 경력은 긴 데 실제로 반려한 경험은 없어서 성묘 입양하는게 로망이에요",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043306.1.14",
          "form": "병원비같은게 많이들더라구요",
          "original_form": "병원비같은게 많이들더라구요",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043306.1.15",
          "form": "맞아요ㅠㅠ 동물병원은 부르는 게 값이더라고요",
          "original_form": "맞아요ㅠㅠ 동물병원은 부르는 게 값이더라고요",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043306.1.16",
          "form": "안정적으로 모으고 공간도 좀더 넓은곳가서",
          "original_form": "안정적으로 모으고 공간도 좀더 넓은곳가서",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043306.1.17",
          "form": "키우고싶어요",
          "original_form": "키우고싶어요",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043306.1.18",
          "form": "지금 사는곳이 투룸이긴한데 한곳은 옷방이라",
          "original_form": "지금 사는곳이 투룸이긴한데 한곳은 옷방이라",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043306.1.19",
          "form": "사실상 고양이는 방한개에서 살거같아서요ㅠ",
          "original_form": "사실상 고양이는 방한개에서 살거같아서요ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100043306.1.20",
          "form": "이미 너무 좋은 집사님이시네요",
          "original_form": "이미 너무 좋은 집사님이시네요",
          "speaker_id": "2",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100043306.1.21",
          "form": "전 본가살이 중이라 공간은 괜찮은데 병원비가 걱정이라 커리어부터 쌓으려고요",
          "original_form": "전 본가살이 중이라 공간은 괜찮은데 병원비가 걱정이라 커리어부터 쌓으려고요",
          "speaker_id": "2",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100043306.1.22",
          "form": "애기 아플 때 아끼지 않고 검사 받게 하고 치료해줘야죠ㅠㅠ",
          "original_form": "애기 아플 때 아끼지 않고 검사 받게 하고 치료해줘야죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043306.1.23",
          "form": "그죠...ㅠㅠㅠㅠ 내선택으로 데려오는건데 행복하게 해줘야죠",
          "original_form": "그죠...ㅠㅠㅠㅠ 내선택으로 데려오는건데 행복하게 해줘야죠",
          "speaker_id": "1",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043306.1.24",
          "form": "이것저것다해줄 돈이 필요하네요ㅠㅠ",
          "original_form": "이것저것다해줄 돈이 필요하네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:07"
        }
      ]
    }
