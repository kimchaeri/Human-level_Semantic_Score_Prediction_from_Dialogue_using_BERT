        },
        {
          "id": "MDRW2100040023.1.2",
          "form": "먹을땐참좋은데요",
          "original_form": "먹을땐참좋은데요",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100040023.1.3",
          "form": "설거지참귀찮네요",
          "original_form": "설거지참귀찮네요",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100040023.1.4",
          "form": "이래서 사람이 성공해야 합니다",
          "original_form": "이래서 사람이 성공해야 합니다",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100040023.1.5",
          "form": "이번 생은 글렀고 다음생애엔",
          "original_form": "이번 생은 글렀고 다음생애엔",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100040023.1.6",
          "form": "이재용 증손주로 태어날 예정입니다",
          "original_form": "이재용 증손주로 태어날 예정입니다",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100040023.1.7",
          "form": "ㅋㄱㅋㅋㅋㄱㅋㅋㅋ 구체적이신데요 증손주ㅋㅋㅋㅋ",
          "original_form": "ㅋㄱㅋㅋㅋㄱㅋㅋㅋ 구체적이신데요 증손주ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100040023.1.8",
          "form": "포지션이확실하신ㄱㅋㅋ",
          "original_form": "포지션이확실하신ㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100040023.1.9",
          "form": "name1님도 지금부터 정하세요!",
          "original_form": "&name1&님도 지금부터 정하세요!",
          "speaker_id": "2",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100040023.1.10",
          "form": "경쟁률이 세니 선점해야쥬",
          "original_form": "경쟁률이 세니 선점해야쥬",
          "speaker_id": "2",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100040023.1.11",
          "form": "ㅋㅋㅋㅋ저 그 증손주 사촌",
          "original_form": "ㅋㅋㅋㅋ저 그 증손주 사촌",
          "speaker_id": "1",
          "time": "20210904 01:40"
        },
        {
          "id": "MDRW2100040023.1.12",
          "form": "ㄱㅋㅈㄱㅈㄱㅈㄱㅈㅋ",
          "original_form": "ㄱㅋㅈㄱㅈㄱㅈㄱㅈㅋ",
          "speaker_id": "1",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100040023.1.13",
          "form": "맛있는거많이먹겠져..",
          "original_form": "맛있는거많이먹겠져..",
          "speaker_id": "1",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100040023.1.14",
          "form": "으아",
          "original_form": "으아",
          "speaker_id": "1",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100040023.1.15",
          "form": "전 소박하게 소고기 대신 삼겹살 위주로 먹으며 친근한 재벌로 이미지메이킹할 예정입니다ㅎㅈㅎ",
          "original_form": "전 소박하게 소고기 대신 삼겹살 위주로 먹으며 친근한 재벌로 이미지메이킹할 예정입니다ㅎㅈㅎ",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100040023.1.16",
          "form": "ㅋㅋㅋㅋㄱ이미지메이킹까지ㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㄱ이미지메이킹까지ㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:19"
        },
        {
          "id": "MDRW2100040023.1.17",
          "form": "아맛잇겄네요삼겹살",
