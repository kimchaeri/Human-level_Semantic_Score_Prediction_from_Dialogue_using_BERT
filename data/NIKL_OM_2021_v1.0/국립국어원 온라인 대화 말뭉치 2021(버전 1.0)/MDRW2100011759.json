      "utterance": [
        {
          "id": "MDRW2100011759.1.1",
          "form": "남자들이나 좀 필수로 맞았으면 좋겠어요",
          "original_form": "남자들이나 좀 필수로 맞았으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210824 15:47"
        },
        {
          "id": "MDRW2100011759.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 15:48"
        },
        {
          "id": "MDRW2100011759.1.3",
          "form": "애초에 그러면 전염되지도 않는데",
          "original_form": "애초에 그러면 전염되지도 않는데",
          "speaker_id": "2",
          "time": "20210824 15:48"
        },
        {
          "id": "MDRW2100011759.1.4",
          "form": "근데 가다실도 모든 성병을 막진 못한다고 하더라구요",
          "original_form": "근데 가다실도 모든 성병을 막진 못한다고 하더라구요",
          "speaker_id": "2",
          "time": "20210824 15:48"
        },
        {
          "id": "MDRW2100011759.1.5",
          "form": "맞아요...요즘은 성매수하는 남자들",
          "original_form": "맞아요...요즘은 성매수하는 남자들",
          "speaker_id": "1",
          "time": "20210824 15:48"
        },
        {
          "id": "MDRW2100011759.1.6",
          "form": "가다실 접종 인증하면 할인받고 그래요",
          "original_form": "가다실 접종 인증하면 할인받고 그래요",
          "speaker_id": "1",
          "time": "20210824 15:48"
        },
        {
          "id": "MDRW2100011759.1.7",
          "form": "기기괴괴",
          "original_form": "기기괴괴",
          "speaker_id": "1",
          "time": "20210824 15:48"
        },
        {
          "id": "MDRW2100011759.1.8",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "2",
          "time": "20210824 15:48"
        },
        {
          "id": "MDRW2100011759.1.9",
          "form": "별의별",
          "original_form": "별의별",
          "speaker_id": "2",
          "time": "20210824 15:48"
        },
        {
          "id": "MDRW2100011759.1.10",
          "form": "ㄴㅔ ㅋㅋㅋㅋ",
          "original_form": "ㄴㅔ ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:48"
        },
        {
          "id": "MDRW2100011759.1.11",
          "form": "어이없어요",
          "original_form": "어이없어요",
          "speaker_id": "2",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011759.1.12",
          "form": "여자분들이 좀 더 안전해지는건 다행이긴한데 너무 이상하지않나요",
          "original_form": "여자분들이 좀 더 안전해지는건 다행이긴한데 너무 이상하지않나요",
          "speaker_id": "1",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011759.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011759.1.14",
          "form": "접종 비율이 늘어나야한다는 건 아는데",
          "original_form": "접종 비율이 늘어나야한다는 건 아는데",
          "speaker_id": "2",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011759.1.15",
          "form": "이게 또 저런식으로",
          "original_form": "이게 또 저런식으로",
          "speaker_id": "2",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011759.1.16",
          "form": "진짜 기기괴괴",
          "original_form": "진짜 기기괴괴",
          "speaker_id": "2",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011759.1.17",
          "form": "불쾌한ㅋㅋㅋㅋ",
          "original_form": "불쾌한ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011759.1.18",
          "form": "이게 또 다 세금으로 나가는거니까",
          "original_form": "이게 또 다 세금으로 나가는거니까",
          "speaker_id": "2",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011759.1.19",
          "form": "남자들 성매수율도 너무 높지않나요",
          "original_form": "남자들 성매수율도 너무 높지않나요",
          "speaker_id": "1",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011759.1.20",
          "form": "짜증나욬ㅋㅋㅋ",
          "original_form": "짜증나욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011759.1.21",
          "form": "맞아요 저 처음 알게 됐을 때",
          "original_form": "맞아요 저 처음 알게 됐을 때",
          "speaker_id": "2",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011759.1.22",
          "form": "엄청 충격받았어요",
          "original_form": "엄청 충격받았어요",
          "speaker_id": "2",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011759.1.23",
          "form": "사실 그보다 더 많을 가능성이 높은거잖아요",
          "original_form": "사실 그보다 더 많을 가능성이 높은거잖아요",
          "speaker_id": "2",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011759.1.24",
          "form": "맞아요 엄청나요 카페보다 성매매업소가 많다니",
          "original_form": "맞아요 엄청나요 카페보다 성매매업소가 많다니",
