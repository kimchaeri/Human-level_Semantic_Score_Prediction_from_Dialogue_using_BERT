      "utterance": [
        {
          "id": "MDRW2100001495.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211015 17:40"
        },
        {
          "id": "MDRW2100001495.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211015 17:40"
        },
        {
          "id": "MDRW2100001495.1.3",
          "form": "혹시 즐겨보는 유튜버 있으세요?",
          "original_form": "혹시 즐겨보는 유튜버 있으세요?",
          "speaker_id": "2",
          "time": "20211015 17:40"
        },
        {
          "id": "MDRW2100001495.1.4",
          "form": "네 있습니다. 종이tv 신의한수",
          "original_form": "네 있습니다. 종이tv 신의한수",
          "speaker_id": "1",
          "time": "20211015 17:40"
        },
        {
          "id": "MDRW2100001495.1.5",
          "form": "정치",
          "original_form": "정치",
          "speaker_id": "1",
          "time": "20211015 17:40"
        },
        {
          "id": "MDRW2100001495.1.6",
          "form": "정치유튜버인가요??",
          "original_form": "정치유튜버인가요??",
          "speaker_id": "2",
          "time": "20211015 17:41"
        },
        {
          "id": "MDRW2100001495.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211015 17:41"
        },
        {
          "id": "MDRW2100001495.1.8",
          "form": "오호 저는 정치유튜브는 잘안봐서 몰랐네요..!",
          "original_form": "오호 저는 정치유튜브는 잘안봐서 몰랐네요..!",
          "speaker_id": "2",
          "time": "20211015 17:41"
        },
        {
          "id": "MDRW2100001495.1.9",
          "form": "저 분 말고는 안보세요?",
          "original_form": "저 분 말고는 안보세요?",
          "speaker_id": "2",
          "time": "20211015 17:41"
        },
        {
          "id": "MDRW2100001495.1.10",
          "form": "방탄tv ㅋ",
          "original_form": "방탄tv ㅋ",
          "speaker_id": "1",
          "time": "20211015 17:42"
        },
        {
          "id": "MDRW2100001495.1.11",
          "form": "오 ㅋㅋㅋㅋㅋㅋㅋㅋ 방탄소년단 공식채널 맞나요?",
          "original_form": "오 ㅋㅋㅋㅋㅋㅋㅋㅋ 방탄소년단 공식채널 맞나요?",
          "speaker_id": "2",
          "time": "20211015 17:42"
        },
        {
          "id": "MDRW2100001495.1.12",
          "form": "다양하게 보시네요 ㅎㅎ",
          "original_form": "다양하게 보시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 17:42"
        },
        {
          "id": "MDRW2100001495.1.13",
          "form": "어썸한끼 하루한끼",
          "original_form": "어썸한끼 하루한끼",
          "speaker_id": "1",
          "time": "20211015 17:43"
        },
        {
          "id": "MDRW2100001495.1.14",
          "form": "다양하게 봅니다",
          "original_form": "다양하게 봅니다",
          "speaker_id": "1",
          "time": "20211015 17:43"
        },
        {
          "id": "MDRW2100001495.1.15",
          "form": "오 하루한끼! 저도 가끔 봐요 ㅎㅎ",
          "original_form": "오 하루한끼! 저도 가끔 봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 17:43"
        },
        {
          "id": "MDRW2100001495.1.16",
          "form": "막 특별한 레시피가 있는건아닌데",
          "original_form": "막 특별한 레시피가 있는건아닌데",
          "speaker_id": "2",
          "time": "20211015 17:43"
        },
        {
          "id": "MDRW2100001495.1.17",
          "form": "어썸하은 ㅋ",
          "original_form": "어썸하은 ㅋ",
          "speaker_id": "1",
          "time": "20211015 17:43"
        },
        {
          "id": "MDRW2100001495.1.18",
          "form": "현실적으로 따라해보기 좋은",
          "original_form": "현실적으로 따라해보기 좋은",
          "speaker_id": "2",
          "time": "20211015 17:43"
        },
        {
          "id": "MDRW2100001495.1.19",
          "form": "아 그 춤 잘추는!",
          "original_form": "아 그 춤 잘추는!",
          "speaker_id": "2",
          "time": "20211015 17:43"
        },
        {
          "id": "MDRW2100001495.1.20",
          "form": "그럼요 좋아해요",
          "original_form": "그럼요 좋아해요",
          "speaker_id": "1",
          "time": "20211015 17:43"
        },
        {
          "id": "MDRW2100001495.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ정말 잘추더라구요..몸치인 저는 그저 존경\r\n",
