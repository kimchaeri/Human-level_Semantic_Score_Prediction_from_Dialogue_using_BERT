      "utterance": [
        {
          "id": "MDRW2100030301.1.1",
          "form": "돌아왔어요",
          "original_form": "돌아왔어요",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030301.1.2",
          "form": "내일은 뭐 먹을 거예요",
          "original_form": "내일은 뭐 먹을 거예요",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030301.1.3",
          "form": "내일은",
          "original_form": "내일은",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030301.1.4",
          "form": "남은볶음바비랑",
          "original_form": "남은볶음바비랑",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030301.1.5",
          "form": "고기랑",
          "original_form": "고기랑",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030301.1.6",
          "form": "된찌요",
          "original_form": "된찌요",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030301.1.7",
          "form": "볶음밥은 한 번에 볶은 거예요",
          "original_form": "볶음밥은 한 번에 볶은 거예요",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030301.1.8",
          "form": "언제 끝나요",
          "original_form": "언제 끝나요",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030301.1.9",
          "form": "아는분이 챙겨주셨어요",
          "original_form": "아는분이 챙겨주셨어요",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030301.1.10",
          "form": "혼자먹어서 끝나질 않아요",
          "original_form": "혼자먹어서 끝나질 않아요",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030301.1.11",
          "form": "질리지 않나요",
          "original_form": "질리지 않나요",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030301.1.12",
          "form": "이제1인분남았어요",
          "original_form": "이제1인분남았어요",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030301.1.13",
          "form": "왜 혼자 먹어요",
          "original_form": "왜 혼자 먹어요",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030301.1.14",
          "form": "카레부어이틀먹고",
          "original_form": "카레부어이틀먹고",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030301.1.15",
          "form": "카레 여왕이에요",
          "original_form": "카레 여왕이에요",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030301.1.16",
          "form": "혼자서 거의 해치워요",
          "original_form": "혼자서 거의 해치워요",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030301.1.17",
          "form": "네 카레여왕이맛나요",
          "original_form": "네 카레여왕이맛나요",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030301.1.18",
          "form": "오늘된찌했으니",
          "original_form": "오늘된찌했으니",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030301.1.19",
          "form": "이제낼이랑 모레먹겠죠",
          "original_form": "이제낼이랑 모레먹겠죠",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030301.1.20",
          "form": "마늘 맛이 맛있지요",
          "original_form": "마늘 맛이 맛있지요",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030301.1.21",
          "form": "전망고바나나맛",
          "original_form": "전망고바나나맛",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030301.1.22",
          "form": "먹었어요",
          "original_form": "먹었어요",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030301.1.23",
          "form": "하지만마늘한가득넣고 만들었어요",
          "original_form": "하지만마늘한가득넣고 만들었어요",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030301.1.24",
          "form": "다먹어 가는 거 축하해요",
          "original_form": "다먹어 가는 거 축하해요",
          "speaker_id": "1",
          "time": "20210903 23:47"
        }
      ]
