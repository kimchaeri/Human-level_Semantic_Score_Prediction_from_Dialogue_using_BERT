      "utterance": [
        {
          "id": "MDRW2100011816.1.1",
          "form": "재택근무 해보셨나요?",
          "original_form": "재택근무 해보셨나요?",
          "speaker_id": "1",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011816.1.2",
          "form": "네 재택알바도 재택근무로 치면요",
          "original_form": "네 재택알바도 재택근무로 치면요",
          "speaker_id": "2",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100011816.1.3",
          "form": "옥돔님은요?",
          "original_form": "옥돔님은요?",
          "speaker_id": "2",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100011816.1.4",
          "form": "요즘은 아니고",
          "original_form": "요즘은 아니고",
          "speaker_id": "1",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011816.1.5",
          "form": "회사 다닐때",
          "original_form": "회사 다닐때",
          "speaker_id": "1",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011816.1.6",
          "form": "확장이전한다고 해본적있어요",
          "original_form": "확장이전한다고 해본적있어요",
          "speaker_id": "1",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011816.1.7",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100011816.1.8",
          "form": "재택근무하면 직장이랑 먼곳에 살아도 될거같아요",
          "original_form": "재택근무하면 직장이랑 먼곳에 살아도 될거같아요",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100011816.1.9",
          "form": "저는 재택근무 안맞아요",
          "original_form": "저는 재택근무 안맞아요",
          "speaker_id": "1",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011816.1.10",
          "form": "집중이 잘 안돼요",
          "original_form": "집중이 잘 안돼요",
          "speaker_id": "1",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011816.1.11",
          "form": "아.. 그러시군요",
          "original_form": "아.. 그러시군요",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011816.1.12",
          "form": "그게 문제네요",
          "original_form": "그게 문제네요",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011816.1.13",
          "form": "일하는건지 쉬는건지 구분이 안돼요",
          "original_form": "일하는건지 쉬는건지 구분이 안돼요",
          "speaker_id": "1",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100011816.1.14",
          "form": "정신차리느라 시간을 다잡아먹네요",
          "original_form": "정신차리느라 시간을 다잡아먹네요",
          "speaker_id": "1",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100011816.1.15",
          "form": "수미님은 집에서 일잘되시나요?",
          "original_form": "수미님은 집에서 일잘되시나요?",
          "speaker_id": "1",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100011816.1.16",
          "form": "저는 알바만 해본거라 애매한데요",
          "original_form": "저는 알바만 해본거라 애매한데요",
          "speaker_id": "2",
          "time": "20210824 16:45"
        },
        {
          "id": "MDRW2100011816.1.17",
          "form": "공부를 일로 생각해보면.. 집은 편차가 큰거같아요",
          "original_form": "공부를 일로 생각해보면.. 집은 편차가 큰거같아요",
          "speaker_id": "2",
          "time": "20210824 16:45"
        },
        {
          "id": "MDRW2100011816.1.18",
          "form": "수미님은 집중안되실때 어떻게 하시나요?",
          "original_form": "수미님은 집중안되실때 어떻게 하시나요?",
          "speaker_id": "1",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100011816.1.19",
          "form": "독서실이나 학교에서 하면 그럭저럭하게는 하는데 정말 집중해서 잘되는 곳은 집 같더라구요. 집을 일하는 모드로 꾸며놓고 밖에서처럼 옷입고 하면 좋을거같아요",
          "original_form": "독서실이나 학교에서 하면 그럭저럭하게는 하는데 정말 집중해서 잘되는 곳은 집 같더라구요. 집을 일하는 모드로 꾸며놓고 밖에서처럼 옷입고 하면 좋을거같아요",
          "speaker_id": "2",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100011816.1.20",
          "form": "그냥 멍때리거나 쉬어요 잠자거나요",
          "original_form": "그냥 멍때리거나 쉬어요 잠자거나요",
          "speaker_id": "2",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100011816.1.21",
          "form": "저는 산책해요 확실히 뇌가 휴식이 돼서요",
          "original_form": "저는 산책해요 확실히 뇌가 휴식이 돼서요",
          "speaker_id": "1",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100011816.1.22",
          "form": "산책 좋죠",
          "original_form": "산책 좋죠",
          "speaker_id": "2",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011816.1.23",
          "form": "저는 뭘 하다보면 과부하되어서 쉬어야겠다 싶은 순간에",
          "original_form": "저는 뭘 하다보면 과부하되어서 쉬어야겠다 싶은 순간에",
          "speaker_id": "2",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011816.1.24",
          "form": "미련하게 계속 붙들고 있게 되더라구요",
          "original_form": "미련하게 계속 붙들고 있게 되더라구요",
          "speaker_id": "2",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011816.1.25",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011816.1.26",
          "form": "아직 못고친 습관인데",
          "original_form": "아직 못고친 습관인데",
          "speaker_id": "1",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011816.1.27",
          "form": "저만 그런게 아니네요",
          "original_form": "저만 그런게 아니네요",
          "speaker_id": "2",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011816.1.28",
          "form": "그러다 번아웃 오더라고요ㅋㅋ",
          "original_form": "그러다 번아웃 오더라고요ㅋㅋ",
          "speaker_id": "1",
