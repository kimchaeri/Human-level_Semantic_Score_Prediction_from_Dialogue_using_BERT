      "utterance": [
        {
          "id": "MDRW2100019058.1.1",
          "form": "반려동물 기르시나요?",
          "original_form": "반려동물 기르시나요?",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019058.1.2",
          "form": "부모님댁에 레브라도 리트리버가 있는데",
          "original_form": "부모님댁에 레브라도 리트리버가 있는데",
          "speaker_id": "1",
          "time": "20210827 17:50"
        },
        {
          "id": "MDRW2100019058.1.3",
          "form": "저는 아파트에 살아서 못 길러요",
          "original_form": "저는 아파트에 살아서 못 길러요",
          "speaker_id": "1",
          "time": "20210827 17:50"
        },
        {
          "id": "MDRW2100019058.1.4",
          "form": "리트리버 너무 귀엽고 사랑스러워요",
          "original_form": "리트리버 너무 귀엽고 사랑스러워요",
          "speaker_id": "1",
          "time": "20210827 17:50"
        },
        {
          "id": "MDRW2100019058.1.5",
          "form": "우와 리트리버! 제 위시 강아지에요",
          "original_form": "우와 리트리버! 제 위시 강아지에요",
          "speaker_id": "2",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019058.1.6",
          "form": "아파트에서 반려견기르기 쉽지않죠 ㅗ",
          "original_form": "아파트에서 반려견기르기 쉽지않죠 ㅗ",
          "speaker_id": "2",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019058.1.7",
          "form": "어머 오타죄송해요!! ㅠㅠㅠ",
          "original_form": "어머 오타죄송해요!! ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100019058.1.8",
          "form": "맞아요 역시 강아지는 마당이 있는 집에서길러야해요",
          "original_form": "맞아요 역시 강아지는 마당이 있는 집에서길러야해요",
          "speaker_id": "1",
          "time": "20210827 18:59"
        },
        {
          "id": "MDRW2100019058.1.9",
          "form": "우리나라 실내 바닥이 미끄러워서 강아지들 슬개골 탈구가 많다는 기사를 본적이 있어요",
          "original_form": "우리나라 실내 바닥이 미끄러워서 강아지들 슬개골 탈구가 많다는 기사를 본적이 있어요",
          "speaker_id": "1",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019058.1.10",
          "form": "어머나...  가엾어라",
          "original_form": "어머나...  가엾어라",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019058.1.11",
          "form": "역시 키우셔서 그런지 정보를 잘알고계시네요!",
          "original_form": "역시 키우셔서 그런지 정보를 잘알고계시네요!",
          "speaker_id": "2",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019058.1.12",
          "form": "사실 잘 알지 못해요",
          "original_form": "사실 잘 알지 못해요",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019058.1.13",
          "form": "동물 좋아하세요",
          "original_form": "동물 좋아하세요",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019058.1.14",
          "form": "아하~ 어떤동물젤좋아하세요?",
          "original_form": "아하~ 어떤동물젤좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019058.1.15",
          "form": "저는 개 좋아해요",
          "original_form": "저는 개 좋아해요",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019058.1.16",
          "form": "name2님은?",
          "original_form": "&name2&님은?",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019058.1.17",
          "form": "아하! 전 토끼요 ㅋㅋㅋ",
          "original_form": "아하! 전 토끼요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019058.1.18",
          "form": "토끼 키워보셨어요?",
          "original_form": "토끼 키워보셨어요?",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019058.1.19",
          "form": "아뇨~ 친구키우는거만봤어요 ㅠ",
          "original_form": "아뇨~ 친구키우는거만봤어요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100019058.1.20",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019058.1.21",
          "form": "토끼 귀엽죠",
          "original_form": "토끼 귀엽죠",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019058.1.22",
          "form": "초등학생때 키워봤었는데",
          "original_form": "초등학생때 키워봤었는데",
