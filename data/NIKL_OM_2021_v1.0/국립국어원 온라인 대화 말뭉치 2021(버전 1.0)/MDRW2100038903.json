        },
        {
          "id": "MDRW2100038903.1.3",
          "form": "name1님 맞으실까요?",
          "original_form": "&name1&님 맞으실까요?",
          "speaker_id": "2",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100038903.1.4",
          "form": "사실 계모 입니다 헷",
          "original_form": "사실 계모 입니다 헷",
          "speaker_id": "1",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100038903.1.5",
          "form": "이거 잡담해도 되나?",
          "original_form": "이거 잡담해도 되나?",
          "speaker_id": "1",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100038903.1.6",
          "form": "안되오",
          "original_form": "안되오",
          "speaker_id": "2",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100038903.1.7",
          "form": "집중하세오",
          "original_form": "집중하세오",
          "speaker_id": "2",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100038903.1.8",
          "form": "뮤지컬 좋아하시나오?",
          "original_form": "뮤지컬 좋아하시나오?",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100038903.1.9",
          "form": "않이오...",
          "original_form": "않이오...",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100038903.1.10",
          "form": "죄송함미다....",
          "original_form": "죄송함미다....",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100038903.1.11",
          "form": "연극은요...?",
          "original_form": "연극은요...?",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100038903.1.12",
          "form": "잘못찾아온건가....",
          "original_form": "잘못찾아온건가....",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100038903.1.13",
          "form": "죄송함미다...",
          "original_form": "죄송함미다...",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100038903.1.14",
          "form": "연극 좋아오",
          "original_form": "연극 좋아오",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038903.1.15",
          "form": "구름빵 보고싶었는데 못봤어오...",
          "original_form": "구름빵 보고싶었는데 못봤어오...",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038903.1.16",
          "form": "연극 본거 있나오?",
          "original_form": "연극 본거 있나오?",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038903.1.17",
          "form": "저는 마지막으로 본 연극 기억 안나요..",
          "original_form": "저는 마지막으로 본 연극 기억 안나요..",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038903.1.18",
          "form": "음...바냐아저씨 보다가 졸았어오....",
          "original_form": "음...바냐아저씨 보다가 졸았어오....",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038903.1.19",
          "form": "구름빵 재밌나요",
          "original_form": "구름빵 재밌나요",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038903.1.20",
          "form": "고양이가 귀여워요",
          "original_form": "고양이가 귀여워요",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038903.1.21",
          "form": "냥이짱!",
          "original_form": "냥이짱!",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038903.1.22",
          "form": "연극 보다가 졸면 클나요",
          "original_form": "연극 보다가 졸면 클나요",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038903.1.23",
          "form": "잡혀감ㅇㅇ",
          "original_form": "잡혀감ㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038903.1.24",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038903.1.25",
          "form": "꿀팁 감사합니다",
          "original_form": "꿀팁 감사합니다",
          "speaker_id": "2",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038903.1.26",
          "form": "범칙금이 있나요?",
          "original_form": "범칙금이 있나요?",
          "speaker_id": "2",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038903.1.27",
          "form": "네네 냥펀치 맞습니다",
          "original_form": "네네 냥펀치 맞습니다",
          "speaker_id": "1",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038903.1.28",
          "form": "뮤지컬 추천해주세요",
          "original_form": "뮤지컬 추천해주세요",
          "speaker_id": "2",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038903.1.29",
          "form": "지킬앤하이드요",
          "original_form": "지킬앤하이드요",
          "speaker_id": "1",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038903.1.30",
          "form": "앗 너무 무섭네요 ㄷㄷㄷ 졸지 말아야겠어오",
          "original_form": "앗 너무 무섭네요 ㄷㄷㄷ 졸지 말아야겠어오",
          "speaker_id": "2",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038903.1.31",
          "form": "오 언제 보셨나요?",
          "original_form": "오 언제 보셨나요?",
          "speaker_id": "2",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038903.1.32",
          "form": "살면서 딱 한번봄",
          "original_form": "살면서 딱 한번봄",
          "speaker_id": "1",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038903.1.33",
          "form": "20대 초반...?",
          "original_form": "20대 초반...?",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038903.1.34",
          "form": "누구 나오는 거임미까",
          "original_form": "누구 나오는 거임미까",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038903.1.35",
          "form": "덕질하다가 넘 비싸서 아이돌로 다시 갈아탐",
          "original_form": "덕질하다가 넘 비싸서 아이돌로 다시 갈아탐",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038903.1.36",
          "form": "류정한 입니다",
          "original_form": "류정한 입니다",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038903.1.37",
          "form": "엌ㅋㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038903.1.38",
          "form": "인정합니다",
          "original_form": "인정합니다",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038903.1.39",
          "form": "너무 비싸요",
          "original_form": "너무 비싸요",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038903.1.40",
          "form": "와 저 분",
          "original_form": "와 저 분",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038903.1.41",
          "form": "이름만 알아요...",
          "original_form": "이름만 알아요...",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038903.1.42",
          "form": "유명한분이죠",
          "original_form": "유명한분이죠",
          "speaker_id": "1",
          "time": "20210902 22:51"
        }
      ]
