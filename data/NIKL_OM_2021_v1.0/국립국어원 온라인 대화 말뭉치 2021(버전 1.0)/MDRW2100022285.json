        },
        {
          "id": "MDRW2100022285.1.3",
          "form": "저녁엔 뭐드실거예요??",
          "original_form": "저녁엔 뭐드실거예요??",
          "speaker_id": "2",
          "time": "20210829 12:59"
        },
        {
          "id": "MDRW2100022285.1.4",
          "form": "생각중이요",
          "original_form": "생각중이요",
          "speaker_id": "1",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022285.1.5",
          "form": "평소엔 뭐 즐기세요?",
          "original_form": "평소엔 뭐 즐기세요?",
          "speaker_id": "2",
          "time": "20210829 13:26"
        },
        {
          "id": "MDRW2100022285.1.6",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "1",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100022285.1.7",
          "form": "선호하는 브랜드있나요?",
          "original_form": "선호하는 브랜드있나요?",
          "speaker_id": "2",
          "time": "20210829 14:08"
