      "utterance": [
        {
          "id": "MDRW2100030635.1.1",
          "form": "아니 그말이 아니자나요",
          "original_form": "아니 그말이 아니자나요",
          "speaker_id": "2",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100030635.1.2",
          "form": "지금 웃겨서 막 웃고있죠?",
          "original_form": "지금 웃겨서 막 웃고있죠?",
          "speaker_id": "2",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100030635.1.3",
          "form": "그 정도까진 아니네염ㅋㅋ",
          "original_form": "그 정도까진 아니네염ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100030635.1.4",
          "form": "오늘 주말인데\r\n데이트 하세요?",
          "original_form": "오늘 주말인데\r\n데이트 하세요?",
          "speaker_id": "1",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100030635.1.5",
          "form": "아 네....",
          "original_form": "아 네....",
          "speaker_id": "2",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100030635.1.6",
          "form": "여친이 백신2차 맞고",
          "original_form": "여친이 백신2차 맞고",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030635.1.7",
          "form": "살아난지 얼마안되서",
          "original_form": "살아난지 얼마안되서",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030635.1.8",
          "form": "제가 직접가려구여",
          "original_form": "제가 직접가려구여",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030635.1.9",
          "form": "원래는 직접 안가셨나요?",
          "original_form": "원래는 직접 안가셨나요?",
          "speaker_id": "1",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030635.1.10",
          "form": "아침일찍 엔진오일 갈고왔어요",
          "original_form": "아침일찍 엔진오일 갈고왔어요",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030635.1.11",
          "form": "네 직접안갔어요",
          "original_form": "네 직접안갔어요",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030635.1.12",
          "form": "여친이 차로 데릴러왔다능",
          "original_form": "여친이 차로 데릴러왔다능",
          "speaker_id": "2",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100030635.1.13",
          "form": "하핫",
          "original_form": "하핫",
          "speaker_id": "2",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100030635.1.14",
          "form": "저희 남편은 연애때\r\n항상 집앞에 와서\r\n기다리고 ㅋㅋ 제가 앉는 자리에\r\n꽃 한송이는 준비해뒀었는데",
          "original_form": "저희 남편은 연애때\r\n항상 집앞에 와서\r\n기다리고 ㅋㅋ 제가 앉는 자리에\r\n꽃 한송이는 준비해뒀었는데",
          "speaker_id": "1",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100030635.1.15",
          "form": "그런 허위사실",
          "original_form": "그런 허위사실",
          "speaker_id": "2",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100030635.1.16",
          "form": "name2님 어디\r\n몸이 불편하신분은 아니시죠...?",
          "original_form": "&name2&님 어디\r\n몸이 불편하신분은 아니시죠...?",
          "speaker_id": "1",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100030635.1.17",
          "form": "현재소망을 말씀하시면 안됩니다",
          "original_form": "현재소망을 말씀하시면 안됩니다",
          "speaker_id": "2",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100030635.1.18",
          "form": "저 사지 멀쩡한대요...?",
          "original_form": "저 사지 멀쩡한대요...?",
          "speaker_id": "2",
          "time": "20210904 10:48"
        },
        {
          "id": "MDRW2100030635.1.19",
          "form": "왜 또 저 나.쁜사람 만들라고 하세요?",
          "original_form": "왜 또 저 나.쁜사람 만들라고 하세요?",
          "speaker_id": "2",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030635.1.20",
          "form": "ㅋㅋㅋ진짜에요",
          "original_form": "ㅋㅋㅋ진짜에요",
          "speaker_id": "1",
          "time": "20210904 10:56"
        },
        {
          "id": "MDRW2100030635.1.21",
          "form": "물론 지금은 꽃 안사줍니다",
          "original_form": "물론 지금은 꽃 안사줍니다",
          "speaker_id": "1",
          "time": "20210904 10:57"
        },
        {
          "id": "MDRW2100030635.1.22",
          "form": "전 꽃 두번 사줘봤어요",
          "original_form": "전 꽃 두번 사줘봤어요",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030635.1.23",
          "form": "기념일이었나요?",
          "original_form": "기념일이었나요?",
          "speaker_id": "1",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100030635.1.24",
          "form": "아뇨 서프라이즈로",
          "original_form": "아뇨 서프라이즈로",
          "speaker_id": "2",
          "time": "20210904 11:10"
        },
        {
          "id": "MDRW2100030635.1.25",
          "form": "근데 시들지 않는꽃 그거 한번 사줬는데",
          "original_form": "근데 시들지 않는꽃 그거 한번 사줬는데",
          "speaker_id": "2",
          "time": "20210904 11:10"
        },
        {
          "id": "MDRW2100030635.1.26",
          "form": "그거  싫어하던데요?ㅜㅜ",
          "original_form": "그거  싫어하던데요?ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 11:11"
        },
        {
          "id": "MDRW2100030635.1.27",
          "form": "저는 생화 좋아합니다ㅋㅋㅋ",
          "original_form": "저는 생화 좋아합니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:11"
        }
      ]
    }
