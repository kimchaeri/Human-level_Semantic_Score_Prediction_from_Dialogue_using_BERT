      "utterance": [
        {
          "id": "MDRW2100039921.1.1",
          "form": "저는 지금 나혼산 기다리고 있어요 ㅎㅎ",
          "original_form": "저는 지금 나혼산 기다리고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039921.1.2",
          "form": "뭐하시는 중이신가요",
          "original_form": "뭐하시는 중이신가요",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039921.1.3",
          "form": "전 지금 펜트하우스 틀어놨어요ㅎㅎ",
          "original_form": "전 지금 펜트하우스 틀어놨어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039921.1.4",
          "form": "매주 나혼산 챙겨보세요?",
          "original_form": "매주 나혼산 챙겨보세요?",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039921.1.5",
          "form": "3은 2까지만 봤는데",
          "original_form": "3은 2까지만 봤는데",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100039921.1.6",
          "form": "볼만한가요?",
          "original_form": "볼만한가요?",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100039921.1.7",
          "form": "그냥...틀어놨어요ㅋㅋㅋ",
          "original_form": "그냥...틀어놨어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100039921.1.8",
          "form": "이제 곧 끝난다길래 어떻게 되나 보는데",
          "original_form": "이제 곧 끝난다길래 어떻게 되나 보는데",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100039921.1.9",
          "form": "트로트를 부르네요...",
          "original_form": "트로트를 부르네요...",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100039921.1.10",
          "form": "저두 나혼산 걍틀어요 ㅋㅋㅋ",
          "original_form": "저두 나혼산 걍틀어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039921.1.11",
          "form": "아 그장면 웃겨서",
          "original_form": "아 그장면 웃겨서",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039921.1.12",
          "form": "저도 지나다가 봤어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 지나다가 봤어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039921.1.13",
          "form": "핵심장면 봤네요",
          "original_form": "핵심장면 봤네요",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039921.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ진짜 당황했어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ진짜 당황했어요",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039921.1.15",
          "form": "아니 갑자기 트로트...ㅋㅋㅋㅋㅋ",
          "original_form": "아니 갑자기 트로트...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039921.1.16",
          "form": "돈이 궁한가바요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "돈이 궁한가바요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100039921.1.17",
          "form": "심지어 울면서 부르고 있어서 너무 웃겼네요ㅋㅋㅋㄱㅋ",
          "original_form": "심지어 울면서 부르고 있어서 너무 웃겼네요ㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100039921.1.18",
          "form": "우는거였군요? ㅋㅋㅋㅋㅋㅋ 생각도 못해써요",
          "original_form": "우는거였군요? ㅋㅋㅋㅋㅋㅋ 생각도 못해써요",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100039921.1.19",
          "form": "특유의 표정이라 ㅋㅋㅋ",
          "original_form": "특유의 표정이라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100039921.1.20",
          "form": "아 맞아요ㅋㅋㅋㅋ특유 표정이 있죠",
          "original_form": "아 맞아요ㅋㅋㅋㅋ특유 표정이 있죠",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100039921.1.21",
          "form": "진짜 배우들 고생하는거 같아요",
          "original_form": "진짜 배우들 고생하는거 같아요",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100039921.1.22",
          "form": "저는 편스토랑 보고있는데",
          "original_form": "저는 편스토랑 보고있는데",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100039921.1.23",
          "form": "미역국을 무슨...",
          "original_form": "미역국을 무슨...",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100039921.1.24",
          "form": "고기를 사과넣고 육수냈어요",
          "original_form": "고기를 사과넣고 육수냈어요",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100039921.1.25",
          "form": "네...??",
          "original_form": "네...??",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100039921.1.26",
          "form": "통양파일수도있지만",
          "original_form": "통양파일수도있지만",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100039921.1.27",
          "form": "사과같은느낌...",
          "original_form": "사과같은느낌...",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100039921.1.28",
          "form": "대파도 넣고 무슨 수육처럼..ㅡ",
          "original_form": "대파도 넣고 무슨 수육처럼..ㅡ",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100039921.1.29",
          "form": "과일 넣고 육수내는건 보통 장아찌같은거 만들때 하지 않나요...?",
          "original_form": "과일 넣고 육수내는건 보통 장아찌같은거 만들때 하지 않나요...?",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100039921.1.30",
          "form": "양파였을까요...",
          "original_form": "양파였을까요...",
          "speaker_id": "2",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100039921.1.31",
          "form": "요리 프로그램 보다보면 종종 충격적일때 있어요...",
          "original_form": "요리 프로그램 보다보면 종종 충격적일때 있어요...",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100039921.1.32",
          "form": "양파도 단맛나서 무슨맛일지 상상이안가요",
          "original_form": "양파도 단맛나서 무슨맛일지 상상이안가요",
          "speaker_id": "2",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100039921.1.33",
          "form": "아니 사과가 맞네요 ㅋㅋㅋㅋ",
          "original_form": "아니 사과가 맞네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100039921.1.34",
          "form": "아이고....",
          "original_form": "아이고....",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100039921.1.35",
          "form": "그냥 요리는 백주부유튜브가 제일 좋은거 같아요",
          "original_form": "그냥 요리는 백주부유튜브가 제일 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100039921.1.36",
          "form": "요리 잘하는 백종원 같은 남자 부럽다는요 ㅜㅜㅋ",
          "original_form": "요리 잘하는 백종원 같은 남자 부럽다는요 ㅜㅜㅋ",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100039921.1.37",
          "form": "맞아요 그냥 감으로 대충 넣어도 맛있는 요리 하고싶어요ㅋㅋ",
          "original_form": "맞아요 그냥 감으로 대충 넣어도 맛있는 요리 하고싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100039921.1.38",
          "form": "레시피대로 해도 맛이 안나서 문제에요 ㅋㅋㅋㅋ",
          "original_form": "레시피대로 해도 맛이 안나서 문제에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039921.1.39",
          "form": "대충 눈대강으로 하다가 망하고ㅋㅋㅋ백주부쌤 말듣고 정확하게 계량해야되요",
          "original_form": "대충 눈대강으로 하다가 망하고ㅋㅋㅋ백주부쌤 말듣고 정확하게 계량해야되요",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100039921.1.40",
          "form": "그 생략도 문제라는요 ㅋㅋㅋㅋㅋ 요알못들은 하나씩 없어도 되겟지 이러능 ㅎㅎㅎ",
          "original_form": "그 생략도 문제라는요 ㅋㅋㅋㅋㅋ 요알못들은 하나씩 없어도 되겟지 이러능 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:18"
        }
      ]
    }
