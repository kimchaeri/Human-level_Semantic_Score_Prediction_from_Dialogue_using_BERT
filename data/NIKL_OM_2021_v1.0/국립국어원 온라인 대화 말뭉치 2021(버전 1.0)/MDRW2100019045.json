      "utterance": [
        {
          "id": "MDRW2100019045.1.1",
          "form": "유니화요?",
          "original_form": "유니화요?",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019045.1.2",
          "form": "네 회사에서 신는 거",
          "original_form": "네 회사에서 신는 거",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019045.1.3",
          "form": "아 새로 장만하시는 거예요?",
          "original_form": "아 새로 장만하시는 거예요?",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019045.1.4",
          "form": "지금 신고있는 거 멀쩡하긴한데 한치수 컸으면해서요",
          "original_form": "지금 신고있는 거 멀쩡하긴한데 한치수 컸으면해서요",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019045.1.5",
          "form": "구두신고있음 발 붓고하니까요",
          "original_form": "구두신고있음 발 붓고하니까요",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019045.1.6",
          "form": "네ㅜㅜ제가 칼발이어서 신발사이즈가 늘 애매해요",
          "original_form": "네ㅜㅜ제가 칼발이어서 신발사이즈가 늘 애매해요",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019045.1.7",
          "form": "발볼 넓은 편이세요?",
          "original_form": "발볼 넓은 편이세요?",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019045.1.8",
          "form": "아뇨 길쭉한 발이라...",
          "original_form": "아뇨 길쭉한 발이라...",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019045.1.9",
          "form": "신발사기힘드시겠어요...",
          "original_form": "신발사기힘드시겠어요...",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019045.1.10",
          "form": "그런편이죠ㅜㅜ",
          "original_form": "그런편이죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019045.1.11",
          "form": "저도 이번에 흰운동화 하나 샀는데 가죽이라 못신겠어요ㅋㄱㄱ",
          "original_form": "저도 이번에 흰운동화 하나 샀는데 가죽이라 못신겠어요ㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019045.1.12",
          "form": "가죽이면 관리하기어렵겠어요",
          "original_form": "가죽이면 관리하기어렵겠어요",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019045.1.13",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019045.1.14",
          "form": "저렴한 스니커즈랑 번갈아신으세요",
          "original_form": "저렴한 스니커즈랑 번갈아신으세요",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019045.1.15",
          "form": "저도 그럴까 생각중이에요",
          "original_form": "저도 그럴까 생각중이에요",
          "speaker_id": "2",
