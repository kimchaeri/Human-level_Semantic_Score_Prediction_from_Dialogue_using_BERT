      "utterance": [
        {
          "id": "MDRW2100018946.1.1",
          "form": "강철부대란 군대 관련 예능 보셨나요?",
          "original_form": "강철부대란 군대 관련 예능 보셨나요?",
          "speaker_id": "1",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100018946.1.2",
          "form": "컷본으로는 본 적 있어요",
          "original_form": "컷본으로는 본 적 있어요",
          "speaker_id": "2",
          "time": "20210827 13:55"
        },
        {
          "id": "MDRW2100018946.1.3",
          "form": "저는 원래 한번도 안봤다가 1.2화 봤는데 재밌기는 하더라구요",
          "original_form": "저는 원래 한번도 안봤다가 1.2화 봤는데 재밌기는 하더라구요",
          "speaker_id": "1",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018946.1.4",
          "form": "혹시 군대 생활 경험 잇으신가요?",
          "original_form": "혹시 군대 생활 경험 잇으신가요?",
          "speaker_id": "1",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018946.1.5",
          "form": "아니요 있으신가요?",
          "original_form": "아니요 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018946.1.6",
          "form": "저는 해병대캠프 가본적있어요",
          "original_form": "저는 해병대캠프 가본적있어요",
          "speaker_id": "1",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100018946.1.7",
          "form": "색다른 체험이었어요",
          "original_form": "색다른 체험이었어요",
          "speaker_id": "1",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100018946.1.8",
          "form": "해병대 캠프를 가면 어떤 체험을 하나요?",
          "original_form": "해병대 캠프를 가면 어떤 체험을 하나요?",
          "speaker_id": "2",
          "time": "20210827 21:05"
        },
        {
          "id": "MDRW2100018946.1.9",
          "form": "훈련도 하고 식사도 하고 ..보트 머리위에 드는 훈련도 하고 그랬어요",
          "original_form": "훈련도 하고 식사도 하고 ..보트 머리위에 드는 훈련도 하고 그랬어요",
          "speaker_id": "1",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100018946.1.10",
          "form": "며칠정도 하는 체험이었나요?",
          "original_form": "며칠정도 하는 체험이었나요?",
          "speaker_id": "2",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100018946.1.11",
          "form": "일주일이요 ㅎ",
          "original_form": "일주일이요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100018946.1.12",
          "form": "나름 재밌엇어요",
          "original_form": "나름 재밌엇어요",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100018946.1.13",
          "form": "식판도 오와 열을 맞춰서 놓는게 신기했어요",
          "original_form": "식판도 오와 열을 맞춰서 놓는게 신기했어요",
          "speaker_id": "1",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100018946.1.14",
          "form": "일주일이면 제법 본격적이네요",
          "original_form": "일주일이면 제법 본격적이네요",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100018946.1.15",
          "form": "금액은 어느정도였나요?",
          "original_form": "금액은 어느정도였나요?",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100018946.1.16",
          "form": "회사연수때 간거라 금액은 모르겠어요 ㅎㅎ",
          "original_form": "회사연수때 간거라 금액은 모르겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100018946.1.17",
          "form": "회사에서 해병대캠프를 보낸건가요?",
          "original_form": "회사에서 해병대캠프를 보낸건가요?",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100018946.1.18",
          "form": "네..ㅋㅋㅋ",
          "original_form": "네..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100018946.1.19",
          "form": "남녀불문 동기들끼리 다같이 갔었는데 확실히 전우애는 키워지기는 하더라구요",
          "original_form": "남녀불문 동기들끼리 다같이 갔었는데 확실히 전우애는 키워지기는 하더라구요",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100018946.1.20",
          "form": "그렇군요 와 회사에서 해병대 캠프라니 신입들끼리 간거였죠?",
          "original_form": "그렇군요 와 회사에서 해병대 캠프라니 신입들끼리 간거였죠?",
          "speaker_id": "2",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100018946.1.21",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100018946.1.22",
          "form": "대학교나 회사나 어디나 아직까지 군대문화가 조금씩은 다 있는거 같아요",
          "original_form": "대학교나 회사나 어디나 아직까지 군대문화가 조금씩은 다 있는거 같아요",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100018946.1.23",
          "form": "맞아요 군기는 어디에나 다 있으니까요",
          "original_form": "맞아요 군기는 어디에나 다 있으니까요",
