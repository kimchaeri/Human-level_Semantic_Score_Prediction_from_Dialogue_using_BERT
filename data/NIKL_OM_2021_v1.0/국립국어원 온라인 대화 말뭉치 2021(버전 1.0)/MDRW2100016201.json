        {
          "id": "MDRW2100016201.1.1",
          "form": "오 맛집 된장찌개 또 완성됐나 ㅎㅎ",
          "original_form": "오 맛집 된장찌개 또 완성됐나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100016201.1.2",
          "form": "된장찌개 양념 있으니 진짜 편해",
          "original_form": "된장찌개 양념 있으니 진짜 편해",
          "speaker_id": "1",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100016201.1.3",
          "form": "글치~ 따로 간 맞출 필요도 없고 ㅎㅎ",
          "original_form": "글치~ 따로 간 맞출 필요도 없고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100016201.1.4",
          "form": "야채만 듬뿍 넣으면 돼",
          "original_form": "야채만 듬뿍 넣으면 돼",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016201.1.5",
          "form": "야채 다양하게 넣었어?",
          "original_form": "야채 다양하게 넣었어?",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016201.1.6",
          "form": "ㅇㅇ 양파랑 파, 애호박, 버섯",
          "original_form": "ㅇㅇ 양파랑 파, 애호박, 버섯",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016201.1.7",
          "form": "ㅎㅎ 건강하네",
          "original_form": "ㅎㅎ 건강하네",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016201.1.8",
          "form": "그리고 두부도 넣고",
          "original_form": "그리고 두부도 넣고",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016201.1.9",
          "form": "밥도둑이겠다~ 맛있겠다",
          "original_form": "밥도둑이겠다~ 맛있겠다",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016201.1.10",
          "form": "삼겹살이랑 같이 먹으면 완전 꿀맛이지",
          "original_form": "삼겹살이랑 같이 먹으면 완전 꿀맛이지",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016201.1.11",
          "form": "애들이랑 점심 맛있게 드삼",
          "original_form": "애들이랑 점심 맛있게 드삼",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016201.1.12",
          "form": "웅 ㅎㅎ 신랑도",
          "original_form": "웅 ㅎㅎ 신랑도",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016201.1.13",
          "form": "난 오늘 버거나 먹으러 가야겠다",
          "original_form": "난 오늘 버거나 먹으러 가야겠다",
