      "utterance": [
        {
          "id": "MDRW2100008641.1.1",
          "form": "특히 짝바꾸기할때 ㅠㅠ 힘들었어요..",
          "original_form": "특히 짝바꾸기할때 ㅠㅠ 힘들었어요..",
          "speaker_id": "1",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008641.1.2",
          "form": "아 짝바꾸기 ㅠ 진짜 제발 좋은 친구 걸려라 하고 기도했던",
          "original_form": "아 짝바꾸기 ㅠ 진짜 제발 좋은 친구 걸려라 하고 기도했던",
          "speaker_id": "2",
          "time": "20210822 21:06"
        },
        {
          "id": "MDRW2100008641.1.3",
          "form": "저희 학교얘기나 해볼까요 ㅎㅎㅎ",
          "original_form": "저희 학교얘기나 해볼까요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:24"
        },
        {
          "id": "MDRW2100008641.1.4",
          "form": "저는 예전 생각하면 새록새록 ㅠㅠ",
          "original_form": "저는 예전 생각하면 새록새록 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:24"
        },
        {
          "id": "MDRW2100008641.1.5",
          "form": "학교 얘기면 공부가 젤 먼저 떠오르네요 ㅋㅋㅋ",
          "original_form": "학교 얘기면 공부가 젤 먼저 떠오르네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100008641.1.6",
          "form": "초등학생때가 좋았지하고 회상하고 그랬어요",
          "original_form": "초등학생때가 좋았지하고 회상하고 그랬어요",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100008641.1.7",
          "form": "아아 ㅋㅋㅋㅋㅋ",
          "original_form": "아아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100008641.1.8",
          "form": "초등학교때는 좋죠 ㅠㅠ",
          "original_form": "초등학교때는 좋죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100008641.1.9",
          "form": "공부생갇 안해도 되고 ㅠㅠ",
          "original_form": "공부생갇 안해도 되고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100008641.1.10",
          "form": "무엇보다 급식이 너무 맛있었어요",
          "original_form": "무엇보다 급식이 너무 맛있었어요",
          "speaker_id": "1",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100008641.1.11",
          "form": "오 급식 맛있는 학교 너무 좋죠 !!",
          "original_form": "오 급식 맛있는 학교 너무 좋죠 !!",
          "speaker_id": "2",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100008641.1.12",
          "form": "그쪽 학교는 어때요?",
          "original_form": "그쪽 학교는 어때요?",
          "speaker_id": "1",
          "time": "20210822 21:44"
        },
        {
          "id": "MDRW2100008641.1.13",
          "form": "맛있나요 ㅋㅋ",
          "original_form": "맛있나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:44"
        },
        {
          "id": "MDRW2100008641.1.14",
          "form": "중학교땐 별로였는데 고등학교 오니까 맛있더라구요 ㅎㅎ",
          "original_form": "중학교땐 별로였는데 고등학교 오니까 맛있더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100008641.1.15",
          "form": "여러가지 다양하게 나와가지고",
          "original_form": "여러가지 다양하게 나와가지고",
          "speaker_id": "2",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100008641.1.16",
          "form": "요즘은 고등학교도 무상급식이던가요?",
          "original_form": "요즘은 고등학교도 무상급식이던가요?",
          "speaker_id": "1",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100008641.1.17",
          "form": "아주 좋은 일니네요!!",
          "original_form": "아주 좋은 일니네요!!",
          "speaker_id": "1",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100008641.1.18",
          "form": "저는 고등학교때 망해서 ㅋㅋㅋ",
          "original_form": "저는 고등학교때 망해서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100008641.1.19",
          "form": "지금 대학보단 ...낫지만요",
          "original_form": "지금 대학보단 ...낫지만요",
          "speaker_id": "1",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100008641.1.20",
          "form": "헉 학식 별로이신가요 ㅠ ㅠ",
          "original_form": "헉 학식 별로이신가요 ㅠ ㅠ",
          "speaker_id": "2",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100008641.1.21",
          "form": "맛있는 걸 먹어야 공부할 힘도 나는 법인데",
          "original_form": "맛있는 걸 먹어야 공부할 힘도 나는 법인데",
          "speaker_id": "2",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100008641.1.22",
          "form": "진짜 너무 별로예요 ㅋㅋㅋㅋ",
          "original_form": "진짜 너무 별로예요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:58"
        },
        {
          "id": "MDRW2100008641.1.23",
          "form": "맨날 급식먹고싶다 그러잖아요 ㅠㅠ",
          "original_form": "맨날 급식먹고싶다 그러잖아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:58"
        },
        {
          "id": "MDRW2100008641.1.24",
          "form": "한솥이 저희 집입니다..",
          "original_form": "한솥이 저희 집입니다..",
          "speaker_id": "1",
          "time": "20210822 21:58"
        },
        {
          "id": "MDRW2100008641.1.25",
          "form": "세상에 ㅠㅠ ",
          "original_form": "세상에 ㅠㅠ ",
          "speaker_id": "2",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100008641.1.26",
          "form": "밥.. 맛있는 곳이 최고예요..",
          "original_form": "밥.. 맛있는 곳이 최고예요..",
          "speaker_id": "1",
