      "utterance": [
        {
          "id": "MDRW2100011482.1.1",
          "form": "가상세계는 vr같은거죠?",
          "original_form": "가상세계는 vr같은거죠?",
          "speaker_id": "2",
          "time": "20210824 13:11"
        },
        {
          "id": "MDRW2100011482.1.2",
          "form": "혹시 레디 플레이어 원 영화 봤나요?",
          "original_form": "혹시 레디 플레이어 원 영화 봤나요?",
          "speaker_id": "1",
          "time": "20210824 13:15"
        },
        {
          "id": "MDRW2100011482.1.3",
          "form": "vr도 하나의 예가 될 수 있고",
          "original_form": "vr도 하나의 예가 될 수 있고",
          "speaker_id": "1",
          "time": "20210824 13:17"
        },
        {
          "id": "MDRW2100011482.1.4",
          "form": "vr없이 하는  제페토 같은 앱도 메타버스...!",
          "original_form": "vr없이 하는  제페토 같은 앱도 메타버스...!",
          "speaker_id": "1",
          "time": "20210824 13:18"
        },
        {
          "id": "MDRW2100011482.1.5",
          "form": "오... 첨들어보네요",
          "original_form": "오... 첨들어보네요",
          "speaker_id": "2",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100011482.1.6",
          "form": "가상세계라는게 결국 게임같은거죠?",
          "original_form": "가상세계라는게 결국 게임같은거죠?",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011482.1.7",
          "form": "게임도 될 수 있고 소통의 장도 될 수 있는...?",
          "original_form": "게임도 될 수 있고 소통의 장도 될 수 있는...?",
          "speaker_id": "1",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011482.1.8",
          "form": "언젠가 한번 해보고싶네요",
          "original_form": "언젠가 한번 해보고싶네요",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100011482.1.9",
          "form": "근데 약간 펜팔같이 그냥",
          "original_form": "근데 약간 펜팔같이 그냥",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100011482.1.10",
          "form": "문자 보내놓고는 언제 답올지 모르고 누군지도 모르는게",
          "original_form": "문자 보내놓고는 언제 답올지 모르고 누군지도 모르는게",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100011482.1.11",
          "form": "더 재밌는것같긴 해요",
          "original_form": "더 재밌는것같긴 해요",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100011482.1.12",
          "form": "이거처럼",
          "original_form": "이거처럼",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100011482.1.13",
          "form": "으르신취향이신가여ㅋㅋ",
          "original_form": "으르신취향이신가여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:16"
        },
        {
          "id": "MDRW2100011482.1.14",
          "form": "으르렁 취향입니다",
          "original_form": "으르렁 취향입니다",
          "speaker_id": "2",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100011482.1.15",
          "form": "전 메타버스 얼른 서비스됐으면 좋겠는데",
          "original_form": "전 메타버스 얼른 서비스됐으면 좋겠는데",
          "speaker_id": "1",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100011482.1.16",
          "form": "그거 되면 뭐 해외도 갈 수 있나여",
          "original_form": "그거 되면 뭐 해외도 갈 수 있나여",
          "speaker_id": "2",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100011482.1.17",
          "form": "그쵸ㅋㅋㅋ만약 미각 후각 촉각 다 진짜 같이 되면 전 맛있는 음식 많이 먹을거에요",
          "original_form": "그쵸ㅋㅋㅋ만약 미각 후각 촉각 다 진짜 같이 되면 전 맛있는 음식 많이 먹을거에요",
          "speaker_id": "1",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100011482.1.18",
          "form": "거기서 먹어도 살 안찌니까!",
          "original_form": "거기서 먹어도 살 안찌니까!",
          "speaker_id": "1",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100011482.1.19",
          "form": "헐 그런것도돼요?",
          "original_form": "헐 그런것도돼요?",
          "speaker_id": "2",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100011482.1.20",
          "form": "넹ㅋㅋ레디플레이어원 영화 한 번 시간날 때 봐봐요",
          "original_form": "넹ㅋㅋ레디플레이어원 영화 한 번 시간날 때 봐봐요",
          "speaker_id": "1",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100011482.1.21",
          "form": "그거 될때쯤 할아버지 돼있을듯",
          "original_form": "그거 될때쯤 할아버지 돼있을듯",
          "speaker_id": "2",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100011482.1.22",
          "form": "이미 지금 실제로도 후각하고 촉각 연구중이래요",
          "original_form": "이미 지금 실제로도 후각하고 촉각 연구중이래요",
          "speaker_id": "1",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100011482.1.23",
          "form": "미각도",
          "original_form": "미각도",
          "speaker_id": "1",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100011482.1.24",
          "form": "하긴 김연아의 햅틱 쓰던 시절 생각하면 스마트폰도 거의 뭐",
          "original_form": "하긴 김연아의 햅틱 쓰던 시절 생각하면 스마트폰도 거의 뭐",
          "speaker_id": "2",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100011482.1.25",
          "form": "메타버스도 10년 안에 될지도?",
          "original_form": "메타버스도 10년 안에 될지도?",
          "speaker_id": "2",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100011482.1.26",
          "form": "어느정도 진전있는 거 보면 20년 내로 완벽한게 나올거 같아요",
          "original_form": "어느정도 진전있는 거 보면 20년 내로 완벽한게 나올거 같아요",
          "speaker_id": "1",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100011482.1.27",
          "form": "아ㅋㅋ그러고보니 메타버스 지금도 서비스되고있긴한데",
          "original_form": "아ㅋㅋ그러고보니 메타버스 지금도 서비스되고있긴한데",
          "speaker_id": "1",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100011482.1.28",
          "form": "컴퓨터로 땅사는거 기사봤어요?",
          "original_form": "컴퓨터로 땅사는거 기사봤어요?",
          "speaker_id": "1",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100011482.1.29",
          "form": "실제로 돈내고 가상화폐마냥 가상토지구매ㅋㅋㅋ",
          "original_form": "실제로 돈내고 가상화폐마냥 가상토지구매ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100011482.1.30",
          "form": "제 2의 비트코인인가ㅋㅋㅋㅋㅋ",
          "original_form": "제 2의 비트코인인가ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100011482.1.31",
          "form": "현실에서 도망갈 장소가 만들어지고있는건가",
          "original_form": "현실에서 도망갈 장소가 만들어지고있는건가",
          "speaker_id": "2",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100011482.1.32",
          "form": "맨날 캡슐안에만 있는건가 그럼...",
          "original_form": "맨날 캡슐안에만 있는건가 그럼...",
          "speaker_id": "1",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100011482.1.33",
          "form": "가상세계에서는 나도 차은우가 될 수 있는건가요",
          "original_form": "가상세계에서는 나도 차은우가 될 수 있는건가요",
          "speaker_id": "2",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100011482.1.34",
          "form": "네ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:38"
        },
        {
          "id": "MDRW2100011482.1.35",
          "form": "근데 현실 돌아오면 현타올거같아요...",
          "original_form": "근데 현실 돌아오면 현타올거같아요...",
          "speaker_id": "1",
          "time": "20210824 21:38"
        },
        {
          "id": "MDRW2100011482.1.36",
          "form": "차은우랑 저는 같은 인간입니다",
          "original_form": "차은우랑 저는 같은 인간입니다",
          "speaker_id": "2",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100011482.1.37",
          "form": "아... 아닌가",
          "original_form": "아... 아닌가",
          "speaker_id": "2",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100011482.1.38",
          "form": "유인원이랑 인간만큼 차이가 있으려나",
          "original_form": "유인원이랑 인간만큼 차이가 있으려나",
          "speaker_id": "2",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100011482.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋ가상세계에서 소녀시대 윤아가 되었다가 현실로 돌아오면 전 현타올거에요...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ가상세계에서 소녀시대 윤아가 되었다가 현실로 돌아오면 전 현타올거에요...",
          "speaker_id": "1",
