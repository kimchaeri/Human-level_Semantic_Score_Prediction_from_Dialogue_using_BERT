      "utterance": [
        {
          "id": "MDRW2100030495.1.1",
          "form": "백신 예약 하심 ?",
          "original_form": "백신 예약 하심 ?",
          "speaker_id": "1",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030495.1.2",
          "form": "아뇨 예약 안했어요",
          "original_form": "아뇨 예약 안했어요",
          "speaker_id": "2",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030495.1.3",
          "form": "name1님은여?",
          "original_form": "&name1&님은여?",
          "speaker_id": "2",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030495.1.4",
          "form": "햇어요",
          "original_form": "햇어요",
          "speaker_id": "1",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030495.1.5",
          "form": "왜안햇어요",
          "original_form": "왜안햇어요",
          "speaker_id": "1",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030495.1.6",
          "form": "어차피 델타변이에는 효과도 없다하고",
          "original_form": "어차피 델타변이에는 효과도 없다하고",
          "speaker_id": "2",
          "time": "20210904 02:11"
        },
        {
          "id": "MDRW2100030495.1.7",
          "form": "좀 무섭기도하고",
          "original_form": "좀 무섭기도하고",
          "speaker_id": "2",
          "time": "20210904 02:11"
        },
        {
          "id": "MDRW2100030495.1.8",
          "form": "그래도 덜죽는다하던데요",
          "original_form": "그래도 덜죽는다하던데요",
          "speaker_id": "1",
          "time": "20210904 02:11"
        },
        {
          "id": "MDRW2100030495.1.9",
          "form": "name1님 후기보고 고민해볼게요",
          "original_form": "&name1&님 후기보고 고민해볼게요",
          "speaker_id": "2",
          "time": "20210904 02:12"
        },
        {
          "id": "MDRW2100030495.1.10",
          "form": "언제 맞아요 백신?",
          "original_form": "언제 맞아요 백신?",
          "speaker_id": "2",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030495.1.11",
          "form": "추석전이요",
          "original_form": "추석전이요",
          "speaker_id": "1",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030495.1.12",
          "form": "금욜",
          "original_form": "금욜",
          "speaker_id": "1",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030495.1.13",
          "form": "추석때 푹 쉬어야겠네요",
          "original_form": "추석때 푹 쉬어야겠네요",
          "speaker_id": "2",
          "time": "20210904 02:17"
        },
        {
          "id": "MDRW2100030495.1.14",
          "form": "아무렇지도 않음 좋겟네요",
          "original_form": "아무렇지도 않음 좋겟네요",
          "speaker_id": "1",
          "time": "20210904 02:32"
        },
        {
          "id": "MDRW2100030495.1.15",
          "form": "놀게",
          "original_form": "놀게",
          "speaker_id": "1",
          "time": "20210904 02:32"
        },
        {
          "id": "MDRW2100030495.1.16",
          "form": "화이자 맞으세요?",
          "original_form": "화이자 맞으세요?",
          "speaker_id": "2",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100030495.1.17",
          "form": "아직 몰라요",
          "original_form": "아직 몰라요",
          "speaker_id": "1",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100030495.1.18",
          "form": "언제알려줘요?",
          "original_form": "언제알려줘요?",
          "speaker_id": "1",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100030495.1.19",
          "form": "아 진짜요?? 본인이 선택하는거 아니에요?",
          "original_form": "아 진짜요?? 본인이 선택하는거 아니에요?",
          "speaker_id": "2",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100030495.1.20",
          "form": "네 아님 모더나일수도잇어용",
          "original_form": "네 아님 모더나일수도잇어용",
          "speaker_id": "1",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100030495.1.21",
          "form": "글쿤요 근데 왜 주변사람들은 다 자기 뭐 맞을거라고 얘기했나 몰겄네요",
          "original_form": "글쿤요 근데 왜 주변사람들은 다 자기 뭐 맞을거라고 얘기했나 몰겄네요",
          "speaker_id": "2",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100030495.1.22",
          "form": "근데 몇일전에 카톡 온대요 먼지",
          "original_form": "근데 몇일전에 카톡 온대요 먼지",
