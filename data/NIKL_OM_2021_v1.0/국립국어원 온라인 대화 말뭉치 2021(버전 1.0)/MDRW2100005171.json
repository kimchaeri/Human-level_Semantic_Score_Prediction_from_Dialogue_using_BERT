      "utterance": [
        {
          "id": "MDRW2100005171.1.1",
          "form": "뮤지컬 보는 거 좋아해?",
          "original_form": "뮤지컬 보는 거 좋아해?",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005171.1.2",
          "form": "웅웅 넘조아해",
          "original_form": "웅웅 넘조아해",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005171.1.3",
          "form": "근데 자주는 못봐 비싸기도하곸ㅋㅋ 멀기도해서ㅠㅠ",
          "original_form": "근데 자주는 못봐 비싸기도하곸ㅋㅋ 멀기도해서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005171.1.4",
          "form": "맞아맞아 나도 뮤지컬 보는 거 좋아하는데 한 번 가면 좀 부담돼",
          "original_form": "맞아맞아 나도 뮤지컬 보는 거 좋아하는데 한 번 가면 좀 부담돼",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005171.1.5",
          "form": "그렇다고 저렴한 곳에서 보자니 무대랑 너무 멀러ㅠ",
          "original_form": "그렇다고 저렴한 곳에서 보자니 무대랑 너무 멀러ㅠ",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005171.1.6",
          "form": "맞아맞아 이왕볼거 비싸게 좋은데서 봐야지",
          "original_form": "맞아맞아 이왕볼거 비싸게 좋은데서 봐야지",
          "speaker_id": "1",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005171.1.7",
          "form": "맞아 2시간을 달려서 가는뎈ㅋㅋㅋ",
          "original_form": "맞아 2시간을 달려서 가는뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005171.1.8",
          "form": "제일 최근엔 뭐봤어?",
          "original_form": "제일 최근엔 뭐봤어?",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005171.1.9",
          "form": "나 사실 뮤지컬은 하나밖에 안 봤엉",
          "original_form": "나 사실 뮤지컬은 하나밖에 안 봤엉",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005171.1.10",
          "form": "오페라의 유령",
          "original_form": "오페라의 유령",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005171.1.11",
          "form": "너무 좋았어서 그 기억으로 사는 중ㅠㅠ",
          "original_form": "너무 좋았어서 그 기억으로 사는 중ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005171.1.12",
          "form": "오 띵작을 봤네",
          "original_form": "오 띵작을 봤네",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005171.1.13",
          "form": "너는 뭐봤어?",
          "original_form": "너는 뭐봤어?",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005171.1.14",
          "form": "난 웃는남자!! 랑 귀환",
          "original_form": "난 웃는남자!! 랑 귀환",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005171.1.15",
          "form": "오페라의유령 영화로만 봤는데 영화랑 많이달라?.?",
          "original_form": "오페라의유령 영화로만 봤는데 영화랑 많이달라?.?",
          "speaker_id": "2",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005171.1.16",
          "form": "연출에 차이만 있을 뿐 영화랑 완전 똑같아!",
          "original_form": "연출에 차이만 있을 뿐 영화랑 완전 똑같아!",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005171.1.17",
          "form": "와 더 보고싶어짐..ㅠ",
          "original_form": "와 더 보고싶어짐..ㅠ",
          "speaker_id": "2",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005171.1.18",
          "form": "또 내한했으면 좋겠다..",
          "original_form": "또 내한했으면 좋겠다..",
          "speaker_id": "1",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005171.1.19",
          "form": "그럼 너 콘서트 가 본 적 있어?",
          "original_form": "그럼 너 콘서트 가 본 적 있어?",
          "speaker_id": "1",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005171.1.20",
          "form": "ㄴㅏ..엑소는 못가봤구",
          "original_form": "ㄴㅏ..엑소는 못가봤구",
          "speaker_id": "2",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005171.1.21",
          "form": "이선희랑 컬투 콘서트 가봤서",
          "original_form": "이선희랑 컬투 콘서트 가봤서",
          "speaker_id": "2",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005171.1.22",
          "form": "우와우와 대박",
          "original_form": "우와우와 대박",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005171.1.23",
          "form": "어땠어?? 재밌었어??",
          "original_form": "어땠어?? 재밌었어??",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005171.1.24",
          "form": "이선희 콘서트는 ㄹㅇ 귀호강이어ㅛ고",
          "original_form": "이선희 콘서트는 ㄹㅇ 귀호강이어ㅛ고",
          "speaker_id": "2",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005171.1.25",
          "form": "컬투는 토크쇼에 노래가 가미된느낌ㅋㅋㅋㅋ",
          "original_form": "컬투는 토크쇼에 노래가 가미된느낌ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005171.1.26",
          "form": "와와 재밌었겠다ㅠㅠ",
          "original_form": "와와 재밌었겠다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 21:06"
        }
      ]
