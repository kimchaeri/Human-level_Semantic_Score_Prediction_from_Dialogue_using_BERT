      "utterance": [
        {
          "id": "MDRW2100016377.1.1",
          "form": "유튭 보시나요",
          "original_form": "유튭 보시나요",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016377.1.2",
          "form": "전 집순이에 영화덕후라서 많이봐요",
          "original_form": "전 집순이에 영화덕후라서 많이봐요",
          "speaker_id": "2",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016377.1.3",
          "form": "유툽은 많이 보진 않아요",
          "original_form": "유툽은 많이 보진 않아요",
          "speaker_id": "2",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016377.1.4",
          "form": "넷플은 보시나요",
          "original_form": "넷플은 보시나요",
          "speaker_id": "1",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100016377.1.5",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100016377.1.6",
          "form": "어떤거보시나요",
          "original_form": "어떤거보시나요",
          "speaker_id": "1",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100016377.1.7",
          "form": "기묘한이야기 좋아해요",
          "original_form": "기묘한이야기 좋아해요",
          "speaker_id": "2",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100016377.1.8",
          "form": "넷플 보세요?",
          "original_form": "넷플 보세요?",
          "speaker_id": "2",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100016377.1.9",
          "form": "네 봅니다ㅎㅎ",
          "original_form": "네 봅니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100016377.1.10",
          "form": "최근엔 뭐보셧나요?",
          "original_form": "최근엔 뭐보셧나요?",
          "speaker_id": "2",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100016377.1.11",
          "form": "영화관에서는 모가디슈 봣어요",
          "original_form": "영화관에서는 모가디슈 봣어요",
          "speaker_id": "2",
          "time": "20210826 13:00"
        },
        {
          "id": "MDRW2100016377.1.12",
          "form": "오 저도 모가디슈봤어요",
          "original_form": "오 저도 모가디슈봤어요",
          "speaker_id": "1",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100016377.1.13",
          "form": "유툽은 주로 어떤거보세요?",
          "original_form": "유툽은 주로 어떤거보세요?",
          "speaker_id": "2",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100016377.1.14",
          "form": "여행 유튭이요",
          "original_form": "여행 유튭이요",
          "speaker_id": "1",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100016377.1.15",
          "form": "추천유투버잇나요?",
          "original_form": "추천유투버잇나요?",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016377.1.16",
          "form": "뜨랑낄로요!",
          "original_form": "뜨랑낄로요!",
