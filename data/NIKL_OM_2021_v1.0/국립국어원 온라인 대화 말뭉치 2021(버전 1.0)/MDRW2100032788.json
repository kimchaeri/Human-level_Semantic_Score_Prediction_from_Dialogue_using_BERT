      "utterance": [
        {
          "id": "MDRW2100032788.1.1",
          "form": "내 새로 산 버즈 2봤어???",
          "original_form": "내 새로 산 버즈 2봤어???",
          "speaker_id": "1",
          "time": "20210906 01:13"
        },
        {
          "id": "MDRW2100032788.1.2",
          "form": "응 귀엽더라 얼마주고샀어",
          "original_form": "응 귀엽더라 얼마주고샀어",
          "speaker_id": "2",
          "time": "20210906 01:13"
        },
        {
          "id": "MDRW2100032788.1.3",
          "form": "15만원 정도",
          "original_form": "15만원 정도",
          "speaker_id": "1",
          "time": "20210906 01:14"
        },
        {
          "id": "MDRW2100032788.1.4",
          "form": "원래 쓰던거 있잖아",
          "original_form": "원래 쓰던거 있잖아",
          "speaker_id": "2",
          "time": "20210906 01:15"
        },
        {
          "id": "MDRW2100032788.1.5",
          "form": "원래 쓰던거 있는데 2년 썼더니 요즘 지지직거리더라구",
          "original_form": "원래 쓰던거 있는데 2년 썼더니 요즘 지지직거리더라구",
          "speaker_id": "1",
          "time": "20210906 01:15"
        },
        {
          "id": "MDRW2100032788.1.6",
          "form": "2년 써서 수리하기도 좀 그렇고",
          "original_form": "2년 써서 수리하기도 좀 그렇고",
          "speaker_id": "1",
          "time": "20210906 01:15"
        },
        {
          "id": "MDRW2100032788.1.7",
          "form": "새로 산것도 똑같은 기능이야???",
          "original_form": "새로 산것도 똑같은 기능이야???",
          "speaker_id": "2",
          "time": "20210906 01:16"
        },
        {
          "id": "MDRW2100032788.1.8",
          "form": "나온지 얼마안돼서 좀 더 기능이 많지",
          "original_form": "나온지 얼마안돼서 좀 더 기능이 많지",
          "speaker_id": "1",
          "time": "20210906 01:16"
        },
        {
          "id": "MDRW2100032788.1.9",
          "form": "크기도 작고",
          "original_form": "크기도 작고",
          "speaker_id": "1",
          "time": "20210906 01:16"
        },
        {
          "id": "MDRW2100032788.1.10",
          "form": "노이즈캔슬링이라는 기능도 있어",
          "original_form": "노이즈캔슬링이라는 기능도 있어",
          "speaker_id": "1",
          "time": "20210906 01:16"
        },
        {
          "id": "MDRW2100032788.1.11",
          "form": "그게 뭔데",
          "original_form": "그게 뭔데",
          "speaker_id": "2",
          "time": "20210906 01:17"
        },
        {
          "id": "MDRW2100032788.1.12",
          "form": "주변 소음을 줄여주는거래",
          "original_form": "주변 소음을 줄여주는거래",
          "speaker_id": "1",
          "time": "20210906 01:17"
        },
        {
          "id": "MDRW2100032788.1.13",
          "form": "집에서 써봤는데 집에서는 효과없고",
          "original_form": "집에서 써봤는데 집에서는 효과없고",
          "speaker_id": "1",
          "time": "20210906 01:17"
        },
        {
          "id": "MDRW2100032788.1.14",
          "form": "지하철이 대박이더라구",
          "original_form": "지하철이 대박이더라구",
          "speaker_id": "1",
          "time": "20210906 01:18"
        },
        {
          "id": "MDRW2100032788.1.15",
          "form": "왜 지하철이 엄청시끄러워서 그런가",
          "original_form": "왜 지하철이 엄청시끄러워서 그런가",
          "speaker_id": "2",
          "time": "20210906 01:18"
        },
        {
          "id": "MDRW2100032788.1.16",
          "form": "그런가봐 지하철에서 덜커덩하는 소리가 덜 들려",
          "original_form": "그런가봐 지하철에서 덜커덩하는 소리가 덜 들려",
          "speaker_id": "1",
          "time": "20210906 01:18"
        },
        {
          "id": "MDRW2100032788.1.17",
          "form": "엄마도 하나 갖고싶네~",
          "original_form": "엄마도 하나 갖고싶네~",
          "speaker_id": "2",
          "time": "20210906 01:19"
        },
        {
          "id": "MDRW2100032788.1.18",
          "form": "엄마 돈으로 사",
          "original_form": "엄마 돈으로 사",
