        {
          "id": "MDRW2100016576.1.1",
          "form": "name2님음 건강하신지",
          "original_form": "&name2&님음 건강하신지",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016576.1.2",
          "form": "요즘 식도염 위염 도졌어요..ㅜㅜ",
          "original_form": "요즘 식도염 위염 도졌어요..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 14:56"
        },
        {
          "id": "MDRW2100016576.1.3",
          "form": "하이고 왜그런담 ㅠ 스트레슨가",
          "original_form": "하이고 왜그런담 ㅠ 스트레슨가",
          "speaker_id": "1",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016576.1.4",
          "form": "맞아요 다 회사 스트레스때문",
          "original_form": "맞아요 다 회사 스트레스때문",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016576.1.5",
          "form": "퇴사하면 다 나을거같은데",
          "original_form": "퇴사하면 다 나을거같은데",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016576.1.6",
          "form": "아휴ㅠㅠ증말... 영양제는드시는지 ?",
          "original_form": "아휴ㅠㅠ증말... 영양제는드시는지 ?",
          "speaker_id": "1",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016576.1.7",
          "form": "위건장 장건강에좋다는 유산균 먹고있는데ㅜㅜ",
          "original_form": "위건장 장건강에좋다는 유산균 먹고있는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 16:08"
        },
        {
          "id": "MDRW2100016576.1.8",
          "form": "뭐 추천해주실만한 영영제라도?",
          "original_form": "뭐 추천해주실만한 영영제라도?",
          "speaker_id": "2",
          "time": "20210826 16:08"
        },
        {
          "id": "MDRW2100016576.1.9",
          "form": "걍 물어밧어요 사실 저도잘 안챙겨먹어서",
          "original_form": "걍 물어밧어요 사실 저도잘 안챙겨먹어서",
          "speaker_id": "1",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016576.1.10",
          "form": "근데 다들 멀티비타민 오메가3 유산균 필수3개로추천하드라구욤...",
          "original_form": "근데 다들 멀티비타민 오메가3 유산균 필수3개로추천하드라구욤...",
          "speaker_id": "1",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016576.1.11",
          "form": "요즘 운동하시는거 있나요 ^^",
          "original_form": "요즘 운동하시는거 있나요 ^^",
          "speaker_id": "2",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016576.1.12",
          "form": "검도요검도",
          "original_form": "검도요검도",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016576.1.13",
          "form": "최고의검객이되려규요 건강하게!!!",
          "original_form": "최고의검객이되려규요 건강하게!!!",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016576.1.14",
          "form": "검도복입은거 완전 멋있으실듯 +!!!",
          "original_form": "검도복입은거 완전 멋있으실듯 +!!!",
          "speaker_id": "2",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100016576.1.15",
          "form": "검도는 그 칼 사야하나요",
          "original_form": "검도는 그 칼 사야하나요",
          "speaker_id": "2",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100016576.1.16",
          "form": "얼마에요",
          "original_form": "얼마에요",
          "speaker_id": "2",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100016576.1.17",
          "form": "이벤트하면 공짜루도 줘용",
          "original_form": "이벤트하면 공짜루도 줘용",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016576.1.18",
          "form": "보여드리구싶네용 검도하실생각 없나요",
          "original_form": "보여드리구싶네용 검도하실생각 없나요",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016576.1.19",
          "form": "돈내고 운동을 왜합니까???",
          "original_form": "돈내고 운동을 왜합니까???",
          "speaker_id": "2",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016576.1.20",
          "form": "돈줘도 할까말까입니다!!",
          "original_form": "돈줘도 할까말까입니다!!",
          "speaker_id": "2",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016576.1.21",
          "form": "ㅎㅎ...건강챙기셔야죠..",
          "original_form": "ㅎㅎ...건강챙기셔야죠..",
          "speaker_id": "1",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016576.1.22",
          "form": "아...그쵸ㅡ....그래서 제 건강이 이모양이꼴인가봅니다 ㅎㅎ",
          "original_form": "아...그쵸ㅡ....그래서 제 건강이 이모양이꼴인가봅니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100016576.1.23",
          "form": "건강검진 한번해야하는데 말이죠",
          "original_form": "건강검진 한번해야하는데 말이죠",
          "speaker_id": "2",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100016576.1.24",
          "form": "꼭 함 받아보세요 저도받아야하는데..",
          "original_form": "꼭 함 받아보세요 저도받아야하는데..",
