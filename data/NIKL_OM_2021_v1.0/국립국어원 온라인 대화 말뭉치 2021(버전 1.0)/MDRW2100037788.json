      "utterance": [
        {
          "id": "MDRW2100037788.1.1",
          "form": "ㅇㅈ",
          "original_form": "ㅇㅈ",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100037788.1.2",
          "form": "나 지중해 가고시퍼",
          "original_form": "나 지중해 가고시퍼",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100037788.1.3",
          "form": "하연 벽에 파란 지붕",
          "original_form": "하연 벽에 파란 지붕",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100037788.1.4",
          "form": "이쁜 건물!",
          "original_form": "이쁜 건물!",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100037788.1.5",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100037788.1.6",
          "form": "파란 하늘",
          "original_form": "파란 하늘",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100037788.1.7",
          "form": "그리스? 뭐 그런 쪽",
          "original_form": "그리스? 뭐 그런 쪽",
          "speaker_id": "1",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100037788.1.8",
          "form": "에메랄드빛 바다!",
          "original_form": "에메랄드빛 바다!",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037788.1.9",
          "form": "꺄아ㅏ",
          "original_form": "꺄아ㅏ",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037788.1.10",
          "form": "마자마자",
          "original_form": "마자마자",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037788.1.11",
          "form": "유렵 가고시퍼",
          "original_form": "유렵 가고시퍼",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037788.1.12",
          "form": "지리는 모르나 하얗고 파랗고 예쁜ㅋㅋㅋ",
          "original_form": "지리는 모르나 하얗고 파랗고 예쁜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037788.1.13",
          "form": "유럽가서",
          "original_form": "유럽가서",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037788.1.14",
          "form": "와인도 먹고",
          "original_form": "와인도 먹고",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037788.1.15",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037788.1.16",
          "form": "치즈!",
          "original_form": "치즈!",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037788.1.17",
          "form": "꺄아!",
          "original_form": "꺄아!",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037788.1.18",
          "form": "이탈리아 가서",
          "original_form": "이탈리아 가서",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037788.1.19",
          "form": "젤라또 먹어보고시퍼",
          "original_form": "젤라또 먹어보고시퍼",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037788.1.20",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "1",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037788.1.21",
          "form": "꿈파 보고 젤라또 너무 먹고 싶었음",
          "original_form": "꿈파 보고 젤라또 너무 먹고 싶었음",
          "speaker_id": "1",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037788.1.22",
          "form": "맛있겠다...",
          "original_form": "맛있겠다...",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037788.1.23",
          "form": "ㅇㅈ",
          "original_form": "ㅇㅈ",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037788.1.24",
          "form": "냠냠...",
          "original_form": "냠냠...",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037788.1.25",
          "form": "아 식도락 여행이 최고야",
          "original_form": "아 식도락 여행이 최고야",
          "speaker_id": "1",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037788.1.26",
          "form": "맛있는 거 먹으러 다니면 그게 힐링이지",
          "original_form": "맛있는 거 먹으러 다니면 그게 힐링이지",
          "speaker_id": "1",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037788.1.27",
          "form": "마자마자!",
          "original_form": "마자마자!",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100037788.1.28",
          "form": "나중에 여행가면",
          "original_form": "나중에 여행가면",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100037788.1.29",
          "form": "사진 많이 찍자!",
          "original_form": "사진 많이 찍자!",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100037788.1.30",
          "form": "다 남기자!",
          "original_form": "다 남기자!",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100037788.1.31",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "1",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100037788.1.32",
          "form": "누구 결혼하면 내가 다 모아서",
          "original_form": "누구 결혼하면 내가 다 모아서",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100037788.1.33",
          "form": "영상 만들어서",
          "original_form": "영상 만들어서",
          "speaker_id": "2",
          "time": "20210901 18:31"
        }
      ]
    }
  ]
