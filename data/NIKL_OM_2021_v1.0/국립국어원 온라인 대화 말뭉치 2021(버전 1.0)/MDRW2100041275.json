      "utterance": [
        {
          "id": "MDRW2100041275.1.1",
          "form": "근데 진짜 만만한 해외여행",
          "original_form": "근데 진짜 만만한 해외여행",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100041275.1.2",
          "form": "일본이 최고였는데",
          "original_form": "일본이 최고였는데",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100041275.1.3",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100041275.1.4",
          "form": "그건 ㅇㅈ",
          "original_form": "그건 ㅇㅈ",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100041275.1.5",
          "form": "일본도 가면되지머 ㅋ",
          "original_form": "일본도 가면되지머 ㅋ",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100041275.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100041275.1.7",
          "form": "난 아빠가 싫어하드랔ㅋㅋㅋㅋㅋ",
          "original_form": "난 아빠가 싫어하드랔ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100041275.1.8",
          "form": "그래서 카메라 사는것도",
          "original_form": "그래서 카메라 사는것도",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100041275.1.9",
          "form": "여.자.한.테 조국이 어딨음 ㅎㅎ",
          "original_form": "여.자.한.테 조국이 어딨음 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100041275.1.10",
          "form": "돈 안 보태 준다길래",
          "original_form": "돈 안 보태 준다길래",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100041275.1.11",
          "form": "근데 아버님",
          "original_form": "근데 아버님",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100041275.1.12",
          "form": "일본만화책 좋아하시지않음?",
          "original_form": "일본만화책 좋아하시지않음?",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100041275.1.13",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100041275.1.14",
          "form": "ㄴㄴ 그거 한국꺼임",
          "original_form": "ㄴㄴ 그거 한국꺼임",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100041275.1.15",
          "form": "아진짜?????????",
          "original_form": "아진짜?????????",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100041275.1.16",
          "form": "대통령 상도 받았다나 어쩐다나",
          "original_form": "대통령 상도 받았다나 어쩐다나",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100041275.1.17",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100041275.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100041275.1.19",
          "form": "헐...이번달에 들은말중에 제일놀랏다",
          "original_form": "헐...이번달에 들은말중에 제일놀랏다",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100041275.1.20",
          "form": "열혈강호 말하는거 맞제",
          "original_form": "열혈강호 말하는거 맞제",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100041275.1.21",
          "form": "아 그거였나? ㅇㅇ맞음",
          "original_form": "아 그거였나? ㅇㅇ맞음",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100041275.1.22",
          "form": "응 한국꺼ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "응 한국꺼ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100041275.1.23",
          "form": "그래서 내가 코난 전권 살랬는데",
          "original_form": "그래서 내가 코난 전권 살랬는데",
          "speaker_id": "2",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100041275.1.24",
          "form": "표.지.보.고 당연히 일본만화인줄 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "표.지.보.고 당연히 일본만화인줄 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100041275.1.25",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100041275.1.26",
          "form": "막힌거얔ㅋㅋㅋㅋㅋㅋ",
          "original_form": "막힌거얔ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100041275.1.27",
          "form": "아빠를 보라면서",
          "original_form": "아빠를 보라면서",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100041275.1.28",
          "form": "암튼",
          "original_form": "암튼",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100041275.1.29",
          "form": "본론으로 돌아와서",
          "original_form": "본론으로 돌아와서",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100041275.1.30",
          "form": "ㅇㅋㅇㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅋㅇㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100041275.1.31",
          "form": "경주 어디어디 가봤어??",
          "original_form": "경주 어디어디 가봤어??",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100041275.1.32",
          "form": "초딩중딩때 수련회로 불국사석굴암밖에 안가봄",
          "original_form": "초딩중딩때 수련회로 불국사석굴암밖에 안가봄",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100041275.1.33",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
