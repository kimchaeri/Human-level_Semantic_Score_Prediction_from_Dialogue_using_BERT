      "utterance": [
        {
          "id": "MDRW2100017407.1.1",
          "form": "혹시 강아지 키워요?",
          "original_form": "혹시 강아지 키워요?",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017407.1.2",
          "form": "아니요 유투브로 강아지 보면서 귀여워만 해요 ㅠㅠ",
          "original_form": "아니요 유투브로 강아지 보면서 귀여워만 해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017407.1.3",
          "form": "강아지 키우세요?",
          "original_form": "강아지 키우세요?",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017407.1.4",
          "form": "저도 안 키워요ㅋㅋㅋ 그냥 친구네 강아지 귀여워하고",
          "original_form": "저도 안 키워요ㅋㅋㅋ 그냥 친구네 강아지 귀여워하고",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017407.1.5",
          "form": "저도 랜선으로만 귀여워하네요ㅋㅋㅋ",
          "original_form": "저도 랜선으로만 귀여워하네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017407.1.6",
          "form": "네 정말 준비가 됏을때 키우려구요 ㅋㅋㅋㅋ",
          "original_form": "네 정말 준비가 됏을때 키우려구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017407.1.7",
          "form": "친구네 강아지는 몇살이에요?",
          "original_form": "친구네 강아지는 몇살이에요?",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017407.1.8",
          "form": "4년전 쯤에 막 태어난지 얼마 안됐을때 친구집 왔어요",
          "original_form": "4년전 쯤에 막 태어난지 얼마 안됐을때 친구집 왔어요",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017407.1.9",
          "form": "봄이 될 때쯤 가족이 되어서 이름 name3라고 지어줬어요",
          "original_form": "봄이 될 때쯤 가족이 되어서 이름 &name3&라고 지어줬어요",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017407.1.10",
          "form": "이름 너무 예쁘네요",
          "original_form": "이름 너무 예쁘네요",
          "speaker_id": "2",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017407.1.11",
          "form": "친구 어머니께서 이름 name3로 짓고싶어하셨는데",
          "original_form": "친구 어머니께서 이름 &name3&로 짓고싶어하셨는데",
          "speaker_id": "1",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017407.1.12",
          "form": "친구가 기겁하며 name3로 지어갔어요",
          "original_form": "친구가 기겁하며 &name3&로 지어갔어요",
          "speaker_id": "1",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017407.1.13",
          "form": "근데 name3도 나름 레트로하고 힙한 느낌이 있어서 저는 마음에 들던데ㅋㅋㅋㅋ",
          "original_form": "근데 &name3&도 나름 레트로하고 힙한 느낌이 있어서 저는 마음에 들던데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017407.1.14",
          "form": "name3 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "&name3& ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017407.1.15",
          "form": "레트로하긴 한데 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "레트로하긴 한데 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017407.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ강아지 이름을 촌스럽게 지어야한다는 주의셨는지",
          "original_form": "ㅋㅋㅋㅋㅋㅋ강아지 이름을 촌스럽게 지어야한다는 주의셨는지",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017407.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋname3가 더 예쁜것같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ&name3&가 더 예쁜것같아요",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017407.1.18",
          "form": "맞아요 name3 예쁜 이름이에요",
          "original_form": "맞아요 &name3& 예쁜 이름이에요",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017407.1.19",
          "form": "근데 name3 이름 가진 강아지들 많긴 하더라구요",
          "original_form": "근데 &name3& 이름 가진 강아지들 많긴 하더라구요",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017407.1.20",
          "form": "name3라 지었으면 좀 독보적이지 않았을지",
          "original_form": "&name3&라 지었으면 좀 독보적이지 않았을지",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017407.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ아 그건그렇네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 그건그렇네요",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017407.1.22",
          "form": "돋보이긴 했을듯ㅋㅋㅋ",
          "original_form": "돋보이긴 했을듯ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017407.1.23",
          "form": "네 다음에 강아지 키우게 되시면 한번 고려해주세요",
          "original_form": "네 다음에 강아지 키우게 되시면 한번 고려해주세요",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017407.1.24",
          "form": "저는 원래 음식이름을 지으려고 햇거든요 ㅋㅋㅋㅋ",
          "original_form": "저는 원래 음식이름을 지으려고 햇거든요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017407.1.25",
          "form": "흔하지만 ㅋㅋㅋㅋㅋ",
          "original_form": "흔하지만 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017407.1.26",
          "form": "와 음식도 좋죠",
          "original_form": "와 음식도 좋죠",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017407.1.27",
          "form": "초코 이런 이름 많자나요",
          "original_form": "초코 이런 이름 많자나요",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017407.1.28",
          "form": "스콘 머핀 이런거 생각햇엇거든요",
          "original_form": "스콘 머핀 이런거 생각햇엇거든요",
          "speaker_id": "2",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100017407.1.29",
          "form": "아 좀 더 세련됐네요ㅋㅋㅋㅋ",
          "original_form": "아 좀 더 세련됐네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017407.1.30",
          "form": "본적없어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "본적없어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017407.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ세련되어서라기보단 디저트를 워낙 좋아해서 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ세련되어서라기보단 디저트를 워낙 좋아해서 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017407.1.32",
          "form": "ㅋㅋㅋㅋ부르다보면 디저트 생각 나겠어요",
          "original_form": "ㅋㅋㅋㅋ부르다보면 디저트 생각 나겠어요",
          "speaker_id": "1",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100017407.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋname1님은 생각해둔 이름같은거 있으세요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ&name1&님은 생각해둔 이름같은거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 20:04"
        },
        {
          "id": "MDRW2100017407.1.34",
          "form": "저는 진짜 이름 너무 직관적으로 지어서",
          "original_form": "저는 진짜 이름 너무 직관적으로 지어서",
          "speaker_id": "1",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100017407.1.35",
          "form": "옛날에 키운 달팽이들도 달피 해피 미피 소피 이런식으로 이어갔는데",
          "original_form": "옛날에 키운 달팽이들도 달피 해피 미피 소피 이런식으로 이어갔는데",
          "speaker_id": "1",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100017407.1.36",
          "form": "강아지 키우면 아마 몽몽이",
          "original_form": "강아지 키우면 아마 몽몽이",
          "speaker_id": "1",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100017407.1.37",
          "form": "ㅋㅋㅋㅋㅋ단순한데 귀여운데요",
          "original_form": "ㅋㅋㅋㅋㅋ단순한데 귀여운데요",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
