      "utterance": [
        {
          "id": "MDRW2100003856.1.1",
          "form": "전공이 뭐예요?",
          "original_form": "전공이 뭐예요?",
          "speaker_id": "2",
          "time": "20210821 13:00"
        },
        {
          "id": "MDRW2100003856.1.2",
          "form": "조리를 전공했어요",
          "original_form": "조리를 전공했어요",
          "speaker_id": "1",
          "time": "20210821 13:02"
        },
        {
          "id": "MDRW2100003856.1.3",
          "form": "오 요리 하셨어요? 멋있다",
          "original_form": "오 요리 하셨어요? 멋있다",
          "speaker_id": "2",
          "time": "20210821 13:03"
        },
        {
          "id": "MDRW2100003856.1.4",
          "form": "전 외국어 전공했어요",
          "original_form": "전 외국어 전공했어요",
          "speaker_id": "2",
          "time": "20210821 13:03"
        },
        {
          "id": "MDRW2100003856.1.5",
          "form": "우와 외국어 더 멋지세요",
          "original_form": "우와 외국어 더 멋지세요",
          "speaker_id": "1",
          "time": "20210821 13:03"
        },
        {
          "id": "MDRW2100003856.1.6",
          "form": "저는 언어쪽은 뭔가 어려워서",
          "original_form": "저는 언어쪽은 뭔가 어려워서",
          "speaker_id": "1",
          "time": "20210821 13:03"
        },
        {
          "id": "MDRW2100003856.1.7",
          "form": "어떤 외국어를 전공하셨나요",
          "original_form": "어떤 외국어를 전공하셨나요",
          "speaker_id": "1",
          "time": "20210821 13:04"
        },
        {
          "id": "MDRW2100003856.1.8",
          "form": "조리면 실습이 많겠네요",
          "original_form": "조리면 실습이 많겠네요",
          "speaker_id": "2",
          "time": "20210821 13:05"
        },
        {
          "id": "MDRW2100003856.1.9",
          "form": "전 러시아어 전공했어요",
          "original_form": "전 러시아어 전공했어요",
          "speaker_id": "2",
          "time": "20210821 13:05"
        },
        {
          "id": "MDRW2100003856.1.10",
          "form": "아무래도 다른 전공보다는 많은 편인거 같아요",
          "original_form": "아무래도 다른 전공보다는 많은 편인거 같아요",
          "speaker_id": "1",
          "time": "20210821 13:05"
        },
        {
          "id": "MDRW2100003856.1.11",
          "form": "우와 러시아어 너무 멋져요",
          "original_form": "우와 러시아어 너무 멋져요",
          "speaker_id": "1",
          "time": "20210821 13:05"
        },
        {
          "id": "MDRW2100003856.1.12",
          "form": "러시아어는 발음이 어렵지 않은가요?",
          "original_form": "러시아어는 발음이 어렵지 않은가요?",
          "speaker_id": "1",
          "time": "20210821 13:06"
        },
        {
          "id": "MDRW2100003856.1.13",
          "form": "실습이 많으면 과제도 많겠네요?",
          "original_form": "실습이 많으면 과제도 많겠네요?",
          "speaker_id": "2",
          "time": "20210821 13:09"
        },
        {
          "id": "MDRW2100003856.1.14",
          "form": "네 정말 어려워서 1학년때 내가 이걸 왜 하지",
          "original_form": "네 정말 어려워서 1학년때 내가 이걸 왜 하지",
          "speaker_id": "2",
          "time": "20210821 13:09"
        },
        {
          "id": "MDRW2100003856.1.15",
          "form": "자괴감도 들었어요",
          "original_form": "자괴감도 들었어요",
          "speaker_id": "2",
          "time": "20210821 13:10"
        },
        {
          "id": "MDRW2100003856.1.16",
          "form": "어떤 기분일지 알거같아요",
          "original_form": "어떤 기분일지 알거같아요",
          "speaker_id": "1",
          "time": "20210821 13:10"
        },
        {
          "id": "MDRW2100003856.1.17",
          "form": "약간 교수님들 나는 다른 교수님한테도 이만큼 받았는데 아랑곳하지않고 또 이만큼..",
          "original_form": "약간 교수님들 나는 다른 교수님한테도 이만큼 받았는데 아랑곳하지않고 또 이만큼..",
          "speaker_id": "1",
          "time": "20210821 13:10"
        },
        {
          "id": "MDRW2100003856.1.18",
          "form": "전 기계가 아닌데 말이죠",
          "original_form": "전 기계가 아닌데 말이죠",
          "speaker_id": "1",
          "time": "20210821 13:10"
        },
        {
          "id": "MDRW2100003856.1.19",
          "form": "너무 많은 걸 바라시더라구요",
          "original_form": "너무 많은 걸 바라시더라구요",
          "speaker_id": "2",
          "time": "20210821 13:13"
        },
        {
          "id": "MDRW2100003856.1.20",
          "form": "정말 잘 모르는데",
          "original_form": "정말 잘 모르는데",
          "speaker_id": "2",
          "time": "20210821 13:14"
        },
        {
          "id": "MDRW2100003856.1.21",
          "form": "근데 4학년되니깐 될때로 돼라식이었어요",
          "original_form": "근데 4학년되니깐 될때로 돼라식이었어요",
          "speaker_id": "2",
          "time": "20210821 13:14"
        },
        {
          "id": "MDRW2100003856.1.22",
          "form": "진짜요",
          "original_form": "진짜요",
          "speaker_id": "1",
          "time": "20210821 13:15"
        },
        {
          "id": "MDRW2100003856.1.23",
          "form": "그 좀 결국엔 어쩔거야~~ 이렇게 되더라구요ㅠ",
          "original_form": "그 좀 결국엔 어쩔거야~~ 이렇게 되더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210821 13:15"
        },
        {
          "id": "MDRW2100003856.1.24",
          "form": "생각해보면 과제 지옥이었던거같아요",
          "original_form": "생각해보면 과제 지옥이었던거같아요",
          "speaker_id": "1",
          "time": "20210821 13:16"
        },
        {
          "id": "MDRW2100003856.1.25",
          "form": "저도요 전 과제는 학년 올라갈수록 문장 해석이고",
          "original_form": "저도요 전 과제는 학년 올라갈수록 문장 해석이고",
          "speaker_id": "2",
          "time": "20210821 13:17"
        },
        {
          "id": "MDRW2100003856.1.26",
          "form": "너무 어렵죠ㅠ",
          "original_form": "너무 어렵죠ㅠ",
          "speaker_id": "1",
          "time": "20210821 13:17"
        },
        {
          "id": "MDRW2100003856.1.27",
          "form": "시험은 말하기 쓰기가 대부분이었어요",
          "original_form": "시험은 말하기 쓰기가 대부분이었어요",
          "speaker_id": "2",
          "time": "20210821 13:17"
        },
        {
          "id": "MDRW2100003856.1.28",
          "form": "아이구.. 힘드셨겠어요",
          "original_form": "아이구.. 힘드셨겠어요",
          "speaker_id": "1",
          "time": "20210821 13:18"
        }
