      "utterance": [
        {
          "id": "MDRW2100002188.1.1",
          "form": "자취하면 집안일이 엄청 많아진다던데",
          "original_form": "자취하면 집안일이 엄청 많아진다던데",
          "speaker_id": "2",
          "time": "20210820 00:21"
        },
        {
          "id": "MDRW2100002188.1.2",
          "form": "최근에 나는 50사단 앞에 갔었어.",
          "original_form": "최근에 나는 50사단 앞에 갔었어.",
          "speaker_id": "1",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002188.1.3",
          "form": "나는 며칠전에 50사단 앞에 갔단다.",
          "original_form": "나는 며칠전에 50사단 앞에 갔단다.",
          "speaker_id": "1",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002188.1.4",
          "form": "그쪽에 우리가족이 이용하는 정육점이 있기 때문이지.",
          "original_form": "그쪽에 우리가족이 이용하는 정육점이 있기 때문이지.",
          "speaker_id": "1",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002188.1.5",
          "form": "그런데 내가 거기에서 뭘 봤는지 아니?",
          "original_form": "그런데 내가 거기에서 뭘 봤는지 아니?",
          "speaker_id": "1",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002188.1.6",
          "form": "뭘?",
          "original_form": "뭘?",
          "speaker_id": "2",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002188.1.7",
          "form": "군대 앞에 그렇게 많은 사람들은 처음 봤단다.",
          "original_form": "군대 앞에 그렇게 많은 사람들은 처음 봤단다.",
          "speaker_id": "1",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002188.1.8",
          "form": "아마 그 날이 입대일이었나봐.",
          "original_form": "아마 그 날이 입대일이었나봐.",
          "speaker_id": "1",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002188.1.9",
          "form": "왜그렇게 많았대?",
          "original_form": "왜그렇게 많았대?",
          "speaker_id": "2",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002188.1.10",
          "form": "아아 우와 신기하다",
          "original_form": "아아 우와 신기하다",
          "speaker_id": "2",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002188.1.11",
          "form": "근데 너 오빠 입대할때 따라가보지않았어?",
          "original_form": "근데 너 오빠 입대할때 따라가보지않았어?",
          "speaker_id": "2",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002188.1.12",
          "form": "입대준비물품을 파는 아저씨와, 사람들. 그리고 고깃집에는 차들이 즐비해있었어.",
          "original_form": "입대준비물품을 파는 아저씨와, 사람들. 그리고 고깃집에는 차들이 즐비해있었어.",
          "speaker_id": "1",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002188.1.13",
          "form": "아마 마지막 만찬을 즐기는 것이겠지.",
          "original_form": "아마 마지막 만찬을 즐기는 것이겠지.",
          "speaker_id": "1",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002188.1.14",
          "form": "우와 입대준비물품도 파는구나",
          "original_form": "우와 입대준비물품도 파는구나",
          "speaker_id": "2",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002188.1.15",
          "form": "나는 오빠가 입대할 때 많이 가고싶었어.",
          "original_form": "나는 오빠가 입대할 때 많이 가고싶었어.",
          "speaker_id": "1",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002188.1.16",
          "form": "하지만 그당시 나는 고삼이었고,",
          "original_form": "하지만 그당시 나는 고삼이었고,",
          "speaker_id": "1",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002188.1.17",
          "form": "하긴 50사단 근처에는 맛집이 많으니까 만찬 즐기기 좋겠다",
          "original_form": "하긴 50사단 근처에는 맛집이 많으니까 만찬 즐기기 좋겠다",
          "speaker_id": "2",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002188.1.18",
          "form": "잠깐 외출 나왔을 때에도 선생님이 만류하여 갈 수 없었어.",
          "original_form": "잠깐 외출 나왔을 때에도 선생님이 만류하여 갈 수 없었어.",
          "speaker_id": "1",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002188.1.19",
          "form": "아 그럼 진짜로 처음 봤겠네",
          "original_form": "아 그럼 진짜로 처음 봤겠네",
          "speaker_id": "2",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002188.1.20",
          "form": "나도 한번도 본적없어",
          "original_form": "나도 한번도 본적없어",
          "speaker_id": "2",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002188.1.21",
          "form": "나는 남자형제가 없으니까",
          "original_form": "나는 남자형제가 없으니까",
          "speaker_id": "2",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002188.1.22",
          "form": "보호자들만 만찬을 즐기던것일까?",
          "original_form": "보호자들만 만찬을 즐기던것일까?",
          "speaker_id": "1",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002188.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아무래도 보호자들도 힘들지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아무래도 보호자들도 힘들지",
          "speaker_id": "2",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002188.1.24",
          "form": "머리를 밀은. 사람들은. 볼 수가 없었어.",
          "original_form": "머리를 밀은. 사람들은. 볼 수가 없었어.",
          "speaker_id": "1",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002188.1.25",
          "form": "보호자들만 만찬을 즐기던 것일까?",
          "original_form": "보호자들만 만찬을 즐기던 것일까?",
          "speaker_id": "1",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002188.1.26",
          "form": "헐 그럼 진짜 전부 보호자들인가봐",
          "original_form": "헐 그럼 진짜 전부 보호자들인가봐",
          "speaker_id": "2",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002188.1.27",
          "form": "아무래도 밥은 먹고 살아야하니까",
          "original_form": "아무래도 밥은 먹고 살아야하니까",
          "speaker_id": "2",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002188.1.28",
          "form": "왜 아이는 두고",
          "original_form": "왜 아이는 두고",
          "speaker_id": "1",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002188.1.29",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002188.1.30",
          "form": "아이를 군대로 떠나보낸 슬픔을 소고기로 치유했던 것일까?",
          "original_form": "아이를 군대로 떠나보낸 슬픔을 소고기로 치유했던 것일까?",
          "speaker_id": "1",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002188.1.31",
          "form": "아무튼 신기한 경험이었단다.",
          "original_form": "아무튼 신기한 경험이었단다.",
          "speaker_id": "1",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002188.1.32",
          "form": "입대릏 하는 무리는 처음봤어.",
          "original_form": "입대릏 하는 무리는 처음봤어.",
          "speaker_id": "1",
