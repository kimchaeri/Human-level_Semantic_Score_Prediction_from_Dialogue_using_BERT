      "utterance": [
        {
          "id": "MDRW2100014225.1.1",
          "form": "그거 보셨나보네",
          "original_form": "그거 보셨나보네",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014225.1.2",
          "form": "뭐지",
          "original_form": "뭐지",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014225.1.3",
          "form": "그 얼굴 매일 바뀌는",
          "original_form": "그 얼굴 매일 바뀌는",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014225.1.4",
          "form": "뷰티",
          "original_form": "뷰티",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014225.1.5",
          "form": "인사이드",
          "original_form": "인사이드",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014225.1.6",
          "form": "크으...거기서 이진욱 등장할 때 진짜 존잘존잘",
          "original_form": "크으...거기서 이진욱 등장할 때 진짜 존잘존잘",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014225.1.7",
          "form": "남자가 봐도 심쿵",
          "original_form": "남자가 봐도 심쿵",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014225.1.8",
          "form": "하루만 잘생겨져보고싶네요",
          "original_form": "하루만 잘생겨져보고싶네요",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014225.1.9",
          "form": "ㅋㅋㅋㅋ저는 계속 이뻐지고싶네요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ저는 계속 이뻐지고싶네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014225.1.10",
          "form": "어유",
          "original_form": "어유",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014225.1.11",
          "form": "퉤",
          "original_form": "퉤",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014225.1.12",
          "form": "재수읎어",
          "original_form": "재수읎어",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014225.1.13",
          "form": "지금 본인의 아름다움이 지속되면 좋겠다는거죠?",
          "original_form": "지금 본인의 아름다움이 지속되면 좋겠다는거죠?",
          "speaker_id": "2",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014225.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아뇨",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아뇨",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014225.1.15",
          "form": "이뻐지면 계속 이뻐지고싶다는거ㅋㅋ",
          "original_form": "이뻐지면 계속 이뻐지고싶다는거ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014225.1.16",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014225.1.17",
          "form": "전 봤어요",
          "original_form": "전 봤어요",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014225.1.18",
          "form": "누님의 자신감을",
          "original_form": "누님의 자신감을",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014225.1.19",
          "form": "....?",
          "original_form": "....?",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014225.1.20",
          "form": "뭐 제가 어느정도 좀 되긴하는데ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "뭐 제가 어느정도 좀 되긴하는데ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014225.1.21",
          "form": "자연미인치고는 이정도면ㅋ",
          "original_form": "자연미인치고는 이정도면ㅋ",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014225.1.22",
          "form": "퉤",
          "original_form": "퉤",
          "speaker_id": "2",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014225.1.23",
          "form": "ㅋㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014225.1.24",
          "form": "엄청 이뻤으면 연예인했죠~",
          "original_form": "엄청 이뻤으면 연예인했죠~",
          "speaker_id": "1",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014225.1.25",
          "form": "뷰티인사이드 참 영화가",
          "original_form": "뷰티인사이드 참 영화가",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014225.1.26",
          "form": "의의는 좋은데",
          "original_form": "의의는 좋은데",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014225.1.27",
          "form": "결국 외모가 모든걸 압도하는것같았어서",
          "original_form": "결국 외모가 모든걸 압도하는것같았어서",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014225.1.28",
          "form": "기분이 묘하더라구요",
          "original_form": "기분이 묘하더라구요",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014225.1.29",
          "form": "ㅋㅋㅋㅋ좀 그런게 있죠",
          "original_form": "ㅋㅋㅋㅋ좀 그런게 있죠",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014225.1.30",
          "form": "진짜 막",
          "original_form": "진짜 막",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014225.1.31",
          "form": "키 185에 존잘로 살면",
          "original_form": "키 185에 존잘로 살면",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014225.1.32",
          "form": "세상이 다르겠죠?",
          "original_form": "세상이 다르겠죠?",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014225.1.33",
          "form": "아마도....?",
          "original_form": "아마도....?",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014225.1.34",
          "form": "아ㅋㅋ만약에",
          "original_form": "아ㅋㅋ만약에",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014225.1.35",
          "form": "벤자민 버튼의 시간은 거꾸로 간다 처럼",
          "original_form": "벤자민 버튼의 시간은 거꾸로 간다 처럼",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014225.1.36",
          "form": "성인일 때는 진짜 존잘에 키도커지잖아요",
          "original_form": "성인일 때는 진짜 존잘에 키도커지잖아요",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014225.1.37",
          "form": "선택권이 있다면 존잘+키를 얻고 벤자민처럼 살 건가요 아님 지금처럼 살 건가요?",
          "original_form": "선택권이 있다면 존잘+키를 얻고 벤자민처럼 살 건가요 아님 지금처럼 살 건가요?",
          "speaker_id": "1",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014225.1.38",
          "form": "당연히 벤자민이죠",
          "original_form": "당연히 벤자민이죠",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014225.1.39",
          "form": "외모는 둘째치고 대충 언제죽을지 감이 잡히잖아요",
          "original_form": "외모는 둘째치고 대충 언제죽을지 감이 잡히잖아요",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014225.1.40",
          "form": "솔직히 존잘로 살아보고싶긴 한데",
          "original_form": "솔직히 존잘로 살아보고싶긴 한데",
          "speaker_id": "2",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014225.1.41",
          "form": "지금도 뭐 나름 만족해서...",
          "original_form": "지금도 뭐 나름 만족해서...",
          "speaker_id": "2",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014225.1.42",
          "form": "ㅋㅋㅋㅋㅋㅋ아 벤요한씨",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 벤요한씨",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014225.1.43",
          "form": "하긴 아기가 되면 곧 가는구나 싶겠네요",
          "original_form": "하긴 아기가 되면 곧 가는구나 싶겠네요",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014225.1.44",
          "form": "외모는 자기만족 아니겠습니까 허허",
          "original_form": "외모는 자기만족 아니겠습니까 허허",
          "speaker_id": "2",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014225.1.45",
          "form": "누님은 벤자민 하실건가요",
          "original_form": "누님은 벤자민 하실건가요",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014225.1.46",
          "form": "아뇨 저는 이대로 살아갈래요ㅋㅋㅋㅋㅋ지금도 만족하는편이라ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 저는 이대로 살아갈래요ㅋㅋㅋㅋㅋ지금도 만족하는편이라ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014225.1.47",
          "form": "언제 떠날지 아는 거는 솔깃하긴하지만...",
          "original_form": "언제 떠날지 아는 거는 솔깃하긴하지만...",
          "speaker_id": "1",
          "time": "20210825 18:44"
        }
