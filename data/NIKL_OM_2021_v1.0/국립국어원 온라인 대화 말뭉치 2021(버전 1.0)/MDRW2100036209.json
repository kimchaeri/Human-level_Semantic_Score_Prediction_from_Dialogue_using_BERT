      "utterance": [
        {
          "id": "MDRW2100036209.1.1",
          "form": "으악이야",
          "original_form": "으악이야",
          "speaker_id": "1",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100036209.1.2",
          "form": "모기가 두방이나물렷어",
          "original_form": "모기가 두방이나물렷어",
          "speaker_id": "1",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100036209.1.3",
          "form": "ㅋㅋㅋ요즘 모기 독할텐데..",
          "original_form": "ㅋㅋㅋ요즘 모기 독할텐데..",
          "speaker_id": "2",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100036209.1.4",
          "form": "난물고잇던녀석을",
          "original_form": "난물고잇던녀석을",
          "speaker_id": "1",
          "time": "20210830 19:05"
        },
        {
          "id": "MDRW2100036209.1.5",
          "form": "잡아죽엿어피가나옴",
          "original_form": "잡아죽엿어피가나옴",
          "speaker_id": "1",
          "time": "20210830 19:05"
        },
        {
          "id": "MDRW2100036209.1.6",
          "form": "따끔하더라고ㅠㅠ",
          "original_form": "따끔하더라고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 19:05"
        },
        {
          "id": "MDRW2100036209.1.7",
          "form": "ㅋㅋ헉 되게 잘느끼나봐!!",
          "original_form": "ㅋㅋ헉 되게 잘느끼나봐!!",
          "speaker_id": "2",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036209.1.8",
          "form": "난 볼을 물려도 잘 몰라...",
          "original_form": "난 볼을 물려도 잘 몰라...",
          "speaker_id": "2",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036209.1.9",
          "form": "헉 ㅋㄴㅋㅋ",
          "original_form": "헉 ㅋㄴㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:14"
        },
        {
          "id": "MDRW2100036209.1.10",
          "form": "난자다가 때려서잡아봄",
          "original_form": "난자다가 때려서잡아봄",
          "speaker_id": "1",
          "time": "20210830 19:14"
        },
        {
          "id": "MDRW2100036209.1.11",
          "form": "근데.또 피뽑아먹울때 때려잡음안된다던데",
          "original_form": "근데.또 피뽑아먹울때 때려잡음안된다던데",
          "speaker_id": "1",
          "time": "20210830 19:14"
        },
        {
          "id": "MDRW2100036209.1.12",
          "form": "여름에 넘더워서 가을모기가많데",
          "original_form": "여름에 넘더워서 가을모기가많데",
          "speaker_id": "1",
          "time": "20210830 19:14"
        },
        {
          "id": "MDRW2100036209.1.13",
          "form": "헉!! 오늘 먹는 중에 잡은거잖아!!",
          "original_form": "헉!! 오늘 먹는 중에 잡은거잖아!!",
          "speaker_id": "2",
          "time": "20210830 19:15"
        },
        {
          "id": "MDRW2100036209.1.14",
          "form": "빨리 추워져서 요 모기들 다 사라져야해!!",
          "original_form": "빨리 추워져서 요 모기들 다 사라져야해!!",
          "speaker_id": "2",
          "time": "20210830 19:15"
        },
        {
          "id": "MDRW2100036209.1.15",
          "form": "나 내몸에 붙어잇을때 몇번때려잡아봣ㅇㅓ",
          "original_form": "나 내몸에 붙어잇을때 몇번때려잡아봣ㅇㅓ",
          "speaker_id": "1",
          "time": "20210830 19:15"
        },
        {
          "id": "MDRW2100036209.1.16",
          "form": "나물리때 좀 잘느끼는거같아 ㅋㅋㅋㅋ쏠때ㅡㅡ",
          "original_form": "나물리때 좀 잘느끼는거같아 ㅋㅋㅋㅋ쏠때ㅡㅡ",
          "speaker_id": "1",
          "time": "20210830 19:16"
        },
        {
          "id": "MDRW2100036209.1.17",
          "form": "ㅋㅋ오 진짜 신기하다 ㅋㅋㅋ울엄마도 물리는거 잘느끼더라",
          "original_form": "ㅋㅋ오 진짜 신기하다 ㅋㅋㅋ울엄마도 물리는거 잘느끼더라",
          "speaker_id": "2",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100036209.1.18",
          "form": "그래서 나보고 곰이랬어",
          "original_form": "그래서 나보고 곰이랬어",
          "speaker_id": "2",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100036209.1.19",
          "form": "아 목쪽에 물렷는데 따갑고 간지럽네 ㅠ",
          "original_form": "아 목쪽에 물렷는데 따갑고 간지럽네 ㅠ",
          "speaker_id": "1",
          "time": "20210830 19:18"
        },
        {
          "id": "MDRW2100036209.1.20",
          "form": "ㅠㅠ긁지말고!!",
          "original_form": "ㅠㅠ긁지말고!!",
          "speaker_id": "2",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100036209.1.21",
          "form": "물리자마자 씻어내라던가? 뭐 그런거 본 거 같은데!",
          "original_form": "물리자마자 씻어내라던가? 뭐 그런거 본 거 같은데!",
          "speaker_id": "2",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100036209.1.22",
          "form": "모기침을 빼내야한다는거 봣어!!",
          "original_form": "모기침을 빼내야한다는거 봣어!!",
          "speaker_id": "2",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100036209.1.23",
          "form": "아 긍가 계속 만짐헐",
          "original_form": "아 긍가 계속 만짐헐",
          "speaker_id": "1",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100036209.1.24",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100036209.1.25",
          "form": "한마리더잇는거같은데 안보이넹",
          "original_form": "한마리더잇는거같은데 안보이넹",
          "speaker_id": "1",
          "time": "20210830 19:21"
        },
        {
          "id": "MDRW2100036209.1.26",
          "form": "진짜 날씨가 이상해",
          "original_form": "진짜 날씨가 이상해",
          "speaker_id": "1",
          "time": "20210830 19:21"
        },
        {
          "id": "MDRW2100036209.1.27",
          "form": "그 꽂아두는 모기향같은거 피우면 안되나? 홈키파?",
          "original_form": "그 꽂아두는 모기향같은거 피우면 안되나? 홈키파?",
          "speaker_id": "2",
          "time": "20210830 19:21"
        },
        {
          "id": "MDRW2100036209.1.28",
          "form": "구런거없는디ㅠ",
          "original_form": "구런거없는디ㅠ",
          "speaker_id": "1",
          "time": "20210830 19:24"
        },
        {
          "id": "MDRW2100036209.1.29",
          "form": "뮤튼 기피제가지고다녀야겟어",
          "original_form": "뮤튼 기피제가지고다녀야겟어",
          "speaker_id": "1",
          "time": "20210830 19:24"
        },
        {
          "id": "MDRW2100036209.1.30",
          "form": "마져!! 우리집은 오프? 그거 샀는데 엄마가 효과 좋대 ㅋㅋ",
          "original_form": "마져!! 우리집은 오프? 그거 샀는데 엄마가 효과 좋대 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100036209.1.31",
          "form": "난 전에.보건소갔을때 보건소에서그냥주는거잇길래 가져와봄 !! 써본진않음",
          "original_form": "난 전에.보건소갔을때 보건소에서그냥주는거잇길래 가져와봄 !! 써본진않음",
          "speaker_id": "1",
