      "utterance": [
        {
          "id": "MDRW2100007075.1.1",
          "form": "저는 간장파입니다...",
          "original_form": "저는 간장파입니다...",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007075.1.2",
          "form": "치킨은 양념 이죠 !!",
          "original_form": "치킨은 양념 이죠 !!",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007075.1.3",
          "form": "가슴살을좋아해요...",
          "original_form": "가슴살을좋아해요...",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007075.1.4",
          "form": "양념은 뭔가",
          "original_form": "양념은 뭔가",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007075.1.5",
          "form": "호식이때 트라우마를입어서",
          "original_form": "호식이때 트라우마를입어서",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007075.1.6",
          "form": "안시켜봤어요 근 십년간ㅠ",
          "original_form": "안시켜봤어요 근 십년간ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007075.1.7",
          "form": "아 ㅋㅋㅋ 저는 퍽살 싫고 ㅋ 그 야들살",
          "original_form": "아 ㅋㅋㅋ 저는 퍽살 싫고 ㅋ 그 야들살",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007075.1.8",
          "form": "닭다리요?",
          "original_form": "닭다리요?",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007075.1.9",
          "form": "간장은 푸라닭이 진짜 맛있던데요",
          "original_form": "간장은 푸라닭이 진짜 맛있던데요",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007075.1.10",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007075.1.11",
          "form": "퍽살 좋아해서 애들이 저 좋아해요",
          "original_form": "퍽살 좋아해서 애들이 저 좋아해요",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007075.1.12",
          "form": "블랙알리오머시기",
          "original_form": "블랙알리오머시기",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007075.1.13",
          "form": "다리 자기가 다 먹으려고ㅋㅋ",
          "original_form": "다리 자기가 다 먹으려고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007075.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007075.1.15",
          "form": "푸라닭 블랙알리오 조만간 먹어볼게요",
          "original_form": "푸라닭 블랙알리오 조만간 먹어볼게요",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007075.1.16",
          "form": "저도 제친구중에 있는데",
          "original_form": "저도 제친구중에 있는데",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007075.1.17",
          "form": "음식얘기 좋네요",
          "original_form": "음식얘기 좋네요",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007075.1.18",
          "form": "ㅋㅋㅋ퍽퍽살매니아ㅎ",
          "original_form": "ㅋㅋㅋ퍽퍽살매니아ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007075.1.19",
          "form": "그 친구랑 먹으면 넘 좋아요",
          "original_form": "그 친구랑 먹으면 넘 좋아요",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007075.1.20",
          "form": "약간 치킨전용친구가",
          "original_form": "약간 치킨전용친구가",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007075.1.21",
          "form": "되기도해요",
          "original_form": "되기도해요",
          "speaker_id": "2",
          "time": "20210822 00:28"
        }
      ]
    }
