      "utterance": [
        {
          "id": "MDRW2100004449.1.1",
          "form": "여행가고싶다,,",
          "original_form": "여행가고싶다,,",
          "speaker_id": "1",
          "time": "20210821 16:35"
        },
        {
          "id": "MDRW2100004449.1.2",
          "form": "우리의 좀 투명한 계획이엇던 횡단열차",
          "original_form": "우리의 좀 투명한 계획이엇던 횡단열차",
          "speaker_id": "1",
          "time": "20210821 16:35"
        },
        {
          "id": "MDRW2100004449.1.3",
          "form": "우리의 시베리아 횡단열차...",
          "original_form": "우리의 시베리아 횡단열차...",
          "speaker_id": "2",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004449.1.4",
          "form": "그래...",
          "original_form": "그래...",
          "speaker_id": "1",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004449.1.5",
          "form": "역시..",
          "original_form": "역시..",
          "speaker_id": "2",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004449.1.6",
          "form": "근데 우리의 약간 투명한 계획대로 열차를 탛으면",
          "original_form": "근데 우리의 약간 투명한 계획대로 열차를 탛으면",
          "speaker_id": "2",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004449.1.7",
          "form": "우린 병원을 한 번 거쳐갔을 겨...",
          "original_form": "우린 병원을 한 번 거쳐갔을 겨...",
          "speaker_id": "2",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004449.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그래 그것도 다 투명한 계획속에,, 있었다고 우겨",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그래 그것도 다 투명한 계획속에,, 있었다고 우겨",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004449.1.9",
          "form": "요샌 가고싶은곳도 없다",
          "original_form": "요샌 가고싶은곳도 없다",
          "speaker_id": "1",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004449.1.10",
          "form": "아 여전히 터키는 가보고싶어",
          "original_form": "아 여전히 터키는 가보고싶어",
          "speaker_id": "1",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004449.1.11",
          "form": "난 스위스 스위스!!!",
          "original_form": "난 스위스 스위스!!!",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004449.1.12",
          "form": "어디서 스위스 뽕이찬겨",
          "original_form": "어디서 스위스 뽕이찬겨",
          "speaker_id": "1",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004449.1.13",
          "form": "스위스 여행 유튜브에서.. 사실 스위스는 그냥 계속 예전부터 가고 싶었어",
          "original_form": "스위스 여행 유튜브에서.. 사실 스위스는 그냥 계속 예전부터 가고 싶었어",
          "speaker_id": "2",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004449.1.14",
          "form": "스위스 유명한거 머잇지?",
          "original_form": "스위스 유명한거 머잇지?",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004449.1.15",
          "form": "그 열차? 슈돌에 나왔던거같은데 나은이랑 건후",
          "original_form": "그 열차? 슈돌에 나왔던거같은데 나은이랑 건후",
          "speaker_id": "1",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004449.1.16",
          "form": "자연이 예쁘더라",
          "original_form": "자연이 예쁘더라",
          "speaker_id": "1",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004449.1.17",
          "form": "맞아 그런 것도 있고 거기는 겨울에가도 멋지고 여름에 가도 멋지고 여튼 그냥 너무 멋지더라",
          "original_form": "맞아 그런 것도 있고 거기는 겨울에가도 멋지고 여름에 가도 멋지고 여튼 그냥 너무 멋지더라",
          "speaker_id": "2",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004449.1.18",
          "form": "하 그냥 어디든 떠나고싶다,,",
          "original_form": "하 그냥 어디든 떠나고싶다,,",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004449.1.19",
          "form": "나두 지금 돈 모아둔 것도 있는데... 이 돈이면 괌을 진짜 더 호화스럽게 여행하규 올 수있는데...",
          "original_form": "나두 지금 돈 모아둔 것도 있는데... 이 돈이면 괌을 진짜 더 호화스럽게 여행하규 올 수있는데...",
          "speaker_id": "2",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004449.1.20",
          "form": "헐 얼마잇는데",
          "original_form": "헐 얼마잇는데",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004449.1.21",
          "form": "괌 얼마들엇어?",
          "original_form": "괌 얼마들엇어?",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004449.1.22",
          "form": "괌 한사람단 100씩 가져갔는데",
          "original_form": "괌 한사람단 100씩 가져갔는데",
          "speaker_id": "2",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004449.1.23",
          "form": "비향기 포함 숙소비 포함",
          "original_form": "비향기 포함 숙소비 포함",
          "speaker_id": "2",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004449.1.24",
          "form": "막판에 돈 남아서 펑펑쓰고도 다 못쓰고 옴",
          "original_form": "막판에 돈 남아서 펑펑쓰고도 다 못쓰고 옴",
          "speaker_id": "2",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004449.1.25",
          "form": "오우 며칠갓지?",
          "original_form": "오우 며칠갓지?",
          "speaker_id": "1",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004449.1.26",
          "form": "3박 4일?",
          "original_form": "3박 4일?",
          "speaker_id": "2",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004449.1.27",
          "form": "그럼 100이면 낙낙히 가져가긴햇네 ㅋㅋㄱㅋㄱㄱㅋ",
          "original_form": "그럼 100이면 낙낙히 가져가긴햇네 ㅋㅋㄱㅋㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004449.1.28",
          "form": "맞아 그리고 평소에 돈 안써버릇하다 보니 여행 가서도 돈을 겁나 아끼고 있더라..ㅋㅋㅋㅋㅋ",
          "original_form": "맞아 그리고 평소에 돈 안써버릇하다 보니 여행 가서도 돈을 겁나 아끼고 있더라..ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004449.1.29",
          "form": "헐 그건쪼매 슬프다.....",
          "original_form": "헐 그건쪼매 슬프다.....",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004449.1.30",
          "form": "요즘은 해외고 뭐고 그냥 어디로든 떠나고 싶어.. 바다.. 바다가 보고싶다...",
          "original_form": "요즘은 해외고 뭐고 그냥 어디로든 떠나고 싶어.. 바다.. 바다가 보고싶다...",
          "speaker_id": "2",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004449.1.31",
          "form": "바다,,, 바다가실,,, ?",
          "original_form": "바다,,, 바다가실,,, ?",
          "speaker_id": "1",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004449.1.32",
          "form": "바다 가고싶다... 공부만 아니면....ㅜㅜ 가서 조개구이도 먹고, 불꽃놀이도 하고..",
          "original_form": "바다 가고싶다... 공부만 아니면....ㅜㅜ 가서 조개구이도 먹고, 불꽃놀이도 하고..",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100004449.1.33",
          "form": "하루도 못빼는군아  와우..그럼 12월 2월까지,,",
          "original_form": "하루도 못빼는군아  와우..그럼 12월 2월까지,,",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100004449.1.34",
