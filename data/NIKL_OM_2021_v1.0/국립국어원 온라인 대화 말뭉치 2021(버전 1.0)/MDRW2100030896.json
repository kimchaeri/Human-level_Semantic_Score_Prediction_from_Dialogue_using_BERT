          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210904 14:00"
        },
        {
          "id": "MDRW2100030896.1.7",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210904 14:00"
        },
        {
          "id": "MDRW2100030896.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:00"
        },
        {
          "id": "MDRW2100030896.1.9",
          "form": "스릴러 별로..",
          "original_form": "스릴러 별로..",
          "speaker_id": "2",
          "time": "20210904 14:00"
        },
        {
          "id": "MDRW2100030896.1.10",
          "form": "좀비는요?",
          "original_form": "좀비는요?",
          "speaker_id": "2",
          "time": "20210904 14:00"
        },
        {
          "id": "MDRW2100030896.1.11",
          "form": "그 한동안",
          "original_form": "그 한동안",
          "speaker_id": "1",
          "time": "20210904 14:00"
        },
        {
          "id": "MDRW2100030896.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210904 14:00"
        },
        {
          "id": "MDRW2100030896.1.13",
          "form": "좀비 영화",
          "original_form": "좀비 영화",
          "speaker_id": "1",
          "time": "20210904 14:00"
        },
        {
          "id": "MDRW2100030896.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210904 14:00"
        },
        {
          "id": "MDRW2100030896.1.15",
          "form": "역시 찾아 보진 않는...",
          "original_form": "역시 찾아 보진 않는...",
          "speaker_id": "1",
          "time": "20210904 14:00"
        },
        {
          "id": "MDRW2100030896.1.16",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "2",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.17",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "1",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.18",
          "form": "킹덤은 봤어요",
          "original_form": "킹덤은 봤어요",
          "speaker_id": "1",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.19",
          "form": "저도좀비 싫어해요",
          "original_form": "저도좀비 싫어해요",
          "speaker_id": "2",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.20",
          "form": "재밌긴한데",
          "original_form": "재밌긴한데",
          "speaker_id": "1",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.21",
          "form": "킹덤 안봐써욬",
          "original_form": "킹덤 안봐써욬",
          "speaker_id": "2",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.22",
          "form": "앞서 말씀드린 것처럼",
          "original_form": "앞서 말씀드린 것처럼",
          "speaker_id": "1",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.25",
          "form": "아 그죸ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 그죸ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.26",
          "form": "콘텐츠 소비 주도권 문제로",
          "original_form": "콘텐츠 소비 주도권 문제로",
          "speaker_id": "1",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.28",
          "form": "속도 1.3배속하고",
          "original_form": "속도 1.3배속하고",
          "speaker_id": "1",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.29",
          "form": "막바지 가면",
          "original_form": "막바지 가면",
          "speaker_id": "1",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.30",
          "form": "주변에 그런 사람들 있어요??",
          "original_form": "주변에 그런 사람들 있어요??",
          "speaker_id": "2",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.31",
          "form": "넘기면서 봤...",
          "original_form": "넘기면서 봤...",
          "speaker_id": "1",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.32",
          "form": "전 처음봐서 신기해요",
          "original_form": "전 처음봐서 신기해요",
          "speaker_id": "2",
          "time": "20210904 14:01"
