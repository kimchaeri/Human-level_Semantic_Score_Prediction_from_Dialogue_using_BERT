          "id": "MDRW2100030896.1.43",
          "form": "야",
          "original_form": "야",
          "speaker_id": "1",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030896.1.44",
          "form": "너 왜 그렇게 보냐",
          "original_form": "너 왜 그렇게 보냐",
          "speaker_id": "1",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030896.1.45",
          "form": "제대로 보자",
          "original_form": "제대로 보자",
          "speaker_id": "1",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100030896.1.46",
          "form": "하면서 데려가는...",
          "original_form": "하면서 데려가는...",
          "speaker_id": "1",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100030896.1.47",
          "form": "조커도 그런 식으로 봤었죠...",
          "original_form": "조커도 그런 식으로 봤었죠...",
          "speaker_id": "1",
          "time": "20210904 14:03"
        },
