          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.34",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030896.1.35",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030896.1.36",
          "form": "이러다보니",
          "original_form": "이러다보니",
          "speaker_id": "1",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030896.1.37",
          "form": "주변에서",
          "original_form": "주변에서",
          "speaker_id": "1",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030896.1.38",
          "form": "역시 사람을 많이 만나봐야..",
          "original_form": "역시 사람을 많이 만나봐야..",
          "speaker_id": "2",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030896.1.39",
          "form": "영화관 데려가는 케이스...",
          "original_form": "영화관 데려가는 케이스...",
          "speaker_id": "1",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030896.1.40",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030896.1.41",
          "form": "주변에서 싫어해요?",
          "original_form": "주변에서 싫어해요?",
          "speaker_id": "2",
          "time": "20210904 14:02"
        },
