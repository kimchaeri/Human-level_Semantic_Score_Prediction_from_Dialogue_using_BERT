      "utterance": [
        {
          "id": "MDRW2100007373.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210822 09:21"
        },
        {
          "id": "MDRW2100007373.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007373.1.3",
          "form": "이상형이 어떻게 되세요?",
          "original_form": "이상형이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007373.1.4",
          "form": "센스있는 사람이요! name1님은요?",
          "original_form": "센스있는 사람이요! &name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100007373.1.5",
          "form": "음 저는 귀엽고 밝은 사람이요!",
          "original_form": "음 저는 귀엽고 밝은 사람이요!",
          "speaker_id": "1",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007373.1.6",
          "form": "근데 이상형은 진짜 이상형일 뿐인 것 같아요 ㅋㅋ",
          "original_form": "근데 이상형은 진짜 이상형일 뿐인 것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100007373.1.7",
          "form": "아 그렇죠 ㅋㅋ 자기한테 맞는 사람이 좋죠",
          "original_form": "아 그렇죠 ㅋㅋ 자기한테 맞는 사람이 좋죠",
          "speaker_id": "1",
          "time": "20210822 11:20"
        },
        {
          "id": "MDRW2100007373.1.8",
          "form": "그게 제일 어렵죠ㅎㅎ",
          "original_form": "그게 제일 어렵죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 11:22"
        },
        {
          "id": "MDRW2100007373.1.9",
          "form": "네 맞아요ㅎㅎ",
          "original_form": "네 맞아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007373.1.10",
          "form": "연애중이세요?",
          "original_form": "연애중이세요?",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007373.1.11",
          "form": "시국이 시국인지라 쉬고(?) 있어요ㅎㅎ",
          "original_form": "시국이 시국인지라 쉬고(?) 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007373.1.12",
          "form": "혼자 노는거 좋아하는 편이기도 하고요ㅎㅎ 하산님은요?",
          "original_form": "혼자 노는거 좋아하는 편이기도 하고요ㅎㅎ 하산님은요?",
          "speaker_id": "2",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007373.1.13",
          "form": "저도 연애는 안하고 있어요ㅎㅎ",
          "original_form": "저도 연애는 안하고 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007373.1.14",
          "form": "소개팅은 하시나요",
          "original_form": "소개팅은 하시나요",
          "speaker_id": "1",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007373.1.15",
          "form": "자만추 아시나요 ㅎㅎ 어색해서 소개팅은 못하겠더라구요ㅠㅠ",
          "original_form": "자만추 아시나요 ㅎㅎ 어색해서 소개팅은 못하겠더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 12:23"
        },
        {
          "id": "MDRW2100007373.1.16",
          "form": "그쵸 근데 소개팅 시켜준대서 자만추라고 하면 구박하잖아요",
          "original_form": "그쵸 근데 소개팅 시켜준대서 자만추라고 하면 구박하잖아요",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007373.1.17",
          "form": "나쁜 사람들이네요",
          "original_form": "나쁜 사람들이네요",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007373.1.18",
          "form": "ㅋㅋㅋㅋ지방에서 공기업 다니시면 소개팅 많이 들어오지 않아요?",
          "original_form": "ㅋㅋㅋㅋ지방에서 공기업 다니시면 소개팅 많이 들어오지 않아요?",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007373.1.19",
          "form": "뭐 보통보통ㅎㅎ",
          "original_form": "뭐 보통보통ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007373.1.20",
          "form": "저는 서울 살아요",
          "original_form": "저는 서울 살아요",
          "speaker_id": "1",
          "time": "20210822 12:28"
        }
      ]
