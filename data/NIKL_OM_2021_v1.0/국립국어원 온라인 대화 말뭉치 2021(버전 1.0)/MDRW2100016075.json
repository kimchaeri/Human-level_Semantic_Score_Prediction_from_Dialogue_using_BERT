      "utterance": [
        {
          "id": "MDRW2100016075.1.1",
          "form": "즐겨보는 드라마가있나요",
          "original_form": "즐겨보는 드라마가있나요",
          "speaker_id": "2",
          "time": "20210826 09:23"
        },
        {
          "id": "MDRW2100016075.1.2",
          "form": "전 넷플릭스 더 소사이어티",
          "original_form": "전 넷플릭스 더 소사이어티",
          "speaker_id": "1",
          "time": "20210826 09:25"
        },
        {
          "id": "MDRW2100016075.1.3",
          "form": "요즘 재밌게 보고 있어요",
          "original_form": "요즘 재밌게 보고 있어요",
          "speaker_id": "1",
          "time": "20210826 09:25"
        },
        {
          "id": "MDRW2100016075.1.4",
          "form": "어떤 드라마 보시나용",
          "original_form": "어떤 드라마 보시나용",
          "speaker_id": "1",
          "time": "20210826 09:25"
        },
        {
          "id": "MDRW2100016075.1.5",
          "form": "더 소사이어티는 외국드라마인가요?진짜",
          "original_form": "더 소사이어티는 외국드라마인가요?진짜",
          "speaker_id": "2",
          "time": "20210826 09:26"
        },
        {
          "id": "MDRW2100016075.1.6",
          "form": "저는 한드봐요",
          "original_form": "저는 한드봐요",
          "speaker_id": "2",
          "time": "20210826 09:26"
        },
        {
          "id": "MDRW2100016075.1.7",
          "form": "최근에 펜트하우스 봤어요",
          "original_form": "최근에 펜트하우스 봤어요",
          "speaker_id": "2",
          "time": "20210826 09:26"
        },
        {
          "id": "MDRW2100016075.1.8",
          "form": "더 소사이어티의 장르는 무엇인가요",
          "original_form": "더 소사이어티의 장르는 무엇인가요",
          "speaker_id": "2",
          "time": "20210826 09:27"
        },
        {
          "id": "MDRW2100016075.1.9",
          "form": "약간 과학쪽인가 음..",
          "original_form": "약간 과학쪽인가 음..",
          "speaker_id": "2",
          "time": "20210826 09:27"
        },
        {
          "id": "MDRW2100016075.1.10",
          "form": "음 미스테리물인데",
          "original_form": "음 미스테리물인데",
          "speaker_id": "1",
          "time": "20210826 09:28"
        },
        {
          "id": "MDRW2100016075.1.11",
          "form": "미스테리물 재밌겠다",
          "original_form": "미스테리물 재밌겠다",
          "speaker_id": "2",
          "time": "20210826 09:28"
        },
        {
          "id": "MDRW2100016075.1.12",
          "form": "학교학생들 빼고 다 사라지는",
          "original_form": "학교학생들 빼고 다 사라지는",
          "speaker_id": "1",
          "time": "20210826 09:28"
        },
        {
          "id": "MDRW2100016075.1.13",
          "form": "헤",
          "original_form": "헤",
          "speaker_id": "2",
          "time": "20210826 09:28"
        },
        {
          "id": "MDRW2100016075.1.14",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210826 09:28"
        },
        {
          "id": "MDRW2100016075.1.15",
          "form": "평행세계 관련물?",
          "original_form": "평행세계 관련물?",
          "speaker_id": "1",
          "time": "20210826 09:28"
        },
        {
          "id": "MDRW2100016075.1.16",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 09:28"
        },
        {
          "id": "MDRW2100016075.1.17",
          "form": "재밋을거같아요",
          "original_form": "재밋을거같아요",
          "speaker_id": "2",
          "time": "20210826 09:29"
        },
        {
          "id": "MDRW2100016075.1.18",
          "form": "시간 날 때 한번 봐봐아겠어요",
          "original_form": "시간 날 때 한번 봐봐아겠어요",
          "speaker_id": "2",
          "time": "20210826 09:29"
        },
        {
          "id": "MDRW2100016075.1.19",
          "form": "드라마의 어떤 점이 가장 기억에 남던가요",
          "original_form": "드라마의 어떤 점이 가장 기억에 남던가요",
          "speaker_id": "2",
          "time": "20210826 09:29"
        },
        {
          "id": "MDRW2100016075.1.20",
          "form": "신선했어요",
          "original_form": "신선했어요",
          "speaker_id": "1",
          "time": "20210826 09:32"
        },
        {
          "id": "MDRW2100016075.1.21",
          "form": "어떤점이요",
          "original_form": "어떤점이요",
          "speaker_id": "2",
          "time": "20210826 09:33"
        },
        {
          "id": "MDRW2100016075.1.22",
          "form": "평행세계? 아니면 학생들만 남는다는 점?",
          "original_form": "평행세계? 아니면 학생들만 남는다는 점?",
          "speaker_id": "2",
          "time": "20210826 09:33"
        },
        {
          "id": "MDRW2100016075.1.23",
          "form": "음 학생들만 세상에 남겨지는 부분이요 ㅎㅎ",
          "original_form": "음 학생들만 세상에 남겨지는 부분이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 09:34"
        },
        {
          "id": "MDRW2100016075.1.24",
          "form": "아무래도 그 부분이 가장 인상적이긴 할 것 같아요",
          "original_form": "아무래도 그 부분이 가장 인상적이긴 할 것 같아요",
          "speaker_id": "2",
          "time": "20210826 09:34"
        },
        {
          "id": "MDRW2100016075.1.25",
          "form": "기억에 남는 장면은 요?",
          "original_form": "기억에 남는 장면은 요?",
          "speaker_id": "2",
          "time": "20210826 09:34"
        },
        {
          "id": "MDRW2100016075.1.26",
          "form": "식료품이 바닥나서 농작을 하는 모습이 기억에 남아요",
          "original_form": "식료품이 바닥나서 농작을 하는 모습이 기억에 남아요",
          "speaker_id": "1",
