      "utterance": [
        {
          "id": "MDRW2100008558.1.1",
          "form": "우린 저녁먹고 슬슬 집에 가야겠군#저녁에 고기꿉나?",
          "original_form": "우린 저녁먹고 슬슬 집에 가야겠군#저녁에 고기꿉나?",
          "speaker_id": "1",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008558.1.2",
          "form": "아니 저녁에 고기안굽는다ㅋㅋㅋ",
          "original_form": "아니 저녁에 고기안굽는다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008558.1.3",
          "form": "신랑이 고기구워먹고싶고 ㅋㅋ 첫째는 회먹고싶대",
          "original_form": "신랑이 고기구워먹고싶고 ㅋㅋ 첫째는 회먹고싶대",
          "speaker_id": "2",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008558.1.4",
          "form": "아....ㅋㅋㅋㅋ",
          "original_form": "아....ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008558.1.5",
          "form": "이야~먹을줄 안다이~",
          "original_form": "이야~먹을줄 안다이~",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008558.1.6",
          "form": "회맛을 아나??",
          "original_form": "회맛을 아나??",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008558.1.7",
          "form": "가족 입맛 다다름 ㅋㅋㅋㅋ",
          "original_form": "가족 입맛 다다름 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008558.1.8",
          "form": "회,초밥 좋아한다",
          "original_form": "회,초밥 좋아한다",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008558.1.9",
          "form": "아고 진짜 비싼것만 까다로운것만 좋아한다 ㅋㅋ",
          "original_form": "아고 진짜 비싼것만 까다로운것만 좋아한다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008558.1.10",
          "form": "우리 첫째 ^^ 하하",
          "original_form": "우리 첫째 ^^ 하하",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008558.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ울첫찌는 물고기쪽은 아예..ㅠㅠ뭐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ울첫찌는 물고기쪽은 아예..ㅠㅠ뭐",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008558.1.12",
          "form": "먹을게 없다..늘 치킨.떡볶이..",
          "original_form": "먹을게 없다..늘 치킨.떡볶이..",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008558.1.13",
          "form": "난도 초밥먹곱다이~",
          "original_form": "난도 초밥먹곱다이~",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008558.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ쿠우쿠우 나중에 가자ㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ쿠우쿠우 나중에 가자ㅋ",
          "speaker_id": "2",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008558.1.15",
          "form": "애들 방학 마치고ㅋㅋ",
          "original_form": "애들 방학 마치고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008558.1.16",
          "form": "조용할때 한번 가자ㅋㅋ",
          "original_form": "조용할때 한번 가자ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008558.1.17",
          "form": "좋다좋다ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "좋다좋다ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008558.1.18",
          "form": "원없이 먹어보드라거~",
          "original_form": "원없이 먹어보드라거~",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008558.1.19",
          "form": "응 응 우리는 마트왔다 ㅋㅋ저녁먹을거 좀 사가려구ㅋ",
          "original_form": "응 응 우리는 마트왔다 ㅋㅋ저녁먹을거 좀 사가려구ㅋ",
          "speaker_id": "2",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008558.1.20",
          "form": "애들 씻기고왔다ㅋ",
          "original_form": "애들 씻기고왔다ㅋ",
          "speaker_id": "2",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100008558.1.21",
          "form": "우리도 씻고 이제 잘준비 중~",
          "original_form": "우리도 씻고 이제 잘준비 중~",
          "speaker_id": "1",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008558.1.22",
          "form": "ㅋㅋ 오늘도 아이들이 일찍잤으면 좋겠다ㅋㅋ",
          "original_form": "ㅋㅋ 오늘도 아이들이 일찍잤으면 좋겠다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:54"
        },
        {
          "id": "MDRW2100008558.1.23",
          "form": "오늘도 고생했다이 ㅋㅋ육아한다고ㅋㅋ",
          "original_form": "오늘도 고생했다이 ㅋㅋ육아한다고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:54"
        },
        {
          "id": "MDRW2100008558.1.24",
          "form": "그래~언넝 마무리하고 잡다이~",
          "original_form": "그래~언넝 마무리하고 잡다이~",
          "speaker_id": "1",
          "time": "20210822 19:56"
        },
        {
          "id": "MDRW2100008558.1.25",
          "form": "학교다닐땐 지금 육퇸데 딱 ㅋㅋㅋㅋㅋ",
          "original_form": "학교다닐땐 지금 육퇸데 딱 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:56"
        },
        {
          "id": "MDRW2100008558.1.26",
          "form": "방학은 안되는구먼~",
          "original_form": "방학은 안되는구먼~",
          "speaker_id": "1",
          "time": "20210822 19:56"
        },
        {
          "id": "MDRW2100008558.1.27",
          "form": "ㅋㅋㅋㄱㅋㅋ방학은 힘들지ㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㅋㅋ방학은 힘들지ㅋㅋ",
