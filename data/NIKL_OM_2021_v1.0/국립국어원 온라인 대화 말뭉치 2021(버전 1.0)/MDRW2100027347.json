        },
        {
          "id": "MDRW2100027347.1.2",
          "form": "안녕하세요. 최근에 본 공연이 있나요",
          "original_form": "안녕하세요. 최근에 본 공연이 있나요",
          "speaker_id": "2",
          "time": "20210903 19:40"
        },
        {
          "id": "MDRW2100027347.1.3",
          "form": "공연은없고 영화요",
          "original_form": "공연은없고 영화요",
          "speaker_id": "1",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100027347.1.4",
          "form": "무슨 영화 보셨나요",
          "original_form": "무슨 영화 보셨나요",
          "speaker_id": "2",
          "time": "20210903 19:43"
        },
        {
          "id": "MDRW2100027347.1.5",
          "form": "모가디슈요",
          "original_form": "모가디슈요",
          "speaker_id": "1",
          "time": "20210903 19:44"
        },
        {
          "id": "MDRW2100027347.1.6",
          "form": "평 좋던데 어떠셨어요?",
          "original_form": "평 좋던데 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210903 19:46"
        },
        {
          "id": "MDRW2100027347.1.7",
          "form": "전 너무재미있었어요",
          "original_form": "전 너무재미있었어요",
          "speaker_id": "1",
          "time": "20210903 19:47"
        },
        {
          "id": "MDRW2100027347.1.8",
          "form": "오오 극장에 사람은 많던가요",
          "original_form": "오오 극장에 사람은 많던가요",
          "speaker_id": "2",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100027347.1.9",
          "form": "아니요..10명정도",
          "original_form": "아니요..10명정도",
          "speaker_id": "1",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100027347.1.10",
          "form": "조조라",
          "original_form": "조조라",
          "speaker_id": "1",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100027347.1.11",
          "form": "요새 조조가 얼마 정도 하죠?",
          "original_form": "요새 조조가 얼마 정도 하죠?",
          "speaker_id": "2",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100027347.1.12",
          "form": "만원였던가? 잘기억이..애들꺼 같이 결재해서",
          "original_form": "만원였던가? 잘기억이..애들꺼 같이 결재해서",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100027347.1.13",
          "form": "정확히 기억이 안나네요..",
          "original_form": "정확히 기억이 안나네요..",
          "speaker_id": "1",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100027347.1.14",
          "form": "요새는 진짜 조조도 비싸네요ㅜ",
          "original_form": "요새는 진짜 조조도 비싸네요ㅜ",
          "speaker_id": "2",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100027347.1.15",
          "form": "원래 영화 자주 보시나요",
          "original_form": "원래 영화 자주 보시나요",
          "speaker_id": "2",
          "time": "20210903 22:10"
        },
