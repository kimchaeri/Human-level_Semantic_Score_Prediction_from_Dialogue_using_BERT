      "utterance": [
        {
          "id": "MDRW2100002920.1.1",
          "form": "이제 여름은 다 갔나봐 ㅎㅎ",
          "original_form": "이제 여름은 다 갔나봐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 21:22"
        },
        {
          "id": "MDRW2100002920.1.2",
          "form": "점심에 바깥에 별로 안더웠옹",
          "original_form": "점심에 바깥에 별로 안더웠옹",
          "speaker_id": "2",
          "time": "20210820 21:22"
        },
        {
          "id": "MDRW2100002920.1.3",
          "form": "웅웅~ 바람도 시원해지고~",
          "original_form": "웅웅~ 바람도 시원해지고~",
          "speaker_id": "1",
          "time": "20210820 21:41"
        },
        {
          "id": "MDRW2100002920.1.4",
          "form": "이제 찐 여름은 다갔나보오",
          "original_form": "이제 찐 여름은 다갔나보오",
          "speaker_id": "2",
          "time": "20210820 21:44"
        },
        {
          "id": "MDRW2100002920.1.5",
          "form": "가을 좋아~",
          "original_form": "가을 좋아~",
          "speaker_id": "2",
          "time": "20210820 21:44"
        },
        {
          "id": "MDRW2100002920.1.6",
          "form": "웅웅~~",
          "original_form": "웅웅~~",
          "speaker_id": "1",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100002920.1.7",
          "form": "가을이 제일 길었음 좋겠는데",
          "original_form": "가을이 제일 길었음 좋겠는데",
          "speaker_id": "1",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100002920.1.8",
          "form": "봄보다 더 짧은듯한 느낌..ㅠ",
          "original_form": "봄보다 더 짧은듯한 느낌..ㅠ",
          "speaker_id": "1",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100002920.1.9",
          "form": "마자 ㅠㅋㅋ 가을이 딱 좋은데",
          "original_form": "마자 ㅠㅋㅋ 가을이 딱 좋은데",
          "speaker_id": "2",
          "time": "20210820 22:39"
        },
        {
          "id": "MDRW2100002920.1.10",
          "form": "가을 시원한 날에 한강가고싶당",
          "original_form": "가을 시원한 날에 한강가고싶당",
          "speaker_id": "2",
          "time": "20210820 22:39"
        },
        {
          "id": "MDRW2100002920.1.11",
          "form": "웅웅~ 한강에서 치킨 뜯고 싶다~",
          "original_form": "웅웅~ 한강에서 치킨 뜯고 싶다~",
          "speaker_id": "1",
          "time": "20210820 23:15"
        },
        {
          "id": "MDRW2100002920.1.12",
          "form": "자전거도 타고~",
          "original_form": "자전거도 타고~",
          "speaker_id": "1",
          "time": "20210820 23:15"
        },
        {
          "id": "MDRW2100002920.1.13",
          "form": "마자~ ㅠ 시원한 바람을 즐기며~",
          "original_form": "마자~ ㅠ 시원한 바람을 즐기며~",
          "speaker_id": "2",
          "time": "20210820 23:19"
        },
        {
          "id": "MDRW2100002920.1.14",
          "form": "넘 행복하겠다 흑흑",
          "original_form": "넘 행복하겠다 흑흑",
          "speaker_id": "2",
          "time": "20210820 23:20"
        },
        {
          "id": "MDRW2100002920.1.15",
          "form": "코로나 좀 잠잠해지면 가자~",
          "original_form": "코로나 좀 잠잠해지면 가자~",
          "speaker_id": "2",
          "time": "20210820 23:20"
        },
        {
          "id": "MDRW2100002920.1.16",
          "form": "웅웅~~ 저번처럼 자전거로 한강투어~ㅋㅋㅋㅋ",
          "original_form": "웅웅~~ 저번처럼 자전거로 한강투어~ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100002920.1.17",
          "form": "치킨도 뜯고~~ 좋다좋다~",
          "original_form": "치킨도 뜯고~~ 좋다좋다~",
          "speaker_id": "1",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100002920.1.18",
          "form": "웅웅 ㅋㅋㅋ 가자가자 넘 좋을듯",
          "original_form": "웅웅 ㅋㅋㅋ 가자가자 넘 좋을듯",
          "speaker_id": "2",
          "time": "20210821 00:02"
        },
        {
          "id": "MDRW2100002920.1.19",
          "form": "저번처럼 오전에 출발하는게 사람들 별로 없고 좋겠지??ㅎ",
          "original_form": "저번처럼 오전에 출발하는게 사람들 별로 없고 좋겠지??ㅎ",
          "speaker_id": "1",
          "time": "20210821 00:06"
        },
        {
          "id": "MDRW2100002920.1.20",
          "form": "웅웅!ㅋㅋ 오전부터 시원하게 ~ 즐기는게 좋겠어~",
          "original_form": "웅웅!ㅋㅋ 오전부터 시원하게 ~ 즐기는게 좋겠어~",
          "speaker_id": "2",
          "time": "20210821 00:28"
        },
        {
          "id": "MDRW2100002920.1.21",
          "form": "돗자리 깔고 누워서 쉬고도 싶당~",
          "original_form": "돗자리 깔고 누워서 쉬고도 싶당~",
          "speaker_id": "2",
          "time": "20210821 00:28"
        },
        {
          "id": "MDRW2100002920.1.22",
          "form": "ㅋㅋㅋ돗자리 미니미니한 걸로 하나 구입해야되나~ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ돗자리 미니미니한 걸로 하나 구입해야되나~ㅋㅋㅋ",
