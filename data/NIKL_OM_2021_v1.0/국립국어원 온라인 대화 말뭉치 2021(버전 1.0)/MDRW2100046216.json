      "utterance": [
        {
          "id": "MDRW2100046216.1.1",
          "form": "연애 중이신가용",
          "original_form": "연애 중이신가용",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046216.1.2",
          "form": "아니요 연애중이세요?",
          "original_form": "아니요 연애중이세요?",
          "speaker_id": "2",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046216.1.3",
          "form": "저도 솔로예요!!!",
          "original_form": "저도 솔로예요!!!",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046216.1.4",
          "form": "사람만나기가 어렵워요",
          "original_form": "사람만나기가 어렵워요",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046216.1.5",
          "form": "코로나라 ㅜㅜ",
          "original_form": "코로나라 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046216.1.6",
          "form": "마지막연애가 언제세요",
          "original_form": "마지막연애가 언제세요",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046216.1.7",
          "form": "몇년전이요 .. 3년된듯해요",
          "original_form": "몇년전이요 .. 3년된듯해요",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100046216.1.8",
          "form": "저는 아직 안해봣어여 ㅠㅠ",
          "original_form": "저는 아직 안해봣어여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046216.1.9",
          "form": "소개로 만나셧나요?",
          "original_form": "소개로 만나셧나요?",
          "speaker_id": "1",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046216.1.10",
          "form": "그땐 혼자가 좋아 햇는데 이젠 놀아줄  친구들도 줄어들고",
          "original_form": "그땐 혼자가 좋아 햇는데 이젠 놀아줄  친구들도 줄어들고",
          "speaker_id": "2",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046216.1.11",
          "form": "아 동창이었어요 ㅋㅋ",
          "original_form": "아 동창이었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046216.1.12",
          "form": "우아 ㅜㅜ",
          "original_form": "우아 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046216.1.13",
          "form": "저는 아직 혼자가 좋은거같아요 ㅋㅋ",
          "original_form": "저는 아직 혼자가 좋은거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046216.1.14",
          "form": "점점 혼자가 심심해져요 .",
          "original_form": "점점 혼자가 심심해져요 .",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046216.1.15",
          "form": "막상 소개 받을려면 어색하드라구요 ㅠㅠ",
          "original_form": "막상 소개 받을려면 어색하드라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046216.1.16",
          "form": "네 첫만남의 어색함이람",
          "original_form": "네 첫만남의 어색함이람",
          "speaker_id": "2",
          "time": "20210908 11:26"
        },
