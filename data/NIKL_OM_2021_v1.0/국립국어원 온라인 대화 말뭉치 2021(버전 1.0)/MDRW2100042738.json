      "utterance": [
        {
          "id": "MDRW2100042738.1.1",
          "form": "그러니까요 머리도 덜마르고 부시시하네요ㅋㅋ",
          "original_form": "그러니까요 머리도 덜마르고 부시시하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:20"
        },
        {
          "id": "MDRW2100042738.1.2",
          "form": "맞아요 비오면 머리도 부스스..",
          "original_form": "맞아요 비오면 머리도 부스스..",
          "speaker_id": "1",
          "time": "20210907 08:20"
        },
        {
          "id": "MDRW2100042738.1.3",
          "form": "너무나 집에 있어야하는 날씬데요ㅜ",
          "original_form": "너무나 집에 있어야하는 날씬데요ㅜ",
          "speaker_id": "2",
          "time": "20210907 08:23"
        },
        {
          "id": "MDRW2100042738.1.4",
          "form": "회사는 댁에서 머나요?",
          "original_form": "회사는 댁에서 머나요?",
          "speaker_id": "2",
          "time": "20210907 08:23"
        },
        {
          "id": "MDRW2100042738.1.5",
          "form": "음 한 30분 거리요",
          "original_form": "음 한 30분 거리요",
          "speaker_id": "1",
          "time": "20210907 08:23"
        },
        {
          "id": "MDRW2100042738.1.6",
          "form": "근데 비 오는데 지하철 타러 오느라 좀 늦었어요ㅜ",
          "original_form": "근데 비 오는데 지하철 타러 오느라 좀 늦었어요ㅜ",
          "speaker_id": "1",
          "time": "20210907 08:24"
        },
        {
          "id": "MDRW2100042738.1.7",
          "form": "아 저도 30분정도... 그래도 가까운편이네요!",
          "original_form": "아 저도 30분정도... 그래도 가까운편이네요!",
          "speaker_id": "2",
          "time": "20210907 08:25"
        },
        {
          "id": "MDRW2100042738.1.8",
          "form": "네네 그나마 다행이죠",
          "original_form": "네네 그나마 다행이죠",
          "speaker_id": "1",
          "time": "20210907 08:25"
        },
        {
          "id": "MDRW2100042738.1.9",
          "form": "전 직장은 2시간 거리라",
          "original_form": "전 직장은 2시간 거리라",
          "speaker_id": "1",
          "time": "20210907 08:25"
        },
        {
          "id": "MDRW2100042738.1.10",
          "form": "비 오는 날 진짜 고역이었거든요",
          "original_form": "비 오는 날 진짜 고역이었거든요",
          "speaker_id": "1",
          "time": "20210907 08:25"
        },
        {
          "id": "MDRW2100042738.1.11",
          "form": "아휴...ㅜㅠ",
          "original_form": "아휴...ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 08:25"
        },
        {
          "id": "MDRW2100042738.1.12",
          "form": "트렌치입으려다 가디건입었는데 큰일날뻔했네요",
          "original_form": "트렌치입으려다 가디건입었는데 큰일날뻔했네요",
          "speaker_id": "2",
          "time": "20210907 08:26"
        },
        {
          "id": "MDRW2100042738.1.13",
          "form": "아이고 비오는날 트렌치는",
          "original_form": "아이고 비오는날 트렌치는",
          "speaker_id": "1",
          "time": "20210907 08:26"
        },
        {
          "id": "MDRW2100042738.1.14",
          "form": "탁월한 선택하셨네요",
          "original_form": "탁월한 선택하셨네요",
          "speaker_id": "1",
          "time": "20210907 08:26"
        },
        {
          "id": "MDRW2100042738.1.15",
          "form": "ㅋㅋ 너무 길어서 비 닦고다니는 느낌...",
          "original_form": "ㅋㅋ 너무 길어서 비 닦고다니는 느낌...",
          "speaker_id": "2",
          "time": "20210907 08:27"
        },
        {
          "id": "MDRW2100042738.1.16",
          "form": "이랄까요..",
          "original_form": "이랄까요..",
          "speaker_id": "2",
          "time": "20210907 08:27"
        },
        {
          "id": "MDRW2100042738.1.17",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210907 08:27"
        },
        {
          "id": "MDRW2100042738.1.18",
          "form": "무겁고",
          "original_form": "무겁고",
          "speaker_id": "1",
          "time": "20210907 08:27"
        },
        {
          "id": "MDRW2100042738.1.19",
          "form": "투톤 된다고요ㅜ",
