      "utterance": [
        {
          "id": "MDRW2100027168.1.1",
          "form": "아는 오빠 동생이였죠!",
          "original_form": "아는 오빠 동생이였죠!",
          "speaker_id": "2",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027168.1.2",
          "form": "우와아아 연상",
          "original_form": "우와아아 연상",
          "speaker_id": "1",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027168.1.3",
          "form": "연상 좋아요 ㅋㅋㅋ 뭔가 듬직한 느낌!",
          "original_form": "연상 좋아요 ㅋㅋㅋ 뭔가 듬직한 느낌!",
          "speaker_id": "1",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027168.1.4",
          "form": "맞어요 너무 좋아요",
          "original_form": "맞어요 너무 좋아요",
          "speaker_id": "2",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027168.1.5",
          "form": "원래 좋아하던 오빠였기도했고",
          "original_form": "원래 좋아하던 오빠였기도했고",
          "speaker_id": "2",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027168.1.6",
          "form": "지금이 너무 설레요 ㅎ",
          "original_form": "지금이 너무 설레요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027168.1.7",
          "form": "우와아앙 대박....",
          "original_form": "우와아앙 대박....",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027168.1.8",
          "form": "쟁취하셨군요 사랑을!!!",
          "original_form": "쟁취하셨군요 사랑을!!!",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027168.1.9",
          "form": "맞아요!!! 오랜시간걸렸죠",
          "original_form": "맞아요!!! 오랜시간걸렸죠",
          "speaker_id": "2",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027168.1.10",
          "form": "근데전에 사귄남자랑엄청오래가서",
          "original_form": "근데전에 사귄남자랑엄청오래가서",
          "speaker_id": "2",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027168.1.11",
          "form": "먼가 지금이 더 조심스러운거같아요",
          "original_form": "먼가 지금이 더 조심스러운거같아요",
          "speaker_id": "2",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027168.1.12",
          "form": "우와... 진짜 오래 사귀시는 구나. 저는 연애하면 일년을 못넘겨요 ㅋㅋ 크리스마스의 저주예요 진짜",
          "original_form": "우와... 진짜 오래 사귀시는 구나. 저는 연애하면 일년을 못넘겨요 ㅋㅋ 크리스마스의 저주예요 진짜",
          "speaker_id": "1",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027168.1.13",
          "form": "저도 처음가봤어요 1년 넘긴적",
          "original_form": "저도 처음가봤어요 1년 넘긴적",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027168.1.14",
          "form": "부럽다ㅠㅠ 저도 좀 오래 사귀고 싶어요",
          "original_form": "부럽다ㅠㅠ 저도 좀 오래 사귀고 싶어요",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027168.1.15",
          "form": "이번에도 오래 가셔서 백년해로 만수무강 하세요",
          "original_form": "이번에도 오래 가셔서 백년해로 만수무강 하세요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027168.1.16",
          "form": "꼭 오래가고싶네요!!",
          "original_form": "꼭 오래가고싶네요!!",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027168.1.17",
          "form": "name1도 오래가는연애하셔야죠!",
          "original_form": "&name1&도 오래가는연애하셔야죠!",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027168.1.18",
          "form": "그러실 거예용",
          "original_form": "그러실 거예용",
          "speaker_id": "1",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027168.1.19",
          "form": "으아아 그래야하는데ㅠㅠ",
          "original_form": "으아아 그래야하는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100027168.1.20",
          "form": "사람이 없네요!ㅜㅜ",
          "original_form": "사람이 없네요!ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100027168.1.21",
          "form": "소개팅앱은",
          "original_form": "소개팅앱은",
          "speaker_id": "2",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100027168.1.22",
          "form": "써보셨나요?",
          "original_form": "써보셨나요?",
          "speaker_id": "2",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100027168.1.23",
          "form": "네! 근데 친구만 먹고 말았어요 히히",
          "original_form": "네! 근데 친구만 먹고 말았어요 히히",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027168.1.24",
          "form": "아이공 ...ㅋㅋㅋㅋㅋㅋ 마음이아프네요",
          "original_form": "아이공 ...ㅋㅋㅋㅋㅋㅋ 마음이아프네요",
          "speaker_id": "2",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100027168.1.25",
          "form": "ㅋㅋㅋㅋ진짜 찐친 생겨서 거의 4년째 알고 지내요!",
          "original_form": "ㅋㅋㅋㅋ진짜 찐친 생겨서 거의 4년째 알고 지내요!",
          "speaker_id": "1",
          "time": "20210902 13:16"
        },
        {
          "id": "MDRW2100027168.1.26",
          "form": "코로나여서 자주 못보지만 같이 있으면 재밌어요~",
          "original_form": "코로나여서 자주 못보지만 같이 있으면 재밌어요~",
