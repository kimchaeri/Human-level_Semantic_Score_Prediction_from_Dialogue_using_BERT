      "utterance": [
        {
          "id": "MDRW2100032376.1.1",
          "form": "그리고 다른 작품 읽을 때마다 나의 상식이 부족한 점이나 자료조사가 안되는 점만 자꾸 생각나서",
          "original_form": "그리고 다른 작품 읽을 때마다 나의 상식이 부족한 점이나 자료조사가 안되는 점만 자꾸 생각나서",
          "speaker_id": "1",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100032376.1.2",
          "form": "이 점은 글 쓰고싶은 의욕을 꺾여",
          "original_form": "이 점은 글 쓰고싶은 의욕을 꺾여",
          "speaker_id": "1",
          "time": "20210905 16:38"
        },
        {
          "id": "MDRW2100032376.1.3",
          "form": "다독을 해도 내용이 머리에 들어오는 게 아니라 딱 스낵컬쳐로 즐기고 마니까",
          "original_form": "다독을 해도 내용이 머리에 들어오는 게 아니라 딱 스낵컬쳐로 즐기고 마니까",
          "speaker_id": "1",
          "time": "20210905 16:38"
        },
        {
          "id": "MDRW2100032376.1.4",
          "form": "머리가 텅텅",
          "original_form": "머리가 텅텅",
          "speaker_id": "1",
          "time": "20210905 16:38"
        },
        {
          "id": "MDRW2100032376.1.5",
          "form": "근데 보통 작가들이 글쓰는 방식이 거기서부터 시작하지 않아?",
          "original_form": "근데 보통 작가들이 글쓰는 방식이 거기서부터 시작하지 않아?",
          "speaker_id": "2",
          "time": "20210905 16:40"
        },
        {
          "id": "MDRW2100032376.1.6",
          "form": "나라면 어떻게 쓸까.",
          "original_form": "나라면 어떻게 쓸까.",
          "speaker_id": "2",
          "time": "20210905 16:40"
        },
        {
          "id": "MDRW2100032376.1.7",
          "form": "토가시가 그렇게 연구하면서 작품 본다던데",
          "original_form": "토가시가 그렇게 연구하면서 작품 본다던데",
          "speaker_id": "2",
          "time": "20210905 16:41"
        },
        {
          "id": "MDRW2100032376.1.8",
          "form": "스낵컬쳐라 해도 읽는 건 전부 재산이라",
          "original_form": "스낵컬쳐라 해도 읽는 건 전부 재산이라",
          "speaker_id": "2",
          "time": "20210905 16:41"
        },
        {
          "id": "MDRW2100032376.1.9",
          "form": "쓰면 언젠간 조금씩 도움도 될거구",
          "original_form": "쓰면 언젠간 조금씩 도움도 될거구",
          "speaker_id": "2",
          "time": "20210905 16:41"
        },
        {
          "id": "MDRW2100032376.1.10",
          "form": "흐흥 ㅠㅠㅠ",
          "original_form": "흐흥 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 16:42"
        },
        {
          "id": "MDRW2100032376.1.11",
          "form": "줄줄 쓰고싶어..... 나도......",
          "original_form": "줄줄 쓰고싶어..... 나도......",
          "speaker_id": "1",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032376.1.12",
          "form": "조금씩 써보자 ㅇ0ㅇ 신작은 연금이니까!",
          "original_form": "조금씩 써보자 ㅇ0ㅇ 신작은 연금이니까!",
          "speaker_id": "2",
          "time": "20210905 16:46"
        },
        {
          "id": "MDRW2100032376.1.13",
          "form": "웅....그래야지.....",
          "original_form": "웅....그래야지.....",
          "speaker_id": "1",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100032376.1.14",
          "form": "전에 소설쓰기의 모든것?이라는 책을 사서 봤는데도 나는 좀 참고도 안하는 거 같음",
          "original_form": "전에 소설쓰기의 모든것?이라는 책을 사서 봤는데도 나는 좀 참고도 안하는 거 같음",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100032376.1.15",
          "form": "바보인가",
          "original_form": "바보인가",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100032376.1.16",
          "form": "참고하라고 사서 읽고 읽기만함",
          "original_form": "참고하라고 사서 읽고 읽기만함",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100032376.1.17",
          "form": "읽었으니까 글 쓴 거 아니겠엉?",
          "original_form": "읽었으니까 글 쓴 거 아니겠엉?",
          "speaker_id": "2",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100032376.1.18",
          "form": "난 그거 다 알음알음 자산이 됐을거라고 생각해",
          "original_form": "난 그거 다 알음알음 자산이 됐을거라고 생각해",
          "speaker_id": "2",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100032376.1.19",
          "form": "뱉뱉",
          "original_form": "뱉뱉",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100032376.1.20",
          "form": "글써서 부자가 되도록해",
          "original_form": "글써서 부자가 되도록해",
          "speaker_id": "2",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100032376.1.21",
          "form": "내 꿈이야",
          "original_form": "내 꿈이야",
          "speaker_id": "1",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100032376.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ내 취향이 많이 나오면",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ내 취향이 많이 나오면",
          "speaker_id": "1",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100032376.1.23",
          "form": "쓸일도없는데",
          "original_form": "쓸일도없는데",
          "speaker_id": "1",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100032376.1.24",
          "form": "나올때가되어따....",
          "original_form": "나올때가되어따....",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032376.1.25",
          "form": "아까 지엘읽는다고햇자나",
          "original_form": "아까 지엘읽는다고햇자나",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032376.1.26",
          "form": "여기도 공수?는 고정적이네",
          "original_form": "여기도 공수?는 고정적이네",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032376.1.27",
          "form": "다 공쪽이 능동적극이고",
          "original_form": "다 공쪽이 능동적극이고",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032376.1.28",
          "form": "수가 수동적인편에",
          "original_form": "수가 수동적인편에",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032376.1.29",
          "form": "지엘은 좀 다를 줄 알았다",
          "original_form": "지엘은 좀 다를 줄 알았다",
          "speaker_id": "1",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100032376.1.30",
          "form": "에이 어디가나 다 똑같지",
          "original_form": "에이 어디가나 다 똑같지",
          "speaker_id": "2",
          "time": "20210905 17:16"
        },
        {
          "id": "MDRW2100032376.1.31",
          "form": "지엘이기에 기대할만한 장면이 없어",
          "original_form": "지엘이기에 기대할만한 장면이 없어",
          "speaker_id": "1",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032376.1.32",
          "form": "ㅠㅠㅠ눈물 ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ눈물 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032376.1.33",
          "form": "읽은 만화 중에",
          "original_form": "읽은 만화 중에",
          "speaker_id": "2",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032376.1.34",
          "form": "느닷없이 지엘 조연이",
          "original_form": "느닷없이 지엘 조연이",
          "speaker_id": "2",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032376.1.35",
          "form": "나왔는데",
          "original_form": "나왔는데",
          "speaker_id": "2",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100032376.1.36",
          "form": "너무 엄해서 당황했음ㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무 엄해서 당황했음ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100032376.1.37",
          "form": "어떤건뎅?",
          "original_form": "어떤건뎅?",
          "speaker_id": "1",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100032376.1.38",
          "form": "헤테로물인데 지엘컾이 나와?",
          "original_form": "헤테로물인데 지엘컾이 나와?",
          "speaker_id": "1",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032376.1.39",
          "form": "걍 현태판타지 만화였는데",
          "original_form": "걍 현태판타지 만화였는데",
          "speaker_id": "2",
          "time": "20210905 17:25"
        },
        {
          "id": "MDRW2100032376.1.40",
          "form": "정확히는",
          "original_form": "정확히는",
          "speaker_id": "2",
          "time": "20210905 17:25"
        },
        {
          "id": "MDRW2100032376.1.41",
          "form": "마법세계에 살던 주인공이 현대로 넘어온",
          "original_form": "마법세계에 살던 주인공이 현대로 넘어온",
          "speaker_id": "2",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100032376.1.42",
          "form": "인외마경",
          "original_form": "인외마경",
          "speaker_id": "2",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100032376.1.43",
          "form": "배경의 작품인데 거기 나오는 조연이",
          "original_form": "배경의 작품인데 거기 나오는 조연이",
          "speaker_id": "2",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100032376.1.44",
          "form": "넘나 당당하게",
          "original_form": "넘나 당당하게",
          "speaker_id": "2",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100032376.1.45",
          "form": "성적지향을 밝히지 않고",
          "original_form": "성적지향을 밝히지 않고",
          "speaker_id": "2",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100032376.1.46",
          "form": "꺄아 꺄아",
          "original_form": "꺄아 꺄아",
          "speaker_id": "2",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100032376.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ꺄아꺄아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ꺄아꺄아",
          "speaker_id": "1",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100032376.1.48",
          "form": "나의 마녀에서도 은은하게 나왔지",
          "original_form": "나의 마녀에서도 은은하게 나왔지",
          "speaker_id": "1",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100032376.1.49",
          "form": "나의 마녀는 은은했지만",
          "original_form": "나의 마녀는 은은했지만",
          "speaker_id": "2",
          "time": "20210905 17:27"
        }
      ]
    }
