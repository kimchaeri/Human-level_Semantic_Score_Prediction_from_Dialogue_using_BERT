      "utterance": [
        {
          "id": "MDRW2100037930.1.1",
          "form": "첨엔 컨셉인줄 알았는데",
          "original_form": "첨엔 컨셉인줄 알았는데",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037930.1.2",
          "form": "ㄹㅇ 스토리가 산으로 가...",
          "original_form": "ㄹㅇ 스토리가 산으로 가...",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037930.1.3",
          "form": "ㅇㅇ......제발",
          "original_form": "ㅇㅇ......제발",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037930.1.4",
          "form": "완결 잘 나는 중겜",
          "original_form": "완결 잘 나는 중겜",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037930.1.5",
          "form": "하나만 보여달라......",
          "original_form": "하나만 보여달라......",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037930.1.6",
          "form": "멘스 아직 안밀어봣지?",
          "original_form": "멘스 아직 안밀어봣지?",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037930.1.7",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037930.1.8",
          "form": "나 이제 퇴근해서",
          "original_form": "나 이제 퇴근해서",
          "speaker_id": "1",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037930.1.9",
          "form": "이따 씻고 집가서 쭉 밀려고",
          "original_form": "이따 씻고 집가서 쭉 밀려고",
          "speaker_id": "1",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037930.1.10",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037930.1.11",
          "form": "산호궁 진짜 몽환적인 느낌이다",
          "original_form": "산호궁 진짜 몽환적인 느낌이다",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037930.1.12",
          "form": "코코미랑 엄청 잘어울리네",
          "original_form": "코코미랑 엄청 잘어울리네",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037930.1.13",
          "form": "ㅇㅇ거기는 진짜 잘뽑은것같아",
          "original_form": "ㅇㅇ거기는 진짜 잘뽑은것같아",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037930.1.14",
          "form": "절벽이 빡치게 생긴거 빼면",
          "original_form": "절벽이 빡치게 생긴거 빼면",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037930.1.15",
          "form": "ㅇㅇㅋㅋㅋㅋㅋ 절벽은 진짜 ㅎ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋ 절벽은 진짜 ㅎ",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037930.1.16",
          "form": "기어오르라고 만든 구조가 아니긴 하더라..",
          "original_form": "기어오르라고 만든 구조가 아니긴 하더라..",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037930.1.17",
          "form": "꼭 번개씨앗으로 이동해야함...",
          "original_form": "꼭 번개씨앗으로 이동해야함...",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037930.1.18",
          "form": "ㅇㅇ........",
          "original_form": "ㅇㅇ........",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037930.1.19",
          "form": "악으로 깡으로 기어오르긴했는데",
          "original_form": "악으로 깡으로 기어오르긴했는데",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037930.1.20",
          "form": "ㄹㅇ겁나 짜증나",
          "original_form": "ㄹㅇ겁나 짜증나",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037930.1.21",
          "form": "난 올라가다가 꼭 턱?같은데 막혀서...",
          "original_form": "난 올라가다가 꼭 턱?같은데 막혀서...",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037930.1.22",
          "form": "눈동자도 빡치게",
          "original_form": "눈동자도 빡치게",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037930.1.23",
          "form": "아ㅇㅇㅇㅇ",
          "original_form": "아ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037930.1.24",
          "form": "눈동자 하나",
          "original_form": "눈동자 하나",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037930.1.25",
          "form": "절벽 안쪽 바로 밑",
          "original_form": "절벽 안쪽 바로 밑",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037930.1.26",
          "form": "이래서 먹기 개힘들어",
          "original_form": "이래서 먹기 개힘들어",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037930.1.27",
          "form": "작은 동굴?천장에",
          "original_form": "작은 동굴?천장에",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037930.1.28",
          "form": "있는데",
          "original_form": "있는데",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037930.1.29",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037930.1.30",
          "form": "거기 바로 밑에 벽은",
          "original_form": "거기 바로 밑에 벽은",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037930.1.31",
          "form": "황성을 써도",
          "original_form": "황성을 써도",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037930.1.32",
          "form": "막히는거야",
          "original_form": "막히는거야",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037930.1.33",
          "form": "누가 아이템 배치를 이모양으로 하랬지요...?",
          "original_form": "누가 아이템 배치를 이모양으로 하랬지요...?",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037930.1.34",
          "form": "그래서 아예 바깥쪽에서 벽을 잡고",
          "original_form": "그래서 아예 바깥쪽에서 벽을 잡고",
          "speaker_id": "2",
          "time": "20210901 22:13"
        }
      ]
    }
