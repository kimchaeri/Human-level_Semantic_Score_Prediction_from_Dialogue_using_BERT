      "utterance": [
        {
          "id": "MDRW2100013450.1.1",
          "form": "당연 코카콜라지",
          "original_form": "당연 코카콜라지",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013450.1.2",
          "form": "아두나두",
          "original_form": "아두나두",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013450.1.3",
          "form": "펩시는 별로얍",
          "original_form": "펩시는 별로얍",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013450.1.4",
          "form": "왜냐면",
          "original_form": "왜냐면",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013450.1.5",
          "form": "너무달아",
          "original_form": "너무달아",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013450.1.6",
          "form": "ㅇㅈ",
          "original_form": "ㅇㅈ",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013450.1.7",
          "form": "글고 쫌 탄산이많아",
          "original_form": "글고 쫌 탄산이많아",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013450.1.8",
          "form": "코카콜라보다",
          "original_form": "코카콜라보다",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013450.1.9",
          "form": "더심한듯",
          "original_form": "더심한듯",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013450.1.10",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013450.1.11",
          "form": "그래서안먹구샆어",
          "original_form": "그래서안먹구샆어",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013450.1.12",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013450.1.13",
          "form": "치킨집에서 자꾸 줘 ㅡㅡ",
          "original_form": "치킨집에서 자꾸 줘 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013450.1.14",
          "form": "으악",
          "original_form": "으악",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013450.1.15",
          "form": "코카콜라로 주세요",
          "original_form": "코카콜라로 주세요",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013450.1.16",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013450.1.17",
          "form": "펩시싫어요",
          "original_form": "펩시싫어요",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013450.1.18",
          "form": "아파요",
          "original_form": "아파요",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013450.1.19",
          "form": "진짜에요",
          "original_form": "진짜에요",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013450.1.20",
          "form": "근데 또 콜라가 아예없음 좀그래..",
          "original_form": "근데 또 콜라가 아예없음 좀그래..",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013450.1.21",
          "form": "그것도마저..",
          "original_form": "그것도마저..",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013450.1.22",
          "form": "잇긴해야해",
          "original_form": "잇긴해야해",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013450.1.23",
          "form": "그게 코카엿우명",
          "original_form": "그게 코카엿우명",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013450.1.24",
          "form": "ㅁㅈ 펩시는 가라!!!",
          "original_form": "ㅁㅈ 펩시는 가라!!!",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013450.1.25",
          "form": "피자는 머가젤 좋아?",
          "original_form": "피자는 머가젤 좋아?",
