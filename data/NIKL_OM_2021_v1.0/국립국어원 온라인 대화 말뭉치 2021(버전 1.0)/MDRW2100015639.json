      "utterance": [
        {
          "id": "MDRW2100015639.1.1",
          "form": "최근에 구매한 물건 있으세요?",
          "original_form": "최근에 구매한 물건 있으세요?",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015639.1.2",
          "form": "음 가방요",
          "original_form": "음 가방요",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015639.1.3",
          "form": "뭐 사셨어요?",
          "original_form": "뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015639.1.4",
          "form": "저는 폼롤러요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 폼롤러요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015639.1.5",
          "form": "가방 뭐사셨는지 여쭤봐도 될까요?",
          "original_form": "가방 뭐사셨는지 여쭤봐도 될까요?",
          "speaker_id": "2",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015639.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015639.1.7",
          "form": "아들 가방요",
          "original_form": "아들 가방요",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015639.1.8",
          "form": "tomy 가방요",
          "original_form": "tomy 가방요",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015639.1.9",
          "form": "케주얼한 가벼운 가방요",
          "original_form": "케주얼한 가벼운 가방요",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015639.1.10",
          "form": "가방 좋아하세요?",
          "original_form": "가방 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015639.1.11",
          "form": "아 가방을 좋아하는건 아니구요 새로 사야겠다 싶어서 고민하던중이엇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 가방을 좋아하는건 아니구요 새로 사야겠다 싶어서 고민하던중이엇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015639.1.12",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015639.1.13",
          "form": "전가방 잘모르겠어요",
          "original_form": "전가방 잘모르겠어요",
          "speaker_id": "1",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015639.1.14",
          "form": "어떤 메이커 보셨어요?",
          "original_form": "어떤 메이커 보셨어요?",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015639.1.15",
          "form": "저도 잘 몰라서 ㅋㅋㅋㅋ주변에 잘아는사람에게 물어볼까 하던 참이엇어요",
          "original_form": "저도 잘 몰라서 ㅋㅋㅋㅋ주변에 잘아는사람에게 물어볼까 하던 참이엇어요",
          "speaker_id": "2",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015639.1.16",
          "form": "잘아는 사람 물어보세요 ㅋㅋ",
          "original_form": "잘아는 사람 물어보세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015639.1.17",
          "form": "전 기냥 캉골 코치밖에  몰라요 ㅜㅜ",
          "original_form": "전 기냥 캉골 코치밖에  몰라요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015639.1.18",
          "form": "ㅋㅋㅋㅋㅋ맞아요 저도 캉골 아는정도",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 저도 캉골 아는정도",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015639.1.19",
          "form": "기냥 막메기 좋아요",
          "original_form": "기냥 막메기 좋아요",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015639.1.20",
          "form": "디자인도 심플하고",
          "original_form": "디자인도 심플하고",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015639.1.21",
          "form": "ㅋㅋㅋㅋㅋ맞아요 깔끔",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 깔끔",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015639.1.22",
          "form": "네 학생들도 많이 하고",
          "original_form": "네 학생들도 많이 하고",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015639.1.23",
          "form": "맞아요 캉골은 사는 연령층이 다양한것같아요",
          "original_form": "맞아요 캉골은 사는 연령층이 다양한것같아요",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015639.1.24",
          "form": "네 ㅎㅎ가격도 착해요",
          "original_form": "네 ㅎㅎ가격도 착해요",
          "speaker_id": "1",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015639.1.25",
          "form": "부담없고",
          "original_form": "부담없고",
          "speaker_id": "1",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015639.1.26",
          "form": "가격도 중요하죠 ㅋㅋㅋㅋ",
          "original_form": "가격도 중요하죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015639.1.27",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015639.1.28",
          "form": "같은 연령대 친구가 뭐 매는데요?",
          "original_form": "같은 연령대 친구가 뭐 매는데요?",
          "speaker_id": "1",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015639.1.29",
          "form": "천차만별인것같아요",
          "original_form": "천차만별인것같아요",
          "speaker_id": "2",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015639.1.30",
