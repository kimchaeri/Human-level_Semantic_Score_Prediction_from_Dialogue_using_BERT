      "utterance": [
        {
          "id": "MDRW2100040355.1.1",
          "form": "일반 김치도 직접 담근 곳이 많아서 좋았어요",
          "original_form": "일반 김치도 직접 담근 곳이 많아서 좋았어요",
          "speaker_id": "2",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100040355.1.2",
          "form": "광역시 같은 곳은 대부분 중국 김치 거든요",
          "original_form": "광역시 같은 곳은 대부분 중국 김치 거든요",
          "speaker_id": "2",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100040355.1.3",
          "form": "ㅎ...ㅎ",
          "original_form": "ㅎ...ㅎ",
          "speaker_id": "1",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100040355.1.4",
          "form": "전라도는 농사를 짓다 보니 직접 담금 김치가 많아요",
          "original_form": "전라도는 농사를 짓다 보니 직접 담금 김치가 많아요",
          "speaker_id": "2",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100040355.1.5",
          "form": "김치맛이  다른게 확 느껴지나요!",
          "original_form": "김치맛이  다른게 확 느껴지나요!",
          "speaker_id": "1",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040355.1.6",
          "form": "완전 달라요",
          "original_form": "완전 달라요",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040355.1.7",
          "form": "신 김치는 먹는 순간",
          "original_form": "신 김치는 먹는 순간",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040355.1.8",
          "form": "바로 라면 각",
          "original_form": "바로 라면 각",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040355.1.9",
          "form": "ㅋㅋㅋ왜 내가 라면을 시키지않았나 자괴감이들어요ㅋㅋ",
          "original_form": "ㅋㅋㅋ왜 내가 라면을 시키지않았나 자괴감이들어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040355.1.10",
          "form": "아닠 내일 당장 전라도 내려가야 할 것 같은 이 기분",
          "original_form": "아닠 내일 당장 전라도 내려가야 할 것 같은 이 기분",
          "speaker_id": "1",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100040355.1.11",
          "form": "왜 아직 전라도를 안 가 봤나 자괴감 드네요ㅎㅎ",
          "original_form": "왜 아직 전라도를 안 가 봤나 자괴감 드네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100040355.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ음  좀 멀긴 하지만",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ음  좀 멀긴 하지만",
          "speaker_id": "2",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100040355.1.13",
          "form": "전라남도 고흥도 좋아요",
          "original_form": "전라남도 고흥도 좋아요",
          "speaker_id": "2",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100040355.1.14",
          "form": "거긴 뭐가 유명한가요!!",
          "original_form": "거긴 뭐가 유명한가요!!",
          "speaker_id": "1",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100040355.1.15",
          "form": "나로호 우주발사전망대가 있는데 전망대는별로고",
          "original_form": "나로호 우주발사전망대가 있는데 전망대는별로고",
          "speaker_id": "2",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100040355.1.16",
          "form": "끝에 있다보니 바닷가 근처거든요",
          "original_form": "끝에 있다보니 바닷가 근처거든요",
          "speaker_id": "2",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100040355.1.17",
          "form": "진짜 맑고파래요! 멋진바다보시고",
          "original_form": "진짜 맑고파래요! 멋진바다보시고",
          "speaker_id": "2",
          "time": "20210904 17:05"
        },
        {
          "id": "MDRW2100040355.1.18",
          "form": "유명한 기사식당이 있어요ㅋㅋㅋㄱㅋ",
          "original_form": "유명한 기사식당이 있어요ㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210904 17:05"
        },
        {
          "id": "MDRW2100040355.1.19",
          "form": "삼겹살백반집인데",
          "original_form": "삼겹살백반집인데",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100040355.1.20",
          "form": "기사식당이면 맛은 입증이네요ㄷㄷ",
          "original_form": "기사식당이면 맛은 입증이네요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100040355.1.21",
          "form": "반찬부터 다맛있어요ㅋㅋㅋ",
          "original_form": "반찬부터 다맛있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100040355.1.22",
          "form": "백반에 삼겹살이 같이 나오나요ㄷㄷ",
          "original_form": "백반에 삼겹살이 같이 나오나요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210904 17:07"
        },
        {
          "id": "MDRW2100040355.1.23",
          "form": "아님 삼겹도 파는 백반집인가요ㄷㄷ",
          "original_form": "아님 삼겹도 파는 백반집인가요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210904 17:07"
        },
        {
          "id": "MDRW2100040355.1.24",
          "form": "메뉴가 삼겹살백반인데 삼겹살은 1차로 구워져서 나오구요",
          "original_form": "메뉴가 삼겹살백반인데 삼겹살은 1차로 구워져서 나오구요",
          "speaker_id": "2",
          "time": "20210904 17:07"
        },
        {
          "id": "MDRW2100040355.1.25",
          "form": "백반에 삼겹이 포함된 전라도 스케일ㄷㄷ",
          "original_form": "백반에 삼겹이 포함된 전라도 스케일ㄷㄷ",
          "speaker_id": "1",
          "time": "20210904 17:07"
        }
      ]
