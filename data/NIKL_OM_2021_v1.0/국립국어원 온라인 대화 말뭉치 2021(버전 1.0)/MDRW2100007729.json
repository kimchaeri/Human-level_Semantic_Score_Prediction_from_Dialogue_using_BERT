      "utterance": [
        {
          "id": "MDRW2100007729.1.1",
          "form": "요즘 얼굴에 여드름 많이 나는데 왜일까 ?",
          "original_form": "요즘 얼굴에 여드름 많이 나는데 왜일까 ?",
          "speaker_id": "1",
          "time": "20210822 10:00"
        },
        {
          "id": "MDRW2100007729.1.2",
          "form": "그니까 여드름 나네ㅠ",
          "original_form": "그니까 여드름 나네ㅠ",
          "speaker_id": "2",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007729.1.3",
          "form": "몸이 힘들고 스트레스 때문에 더한것 아닐까?",
          "original_form": "몸이 힘들고 스트레스 때문에 더한것 아닐까?",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100007729.1.4",
          "form": "맞아 스트레스 받아ㅠ",
          "original_form": "맞아 스트레스 받아ㅠ",
          "speaker_id": "2",
          "time": "20210822 10:07"
        },
        {
          "id": "MDRW2100007729.1.5",
          "form": "잠을 잘못자니깐 \r\n스트레스도 더하겠지",
          "original_form": "잠을 잘못자니깐 \r\n스트레스도 더하겠지",
          "speaker_id": "1",
          "time": "20210822 10:08"
        },
        {
          "id": "MDRW2100007729.1.6",
          "form": "맞아 잠이 건강인데",
          "original_form": "맞아 잠이 건강인데",
          "speaker_id": "2",
          "time": "20210822 10:18"
        },
        {
          "id": "MDRW2100007729.1.7",
          "form": "그럼 여드름 약이라도 꾸준히 발라",
          "original_form": "그럼 여드름 약이라도 꾸준히 발라",
          "speaker_id": "1",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007729.1.8",
          "form": "그럴려고 하는데 자꾸까먹네",
          "original_form": "그럴려고 하는데 자꾸까먹네",
          "speaker_id": "2",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007729.1.9",
          "form": "자주 얘기 해줄께",
          "original_form": "자주 얘기 해줄께",
          "speaker_id": "1",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100007729.1.10",
          "form": "저녁마다 운동하는것 힘들지 않아",
          "original_form": "저녁마다 운동하는것 힘들지 않아",
          "speaker_id": "2",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100007729.1.11",
          "form": "조금 지치긴 하는데 건강하려면 해야지",
          "original_form": "조금 지치긴 하는데 건강하려면 해야지",
          "speaker_id": "1",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007729.1.12",
          "form": "나두 같이 하면 좋을테데",
          "original_form": "나두 같이 하면 좋을테데",
          "speaker_id": "2",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007729.1.13",
          "form": "그래 온가족이 건강하면 좋지\r\n유산소 운동 좋아",
          "original_form": "그래 온가족이 건강하면 좋지\r\n유산소 운동 좋아",
          "speaker_id": "1",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007729.1.14",
          "form": "내가 게을러서 안되네",
          "original_form": "내가 게을러서 안되네",
          "speaker_id": "2",
          "time": "20210822 10:55"
        },
        {
          "id": "MDRW2100007729.1.15",
          "form": "건강하려면 좀 부지런하긴 해야해",
          "original_form": "건강하려면 좀 부지런하긴 해야해",
          "speaker_id": "1",
          "time": "20210822 10:55"
        },
        {
          "id": "MDRW2100007729.1.16",
          "form": "운동하면 체력도 좋아지려나 ?",
          "original_form": "운동하면 체력도 좋아지려나 ?",
          "speaker_id": "2",
          "time": "20210822 10:56"
        },
        {
          "id": "MDRW2100007729.1.17",
          "form": "엄청 좋아져\r\n꾸준히 하면 난 느껴지는것 같아",
          "original_form": "엄청 좋아져\r\n꾸준히 하면 난 느껴지는것 같아",
          "speaker_id": "1",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007729.1.18",
          "form": "운동후 아픈건 덜해보이긴 하네",
          "original_form": "운동후 아픈건 덜해보이긴 하네",
          "speaker_id": "2",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007729.1.19",
          "form": "훨씬 덜해 근데 안하는 날은 좀 아픈것 같아",
          "original_form": "훨씬 덜해 근데 안하는 날은 좀 아픈것 같아",
          "speaker_id": "1",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007729.1.20",
          "form": "그래서 사람들이 매일 운동을 하나 ?",
          "original_form": "그래서 사람들이 매일 운동을 하나 ?",
          "speaker_id": "2",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007729.1.21",
          "form": "넌 아직 아픈데가 없으니 실감이 안날거야",
          "original_form": "넌 아직 아픈데가 없으니 실감이 안날거야",
          "speaker_id": "1",
          "time": "20210822 11:01"
        },
        {
          "id": "MDRW2100007729.1.22",
          "form": "직접 와닿지는 않아\r\n잘모르겠어 ?",
          "original_form": "직접 와닿지는 않아\r\n잘모르겠어 ?",
          "speaker_id": "2",
          "time": "20210822 11:01"
        },
        {
          "id": "MDRW2100007729.1.23",
          "form": "어른들이 그러잖아 !\r\n너도 늙은면 다 안다고 ?",
          "original_form": "어른들이 그러잖아 !\r\n너도 늙은면 다 안다고 ?",
          "speaker_id": "1",
          "time": "20210822 11:02"
        },
        {
          "id": "MDRW2100007729.1.24",
          "form": "글쎄 ! 더 나이들면 운동에 절실함을 느낄까 ?",
          "original_form": "글쎄 ! 더 나이들면 운동에 절실함을 느낄까 ?",
          "speaker_id": "2",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100007729.1.25",
          "form": "그럼 그때는 확 느낄걸",
          "original_form": "그럼 그때는 확 느낄걸",
          "speaker_id": "1",
          "time": "20210822 11:03"
        }
      ]
    }
  ]
