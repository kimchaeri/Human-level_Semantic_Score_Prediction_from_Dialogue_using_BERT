        {
          "id": "MDRW2100040261.1.1",
          "form": "젤사고시픈거하나머죠",
          "original_form": "젤사고시픈거하나머죠",
          "speaker_id": "2",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040261.1.2",
          "form": "가을 점퍼..?ㅋㅋㅋㅋㅋ 아니다 가디건으로 할게요",
          "original_form": "가을 점퍼..?ㅋㅋㅋㅋㅋ 아니다 가디건으로 할게요",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040261.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040261.1.4",
          "form": "어디거용",
          "original_form": "어디거용",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040261.1.5",
          "form": "그로브 스토어라는 브랜드 좋아해용",
          "original_form": "그로브 스토어라는 브랜드 좋아해용",
          "speaker_id": "1",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040261.1.6",
          "form": "오제친구페이보릿",
          "original_form": "오제친구페이보릿",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040261.1.7",
          "form": "잘통하실듯",
          "original_form": "잘통하실듯",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040261.1.8",
          "form": "센스 있는 친구인가보네요...^^",
          "original_form": "센스 있는 친구인가보네요...^^",
          "speaker_id": "1",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040261.1.9",
          "form": "네 센스짱",
          "original_form": "네 센스짱",
          "speaker_id": "2",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040261.1.10",
          "form": "가디건 잘어울리실듯",
          "original_form": "가디건 잘어울리실듯",
          "speaker_id": "2",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040261.1.11",
          "form": "어멋 감사해요",
          "original_form": "어멋 감사해요",
          "speaker_id": "1",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100040261.1.12",
          "form": "먹짱님은 자켓 잘어울릴실것같아요",
          "original_form": "먹짱님은 자켓 잘어울릴실것같아요",
          "speaker_id": "1",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100040261.1.13",
          "form": "아어케아셧어요",
          "original_form": "아어케아셧어요",
          "speaker_id": "2",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100040261.1.14",
          "form": "그냥 그럴 것 같은 느낌이 들었어요 ㅋㅋㅋㅋ",
          "original_form": "그냥 그럴 것 같은 느낌이 들었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040261.1.15",
          "form": "통찰력이 조으시네요",
          "original_form": "통찰력이 조으시네요",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100040261.1.16",
          "form": "보여드리고싶어요",
          "original_form": "보여드리고싶어요",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100040261.1.17",
          "form": "제자켓",
          "original_form": "제자켓",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100040261.1.18",
          "form": "자켓 ㅎ",
          "original_form": "자켓 ㅎ",
          "speaker_id": "2",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100040261.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ 어떤자켓인데요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 어떤자켓인데요",
          "speaker_id": "1",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100040261.1.20",
          "form": "제자켓이요 많아요",
          "original_form": "제자켓이요 많아요",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100040261.1.21",
          "form": "갈색도 잇어요",
          "original_form": "갈색도 잇어요",
          "speaker_id": "2",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100040261.1.22",
          "form": "아 네 전 하늘색도 있고 브라운도 있고~ 다른색도 사고싶어요",
          "original_form": "아 네 전 하늘색도 있고 브라운도 있고~ 다른색도 사고싶어요",
