      "utterance": [
        {
          "id": "MDRW2100032828.1.1",
          "form": "오리고기가 들어가?",
          "original_form": "오리고기가 들어가?",
          "speaker_id": "1",
          "time": "20210906 00:34"
        },
        {
          "id": "MDRW2100032828.1.2",
          "form": "아니 다른 조류가 들어가",
          "original_form": "아니 다른 조류가 들어가",
          "speaker_id": "2",
          "time": "20210906 00:34"
        },
        {
          "id": "MDRW2100032828.1.3",
          "form": "참새고기 들어가?",
          "original_form": "참새고기 들어가?",
          "speaker_id": "1",
          "time": "20210906 00:34"
        },
        {
          "id": "MDRW2100032828.1.4",
          "form": "세상에 참새 먹을게있어?",
          "original_form": "세상에 참새 먹을게있어?",
          "speaker_id": "2",
          "time": "20210906 00:35"
        },
        {
          "id": "MDRW2100032828.1.5",
          "form": "그러게 근데 시골에서는 먹는 사람도 있대 ㅋㅋㅋㅋㅋ",
          "original_form": "그러게 근데 시골에서는 먹는 사람도 있대 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100032828.1.6",
          "form": "가축조류가 들어가?",
          "original_form": "가축조류가 들어가?",
          "speaker_id": "1",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100032828.1.7",
          "form": "응 가축조류들어가",
          "original_form": "응 가축조류들어가",
          "speaker_id": "2",
          "time": "20210906 00:37"
        },
        {
          "id": "MDRW2100032828.1.8",
          "form": "식재료에 쓰이는 조류가 흔하게 키우는 거니?",
          "original_form": "식재료에 쓰이는 조류가 흔하게 키우는 거니?",
          "speaker_id": "1",
          "time": "20210906 00:37"
        },
        {
          "id": "MDRW2100032828.1.9",
          "form": "시골은 다이나믹하구나",
          "original_form": "시골은 다이나믹하구나",
          "speaker_id": "2",
          "time": "20210906 00:37"
        },
        {
          "id": "MDRW2100032828.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅇ그러게",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅇ그러게",
          "speaker_id": "1",
          "time": "20210906 00:37"
        },
        {
          "id": "MDRW2100032828.1.11",
          "form": "응 흔하게 키워",
          "original_form": "응 흔하게 키워",
          "speaker_id": "2",
          "time": "20210906 00:37"
        },
        {
          "id": "MDRW2100032828.1.12",
          "form": "매우 흔할듯",
          "original_form": "매우 흔할듯",
          "speaker_id": "2",
          "time": "20210906 00:37"
        },
        {
          "id": "MDRW2100032828.1.13",
          "form": "칠면조 고기가 들어가나보군!",
          "original_form": "칠면조 고기가 들어가나보군!",
          "speaker_id": "1",
          "time": "20210906 00:37"
        },
        {
          "id": "MDRW2100032828.1.14",
          "form": "맞지?",
          "original_form": "맞지?",
          "speaker_id": "1",
          "time": "20210906 00:37"
        },
        {
          "id": "MDRW2100032828.1.15",
          "form": "아니..., ,.",
          "original_form": "아니..., ,.",
          "speaker_id": "2",
          "time": "20210906 00:38"
        },
        {
          "id": "MDRW2100032828.1.16",
          "form": "일부러 틀리는거니",
          "original_form": "일부러 틀리는거니",
          "speaker_id": "2",
          "time": "20210906 00:38"
        },
        {
          "id": "MDRW2100032828.1.17",
          "form": "알겠다 닭고기 들어가는 요리니",
          "original_form": "알겠다 닭고기 들어가는 요리니",
          "speaker_id": "1",
          "time": "20210906 00:38"
        },
        {
          "id": "MDRW2100032828.1.18",
          "form": "응 맞아",
          "original_form": "응 맞아",
          "speaker_id": "2",
