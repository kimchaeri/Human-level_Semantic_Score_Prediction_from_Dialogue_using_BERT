        {
          "id": "MDRW2100045549.1.1",
          "form": "이번엔 뭘로 해볼까요?",
          "original_form": "이번엔 뭘로 해볼까요?",
          "speaker_id": "2",
          "time": "20210908 03:23"
        },
        {
          "id": "MDRW2100045549.1.2",
          "form": "과일 맞추기 어때요?",
          "original_form": "과일 맞추기 어때요?",
          "speaker_id": "1",
          "time": "20210908 03:23"
        },
        {
          "id": "MDRW2100045549.1.3",
          "form": "좋아요! 최근에 먹은걸로!!",
          "original_form": "좋아요! 최근에 먹은걸로!!",
          "speaker_id": "2",
          "time": "20210908 03:25"
        },
        {
          "id": "MDRW2100045549.1.4",
          "form": "앗 좋아요",
          "original_form": "앗 좋아요",
          "speaker_id": "1",
          "time": "20210908 03:26"
        },
        {
          "id": "MDRW2100045549.1.5",
          "form": "제가 먼저 맞춰볼까요?",
          "original_form": "제가 먼저 맞춰볼까요?",
          "speaker_id": "1",
