        {
          "id": "MDRW2100002618.1.1",
          "form": "여행좋아하시나요?",
          "original_form": "여행좋아하시나요?",
          "speaker_id": "2",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002618.1.2",
          "form": "좋죠ㅋㅋㅋ",
          "original_form": "좋죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002618.1.3",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002618.1.4",
          "form": "네 근데 저는 많이 걷는 여행말고ㅋㅋ. 힐링 위주요ㅋ",
          "original_form": "네 근데 저는 많이 걷는 여행말고ㅋㅋ. 힐링 위주요ㅋ",
          "speaker_id": "2",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002618.1.5",
          "form": "오 저랑같네요",
          "original_form": "오 저랑같네요",
          "speaker_id": "1",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002618.1.6",
          "form": "스파나 온천 이런거너무가고싶어요",
          "original_form": "스파나 온천 이런거너무가고싶어요",
          "speaker_id": "1",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002618.1.7",
          "form": "그쵸ㅋㅋ저도 그런거 좋아해요 호캉스",
          "original_form": "그쵸ㅋㅋ저도 그런거 좋아해요 호캉스",
          "speaker_id": "2",
          "time": "20210820 14:18"
        },
        {
          "id": "MDRW2100002618.1.8",
          "form": "아 저도요 진짜",
          "original_form": "아 저도요 진짜",
          "speaker_id": "1",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002618.1.9",
          "form": "호캉스가짱인거같아요!!",
          "original_form": "호캉스가짱인거같아요!!",
          "speaker_id": "1",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002618.1.10",
          "form": "ㅋㅋ여행은 쉬는거죠ㅋㅋ근데 애랑가니 더 피곤이 쌓이는 불편한 사실",
          "original_form": "ㅋㅋ여행은 쉬는거죠ㅋㅋ근데 애랑가니 더 피곤이 쌓이는 불편한 사실",
          "speaker_id": "2",
          "time": "20210820 14:27"
        },
        {
          "id": "MDRW2100002618.1.11",
          "form": "아 아이랑 가면아무래도그러겠어요ㅠ",
          "original_form": "아 아이랑 가면아무래도그러겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002618.1.12",
          "form": "그래도일찍키워놓으면 좋을것같아요!!!",
          "original_form": "그래도일찍키워놓으면 좋을것같아요!!!",
          "speaker_id": "1",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002618.1.13",
          "form": "ㅠㅠ네 아직은 어린애도 있어서 좀 그런데 크면 나아지겠죠ㅡ",
          "original_form": "ㅠㅠ네 아직은 어린애도 있어서 좀 그런데 크면 나아지겠죠ㅡ",
          "speaker_id": "2",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002618.1.14",
          "form": "저는 아직미혼이라",
          "original_form": "저는 아직미혼이라",
          "speaker_id": "1",
          "time": "20210820 14:38"
        },
        {
          "id": "MDRW2100002618.1.15",
          "form": "부럽네요ㅠㅠ",
          "original_form": "부럽네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 14:38"
        },
        {
          "id": "MDRW2100002618.1.16",
          "form": "여름휴가는. 다녀오셨어요?",
          "original_form": "여름휴가는. 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210820 14:51"
        },
        {
          "id": "MDRW2100002618.1.17",
          "form": "아니요ㅠㅠ",
          "original_form": "아니요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002618.1.18",
          "form": "다녀오셨나요?",
          "original_form": "다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002618.1.19",
          "form": "아니요ㅠ저도 아직 못 갔어요ㅠㅠ",
