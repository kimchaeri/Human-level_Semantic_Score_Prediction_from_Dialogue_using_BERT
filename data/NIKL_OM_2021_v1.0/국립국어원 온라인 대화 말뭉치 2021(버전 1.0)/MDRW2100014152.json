      "utterance": [
        {
          "id": "MDRW2100014152.1.1",
          "form": "저 어릴때 교회가서 많이 먹었어요",
          "original_form": "저 어릴때 교회가서 많이 먹었어요",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014152.1.2",
          "form": "전 교회에서 빅파이",
          "original_form": "전 교회에서 빅파이",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014152.1.3",
          "form": "많이 먹엇어요",
          "original_form": "많이 먹엇어요",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014152.1.4",
          "form": "치킨",
          "original_form": "치킨",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014152.1.5",
          "form": "피자",
          "original_form": "피자",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014152.1.6",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014152.1.7",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014152.1.8",
          "form": "장난아니잖아",
          "original_form": "장난아니잖아",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014152.1.9",
          "form": "그 교ㅚ 돈이많아서",
          "original_form": "그 교ㅚ 돈이많아서",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014152.1.10",
          "form": "와 완전 부러워",
          "original_form": "와 완전 부러워",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014152.1.11",
          "form": "맨날 끝나면 치킨이랑피자 시켜줬어요",
          "original_form": "맨날 끝나면 치킨이랑피자 시켜줬어요",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014152.1.12",
          "form": "생각해보면 저희교회는",
          "original_form": "생각해보면 저희교회는",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014152.1.13",
          "form": "코묻은 애기들 돈 뺏어서",
          "original_form": "코묻은 애기들 돈 뺏어서",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014152.1.14",
          "form": "윗층 라운지에 카페도있고",
          "original_form": "윗층 라운지에 카페도있고",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014152.1.15",
          "form": "신축교회 지엇어요",
          "original_form": "신축교회 지엇어요",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014152.1.16",
          "form": "그건 교회공통인가봐요",
          "original_form": "그건 교회공통인가봐요",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014152.1.17",
          "form": "간식 짜잘하게 받고",
          "original_form": "간식 짜잘하게 받고",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014152.1.18",
          "form": "돼지저금통 내놓기",
          "original_form": "돼지저금통 내놓기",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014152.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014152.1.20",
          "form": "기독교의 인생모토죠",
          "original_form": "기독교의 인생모토죠",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014152.1.21",
          "form": "뭣모르는 애기들한테 헌금받기",
          "original_form": "뭣모르는 애기들한테 헌금받기",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014152.1.22",
          "form": "저금통 통째로..",
          "original_form": "저금통 통째로..",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014152.1.23",
          "form": "왜 그 교회에서 여름캠프??",
          "original_form": "왜 그 교회에서 여름캠프??",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014152.1.24",
          "form": "그거가면",
          "original_form": "그거가면",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014152.1.25",
          "form": "밤에 무슨영화보여주잖아요",
          "original_form": "밤에 무슨영화보여주잖아요",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014152.1.26",
          "form": "악 완전 싫어",
          "original_form": "악 완전 싫어",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014152.1.27",
          "form": "엥??",
          "original_form": "엥??",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014152.1.28",
          "form": "이게 지옥의 모습이라면서",
          "original_form": "이게 지옥의 모습이라면서",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014152.1.29",
          "form": "저희는 광기의 기도쑈",
          "original_form": "저희는 광기의 기도쑈",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014152.1.30",
          "form": "했는데요",
          "original_form": "했는데요",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014152.1.31",
          "form": "사람들눈입으로",
          "original_form": "사람들눈입으로",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014152.1.32",
          "form": "벌레들어갔다나왔다",
          "original_form": "벌레들어갔다나왔다",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014152.1.33",
          "form": "성경 구절 못 외우면 밥도 안줫어요",
          "original_form": "성경 구절 못 외우면 밥도 안줫어요",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014152.1.34",
          "form": "으악",
          "original_form": "으악",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014152.1.35",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014152.1.36",
          "form": "도른",
          "original_form": "도른",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014152.1.37",
          "form": "그거보여주고",
          "original_form": "그거보여주고",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014152.1.38",
          "form": "지옥안가려면 기도해야겠죠?",
          "original_form": "지옥안가려면 기도해야겠죠?",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014152.1.39",
          "form": "이러는거",
          "original_form": "이러는거",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014152.1.40",
          "form": "공포정치",
          "original_form": "공포정치",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014152.1.41",
          "form": "이런말 해도 되나요",
          "original_form": "이런말 해도 되나요",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014152.1.42",
          "form": "종교는 정신병이다!!!",
          "original_form": "종교는 정신병이다!!!",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014152.1.43",
          "form": "하지만 어릴적부터 기묘했던...저는....",
          "original_form": "하지만 어릴적부터 기묘했던...저는....",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014152.1.44",
          "form": "스톡홀름 증후군!!!!",
          "original_form": "스톡홀름 증후군!!!!",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014152.1.45",
          "form": "그걸보고...꽤나...",
          "original_form": "그걸보고...꽤나...",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014152.1.46",
          "form": "지옥을 원했군요",
          "original_form": "지옥을 원했군요",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014152.1.47",
          "form": "오~",
          "original_form": "오~",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014152.1.48",
          "form": "사탄교 입단할 만 했네요",
          "original_form": "사탄교 입단할 만 했네요",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014152.1.49",
          "form": "그렇게됐던거죠...",
          "original_form": "그렇게됐던거죠...",
          "speaker_id": "2",
          "time": "20210825 17:34"
