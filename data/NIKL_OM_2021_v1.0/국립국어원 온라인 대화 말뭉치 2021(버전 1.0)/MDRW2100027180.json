      "utterance": [
        {
          "id": "MDRW2100027180.1.1",
          "form": "에일리팬들이",
          "original_form": "에일리팬들이",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027180.1.2",
          "form": "넹 휜이사진뜨고",
          "original_form": "넹 휜이사진뜨고",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027180.1.3",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027180.1.4",
          "form": "왜에일리때는이런거없냐고했었단말임",
          "original_form": "왜에일리때는이런거없냐고했었단말임",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027180.1.5",
          "form": "그럴만하지",
          "original_form": "그럴만하지",
          "speaker_id": "1",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100027180.1.6",
          "form": "근데바로에일리가라사장이찍자했었는데우린앨범곧나올거라거절했다고",
          "original_form": "근데바로에일리가라사장이찍자했었는데우린앨범곧나올거라거절했다고",
          "speaker_id": "2",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100027180.1.7",
          "form": "가수가둘이고에일리니 최근이니",
          "original_form": "가수가둘이고에일리니 최근이니",
          "speaker_id": "1",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100027180.1.8",
          "form": "ㅋㅋㅋㄱㄱㅋ아하",
          "original_form": "ㅋㅋㅋㄱㄱㅋ아하",
          "speaker_id": "1",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100027180.1.9",
          "form": "바로얘기하더라고",
          "original_form": "바로얘기하더라고",
          "speaker_id": "2",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100027180.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100027180.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㄱ일잘하네 라사장",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱ일잘하네 라사장",
          "speaker_id": "1",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100027180.1.12",
          "form": "아무래도에일리가트위치하고있어서바로얘기봤나봄",
          "original_form": "아무래도에일리가트위치하고있어서바로얘기봤나봄",
          "speaker_id": "2",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100027180.1.13",
          "form": "아니 활발한활동만 부타캐여",
          "original_form": "아니 활발한활동만 부타캐여",
          "speaker_id": "1",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100027180.1.14",
