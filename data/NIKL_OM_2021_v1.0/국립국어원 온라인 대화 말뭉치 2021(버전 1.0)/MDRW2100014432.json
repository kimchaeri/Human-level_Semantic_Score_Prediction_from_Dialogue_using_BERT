      "utterance": [
        {
          "id": "MDRW2100014432.1.1",
          "form": "연애/결혼관이 궁금해요~!",
          "original_form": "연애/결혼관이 궁금해요~!",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014432.1.2",
          "form": "당신은 어떤기준이 있어요?",
          "original_form": "당신은 어떤기준이 있어요?",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014432.1.3",
          "form": "음 내년말쯤 결혼예정인데\r\n제일 우선순위는 인성이 중요해요~!",
          "original_form": "음 내년말쯤 결혼예정인데\r\n제일 우선순위는 인성이 중요해요~!",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014432.1.4",
          "form": "결혼예정이신 상대가 있으신거예요?",
          "original_form": "결혼예정이신 상대가 있으신거예요?",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014432.1.5",
          "form": "네 5년째 만나고있어요.",
          "original_form": "네 5년째 만나고있어요.",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014432.1.6",
          "form": "5년이면 상대방에 대해 많이 아시겠네요",
          "original_form": "5년이면 상대방에 대해 많이 아시겠네요",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014432.1.7",
          "form": "어디서 만나셨어요?",
          "original_form": "어디서 만나셨어요?",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014432.1.8",
          "form": "소개로만났는데\r\n가족보다 더 가족같은 사이죠",
          "original_form": "소개로만났는데\r\n가족보다 더 가족같은 사이죠",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014432.1.9",
          "form": "정말좋습니다 ㅎㅎ",
          "original_form": "정말좋습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014432.1.10",
          "form": "비혼주의로살다가 결혼을 결심하게됏어요",
          "original_form": "비혼주의로살다가 결혼을 결심하게됏어요",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014432.1.11",
          "form": "내년에는 문서로도 가족되시겠네요",
          "original_form": "내년에는 문서로도 가족되시겠네요",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014432.1.12",
          "form": "비혼주의셨으면 결혼 생각하시기 쉽지않으셨겠어요",
          "original_form": "비혼주의셨으면 결혼 생각하시기 쉽지않으셨겠어요",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014432.1.13",
          "form": "두분 다 비혼주의셨어요?",
          "original_form": "두분 다 비혼주의셨어요?",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014432.1.14",
          "form": "저만!",
          "original_form": "저만!",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014432.1.15",
          "form": "처음부터 좋은사람인줄알았는데",
          "original_form": "처음부터 좋은사람인줄알았는데",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014432.1.16",
          "form": "오랜시간지켜보니",
          "original_form": "오랜시간지켜보니",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014432.1.17",
          "form": "정말 좋은사람이더라구요",
          "original_form": "정말 좋은사람이더라구요",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014432.1.18",
          "form": "저한테 잘맞는!",
          "original_form": "저한테 잘맞는!",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014432.1.19",
          "form": "축하해요~",
          "original_form": "축하해요~",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014432.1.20",
          "form": "주관을 변경할 정도로 좋은 사람을 만나신걸요ㅎ",
          "original_form": "주관을 변경할 정도로 좋은 사람을 만나신걸요ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014432.1.21",
          "form": "뭐든 장단점이 잇을테니",
          "original_form": "뭐든 장단점이 잇을테니",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014432.1.22",
          "form": "좋은것만가질수없고",
          "original_form": "좋은것만가질수없고",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014432.1.23",
          "form": "내려놓고",
          "original_form": "내려놓고",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014432.1.24",
          "form": "비우는중이에요 ㅎㅎ",
          "original_form": "비우는중이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014432.1.25",
          "form": "연애중이신가요?",
          "original_form": "연애중이신가요?",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014432.1.26",
          "form": "아니요ㅎㅎ",
          "original_form": "아니요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014432.1.27",
          "form": "사람을 잘 못 믿어요",
          "original_form": "사람을 잘 못 믿어요",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014432.1.28",
          "form": "그렇다고 제가 가면?쓰고 행동도 잘 못 하고요",
          "original_form": "그렇다고 제가 가면?쓰고 행동도 잘 못 하고요",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014432.1.29",
          "form": "그러다보니 혼자가 자연스럽고 편해진거같아요",
          "original_form": "그러다보니 혼자가 자연스럽고 편해진거같아요",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014432.1.30",
          "form": "오 저도 완전",
          "original_form": "오 저도 완전",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014432.1.31",
          "form": "그랬어요",
          "original_form": "그랬어요",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014432.1.32",
          "form": "낯가림진짜심하고",
          "original_form": "낯가림진짜심하고",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014432.1.33",
          "form": "집순이에",
          "original_form": "집순이에",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014432.1.34",
          "form": "저는 귀차니즘도 엄청 심해요....",
          "original_form": "저는 귀차니즘도 엄청 심해요....",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014432.1.35",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014432.1.36",
          "form": "집순잏",
          "original_form": "집순잏",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014432.1.37",
          "form": "저도 낯가림 심해요",
          "original_form": "저도 낯가림 심해요",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014432.1.38",
          "form": "상대방은 잘 모른다는데\r\n전 상대방 눈을 잘 못봐요ㅎㅎ",
          "original_form": "상대방은 잘 모른다는데\r\n전 상대방 눈을 잘 못봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014432.1.39",
          "form": "그냥 얼굴 언저리 보고 얘기해요",
          "original_form": "그냥 얼굴 언저리 보고 얘기해요",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014432.1.40",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014432.1.41",
          "form": "저는 진짜 만나는사람만 보고",
          "original_form": "저는 진짜 만나는사람만 보고",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014432.1.42",
          "form": "그마저도",
          "original_form": "그마저도",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014432.1.43",
          "form": "코시국이라",
          "original_form": "코시국이라",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014432.1.44",
          "form": "더 나갈일도없고",
          "original_form": "더 나갈일도없고",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014432.1.45",
          "form": "ㅎㅎㅎ 저두요",
          "original_form": "ㅎㅎㅎ 저두요",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014432.1.46",
          "form": "ㅜㅜ근데 또 이렇게 가만히있는게",
          "original_form": "ㅜㅜ근데 또 이렇게 가만히있는게",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014432.1.47",
          "form": "너무 좋고",
          "original_form": "너무 좋고",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014432.1.48",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014432.1.49",
          "form": "새로운사람 만나기더 힘들어지긴했어요",
          "original_form": "새로운사람 만나기더 힘들어지긴했어요",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014432.1.50",
          "form": "방구석집수니삶이 좋은데\r\n또 너무 가마니있나 싶기도하고",
          "original_form": "방구석집수니삶이 좋은데\r\n또 너무 가마니있나 싶기도하고",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014432.1.51",
          "form": "코시국으로 회식 안해서 좋아요ㅋㅋ",
          "original_form": "코시국으로 회식 안해서 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014432.1.52",
          "form": "와..",
          "original_form": "와..",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014432.1.53",
          "form": "유일한 최고 장점",
          "original_form": "유일한 최고 장점",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014432.1.54",
          "form": "회식 진짜 진짜 너무 싫었어요ㅠㅠ",
          "original_form": "회식 진짜 진짜 너무 싫었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014432.1.55",
          "form": "ㅋㅋㅋ그쵸ㅋ",
          "original_form": "ㅋㅋㅋ그쵸ㅋ",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014432.1.56",
          "form": "이렇게 채팅하고있는 지금도 엄청",
          "original_form": "이렇게 채팅하고있는 지금도 엄청",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014432.1.57",
          "form": "신기방기",
          "original_form": "신기방기",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014432.1.58",
          "form": "원치않고 돈도 더 안주면서 업무의 연장이라는ㅋ",
          "original_form": "원치않고 돈도 더 안주면서 업무의 연장이라는ㅋ",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014432.1.59",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014432.1.60",
          "form": "모르는사람이랑 연애랑결혼에대해서 떠드는게",
          "original_form": "모르는사람이랑 연애랑결혼에대해서 떠드는게",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014432.1.61",
          "form": "엄청나게ㅋㅋ신기해욤",
          "original_form": "엄청나게ㅋㅋ신기해욤",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014432.1.62",
          "form": "이렇게 전혀 모르는 분이랑 대화 첨해봐요",
          "original_form": "이렇게 전혀 모르는 분이랑 대화 첨해봐요",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014432.1.63",
          "form": "인터넷서 보면 무작위어플로 결혼하시는 분도 있던데",
          "original_form": "인터넷서 보면 무작위어플로 결혼하시는 분도 있던데",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014432.1.64",
          "form": "어떻게 만나고 결혼하는건가 이해 안됐었는데",
          "original_form": "어떻게 만나고 결혼하는건가 이해 안됐었는데",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
