      "utterance": [
        {
          "id": "MDRW2100020990.1.1",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "2",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100020990.1.2",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "2",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100020990.1.3",
          "form": "진짜 신라면맛 나는데",
          "original_form": "진짜 신라면맛 나는데",
          "speaker_id": "1",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100020990.1.4",
          "form": "조금 더 단맛이 섞인느낌",
          "original_form": "조금 더 단맛이 섞인느낌",
          "speaker_id": "1",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100020990.1.5",
          "form": "오 그래요?",
          "original_form": "오 그래요?",
          "speaker_id": "2",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100020990.1.6",
          "form": "근데 좀 매워요",
          "original_form": "근데 좀 매워요",
          "speaker_id": "1",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100020990.1.7",
          "form": "단맛은 의외네요",
          "original_form": "단맛은 의외네요",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020990.1.8",
          "form": "맵단맵단인가봐요 ㅋㅋㅋ",
          "original_form": "맵단맵단인가봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020990.1.9",
          "form": "불닭보다는",
          "original_form": "불닭보다는",
          "speaker_id": "1",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020990.1.10",
          "form": "좀 덜매운데",
          "original_form": "좀 덜매운데",
          "speaker_id": "1",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020990.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020990.1.12",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020990.1.13",
          "form": "그래도 매워요",
          "original_form": "그래도 매워요",
          "speaker_id": "1",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020990.1.14",
          "form": "적당히매운가봐요",
          "original_form": "적당히매운가봐요",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020990.1.15",
          "form": "다음에는 치즈넣어 먹으려구요",
          "original_form": "다음에는 치즈넣어 먹으려구요",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020990.1.16",
          "form": "오 그럼 더 맛나겠어요",
          "original_form": "오 그럼 더 맛나겠어요",
          "speaker_id": "2",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020990.1.17",
          "form": "역시 매울때는 치즈죠 ㅋㅋㄱ",
          "original_form": "역시 매울때는 치즈죠 ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020990.1.18",
          "form": "매운거랑 치즈는 찰떡궁합이죠",
          "original_form": "매운거랑 치즈는 찰떡궁합이죠",
          "speaker_id": "2",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020990.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020990.1.20",
          "form": "볶음밥할때도 좋죠",
          "original_form": "볶음밥할때도 좋죠",
