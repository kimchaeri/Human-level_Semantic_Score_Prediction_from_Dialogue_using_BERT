      "utterance": [
        {
          "id": "MDRW2100026848.1.1",
          "form": "ㅎㅎ 암튼 남들보는게 뭐가 중요하다고ㅎ",
          "original_form": "ㅎㅎ 암튼 남들보는게 뭐가 중요하다고ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026848.1.2",
          "form": "내가족이 잘살면되는거지ㅎ",
          "original_form": "내가족이 잘살면되는거지ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026848.1.3",
          "form": "너무 너므 힘들다 해서 돈좀 빌려줬는데 ㅎㅎㅎ",
          "original_form": "너무 너므 힘들다 해서 돈좀 빌려줬는데 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026848.1.4",
          "form": "아아ㅎㅎ",
          "original_form": "아아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026848.1.5",
          "form": "그런데요?",
          "original_form": "그런데요?",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026848.1.6",
          "form": "다시 사정 좋아졌는데",
          "original_form": "다시 사정 좋아졌는데",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026848.1.7",
          "form": "ㄱ돈갚을 생각이 없음 ㅋ",
          "original_form": "ㄱ돈갚을 생각이 없음 ㅋ",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026848.1.8",
          "form": "그리고 사치 하거 ㅎㅎ",
          "original_form": "그리고 사치 하거 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026848.1.9",
          "form": "ㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026848.1.10",
          "form": "말해보셨어요?",
          "original_form": "말해보셨어요?",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026848.1.11",
          "form": "아아아아 물론  와이프분이랑 같다는건 아닌데",
          "original_form": "아아아아 물론  와이프분이랑 같다는건 아닌데",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026848.1.12",
          "form": "좀 비슷한 부분이 많아서 생각낫어여",
          "original_form": "좀 비슷한 부분이 많아서 생각낫어여",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026848.1.13",
          "form": "사치를 떠나서ㅎ 계산을하면서 살면좋은데",
          "original_form": "사치를 떠나서ㅎ 계산을하면서 살면좋은데",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026848.1.14",
          "form": "왜 그게 중요시되는지ㅎ 이해를 못하겟아요ㅎㅎ",
          "original_form": "왜 그게 중요시되는지ㅎ 이해를 못하겟아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026848.1.15",
          "form": "음 근데 뭐 ㅎㅎ",
          "original_form": "음 근데 뭐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026848.1.16",
          "form": "사람이니까 다다르니까 글쵸 ㅎㅎ",
          "original_form": "사람이니까 다다르니까 글쵸 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026848.1.17",
          "form": "글져ㅎ 그렇겟죠ㅎㅎ",
          "original_form": "글져ㅎ 그렇겟죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026848.1.18",
          "form": "그렇세 생가해야 ㅎㅎ 스트레스 안바다용",
          "original_form": "그렇세 생가해야 ㅎㅎ 스트레스 안바다용",
          "speaker_id": "1",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026848.1.19",
          "form": "뭐암튼 아내욕은 여기까지해야겟어요ㅎㅎㅎㅎㅎㅎ",
          "original_form": "뭐암튼 아내욕은 여기까지해야겟어요ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026848.1.20",
          "form": "저는 아직더 그던 못받앗지만 참 이런 사람도 잇지 하고 넘겨여 ㅎㅎ",
          "original_form": "저는 아직더 그던 못받앗지만 참 이런 사람도 잇지 하고 넘겨여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026848.1.21",
          "form": "같이 사이좋게 애기를 키워야해서ㅎ",
          "original_form": "같이 사이좋게 애기를 키워야해서ㅎ",
