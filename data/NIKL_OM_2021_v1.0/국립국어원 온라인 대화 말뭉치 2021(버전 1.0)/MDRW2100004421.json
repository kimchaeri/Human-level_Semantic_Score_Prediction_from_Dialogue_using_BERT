      "utterance": [
        {
          "id": "MDRW2100004421.1.1",
          "form": "혹시 반려동물 키우시나요?",
          "original_form": "혹시 반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004421.1.2",
          "form": "저는 안키워요!",
          "original_form": "저는 안키워요!",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004421.1.3",
          "form": "name1님 키우세요?",
          "original_form": "&name1&님 키우세요?",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004421.1.4",
          "form": "엇 저도 안키워욬ㅋㅋㅋ다른 대화주제로 바꾸시죠",
          "original_form": "엇 저도 안키워욬ㅋㅋㅋ다른 대화주제로 바꾸시죠",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004421.1.5",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210821 16:35"
        },
        {
          "id": "MDRW2100004421.1.6",
          "form": "아뇨!! 키우고싶지만ㅠㅠ",
          "original_form": "아뇨!! 키우고싶지만ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004421.1.7",
          "form": "name1님은영?",
          "original_form": "&name1&님은영?",
          "speaker_id": "2",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004421.1.8",
          "form": "저도 안키우고 있어요",
          "original_form": "저도 안키우고 있어요",
          "speaker_id": "1",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004421.1.9",
          "form": "너무 키우고싶지만 알러지땜에ㅜㅜ",
          "original_form": "너무 키우고싶지만 알러지땜에ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004421.1.10",
          "form": "독립하면 키우고싶어요",
          "original_form": "독립하면 키우고싶어요",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004421.1.11",
          "form": "알러지 있으시구나ㅠㅠ",
          "original_form": "알러지 있으시구나ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004421.1.12",
          "form": "가족분이용?",
          "original_form": "가족분이용?",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004421.1.13",
          "form": "네네 그리고 부모님이 어렸을때 동물키웠는데 죽었을때 너무 슬퍼서 못키우시겠대요ㅠㅠ",
          "original_form": "네네 그리고 부모님이 어렸을때 동물키웠는데 죽었을때 너무 슬퍼서 못키우시겠대요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004421.1.14",
          "form": "반려동물 이름도 기억하시고 계세요ㅜㅜ 참 그런거보면 반려동물을 입양하는건 신중해야해요ㅠㅠ",
          "original_form": "반려동물 이름도 기억하시고 계세요ㅜㅜ 참 그런거보면 반려동물을 입양하는건 신중해야해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004421.1.15",
          "form": "아 헐ㅠㅠㅠㅡ맞아요",
          "original_form": "아 헐ㅠㅠㅠㅡ맞아요",
          "speaker_id": "2",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004421.1.16",
          "form": "저도 햄스터 키웠었는데",
          "original_form": "저도 햄스터 키웠었는데",
          "speaker_id": "2",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004421.1.17",
          "form": "2일동안 내내 울었어요ㅠㅜ",
          "original_form": "2일동안 내내 울었어요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004421.1.18",
          "form": "에구ㅜㅜ",
          "original_form": "에구ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004421.1.19",
          "form": "아 저도 키웠다면 키운걸까 싶지만 금붕엌ㅋㅋ키웠었어요",
          "original_form": "아 저도 키웠다면 키운걸까 싶지만 금붕엌ㅋㅋ키웠었어요",
          "speaker_id": "1",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004421.1.20",
          "form": "어릴때라 금붕어 죽으면 둥둥뜨잖아요",
          "original_form": "어릴때라 금붕어 죽으면 둥둥뜨잖아요",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004421.1.21",
          "form": "근데 그 모습이 너무 무서워서 처리못하면 엄마가 대신 버리고(?) 그랬던 기억이 있네요",
          "original_form": "근데 그 모습이 너무 무서워서 처리못하면 엄마가 대신 버리고(?) 그랬던 기억이 있네요",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004421.1.22",
          "form": "헉ㅠㅠㅠㅠ 그죠 무서워요 죽은거 보는게..,",
          "original_form": "헉ㅠㅠㅠㅠ 그죠 무서워요 죽은거 보는게..,",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004421.1.23",
          "form": "맞아요ㅜㅜ 꿈에 나올거같고",
          "original_form": "맞아요ㅜㅜ 꿈에 나올거같고",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004421.1.24",
          "form": "그런가보면 지금은 키우고싶어도 나중에는 결국 안키울거같기도 해요",
          "original_form": "그런가보면 지금은 키우고싶어도 나중에는 결국 안키울거같기도 해요",
          "speaker_id": "1",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004421.1.25",
          "form": "동물 키우는 비용도 만만치않고요..",
          "original_form": "동물 키우는 비용도 만만치않고요..",
          "speaker_id": "1",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004421.1.26",
          "form": "맞아요 여건이 안되면 동물한테도 미안하고..쪼들리고..",
          "original_form": "맞아요 여건이 안되면 동물한테도 미안하고..쪼들리고..",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004421.1.27",
          "form": "그니까여 공간이 너무 좁아도 동물학대인거같고.. 좋은 사료 먹이고 싶는데 못그러면 너무 미안할거같아요",
          "original_form": "그니까여 공간이 너무 좁아도 동물학대인거같고.. 좋은 사료 먹이고 싶는데 못그러면 너무 미안할거같아요",
          "speaker_id": "1",
          "time": "20210821 17:17"
        }
      ]
