      "utterance": [
        {
          "id": "MDRW2100038044.1.1",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100038044.1.2",
          "form": "나는 솔로",
          "original_form": "나는 솔로",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100038044.1.3",
          "form": "이런 거 보세요?",
          "original_form": "이런 거 보세요?",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100038044.1.4",
          "form": "영상클립 뜨는거 본 적 있어요",
          "original_form": "영상클립 뜨는거 본 적 있어요",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038044.1.5",
          "form": "그거 보시나요?",
          "original_form": "그거 보시나요?",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038044.1.6",
          "form": "네 지금 시즌2 첫방이라",
          "original_form": "네 지금 시즌2 첫방이라",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100038044.1.7",
          "form": "보고있어요ㅋㅋㅋㅋㅋ",
          "original_form": "보고있어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100038044.1.8",
          "form": "재밌나봐요ㅋㅋㅋㅋ",
          "original_form": "재밌나봐요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100038044.1.9",
          "form": "네ㅋㅋ 근데 1이 더 재밌어요",
          "original_form": "네ㅋㅋ 근데 1이 더 재밌어요",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100038044.1.10",
          "form": "보시는 거 있으세요?",
          "original_form": "보시는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100038044.1.11",
          "form": "전 유퀴즈 봐요",
          "original_form": "전 유퀴즈 봐요",
          "speaker_id": "1",
          "time": "20210902 08:16"
        },
        {
          "id": "MDRW2100038044.1.12",
          "form": "유퀴즈 무슨 편",
          "original_form": "유퀴즈 무슨 편",
          "speaker_id": "2",
          "time": "20210902 08:25"
        },
        {
          "id": "MDRW2100038044.1.13",
          "form": "좋아하세요",
          "original_form": "좋아하세요",
          "speaker_id": "2",
          "time": "20210902 08:25"
        },
        {
          "id": "MDRW2100038044.1.14",
          "form": "어제 방송도 재밌었어요",
          "original_form": "어제 방송도 재밌었어요",
          "speaker_id": "1",
          "time": "20210902 08:37"
        },
        {
          "id": "MDRW2100038044.1.15",
          "form": "몇주간 올림픽 선수들 나왔는데 다들 재밌더라구요",
          "original_form": "몇주간 올림픽 선수들 나왔는데 다들 재밌더라구요",
          "speaker_id": "1",
          "time": "20210902 08:38"
        },
        {
          "id": "MDRW2100038044.1.16",
          "form": "ㅋㅋ 아 맞아요",
          "original_form": "ㅋㅋ 아 맞아요",
          "speaker_id": "2",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100038044.1.17",
          "form": "어젠 뭐였나요?",
          "original_form": "어젠 뭐였나요?",
          "speaker_id": "2",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100038044.1.18",
          "form": "소녀시대???!",
          "original_form": "소녀시대???!",
          "speaker_id": "2",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100038044.1.19",
          "form": "네 소녀시대 완전체로 나왔어요",
          "original_form": "네 소녀시대 완전체로 나왔어요",
          "speaker_id": "1",
          "time": "20210902 08:53"
        },
        {
          "id": "MDRW2100038044.1.20",
          "form": "많이 재밌었나요?",
          "original_form": "많이 재밌었나요?",
          "speaker_id": "2",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100038044.1.21",
          "form": "까먹고 못봤네여",
          "original_form": "까먹고 못봤네여",
          "speaker_id": "2",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100038044.1.22",
          "form": "네 재밌었어요",
          "original_form": "네 재밌었어요",
          "speaker_id": "1",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100038044.1.23",
          "form": "키싱유도 잠깐 불러주고ㅋㅋㅋ 추억돋았네요",
          "original_form": "키싱유도 잠깐 불러주고ㅋㅋㅋ 추억돋았네요",
          "speaker_id": "1",
