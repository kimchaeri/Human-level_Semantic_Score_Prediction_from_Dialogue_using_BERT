      "utterance": [
        {
          "id": "MDRW2100034886.1.1",
          "form": "근데 맛있는 거 먹고 싶긴 하다",
          "original_form": "근데 맛있는 거 먹고 싶긴 하다",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034886.1.2",
          "form": "더 늦기 전에 시켜 먹자!",
          "original_form": "더 늦기 전에 시켜 먹자!",
          "speaker_id": "2",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100034886.1.3",
          "form": "하ㅜ 배달음싣 돈 아까워서 끊어야 되는디",
          "original_form": "하ㅜ 배달음싣 돈 아까워서 끊어야 되는디",
          "speaker_id": "1",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100034886.1.4",
          "form": "맨날 못 끊고 중독됨",
          "original_form": "맨날 못 끊고 중독됨",
          "speaker_id": "1",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100034886.1.5",
          "form": "맞아...",
          "original_form": "맞아...",
          "speaker_id": "2",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100034886.1.6",
          "form": "배민에 소소떡볶이라고",
          "original_form": "배민에 소소떡볶이라고",
          "speaker_id": "1",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100034886.1.7",
          "form": "그래서 최대한 쿠폰 써서 시켜먹는중",
          "original_form": "그래서 최대한 쿠폰 써서 시켜먹는중",
          "speaker_id": "2",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100034886.1.8",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "2",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100034886.1.9",
          "form": "가게 있는데 떡볶이 맛있더라",
          "original_form": "가게 있는데 떡볶이 맛있더라",
          "speaker_id": "1",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100034886.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034886.1.11",
          "form": "나도 쿠폰 열심히 쓰는 중....",
          "original_form": "나도 쿠폰 열심히 쓰는 중....",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034886.1.12",
          "form": "맨날 주문하기 전에 배달앱만 한시간은 보는 듯",
          "original_form": "맨날 주문하기 전에 배달앱만 한시간은 보는 듯",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034886.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034886.1.14",
          "form": "어쩔 수 없어..~",
          "original_form": "어쩔 수 없어..~",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034886.1.15",
          "form": "요기요도 할인 많이 한다던데",
          "original_form": "요기요도 할인 많이 한다던데",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034886.1.16",
          "form": "점심 할인?은 하는데",
          "original_form": "점심 할인?은 하는데",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034886.1.17",
          "form": "다시 배민 쓰게 되더라",
          "original_form": "다시 배민 쓰게 되더라",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034886.1.18",
          "form": "갠적으로 그랬어",
          "original_form": "갠적으로 그랬어",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034886.1.19",
          "form": "아하 글쿠만",
          "original_form": "아하 글쿠만",
          "speaker_id": "2",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100034886.1.20",
          "form": "배민이 좀 더 편하긴 해",
          "original_form": "배민이 좀 더 편하긴 해",
          "speaker_id": "2",
          "time": "20210828 21:37"
        },
        {
