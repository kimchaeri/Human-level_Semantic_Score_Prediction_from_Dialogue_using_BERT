      "utterance": [
        {
          "id": "MDRW2100013298.1.1",
          "form": "오늘 날씨 어때요?",
          "original_form": "오늘 날씨 어때요?",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013298.1.2",
          "form": "좋나요?",
          "original_form": "좋나요?",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013298.1.3",
          "form": "제가 있는 곳의 날씨는 우중충해서 괜히 외롭네요",
          "original_form": "제가 있는 곳의 날씨는 우중충해서 괜히 외롭네요",
          "speaker_id": "1",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100013298.1.4",
          "form": "날씨때문에 외로워지는 느낌..",
          "original_form": "날씨때문에 외로워지는 느낌..",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013298.1.5",
          "form": "여기도 우중충하네요",
          "original_form": "여기도 우중충하네요",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013298.1.6",
          "form": "더그래요",
          "original_form": "더그래요",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013298.1.7",
          "form": "어제 비가많이와서 우산을가져갔더니",
          "original_form": "어제 비가많이와서 우산을가져갔더니",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013298.1.8",
          "form": "예",
          "original_form": "예",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013298.1.9",
          "form": "공기에서 좋은 냄새가 나네요",
          "original_form": "공기에서 좋은 냄새가 나네요",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013298.1.10",
          "form": "멈췄다가 또 내리고 멈췄다 또 내리더라고요",
          "original_form": "멈췄다가 또 내리고 멈췄다 또 내리더라고요",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013298.1.11",
          "form": "짐만 되었네요",
          "original_form": "짐만 되었네요",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013298.1.12",
          "form": "그래도 우산 챙기는게 낫지 않나요?",
          "original_form": "그래도 우산 챙기는게 낫지 않나요?",
          "speaker_id": "1",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013298.1.13",
          "form": "요즘 늦장마니까 우산챙기는게 좋긴하죠",
          "original_form": "요즘 늦장마니까 우산챙기는게 좋긴하죠",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013298.1.14",
          "form": "비올때 우산 챙기는건 짐은 아닐거에요^^",
          "original_form": "비올때 우산 챙기는건 짐은 아닐거에요^^",
          "speaker_id": "1",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013298.1.15",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013298.1.16",
          "form": "오늘도 비가 오려나요",
          "original_form": "오늘도 비가 오려나요",
          "speaker_id": "1",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013298.1.17",
          "form": "태풍일까요?",
          "original_form": "태풍일까요?",
          "speaker_id": "1",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013298.1.18",
          "form": "한번 기상청에서 찾아봐야겠네요",
          "original_form": "한번 기상청에서 찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013298.1.19",
          "form": "태풍 조심하세요",
          "original_form": "태풍 조심하세요",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013298.1.20",
          "form": "감사해요",
          "original_form": "감사해요",
