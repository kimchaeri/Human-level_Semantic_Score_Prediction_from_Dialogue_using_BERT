      "utterance": [
        {
          "id": "MDRW2100047362.1.1",
          "form": "ㅋㅋ안녕하세요",
          "original_form": "ㅋㅋ안녕하세요",
          "speaker_id": "2",
          "time": "20210826 13:32"
        },
        {
          "id": "MDRW2100047362.1.2",
          "form": "오늘 점심 뭐 하셨어요?",
          "original_form": "오늘 점심 뭐 하셨어요?",
          "speaker_id": "2",
          "time": "20210826 13:32"
        },
        {
          "id": "MDRW2100047362.1.3",
          "form": "저 아직이요ㅋㅋㅋ",
          "original_form": "저 아직이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100047362.1.4",
          "form": "뭐드셨나요?!",
          "original_form": "뭐드셨나요?!",
          "speaker_id": "1",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100047362.1.5",
          "form": "아 휴 배고프신데",
          "original_form": "아 휴 배고프신데",
          "speaker_id": "2",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100047362.1.6",
          "form": "어째요",
          "original_form": "어째요",
          "speaker_id": "2",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100047362.1.7",
          "form": "전 된장찌개 계란 후라이",
          "original_form": "전 된장찌개 계란 후라이",
          "speaker_id": "2",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100047362.1.8",
          "form": "한식이요",
          "original_form": "한식이요",
          "speaker_id": "2",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100047362.1.9",
          "form": "왜 안드셨어요",
          "original_form": "왜 안드셨어요",
          "speaker_id": "2",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100047362.1.10",
          "form": "이제 먹으려구요ㅋㅋㅋ",
          "original_form": "이제 먹으려구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100047362.1.11",
          "form": "오 최고의 점심이네요",
          "original_form": "오 최고의 점심이네요",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100047362.1.12",
          "form": "네 ㅎㅎ왠지 건강해 졌어요",
          "original_form": "네 ㅎㅎ왠지 건강해 졌어요",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100047362.1.13",
          "form": "뭐 드시려고요?",
          "original_form": "뭐 드시려고요?",
          "speaker_id": "2",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100047362.1.14",
          "form": "저 아직 고민중이랍니다ㅋㅋㅋㅋ",
          "original_form": "저 아직 고민중이랍니다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100047362.1.15",
          "form": "심도있게요",
          "original_form": "심도있게요",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100047362.1.16",
          "form": "그럼요 심도있게 고민 하셔야 해요",
          "original_form": "그럼요 심도있게 고민 하셔야 해요",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100047362.1.17",
          "form": "사람이 먹고사는게 엄청 중요해요",
          "original_form": "사람이 먹고사는게 엄청 중요해요",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100047362.1.18",
          "form": "아 넘 늦음 저녁돼요",
          "original_form": "아 넘 늦음 저녁돼요",
          "speaker_id": "2",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100047362.1.19",
          "form": "어서 고르셔요 ㅎㅎ",
          "original_form": "어서 고르셔요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100047362.1.20",
          "form": "ㅋㅋㅋ저 비빔밥",
          "original_form": "ㅋㅋㅋ저 비빔밥",
          "speaker_id": "1",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100047362.1.21",
          "form": "먹으러구요 나물비빔밥!",
          "original_form": "먹으러구요 나물비빔밥!",
          "speaker_id": "1",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100047362.1.22",
          "form": "오 좋은데요",
          "original_form": "오 좋은데요",
          "speaker_id": "2",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100047362.1.23",
          "form": "오늘은 한식입니다",
          "original_form": "오늘은 한식입니다",
          "speaker_id": "2",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100047362.1.24",
          "form": "고추장에 쓱쓱 잘고르셨어요",
          "original_form": "고추장에 쓱쓱 잘고르셨어요",
          "speaker_id": "2",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100047362.1.25",
          "form": "아 생각하니 침고입니다 역시 고추장은",
          "original_form": "아 생각하니 침고입니다 역시 고추장은",
          "speaker_id": "2",
          "time": "20210826 14:07"
        },
        {
          "id": "MDRW2100047362.1.26",
          "form": "ㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100047362.1.27",
          "form": "참기름 한방울 또옥~",
          "original_form": "참기름 한방울 또옥~",
          "speaker_id": "1",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100047362.1.28",
          "form": "네 낼 해먹어야 겠어요 ㅎㅎ",
          "original_form": "네 낼 해먹어야 겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100047362.1.29",
          "form": "지금은 아들이 치킨 사러 갔어요 ㅋㅋ",
          "original_form": "지금은 아들이 치킨 사러 갔어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100047362.1.30",
          "form": "ㅋㅋㅋㅋ아드님이",
          "original_form": "ㅋㅋㅋㅋ아드님이",
          "speaker_id": "1",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100047362.1.31",
          "form": "아주 열일이시군요?",
          "original_form": "아주 열일이시군요?",
          "speaker_id": "1",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100047362.1.32",
          "form": "갑자기 아들이갖고싶어요!!!!!!",
          "original_form": "갑자기 아들이갖고싶어요!!!!!!",
          "speaker_id": "1",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100047362.1.33",
          "form": "ㅋㅋ사왔어요 ㅋㅋ",
          "original_form": "ㅋㅋ사왔어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100047362.1.34",
          "form": "바삭바삭",
          "original_form": "바삭바삭",
          "speaker_id": "2",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100047362.1.35",
          "form": "먹고있어요",
          "original_form": "먹고있어요",
          "speaker_id": "2",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100047362.1.36",
          "form": "장이 섰거든요 아파트",
          "original_form": "장이 섰거든요 아파트",
          "speaker_id": "2",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100047362.1.37",
          "form": "ㅋㅋ아들 착해요",
          "original_form": "ㅋㅋ아들 착해요",
