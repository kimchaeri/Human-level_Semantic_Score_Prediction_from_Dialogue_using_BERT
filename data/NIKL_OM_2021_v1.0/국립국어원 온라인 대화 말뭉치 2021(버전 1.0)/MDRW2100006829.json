      "utterance": [
        {
          "id": "MDRW2100006829.1.1",
          "form": "저는... 시트콤은 브나나만 봐서 그쪽은 추천할게 없지만",
          "original_form": "저는... 시트콤은 브나나만 봐서 그쪽은 추천할게 없지만",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100006829.1.2",
          "form": "루시퍼랑 엄브렐라 아카데미",
          "original_form": "루시퍼랑 엄브렐라 아카데미",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100006829.1.3",
          "form": "재미있게봤어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "재미있게봤어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100006829.1.4",
          "form": "엄브아카",
          "original_form": "엄브아카",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100006829.1.5",
          "form": "브나나 추천해주신 분한테 추천 받았는데",
          "original_form": "브나나 추천해주신 분한테 추천 받았는데",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100006829.1.6",
          "form": "아직 못봤네요...",
          "original_form": "아직 못봤네요...",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100006829.1.7",
          "form": "ㅋㅋㅋㅋ시간나실때 한 번 보셔요",
          "original_form": "ㅋㅋㅋㅋ시간나실때 한 번 보셔요",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006829.1.8",
          "form": "아직 시즌도 많이 나온거 아니라서",
          "original_form": "아직 시즌도 많이 나온거 아니라서",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006829.1.9",
          "form": "가볍게 볼 수 있어요",
          "original_form": "가볍게 볼 수 있어요",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006829.1.10",
          "form": "넵 ㅋㅋㅋ",
          "original_form": "넵 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006829.1.11",
          "form": "혹시 넷플릭스 말고도",
          "original_form": "혹시 넷플릭스 말고도",
          "speaker_id": "1",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100006829.1.12",
          "form": "사용하는 서비스 있으신가요??",
          "original_form": "사용하는 서비스 있으신가요??",
          "speaker_id": "1",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100006829.1.13",
          "form": "앗 아뇨 넷플릭스만 있어요 ㅋㅋㅋㅋ",
          "original_form": "앗 아뇨 넷플릭스만 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100006829.1.14",
          "form": "아 전 스트리밍 사이트 진짜 많이 이용하거든요...",
          "original_form": "아 전 스트리밍 사이트 진짜 많이 이용하거든요...",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100006829.1.15",
          "form": "혹시 디즈니 플러스 나오면 하실건가요??",
          "original_form": "혹시 디즈니 플러스 나오면 하실건가요??",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100006829.1.16",
          "form": "디즈니 플러스는 조금 고민중이예요 ㅠㅠㅋㅋㅋㅋ",
          "original_form": "디즈니 플러스는 조금 고민중이예요 ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006829.1.17",
          "form": "국내에 들어오는 디즈니 플러스에",
          "original_form": "국내에 들어오는 디즈니 플러스에",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006829.1.18",
          "form": "어떤것들 있는지 좀 파악하고 생각해봐야할거같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "어떤것들 있는지 좀 파악하고 생각해봐야할거같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006829.1.19",
          "form": "사실 스트리밍 사이트 결제만해두고 ㅠㅠ 알차게 이용을 잘 못해서요",
          "original_form": "사실 스트리밍 사이트 결제만해두고 ㅠㅠ 알차게 이용을 잘 못해서요",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006829.1.20",
          "form": "사실 저도 가입은 많이 했는데",
          "original_form": "사실 저도 가입은 많이 했는데",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006829.1.21",
          "form": "쓰는 것만 쓰는 거 같아요",
          "original_form": "쓰는 것만 쓰는 거 같아요",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006829.1.22",
          "form": "혹시 딪플 사용하시면",
          "original_form": "혹시 딪플 사용하시면",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006829.1.23",
          "form": "해밀턴이라고 디즈니 플러스에서 독점 중계하는 뮤지컬 꼭 보세요!!",
          "original_form": "해밀턴이라고 디즈니 플러스에서 독점 중계하는 뮤지컬 꼭 보세요!!",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006829.1.24",
          "form": "진짜 재밌어요...",
          "original_form": "진짜 재밌어요...",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006829.1.25",
          "form": "헐 진짜요??",
          "original_form": "헐 진짜요??",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006829.1.26",
          "form": "메모해둘게요 ㅠㅠㅠ",
          "original_form": "메모해둘게요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006829.1.27",
          "form": "ㅋㅋㅋㅋ 감사합니다...",
          "original_form": "ㅋㅋㅋㅋ 감사합니다...",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100006829.1.28",
          "form": "브나나 완결났다는데",
          "original_form": "브나나 완결났다는데",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100006829.1.29",
          "form": "넷플릭스엔 언제 들어올지 모르겠네요...",
          "original_form": "넷플릭스엔 언제 들어올지 모르겠네요...",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100006829.1.30",
          "form": "ㅠㅠ...",
          "original_form": "ㅠㅠ...",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100006829.1.31",
          "form": "저 사실 ㅋㅋㅋㅋ",
          "original_form": "저 사실 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100006829.1.32",
          "form": "피콕이라고 해외 앱 결제해서 봤어요...",
          "original_form": "피콕이라고 해외 앱 결제해서 봤어요...",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100006829.1.33",
          "form": "재밌더라고요...",
          "original_form": "재밌더라고요...",
          "speaker_id": "1",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100006829.1.34",
          "form": "와 진짜요???",
          "original_form": "와 진짜요???",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006829.1.35",
          "form": "전... 자막이 필요해서",
          "original_form": "전... 자막이 필요해서",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006829.1.36",
          "form": "넷플릭스에서 들여올때까지",
          "original_form": "넷플릭스에서 들여올때까지",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006829.1.37",
          "form": "기다리는 중이에요 ㅠㅠㅠㅠㅠ",
          "original_form": "기다리는 중이에요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006829.1.38",
          "form": "빨리 들어왔응 좋겠네요 ㅠㅠ",
          "original_form": "빨리 들어왔응 좋겠네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100006829.1.39",
          "form": "그러니까요 ㅠㅠㅠㅠ",
          "original_form": "그러니까요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100006829.1.40",
          "form": "넷플릭스에서 다른거 재밌게 보신건",
          "original_form": "넷플릭스에서 다른거 재밌게 보신건",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100006829.1.41",
          "form": "어떤거 있으신가요??",
          "original_form": "어떤거 있으신가요??",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100006829.1.42",
          "form": "저 셜록이요!!",
          "original_form": "저 셜록이요!!",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100006829.1.43",
          "form": "아 셜록 재밌죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 셜록 재밌죠 ㅋㅋㅋㅋㅋㅋ",
