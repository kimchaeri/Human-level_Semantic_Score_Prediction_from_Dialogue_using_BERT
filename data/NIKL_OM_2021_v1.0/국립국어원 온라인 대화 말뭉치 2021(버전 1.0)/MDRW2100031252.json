      "utterance": [
        {
          "id": "MDRW2100031252.1.1",
          "form": "커피좋아하세요?",
          "original_form": "커피좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031252.1.2",
          "form": "네! 아이스아메리카노가 최애입니다",
          "original_form": "네! 아이스아메리카노가 최애입니다",
          "speaker_id": "1",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100031252.1.3",
          "form": "커피를 잘아시네요ㅎㅎ",
          "original_form": "커피를 잘아시네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:26"
        },
        {
          "id": "MDRW2100031252.1.4",
          "form": "저는 초딩입맛이라ㅋㅋ",
          "original_form": "저는 초딩입맛이라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:26"
        },
        {
          "id": "MDRW2100031252.1.5",
          "form": "헉 뭐좋아하세요?!",
          "original_form": "헉 뭐좋아하세요?!",
          "speaker_id": "1",
          "time": "20210904 20:43"
        },
        {
          "id": "MDRW2100031252.1.6",
          "form": "캬라멜마끼아또요ㅋ",
          "original_form": "캬라멜마끼아또요ㅋ",
          "speaker_id": "2",
          "time": "20210904 22:31"
        },
        {
          "id": "MDRW2100031252.1.7",
          "form": "아 맛있죠!",
          "original_form": "아 맛있죠!",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100031252.1.8",
          "form": "저는 근데 무조건 아이스 입니다",
          "original_form": "저는 근데 무조건 아이스 입니다",
          "speaker_id": "1",
          "time": "20210904 22:44"
        },
        {
          "id": "MDRW2100031252.1.9",
          "form": "저도 사계절내내 아이스요ㅋ",
          "original_form": "저도 사계절내내 아이스요ㅋ",
          "speaker_id": "2",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100031252.1.10",
          "form": "얼죽아!!",
          "original_form": "얼죽아!!",
          "speaker_id": "1",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100031252.1.11",
          "form": "ㅋㅋㅋㅋㅋ아아랑 어울리는 디저트 뭐있나요?",
          "original_form": "ㅋㅋㅋㅋㅋ아아랑 어울리는 디저트 뭐있나요?",
          "speaker_id": "2",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031252.1.12",
          "form": "음,,",
          "original_form": "음,,",
          "speaker_id": "1",
          "time": "20210905 00:37"
        },
        {
          "id": "MDRW2100031252.1.13",
          "form": "엄청 고민되네요 뭐가있지,,",
          "original_form": "엄청 고민되네요 뭐가있지,,",
          "speaker_id": "1",
          "time": "20210905 00:37"
        },
        {
          "id": "MDRW2100031252.1.14",
          "form": "의외로 케이크는별로에요 저는 빵을 탄산이랑 먹어서 ,,",
          "original_form": "의외로 케이크는별로에요 저는 빵을 탄산이랑 먹어서 ,,",
          "speaker_id": "1",
          "time": "20210905 00:38"
        },
        {
          "id": "MDRW2100031252.1.15",
          "form": "ㅋㅋㅋㅋ탄산이랑 먹혀요?",
          "original_form": "ㅋㅋㅋㅋ탄산이랑 먹혀요?",
          "speaker_id": "2",
          "time": "20210905 01:03"
        },
        {
          "id": "MDRW2100031252.1.16",
          "form": "저 탄산을 좋아해서,,ㅋㄱㅋㅋㅋㅋㄱㅋ",
          "original_form": "저 탄산을 좋아해서,,ㅋㄱㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210905 01:03"
        },
        {
          "id": "MDRW2100031252.1.17",
          "form": "저는 우유 안좋아하는데..빵먹을땐 그리 먹고싶더라구요",
          "original_form": "저는 우유 안좋아하는데..빵먹을땐 그리 먹고싶더라구요",
          "speaker_id": "2",
          "time": "20210905 01:03"
        },
        {
          "id": "MDRW2100031252.1.18",
          "form": "아아는 곁들임없이 그냥 더운날 탁!  마시면",
          "original_form": "아아는 곁들임없이 그냥 더운날 탁!  마시면",
          "speaker_id": "1",
          "time": "20210905 01:03"
        },
        {
          "id": "MDRW2100031252.1.19",
          "form": "최고같아요",
          "original_form": "최고같아요",
          "speaker_id": "1",
          "time": "20210905 01:03"
        }
      ]
