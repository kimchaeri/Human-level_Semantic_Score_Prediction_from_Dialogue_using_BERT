      "utterance": [
        {
          "id": "MDRW2100017160.1.1",
          "form": "가끔이요!",
          "original_form": "가끔이요!",
          "speaker_id": "2",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017160.1.2",
          "form": "왜요? 사라없으면",
          "original_form": "왜요? 사라없으면",
          "speaker_id": "1",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017160.1.3",
          "form": "뭐가 힘든가요",
          "original_form": "뭐가 힘든가요",
          "speaker_id": "1",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017160.1.4",
          "form": "사람 없다고 해서",
          "original_form": "사람 없다고 해서",
          "speaker_id": "2",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017160.1.5",
          "form": "자유시간이 있는 건 아니에요",
          "original_form": "자유시간이 있는 건 아니에요",
          "speaker_id": "2",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100017160.1.6",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017160.1.7",
          "form": "자거나 책 읽거나",
          "original_form": "자거나 책 읽거나",
          "speaker_id": "2",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017160.1.8",
          "form": "그런 행동이 금지에요",
          "original_form": "그런 행동이 금지에요",
          "speaker_id": "2",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017160.1.9",
          "form": "계속 돌아다녀야해요 ㅎㅎ",
          "original_form": "계속 돌아다녀야해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017160.1.10",
          "form": "그럼 그냥 대기 쭉하고있는거에요?",
          "original_form": "그럼 그냥 대기 쭉하고있는거에요?",
          "speaker_id": "1",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017160.1.11",
          "form": "아님 그냥 대기...",
          "original_form": "아님 그냥 대기...",
          "speaker_id": "2",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017160.1.12",
          "form": "와 힘드셨겠어요",
          "original_form": "와 힘드셨겠어요",
          "speaker_id": "1",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017160.1.13",
          "form": "네 자게해줬으면 좋겠는데...",
          "original_form": "네 자게해줬으면 좋겠는데...",
          "speaker_id": "2",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017160.1.14",
          "form": "할짓없이 대기하는거 제일 힘들잖아요",
          "original_form": "할짓없이 대기하는거 제일 힘들잖아요",
          "speaker_id": "1",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100017160.1.15",
          "form": "네 ㅎㅎ 핸드폰도 못하고",
          "original_form": "네 ㅎㅎ 핸드폰도 못하고",
          "speaker_id": "2",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100017160.1.16",
          "form": "약간 수직적 문화인가요",
          "original_form": "약간 수직적 문화인가요",
          "speaker_id": "1",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100017160.1.17",
          "form": "아 그건 항공사마다 달라요",
          "original_form": "아 그건 항공사마다 달라요",
          "speaker_id": "2",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100017160.1.18",
          "form": "국내는 심하다고 들었어요",
          "original_form": "국내는 심하다고 들었어요",
          "speaker_id": "2",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100017160.1.19",
          "form": "다르군요",
          "original_form": "다르군요",
          "speaker_id": "1",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100017160.1.20",
          "form": "아시아나 이런데고",
          "original_form": "아시아나 이런데고",
