      "utterance": [
        {
          "id": "MDRW2100042126.1.1",
          "form": "저는 예능좋아해요 ㅎㅎ",
          "original_form": "저는 예능좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:06"
        },
        {
          "id": "MDRW2100042126.1.2",
          "form": "요즘 뭐 보세요?",
          "original_form": "요즘 뭐 보세요?",
          "speaker_id": "1",
          "time": "20210906 20:08"
        },
        {
          "id": "MDRW2100042126.1.3",
          "form": "원래 놀면뭐하니 안봤는데 ㅎ 저번주껀 웃기더라구요!",
          "original_form": "원래 놀면뭐하니 안봤는데 ㅎ 저번주껀 웃기더라구요!",
          "speaker_id": "2",
          "time": "20210906 20:08"
        },
        {
          "id": "MDRW2100042126.1.4",
          "form": "1박2일도 보고 ㅎ 원래는 신서유기 좋아햇어요 ㅋㅋ",
          "original_form": "1박2일도 보고 ㅎ 원래는 신서유기 좋아햇어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100042126.1.5",
          "form": "아 진짜 울뻔했어요ㅜㅠ",
          "original_form": "아 진짜 울뻔했어요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210906 20:10"
        },
        {
          "id": "MDRW2100042126.1.6",
          "form": "어제 일박 보셨나요?",
          "original_form": "어제 일박 보셨나요?",
          "speaker_id": "1",
          "time": "20210906 20:11"
        },
        {
          "id": "MDRW2100042126.1.7",
          "form": "아뇨 어제껀 못봐서 좀있다 다시보기보려구요!",
          "original_form": "아뇨 어제껀 못봐서 좀있다 다시보기보려구요!",
          "speaker_id": "2",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042126.1.8",
          "form": "아 진짜... 재밌었어요ㅋㅋ",
          "original_form": "아 진짜... 재밌었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042126.1.9",
          "form": "잼잇어서 울뻔하셧다는거죠? ㅎㅎ",
          "original_form": "잼잇어서 울뻔하셧다는거죠? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042126.1.10",
          "form": "놀뭐 저도 안보다가 정준하나옴서 봐요",
          "original_form": "놀뭐 저도 안보다가 정준하나옴서 봐요",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100042126.1.11",
          "form": "너무 웃겼어요ㅋㅋ",
          "original_form": "너무 웃겼어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100042126.1.12",
          "form": "혹시 그중 더 편애하는 멤버가 있으세요?",
          "original_form": "혹시 그중 더 편애하는 멤버가 있으세요?",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100042126.1.13",
          "form": "1박2일이요!? 전 김선호요!!!",
          "original_form": "1박2일이요!? 전 김선호요!!!",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042126.1.14",
          "form": "저도!!!!",
          "original_form": "저도!!!!",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042126.1.15",
          "form": "그럼 어제 편 매우 추천합니다",
          "original_form": "그럼 어제 편 매우 추천합니다",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042126.1.16",
          "form": "ㅋㅋㅋㅋ9시부터 밥먹으면서 ㅋㅋ보고 잇어도ㅠㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ9시부터 밥먹으면서 ㅋㅋ보고 잇어도ㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042126.1.17",
          "form": "그럼 갯마을 차차차보세요??",
          "original_form": "그럼 갯마을 차차차보세요??",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042126.1.18",
          "form": "아뇨 철저하게 예능만ㅋㅋ",
          "original_form": "아뇨 철저하게 예능만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042126.1.19",
          "form": "재밌대요 그 드라마",
          "original_form": "재밌대요 그 드라마",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042126.1.20",
          "form": "저도 안봣는데ㅜㅋㅋㅋ",
          "original_form": "저도 안봣는데ㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042126.1.21",
          "form": "저는 나중에 모아서 정주행하려고 그냥 안챙겨봐요",
          "original_form": "저는 나중에 모아서 정주행하려고 그냥 안챙겨봐요",
          "speaker_id": "2",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042126.1.22",
          "form": "저도 그러다 넘긴 드라마들이...ㅜㅋㅋ",
          "original_form": "저도 그러다 넘긴 드라마들이...ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042126.1.23",
          "form": "빈센조도 중간에 하차했어요",
          "original_form": "빈센조도 중간에 하차했어요",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042126.1.24",
          "form": "전 안본드라마네요!",
          "original_form": "전 안본드라마네요!",
