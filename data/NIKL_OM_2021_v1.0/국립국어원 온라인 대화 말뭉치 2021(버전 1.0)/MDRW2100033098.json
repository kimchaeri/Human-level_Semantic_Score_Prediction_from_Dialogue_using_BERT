      "utterance": [
        {
          "id": "MDRW2100033098.1.1",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "1",
          "time": "20210906 16:10"
        },
        {
          "id": "MDRW2100033098.1.2",
          "form": "이게 짱이지",
          "original_form": "이게 짱이지",
          "speaker_id": "2",
          "time": "20210906 16:10"
        },
        {
          "id": "MDRW2100033098.1.3",
          "form": "항상 반려동물 키우고싶었는데",
          "original_form": "항상 반려동물 키우고싶었는데",
          "speaker_id": "1",
          "time": "20210906 16:10"
        },
        {
          "id": "MDRW2100033098.1.4",
          "form": "우리집에는 마리모랑 구피밖에없어요",
          "original_form": "우리집에는 마리모랑 구피밖에없어요",
          "speaker_id": "1",
          "time": "20210906 16:10"
        },
        {
          "id": "MDRW2100033098.1.5",
          "form": "헐 저도요 집에 금붕어랑 식물밖에 없어요",
          "original_form": "헐 저도요 집에 금붕어랑 식물밖에 없어요",
          "speaker_id": "2",
          "time": "20210906 16:10"
        },
        {
          "id": "MDRW2100033098.1.6",
          "form": "그래서 항상 친구네집 고양이를 보러가는데",
          "original_form": "그래서 항상 친구네집 고양이를 보러가는데",
          "speaker_id": "1",
          "time": "20210906 16:11"
        },
        {
          "id": "MDRW2100033098.1.7",
          "form": "고양이나 강아지 키우고 싶어요",
          "original_form": "고양이나 강아지 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210906 16:11"
        },
        {
          "id": "MDRW2100033098.1.8",
          "form": "반년에 한번씩 보러가서 그녀석이 내얼굴을 맨날 까먹어요",
          "original_form": "반년에 한번씩 보러가서 그녀석이 내얼굴을 맨날 까먹어요",
          "speaker_id": "1",
          "time": "20210906 16:11"
        },
        {
          "id": "MDRW2100033098.1.9",
          "form": "용서하지못한다",
          "original_form": "용서하지못한다",
          "speaker_id": "1",
          "time": "20210906 16:11"
        },
        {
          "id": "MDRW2100033098.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 야옹아 너무하다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 야옹아 너무하다",
          "speaker_id": "2",
          "time": "20210906 16:11"
        },
        {
          "id": "MDRW2100033098.1.11",
          "form": "고양이 어떻게 생겼어요?",
          "original_form": "고양이 어떻게 생겼어요?",
          "speaker_id": "2",
          "time": "20210906 16:11"
        },
        {
          "id": "MDRW2100033098.1.12",
          "form": "고양이 뱅갈 고양이예요",
          "original_form": "고양이 뱅갈 고양이예요",
          "speaker_id": "1",
          "time": "20210906 16:12"
        },
        {
          "id": "MDRW2100033098.1.13",
          "form": "병원에서 비만이래",
          "original_form": "병원에서 비만이래",
          "speaker_id": "1",
          "time": "20210906 16:12"
        },
        {
          "id": "MDRW2100033098.1.14",
          "form": "헐 너무 귀엽겠다",
          "original_form": "헐 너무 귀엽겠다",
          "speaker_id": "2",
          "time": "20210906 16:12"
        },
        {
          "id": "MDRW2100033098.1.15",
          "form": "이름은...까까다...",
          "original_form": "이름은...까까다...",
          "speaker_id": "1",
          "time": "20210906 16:12"
        },
        {
          "id": "MDRW2100033098.1.16",
          "form": "헐.... 위험하대요?",
          "original_form": "헐.... 위험하대요?",
          "speaker_id": "2",
          "time": "20210906 16:12"
        },
        {
          "id": "MDRW2100033098.1.17",
          "form": "넘 기여움요",
          "original_form": "넘 기여움요",
          "speaker_id": "1",
          "time": "20210906 16:12"
        },
        {
          "id": "MDRW2100033098.1.18",
          "form": "까까 까까 금지",
          "original_form": "까까 까까 금지",
          "speaker_id": "2",
          "time": "20210906 16:12"
        },
        {
          "id": "MDRW2100033098.1.19",
          "form": "건강에는 지장없을걸요 아마도",
          "original_form": "건강에는 지장없을걸요 아마도",
          "speaker_id": "1",
          "time": "20210906 16:12"
        },
        {
          "id": "MDRW2100033098.1.20",
          "form": "오 다행이다",
          "original_form": "오 다행이다",
          "speaker_id": "2",
          "time": "20210906 16:13"
        },
        {
          "id": "MDRW2100033098.1.21",
          "form": "암튼..  나만 고양이 없어",
          "original_form": "암튼..  나만 고양이 없어",
          "speaker_id": "1",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100033098.1.22",
          "form": "그건 인정합니다",
          "original_form": "그건 인정합니다",
          "speaker_id": "2",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100033098.1.23",
          "form": "고양이 키우고 싶어요",
          "original_form": "고양이 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100033098.1.24",
          "form": "자취하면 꼭 키우고 싶음..",
          "original_form": "자취하면 꼭 키우고 싶음..",
          "speaker_id": "2",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100033098.1.25",
          "form": "그러니까요...",
          "original_form": "그러니까요...",
          "speaker_id": "1",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100033098.1.26",
          "form": "작은 머리로 뭔가 생각한다는게 참 좋습니다",
          "original_form": "작은 머리로 뭔가 생각한다는게 참 좋습니다",
          "speaker_id": "1",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100033098.1.27",
          "form": "자취 한번 해보지 않으셨나요? 그때는 엄두가 안 나시나요",
          "original_form": "자취 한번 해보지 않으셨나요? 그때는 엄두가 안 나시나요",
          "speaker_id": "2",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100033098.1.28",
          "form": "아 ㅋㅋ 레알.... 바보같은 짓 하는 게 너무 귀여움",
          "original_form": "아 ㅋㅋ 레알.... 바보같은 짓 하는 게 너무 귀여움",
          "speaker_id": "2",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100033098.1.29",
          "form": "자취는 하는데 아무래도 애완동물 금지라서.. 하",
          "original_form": "자취는 하는데 아무래도 애완동물 금지라서.. 하",
          "speaker_id": "1",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100033098.1.30",
          "form": "학생신분이라 돈이 없기도 하구요",
          "original_form": "학생신분이라 돈이 없기도 하구요",
          "speaker_id": "1",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100033098.1.31",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100033098.1.32",
          "form": "아...... 아쉽다",
          "original_form": "아...... 아쉽다",
          "speaker_id": "2",
          "time": "20210906 16:17"
        },
        {
          "id": "MDRW2100033098.1.33",
          "form": "키우셨으면",
          "original_form": "키우셨으면",
          "speaker_id": "2",
          "time": "20210906 16:17"
        },
        {
          "id": "MDRW2100033098.1.34",
          "form": "사진 뜯는 건데",
          "original_form": "사진 뜯는 건데",
          "speaker_id": "2",
          "time": "20210906 16:17"
        },
        {
          "id": "MDRW2100033098.1.35",
          "form": "고양이 키우는데 사진 안보여주는건 못참죠",
          "original_form": "고양이 키우는데 사진 안보여주는건 못참죠",
          "speaker_id": "1",
          "time": "20210906 16:17"
        },
        {
          "id": "MDRW2100033098.1.36",
          "form": "ㅋㅋ 당연하죠. 저 키우면 지겹다할 때까지 자랑할 겁니다",
          "original_form": "ㅋㅋ 당연하죠. 저 키우면 지겹다할 때까지 자랑할 겁니다",
          "speaker_id": "2",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100033098.1.37",
          "form": "이름같은건 정하신게 있나요",
          "original_form": "이름같은건 정하신게 있나요",
          "speaker_id": "1",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100033098.1.38",
          "form": "흠.... 고양이는 없는데",
          "original_form": "흠.... 고양이는 없는데",
          "speaker_id": "2",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100033098.1.39",
          "form": "예전에 푸들 일주일 키운 적 있어서",
          "original_form": "예전에 푸들 일주일 키운 적 있어서",
          "speaker_id": "2",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100033098.1.40",
          "form": "그때 이름이 좀 생각나요 ㅋㅋ",
          "original_form": "그때 이름이 좀 생각나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100033098.1.41",
          "form": "애플이었나",
          "original_form": "애플이었나",
          "speaker_id": "2",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100033098.1.42",
          "form": "뜬금없지만 푸들은 근육질이더라구요",
          "original_form": "뜬금없지만 푸들은 근육질이더라구요",
          "speaker_id": "1",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100033098.1.43",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100033098.1.44",
          "form": "전에 친구가 푸들 잠깐 봐준다고 같이 본적있는데",
          "original_form": "전에 친구가 푸들 잠깐 봐준다고 같이 본적있는데",
          "speaker_id": "1",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100033098.1.45",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "2",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100033098.1.46",
          "form": "작은 푸들 상상했다가 깜짝 놀란 기억이있습니다",
          "original_form": "작은 푸들 상상했다가 깜짝 놀란 기억이있습니다",
          "speaker_id": "1",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100033098.1.47",
          "form": "엄청 근육질",
          "original_form": "엄청 근육질",
          "speaker_id": "1",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100033098.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아",
          "speaker_id": "2",
          "time": "20210906 16:20"
        },
        {
          "id": "MDRW2100033098.1.49",
          "form": "서있는 푸들 그냥",
          "original_form": "서있는 푸들 그냥",
          "speaker_id": "2",
          "time": "20210906 16:20"
        },
        {
          "id": "MDRW2100033098.1.50",
          "form": "질 거 같은 이미지 ㄹㅇ",
          "original_form": "질 거 같은 이미지 ㄹㅇ",
          "speaker_id": "2",
          "time": "20210906 16:20"
        },
        {
          "id": "MDRW2100033098.1.51",
          "form": "솔직히 뒷발차기에 질 자신밖엔 없다..",
          "original_form": "솔직히 뒷발차기에 질 자신밖엔 없다..",
          "speaker_id": "1",
          "time": "20210906 16:20"
        },
        {
          "id": "MDRW2100033098.1.52",
          "form": "인정합니다... 너무... 힘이 넘쳐요",
          "original_form": "인정합니다... 너무... 힘이 넘쳐요",
          "speaker_id": "2",
          "time": "20210906 16:20"
        },
        {
          "id": "MDRW2100033098.1.53",
          "form": "글고 진짜",
          "original_form": "글고 진짜",
          "speaker_id": "2",
          "time": "20210906 16:21"
        },
        {
          "id": "MDRW2100033098.1.54",
          "form": "점프도 잘해요. 저 첫날엔 소파 못 올라왔는데",
          "original_form": "점프도 잘해요. 저 첫날엔 소파 못 올라왔는데",
          "speaker_id": "2",
          "time": "20210906 16:21"
        },
        {
          "id": "MDRW2100033098.1.55",
          "form": "일주일만에 소파 점프해서 올라왔어요",
          "original_form": "일주일만에 소파 점프해서 올라왔어요",
          "speaker_id": "2",
          "time": "20210906 16:21"
        }
      ]
