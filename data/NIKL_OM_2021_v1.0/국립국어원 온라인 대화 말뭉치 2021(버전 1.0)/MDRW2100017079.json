      "utterance": [
        {
          "id": "MDRW2100017079.1.1",
          "form": "제가자신없는분야예요",
          "original_form": "제가자신없는분야예요",
          "speaker_id": "1",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100017079.1.2",
          "form": "최근에 읽은 책 있어요?",
          "original_form": "최근에 읽은 책 있어요?",
          "speaker_id": "2",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100017079.1.3",
          "form": "최근 버드 스트라이크라는 책을 읽었어요",
          "original_form": "최근 버드 스트라이크라는 책을 읽었어요",
          "speaker_id": "2",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017079.1.4",
          "form": "재밌었나요?",
          "original_form": "재밌었나요?",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017079.1.5",
          "form": "저최근에본책",
          "original_form": "저최근에본책",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017079.1.6",
          "form": "일본어n4",
          "original_form": "일본어n4",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017079.1.7",
          "form": "ㅋㅋㅋㄱㄲㄱ",
          "original_form": "ㅋㅋㅋㄱㄲㄱ",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017079.1.8",
          "form": "아 맞다 대단한 천재가 여기있으셨지",
          "original_form": "아 맞다 대단한 천재가 여기있으셨지",
          "speaker_id": "2",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017079.1.9",
          "form": "날개달린 종족과 인간의 러브스토리였는데",
          "original_form": "날개달린 종족과 인간의 러브스토리였는데",
          "speaker_id": "2",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017079.1.10",
          "form": "날개달린 종족인 남자 주인공이 도망쳤어요",
          "original_form": "날개달린 종족인 남자 주인공이 도망쳤어요",
          "speaker_id": "2",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017079.1.11",
          "form": "연애 안하고 도망치다니 이게 뭐여",
          "original_form": "연애 안하고 도망치다니 이게 뭐여",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017079.1.12",
          "form": "도망치고끝이예요..?",
          "original_form": "도망치고끝이예요..?",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017079.1.13",
          "form": "설마!!!!",
          "original_form": "설마!!!!",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017079.1.14",
          "form": "네 너무 허무해요",
          "original_form": "네 너무 허무해요",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017079.1.15",
          "form": "너무하다...",
          "original_form": "너무하다...",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017079.1.16",
          "form": "해피엔딩을좋아해요저는",
          "original_form": "해피엔딩을좋아해요저는",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017079.1.17",
          "form": "소설책읽은지오래됐당..",
          "original_form": "소설책읽은지오래됐당..",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017079.1.18",
          "form": "앨리스죽이기였던가",
          "original_form": "앨리스죽이기였던가",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017079.1.19",
          "form": "잔인한거조아요 영상은못봐요",
          "original_form": "잔인한거조아요 영상은못봐요",
          "speaker_id": "1",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017079.1.20",
          "form": "ㅋㅋㅋㅋ역시 소설로 봐야 상상력이 풍부해지져",
          "original_form": "ㅋㅋㅋㅋ역시 소설로 봐야 상상력이 풍부해지져",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017079.1.21",
          "form": "많이잔인한거보고시포요",
          "original_form": "많이잔인한거보고시포요",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017079.1.22",
          "form": "오호호호..",
          "original_form": "오호호호..",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017079.1.23",
          "form": "7년의 밤",
          "original_form": "7년의 밤",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017079.1.24",
          "form": "좀 잔인햇오요",
          "original_form": "좀 잔인햇오요",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017079.1.25",
          "form": "어 본 거 같아요",
          "original_form": "어 본 거 같아요",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017079.1.26",
          "form": "아 방구석1열을 본건가",
          "original_form": "아 방구석1열을 본건가",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017079.1.27",
          "form": "도서 잡지에 대해 말하고있는데 또 경고",
          "original_form": "도서 잡지에 대해 말하고있는데 또 경고",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017079.1.28",
          "form": "영화화 됐어요",
          "original_form": "영화화 됐어요",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017079.1.29",
          "form": "근데 영화보단 책이 최고",
          "original_form": "근데 영화보단 책이 최고",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017079.1.30",
          "form": "맞오 영화화된걸본거같아요",
          "original_form": "맞오 영화화된걸본거같아요",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017079.1.31",
          "form": "책이재밌을거같아요",
          "original_form": "책이재밌을거같아요",
          "speaker_id": "1",
          "time": "20210826 19:09"
        }
      ]
    }
