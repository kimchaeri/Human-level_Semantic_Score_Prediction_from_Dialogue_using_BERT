      "utterance": [
        {
          "id": "MDRW2100013805.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013805.1.2",
          "form": "네 집안일하죠",
          "original_form": "네 집안일하죠",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013805.1.3",
          "form": "제일 자주하는 건 설거지 같아요",
          "original_form": "제일 자주하는 건 설거지 같아요",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013805.1.4",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013805.1.5",
          "form": "쓸기, 닦기 전부 담당 하세요?",
          "original_form": "쓸기, 닦기 전부 담당 하세요?",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013805.1.6",
          "form": "화장실 전담이요",
          "original_form": "화장실 전담이요",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013805.1.7",
          "form": "아 화장실만 전담으로 하시는거에요?",
          "original_form": "아 화장실만 전담으로 하시는거에요?",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013805.1.8",
          "form": "화장실 청소 까다롭지 않나요?",
          "original_form": "화장실 청소 까다롭지 않나요?",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013805.1.9",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013805.1.10",
          "form": "아 락스로 청소하세요?",
          "original_form": "아 락스로 청소하세요?",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013805.1.11",
          "form": "마스크 꼭 쓰고 문 열어두고 하세요 냄새 너무 독해요",
          "original_form": "마스크 꼭 쓰고 문 열어두고 하세요 냄새 너무 독해요",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013805.1.12",
          "form": "머리도 아프고",
          "original_form": "머리도 아프고",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013805.1.13",
          "form": "그쵸 뜨거운물이랑 락스는 절대 같이 쓰지말래요",
          "original_form": "그쵸 뜨거운물이랑 락스는 절대 같이 쓰지말래요",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013805.1.14",
          "form": "안좋은 물질 나온다고 하더라구요",
          "original_form": "안좋은 물질 나온다고 하더라구요",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013805.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100013805.1.16",
          "form": "그래서 더 그런가요",
          "original_form": "그래서 더 그런가요",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100013805.1.17",
          "form": "그래서 머리 아프신거 일 수도 있어요 ㅠㅠ 환기 잘하고 하세요",
          "original_form": "그래서 머리 아프신거 일 수도 있어요 ㅠㅠ 환기 잘하고 하세요",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100013805.1.18",
          "form": "화장실청소 꿀팁 혹시 있으실까요?",
          "original_form": "화장실청소 꿀팁 혹시 있으실까요?",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
