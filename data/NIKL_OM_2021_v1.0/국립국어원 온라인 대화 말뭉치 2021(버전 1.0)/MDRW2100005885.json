      "utterance": [
        {
          "id": "MDRW2100005885.1.1",
          "form": "집놀이족 용품이 뭐가있을까요?",
          "original_form": "집놀이족 용품이 뭐가있을까요?",
          "speaker_id": "2",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100005885.1.2",
          "form": "ㅋㅋㅋ 친구는 닌텐도 하더라구요",
          "original_form": "ㅋㅋㅋ 친구는 닌텐도 하더라구요",
          "speaker_id": "1",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005885.1.3",
          "form": "저는 게임기 없어서 최근에는 펀치니들 구매햇어여",
          "original_form": "저는 게임기 없어서 최근에는 펀치니들 구매햇어여",
          "speaker_id": "1",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100005885.1.4",
          "form": "아 정말요???",
          "original_form": "아 정말요???",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100005885.1.5",
          "form": "닌텐도 너무 비싸요ㅠㅠ",
          "original_form": "닌텐도 너무 비싸요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100005885.1.6",
          "form": "펀치니들이 뭔가용?",
          "original_form": "펀치니들이 뭔가용?",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100005885.1.7",
          "form": "맞아요 비싸요 ㅠㅠ 그래서 안샀어요 ㅋㅋㅋ",
          "original_form": "맞아요 비싸요 ㅠㅠ 그래서 안샀어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100005885.1.8",
          "form": "펀치니들 십자수 같이 바늘로 실을 수놓는거에여 ㅋㅋ",
          "original_form": "펀치니들 십자수 같이 바늘로 실을 수놓는거에여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:30"
        },
        {
          "id": "MDRW2100005885.1.9",
          "form": "러그같은거도 짜고 코스터같은것도 만들수있어여",
          "original_form": "러그같은거도 짜고 코스터같은것도 만들수있어여",
          "speaker_id": "1",
          "time": "20210907 16:30"
        },
        {
          "id": "MDRW2100005885.1.10",
          "form": "그쵸ㅠㅠㅠ",
          "original_form": "그쵸ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 16:30"
        },
        {
          "id": "MDRW2100005885.1.11",
          "form": "오오 괜찮네요!!",
          "original_form": "오오 괜찮네요!!",
          "speaker_id": "2",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100005885.1.12",
          "form": "그 물감으로 숫자 칠해서 그림완성하는 그런 것도 있잖아요 ㅎㅎ",
          "original_form": "그 물감으로 숫자 칠해서 그림완성하는 그런 것도 있잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100005885.1.13",
          "form": "맞아요 ㅋㅋㅋ 그런걸 마니하더라구여",
