      "utterance": [
        {
          "id": "MDRW2100020005.1.1",
          "form": "요즘 퍼스널 컬러가 유행이래",
          "original_form": "요즘 퍼스널 컬러가 유행이래",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100020005.1.2",
          "form": "나 도 그거 해봤어",
          "original_form": "나 도 그거 해봤어",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100020005.1.3",
          "form": "오 그거 해봤어?",
          "original_form": "오 그거 해봤어?",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100020005.1.4",
          "form": "어때?",
          "original_form": "어때?",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100020005.1.5",
          "form": "어떤 거 나왔을지 맞춰봐",
          "original_form": "어떤 거 나왔을지 맞춰봐",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100020005.1.6",
          "form": "글쎄 원 톤인가",
          "original_form": "글쎄 원 톤인가",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100020005.1.7",
          "form": "웜톤",
          "original_form": "웜톤",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100020005.1.8",
          "form": "땡 틀렸어",
          "original_form": "땡 틀렸어",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100020005.1.9",
          "form": "나도 해보고 싶어 궁금해",
          "original_form": "나도 해보고 싶어 궁금해",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100020005.1.10",
          "form": "나는 쿨 톤이 나왔어",
          "original_form": "나는 쿨 톤이 나왔어",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100020005.1.11",
          "form": "나도 쿨톤 일 것 같아",
          "original_form": "나도 쿨톤 일 것 같아",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100020005.1.12",
          "form": "다음에 해보는 게 어때",
          "original_form": "다음에 해보는 게 어때",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020005.1.13",
          "form": "퍼스널 컬러에 맞춰서 화장을 그럼 하고 있어?",
          "original_form": "퍼스널 컬러에 맞춰서 화장을 그럼 하고 있어?",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020005.1.14",
          "form": "그러려고 노력하고 있어",
          "original_form": "그러려고 노력하고 있어",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020005.1.15",
          "form": "어울리는 색깔이 뭔데",
          "original_form": "어울리는 색깔이 뭔데",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020005.1.16",
          "form": "나는 회색 비치도는 밝은 색이 어울린대",
          "original_form": "나는 회색 비치도는 밝은 색이 어울린대",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
