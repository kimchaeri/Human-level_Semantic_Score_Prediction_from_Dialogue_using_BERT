      "utterance": [
        {
          "id": "MDRW2100018179.1.1",
          "form": "안녕하세요 오늘 저녁 집밥으로 드셨나요ㅎㅎ",
          "original_form": "안녕하세요 오늘 저녁 집밥으로 드셨나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018179.1.2",
          "form": "오늘은 간만에 배달로 곱창 시켜먹었어요",
          "original_form": "오늘은 간만에 배달로 곱창 시켜먹었어요",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018179.1.3",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018179.1.4",
          "form": "오 곱창 맛있었겠어요",
          "original_form": "오 곱창 맛있었겠어요",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018179.1.5",
          "form": "저녇 뭐 드셨나요?",
          "original_form": "저녇 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018179.1.6",
          "form": "가끔 요리도 귀찮을 때 딱 시켜먹으면 편하죠ㅎㅎ",
          "original_form": "가끔 요리도 귀찮을 때 딱 시켜먹으면 편하죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018179.1.7",
          "form": "전 집에 제육 있어서 그거 먹었네요",
          "original_form": "전 집에 제육 있어서 그거 먹었네요",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018179.1.8",
          "form": "간편 요리ㅎㅎ",
          "original_form": "간편 요리ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018179.1.9",
          "form": "헐 제육.. 맛있었겠네요",
          "original_form": "헐 제육.. 맛있었겠네요",
          "speaker_id": "2",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018179.1.10",
          "form": "근데 진짜 배달음식 많이 먹으니",
          "original_form": "근데 진짜 배달음식 많이 먹으니",
          "speaker_id": "2",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018179.1.11",
          "form": "소화불량이 생기는 느낌이라 요즘 줄이고 잏어요",
          "original_form": "소화불량이 생기는 느낌이라 요즘 줄이고 잏어요",
          "speaker_id": "2",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018179.1.12",
          "form": "그렇죠 저도 그런 적 있어서 공감해요",
          "original_form": "그렇죠 저도 그런 적 있어서 공감해요",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018179.1.13",
          "form": "집에서 요리해서 드신 적 있으세요?",
          "original_form": "집에서 요리해서 드신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018179.1.14",
          "form": "전 주로 점심은 이것저것 혼자 해먹으로능 편이고",
          "original_form": "전 주로 점심은 이것저것 혼자 해먹으로능 편이고",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018179.1.15",
          "form": "나머지는 엄마가... ㅎㅎ",
          "original_form": "나머지는 엄마가... ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018179.1.16",
          "form": "ㅋㅋㅋ그러시구나 보통 그렇더라고요",
          "original_form": "ㅋㅋㅋ그러시구나 보통 그렇더라고요",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018179.1.17",
          "form": "저희집도 마찬가지예요",
          "original_form": "저희집도 마찬가지예요",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018179.1.18",
          "form": "요리 잘하시는 어무님들 정말 리스펙...",
          "original_form": "요리 잘하시는 어무님들 정말 리스펙...",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018179.1.19",
          "form": "맞아요.. 전 진짜 요리시작하면",
          "original_form": "맞아요.. 전 진짜 요리시작하면",
          "speaker_id": "2",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018179.1.20",
          "form": "온갖 물건들이.다 나와요",
          "original_form": "온갖 물건들이.다 나와요",
          "speaker_id": "2",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018179.1.21",
          "form": "계량컵 큰 숟가락 작은 숟가락 다 나오는데",
          "original_form": "계량컵 큰 숟가락 작은 숟가락 다 나오는데",
          "speaker_id": "2",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018179.1.22",
          "form": "엄마들은 그냥 그런 거 없이 뚝딱뚝딱 하는 거 보면 세상 신기해여",
          "original_form": "엄마들은 그냥 그런 거 없이 뚝딱뚝딱 하는 거 보면 세상 신기해여",
          "speaker_id": "2",
          "time": "20210826 23:35"
        },
        {
          "id": "MDRW2100018179.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 그러니까요 거의 손과 눈이 저울 그 자체잖아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 그러니까요 거의 손과 눈이 저울 그 자체잖아요",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100018179.1.24",
          "form": "가끔 요리해본다고 뭐 양념 얼마나 들어가? 물어보면",
          "original_form": "가끔 요리해본다고 뭐 양념 얼마나 들어가? 물어보면",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100018179.1.25",
          "form": "그냥 살살살살 넣어~~해서 땀흘리는...ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 살살살살 넣어~~해서 땀흘리는...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018179.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 진짜 공감이요ㅋㅋㅋㅋ 살살살 그거 어떻게 하는건데요 진짜 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 진짜 공감이요ㅋㅋㅋㅋ 살살살 그거 어떻게 하는건데요 진짜 ㅋㅋㅋㅋ",
