      "utterance": [
        {
          "id": "MDRW2100017481.1.1",
          "form": "지금이 덥지않아",
          "original_form": "지금이 덥지않아",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017481.1.2",
          "form": "여름에 너무 더웠어",
          "original_form": "여름에 너무 더웠어",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017481.1.3",
          "form": "가을이 다가오네",
          "original_form": "가을이 다가오네",
          "speaker_id": "2",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017481.1.4",
          "form": "늦게 비가 많이와서 너무 습해",
          "original_form": "늦게 비가 많이와서 너무 습해",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017481.1.5",
          "form": "비 많이 안왔으면 좋겠다",
          "original_form": "비 많이 안왔으면 좋겠다",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017481.1.6",
          "form": "이번주 계속 비오지?",
          "original_form": "이번주 계속 비오지?",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017481.1.7",
          "form": "아니 계속 비 안와",
          "original_form": "아니 계속 비 안와",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017481.1.8",
          "form": "다행이다",
          "original_form": "다행이다",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017481.1.9",
          "form": "어느 계절이 좋아?",
          "original_form": "어느 계절이 좋아?",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017481.1.10",
          "form": "봄 가을 어느 계절이 좋아?",
          "original_form": "봄 가을 어느 계절이 좋아?",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017481.1.11",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017481.1.12",
          "form": "추운 거 싫고 더운 거 싫다",
          "original_form": "추운 거 싫고 더운 거 싫다",
          "speaker_id": "2",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017481.1.13",
          "form": "겨울이랑 여름중에 뭐가 더 싫어??",
          "original_form": "겨울이랑 여름중에 뭐가 더 싫어??",
          "speaker_id": "1",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017481.1.14",
          "form": "둘 다 싫어",
          "original_form": "둘 다 싫어",
          "speaker_id": "2",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017481.1.15",
          "form": "가을이 길었으면 좋겠다",
          "original_form": "가을이 길었으면 좋겠다",
          "speaker_id": "1",
          "time": "20210826 19:43"
        }
