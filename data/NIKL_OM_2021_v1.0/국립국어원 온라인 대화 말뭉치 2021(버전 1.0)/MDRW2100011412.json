      "utterance": [
        {
          "id": "MDRW2100011412.1.1",
          "form": "최근에 구매하신 제품 있으세요?",
          "original_form": "최근에 구매하신 제품 있으세요?",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011412.1.2",
          "form": "닌텐도 동숩 샀어요",
          "original_form": "닌텐도 동숩 샀어요",
          "speaker_id": "1",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011412.1.3",
          "form": "동숲이요",
          "original_form": "동숲이요",
          "speaker_id": "1",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011412.1.4",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011412.1.5",
          "form": "제가 갖고싶었던.....ㅎ",
          "original_form": "제가 갖고싶었던.....ㅎ",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011412.1.6",
          "form": "요즘 당근에 자주 올라와요!",
          "original_form": "요즘 당근에 자주 올라와요!",
          "speaker_id": "1",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011412.1.7",
          "form": "사고싶은데 질릴까봐 안사고있어요",
          "original_form": "사고싶은데 질릴까봐 안사고있어요",
          "speaker_id": "2",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100011412.1.8",
          "form": "한창 유행했응 때 진짜 못구했는데 ㅎㅎ",
          "original_form": "한창 유행했응 때 진짜 못구했는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100011412.1.9",
          "form": "그죠 그때 품절대란이였는데",
          "original_form": "그죠 그때 품절대란이였는데",
          "speaker_id": "1",
          "time": "20210824 12:51"
        },
        {
          "id": "MDRW2100011412.1.10",
          "form": "해보고있는데 잔잔한 게임 별로시면 질리실거같아요",
          "original_form": "해보고있는데 잔잔한 게임 별로시면 질리실거같아요",
          "speaker_id": "1",
          "time": "20210824 12:51"
        },
        {
          "id": "MDRW2100011412.1.11",
          "form": "ㅋㅋㅋ 맞아요 ㅠ",
          "original_form": "ㅋㅋㅋ 맞아요 ㅠ",
          "speaker_id": "2",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011412.1.12",
          "form": "저 닌텐도 사려고 노력했는데 맨날 광탈했어요",
          "original_form": "저 닌텐도 사려고 노력했는데 맨날 광탈했어요",
          "speaker_id": "2",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100011412.1.13",
          "form": "요즘은 제값주고 파는데 한참유행일때는 프리미엄 엄청붙었었죠ㅠ",
          "original_form": "요즘은 제값주고 파는데 한참유행일때는 프리미엄 엄청붙었었죠ㅠ",
          "speaker_id": "1",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011412.1.14",
          "form": "맞아요 코로나 초창기라 그런지",
          "original_form": "맞아요 코로나 초창기라 그런지",
          "speaker_id": "2",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100011412.1.15",
          "form": "진짜 핫했어요",
          "original_form": "진짜 핫했어요",
          "speaker_id": "2",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100011412.1.16",
          "form": "저도 그때포기하고 올해 들어서 겨우 스위치 장만했어요",
          "original_form": "저도 그때포기하고 올해 들어서 겨우 스위치 장만했어요",
          "speaker_id": "1",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011412.1.17",
          "form": "안질리세요??",
          "original_form": "안질리세요??",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011412.1.18",
          "form": "네 저는 매일조금씩 소소하게 하고 있어요",
          "original_form": "네 저는 매일조금씩 소소하게 하고 있어요",
          "speaker_id": "1",
          "time": "20210824 14:56"
        }
