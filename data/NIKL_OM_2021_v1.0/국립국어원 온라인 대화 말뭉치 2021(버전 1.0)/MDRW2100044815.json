      "utterance": [
        {
          "id": "MDRW2100044815.1.1",
          "form": "저는 박우진 좋아했는데요!",
          "original_form": "저는 박우진 좋아했는데요!",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044815.1.2",
          "form": "유투브 엄청 열심히 보고",
          "original_form": "유투브 엄청 열심히 보고",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044815.1.3",
          "form": "귀엽죠ㅋㅋ",
          "original_form": "귀엽죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044815.1.4",
          "form": "투표도 하고ㅜ",
          "original_form": "투표도 하고ㅜ",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044815.1.5",
          "form": "전 프듀는 안봤는데",
          "original_form": "전 프듀는 안봤는데",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044815.1.6",
          "form": "급빠져서 ㅋㅋ",
          "original_form": "급빠져서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044815.1.7",
          "form": "4년쯤??",
          "original_form": "4년쯤??",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044815.1.8",
          "form": "ㅋㅋㅋ와 대박",
          "original_form": "ㅋㅋㅋ와 대박",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044815.1.9",
          "form": "제주변에도 김재환 입덕해서",
          "original_form": "제주변에도 김재환 입덕해서",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044815.1.10",
          "form": "말레이시아까지 가시고",
          "original_form": "말레이시아까지 가시고",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044815.1.11",
          "form": "음원홍보도 하시고 그러신분 계셨는데",
          "original_form": "음원홍보도 하시고 그러신분 계셨는데",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044815.1.12",
          "form": "그런 열정 ㅋㅋ 대단해요ㅋㅋ",
          "original_form": "그런 열정 ㅋㅋ 대단해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044815.1.13",
          "form": "그니까요 대단하셨어요 같이 프듀도 달리셨거든요",
          "original_form": "그니까요 대단하셨어요 같이 프듀도 달리셨거든요",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044815.1.14",
          "form": "일단 노래입덕하면 오래가는거 같아요",
          "original_form": "일단 노래입덕하면 오래가는거 같아요",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044815.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044815.1.16",
          "form": "저도 공연다시하면 갈건데ㅜ",
          "original_form": "저도 공연다시하면 갈건데ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044815.1.17",
          "form": "저는 춤으로 입덕하여",
          "original_form": "저는 춤으로 입덕하여",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044815.1.18",
          "form": "금방 탈턱했죠..",
          "original_form": "금방 탈턱했죠..",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044815.1.19",
          "form": "그의 새 그룹을 받아들이지 못했어요...",
          "original_form": "그의 새 그룹을 받아들이지 못했어요...",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044815.1.20",
          "form": "아.. 그런 경우 많은거같아요!",
          "original_form": "아.. 그런 경우 많은거같아요!",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044815.1.21",
          "form": "네 그리고 그 그룹에 탈퇴도 생기고요",
          "original_form": "네 그리고 그 그룹에 탈퇴도 생기고요",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044815.1.22",
          "form": "그래서 노래로 넘어왔죠",
          "original_form": "그래서 노래로 넘어왔죠",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044815.1.23",
          "form": "김재환은 요즘 예능안하나요?",
          "original_form": "김재환은 요즘 예능안하나요?",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044815.1.24",
          "form": "가끔하는거 같은데 탈덕인정중 1달차예요ㅋㅋ",
          "original_form": "가끔하는거 같은데 탈덕인정중 1달차예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044815.1.25",
          "form": "아 생각하시기에도 맘이 좀 멀어지셨군요ㅋㅋ",
          "original_form": "아 생각하시기에도 맘이 좀 멀어지셨군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044815.1.26",
          "form": "ㅋㅋ노래가 좋으면 또 빠질수도ㅋㅋ",
          "original_form": "ㅋㅋ노래가 좋으면 또 빠질수도ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:01"
        }
      ]
    }
  ]
