      "utterance": [
        {
          "id": "MDRW2100013390.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013390.1.2",
          "form": "좋아하는 책이 뭔가요?",
          "original_form": "좋아하는 책이 뭔가요?",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013390.1.3",
          "form": "안녕하세요!\n저는 아직 좋아하는 책이 없어요",
          "original_form": "안녕하세요!\n저는 아직 좋아하는 책이 없어요",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013390.1.4",
          "form": "관심가는건 나의 투쟁이(히틀러)에요",
          "original_form": "관심가는건 나의 투쟁이(히틀러)에요",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013390.1.5",
          "form": "저는 최근에 헝거게임을 읽었어요",
          "original_form": "저는 최근에 헝거게임을 읽었어요",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013390.1.6",
          "form": "영화로도 만들어졌어요",
          "original_form": "영화로도 만들어졌어요",
          "speaker_id": "1",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013390.1.7",
          "form": "만화책은 좋아하세요?",
          "original_form": "만화책은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013390.1.8",
          "form": "저는 헝거게임 영화로 2편까지 봤던걸로 기억해요",
          "original_form": "저는 헝거게임 영화로 2편까지 봤던걸로 기억해요",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013390.1.9",
          "form": "만화책은 일본쪽 많이 좋아해요",
          "original_form": "만화책은 일본쪽 많이 좋아해요",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013390.1.10",
          "form": "추천할만한 만화가 있나요.",
          "original_form": "추천할만한 만화가 있나요.",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013390.1.11",
          "form": "this 커뮤니케이션이라는 만화 괜찮았어요",
          "original_form": "this 커뮤니케이션이라는 만화 괜찮았어요",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013390.1.12",
          "form": "갑자기 나타난 괴물로 인해 세계가 멸망하고 주인공(장교출신 군인)이 생존자들이 거주하는 지하쉴터를 찾게되는데",
          "original_form": "갑자기 나타난 괴물로 인해 세계가 멸망하고 주인공(장교출신 군인)이 생존자들이 거주하는 지하쉴터를 찾게되는데",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013390.1.13",
          "form": "거기서 생체병기로 개조된 아이들과 쉴터를 지키는 내용이에요",
          "original_form": "거기서 생체병기로 개조된 아이들과 쉴터를 지키는 내용이에요",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013390.1.14",
          "form": "오 재미있어보여요",
          "original_form": "오 재미있어보여요",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013390.1.15",
          "form": "다음에 기회가된다면 읽어볼게요",
          "original_form": "다음에 기회가된다면 읽어볼게요",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013390.1.16",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013390.1.17",
          "form": "혹시 추천 도서 있을까요?",
          "original_form": "혹시 추천 도서 있을까요?",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013390.1.18",
          "form": "며칠전에 교보문구에 갔는데 관심 있는 책이 없어서",
          "original_form": "며칠전에 교보문구에 갔는데 관심 있는 책이 없어서",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013390.1.19",
          "form": "빈손으로 나왔어요",
          "original_form": "빈손으로 나왔어요",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013390.1.20",
          "form": "최근에 히가시노 게이고의 최신작인 녹나무의 파수꾼이라는 책을 읽고있어요",
          "original_form": "최근에 히가시노 게이고의 최신작인 녹나무의 파수꾼이라는 책을 읽고있어요",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013390.1.21",
          "form": "서점에 갔다가 표지가예뻐서 샀는데",
          "original_form": "서점에 갔다가 표지가예뻐서 샀는데",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013390.1.22",
          "form": "잔잔하고 훈훈한 이야기예요",
          "original_form": "잔잔하고 훈훈한 이야기예요",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013390.1.23",
          "form": "기회가된다면 읽어보시는걸 추천해요",
          "original_form": "기회가된다면 읽어보시는걸 추천해요",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013390.1.24",
          "form": "작가 이름이 익숙해서 찾아보네",
          "original_form": "작가 이름이 익숙해서 찾아보네",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013390.1.25",
          "form": "니 그 잡화점 작가군요",
          "original_form": "니 그 잡화점 작가군요",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013390.1.26",
          "form": "추천 감사합니다!",
          "original_form": "추천 감사합니다!",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013390.1.27",
          "form": "맞아요 나미야 잡화점",
          "original_form": "맞아요 나미야 잡화점",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013390.1.28",
          "form": "만화책중에선 약속의 네버랜드라는 책을 읽었어요",
          "original_form": "만화책중에선 약속의 네버랜드라는 책을 읽었어요",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013390.1.29",
          "form": "왠지 좋아하실것같아요",
          "original_form": "왠지 좋아하실것같아요",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013390.1.30",
          "form": "네 약간 스릴러물 좋아해서 1부 재미있게 봤어요",
          "original_form": "네 약간 스릴러물 좋아해서 1부 재미있게 봤어요",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013390.1.31",
          "form": "나미야 잡화점",
          "original_form": "나미야 잡화점",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013390.1.32",
          "form": "제가 중학교때 졸업 선물로 담임 선생님께 받았는데",
          "original_form": "제가 중학교때 졸업 선물로 담임 선생님께 받았는데",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013390.1.33",
          "form": "항상 5페이지까지만 읽고 덮었던게 기억나네요",
          "original_form": "항상 5페이지까지만 읽고 덮었던게 기억나네요",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013390.1.34",
          "form": "그리고 선물 받았던 매해 2월마다 읽어야겠다 다짐하며",
          "original_form": "그리고 선물 받았던 매해 2월마다 읽어야겠다 다짐하며",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013390.1.35",
          "form": "책을 꺼내들지만 똑같이 5페이지만 읽었어요",
          "original_form": "책을 꺼내들지만 똑같이 5페이지만 읽었어요",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013390.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013390.1.37",
          "form": "그럴수있어요 저도 그래요",
          "original_form": "그럴수있어요 저도 그래요",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013390.1.38",
          "form": "담임선생님이 책을 선물해주시고 좋은 선생님이시네요",
          "original_form": "담임선생님이 책을 선물해주시고 좋은 선생님이시네요",
          "speaker_id": "1",
          "time": "20210825 14:35"
        }
      ]
    }
