      "utterance": [
        {
          "id": "MDRW2100039779.1.1",
          "form": "저는 잔여백신 초기때 맞아서 7월에 2차 맞았어요",
          "original_form": "저는 잔여백신 초기때 맞아서 7월에 2차 맞았어요",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039779.1.2",
          "form": "님은어때요",
          "original_form": "님은어때요",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039779.1.3",
          "form": "오 저는 일차만 맞았어요",
          "original_form": "오 저는 일차만 맞았어요",
          "speaker_id": "1",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039779.1.4",
          "form": "아스트라 맞으신거예요?",
          "original_form": "아스트라 맞으신거예요?",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039779.1.5",
          "form": "아제+화이자에요",
          "original_form": "아제+화이자에요",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039779.1.6",
          "form": "화이자신가요?",
          "original_form": "화이자신가요?",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039779.1.7",
          "form": "헉 교차는 처음봐요",
          "original_form": "헉 교차는 처음봐요",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039779.1.8",
          "form": "네 저는 화이자요",
          "original_form": "네 저는 화이자요",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039779.1.9",
          "form": "약이 없어서 교차접종 시켜버린 ㅜㅜ",
          "original_form": "약이 없어서 교차접종 시켜버린 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039779.1.10",
          "form": "몸 괜찮으셨어요?",
          "original_form": "몸 괜찮으셨어요?",
          "speaker_id": "1",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039779.1.11",
          "form": "아제1차가 대박이에요",
          "original_form": "아제1차가 대박이에요",
          "speaker_id": "2",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039779.1.12",
          "form": "2일 내내 38도 왔다갔다",
          "original_form": "2일 내내 38도 왔다갔다",
          "speaker_id": "2",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039779.1.13",
          "form": "딱 독감증상",
          "original_form": "딱 독감증상",
          "speaker_id": "2",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039779.1.14",
          "form": "헉 ᆢ 이차 맞으실때는 괜찮으셨어요?",
          "original_form": "헉 ᆢ 이차 맞으실때는 괜찮으셨어요?",
          "speaker_id": "1",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039779.1.15",
          "form": "그때 면역력 됐는지 근육통만 있더라구요",
          "original_form": "그때 면역력 됐는지 근육통만 있더라구요",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039779.1.16",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039779.1.17",
          "form": "저는 팔만 좀 뻐근했어요",
          "original_form": "저는 팔만 좀 뻐근했어요",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039779.1.18",
          "form": "이차가 부작용 더 세다는데 걱정이예요",
          "original_form": "이차가 부작용 더 세다는데 걱정이예요",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039779.1.19",
          "form": "타이레놀로 이겨냅시다 ㅜㅜ",
          "original_form": "타이레놀로 이겨냅시다 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039779.1.20",
          "form": "넵 다맞으셨다니 부러워요",
          "original_form": "넵 다맞으셨다니 부러워요",
          "speaker_id": "1",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039779.1.21",
          "form": "이차는 최근에 맞으신거예요?",
          "original_form": "이차는 최근에 맞으신거예요?",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039779.1.22",
          "form": "7월에 맞았어뇨",
          "original_form": "7월에 맞았어뇨",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039779.1.23",
          "form": "아 이차를 7월에 맞으셨구나",
          "original_form": "아 이차를 7월에 맞으셨구나",
          "speaker_id": "1",
          "time": "20210903 21:36"
        }
