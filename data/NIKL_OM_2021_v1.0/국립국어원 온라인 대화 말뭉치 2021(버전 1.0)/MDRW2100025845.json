      "utterance": [
        {
          "id": "MDRW2100025845.1.1",
          "form": "안녕하세요! 혹시 쿠키런 하시나요",
          "original_form": "안녕하세요! 혹시 쿠키런 하시나요",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025845.1.2",
          "form": "아니욥",
          "original_form": "아니욥",
          "speaker_id": "2",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025845.1.3",
          "form": "앗 그러시군요 좋아하시는 게임 있으신가요?!",
          "original_form": "앗 그러시군요 좋아하시는 게임 있으신가요?!",
          "speaker_id": "1",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025845.1.4",
          "form": "폰게임 한번도안해밧어요",
          "original_form": "폰게임 한번도안해밧어요",
          "speaker_id": "2",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025845.1.5",
          "form": "게임좋아하시나요?",
          "original_form": "게임좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025845.1.6",
          "form": "저는 가볍게 할 수 있는 게임 좋아해요!",
          "original_form": "저는 가볍게 할 수 있는 게임 좋아해요!",
          "speaker_id": "1",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025845.1.7",
          "form": "몇번 안해봤지만 보드게임카페 가는것도 좋아해요",
          "original_form": "몇번 안해봤지만 보드게임카페 가는것도 좋아해요",
          "speaker_id": "1",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025845.1.8",
          "form": "오호 보드게임카페",
          "original_form": "오호 보드게임카페",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025845.1.9",
          "form": "저는 방탈출 한번도안해바서",
          "original_form": "저는 방탈출 한번도안해바서",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025845.1.10",
          "form": "가보고싶어요",
          "original_form": "가보고싶어요",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025845.1.11",
          "form": "저는 방탈출은 딱 한번 해봤는데",
          "original_form": "저는 방탈출은 딱 한번 해봤는데",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025845.1.12",
          "form": "처음해봐서 익숙하지 않아서 실패했었어요ㅋ큐ㅠㅠ",
          "original_form": "처음해봐서 익숙하지 않아서 실패했었어요ㅋ큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025845.1.13",
          "form": "그래도 재밌었던 기억이 나서 또 가고 싶네요...",
          "original_form": "그래도 재밌었던 기억이 나서 또 가고 싶네요...",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025845.1.14",
          "form": "저도 안가봣지만 실패각이에요ㅋㅋㅋㅋ",
          "original_form": "저도 안가봣지만 실패각이에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025845.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ퍼즐 풀기에 익숙하지 않아서요...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ퍼즐 풀기에 익숙하지 않아서요...",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025845.1.16",
          "form": "ㅋㅋ보드게임 추천해주세요",
          "original_form": "ㅋㅋ보드게임 추천해주세요",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025845.1.17",
          "form": "전 루미큐브하고 펭귄얼음깨기 재밌게 했었어요!",
          "original_form": "전 루미큐브하고 펭귄얼음깨기 재밌게 했었어요!",
          "speaker_id": "1",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100025845.1.18",
          "form": "이름도귀엽네요",
          "original_form": "이름도귀엽네요",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025845.1.19",
          "form": "펭귄얼음깨기ㅋㅋ",
          "original_form": "펭귄얼음깨기ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025845.1.20",
          "form": "게임판이 얼음판처럼 생겼어요ㅋㅋㅋㅋ",
          "original_form": "게임판이 얼음판처럼 생겼어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025845.1.21",
          "form": "판 한가운데에 펭귄을 놓고 각자 망치로 얼음을 깨서",
          "original_form": "판 한가운데에 펭귄을 놓고 각자 망치로 얼음을 깨서",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025845.1.22",
          "form": "펭귄 빠뜨리면 게임이 끝나여",
          "original_form": "펭귄 빠뜨리면 게임이 끝나여",
          "speaker_id": "1",
          "time": "20210901 17:35"
        }
      ]
    }
