      "utterance": [
        {
          "id": "MDRW2100025777.1.1",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025777.1.2",
          "form": "고양이 키우고 싶긴 해요",
          "original_form": "고양이 키우고 싶긴 해요",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025777.1.3",
          "form": "오 고양이파신가봐요",
          "original_form": "오 고양이파신가봐요",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025777.1.4",
          "form": "저는 집에 고양이 두마리 있어요!!",
          "original_form": "저는 집에 고양이 두마리 있어요!!",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025777.1.5",
          "form": "귀엽겠네요",
          "original_form": "귀엽겠네요",
          "speaker_id": "2",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100025777.1.6",
          "form": "아직 키울 자신은 없어요ㅋㅋㅋ",
          "original_form": "아직 키울 자신은 없어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100025777.1.7",
          "form": "ㅎㅎ 저는 고양이의 고짜도 몰랐는데",
          "original_form": "ㅎㅎ 저는 고양이의 고짜도 몰랐는데",
          "speaker_id": "1",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100025777.1.8",
          "form": "길냥이 엄마가 집안에 들어와서",
          "original_form": "길냥이 엄마가 집안에 들어와서",
          "speaker_id": "1",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100025777.1.9",
          "form": "옷장안에 애기 두마리 낳았어요",
          "original_form": "옷장안에 애기 두마리 낳았어요",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100025777.1.10",
          "form": "어떻게 들어와서ㅋㅋ 키워달라고 왔군요",
          "original_form": "어떻게 들어와서ㅋㅋ 키워달라고 왔군요",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025777.1.11",
          "form": "그러게요 ㅜㅜ 임신한채로 돌아다녔나봐요",
          "original_form": "그러게요 ㅜㅜ 임신한채로 돌아다녔나봐요",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025777.1.12",
          "form": "그게 4년전이지만요 ㅋㅋㅋ",
          "original_form": "그게 4년전이지만요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025777.1.13",
          "form": "그 애기들 두마리를 키우고있어요",
          "original_form": "그 애기들 두마리를 키우고있어요",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025777.1.14",
          "form": "고양이들은 책임져주는 사람 알아보나봐요ㅋㅋ",
          "original_form": "고양이들은 책임져주는 사람 알아보나봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025777.1.15",
          "form": "어미냥이는 손을안타더라구요 ㅠ",
          "original_form": "어미냥이는 손을안타더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025777.1.16",
          "form": "고양이는 털 날리는게 제일 단점 같아요 ㅎㅎ",
          "original_form": "고양이는 털 날리는게 제일 단점 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025777.1.17",
          "form": "옷에도 다 붙잖아요",
          "original_form": "옷에도 다 붙잖아요",
          "speaker_id": "2",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100025777.1.18",
          "form": "청소도 자주 해줘야된다고 하더라구요",
          "original_form": "청소도 자주 해줘야된다고 하더라구요",
          "speaker_id": "2",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100025777.1.19",
          "form": "맞아요 저 검은옷을 못입어요 ㅎㅎ",
          "original_form": "맞아요 저 검은옷을 못입어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025777.1.20",
          "form": "음식위에 붙는거는 거의 포기 ㅋㅋㅋ 벌써 많이 먹었을듯요",
          "original_form": "음식위에 붙는거는 거의 포기 ㅋㅋㅋ 벌써 많이 먹었을듯요",
          "speaker_id": "1",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100025777.1.21",
          "form": "강아지나 고양이나 털 문제는 똑같대요",
          "original_form": "강아지나 고양이나 털 문제는 똑같대요",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025777.1.22",
          "form": "식사에 집중을 못한다고ㅎㅎ",
          "original_form": "식사에 집중을 못한다고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025777.1.23",
          "form": "헉 강아지도 그런가봐요",
          "original_form": "헉 강아지도 그런가봐요",
          "speaker_id": "1",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100025777.1.24",
          "form": "저는 강아지 산책이 부러워요",
          "original_form": "저는 강아지 산책이 부러워요",
          "speaker_id": "1",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100025777.1.25",
          "form": "산책하면 강아지 자주 보여요",
          "original_form": "산책하면 강아지 자주 보여요",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100025777.1.26",
          "form": "고양이들은 가방에 데리고 다니던데요",
          "original_form": "고양이들은 가방에 데리고 다니던데요",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025777.1.27",
          "form": "우리집애들은 돼지라 제 어깨가 빠질것같아요 ㅋㅋㅋㅋ",
          "original_form": "우리집애들은 돼지라 제 어깨가 빠질것같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:39"
        }
      ]
    }
