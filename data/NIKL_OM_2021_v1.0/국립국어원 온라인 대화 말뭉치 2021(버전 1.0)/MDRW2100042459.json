      "utterance": [
        {
          "id": "MDRW2100042459.1.1",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042459.1.2",
          "form": "저는 가을이 제일 좋구",
          "original_form": "저는 가을이 제일 좋구",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042459.1.3",
          "form": "여름겨울 중에서 고르라면 겨울 고를래요 ㅋㅋㅋ",
          "original_form": "여름겨울 중에서 고르라면 겨울 고를래요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042459.1.4",
          "form": "더운것보단 추운게 낫지 않나..",
          "original_form": "더운것보단 추운게 낫지 않나..",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042459.1.5",
          "form": "ㅋㅋㅋ맞아요 그치만 전 낮의길이가 너무 중요해요ㅠ",
          "original_form": "ㅋㅋㅋ맞아요 그치만 전 낮의길이가 너무 중요해요ㅠ",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042459.1.6",
          "form": "혹시 직장다니시나요? 아직 학생이신가요?",
          "original_form": "혹시 직장다니시나요? 아직 학생이신가요?",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042459.1.7",
          "form": "아하 야외활동 하는거 좋아하세요??",
          "original_form": "아하 야외활동 하는거 좋아하세요??",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042459.1.8",
          "form": "아녀 대학교 졸업한 백수예요 ㅋㅋㅋ",
          "original_form": "아녀 대학교 졸업한 백수예요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042459.1.9",
          "form": "저 학생일땐 몰랐는데 취직하고나니까.... 겨울엔",
          "original_form": "저 학생일땐 몰랐는데 취직하고나니까.... 겨울엔",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042459.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042459.1.11",
          "form": "해뜨기전에 출근하고... 퇴근하면 해져있거든요",
          "original_form": "해뜨기전에 출근하고... 퇴근하면 해져있거든요",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042459.1.12",
          "form": "ㅠㅠㅠㅠㅠㅠㅠ그래서 겨울이 슬퍼졌어요",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠ그래서 겨울이 슬퍼졌어요",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042459.1.13",
          "form": "아아.. ㅜ 뭔지 알아요",
          "original_form": "아아.. ㅜ 뭔지 알아요",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042459.1.14",
          "form": "학교다닐때 과제가 많아서",
          "original_form": "학교다닐때 과제가 많아서",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042459.1.15",
          "form": "내모든 하루가 일하다끝ㅌ나는기분 ㅠ",
          "original_form": "내모든 하루가 일하다끝ㅌ나는기분 ㅠ",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042459.1.16",
          "form": "학교끝나고 과제하다가 집가고 그랬거든요",
          "original_form": "학교끝나고 과제하다가 집가고 그랬거든요",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042459.1.17",
          "form": "밤에는 정말 6시면 해지니까..",
          "original_form": "밤에는 정말 6시면 해지니까..",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042459.1.18",
          "form": "아아아 네네ㅠㅠㅠ",
          "original_form": "아아아 네네ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042459.1.19",
          "form": "흑흑 뭔기분인지 압니다 저도..",
          "original_form": "흑흑 뭔기분인지 압니다 저도..",
