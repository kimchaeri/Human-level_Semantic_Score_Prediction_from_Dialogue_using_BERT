        {
          "id": "MDRW2100005903.1.23",
          "form": "오오옹",
          "original_form": "오오옹",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100005903.1.24",
          "form": "뭔가",
          "original_form": "뭔가",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100005903.1.25",
          "form": "나는 종교라는게",
          "original_form": "나는 종교라는게",
          "speaker_id": "1",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100005903.1.26",
          "form": "신이정말있다기보다는",
          "original_form": "신이정말있다기보다는",
          "speaker_id": "1",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100005903.1.27",
          "form": "잘못같은거 뒤집어씌울수있고",
          "original_form": "잘못같은거 뒤집어씌울수있고",
          "speaker_id": "1",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100005903.1.28",
          "form": "원망의대상을 신으로 두고",
          "original_form": "원망의대상을 신으로 두고",
          "speaker_id": "1",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100005903.1.29",
          "form": "잘되면 신덕",
          "original_form": "잘되면 신덕",
          "speaker_id": "1",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100005903.1.30",
          "form": "못되면 신이벌준거처럼",
          "original_form": "못되면 신이벌준거처럼",
          "speaker_id": "1",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100005903.1.31",
          "form": "뭔가 자신의 능력때문에일어난일이라는걸",
          "original_form": "뭔가 자신의 능력때문에일어난일이라는걸",
          "speaker_id": "1",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100005903.1.32",
          "form": "그런 느낌 좀 있지 ㅋㅋㅋ",
          "original_form": "그런 느낌 좀 있지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100005903.1.33",
          "form": "회피하려고 만든",
          "original_form": "회피하려고 만든",
          "speaker_id": "1",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100005903.1.34",
