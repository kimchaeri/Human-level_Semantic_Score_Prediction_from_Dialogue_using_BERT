      "utterance": [
        {
          "id": "MDRW2100014658.1.1",
          "form": "마카롱 좋아하세요?",
          "original_form": "마카롱 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014658.1.2",
          "form": "닉넴이 눈에 들어오는지 다들 똑같은 질문하시네요ㅎ",
          "original_form": "닉넴이 눈에 들어오는지 다들 똑같은 질문하시네요ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014658.1.3",
          "form": "샌드위치 좋아하세요?",
          "original_form": "샌드위치 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014658.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ 네 좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 네 좋아해요",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014658.1.5",
          "form": "오늘은 BELT샌드위치 먹었습니다",
          "original_form": "오늘은 BELT샌드위치 먹었습니다",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014658.1.6",
          "form": "어디꺼드셨나요?ㅎ",
          "original_form": "어디꺼드셨나요?ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014658.1.7",
          "form": "전 사실 마카롱 잘안먹어요",
          "original_form": "전 사실 마카롱 잘안먹어요",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014658.1.8",
          "form": "뚜레쥬르요",
          "original_form": "뚜레쥬르요",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014658.1.9",
          "form": "반전이네요ㅋㅋ",
          "original_form": "반전이네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014658.1.10",
          "form": "그럼 달리 좋아하는 디저트 있으세요?",
          "original_form": "그럼 달리 좋아하는 디저트 있으세요?",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014658.1.11",
          "form": "저 크림치즈 들어간거나 밀크크레이프요ㄹ",
          "original_form": "저 크림치즈 들어간거나 밀크크레이프요ㄹ",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014658.1.12",
          "form": "와 맛잘알",
          "original_form": "와 맛잘알",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014658.1.13",
          "form": "전 크림치즈+블루베리 조합 좋아해요",
          "original_form": "전 크림치즈+블루베리 조합 좋아해요",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014658.1.14",
          "form": "스벅 블루베리치즈케익도 좋아하시겠어요",
          "original_form": "스벅 블루베리치즈케익도 좋아하시겠어요",
          "speaker_id": "2",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014658.1.15",
          "form": "앗 그건 제 친구 최애 케이크인데요ㅋㅋ",
          "original_form": "앗 그건 제 친구 최애 케이크인데요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014658.1.16",
          "form": "그래서 얼떨결에 같이 가끔 먹어요",
          "original_form": "그래서 얼떨결에 같이 가끔 먹어요",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014658.1.17",
          "form": "홀케이크로...",
          "original_form": "홀케이크로...",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014658.1.18",
          "form": "예전에 먹었을땐 맛났는데 얼마전에먹으니 달더라구요ㅎ",
          "original_form": "예전에 먹었을땐 맛났는데 얼마전에먹으니 달더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014658.1.19",
          "form": "저녁은 샌드위치로 끝인건가요?",
          "original_form": "저녁은 샌드위치로 끝인건가요?",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014658.1.20",
          "form": "네 날씨 때문인지 입맛이 없네요",
          "original_form": "네 날씨 때문인지 입맛이 없네요",
          "speaker_id": "1",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014658.1.21",
          "form": "저도 며칠전부터 그러네요",
          "original_form": "저도 며칠전부터 그러네요",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014658.1.22",
          "form": "선선하니 입맛이 땡겨야되는게 정상인데",
          "original_form": "선선하니 입맛이 땡겨야되는게 정상인데",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014658.1.23",
          "form": "그니까요 오히려 한여름보다 입맛이 없어져서 요상해요",
          "original_form": "그니까요 오히려 한여름보다 입맛이 없어져서 요상해요",
          "speaker_id": "1",
          "time": "20210825 20:39"
        }
      ]
