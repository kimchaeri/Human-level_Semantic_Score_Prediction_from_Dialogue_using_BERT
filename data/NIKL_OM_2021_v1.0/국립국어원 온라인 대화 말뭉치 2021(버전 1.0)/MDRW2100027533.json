      "utterance": [
        {
          "id": "MDRW2100027533.1.1",
          "form": "다시 말해주세요 ㅠㅠ",
          "original_form": "다시 말해주세요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027533.1.2",
          "form": "그래서 진짜 아오 하면서 헤어졌어요 ㅋㅋㅋ",
          "original_form": "그래서 진짜 아오 하면서 헤어졌어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027533.1.3",
          "form": "못봤어요 ㅠㅠ",
          "original_form": "못봤어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027533.1.4",
          "form": "손이 빠르시네요!!ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "손이 빠르시네요!!ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027533.1.5",
          "form": "아 입만 터는 놈이었어요 ㅡㅡ",
          "original_form": "아 입만 터는 놈이었어요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027533.1.6",
          "form": "진짜 생각할수록 열받아요",
          "original_form": "진짜 생각할수록 열받아요",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027533.1.7",
          "form": "헤어질때도 잘 털더라고요",
          "original_form": "헤어질때도 잘 털더라고요",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027533.1.8",
          "form": "헐랭",
          "original_form": "헐랭",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027533.1.9",
          "form": "허언증이였군요",
          "original_form": "허언증이였군요",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027533.1.10",
          "form": "남들은 걔가 진짜 다정한 줄 알았어요 저는 맨날 구박하는 줄 알고 ㅋㅋ",
          "original_form": "남들은 걔가 진짜 다정한 줄 알았어요 저는 맨날 구박하는 줄 알고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027533.1.11",
          "form": "헐랭 그런거 먼지알죠",
          "original_form": "헐랭 그런거 먼지알죠",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027533.1.12",
          "form": "구박이 아니라 답답해서 지르는 건디..",
          "original_form": "구박이 아니라 답답해서 지르는 건디..",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027533.1.13",
          "form": "겉만보고 판단하는게 젤나빠요",
          "original_form": "겉만보고 판단하는게 젤나빠요",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027533.1.14",
          "form": "오히려 해준건 제가 더 많고 뭐 알아보고 하자하는건 다 저고 ㅡㅡ",
          "original_form": "오히려 해준건 제가 더 많고 뭐 알아보고 하자하는건 다 저고 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027533.1.15",
          "form": "헐 먼지알죠 ㅠㅠㅠ",
          "original_form": "헐 먼지알죠 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027533.1.16",
          "form": "걘 진짜 인스타 보면 사랑꾼이에요 ㅋㅋㅋ 에효..",
          "original_form": "걘 진짜 인스타 보면 사랑꾼이에요 ㅋㅋㅋ 에효..",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027533.1.17",
          "form": "말만 번지르게하는놈...",
          "original_form": "말만 번지르게하는놈...",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027533.1.18",
          "form": "저는 인스타 안하거든요 ㅋㅋㅋ",
          "original_form": "저는 인스타 안하거든요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027533.1.19",
          "form": "에이...보여쥬기식 사랑은 너무 싫어요",
          "original_form": "에이...보여쥬기식 사랑은 너무 싫어요",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027533.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027533.1.21",
          "form": "헐진짜요??",
          "original_form": "헐진짜요??",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027533.1.22",
          "form": "그니까 여론에서 제가 막 밀리고? ㅋㅋㅋㅋ",
          "original_form": "그니까 여론에서 제가 막 밀리고? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027533.1.23",
          "form": "저는 인스타 구곁용으로 해요!!",
          "original_form": "저는 인스타 구곁용으로 해요!!",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027533.1.24",
          "form": "네 저 인스타 안해요! 귀찮아성",
          "original_form": "네 저 인스타 안해요! 귀찮아성",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027533.1.25",
          "form": "맛집 좋은 카페 이런거보ㅡ보ㅓ요 ㅋㅋ",
          "original_form": "맛집 좋은 카페 이런거보ㅡ보ㅓ요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027533.1.26",
          "form": "저 만들었다가 비번 까먹었어요ㅠㅠ",
          "original_form": "저 만들었다가 비번 까먹었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027533.1.27",
          "form": "저 그래서 아직도",
          "original_form": "저 그래서 아직도",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027533.1.28",
          "form": "네이버 블로그 보고 맛집 찾아요",
          "original_form": "네이버 블로그 보고 맛집 찾아요",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027533.1.29",
          "form": "ㅋㅋㅋㅋㅋ애들이 다 빵터져요 식당이 뭔가 가족단위 픽이라고",
          "original_form": "ㅋㅋㅋㅋㅋ애들이 다 빵터져요 식당이 뭔가 가족단위 픽이라고",
          "speaker_id": "1",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100027533.1.30",
          "form": "약간 인스타 안하면 불편하긴 한데 편하긴 해요",
          "original_form": "약간 인스타 안하면 불편하긴 한데 편하긴 해요",
          "speaker_id": "1",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100027533.1.31",
          "form": "신경 안써도 되는 그런거는 편해용",
          "original_form": "신경 안써도 되는 그런거는 편해용",
          "speaker_id": "1",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027533.1.32",
          "form": "근데 럽스타 이런건 못하죠 아무래도ㅠㅠ",
          "original_form": "근데 럽스타 이런건 못하죠 아무래도ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:32"
        }
      ]
    }
