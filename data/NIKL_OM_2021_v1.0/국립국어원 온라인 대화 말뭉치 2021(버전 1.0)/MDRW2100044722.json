      "utterance": [
        {
          "id": "MDRW2100044722.1.1",
          "form": "레샤 먹으면 입안이 싹 정리되는 기분이에요",
          "original_form": "레샤 먹으면 입안이 싹 정리되는 기분이에요",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044722.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044722.1.3",
          "form": "애플민트도 좀 그렇지 않나요",
          "original_form": "애플민트도 좀 그렇지 않나요",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044722.1.4",
          "form": "헉 맞아요 그래서 좋아해요 ㅋㅋㅋ",
          "original_form": "헉 맞아요 그래서 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044722.1.5",
          "form": "근데 레샤가 상큼! 한 맛이 있어서 레샤를 쪼끔더 좋아해요",
          "original_form": "근데 레샤가 상큼! 한 맛이 있어서 레샤를 쪼끔더 좋아해요",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044722.1.6",
          "form": "바람과함께사라지다는 어떠세요?!",
          "original_form": "바람과함께사라지다는 어떠세요?!",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044722.1.7",
          "form": "당연히 좋아하죠!",
          "original_form": "당연히 좋아하죠!",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044722.1.8",
          "form": "바함사 사빠딸 뉴치케",
          "original_form": "바함사 사빠딸 뉴치케",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044722.1.9",
          "form": "얘들은 세트예요",
          "original_form": "얘들은 세트예요",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044722.1.10",
          "form": "저랑 잘맞으시네요",
          "original_form": "저랑 잘맞으시네요",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044722.1.11",
          "form": "저 세개 제가 젤 좋아해요",
          "original_form": "저 세개 제가 젤 좋아해요",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044722.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ거의 저거중에 하나 좋아하시면 다들 그렇더라구요 저희는 참입맛이네요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ거의 저거중에 하나 좋아하시면 다들 그렇더라구요 저희는 참입맛이네요!",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044722.1.13",
          "form": "셋이 젤 맛있어요!",
          "original_form": "셋이 젤 맛있어요!",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044722.1.14",
          "form": "새로나온 꼬북칩 맛 드셔보셨나요",
          "original_form": "새로나온 꼬북칩 맛 드셔보셨나요",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044722.1.15",
          "form": "아뇨 무슨 맛이죰!",
          "original_form": "아뇨 무슨 맛이죰!",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044722.1.16",
          "form": "이상해요",
          "original_form": "이상해요",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044722.1.17",
          "form": "드시지마세요",
          "original_form": "드시지마세요",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044722.1.18",
          "form": "머야!ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "머야!ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044722.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044722.1.20",
          "form": "클날뻔했네요 감사합니다",
          "original_form": "클날뻔했네요 감사합니다",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044722.1.21",
          "form": "아몬드봉봉은 어떠세요",
          "original_form": "아몬드봉봉은 어떠세요",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044722.1.22",
          "form": "있으면 먹지만 제가 담지는 않아요",
          "original_form": "있으면 먹지만 제가 담지는 않아요",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044722.1.23",
          "form": "오 아몬드봉봉 궁금했는데 안먹어야겠어요",
          "original_form": "오 아몬드봉봉 궁금했는데 안먹어야겠어요",
          "speaker_id": "2",
          "time": "20210907 23:58"
        }
      ]
    }
