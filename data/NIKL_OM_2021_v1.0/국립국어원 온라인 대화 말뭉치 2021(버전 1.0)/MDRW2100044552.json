      "utterance": [
        {
          "id": "MDRW2100044552.1.1",
          "form": "최근에 산건 없는데",
          "original_form": "최근에 산건 없는데",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044552.1.2",
          "form": "아이패드 사고싶어요",
          "original_form": "아이패드 사고싶어요",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044552.1.3",
          "form": "헉 프로요?",
          "original_form": "헉 프로요?",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044552.1.4",
          "form": "네 프로나 에어 살까 보고 있어요",
          "original_form": "네 프로나 에어 살까 보고 있어요",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044552.1.5",
          "form": "좋당 원래도 태블릿 쓰셧나요",
          "original_form": "좋당 원래도 태블릿 쓰셧나요",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044552.1.6",
          "form": "아 아뇨",
          "original_form": "아 아뇨",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044552.1.7",
          "form": "노트북 있어서 탭 욕심 없었는데",
          "original_form": "노트북 있어서 탭 욕심 없었는데",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044552.1.8",
          "form": "최근에 필기할 일이 많아지니 눈길이 가더라고요",
          "original_form": "최근에 필기할 일이 많아지니 눈길이 가더라고요",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044552.1.9",
          "form": "아항 대학생이요?",
          "original_form": "아항 대학생이요?",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044552.1.10",
          "form": "아뇨 직장인인데",
          "original_form": "아뇨 직장인인데",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044552.1.11",
          "form": "공부하고 있는 게 있어서요",
          "original_form": "공부하고 있는 게 있어서요",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044552.1.12",
          "form": "아아~! ㅋㅋㅋ 얼른 사셔야겟네요",
          "original_form": "아아~! ㅋㅋㅋ 얼른 사셔야겟네요",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044552.1.13",
          "form": "네 추석 보너스 받음 사려고 대기중이예요!!",
          "original_form": "네 추석 보너스 받음 사려고 대기중이예요!!",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044552.1.14",
          "form": "ㅋㅋㅋㅋ 두근둑은",
          "original_form": "ㅋㅋㅋㅋ 두근둑은",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044552.1.15",
          "form": "네 두근두근",
          "original_form": "네 두근두근",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044552.1.16",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044552.1.17",
          "form": "구매하시거나 구매욕구 있으신 것 있나요?",
          "original_form": "구매하시거나 구매욕구 있으신 것 있나요?",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044552.1.18",
          "form": "저요 이쁜자켓 사고싶어요",
          "original_form": "저요 이쁜자켓 사고싶어요",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044552.1.19",
          "form": "오 어떤 스타일 자켓이요?",
          "original_form": "오 어떤 스타일 자켓이요?",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044552.1.20",
          "form": "걍 가을자켓이요 기본 ㅋㅋㅋ베이지? 브라운?",
          "original_form": "걍 가을자켓이요 기본 ㅋㅋㅋ베이지? 브라운?",
          "speaker_id": "1",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044552.1.21",
          "form": "아 기본 자켓이 제일이죠",
          "original_form": "아 기본 자켓이 제일이죠",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044552.1.22",
          "form": "ㅋㅋㅋ ㅜㅜ 물욕이끝이없네요",
          "original_form": "ㅋㅋㅋ ㅜㅜ 물욕이끝이없네요",
          "speaker_id": "1",
          "time": "20210907 23:20"
        }
