        {
          "id": "MDRW2100021464.1.1",
          "form": "사거리차이가",
          "original_form": "사거리차이가",
          "speaker_id": "2",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021464.1.2",
          "form": "나는것도있고",
          "original_form": "나는것도있고",
          "speaker_id": "2",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021464.1.3",
          "form": "베인은 왜 꺼낸거지",
          "original_form": "베인은 왜 꺼낸거지",
          "speaker_id": "1",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021464.1.4",
          "form": "여태 안하다가 ㅋㅋㅋ",
          "original_form": "여태 안하다가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021464.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021464.1.6",
          "form": "직스나와서 한듯",
          "original_form": "직스나와서 한듯",
          "speaker_id": "2",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021464.1.7",
          "form": "사실 양대인이",
          "original_form": "사실 양대인이",
          "speaker_id": "1",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021464.1.8",
          "form": "시즌중 경질당하려고",
          "original_form": "시즌중 경질당하려고",
          "speaker_id": "1",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021464.1.9",
          "form": "2대떡",
          "original_form": "2대떡",
          "speaker_id": "2",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021464.1.10",
          "form": "칸나 키운다음에",
          "original_form": "칸나 키운다음에",
          "speaker_id": "1",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021464.1.11",
          "form": "담원가서 개터는구만",
          "original_form": "담원가서 개터는구만",
          "speaker_id": "1",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021464.1.12",
          "form": "패패승승승",
          "original_form": "패패승승승",
          "speaker_id": "2",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021464.1.13",
          "form": "아니면답없는데",
          "original_form": "아니면답없는데",
          "speaker_id": "2",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021464.1.14",
          "form": "어케하노",
          "original_form": "어케하노",
          "speaker_id": "2",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021464.1.15",
          "form": "담판 그웬 또 하려나",
          "original_form": "담판 그웬 또 하려나",
          "speaker_id": "1",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021464.1.16",
          "form": "설마",
          "original_form": "설마",
          "speaker_id": "2",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100021464.1.17",
          "form": "상체차이 오지게난다",
          "original_form": "상체차이 오지게난다",
          "speaker_id": "1",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100021464.1.18",
          "form": "그건맞지",
          "original_form": "그건맞지",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021464.1.19",
          "form": "근데 롤챔은 질거알았기땜네",
          "original_form": "근데 롤챔은 질거알았기땜네",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021464.1.20",
