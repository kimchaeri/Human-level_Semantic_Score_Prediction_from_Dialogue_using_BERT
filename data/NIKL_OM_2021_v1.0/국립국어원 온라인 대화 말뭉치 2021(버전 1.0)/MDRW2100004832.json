      "utterance": [
        {
          "id": "MDRW2100004832.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004832.1.2",
          "form": "주중에는 출퇴근하느라 바빠서 잘 못하고",
          "original_form": "주중에는 출퇴근하느라 바빠서 잘 못하고",
          "speaker_id": "2",
          "time": "20210821 18:55"
        },
        {
          "id": "MDRW2100004832.1.3",
          "form": "주말에는 좀 해요",
          "original_form": "주말에는 좀 해요",
          "speaker_id": "2",
          "time": "20210821 18:55"
        },
        {
          "id": "MDRW2100004832.1.4",
          "form": "분담 잘하시네여",
          "original_form": "분담 잘하시네여",
          "speaker_id": "1",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004832.1.5",
          "form": "직장핑계로 주중에는 잘 안하게 되더라고요",
          "original_form": "직장핑계로 주중에는 잘 안하게 되더라고요",
          "speaker_id": "2",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004832.1.6",
          "form": "네 시간 핑계로",
          "original_form": "네 시간 핑계로",
          "speaker_id": "1",
          "time": "20210821 18:57"
        },
        {
          "id": "MDRW2100004832.1.7",
          "form": "로봇청소기써서 그래도 청소는 좀 편해요",
          "original_form": "로봇청소기써서 그래도 청소는 좀 편해요",
          "speaker_id": "2",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004832.1.8",
          "form": "편리하게 사시네여",
          "original_form": "편리하게 사시네여",
          "speaker_id": "1",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004832.1.9",
          "form": "집에서 요리같은거 많이 해요?",
          "original_form": "집에서 요리같은거 많이 해요?",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004832.1.10",
          "form": "요똥이에요.",
          "original_form": "요똥이에요.",
          "speaker_id": "1",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004832.1.11",
          "form": "님은",
          "original_form": "님은",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100004832.1.12",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100004832.1.13",
          "form": "저는 자취했어서 생계형 요리 해요 ㅎㅎ",
          "original_form": "저는 자취했어서 생계형 요리 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100004832.1.14",
          "form": "그래도 요리가 되네요",
          "original_form": "그래도 요리가 되네요",
          "speaker_id": "1",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100004832.1.15",
          "form": "살기위해서 요리했죠 ㅎㅎ",
          "original_form": "살기위해서 요리했죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100004832.1.16",
          "form": "패스트푸드랑 사먹는거에 지쳐서 ㅎ",
          "original_form": "패스트푸드랑 사먹는거에 지쳐서 ㅎ",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100004832.1.17",
          "form": "질리셨구나",
          "original_form": "질리셨구나",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100004832.1.18",
