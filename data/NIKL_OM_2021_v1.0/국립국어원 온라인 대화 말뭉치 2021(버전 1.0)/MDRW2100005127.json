      "utterance": [
        {
          "id": "MDRW2100005127.1.1",
          "form": "몰디브먹으러 모히또가좍~",
          "original_form": "몰디브먹으러 모히또가좍~",
          "speaker_id": "2",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005127.1.2",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005127.1.3",
          "form": "",
          "original_form": "{censored}",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005127.1.4",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005127.1.5",
          "form": "숙소바닥이 유리라서",
          "original_form": "숙소바닥이 유리라서",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005127.1.6",
          "form": "침대누버서 바다볼수있대",
          "original_form": "침대누버서 바다볼수있대",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005127.1.7",
          "form": "물곡이 구경하기",
          "original_form": "물곡이 구경하기",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005127.1.8",
          "form": "찰랑~",
          "original_form": "찰랑~",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005127.1.9",
          "form": "난 몰디브 별빛바다?",
          "original_form": "난 몰디브 별빛바다?",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005127.1.10",
          "form": "캬",
          "original_form": "캬",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005127.1.11",
          "form": "별빛바다 하니",
          "original_form": "별빛바다 하니",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005127.1.12",
          "form": "우유니사막",
          "original_form": "우유니사막",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005127.1.13",
          "form": "퍼렁퍼렁반짝이 한번은 보고싶네..",
          "original_form": "퍼렁퍼렁반짝이 한번은 보고싶네..",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005127.1.14",
          "form": "볼리비아도 가보고싶다..",
          "original_form": "볼리비아도 가보고싶다..",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005127.1.15",
          "form": "모아나 생각난다",
          "original_form": "모아나 생각난다",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005127.1.16",
          "form": "하우 퐐이 고오~~~",
          "original_form": "하우 퐐이 고오~~~",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005127.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ볼리비아가 어디지?하고 검색하니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ볼리비아가 어디지?하고 검색하니까",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005127.1.18",
          "form": "영상미 짱!",
          "original_form": "영상미 짱!",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005127.1.19",
          "form": "우유니사막 나옴",
          "original_form": "우유니사막 나옴",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005127.1.20",
          "form": "참나",
          "original_form": "참나",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005127.1.21",
          "form": "모아나 배경지도 좋다",
          "original_form": "모아나 배경지도 좋다",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005127.1.22",
          "form": "하와이 아녀?",
          "original_form": "하와이 아녀?",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005127.1.23",
          "form": "ㅋㄱㄱㅋㄱㄱㅋㄱ 우유니사막 한때 유우니사막이라고 불렀던거 생걱난다..",
          "original_form": "ㅋㄱㄱㅋㄱㄱㅋㄱ 우유니사막 한때 유우니사막이라고 불렀던거 생걱난다..",
          "speaker_id": "2",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005127.1.24",
          "form": "잘몰러",
          "original_form": "잘몰러",
          "speaker_id": "2",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005127.1.25",
          "form": "하와이하니 코코넛크랩? 먹어보고싶어",
          "original_form": "하와이하니 코코넛크랩? 먹어보고싶어",
          "speaker_id": "2",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005127.1.26",
          "form": "코코넛향 나는게",
          "original_form": "코코넛향 나는게",
          "speaker_id": "2",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005127.1.27",
          "form": "맛잇겟다맛잇겟다",
          "original_form": "맛잇겟다맛잇겟다",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005127.1.28",
          "form": "",
          "original_form": "{censored}",
          "speaker_id": "2",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005127.1.29",
          "form": "헐 진짜?",
          "original_form": "헐 진짜?",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005127.1.30",
          "form": "코코넛향 나는구나",
          "original_form": "코코넛향 나는구나",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005127.1.31",
          "form": "그렇다고들었워 풍미가 좋대",
          "original_form": "그렇다고들었워 풍미가 좋대",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005127.1.32",
          "form": "맛잇겟다",
          "original_form": "맛잇겟다",
          "speaker_id": "1",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005127.1.33",
          "form": "커다랗고 통통하니",
          "original_form": "커다랗고 통통하니",
          "speaker_id": "1",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005127.1.34",
          "form": "살이 많아보여",
          "original_form": "살이 많아보여",
          "speaker_id": "1",
          "time": "20210821 20:45"
