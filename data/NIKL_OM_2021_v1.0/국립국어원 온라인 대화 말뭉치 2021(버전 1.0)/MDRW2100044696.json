      "utterance": [
        {
          "id": "MDRW2100044696.1.1",
          "form": "쌀 안 들어가고 밀가루 조금에 고기가 있을수도 없을 수도 있다?",
          "original_form": "쌀 안 들어가고 밀가루 조금에 고기가 있을수도 없을 수도 있다?",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044696.1.2",
          "form": "매운 음식이래나요?",
          "original_form": "매운 음식이래나요?",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044696.1.3",
          "form": "이래",
          "original_form": "이래",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044696.1.4",
          "form": "랬",
          "original_form": "랬",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044696.1.5",
          "form": "고기는 들어가요!",
          "original_form": "고기는 들어가요!",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044696.1.6",
          "form": "두발짐승일수도 있고 다른 걸수도 있습니다",
          "original_form": "두발짐승일수도 있고 다른 걸수도 있습니다",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044696.1.7",
          "form": "ㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044696.1.8",
          "form": "고기가 안 들어가는 게 보편적인 음식인가요?",
          "original_form": "고기가 안 들어가는 게 보편적인 음식인가요?",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044696.1.9",
          "form": "아뇨 고기가 핵심입니다!",
          "original_form": "아뇨 고기가 핵심입니다!",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044696.1.10",
          "form": "이럴수가 뭘까요",
          "original_form": "이럴수가 뭘까요",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044696.1.11",
          "form": "힌트 드릴까요??",
          "original_form": "힌트 드릴까요??",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044696.1.12",
          "form": "튀기는 요리인가요?",
          "original_form": "튀기는 요리인가요?",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044696.1.13",
          "form": "넵!!",
          "original_form": "넵!!",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044696.1.14",
          "form": "아앗 아직 혼자 맞춰보겠어요",
          "original_form": "아앗 아직 혼자 맞춰보겠어요",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044696.1.15",
          "form": "혹시 중식인가요?",
          "original_form": "혹시 중식인가요?",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044696.1.16",
          "form": "아뇨 일식인거같아요",
          "original_form": "아뇨 일식인거같아요",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044696.1.17",
          "form": "앗 근데 쌀이 안 들어간다니",
          "original_form": "앗 근데 쌀이 안 들어간다니",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044696.1.18",
          "form": "아 혹시 쌀이랑 곁들여 먹나요?",
          "original_form": "아 혹시 쌀이랑 곁들여 먹나요?",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044696.1.19",
          "form": "네 조금 곁들여서 나와요",
          "original_form": "네 조금 곁들여서 나와요",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044696.1.20",
          "form": "알겠다!!!! 정답 돈카츠!!",
          "original_form": "알겠다!!!! 정답 돈카츠!!",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044696.1.21",
          "form": "넵 맞아요!",
          "original_form": "넵 맞아요!",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044696.1.22",
          "form": "고기가 땡겨서 간만에 먹었어요 ㅋㅋㅋㅋ",
          "original_form": "고기가 땡겨서 간만에 먹었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044696.1.23",
          "form": "대박 맛있죠 돈카츠 캬",
          "original_form": "대박 맛있죠 돈카츠 캬",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044696.1.24",
          "form": "튀기면 뭐든 다 맛있긴 하지만요 ㅋㅋ",
          "original_form": "튀기면 뭐든 다 맛있긴 하지만요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044696.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 마성의 조리법이예요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 마성의 조리법이예요",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044696.1.26",
          "form": "무슨 돈카츠 드셨나요",
          "original_form": "무슨 돈카츠 드셨나요",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044696.1.27",
          "form": "경양식이요!",
          "original_form": "경양식이요!",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044696.1.28",
          "form": "추억의 맛이에요 ㅋㅋㅋ",
          "original_form": "추억의 맛이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044696.1.29",
          "form": "저도 경양식 좋아해요ㅋㅋㅋㅋ",
          "original_form": "저도 경양식 좋아해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044696.1.30",
          "form": "일식 돈카츠랑 또 다른",
          "original_form": "일식 돈카츠랑 또 다른",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044696.1.31",
          "form": "정겨운 맛이 있어요",
          "original_form": "정겨운 맛이 있어요",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044696.1.32",
          "form": "그쵸 ㅋㅋㅋ 가끔 생각자뇨",
          "original_form": "그쵸 ㅋㅋㅋ 가끔 생각자뇨",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044696.1.33",
          "form": "생각나요",
          "original_form": "생각나요",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044696.1.34",
          "form": "그 엄청 뭉뚝한 칼로",
          "original_form": "그 엄청 뭉뚝한 칼로",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044696.1.35",
          "form": "속속속속 썰어먹는 재미도 좋아요ㅋㅋㅋ",
          "original_form": "속속속속 썰어먹는 재미도 좋아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044696.1.36",
          "form": "거기 나오느 밥 더 달라고 하면 준다던데",
          "original_form": "거기 나오느 밥 더 달라고 하면 준다던데",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044696.1.37",
          "form": "알고계시나요?!",
          "original_form": "알고계시나요?!",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044696.1.38",
          "form": "아 그런가요??",
          "original_form": "아 그런가요??",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044696.1.39",
          "form": "요즘 경양식들은 아예 왕돈까스라고 팔아서",
          "original_form": "요즘 경양식들은 아예 왕돈까스라고 팔아서",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044696.1.40",
          "form": "그것만 먹기에도 배부르더라구요 ㅋㅋㅋㅋ",
          "original_form": "그것만 먹기에도 배부르더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044696.1.41",
          "form": "사실 그것도 그래요ㅋㅋㅋㅋ요즘 돈까스들 엄청 커서",
          "original_form": "사실 그것도 그래요ㅋㅋㅋㅋ요즘 돈까스들 엄청 커서",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044696.1.42",
          "form": "그거 먹는데도 시간 다 가요",
          "original_form": "그거 먹는데도 시간 다 가요",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044696.1.43",
          "form": "분식집에서 드셨나요",
          "original_form": "분식집에서 드셨나요",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044696.1.44",
          "form": "네네 무슨 왕돈까스 집 있어서 거기 갔어요",
          "original_form": "네네 무슨 왕돈까스 집 있어서 거기 갔어요",
          "speaker_id": "1",
          "time": "20210907 23:38"
        }
      ]
