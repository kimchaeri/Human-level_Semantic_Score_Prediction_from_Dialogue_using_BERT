      "utterance": [
        {
          "id": "MDRW2100034431.1.1",
          "form": "여행 좋아하세요",
          "original_form": "여행 좋아하세요",
          "speaker_id": "2",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100034431.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100034431.1.3",
          "form": "여행좋아해요~ 국내해외다좋아용",
          "original_form": "여행좋아해요~ 국내해외다좋아용",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100034431.1.4",
          "form": "어디가 가장 좋으셧어요?",
          "original_form": "어디가 가장 좋으셧어요?",
          "speaker_id": "2",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100034431.1.5",
          "form": "저는 국내는 여수 해외는 미국이요 ㅋㅋ",
          "original_form": "저는 국내는 여수 해외는 미국이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100034431.1.6",
          "form": "오오 미국은 안가봣어요 아직",
          "original_form": "오오 미국은 안가봣어요 아직",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100034431.1.7",
          "form": "어떤 점이 좋으셧어요?",
          "original_form": "어떤 점이 좋으셧어요?",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100034431.1.8",
          "form": "일단 날씨가 너무좋았고 그 특유의 바이브도좋았어요! 음식도 제스타일이에요 느끼한거좋아해서요 ㅋㅋ",
          "original_form": "일단 날씨가 너무좋았고 그 특유의 바이브도좋았어요! 음식도 제스타일이에요 느끼한거좋아해서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100034431.1.9",
          "form": "얼마나 머무셨어요?",
          "original_form": "얼마나 머무셨어요?",
          "speaker_id": "2",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100034431.1.10",
          "form": "저는 동부에서6개월 서부에서6개월이요 ㅋㅋ",
          "original_form": "저는 동부에서6개월 서부에서6개월이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100034431.1.11",
          "form": "와 일년동안 여행하신거예요??",
          "original_form": "와 일년동안 여행하신거예요??",
          "speaker_id": "2",
          "time": "20210828 11:55"
        },
        {
          "id": "MDRW2100034431.1.12",
          "form": "대박부럽",
          "original_form": "대박부럽",
          "speaker_id": "2",
          "time": "20210828 11:55"
        },
        {
          "id": "MDRW2100034431.1.13",
          "form": "네~ 아 한번에 1년간건아니구 6개월 6개월 따로따로갔다왔어요 ㅎㅎ",
          "original_form": "네~ 아 한번에 1년간건아니구 6개월 6개월 따로따로갔다왔어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100034431.1.14",
          "form": "와 6개월도 충분히 길죠",
          "original_form": "와 6개월도 충분히 길죠",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034431.1.15",
          "form": "대박부럽다",
          "original_form": "대박부럽다",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034431.1.16",
          "form": "그럼 미국 여행하면서 제일 좋았던 곳은 어디예요?",
          "original_form": "그럼 미국 여행하면서 제일 좋았던 곳은 어디예요?",
          "speaker_id": "2",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100034431.1.17",
          "form": "아녀요~ ㅋㅋㅋ 저는 서부가좋았어요! 특히 캘리포니아쪽이요~",
          "original_form": "아녀요~ ㅋㅋㅋ 저는 서부가좋았어요! 특히 캘리포니아쪽이요~",
          "speaker_id": "1",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100034431.1.18",
          "form": "캘리포니아 ㅠㅠ 모두가 해피해피하다는 그곳 ㅋㅋㅋㅋ",
          "original_form": "캘리포니아 ㅠㅠ 모두가 해피해피하다는 그곳 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100034431.1.19",
          "form": "코로나 종식되면 가보고싶어요 진짜 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "코로나 종식되면 가보고싶어요 진짜 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100034431.1.20",
          "form": "앜ㅋㅋ해피해피~",
          "original_form": "앜ㅋㅋ해피해피~",
          "speaker_id": "1",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100034431.1.21",
          "form": "저두요! 또가고싶네요 ㅠㅠ",
          "original_form": "저두요! 또가고싶네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100034431.1.22",
          "form": "혼자 가셧어요?",
          "original_form": "혼자 가셧어요?",
          "speaker_id": "2",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100034431.1.23",
          "form": "아뇨 ㅋㅋ 미국은 가족이서 갔어요~",
          "original_form": "아뇨 ㅋㅋ 미국은 가족이서 갔어요~",
          "speaker_id": "1",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100034431.1.24",
          "form": "와 가족여행으로 6개월!!",
          "original_form": "와 가족여행으로 6개월!!",
          "speaker_id": "2",
          "time": "20210828 13:55"
        },
        {
          "id": "MDRW2100034431.1.25",
          "form": "진짜 화목한 가족인가봐요",
          "original_form": "진짜 화목한 가족인가봐요",
          "speaker_id": "2",
          "time": "20210828 13:55"
        },
        {
          "id": "MDRW2100034431.1.26",
          "form": "가족 전체가 평생갈 추억이겟네요",
          "original_form": "가족 전체가 평생갈 추억이겟네요",
          "speaker_id": "2",
          "time": "20210828 13:55"
        },
        {
          "id": "MDRW2100034431.1.27",
          "form": "앜ㅋㅋㅋㅋㅋ 자주 다퉈서 화목하다고 말하기엔 좀 부끄럽네요 ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ 자주 다퉈서 화목하다고 말하기엔 좀 부끄럽네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:56"
        },
        {
          "id": "MDRW2100034431.1.28",
          "form": "마쟈요! 추억은 엄청쌓앗죠 ㅋㅋㅋ",
          "original_form": "마쟈요! 추억은 엄청쌓앗죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:56"
        },
        {
          "id": "MDRW2100034431.1.29",
          "form": "자주다퉈도 금방화해할 수 잇는 사이니까 6개월 가족여행도 가능한거 아닐까요 ㅋㅋㅋㅋㅋ",
          "original_form": "자주다퉈도 금방화해할 수 잇는 사이니까 6개월 가족여행도 가능한거 아닐까요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100034431.1.30",
          "form": "아 그렇게생각하니 그런거같기두해여 ㅋㅋㅋㅋ",
          "original_form": "아 그렇게생각하니 그런거같기두해여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:59"
        }
      ]
    }
