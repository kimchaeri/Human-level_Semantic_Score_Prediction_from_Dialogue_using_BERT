      "utterance": [
        {
          "id": "MDRW2100030634.1.1",
          "form": "결말 부분을 보고 그렇게 아쉬울 수가 없었어요",
          "original_form": "결말 부분을 보고 그렇게 아쉬울 수가 없었어요",
          "speaker_id": "2",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100030634.1.2",
          "form": "정환이 응원했었나염? ㅋㅋ",
          "original_form": "정환이 응원했었나염? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100030634.1.3",
          "form": "그쵸 ㅠ",
          "original_form": "그쵸 ㅠ",
          "speaker_id": "2",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100030634.1.4",
          "form": "누가봐도 정환이 아니었나요 ㅠㅠ",
          "original_form": "누가봐도 정환이 아니었나요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030634.1.5",
          "form": "ㅋㅋ저도 아쉽긴 했어여ㅜㅜ",
          "original_form": "ㅋㅋ저도 아쉽긴 했어여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030634.1.6",
          "form": "제 스탈은 정환",
          "original_form": "제 스탈은 정환{emoji:♡}",
          "speaker_id": "1",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030634.1.7",
          "form": "정환이가 반지 꺼내면서 고백하는거에 그렇게 심장딸렸는데 ㅠ",
          "original_form": "정환이가 반지 꺼내면서 고백하는거에 그렇게 심장딸렸는데 ㅠ",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030634.1.8",
          "form": "뻥이야",
          "original_form": "뻥이야",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030634.1.9",
          "form": "그 한마디에",
          "original_form": "그 한마디에",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030634.1.10",
          "form": "와르르 무너지는 느낌이었죠",
          "original_form": "와르르 무너지는 느낌이었죠",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030634.1.11",
          "form": "동룡이도 좋았어요",
          "original_form": "동룡이도 좋았어요",
          "speaker_id": "2",
          "time": "20210904 10:48"
        },
        {
          "id": "MDRW2100030634.1.12",
          "form": "자주 나오지 않아서 아쉬웠던 친구랄까",
          "original_form": "자주 나오지 않아서 아쉬웠던 친구랄까",
          "speaker_id": "2",
          "time": "20210904 10:48"
        },
        {
          "id": "MDRW2100030634.1.13",
          "form": "ㅋㅋ맞아요ㅋㅋㅋ",
          "original_form": "ㅋㅋ맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:57"
        },
        {
          "id": "MDRW2100030634.1.14",
          "form": "도롱뇽 넘 유쾌한친구ㅋㅋㅋ",
          "original_form": "도롱뇽 넘 유쾌한친구ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:57"
        },
        {
          "id": "MDRW2100030634.1.15",
          "form": "현실친구면 좋겠는ㅋ",
          "original_form": "현실친구면 좋겠는ㅋ",
          "speaker_id": "1",
          "time": "20210904 10:57"
        },
        {
          "id": "MDRW2100030634.1.16",
          "form": "그춐ㅋㅋㅋ 쌍문동 친구들 조합이 진짜로 내 옆에 있는 소꿉 친구면 좋겠다고 생각했어요",
          "original_form": "그춐ㅋㅋㅋ 쌍문동 친구들 조합이 진짜로 내 옆에 있는 소꿉 친구면 좋겠다고 생각했어요",
          "speaker_id": "2",
          "time": "20210904 10:58"
        },
        {
          "id": "MDRW2100030634.1.17",
          "form": "ㅋㅋ 덕선이 부럽ㅋㅋ",
          "original_form": "ㅋㅋ 덕선이 부럽ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100030634.1.18",
          "form": "비슷한 느낌으로 슬의생에 송화도 부러워요",
          "original_form": "비슷한 느낌으로 슬의생에 송화도 부러워요",
          "speaker_id": "2",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100030634.1.19",
          "form": "그쵸? ㅋㅋ 둘다 부럽ㅋㅋ",
          "original_form": "그쵸? ㅋㅋ 둘다 부럽ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030634.1.20",
          "form": "밴드 하는게 정말 최근에 배우들도 즐기고 있는 것 같아서 같이 즐거워 지는 느낌이에요",
          "original_form": "밴드 하는게 정말 최근에 배우들도 즐기고 있는 것 같아서 같이 즐거워 지는 느낌이에요",
          "speaker_id": "2",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030634.1.21",
          "form": "배우들한테도\r\n애정이  느껴지는 작품",
          "original_form": "배우들한테도\r\n애정이  느껴지는 작품{emoji:♡}",
          "speaker_id": "1",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030634.1.22",
          "form": "최근 나온 정경호의 회상 노래도 너무 좋고",
          "original_form": "최근 나온 정경호의 회상 노래도 너무 좋고",
          "speaker_id": "2",
          "time": "20210904 11:13"
        },
        {
          "id": "MDRW2100030634.1.23",
          "form": "김광석님 원곡인 너에게도 좋아요",
          "original_form": "김광석님 원곡인 너에게도 좋아요{emoji:♡}",
          "speaker_id": "2",
          "time": "20210904 11:13"
        },
        {
          "id": "MDRW2100030634.1.24",
          "form": "회상은 전부터\r\n좋아했던 곡인데\r\n슬의생에서 나오니\r\nㄷㅓ 좋더라구요:)",
          "original_form": "회상은 전부터\r\n좋아했던 곡인데\r\n슬의생에서 나오니\r\nㄷㅓ 좋더라구요:)",
          "speaker_id": "1",
          "time": "20210904 11:14"
        },
        {
          "id": "MDRW2100030634.1.25",
          "form": "유연석님이 부른\r\n너에게도 좋았구영ㅋㅋ",
          "original_form": "유연석님이 부른\r\n너에게도 좋았구영ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:15"
        },
        {
          "id": "MDRW2100030634.1.26",
          "form": "회상은 사실 이번에 처음 들었어요 \r\n그 목소리와 분위기가 너무 잘 어울려서 진짜 좋아요!",
          "original_form": "회상은 사실 이번에 처음 들었어요 \r\n그 목소리와 분위기가 너무 잘 어울려서 진짜 좋아요!",
          "speaker_id": "2",
          "time": "20210904 11:15"
        },
        {
          "id": "MDRW2100030634.1.27",
          "form": "이 곡은 여기서 첨 들어봤어여",
          "original_form": "이 곡은 여기서 첨 들어봤어여",
          "speaker_id": "1",
          "time": "20210904 11:15"
        }
      ]
