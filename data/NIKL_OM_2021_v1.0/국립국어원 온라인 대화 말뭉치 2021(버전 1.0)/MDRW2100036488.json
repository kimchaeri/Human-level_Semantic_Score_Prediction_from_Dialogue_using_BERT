      "utterance": [
        {
          "id": "MDRW2100036488.1.1",
          "form": "애기는 정말 금방 크는것 같다가도 그당시는 정말 힘들고 ㅎㅎ 지나고 보면 그시간이 아깝고 그렇더라고요",
          "original_form": "애기는 정말 금방 크는것 같다가도 그당시는 정말 힘들고 ㅎㅎ 지나고 보면 그시간이 아깝고 그렇더라고요",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036488.1.2",
          "form": "지금 애기몇개월이예요?",
          "original_form": "지금 애기몇개월이예요?",
          "speaker_id": "2",
          "time": "20210831 08:05"
        },
        {
          "id": "MDRW2100036488.1.3",
          "form": "작은애는 23개월이요 ㅎㅎ",
          "original_form": "작은애는 23개월이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 08:17"
        },
        {
          "id": "MDRW2100036488.1.4",
          "form": "말도 하겠네요 귀엽겠다",
          "original_form": "말도 하겠네요 귀엽겠다",
          "speaker_id": "2",
          "time": "20210831 08:19"
        },
        {
          "id": "MDRW2100036488.1.5",
          "form": "아직 완벽하진 못해요 중국말처럼 더 들려요 ㅎㅎ",
          "original_form": "아직 완벽하진 못해요 중국말처럼 더 들려요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 08:20"
        },
        {
          "id": "MDRW2100036488.1.6",
          "form": "중국말 ㅋㅋ 귀여울듯",
          "original_form": "중국말 ㅋㅋ 귀여울듯",
          "speaker_id": "2",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036488.1.7",
          "form": "슬슬 늘긴하는데 아직 웃겨요",
          "original_form": "슬슬 늘긴하는데 아직 웃겨요",
          "speaker_id": "1",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036488.1.8",
          "form": "매일매일이 새로울 듯",
          "original_form": "매일매일이 새로울 듯",
          "speaker_id": "2",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100036488.1.9",
          "form": "금방금방 크더라구요. 애들은",
          "original_form": "금방금방 크더라구요. 애들은",
          "speaker_id": "1",
          "time": "20210831 09:53"
        },
        {
          "id": "MDRW2100036488.1.10",
          "form": "남의애는 더빨리 크는 것같아요 삼자입장에서",
          "original_form": "남의애는 더빨리 크는 것같아요 삼자입장에서",
          "speaker_id": "2",
          "time": "20210831 10:46"
        },
        {
          "id": "MDRW2100036488.1.11",
          "form": "맞아요ㅋㅋ우리애보다 남의애가 더 빨리크는것같고",
          "original_form": "맞아요ㅋㅋ우리애보다 남의애가 더 빨리크는것같고",
          "speaker_id": "1",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100036488.1.12",
          "form": "그런것같아요 요즘 어린이집가나요?",
          "original_form": "그런것같아요 요즘 어린이집가나요?",
          "speaker_id": "2",
          "time": "20210831 11:56"
        },
        {
          "id": "MDRW2100036488.1.13",
          "form": "안가요ㅠ코로나땜에ㅠㅠ",
          "original_form": "안가요ㅠ코로나땜에ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 12:01"
        },
        {
          "id": "MDRW2100036488.1.14",
          "form": "안보내고있어여 첨부터",
          "original_form": "안보내고있어여 첨부터",
          "speaker_id": "1",
          "time": "20210831 12:01"
        },
        {
          "id": "MDRW2100036488.1.15",
          "form": "원래 안갔구나",
          "original_form": "원래 안갔구나",
          "speaker_id": "2",
          "time": "20210831 13:51"
        },
        {
          "id": "MDRW2100036488.1.16",
          "form": "네  한번도 안갔어요ㅠㅠ",
          "original_form": "네  한번도 안갔어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 13:51"
        },
        {
          "id": "MDRW2100036488.1.17",
          "form": "친구들 아가들은 원래 갔는데 코로나 때문에 못가나봐요",
          "original_form": "친구들 아가들은 원래 갔는데 코로나 때문에 못가나봐요",
