      "utterance": [
        {
          "id": "MDRW2100039751.1.1",
          "form": "다행이네요ㅎㅎ",
          "original_form": "다행이네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039751.1.2",
          "form": "주위에 동물키우는사람이 있나요?",
          "original_form": "주위에 동물키우는사람이 있나요?",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039751.1.3",
          "form": "네 토끼키우는 친구가 있어요",
          "original_form": "네 토끼키우는 친구가 있어요",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039751.1.4",
          "form": "와 귀여울거같아요ㅎㅎ",
          "original_form": "와 귀여울거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039751.1.5",
          "form": "혹시 이름이뭔가요",
          "original_form": "혹시 이름이뭔가요",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039751.1.6",
          "form": "따로 이름은 안지었더라구요",
          "original_form": "따로 이름은 안지었더라구요",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039751.1.7",
          "form": "name3라고 자주 부르는거 같던데ㅋㅋㅋ",
          "original_form": "&name3&라고 자주 부르는거 같던데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039751.1.8",
          "form": "ㅋㅋㅋㅋname3ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ&name3&ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039751.1.9",
          "form": "귀여워요",
          "original_form": "귀여워요",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039751.1.10",
          "form": "저는 주변에 고양이키우고있다거나 고양이키우고싶은 사람이",
          "original_form": "저는 주변에 고양이키우고있다거나 고양이키우고싶은 사람이",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039751.1.11",
          "form": "많은거같아요",
          "original_form": "많은거같아요",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039751.1.12",
          "form": "고양이 귀엽죠ㅋㅋㅋ",
          "original_form": "고양이 귀엽죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039751.1.13",
          "form": "제 주변엔 강아지 키우는 사람들이 더 많은거 같네요",
          "original_form": "제 주변엔 강아지 키우는 사람들이 더 많은거 같네요",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039751.1.14",
          "form": "사람나이로 따지면 어르신인 강아지도 있는데 귀여워요",
          "original_form": "사람나이로 따지면 어르신인 강아지도 있는데 귀여워요",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039751.1.15",
          "form": "저도 고양이파였는데 요즘은 강아지들이 너무 귀여운거같아요",
          "original_form": "저도 고양이파였는데 요즘은 강아지들이 너무 귀여운거같아요",
          "speaker_id": "2",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039751.1.16",
          "form": "그래서 우열을 못가리겠어요ㅎㅎ",
          "original_form": "그래서 우열을 못가리겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039751.1.17",
          "form": "사람 좋아해서 꼬리 흔드는거 너무 귀여워요ㅋㅋㅋ",
          "original_form": "사람 좋아해서 꼬리 흔드는거 너무 귀여워요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039751.1.18",
          "form": "맞아요ㅎㅎ 강아지만의 사랑스러운점이 있는데",
          "original_form": "맞아요ㅎㅎ 강아지만의 사랑스러운점이 있는데",
          "speaker_id": "2",
          "time": "20210903 22:15"
        },
        {
          "id": "MDRW2100039751.1.19",
          "form": "요즘그런게 참 예뻐보여요",
          "original_form": "요즘그런게 참 예뻐보여요",
          "speaker_id": "2",
          "time": "20210903 22:15"
        },
        {
          "id": "MDRW2100039751.1.20",
          "form": "고양이나 강아지나 볼때마다 이쁘고 귀여워서 고르기 어렵죠ㅋㅋ",
          "original_form": "고양이나 강아지나 볼때마다 이쁘고 귀여워서 고르기 어렵죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039751.1.21",
          "form": "네ㅋㅋ 사실 토끼도 좋고.. 그냥 털달린애들 다 귀여워요....",
          "original_form": "네ㅋㅋ 사실 토끼도 좋고.. 그냥 털달린애들 다 귀여워요....",
          "speaker_id": "2",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100039751.1.22",
          "form": "햄스터도 참 귀여워보여요.. 기니피그랑 친칠라도..",
          "original_form": "햄스터도 참 귀여워보여요.. 기니피그랑 친칠라도..",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039751.1.23",
          "form": "맞아요. 최근에 쿼카 사진 봤는데 진짜 귀엽더라구요",
          "original_form": "맞아요. 최근에 쿼카 사진 봤는데 진짜 귀엽더라구요",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100039751.1.24",
          "form": "쿼카는  호주였나? 거기 가야한다고하더라구요",
          "original_form": "쿼카는  호주였나? 거기 가야한다고하더라구요",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100039751.1.25",
          "form": "기회가되면 가보고싶어요",
          "original_form": "기회가되면 가보고싶어요",
          "speaker_id": "2",
