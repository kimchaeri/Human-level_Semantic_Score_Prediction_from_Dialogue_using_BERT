      "utterance": [
        {
          "id": "MDRW2100007881.1.1",
          "form": "마트배달해야하지않아?",
          "original_form": "마트배달해야하지않아?",
          "speaker_id": "2",
          "time": "20210822 11:39"
        },
        {
          "id": "MDRW2100007881.1.2",
          "form": "응 뭐 시켜야하지",
          "original_form": "응 뭐 시켜야하지",
          "speaker_id": "1",
          "time": "20210822 11:39"
        },
        {
          "id": "MDRW2100007881.1.3",
          "form": "name3 좋아하는 두부도 시키고",
          "original_form": "&name3& 좋아하는 두부도 시키고",
          "speaker_id": "1",
          "time": "20210822 11:39"
        },
        {
          "id": "MDRW2100007881.1.4",
          "form": "이제 name4도 과일 먹으니까",
          "original_form": "이제 &name4&도 과일 먹으니까",
          "speaker_id": "2",
          "time": "20210822 11:39"
        },
        {
          "id": "MDRW2100007881.1.5",
          "form": "바나나시킬까?",
          "original_form": "바나나시킬까?",
          "speaker_id": "2",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007881.1.6",
          "form": "응 그러자 인생 첫 바나나ㅋㅋ",
          "original_form": "응 그러자 인생 첫 바나나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007881.1.7",
          "form": "그리고 우리 먹을건",
          "original_form": "그리고 우리 먹을건",
          "speaker_id": "1",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007881.1.8",
          "form": "피자치즈 사야하지 않아?",
          "original_form": "피자치즈 사야하지 않아?",
          "speaker_id": "2",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007881.1.9",
          "form": "아 그래 양송이 샀으니까 그거 사자",
          "original_form": "아 그래 양송이 샀으니까 그거 사자",
          "speaker_id": "1",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007881.1.10",
          "form": "또 내내 양송이 대파티 예정이니까",
          "original_form": "또 내내 양송이 대파티 예정이니까",
          "speaker_id": "1",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007881.1.11",
          "form": "토마토는 집에 있어?",
          "original_form": "토마토는 집에 있어?",
          "speaker_id": "2",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007881.1.12",
          "form": "응 전에 산거 아직 있어서",
          "original_form": "응 전에 산거 아직 있어서",
          "speaker_id": "1",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007881.1.13",
          "form": "그럼 다른건 뭐 사나",
          "original_form": "그럼 다른건 뭐 사나",
          "speaker_id": "2",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007881.1.14",
          "form": "배달 시키려니 2만원 채우기가 어렵다ㅠ",
          "original_form": "배달 시키려니 2만원 채우기가 어렵다ㅠ",
          "speaker_id": "1",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007881.1.15",
          "form": "그치 네식구인데도 2만원이 어렵네ㅋㅋㅋ",
          "original_form": "그치 네식구인데도 2만원이 어렵네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007881.1.16",
          "form": "넷이지만 먹는 사람은 둘이니깤ㅋㅋㅋ",
          "original_form": "넷이지만 먹는 사람은 둘이니깤ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007881.1.17",
          "form": "아 어제 웨건 바람막이도 샀어",
          "original_form": "아 어제 웨건 바람막이도 샀어",
          "speaker_id": "1",
          "time": "20210822 11:44"
        },
        {
          "id": "MDRW2100007881.1.18",
          "form": "잘해쪄",
          "original_form": "잘해쪄",
          "speaker_id": "2",
          "time": "20210822 11:44"
        },
        {
          "id": "MDRW2100007881.1.19",
          "form": "폴딩같이되는걸로?",
          "original_form": "폴딩같이되는걸로?",
          "speaker_id": "2",
          "time": "20210822 11:44"
        },
        {
          "id": "MDRW2100007881.1.20",
          "form": "응 같이 접혀야 편하니까",
          "original_form": "응 같이 접혀야 편하니까",
          "speaker_id": "1",
          "time": "20210822 11:44"
        },
        {
          "id": "MDRW2100007881.1.21",
          "form": "엄청 추우면 안나가겠지 싶어서ㅋㅋㅋㅋ",
          "original_form": "엄청 추우면 안나가겠지 싶어서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:44"
        },
        {
          "id": "MDRW2100007881.1.22",
          "form": "조금 추워도 말자 싶어섴ㅋㅋㅋ",
          "original_form": "조금 추워도 말자 싶어섴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:44"
        },
        {
          "id": "MDRW2100007881.1.23",
          "form": "응응 겨울엔 옷 따숩게 입을거니까",
          "original_form": "응응 겨울엔 옷 따숩게 입을거니까",
          "speaker_id": "2",
          "time": "20210822 11:45"
        }
      ]
