      "utterance": [
        {
          "id": "MDRW2100030134.1.1",
          "form": "치킨",
          "original_form": "치킨",
          "speaker_id": "1",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030134.1.2",
          "form": "존맛",
          "original_form": "존맛",
          "speaker_id": "1",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030134.1.3",
          "form": "치킨은 진리지",
          "original_form": "치킨은 진리지",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030134.1.4",
          "form": "맥주나 탄산음료람",
          "original_form": "맥주나 탄산음료람",
          "speaker_id": "1",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030134.1.5",
          "form": "먹아야해",
          "original_form": "먹아야해",
          "speaker_id": "1",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030134.1.6",
          "form": "마실게 없으면",
          "original_form": "마실게 없으면",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030134.1.7",
          "form": "죄악이지",
          "original_form": "죄악이지",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030134.1.8",
          "form": "맞어 무도 같이먹어야해",
          "original_form": "맞어 무도 같이먹어야해",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030134.1.9",
          "form": "무는 국무 버리기가 힘들어",
          "original_form": "무는 국무 버리기가 힘들어",
          "speaker_id": "2",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030134.1.10",
          "form": "안버리면 집을때 힘들어",
          "original_form": "안버리면 집을때 힘들어",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030134.1.11",
          "form": "그리고 넘치면 치우기 힘들어",
          "original_form": "그리고 넘치면 치우기 힘들어",
          "speaker_id": "2",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030134.1.12",
          "form": "그런면에서",
          "original_form": "그런면에서",
          "speaker_id": "1",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030134.1.13",
          "form": "순살이 편해",
          "original_form": "순살이 편해",
          "speaker_id": "1",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030134.1.14",
          "form": "치킨도 브랜드중에서",
          "original_form": "치킨도 브랜드중에서",
          "speaker_id": "2",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100030134.1.15",
          "form": "뭐를 제일 좋아해?",
          "original_form": "뭐를 제일 좋아해?",
          "speaker_id": "2",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100030134.1.16",
          "form": "나 멕시카나",
          "original_form": "나 멕시카나",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100030134.1.17",
          "form": "멕시카나 치토스 젤 좋ㅇㄴ",
          "original_form": "멕시카나 치토스 젤 좋ㅇㄴ",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030134.1.18",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030134.1.19",
          "form": "나도 다음에 먹어야 봐야 겠네",
          "original_form": "나도 다음에 먹어야 봐야 겠네",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030134.1.20",
          "form": "사상역에 있는",
          "original_form": "사상역에 있는",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030134.1.21",
          "form": "한신점이 맛집이야",
          "original_form": "한신점이 맛집이야",
          "speaker_id": "1",
          "time": "20210903 22:37"
        }
      ]
