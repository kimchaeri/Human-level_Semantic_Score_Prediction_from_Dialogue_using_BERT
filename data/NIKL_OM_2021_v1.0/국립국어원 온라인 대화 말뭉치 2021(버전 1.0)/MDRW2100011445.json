      "utterance": [
        {
          "id": "MDRW2100011445.1.1",
          "form": "결혼에 대해 어떻게 생각하세요?",
          "original_form": "결혼에 대해 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100011445.1.2",
          "form": "결혼.. 요즘 참 하기 힘든것같아요",
          "original_form": "결혼.. 요즘 참 하기 힘든것같아요",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011445.1.3",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210824 12:47"
        },
        {
          "id": "MDRW2100011445.1.4",
          "form": "이상형은 어떻게되시나요?",
          "original_form": "이상형은 어떻게되시나요?",
          "speaker_id": "1",
          "time": "20210824 12:48"
        },
        {
          "id": "MDRW2100011445.1.5",
          "form": "이상형은 딱히없고 그냥 끌리는 사람이요!",
          "original_form": "이상형은 딱히없고 그냥 끌리는 사람이요!",
          "speaker_id": "2",
          "time": "20210824 12:48"
        },
        {
          "id": "MDRW2100011445.1.6",
          "form": "아 그냥",
          "original_form": "아 그냥",
          "speaker_id": "1",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100011445.1.7",
          "form": "끌리면되나요?!",
          "original_form": "끌리면되나요?!",
          "speaker_id": "1",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100011445.1.8",
          "form": "넵 그쵸ㅎㅎ 이상형 있으세요?",
          "original_form": "넵 그쵸ㅎㅎ 이상형 있으세요?",
          "speaker_id": "2",
          "time": "20210824 12:58"
        },
        {
          "id": "MDRW2100011445.1.9",
          "form": "전 성격이",
          "original_form": "전 성격이",
          "speaker_id": "1",
          "time": "20210824 13:00"
        },
        {
          "id": "MDRW2100011445.1.10",
          "form": "중요하다고생각해요ㅋㅋㅋ",
          "original_form": "중요하다고생각해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:00"
        },
        {
          "id": "MDRW2100011445.1.11",
          "form": "오 맞아요 성격 중요하죠",
          "original_form": "오 맞아요 성격 중요하죠",
          "speaker_id": "2",
          "time": "20210824 13:18"
        },
        {
          "id": "MDRW2100011445.1.12",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011445.1.13",
          "form": "외적인건 어떤게좋나요?",
          "original_form": "외적인건 어떤게좋나요?",
          "speaker_id": "1",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011445.1.14",
          "form": "맑은 눈 좋아해요ㅎㅎ",
          "original_form": "맑은 눈 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011445.1.15",
          "form": "ㅋㅋㅋ좋네요",
          "original_form": "ㅋㅋㅋ좋네요",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100011445.1.16",
          "form": "키는요?",
          "original_form": "키는요?",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100011445.1.17",
