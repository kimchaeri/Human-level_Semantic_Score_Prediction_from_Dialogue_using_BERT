      "utterance": [
        {
          "id": "MDRW2100028403.1.1",
          "form": "색다른맛이네",
          "original_form": "색다른맛이네",
          "speaker_id": "2",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028403.1.2",
          "form": "궁금ㅎ",
          "original_form": "궁금ㅎ",
          "speaker_id": "2",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028403.1.3",
          "form": "난 저녁으로 치즈샐러드먹고\r\n김치우동 먹었어",
          "original_form": "난 저녁으로 치즈샐러드먹고\r\n김치우동 먹었어",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028403.1.4",
          "form": "ㅇㅇ 그 사람은 모짜렐라에 찍어먹드라",
          "original_form": "ㅇㅇ 그 사람은 모짜렐라에 찍어먹드라",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028403.1.5",
          "form": "치즈샐러드 첨 들어봐",
          "original_form": "치즈샐러드 첨 들어봐",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028403.1.6",
          "form": "소식한다 생각했지만ㅎ 대식까진 아니고 중식정도로 배불하게 먹은거같아ㅋㅋㅋ",
          "original_form": "소식한다 생각했지만ㅎ 대식까진 아니고 중식정도로 배불하게 먹은거같아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028403.1.7",
          "form": "사진을 안찍어서",
          "original_form": "사진을 안찍어서",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028403.1.8",
          "form": "아 샐러드에 치즈 갈아서 넣은건가",
          "original_form": "아 샐러드에 치즈 갈아서 넣은건가",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100028403.1.9",
          "form": "아 리코타치즈샐러드 있었지 치즈샐러드 ㅋㅋ",
          "original_form": "아 리코타치즈샐러드 있었지 치즈샐러드 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028403.1.10",
          "form": "시판일텐데 치즈사진? 종류를 못찾겠네",
          "original_form": "시판일텐데 치즈사진? 종류를 못찾겠네",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028403.1.11",
          "form": "큐브모양 치즈가 덩이로 있는거",
          "original_form": "큐브모양 치즈가 덩이로 있는거",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028403.1.12",
          "form": "리코타치즈 아니고",
          "original_form": "리코타치즈 아니고",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028403.1.13",
          "form": "ㅇㅇ맛있었게따!!",
          "original_form": "ㅇㅇ맛있었게따!!",
          "speaker_id": "1",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028403.1.14",
          "form": "큐브모양에 약간분같은거 있고",
          "original_form": "큐브모양에 약간분같은거 있고",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028403.1.15",
          "form": "맛괜찮괜찮",
          "original_form": "맛괜찮괜찮",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028403.1.16",
          "form": "ㅇㅇ 샐러드 좋아",
          "original_form": "ㅇㅇ 샐러드 좋아",
          "speaker_id": "1",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028403.1.17",
          "form": "배송올게많아서 냉동실 음식 먹어치우는중",
          "original_form": "배송올게많아서 냉동실 음식 먹어치우는중",
          "speaker_id": "1",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028403.1.18",
          "form": "냉동실이 모잘라",
          "original_form": "냉동실이 모잘라",
          "speaker_id": "1",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028403.1.19",
          "form": "찾았다ㅋㅋ체다치즈랑 크림치즈ㅋ큐브모양샐러드",
          "original_form": "찾았다ㅋㅋ체다치즈랑 크림치즈ㅋ큐브모양샐러드",
          "speaker_id": "2",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028403.1.20",
          "form": "냉동고만 구매하기도하더라",
          "original_form": "냉동고만 구매하기도하더라",
          "speaker_id": "2",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028403.1.21",
          "form": "요즘 김치냉장고 세우는거 있자나",
          "original_form": "요즘 김치냉장고 세우는거 있자나",
          "speaker_id": "2",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028403.1.22",
          "form": "김냉 맨위만 냉동고로 만들수있더라ㅜ",
          "original_form": "김냉 맨위만 냉동고로 만들수있더라ㅜ",
          "speaker_id": "1",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100028403.1.23",
          "form": "그래? 캄 냉동고를 사야겠네",
          "original_form": "그래? 캄 냉동고를 사야겠네",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028403.1.24",
          "form": "그럼 냉장고 넘 많아서 ㅜㅜ 암튼 열심히 먹는중 ㅋㅋ 떡같은건 조금씩밖에 못 먹지만",
          "original_form": "그럼 냉장고 넘 많아서 ㅜㅜ 암튼 열심히 먹는중 ㅋㅋ 떡같은건 조금씩밖에 못 먹지만",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028403.1.25",
          "form": "나 지금 라떼마신당",
          "original_form": "나 지금 라떼마신당",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028403.1.26",
          "form": "배고픈데 먹을만한게 없어서",
          "original_form": "배고픈데 먹을만한게 없어서",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028403.1.27",
          "form": "오ㅎㅎ",
          "original_form": "오ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028403.1.28",
          "form": "난 참는다ㅋㅋ",
          "original_form": "난 참는다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028403.1.29",
          "form": "커피 어제까지 차가운거 마시다가 오늘 뜨거운거 마셨는데 좋네",
          "original_form": "커피 어제까지 차가운거 마시다가 오늘 뜨거운거 마셨는데 좋네",
