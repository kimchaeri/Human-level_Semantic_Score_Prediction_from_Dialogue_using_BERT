        {
          "id": "MDRW2100045252.1.1",
          "form": "좋아하는 연예인 있으신가요",
          "original_form": "좋아하는 연예인 있으신가요",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045252.1.2",
          "form": "저는 중국 연예인이요",
          "original_form": "저는 중국 연예인이요",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045252.1.3",
          "form": "오 누군지 물어봐도 되나요",
          "original_form": "오 누군지 물어봐도 되나요",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045252.1.4",
          "form": "루한 이라고...",
          "original_form": "루한 이라고...",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045252.1.5",
          "form": "오 전에 엑소 하시던 분",
          "original_form": "오 전에 엑소 하시던 분",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045252.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045252.1.7",
          "form": "그분 귀엽게 생겼던데",
          "original_form": "그분 귀엽게 생겼던데",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045252.1.8",
          "form": "아 사슴 상을 좋아합니다",
          "original_form": "아 사슴 상을 좋아합니다",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045252.1.9",
          "form": "저도 윤아 좋아하는데",
          "original_form": "저도 윤아 좋아하는데",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045252.1.10",
          "form": "맞아요 사슴 상",
          "original_form": "맞아요 사슴 상",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045252.1.11",
          "form": "또 누가 있을까요",
          "original_form": "또 누가 있을까요",
          "speaker_id": "2",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045252.1.12",
          "form": "보통 샤이니 민호 정도",
          "original_form": "보통 샤이니 민호 정도",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045252.1.13",
          "form": "다른 사람도 있던데",
