      "utterance": [
        {
          "id": "MDRW2100022932.1.1",
          "form": "학교다녀?",
          "original_form": "학교다녀?",
          "speaker_id": "1",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100022932.1.2",
          "form": "9월 1일이 개강이야",
          "original_form": "9월 1일이 개강이야",
          "speaker_id": "2",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022932.1.3",
          "form": "우아 난 9월 1일에 태권도 심사바",
          "original_form": "우아 난 9월 1일에 태권도 심사바",
          "speaker_id": "1",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022932.1.4",
          "form": "넌 학교 안다녀?",
          "original_form": "넌 학교 안다녀?",
          "speaker_id": "2",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022932.1.5",
          "form": "다녀다녀",
          "original_form": "다녀다녀",
          "speaker_id": "1",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022932.1.6",
          "form": "대학교",
          "original_form": "대학교",
          "speaker_id": "1",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022932.1.7",
          "form": "몇학년이야?",
          "original_form": "몇학년이야?",
          "speaker_id": "2",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022932.1.8",
          "form": "공부 겁나잘했어",
          "original_form": "공부 겁나잘했어",
          "speaker_id": "1",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022932.1.9",
          "form": "2학년",
          "original_form": "2학년",
          "speaker_id": "1",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022932.1.10",
          "form": "거짓말",
          "original_form": "거짓말",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022932.1.11",
          "form": "시비걸지마라",
          "original_form": "시비걸지마라",
          "speaker_id": "1",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022932.1.12",
          "form": "알겠아 미안해",
          "original_form": "알겠아 미안해",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022932.1.13",
          "form": "수강신청 잘했어?",
          "original_form": "수강신청 잘했어?",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022932.1.14",
          "form": "나 휴학했는데!",
          "original_form": "나 휴학했는데!",
          "speaker_id": "1",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022932.1.15",
          "form": "좋겠다",
          "original_form": "좋겠다",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022932.1.16",
          "form": "넌 학교다녀?",
          "original_form": "넌 학교다녀?",
          "speaker_id": "1",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022932.1.17",
          "form": "나는 반학기 휴학했었어",
          "original_form": "나는 반학기 휴학했었어",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022932.1.18",
          "form": "왜 1학기만했어",
          "original_form": "왜 1학기만했어",
          "speaker_id": "1",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100022932.1.19",
          "form": "1년 신청했는데 그냥 한학기만 하고 복학했어",
          "original_form": "1년 신청했는데 그냥 한학기만 하고 복학했어",
          "speaker_id": "2",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100022932.1.20",
          "form": "엥 그럼 1학기 휴학은 날아가?",
          "original_form": "엥 그럼 1학기 휴학은 날아가?",
          "speaker_id": "1",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100022932.1.21",
          "form": "그냥 없던거 되는거야",
          "original_form": "그냥 없던거 되는거야",
          "speaker_id": "2",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100022932.1.22",
          "form": "우아 짱이다 학교다녀?",
          "original_form": "우아 짱이다 학교다녀?",
          "speaker_id": "1",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100022932.1.23",
          "form": "야",
          "original_form": "야",
          "speaker_id": "1",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022932.1.24",
          "form": "복학하냐고",
          "original_form": "복학하냐고",
          "speaker_id": "1",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022932.1.25",
          "form": "다니지",
          "original_form": "다니지",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022932.1.26",
          "form": "무시하지마라",
          "original_form": "무시하지마라",
          "speaker_id": "1",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022932.1.27",
          "form": "어디다녀",
          "original_form": "어디다녀",
          "speaker_id": "1",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022932.1.28",
          "form": "수도권이야",
          "original_form": "수도권이야",
          "speaker_id": "2",
          "time": "20210829 19:22"
