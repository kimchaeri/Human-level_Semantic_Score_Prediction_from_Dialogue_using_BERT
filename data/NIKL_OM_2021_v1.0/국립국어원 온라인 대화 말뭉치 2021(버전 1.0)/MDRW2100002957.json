      "utterance": [
        {
          "id": "MDRW2100002957.1.1",
          "form": "올림픽 경기 보셨어요?",
          "original_form": "올림픽 경기 보셨어요?",
          "speaker_id": "2",
          "time": "20210820 17:02"
        },
        {
          "id": "MDRW2100002957.1.2",
          "form": "네 특히 양궁이야ㅡ",
          "original_form": "네 특히 양궁이야ㅡ",
          "speaker_id": "1",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002957.1.3",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002957.1.4",
          "form": "안산 너무 대단하더라구요",
          "original_form": "안산 너무 대단하더라구요",
          "speaker_id": "1",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002957.1.5",
          "form": "안산 너무 괜찮더라고요",
          "original_form": "안산 너무 괜찮더라고요",
          "speaker_id": "2",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002957.1.6",
          "form": "심박수뛰는거 보셨어요? 너무신기하더라구요",
          "original_form": "심박수뛰는거 보셨어요? 너무신기하더라구요",
          "speaker_id": "1",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002957.1.7",
          "form": "기술이 엄청 좋아진거 같아요",
          "original_form": "기술이 엄청 좋아진거 같아요",
          "speaker_id": "2",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002957.1.8",
          "form": "글쵸 근데 다른선수들은 막 올라가던데 안산은 너무 평온하더라구요",
          "original_form": "글쵸 근데 다른선수들은 막 올라가던데 안산은 너무 평온하더라구요",
          "speaker_id": "1",
          "time": "20210820 17:06"
        },
        {
          "id": "MDRW2100002957.1.9",
          "form": "어린데도 엄청 침착하고 장하더라고요 \r\n강심장 인듯해요 ㅋㅋ",
          "original_form": "어린데도 엄청 침착하고 장하더라고요 \r\n강심장 인듯해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002957.1.10",
          "form": "어린가요?",
          "original_form": "어린가요?",
          "speaker_id": "1",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002957.1.11",
          "form": "19살인가 아직 고등학생이예요",
          "original_form": "19살인가 아직 고등학생이예요",
          "speaker_id": "2",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002957.1.12",
          "form": "대학생이네요 ㅎㅎ 01년생",
          "original_form": "대학생이네요 ㅎㅎ 01년생",
          "speaker_id": "2",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002957.1.13",
          "form": "와 진짜 어리네요",
          "original_form": "와 진짜 어리네요",
          "speaker_id": "1",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002957.1.14",
          "form": "어린선수가 진짜 너무 침착해요",
          "original_form": "어린선수가 진짜 너무 침착해요",
          "speaker_id": "1",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002957.1.15",
          "form": "거기다 단체전에서는 젤 먼저 쏘던데 쏘고나서 언니들한테 조근조근 얘기하더라고요",
          "original_form": "거기다 단체전에서는 젤 먼저 쏘던데 쏘고나서 언니들한테 조근조근 얘기하더라고요",
          "speaker_id": "2",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002957.1.16",
          "form": "너무 멋져요 완전 한국에 스타탄생이네요",
          "original_form": "너무 멋져요 완전 한국에 스타탄생이네요",
          "speaker_id": "1",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002957.1.17",
          "form": "맞아요 티비에도 안나오고 다음경기 훈련 열심히 하고 있는가봐요",
          "original_form": "맞아요 티비에도 안나오고 다음경기 훈련 열심히 하고 있는가봐요",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002957.1.18",
          "form": "지난번에 모교 찾아간거에 나오던데 그것만 나왔나보네요",
          "original_form": "지난번에 모교 찾아간거에 나오던데 그것만 나왔나보네요",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002957.1.19",
          "form": "아 나오긴 했군요 \r\n숏컷때문에 티비에는 많이 나오긴 싫을거 같아요",
          "original_form": "아 나오긴 했군요 \r\n숏컷때문에 티비에는 많이 나오긴 싫을거 같아요",
          "speaker_id": "2",
          "time": "20210820 17:18"
        }
      ]
