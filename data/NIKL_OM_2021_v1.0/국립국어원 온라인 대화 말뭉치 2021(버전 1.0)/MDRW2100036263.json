      "utterance": [
        {
          "id": "MDRW2100036263.1.1",
          "form": "헐 맞아요 ㅠ 저 처음 입덕한게 엔시티 유",
          "original_form": "헐 맞아요 ㅠ 저 처음 입덕한게 엔시티 유",
          "speaker_id": "2",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100036263.1.2",
          "form": "재현 태일 도영 셋일 때라",
          "original_form": "재현 태일 도영 셋일 때라",
          "speaker_id": "2",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100036263.1.3",
          "form": "셋은 지금도 품고 살아요",
          "original_form": "셋은 지금도 품고 살아요",
          "speaker_id": "2",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100036263.1.4",
          "form": "아 사실 저두 도영이로 입덕했어욬ㅋㅋ",
          "original_form": "아 사실 저두 도영이로 입덕했어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100036263.1.5",
          "form": "아 혹시 그 nct u라면",
          "original_form": "아 혹시 그 nct u라면",
          "speaker_id": "1",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100036263.1.6",
          "form": "위닷츄 .. ?",
          "original_form": "위닷츄 .. ?",
          "speaker_id": "1",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100036263.1.7",
          "form": "네네!!!!!!!!!!!!!!!!",
          "original_form": "네네!!!!!!!!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100036263.1.8",
          "form": "증말 처음 뮤대 보고",
          "original_form": "증말 처음 뮤대 보고",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100036263.1.9",
          "form": "태일한테 폴 인 럽 했거든요...",
          "original_form": "태일한테 폴 인 럽 했거든요...",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100036263.1.10",
          "form": "태일이 .. 최고의 명창 ..",
          "original_form": "태일이 .. 최고의 명창 ..",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100036263.1.11",
          "form": "전 태일이가 젤 웃겨요 엔시티에섴ㅋㅋ",
          "original_form": "전 태일이가 젤 웃겨요 엔시티에섴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100036263.1.12",
          "form": "아 그니까요!! 그 어설픈 행동??",
          "original_form": "아 그니까요!! 그 어설픈 행동??",
          "speaker_id": "2",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100036263.1.13",
          "form": "들이 넘 웃겨요 ㅠ 정우랑 같이",
          "original_form": "들이 넘 웃겨요 ㅠ 정우랑 같이",
          "speaker_id": "2",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100036263.1.14",
          "form": "아 ㅋㅋㅋㄱ맞아욬ㅋㅋㅋㄱ",
          "original_form": "아 ㅋㅋㅋㄱ맞아욬ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100036263.1.15",
          "form": "제 최애 영상중 하나가",
          "original_form": "제 최애 영상중 하나가",
          "speaker_id": "1",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100036263.1.16",
          "form": "태일정우 콜라사이다로 싸우는거에요..",
          "original_form": "태일정우 콜라사이다로 싸우는거에요..",
          "speaker_id": "1",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100036263.1.17",
          "form": "오마이갓 저도 그거 좋아해요",
          "original_form": "오마이갓 저도 그거 좋아해요",
          "speaker_id": "2",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100036263.1.18",
          "form": "삼겹살 먹을 때 콜라 먹는다며",
          "original_form": "삼겹살 먹을 때 콜라 먹는다며",
          "speaker_id": "2",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100036263.1.19",
          "form": "소리치는 정우..",
          "original_form": "소리치는 정우..",
          "speaker_id": "2",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100036263.1.20",
          "form": "아 ㅋㄱㅋㄱㄱ맞아욬ㅋㄱㅋ",
          "original_form": "아 ㅋㄱㅋㄱㄱ맞아욬ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100036263.1.21",
          "form": "콜라 먹짜나~~",
          "original_form": "콜라 먹짜나~~",
          "speaker_id": "1",
          "time": "20210830 20:49"
        },
        {
          "id": "MDRW2100036263.1.22",
          "form": "제 최애포인트는",
          "original_form": "제 최애포인트는",
          "speaker_id": "1",
          "time": "20210830 20:49"
        },
        {
          "id": "MDRW2100036263.1.23",
          "form": "뒤에 하는 말장난이요 ..",
          "original_form": "뒤에 하는 말장난이요 ..",
          "speaker_id": "1",
          "time": "20210830 20:49"
        },
        {
          "id": "MDRW2100036263.1.24",
          "form": "아 맞아요!!! 그 재현 나와서",
          "original_form": "아 맞아요!!! 그 재현 나와서",
          "speaker_id": "2",
          "time": "20210830 20:50"
        },
        {
          "id": "MDRW2100036263.1.25",
          "form": "피가 말라 막 이러는!",
          "original_form": "피가 말라 막 이러는!",
          "speaker_id": "2",
          "time": "20210830 20:50"
        },
        {
          "id": "MDRW2100036263.1.26",
          "form": "아 맞아욬ㅋㄱㄱㅋ",
          "original_form": "아 맞아욬ㅋㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100036263.1.27",
          "form": "나의 라임 오렌지나뭌ㅋㄱ",
          "original_form": "나의 라임 오렌지나뭌ㅋㄱ",
          "speaker_id": "1",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100036263.1.28",
          "form": "이번에 리얼리티 하는거",
          "original_form": "이번에 리얼리티 하는거",
          "speaker_id": "1",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100036263.1.29",
          "form": "아시죠!",
          "original_form": "아시죠!",
          "speaker_id": "1",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100036263.1.30",
          "form": "네네!! 오늘 첫 방송이라고 들었는데",
          "original_form": "네네!! 오늘 첫 방송이라고 들었는데",
          "speaker_id": "2",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100036263.1.31",
          "form": "바빠서 못 봤어요 ㅠ",
          "original_form": "바빠서 못 봤어요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100036263.1.32",
          "form": "보셨나요?!",
          "original_form": "보셨나요?!",
          "speaker_id": "2",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100036263.1.33",
          "form": "아뇨 ㅜㅜㅜ",
          "original_form": "아뇨 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 20:54"
        },
        {
          "id": "MDRW2100036263.1.34",
          "form": "아직 트위터에 짤려진것만",
          "original_form": "아직 트위터에 짤려진것만",
          "speaker_id": "1",
          "time": "20210830 20:54"
        },
        {
          "id": "MDRW2100036263.1.35",
          "form": "봤어요 !",
          "original_form": "봤어요 !",
          "speaker_id": "1",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100036263.1.36",
          "form": "집가는길이여서 ㅜㅜ",
          "original_form": "집가는길이여서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100036263.1.37",
          "form": "으앗 저랑 같구만요.... 근데 오늘 차에서 있던 일만 나왔다고 해서 스스로 위안 중이에요 ^.ㅠ",
          "original_form": "으앗 저랑 같구만요.... 근데 오늘 차에서 있던 일만 나왔다고 해서 스스로 위안 중이에요 ^.ㅠ",
