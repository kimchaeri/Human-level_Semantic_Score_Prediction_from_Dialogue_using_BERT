          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030233.1.4",
          "form": "화잇팅",
          "original_form": "화잇팅",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030233.1.5",
          "form": "그렇긴하지 ㅎㅎ",
          "original_form": "그렇긴하지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030233.1.6",
          "form": "웅!! 그래도 2학기는 좀더",
          "original_form": "웅!! 그래도 2학기는 좀더",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030233.1.7",
          "form": "짧으니까 할만한거같아",
          "original_form": "짧으니까 할만한거같아",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030233.1.8",
          "form": "방학도 길고",
          "original_form": "방학도 길고",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030233.1.9",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030233.1.10",
          "form": "난 방학이 너무 기다려져 ㅎㅎ",
          "original_form": "난 방학이 너무 기다려져 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030233.1.11",
          "form": "이제 금방 졸업이다",
          "original_form": "이제 금방 졸업이다",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030233.1.12",
          "form": "완전 슉슉감",
          "original_form": "완전 슉슉감",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030233.1.13",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030233.1.14",
          "form": "너무 싫다",
          "original_form": "너무 싫다",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030233.1.15",
          "form": "그전까지 친구랑 많이 놀아.. .",
          "original_form": "그전까지 친구랑 많이 놀아.. .",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030233.1.16",
          "form": "학교를 좀 더 다니고 싶긴해 ㅎㅎ",
          "original_form": "학교를 좀 더 다니고 싶긴해 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030233.1.17",
          "form": "응... 알겠어",
          "original_form": "응... 알겠어",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030233.1.18",
          "form": "대학교 가면 잘 못 만나 ㅜㅡㅜ",
          "original_form": "대학교 가면 잘 못 만나 ㅜㅡㅜ",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030233.1.19",
          "form": "슬프다",
          "original_form": "슬프다",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030233.1.20",
          "form": "훌쩍",
          "original_form": "훌쩍",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030233.1.21",
          "form": "ㅜㅡㅜ 슬퍼...",
