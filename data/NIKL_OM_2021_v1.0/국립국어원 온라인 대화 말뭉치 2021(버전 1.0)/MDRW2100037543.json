      "utterance": [
        {
          "id": "MDRW2100037543.1.1",
          "form": "사장님 일있어서 넌지시 물어보길래 ㅋㅋㅋㅋ",
          "original_form": "사장님 일있어서 넌지시 물어보길래 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:51"
        },
        {
          "id": "MDRW2100037543.1.2",
          "form": "걍 나온다했슈 ㅋㅋㅋㅋ",
          "original_form": "걍 나온다했슈 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:51"
        },
        {
          "id": "MDRW2100037543.1.3",
          "form": "아 착한 직원이네 ㅎㅎ",
          "original_form": "아 착한 직원이네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100037543.1.4",
          "form": "잘한다 정말 ㅋㅋㅋㅋㄱ",
          "original_form": "잘한다 정말 ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100037543.1.5",
          "form": "나는 사무실나와서 톡을 제일 많이 하는듯 ㅋㅋㅋㄱ",
          "original_form": "나는 사무실나와서 톡을 제일 많이 하는듯 ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100037543.1.6",
          "form": "나도 그래요 언니ㅋㅋㅋㅋ",
          "original_form": "나도 그래요 언니ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100037543.1.7",
          "form": "여기서 제일 많이 함ㅋㅋㅋㅋ",
          "original_form": "여기서 제일 많이 함ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100037543.1.8",
          "form": "ㅋㅋㅋㅋㄱ 그래",
          "original_form": "ㅋㅋㅋㅋㄱ 그래",
          "speaker_id": "1",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100037543.1.9",
          "form": "신랑이 계속 폰보고 있어서",
          "original_form": "신랑이 계속 폰보고 있어서",
          "speaker_id": "1",
          "time": "20210901 10:36"
        },
        {
          "id": "MDRW2100037543.1.10",
          "form": "집에서 모두 6시~9시 폰금지 시간임",
          "original_form": "집에서 모두 6시~9시 폰금지 시간임",
          "speaker_id": "1",
          "time": "20210901 10:36"
        },
        {
          "id": "MDRW2100037543.1.11",
          "form": "아 너무 웃긴다 ㅋㅋㅋㅋㅋ",
          "original_form": "아 너무 웃긴다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:36"
        },
        {
          "id": "MDRW2100037543.1.12",
          "form": "폰 다 책장위에 올려두고 만지지말라고",
          "original_form": "폰 다 책장위에 올려두고 만지지말라고",
          "speaker_id": "1",
          "time": "20210901 10:36"
        },
        {
          "id": "MDRW2100037543.1.13",
          "form": "아무도 못함ㅋㅋㅋㅋ",
          "original_form": "아무도 못함ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:36"
        },
        {
          "id": "MDRW2100037543.1.14",
          "form": "어 내가 안할꺼니 남도 하지마라고 ㅋㅋㅋ",
          "original_form": "어 내가 안할꺼니 남도 하지마라고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:36"
        },
        {
          "id": "MDRW2100037543.1.15",
          "form": "사무실에서 폭풍 ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "사무실에서 폭풍 ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 10:36"
        },
        {
          "id": "MDRW2100037543.1.16",
          "form": "바쁘다 쇼핑도 해야되고 ㅋㅋㅋㅋ",
          "original_form": "바쁘다 쇼핑도 해야되고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100037543.1.17",
          "form": "그러니까요 언니 쇼핑하고 돈 벌고",
          "original_form": "그러니까요 언니 쇼핑하고 돈 벌고",
          "speaker_id": "2",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100037543.1.18",
          "form": "화장실 가서 똥누고 돈 벌고ㅋㅋㅋㅋ",
          "original_form": "화장실 가서 똥누고 돈 벌고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100037543.1.19",
          "form": "ㅋㅋㅋㅋ  눈치 보면서 계속 손가락 움직이고 있음 응",
          "original_form": "ㅋㅋㅋㅋ  눈치 보면서 계속 손가락 움직이고 있음 응",
          "speaker_id": "1",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100037543.1.20",
          "form": "그래 업무스트레스 없으니 그게어디고\r\nㅋㅋㅋㅋㅋ",
          "original_form": "그래 업무스트레스 없으니 그게어디고\r\nㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:38"
        },
        {
          "id": "MDRW2100037543.1.21",
          "form": "맞아요 근데 이제",
          "original_form": "맞아요 근데 이제",
          "speaker_id": "2",
          "time": "20210901 10:38"
        },
        {
          "id": "MDRW2100037543.1.22",
          "form": "여름 옷 잡아넣어야 되네요",
          "original_form": "여름 옷 잡아넣어야 되네요",
          "speaker_id": "2",
          "time": "20210901 10:38"
        },
        {
          "id": "MDRW2100037543.1.23",
          "form": "겨울 옷은 또 겁나 무거운데ㅋㅋㅋㅋㅋ",
          "original_form": "겨울 옷은 또 겁나 무거운데ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:38"
        },
        {
          "id": "MDRW2100037543.1.24",
          "form": "사계절이 너무 싫네요 ㅋㅋㅋㅋ",
          "original_form": "사계절이 너무 싫네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:38"
        },
        {
          "id": "MDRW2100037543.1.25",
          "form": "그래도 돈 줘서 좋아요 ㅋㅋㅋ",
          "original_form": "그래도 돈 줘서 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100037543.1.26",
          "form": "잘하자 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "잘하자 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100037543.1.27",
          "form": "그래 이래 일하고 돈버니 얼마니 좋으니",
          "original_form": "그래 이래 일하고 돈버니 얼마니 좋으니",
          "speaker_id": "1",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100037543.1.28",
          "form": "맞아요 언니 퇴사하면",
          "original_form": "맞아요 언니 퇴사하면",
          "speaker_id": "2",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100037543.1.29",
          "form": "일자리 구하기 너무 힘들어요ㅋㅋㅋㅋ",
          "original_form": "일자리 구하기 너무 힘들어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:40"
        }
      ]
