      "utterance": [
        {
          "id": "MDRW2100011008.1.1",
          "form": "리그오브레전드 좋아해요",
          "original_form": "리그오브레전드 좋아해요",
          "speaker_id": "2",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011008.1.2",
          "form": "롤 저도 좋아해요",
          "original_form": "롤 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100011008.1.3",
          "form": "티어가 어떻게 되세요?",
          "original_form": "티어가 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011008.1.4",
          "form": "월드오브워크래프트도 좋아해요",
          "original_form": "월드오브워크래프트도 좋아해요",
          "speaker_id": "2",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011008.1.5",
          "form": "전 골드입니다",
          "original_form": "전 골드입니다",
          "speaker_id": "2",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011008.1.6",
          "form": "전 다이아에요",
          "original_form": "전 다이아에요",
          "speaker_id": "1",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011008.1.7",
          "form": "티어가 어디신가요",
          "original_form": "티어가 어디신가요",
          "speaker_id": "2",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011008.1.8",
          "form": "롤 엄청 잘해요",
          "original_form": "롤 엄청 잘해요",
          "speaker_id": "1",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011008.1.9",
          "form": "대단하이네요",
          "original_form": "대단하이네요",
          "speaker_id": "2",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011008.1.10",
          "form": "전 원딜인ㄷㆍ기",
          "original_form": "전 원딜인ㄷㆍ기",
          "speaker_id": "2",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011008.1.11",
          "form": "다음에 같이 게임할 기회되면 도와드릴게요",
          "original_form": "다음에 같이 게임할 기회되면 도와드릴게요",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011008.1.12",
          "form": "어느 라인에 가시나요",
          "original_form": "어느 라인에 가시나요",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011008.1.13",
          "form": "전 미드요",
          "original_form": "전 미드요",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011008.1.14",
          "form": "미드만 가요",
          "original_form": "미드만 가요",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011008.1.15",
          "form": "캐리 부탁해요",
          "original_form": "캐리 부탁해요",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011008.1.16",
          "form": "네 그럴게요",
          "original_form": "네 그럴게요",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011008.1.17",
          "form": "플레티넘 가고싶어요",
          "original_form": "플레티넘 가고싶어요",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011008.1.18",
          "form": "와우는 언제부터 좋아하셨어요?",
          "original_form": "와우는 언제부터 좋아하셨어요?",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011008.1.19",
          "form": "플레티넘 껌이에요",
          "original_form": "플레티넘 껌이에요",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011008.1.20",
          "form": "고등학생 때 부터요",
          "original_form": "고등학생 때 부터요",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011008.1.21",
          "form": "곧 되실거에요ㅎㅎ!",
          "original_form": "곧 되실거에요ㅎㅎ!",
          "speaker_id": "1",
          "time": "20210823 23:44"
        }
