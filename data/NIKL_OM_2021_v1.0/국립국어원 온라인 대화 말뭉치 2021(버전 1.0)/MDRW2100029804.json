      "utterance": [
        {
          "id": "MDRW2100029804.1.1",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "1",
          "time": "20210903 13:58"
        },
        {
          "id": "MDRW2100029804.1.2",
          "form": "네~ 간단하게 빵먹었어요",
          "original_form": "네~ 간단하게 빵먹었어요",
          "speaker_id": "2",
          "time": "20210903 14:01"
        },
        {
          "id": "MDRW2100029804.1.3",
          "form": "식사하셨어요~?",
          "original_form": "식사하셨어요~?",
          "speaker_id": "2",
          "time": "20210903 14:02"
        },
        {
          "id": "MDRW2100029804.1.4",
          "form": "네 저는 도시락 싸다녀요",
          "original_form": "네 저는 도시락 싸다녀요",
          "speaker_id": "1",
          "time": "20210903 14:10"
        },
        {
          "id": "MDRW2100029804.1.5",
          "form": "빵으로 배가 불러요?",
          "original_form": "빵으로 배가 불러요?",
          "speaker_id": "1",
          "time": "20210903 14:10"
        },
        {
          "id": "MDRW2100029804.1.6",
          "form": "헐 부지런하시넹 ㅋㅋㅋ",
          "original_form": "헐 부지런하시넹 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:02"
        },
        {
          "id": "MDRW2100029804.1.7",
          "form": "배가 부르긴한데",
          "original_form": "배가 부르긴한데",
          "speaker_id": "2",
          "time": "20210903 15:02"
        },
        {
          "id": "MDRW2100029804.1.8",
          "form": "금방 꺼지긴해요 ㅠㅠ",
          "original_form": "금방 꺼지긴해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 15:02"
        },
        {
          "id": "MDRW2100029804.1.9",
          "form": "그춐ㅋㅋㅋㅋ",
          "original_form": "그춐ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:04"
        },
        {
          "id": "MDRW2100029804.1.10",
          "form": "도시락 넘 귀찮아요 흑",
          "original_form": "도시락 넘 귀찮아요 흑",
          "speaker_id": "1",
          "time": "20210903 15:04"
        },
        {
          "id": "MDRW2100029804.1.11",
          "form": "맞아유 ㅠㅠ설거지도..늘어난 느낌이랄까...ㅎ",
          "original_form": "맞아유 ㅠㅠ설거지도..늘어난 느낌이랄까...ㅎ",
          "speaker_id": "2",
          "time": "20210903 15:06"
        },
        {
          "id": "MDRW2100029804.1.12",
          "form": "저도 도시락할려다가",
          "original_form": "저도 도시락할려다가",
          "speaker_id": "2",
          "time": "20210903 15:07"
        },
        {
          "id": "MDRW2100029804.1.13",
          "form": "귀찮아서 며칠만에 관뒀어요ㅠㅋㅋㅋ",
          "original_form": "귀찮아서 며칠만에 관뒀어요ㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:07"
        },
        {
          "id": "MDRW2100029804.1.14",
          "form": "엌ㅋㅋ 회사 근처에 먹을 곳이 많은가요?",
          "original_form": "엌ㅋㅋ 회사 근처에 먹을 곳이 많은가요?",
          "speaker_id": "1",
          "time": "20210903 15:09"
        },
        {
          "id": "MDRW2100029804.1.15",
          "form": "저는 시골이라..도시락밖에 답이없아욬ㅋㅋㅋ",
          "original_form": "저는 시골이라..도시락밖에 답이없아욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:09"
        },
        {
          "id": "MDRW2100029804.1.16",
          "form": "아근처에....먹을곳없으면 답없죠 진짱..ㅜㅠ",
          "original_form": "아근처에....먹을곳없으면 답없죠 진짱..ㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 15:18"
        },
        {
          "id": "MDRW2100029804.1.17",
          "form": "구내더있긴한데",
          "original_form": "구내더있긴한데",
          "speaker_id": "2",
          "time": "20210903 15:18"
        },
        {
          "id": "MDRW2100029804.1.18",
          "form": "가끔 맛없는거 나오면",
          "original_form": "가끔 맛없는거 나오면",
          "speaker_id": "2",
          "time": "20210903 15:18"
        },
        {
          "id": "MDRW2100029804.1.19",
          "form": "밖에서먹죠.....ㅎ..ㅎ",
          "original_form": "밖에서먹죠.....ㅎ..ㅎ",
          "speaker_id": "2",
          "time": "20210903 15:18"
        },
        {
          "id": "MDRW2100029804.1.20",
          "form": "오 구내있는 거 제일 부러워요 진짜 ㅠㅠ",
          "original_form": "오 구내있는 거 제일 부러워요 진짜 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100029804.1.21",
          "form": "메뉴고민필요없고",
          "original_form": "메뉴고민필요없고",
          "speaker_id": "1",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100029804.1.22",
          "form": "맞아요 그건 정말 좋은거같아요 ㅎㅎ",
          "original_form": "맞아요 그건 정말 좋은거같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100029804.1.23",
          "form": "그런 거 뭐 있나요?",
          "original_form": "그런 거 뭐 있나요?",
          "speaker_id": "1",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100029804.1.24",
          "form": "아 뭐라닠ㅋㅋㅋㅋ",
          "original_form": "아 뭐라닠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100029804.1.25",
          "form": "저도 누가해주는 밥 먹고싶어요",
          "original_form": "저도 누가해주는 밥 먹고싶어요",
          "speaker_id": "1",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100029804.1.26",
          "form": "맞아요 누가 해주는밥이 제일 맛있고 편해요 ㅋㅋㅋ",
          "original_form": "맞아요 누가 해주는밥이 제일 맛있고 편해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100029804.1.27",
          "form": "정말...음식하나 만들면..",
          "original_form": "정말...음식하나 만들면..",
          "speaker_id": "2",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100029804.1.28",
          "form": "진이빠져요 ..설거지랑 치우기까지하면 ㅋㅋㅋ",
          "original_form": "진이빠져요 ..설거지랑 치우기까지하면 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100029804.1.29",
          "form": "아 진짜요 ㅋㅋㅋ 저는 점심 저녁 다 제가해서 흑",
          "original_form": "아 진짜요 ㅋㅋㅋ 저는 점심 저녁 다 제가해서 흑",
          "speaker_id": "1",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100029804.1.30",
          "form": "넘힘들어요",
          "original_form": "넘힘들어요",
          "speaker_id": "1",
          "time": "20210903 15:36"
        }
      ]
    }
