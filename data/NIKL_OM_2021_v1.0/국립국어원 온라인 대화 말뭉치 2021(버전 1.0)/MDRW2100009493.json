      "utterance": [
        {
          "id": "MDRW2100009493.1.1",
          "form": "점심은 배달인가요?",
          "original_form": "점심은 배달인가요?",
          "speaker_id": "1",
          "time": "20210830 11:12"
        },
        {
          "id": "MDRW2100009493.1.2",
          "form": "아니요 샌드위치 해먹을거 같아요",
          "original_form": "아니요 샌드위치 해먹을거 같아요",
          "speaker_id": "2",
          "time": "20210830 11:16"
        },
        {
          "id": "MDRW2100009493.1.3",
          "form": "점심 뭐드세요?",
          "original_form": "점심 뭐드세요?",
          "speaker_id": "2",
          "time": "20210830 11:16"
        },
        {
          "id": "MDRW2100009493.1.4",
          "form": "직접 해드세요? 뭐뭐 넣어드세요? \r\n저두 샐러드랑 튀김해먹을듯..",
          "original_form": "직접 해드세요? 뭐뭐 넣어드세요? \r\n저두 샐러드랑 튀김해먹을듯..",
          "speaker_id": "1",
          "time": "20210830 11:17"
        },
        {
          "id": "MDRW2100009493.1.5",
          "form": "저는 양상추랑 햄 계란 치즈넣어서 해먹을거 같아요",
          "original_form": "저는 양상추랑 햄 계란 치즈넣어서 해먹을거 같아요",
          "speaker_id": "2",
          "time": "20210830 11:18"
        },
        {
          "id": "MDRW2100009493.1.6",
          "form": "집에 있는 재료 떨이해서 먹으려구요",
          "original_form": "집에 있는 재료 떨이해서 먹으려구요",
          "speaker_id": "2",
          "time": "20210830 11:18"
        },
        {
          "id": "MDRW2100009493.1.7",
          "form": "집에 떨이로 치즈가 있어여? ㅋㅋㅋ",
          "original_form": "집에 떨이로 치즈가 있어여? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:19"
        },
        {
          "id": "MDRW2100009493.1.8",
          "form": "무슨 샐러드 드세요?",
          "original_form": "무슨 샐러드 드세요?",
          "speaker_id": "2",
          "time": "20210830 11:19"
        },
        {
          "id": "MDRW2100009493.1.9",
          "form": "저도 양배추 당근 양파 베이스에.. \r\n그때그때 랜덤으로 석어 먹어요.. \r\n파프리카 브로콜리 상추 방율토맡ㅎ 같은거..",
          "original_form": "저도 양배추 당근 양파 베이스에.. \r\n그때그때 랜덤으로 석어 먹어요.. \r\n파프리카 브로콜리 상추 방율토맡ㅎ 같은거..",
          "speaker_id": "1",
          "time": "20210830 11:19"
        },
        {
          "id": "MDRW2100009493.1.10",
          "form": "건강하게 드시네요",
          "original_form": "건강하게 드시네요",
          "speaker_id": "2",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100009493.1.11",
          "form": "튀김은 뭐예요?",
          "original_form": "튀김은 뭐예요?",
          "speaker_id": "2",
          "time": "20210830 11:20"
        },
        {
          "id": "MDRW2100009493.1.12",
          "form": "야채튀김, 타코야끼, 취킨.. ㅋㅋㅋ",
          "original_form": "야채튀김, 타코야끼, 취킨.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:21"
        },
        {
          "id": "MDRW2100009493.1.13",
          "form": "냉동 엄청 많이 사요..",
          "original_form": "냉동 엄청 많이 사요..",
          "speaker_id": "1",
          "time": "20210830 11:21"
        },
        {
          "id": "MDRW2100009493.1.14",
          "form": "에.프로~~~",
          "original_form": "에.프로~~~",
          "speaker_id": "1",
          "time": "20210830 11:21"
        },
        {
          "id": "MDRW2100009493.1.15",
          "form": "냉동이 관리하기도 편하고 좋죠",
          "original_form": "냉동이 관리하기도 편하고 좋죠",
          "speaker_id": "2",
          "time": "20210830 11:24"
        },
        {
          "id": "MDRW2100009493.1.16",
          "form": "자취생 필수품",
          "original_form": "자취생 필수품",
          "speaker_id": "2",
          "time": "20210830 11:25"
        },
        {
          "id": "MDRW2100009493.1.17",
          "form": "제품추천 해주세요!",
          "original_form": "제품추천 해주세요!",
          "speaker_id": "2",
          "time": "20210830 11:25"
        },
        {
          "id": "MDRW2100009493.1.18",
          "form": "에프 많이 돌이세요? 아님 간편식?",
          "original_form": "에프 많이 돌이세요? 아님 간편식?",
          "speaker_id": "1",
          "time": "20210830 11:26"
        },
        {
          "id": "MDRW2100009493.1.19",
          "form": "에프 돌려요!",
          "original_form": "에프 돌려요!",
          "speaker_id": "2",
          "time": "20210830 11:31"
        },
        {
          "id": "MDRW2100009493.1.20",
          "form": "치킨류는 사세가 유명하죠..",
          "original_form": "치킨류는 사세가 유명하죠..",
          "speaker_id": "1",
          "time": "20210830 11:31"
        },
        {
          "id": "MDRW2100009493.1.21",
          "form": "전 식자재 마트가 근처에 있어서.. \r\n종류별로 다 먹어요.. \r\n탕슉 카라아게 야채튀김 오징어튀김 .. ㅋㅋ김말이..",
          "original_form": "전 식자재 마트가 근처에 있어서.. \r\n종류별로 다 먹어요.. \r\n탕슉 카라아게 야채튀김 오징어튀김 .. ㅋㅋ김말이..",
          "speaker_id": "1",
          "time": "20210830 11:32"
        },
        {
          "id": "MDRW2100009493.1.22",
          "form": "사세 저도 자주먹는데 사두면 너무 든든해요",
          "original_form": "사세 저도 자주먹는데 사두면 너무 든든해요",
          "speaker_id": "2",
          "time": "20210830 11:35"
        },
        {
          "id": "MDRW2100009493.1.23",
          "form": "식자재마트 너무 좋죠 저는 좀 거리가있어서 가끔가요",
          "original_form": "식자재마트 너무 좋죠 저는 좀 거리가있어서 가끔가요",
          "speaker_id": "2",
          "time": "20210830 11:36"
        }
      ]
    }
