      "utterance": [
        {
          "id": "MDRW2100015240.1.1",
          "form": "극장에서 영화보는거 좋아하세요?",
          "original_form": "극장에서 영화보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015240.1.2",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015240.1.3",
          "form": "네 샹치 기다려요 마블",
          "original_form": "네 샹치 기다려요 마블",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015240.1.4",
          "form": "마블좋아하시는구나 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "마블좋아하시는구나 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015240.1.5",
          "form": "네 뭐좋아하세요?",
          "original_form": "네 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015240.1.6",
          "form": "저도 마블 좋아해요 ㅋㅋㅋㅋ그런데 블랙위도우는 아직 못봄 ㅠㅠ",
          "original_form": "저도 마블 좋아해요 ㅋㅋㅋㅋ그런데 블랙위도우는 아직 못봄 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015240.1.7",
          "form": "전보다 중간에 못봤어요",
          "original_form": "전보다 중간에 못봤어요",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015240.1.8",
          "form": "낼보려고요 ㅋㅋ",
          "original_form": "낼보려고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015240.1.9",
          "form": "ㅋㅋㅋㅋ마블영화중에 뭘 제일 좋아하세요?",
          "original_form": "ㅋㅋㅋㅋ마블영화중에 뭘 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015240.1.10",
          "form": "전 다돟아하지만 어벤져스 시리즈요",
          "original_form": "전 다돟아하지만 어벤져스 시리즈요",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015240.1.11",
          "form": "어벤져스 시리즈 좋죠 ㅋㅋㅋㅋㅋ",
          "original_form": "어벤져스 시리즈 좋죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015240.1.12",
          "form": "언제들어도 웅장한 브금부터...",
          "original_form": "언제들어도 웅장한 브금부터...",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015240.1.13",
          "form": "네 아이엄낸하고욮",
          "original_form": "네 아이엄낸하고욮",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015240.1.14",
          "form": "아이언맨하고요",
          "original_form": "아이언맨하고요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015240.1.15",
          "form": "이제 안한다니 ㅋㅋ 아이언맨 아쉬워요",
          "original_form": "이제 안한다니 ㅋㅋ 아이언맨 아쉬워요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015240.1.16",
          "form": "저 피규어도 있었어요 ㅋㅋ",
          "original_form": "저 피규어도 있었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015240.1.17",
          "form": "아이언맨 근데 팔았어요",
          "original_form": "아이언맨 근데 팔았어요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015240.1.18",
          "form": "아이언맨좋아하셧구나 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아이언맨좋아하셧구나 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015240.1.19",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015240.1.20",
          "form": "저는 가오갤 시리즈 엄청 좋아햇는데",
          "original_form": "저는 가오갤 시리즈 엄청 좋아햇는데",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015240.1.21",
          "form": "어벤져스에서 너무... 흑흑....",
          "original_form": "어벤져스에서 너무... 흑흑....",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015240.1.22",
          "form": "저도 가오갤 잼나요",
          "original_form": "저도 가오갤 잼나요",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015240.1.23",
          "form": "사실 안재미있는게 없어요 ㅋㅋ",
          "original_form": "사실 안재미있는게 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015240.1.24",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015240.1.25",
          "form": "캡아 윈터솔져도 좋아햇어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "캡아 윈터솔져도 좋아햇어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015240.1.26",
          "form": "네 잘생기고 착한 히어로",
          "original_form": "네 잘생기고 착한 히어로",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015240.1.27",
          "form": "토르 라그나로크도 너무 좋았고 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "토르 라그나로크도 너무 좋았고 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015240.1.28",
          "form": "맞아요 다른 시리즈보다 밝아서 나았어요",
          "original_form": "맞아요 다른 시리즈보다 밝아서 나았어요",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015240.1.29",
          "form": "맞아요 음악도 너무 좋았어요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 음악도 너무 좋았어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015240.1.30",
          "form": "좀 신경쓴거 같아요",
          "original_form": "좀 신경쓴거 같아요",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015240.1.31",
          "form": "토르 윽 엔드게임에서 살쪄서 엔드게임 충격이였죠 컨셉이 ㅋㅌ",
          "original_form": "토르 윽 엔드게임에서 살쪄서 엔드게임 충격이였죠 컨셉이 ㅋㅌ",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015240.1.32",
          "form": "라그나로크에서 그렇게 멋잇엇는데ㅠㅠ",
          "original_form": "라그나로크에서 그렇게 멋잇엇는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015240.1.33",
