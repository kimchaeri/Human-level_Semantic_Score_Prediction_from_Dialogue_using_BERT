        {
          "id": "MDRW2100033026.1.1",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100033026.1.2",
          "form": "명상이 그럼 인도에서 왔군요",
          "original_form": "명상이 그럼 인도에서 왔군요",
          "speaker_id": "1",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100033026.1.3",
          "form": "재택싫어요 사람만나서 일하는게 더 좋아요",
          "original_form": "재택싫어요 사람만나서 일하는게 더 좋아요",
          "speaker_id": "2",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100033026.1.4",
          "form": "아 불교예요",
          "original_form": "아 불교예요",
          "speaker_id": "2",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100033026.1.5",
          "form": "그 경험이 좋은 재산이 될 거예요",
          "original_form": "그 경험이 좋은 재산이 될 거예요",
          "speaker_id": "1",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100033026.1.6",
          "form": "성지순례로 먼저 다녀왔죠",
          "original_form": "성지순례로 먼저 다녀왔죠",
          "speaker_id": "2",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100033026.1.7",
          "form": "저는 기독교 성지 순례 다녀왔는데ㅎ",
          "original_form": "저는 기독교 성지 순례 다녀왔는데ㅎ",
          "speaker_id": "1",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100033026.1.8",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100033026.1.9",
          "form": "비슷한 듯 다르네요",
          "original_form": "비슷한 듯 다르네요",
          "speaker_id": "1",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100033026.1.10",
          "form": "이스라엘 이집트 요르단 다녀왔어요",
          "original_form": "이스라엘 이집트 요르단 다녀왔어요",
          "speaker_id": "1",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100033026.1.11",
          "form": "기독교성지는 어디어디예여",
          "original_form": "기독교성지는 어디어디예여",
          "speaker_id": "2",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100033026.1.12",
          "form": "아 지금은 가기 힘든 곳들이네",
          "original_form": "아 지금은 가기 힘든 곳들이네",
          "speaker_id": "2",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100033026.1.13",
          "form": "14박 15일 이요",
          "original_form": "14박 15일 이요",
          "speaker_id": "1",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100033026.1.14",
          "form": "네 제가 스무 살 때 갔으니까 오래 전이에요",
          "original_form": "네 제가 스무 살 때 갔으니까 오래 전이에요",
          "speaker_id": "1",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100033026.1.15",
          "form": "멋지십니다",
          "original_form": "멋지십니다",
          "speaker_id": "2",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100033026.1.16",
          "form": "모태신앙이신가",
          "original_form": "모태신앙이신가",
          "speaker_id": "2",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100033026.1.17",
          "form": "일찍 가셨네요",
          "original_form": "일찍 가셨네요",
          "speaker_id": "2",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100033026.1.18",
          "form": "전 29에 깄어요",
          "original_form": "전 29에 깄어요",
          "speaker_id": "2",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100033026.1.19",
          "form": "못해신앙이라하져ㅋ",
          "original_form": "못해신앙이라하져ㅋ",
          "speaker_id": "1",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100033026.1.20",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "2",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100033026.1.21",
          "form": "29살에 가는게 더 좋을 거예요",
          "original_form": "29살에 가는게 더 좋을 거예요",
          "speaker_id": "1",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100033026.1.22",
          "form": "그때는 놀고 먹은 거밖에 기억이 안나요",
          "original_form": "그때는 놀고 먹은 거밖에 기억이 안나요",
          "speaker_id": "1",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100033026.1.23",
          "form": "디자인일 하시면 미적감각이 좋으신 분이겠어요",
          "original_form": "디자인일 하시면 미적감각이 좋으신 분이겠어요",
          "speaker_id": "2",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100033026.1.24",
          "form": "디자인이 감각이란 얘기가 있는데",
          "original_form": "디자인이 감각이란 얘기가 있는데",
          "speaker_id": "1",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100033026.1.25",
          "form": "제가 보기에는 경험이에요",
          "original_form": "제가 보기에는 경험이에요",
          "speaker_id": "1",
          "time": "20210906 14:09"
        }
      ]
