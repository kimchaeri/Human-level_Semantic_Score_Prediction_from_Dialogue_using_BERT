      "utterance": [
        {
          "id": "MDRW2100017251.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017251.1.2",
          "form": "많이 좋아해요 ㅎ",
          "original_form": "많이 좋아해요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017251.1.3",
          "form": "오 최근에 다녀오신 곳이 있나요",
          "original_form": "오 최근에 다녀오신 곳이 있나요",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017251.1.4",
          "form": "이번달에 여름휴가 울릉도 독도 다녀왔어욤",
          "original_form": "이번달에 여름휴가 울릉도 독도 다녀왔어욤",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017251.1.5",
          "form": "우와 너무 멋집니다",
          "original_form": "우와 너무 멋집니다",
          "speaker_id": "2",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017251.1.6",
          "form": "한번도 못가봑어요 엄두도 안나고",
          "original_form": "한번도 못가봑어요 엄두도 안나고",
          "speaker_id": "2",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017251.1.7",
          "form": "독도입도는 진짜 인상깊었어요",
          "original_form": "독도입도는 진짜 인상깊었어요",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017251.1.8",
          "form": "최근에 여행 다녀오신곳 잇으세요?",
          "original_form": "최근에 여행 다녀오신곳 잇으세요?",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017251.1.9",
          "form": "저는 여행안간지 오래네요ㅠ",
          "original_form": "저는 여행안간지 오래네요ㅠ",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017251.1.10",
          "form": "독도ㅠㅠㅠ부럽습니다",
          "original_form": "독도ㅠㅠㅠ부럽습니다",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017251.1.11",
          "form": "코시국 언능 끝낫음해요ㅜㅜ",
          "original_form": "코시국 언능 끝낫음해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017251.1.12",
          "form": "멀리가고싶어요",
          "original_form": "멀리가고싶어요",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017251.1.13",
          "form": "맞아요 외국가고싶어요",
          "original_form": "맞아요 외국가고싶어요",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017251.1.14",
          "form": "ㅠㅠ진심",
          "original_form": "ㅠㅠ진심",
          "speaker_id": "1",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017251.1.15",
          "form": "남친도 외국에있어서 가봐야하는데",
          "original_form": "남친도 외국에있어서 가봐야하는데",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017251.1.16",
          "form": "에고ㅜㅜㅜㅜ",
          "original_form": "에고ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017251.1.17",
          "form": "1년반째못가고있어요",
          "original_form": "1년반째못가고있어요",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017251.1.18",
          "form": "롱디시구나ㅠㅠ",
          "original_form": "롱디시구나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017251.1.19",
          "form": "저는 내년에 결혼하는데",
          "original_form": "저는 내년에 결혼하는데",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017251.1.20",
          "form": "신행은 멀리가고싶어서 미룰꺼에요ㅜ",
          "original_form": "신행은 멀리가고싶어서 미룰꺼에요ㅜ",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017251.1.21",
          "form": "우와 결혼 하시는군요",
          "original_form": "우와 결혼 하시는군요",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017251.1.22",
          "form": "맞아요 코시국 끝나고 신행 가셔요ㅠㅠ어디가고싶으세요",
          "original_form": "맞아요 코시국 끝나고 신행 가셔요ㅠㅠ어디가고싶으세요",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017251.1.23",
          "form": "2주정도 갈수있어서",
          "original_form": "2주정도 갈수있어서",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017251.1.24",
          "form": "유럽이나 호주 생각중인데",
          "original_form": "유럽이나 호주 생각중인데",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017251.1.25",
          "form": "다가고싶어요",
          "original_form": "다가고싶어요",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017251.1.26",
          "form": "우와 그쵸 저도 유럽 가고싶어요",
          "original_form": "우와 그쵸 저도 유럽 가고싶어요",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017251.1.27",
          "form": "저는 한번 다녀왓는데 넘 좋앗어서",
          "original_form": "저는 한번 다녀왓는데 넘 좋앗어서",
          "speaker_id": "1",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017251.1.28",
          "form": "다시가고싶어요",
          "original_form": "다시가고싶어요",
          "speaker_id": "1",
          "time": "20210826 18:41"
        }
      ]
    }
