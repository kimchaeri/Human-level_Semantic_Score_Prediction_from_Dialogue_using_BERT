      "utterance": [
        {
          "id": "MDRW2100039053.1.1",
          "form": "제니 만나러 얼른 대만 가는날이 오길 바랄게요",
          "original_form": "제니 만나러 얼른 대만 가는날이 오길 바랄게요",
          "speaker_id": "2",
          "time": "20210903 08:56"
        },
        {
          "id": "MDRW2100039053.1.2",
          "form": "헤헤 고맙다능~",
          "original_form": "헤헤 고맙다능~{emoji:♡}",
          "speaker_id": "1",
          "time": "20210903 10:26"
        },
        {
          "id": "MDRW2100039053.1.3",
          "form": "그 전에 나도 중국어를 많이 배워놔야하는데...",
          "original_form": "그 전에 나도 중국어를 많이 배워놔야하는데...",
          "speaker_id": "1",
          "time": "20210903 10:26"
        },
        {
          "id": "MDRW2100039053.1.4",
          "form": "ㅋㅋㅋㅋ대만식 중국어 쪼금 다르지 않아?",
          "original_form": "ㅋㅋㅋㅋ대만식 중국어 쪼금 다르지 않아?",
          "speaker_id": "2",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100039053.1.5",
          "form": "중국어 진짜 어려워",
          "original_form": "중국어 진짜 어려워",
          "speaker_id": "2",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100039053.1.6",
          "form": "맞아 되게 다르더라구",
          "original_form": "맞아 되게 다르더라구",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039053.1.7",
          "form": "억양도 다르고...",
          "original_form": "억양도 다르고...",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039053.1.8",
          "form": "어순만 영어랑 같고ㅠ",
          "original_form": "어순만 영어랑 같고ㅠ",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039053.1.9",
          "form": "맞아 나는 한자랑 병음이랑 성조까지 삼박자를 외워야한단게 진짜 어렵더랑",
          "original_form": "맞아 나는 한자랑 병음이랑 성조까지 삼박자를 외워야한단게 진짜 어렵더랑",
          "speaker_id": "2",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100039053.1.10",
          "form": "중딩때 배운게 끝이지만...ㅎㅎ",
          "original_form": "중딩때 배운게 끝이지만...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100039053.1.11",
          "form": "와 대박대박",
          "original_form": "와 대박대박",
          "speaker_id": "1",
          "time": "20210903 14:04"
        },
        {
          "id": "MDRW2100039053.1.12",
          "form": "그래도 지금의 나보다 더 많이 알겠는데?!",
          "original_form": "그래도 지금의 나보다 더 많이 알겠는데?!",
          "speaker_id": "1",
          "time": "20210903 14:04"
        },
        {
          "id": "MDRW2100039053.1.13",
          "form": "중학교 때 중국어를 배웠다니 정말 대단해",
          "original_form": "중학교 때 중국어를 배웠다니 정말 대단해",
          "speaker_id": "1",
          "time": "20210903 14:04"
        },
        {
          "id": "MDRW2100039053.1.14",
          "form": "앜ㅋㅋㅋ아니야",
          "original_form": "앜ㅋㅋㅋ아니야",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100039053.1.15",
          "form": "그냥 학교 수업이여서 배운게 전부",
          "original_form": "그냥 학교 수업이여서 배운게 전부",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100039053.1.16",
          "form": "제니는 대만 어디쪽에 살아?.?",
          "original_form": "제니는 대만 어디쪽에 살아?.?",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100039053.1.17",
          "form": "제니는",
          "original_form": "제니는",
          "speaker_id": "1",
          "time": "20210903 17:02"
        },
        {
          "id": "MDRW2100039053.1.18",
          "form": "타이중에 살아!",
          "original_form": "타이중에 살아!",
          "speaker_id": "1",
          "time": "20210903 17:02"
        },
        {
          "id": "MDRW2100039053.1.19",
          "form": "덕분에 대만 지리에 대해서도 좀 알게됐엌ㅋㅋ",
          "original_form": "덕분에 대만 지리에 대해서도 좀 알게됐엌ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:02"
        },
        {
          "id": "MDRW2100039053.1.20",
          "form": "아아 중남부쪽이었나?",
          "original_form": "아아 중남부쪽이었나?",
          "speaker_id": "2",
          "time": "20210903 17:04"
        },
        {
          "id": "MDRW2100039053.1.21",
          "form": "타이중이랑 카오슝도 여행가보고싶었는데",
          "original_form": "타이중이랑 카오슝도 여행가보고싶었는데",
          "speaker_id": "2",
          "time": "20210903 17:04"
        },
        {
          "id": "MDRW2100039053.1.22",
          "form": "지난번엔 타이베이 위주로만 갔었어 :)",
          "original_form": "지난번엔 타이베이 위주로만 갔었어 :)",
          "speaker_id": "2",
          "time": "20210903 17:05"
        },
        {
          "id": "MDRW2100039053.1.23",
          "form": "맞아맞아!!!",
          "original_form": "맞아맞아!!!",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039053.1.24",
          "form": "타이베이가 타이난 맞지?!",
          "original_form": "타이베이가 타이난 맞지?!",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039053.1.25",
          "form": "아 아닌가?!",
          "original_form": "아 아닌가?!",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039053.1.26",
          "form": "맞아 카오슝 ㅠ",
          "original_form": "맞아 카오슝 ㅠ",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039053.1.27",
          "form": "카오슝도 가고싶어,,",
          "original_form": "카오슝도 가고싶어,,",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039053.1.28",
          "form": "음..같은곳인지모르겠어 수도 ㅎㅎ",
          "original_form": "음..같은곳인지모르겠어 수도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 18:10"
        },
        {
          "id": "MDRW2100039053.1.29",
          "form": "아아 내가 알기론",
          "original_form": "아아 내가 알기론",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039053.1.30",
          "form": "타이난 안에 타이베이가 있는걸로 알아!!!!",
          "original_form": "타이난 안에 타이베이가 있는걸로 알아!!!!",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039053.1.31",
          "form": "물가도 싸고 음식이 그렇게 맛있다는데!",
          "original_form": "물가도 싸고 음식이 그렇게 맛있다는데!",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039053.1.32",
          "form": "아하 그렇구나 ㅎㅎ",
          "original_form": "아하 그렇구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039053.1.33",
          "form": "나는 향신료를 못 먹어서",
          "original_form": "나는 향신료를 못 먹어서",
          "speaker_id": "2",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039053.1.34",
          "form": "일주일동안 디저트로 배채운것 같앜ㅋㅋㅋㅋㅋ",
          "original_form": "일주일동안 디저트로 배채운것 같앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039053.1.35",
          "form": "우육면 이런거 좋아하는사람들에겐 천국",
          "original_form": "우육면 이런거 좋아하는사람들에겐 천국",
          "speaker_id": "2",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100039053.1.36",
          "form": "그렇다면",
          "original_form": "그렇다면",
          "speaker_id": "1",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100039053.1.37",
          "form": "나한텐 정말 천국이네",
          "original_form": "나한텐 정말 천국이네",
          "speaker_id": "1",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100039053.1.38",
          "form": "앜ㅋㅋㅋㅋㅋ ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "앜ㅋㅋㅋㅋㅋ ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100039053.1.39",
          "form": "거기 디저트듀 엄청 많고",
          "original_form": "거기 디저트듀 엄청 많고",
          "speaker_id": "1",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100039053.1.40",
          "form": "타피오카 펄이 잔뜩 들어간 호떡도 있대.....",
          "original_form": "타피오카 펄이 잔뜩 들어간 호떡도 있대.....",
          "speaker_id": "1",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100039053.1.41",
