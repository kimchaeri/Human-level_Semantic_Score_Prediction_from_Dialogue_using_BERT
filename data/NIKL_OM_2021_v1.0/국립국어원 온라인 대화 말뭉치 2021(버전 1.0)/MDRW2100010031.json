      "utterance": [
        {
          "id": "MDRW2100010031.1.1",
          "form": "기숙사 생활하신 경험이 있으신가요?",
          "original_form": "기숙사 생활하신 경험이 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010031.1.2",
          "form": "네 한 세달정도요",
          "original_form": "네 한 세달정도요",
          "speaker_id": "2",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010031.1.3",
          "form": "룸메이트 분과는 잘 맞으셨나요?",
          "original_form": "룸메이트 분과는 잘 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100010031.1.4",
          "form": "저는 언니들 이어서 괜찮았어요",
          "original_form": "저는 언니들 이어서 괜찮았어요",
          "speaker_id": "2",
          "time": "20210823 14:34"
        },
        {
          "id": "MDRW2100010031.1.5",
          "form": "잘 맞으셨다면 다행이네요! 그 부분에서 오는 스트레스도 많으니까요ㅠ",
          "original_form": "잘 맞으셨다면 다행이네요! 그 부분에서 오는 스트레스도 많으니까요ㅠ",
          "speaker_id": "1",
          "time": "20210823 14:37"
        },
        {
          "id": "MDRW2100010031.1.6",
          "form": "기숙사 생활하신적 있으세요?",
          "original_form": "기숙사 생활하신적 있으세요?",
          "speaker_id": "2",
          "time": "20210823 16:52"
        },
        {
          "id": "MDRW2100010031.1.7",
          "form": "저도 짧게 생활해본적 있어요!",
          "original_form": "저도 짧게 생활해본적 있어요!",
          "speaker_id": "1",
          "time": "20210823 17:14"
        },
        {
          "id": "MDRW2100010031.1.8",
          "form": "오 룸메 괜찮았나요?",
          "original_form": "오 룸메 괜찮았나요?",
          "speaker_id": "2",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010031.1.9",
          "form": "생활 패턴이 비슷하고 청소 분담도 괜찮게 이루어져서 짧지만 절 지냈어요ㅎㅎ",
          "original_form": "생활 패턴이 비슷하고 청소 분담도 괜찮게 이루어져서 짧지만 절 지냈어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 17:26"
        },
        {
          "id": "MDRW2100010031.1.10",
          "form": "ㅋㅋ 진짜 룸메 잘만나는거 넘 중요한 것 같아요",
          "original_form": "ㅋㅋ 진짜 룸메 잘만나는거 넘 중요한 것 같아요",
          "speaker_id": "2",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010031.1.11",
          "form": "룸메이트랑 잘 안 맞으면 시시건건 부딪히게 되니까요ㅋㅋ",
          "original_form": "룸메이트랑 잘 안 맞으면 시시건건 부딪히게 되니까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:39"
        },
        {
          "id": "MDRW2100010031.1.12",
          "form": "맞아요 제친구는 룸메언니가 도둑으로 몬적도 있어요",
          "original_form": "맞아요 제친구는 룸메언니가 도둑으로 몬적도 있어요",
          "speaker_id": "2",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100010031.1.13",
          "form": "헐...어떻게 대처하셨대요? 너무 어이가 없으셨겠어요ㅠ",
          "original_form": "헐...어떻게 대처하셨대요? 너무 어이가 없으셨겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210824 19:47"
        },
        {
          "id": "MDRW2100010031.1.14",
          "form": "친구랑 그언니랑 싸웠어요",
          "original_form": "친구랑 그언니랑 싸웠어요",
          "speaker_id": "2",
          "time": "20210824 20:00"
        },
        {
          "id": "MDRW2100010031.1.15",
          "form": "그언니 친구까지 대동해서 난리였었죠 ㅋㅋ",
          "original_form": "그언니 친구까지 대동해서 난리였었죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:00"
        },
        {
          "id": "MDRW2100010031.1.16",
          "form": "아니...같이 사는 동지인데 왜 그런 어이없는 짓을 했을까요ㅠ",
          "original_form": "아니...같이 사는 동지인데 왜 그런 어이없는 짓을 했을까요ㅠ",
          "speaker_id": "1",
          "time": "20210824 21:06"
        }
      ]
