      "utterance": [
        {
          "id": "MDRW2100043931.1.1",
          "form": "배가 부르지만",
          "original_form": "배가 부르지만",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043931.1.2",
          "form": "입이궁금하네요",
          "original_form": "입이궁금하네요",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043931.1.3",
          "form": "저도요ㅠㅋㅋㅋㅋ",
          "original_form": "저도요ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043931.1.4",
          "form": "간식 드시나요??",
          "original_form": "간식 드시나요??",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043931.1.5",
          "form": "저는 초콜렛이랑 먹으려고 홍차 우렸어요 ㅋㅋㅋ",
          "original_form": "저는 초콜렛이랑 먹으려고 홍차 우렸어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043931.1.6",
          "form": "저 지금 고민중이에요",
          "original_form": "저 지금 고민중이에요",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043931.1.7",
          "form": "오 초콜렛에 홍차!!!!!!! 뭔가 고급진데요",
          "original_form": "오 초콜렛에 홍차!!!!!!! 뭔가 고급진데요",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043931.1.8",
          "form": "저는 어제. 사온 딸기요거트롤 케익을 먹을까해요",
          "original_form": "저는 어제. 사온 딸기요거트롤 케익을 먹을까해요",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043931.1.9",
          "form": "우와 왜 안드시고 계세요????",
          "original_form": "우와 왜 안드시고 계세요????",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043931.1.10",
          "form": "딸기요거트롤이라뇨 되게 고급지네요",
          "original_form": "딸기요거트롤이라뇨 되게 고급지네요",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043931.1.11",
          "form": "우유랑 먹어도 맛있겠어요",
          "original_form": "우유랑 먹어도 맛있겠어요",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043931.1.12",
          "form": "꺅",
          "original_form": "꺅",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043931.1.13",
          "form": "저 당장 썰어왔어요ㅋㄲ",
          "original_form": "저 당장 썰어왔어요ㅋㄲ",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043931.1.14",
          "form": "우와 ㅋㅋㅋㅋㅋ",
          "original_form": "우와 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043931.1.15",
          "form": "사진 달라고 하고싶어요 ㅋㅋㅋ",
          "original_form": "사진 달라고 하고싶어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043931.1.16",
          "form": "먹어보겠습니다 ㅎ 일찍 자야해서 우유는 못먹고 물에다 먹을게요",
          "original_form": "먹어보겠습니다 ㅎ 일찍 자야해서 우유는 못먹고 물에다 먹을게요",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043931.1.17",
          "form": "딸기요거트롤은 어디서 파는 건가요 ㅋㅋㅋㅋ",
          "original_form": "딸기요거트롤은 어디서 파는 건가요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043931.1.18",
          "form": "네!!!",
          "original_form": "네!!!",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043931.1.19",
          "form": "아 보내고싶지만 아숩네요",
          "original_form": "아 보내고싶지만 아숩네요",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043931.1.20",
          "form": "감상 알려주세요 ㅋㅋㅋㅋ",
          "original_form": "감상 알려주세요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043931.1.21",
          "form": "요고 몽슈슈꺼에요!",
          "original_form": "요고 몽슈슈꺼에요!",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043931.1.22",
          "form": "아시나요!",
          "original_form": "아시나요!",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043931.1.23",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043931.1.24",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043931.1.25",
          "form": "사실 집앞에 있어요",
          "original_form": "사실 집앞에 있어요",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043931.1.26",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043931.1.27",
          "form": "내일 가봐야겠네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "내일 가봐야겠네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043931.1.28",
          "form": "항상 복주머니만 살까말까 하다가 말았어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "항상 복주머니만 살까말까 하다가 말았어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043931.1.29",
          "form": "헐ㅋㅋ 집앞에 있다니 부럽네요",
          "original_form": "헐ㅋㅋ 집앞에 있다니 부럽네요",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043931.1.30",
          "form": "전 맘먹고나가야하는데ㅋㅋㅋㅋ",
          "original_form": "전 맘먹고나가야하는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043931.1.31",
          "form": "내일 먹어주세요!!!!!",
          "original_form": "내일 먹어주세요!!!!!",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043931.1.32",
          "form": "사실 백화점이 이번에 생겼는데요 거기 들어가있더라구요 ㅋㅋㅋ",
          "original_form": "사실 백화점이 이번에 생겼는데요 거기 들어가있더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043931.1.33",
          "form": "네 함 가봐야겠어요 ㅋㅋㅋ",
          "original_form": "네 함 가봐야겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043931.1.34",
          "form": "맛 어땡ㅅ???",
          "original_form": "맛 어땡ㅅ???",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043931.1.35",
          "form": "어때요???",
          "original_form": "어때요???",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043931.1.36",
          "form": "아하~맞아요 저도 백화점에서 샀어요 ㅎ",
          "original_form": "아하~맞아요 저도 백화점에서 샀어요 ㅎ",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100043931.1.37",
          "form": "저는 좋아해요 근데 아무래도 기본적인 도지마롤이 제일맛있는",
          "original_form": "저는 좋아해요 근데 아무래도 기본적인 도지마롤이 제일맛있는",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043931.1.38",
          "form": "도지마롤 맛있나요??? 저 아직 못먹어봤어요 ㅋㅋㅋ",
          "original_form": "도지마롤 맛있나요??? 저 아직 못먹어봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100043931.1.39",
          "form": "전 그게 젤 맛있어요",
          "original_form": "전 그게 젤 맛있어요",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043931.1.40",
          "form": "예전에는 일본가면 무조건 그거 사먹었는데",
          "original_form": "예전에는 일본가면 무조건 그거 사먹었는데",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043931.1.41",
          "form": "어느순간 한국에 입점했더라고요",
          "original_form": "어느순간 한국에 입점했더라고요",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043931.1.42",
          "form": "우와 ㅇ0ㅇ",
          "original_form": "우와 ㅇ0ㅇ",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100043931.1.43",
          "form": "일본이랑 맛 비슷한가요???",
          "original_form": "일본이랑 맛 비슷한가요???",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100043931.1.44",
          "form": "네 ㅋㄱㅋ비슷해요!",
          "original_form": "네 ㅋㄱㅋ비슷해요!",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100043931.1.45",
          "form": "저 몽슈슈를 애프터눈티세트로만 한 번 먹어보고 못먹어봤어요",
          "original_form": "저 몽슈슈를 애프터눈티세트로만 한 번 먹어보고 못먹어봤어요",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100043931.1.46",
          "form": "헐 내일 가면 도지마롤부터 먹어야겠어요 ㅋㅋㅋㅋ",
          "original_form": "헐 내일 가면 도지마롤부터 먹어야겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100043931.1.47",
          "form": "그땐 진짜 집착적으로 거기가서 사고 그랬는데 ㅎ",
          "original_form": "그땐 진짜 집착적으로 거기가서 사고 그랬는데 ㅎ",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100043931.1.48",
          "form": "이젠 먹고싶을때가서 사오니깐 좋아요 ㅎ",
          "original_form": "이젠 먹고싶을때가서 사오니깐 좋아요 ㅎ",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100043931.1.49",
          "form": "너무 기대는마세요ㅠ",
          "original_form": "너무 기대는마세요ㅠ",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100043931.1.50",
          "form": "제가 사실 롤케이크 좋아해서요ㅋㅋㅋㅋㅋ",
          "original_form": "제가 사실 롤케이크 좋아해서요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100043931.1.51",
          "form": "백화점 가면 먹을게 넘 많아서 항상 사려다가 다른 거 샀는데 이참에 한번 먹어봐야겠어요ㅠㅋㅋㅋ",
          "original_form": "백화점 가면 먹을게 넘 많아서 항상 사려다가 다른 거 샀는데 이참에 한번 먹어봐야겠어요ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100043931.1.52",
          "form": "그런가요!",
          "original_form": "그런가요!",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100043931.1.53",
          "form": "맞아요..백화점 맛있는거 너무많아요",
          "original_form": "맞아요..백화점 맛있는거 너무많아요",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100043931.1.54",
          "form": "이거저거 사다가 카드값 거덜나는",
          "original_form": "이거저거 사다가 카드값 거덜나는",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100043931.1.55",
          "form": "맞아요!!!!@",
          "original_form": "맞아요!!!!@",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100043931.1.56",
          "form": "진짜 카드값이 야금야금 ㅋㅋㅋㅋ",
          "original_form": "진짜 카드값이 야금야금 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100043931.1.57",
          "form": "백화점 디저트 중 가장 맛난 건 어떤거였어요??",
          "original_form": "백화점 디저트 중 가장 맛난 건 어떤거였어요??",
