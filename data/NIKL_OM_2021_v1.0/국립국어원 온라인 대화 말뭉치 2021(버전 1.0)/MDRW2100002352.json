      "utterance": [
        {
          "id": "MDRW2100002352.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002352.1.2",
          "form": "쉽게할수있는 음식 레시피 있으세요?",
          "original_form": "쉽게할수있는 음식 레시피 있으세요?",
          "speaker_id": "2",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002352.1.3",
          "form": "음,, 파스타요! ㅋㅋ 의외로 쉬워요",
          "original_form": "음,, 파스타요! ㅋㅋ 의외로 쉬워요",
          "speaker_id": "1",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002352.1.4",
          "form": "저는 고기파라 고기류 좋아해요",
          "original_form": "저는 고기파라 고기류 좋아해요",
          "speaker_id": "2",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002352.1.5",
          "form": "오 파스타 크림? 토마토?",
          "original_form": "오 파스타 크림? 토마토?",
          "speaker_id": "2",
          "time": "20210820 10:11"
        },
        {
          "id": "MDRW2100002352.1.6",
          "form": "저두 고기 넘 좋아해요!! ㅠ.ㅠ 전 삼겹살",
          "original_form": "저두 고기 넘 좋아해요!! ㅠ.ㅠ 전 삼겹살",
          "speaker_id": "1",
          "time": "20210820 10:11"
        },
        {
          "id": "MDRW2100002352.1.7",
          "form": "음 크림도 좋고 토마토도 좋아요 둘다 은근 쉬워요ㅎㅎ 레시피만있다면~",
          "original_form": "음 크림도 좋고 토마토도 좋아요 둘다 은근 쉬워요ㅎㅎ 레시피만있다면~",
          "speaker_id": "1",
          "time": "20210820 10:11"
        },
        {
          "id": "MDRW2100002352.1.8",
          "form": "고기도 썰어넣으면 더 맛나고 ^^",
          "original_form": "고기도 썰어넣으면 더 맛나고 ^^",
          "speaker_id": "1",
          "time": "20210820 10:11"
        },
        {
          "id": "MDRW2100002352.1.9",
          "form": "파스타에도 고기요?",
          "original_form": "파스타에도 고기요?",
          "speaker_id": "2",
          "time": "20210820 10:12"
        },
        {
          "id": "MDRW2100002352.1.10",
          "form": "네 스테이크 같은거요! ㅎㅎ 아니면 닭 고기",
          "original_form": "네 스테이크 같은거요! ㅎㅎ 아니면 닭 고기",
          "speaker_id": "1",
          "time": "20210820 10:12"
        },
        {
          "id": "MDRW2100002352.1.11",
          "form": "아.., 스테이크.., 닭고기는 닭가슴살인가요?",
          "original_form": "아.., 스테이크.., 닭고기는 닭가슴살인가요?",
          "speaker_id": "2",
          "time": "20210820 10:13"
        },
        {
          "id": "MDRW2100002352.1.12",
          "form": "넵! 그럼 맛있어용",
          "original_form": "넵! 그럼 맛있어용",
          "speaker_id": "1",
          "time": "20210820 10:13"
        },
        {
          "id": "MDRW2100002352.1.13",
          "form": "그래요? 그거 넣을생걱은 한번도 안해봤어요",
          "original_form": "그래요? 그거 넣을생걱은 한번도 안해봤어요",
          "speaker_id": "2",
          "time": "20210820 10:16"
        },
        {
          "id": "MDRW2100002352.1.14",
          "form": "주말에 한번 시도해봐야겠어요",
          "original_form": "주말에 한번 시도해봐야겠어요",
          "speaker_id": "2",
          "time": "20210820 10:16"
        },
        {
          "id": "MDRW2100002352.1.15",
          "form": "넴넴 추천드려요 ㅎㅎ",
          "original_form": "넴넴 추천드려요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 10:16"
        },
        {
          "id": "MDRW2100002352.1.16",
          "form": "막창 대창 이런거드시나요?",
          "original_form": "막창 대창 이런거드시나요?",
          "speaker_id": "2",
          "time": "20210820 10:17"
        },
        {
          "id": "MDRW2100002352.1.17",
          "form": "오 ㅠㅠ좋아해용!! 비싸서 잘 못먹는 ㅠㅋㅋ",
          "original_form": "오 ㅠㅠ좋아해용!! 비싸서 잘 못먹는 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 10:17"
        },
        {
