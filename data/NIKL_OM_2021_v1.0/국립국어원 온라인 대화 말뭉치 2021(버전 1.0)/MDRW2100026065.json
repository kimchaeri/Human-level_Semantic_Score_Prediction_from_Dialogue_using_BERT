      "utterance": [
        {
          "id": "MDRW2100026065.1.1",
          "form": "요즘은 잘 안하네요",
          "original_form": "요즘은 잘 안하네요",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026065.1.2",
          "form": "예전엔 엄청 했었는데",
          "original_form": "예전엔 엄청 했었는데",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026065.1.3",
          "form": "잔 당근",
          "original_form": "잔 당근",
          "speaker_id": "1",
          "time": "20210901 19:31"
        },
        {
          "id": "MDRW2100026065.1.4",
          "form": "당근마켓 이용하시나보네요",
          "original_form": "당근마켓 이용하시나보네요",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026065.1.5",
          "form": "최근에 사신거있나요?",
          "original_form": "최근에 사신거있나요?",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026065.1.6",
          "form": "팔기만요",
          "original_form": "팔기만요",
          "speaker_id": "1",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100026065.1.7",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "2",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100026065.1.8",
          "form": "동네라서 편해여",
          "original_form": "동네라서 편해여",
          "speaker_id": "1",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026065.1.9",
          "form": "맞아요 당근이 동네 직거래하기에",
          "original_form": "맞아요 당근이 동네 직거래하기에",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026065.1.10",
          "form": "좋은 어플이죠",
          "original_form": "좋은 어플이죠",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026065.1.11",
          "form": "빠르거여",
          "original_form": "빠르거여",
          "speaker_id": "1",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026065.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026065.1.13",
          "form": "다만 네고가 ㅋㅋ",
          "original_form": "다만 네고가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026065.1.14",
          "form": "네고 흠..",
          "original_form": "네고 흠..",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026065.1.15",
          "form": "네고 별로에요 좀 ㅠㅠ",
          "original_form": "네고 별로에요 좀 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026065.1.16",
          "form": "싸게 파는데 그걸 또 네고할려고 하는 사람들은 더욱더 그렇고요",
          "original_form": "싸게 파는데 그걸 또 네고할려고 하는 사람들은 더욱더 그렇고요",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026065.1.17",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210901 19:50"
        },
        {
          "id": "MDRW2100026065.1.18",
          "form": "너무 비호감..",
          "original_form": "너무 비호감..",
          "speaker_id": "2",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100026065.1.19",
          "form": "악성이죠",
          "original_form": "악성이죠",
          "speaker_id": "1",
