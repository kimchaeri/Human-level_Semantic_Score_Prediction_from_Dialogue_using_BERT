      "utterance": [
        {
          "id": "MDRW2100032685.1.1",
          "form": "주말에 데이트하셨나요?",
          "original_form": "주말에 데이트하셨나요?",
          "speaker_id": "1",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100032685.1.2",
          "form": "넷플 격려 감사합니다",
          "original_form": "넷플 격려 감사합니다",
          "speaker_id": "2",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100032685.1.3",
          "form": "덕분에 결혼까지 가겠어요",
          "original_form": "덕분에 결혼까지 가겠어요",
          "speaker_id": "2",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100032685.1.4",
          "form": "뿌듯하네요",
          "original_form": "뿌듯하네요",
          "speaker_id": "1",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100032685.1.5",
          "form": "주말에 데이트 하고 왔습니다",
          "original_form": "주말에 데이트 하고 왔습니다",
          "speaker_id": "2",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100032685.1.6",
          "form": "뭐하셨어요?",
          "original_form": "뭐하셨어요?",
          "speaker_id": "1",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100032685.1.7",
          "form": "뽀뽀요",
          "original_form": "뽀뽀요",
          "speaker_id": "2",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100032685.1.8",
          "form": "어멋",
          "original_form": "어멋",
          "speaker_id": "1",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100032685.1.9",
          "form": "장난이구요 낙안읍성 갔다왔습니다",
          "original_form": "장난이구요 낙안읍성 갔다왔습니다",
          "speaker_id": "2",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100032685.1.10",
          "form": "장난 아니잖아요",
          "original_form": "장난 아니잖아요",
          "speaker_id": "1",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100032685.1.11",
          "form": "순천에 있는 거에요",
          "original_form": "순천에 있는 거에요",
          "speaker_id": "2",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100032685.1.12",
          "form": "그건 맞아요",
          "original_form": "그건 맞아요",
          "speaker_id": "2",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100032685.1.13",
          "form": "나이가 좀 있으신가 봐요?",
          "original_form": "나이가 좀 있으신가 봐요?",
          "speaker_id": "1",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100032685.1.14",
          "form": "낙안읍성도 가고 뽀뽀도 했죠",
          "original_form": "낙안읍성도 가고 뽀뽀도 했죠",
          "speaker_id": "2",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100032685.1.15",
          "form": "저희 부모님 주말 데이트 코스네요",
          "original_form": "저희 부모님 주말 데이트 코스네요",
          "speaker_id": "1",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100032685.1.16",
          "form": "나이 있는 걸 어떻게 아셨네요",
          "original_form": "나이 있는 걸 어떻게 아셨네요",
          "speaker_id": "2",
          "time": "20210905 22:19"
        },
        {
          "id": "MDRW2100032685.1.17",
          "form": "부모님이 핫플을 좋아하시나봐요",
          "original_form": "부모님이 핫플을 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210905 22:19"
        }
