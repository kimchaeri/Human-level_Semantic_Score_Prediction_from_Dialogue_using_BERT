      "utterance": [
        {
          "id": "MDRW2100030704.1.1",
          "form": "매운 음식은 정말 매력적입니다",
          "original_form": "매운 음식은 정말 매력적입니다",
          "speaker_id": "1",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100030704.1.2",
          "form": "편의점에 가면 고추짜장 맛이 나는 라면도 팔더라고요",
          "original_form": "편의점에 가면 고추짜장 맛이 나는 라면도 팔더라고요",
          "speaker_id": "2",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100030704.1.3",
          "form": "순대도 맵게 볶은 거 좋아해요",
          "original_form": "순대도 맵게 볶은 거 좋아해요",
          "speaker_id": "1",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100030704.1.4",
          "form": "김밥 다 드셨나요",
          "original_form": "김밥 다 드셨나요",
          "speaker_id": "1",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100030704.1.5",
          "form": "순대전골 먹고 싶네요",
          "original_form": "순대전골 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100030704.1.6",
          "form": "먹으면서 채팅 하더니 대단합니다",
          "original_form": "먹으면서 채팅 하더니 대단합니다",
          "speaker_id": "1",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100030704.1.7",
          "form": "금방 다 먹었습니다",
          "original_form": "금방 다 먹었습니다",
          "speaker_id": "2",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100030704.1.8",
          "form": "동대신동 순대전골이야",
          "original_form": "동대신동 순대전골이야",
          "speaker_id": "1",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100030704.1.9",
          "form": "제 먹을 것에 대한 의료기 넘쳐 나서요",
          "original_form": "제 먹을 것에 대한 의료기 넘쳐 나서요",
          "speaker_id": "2",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100030704.1.10",
          "form": "거기 맛있었어요",
          "original_form": "거기 맛있었어요",
          "speaker_id": "1",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100030704.1.11",
          "form": "곱창도 넣어서 먹으면 맛있고",
          "original_form": "곱창도 넣어서 먹으면 맛있고",
          "speaker_id": "2",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100030704.1.12",
          "form": "순대 전골은 가격이 사악합니다",
          "original_form": "순대 전골은 가격이 사악합니다",
          "speaker_id": "1",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100030704.1.13",
          "form": "맞아요 수제비 살이가 쫀득하니 맛이 있죠",
          "original_form": "맞아요 수제비 살이가 쫀득하니 맛이 있죠",
          "speaker_id": "2",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100030704.1.14",
          "form": "맛이 있고 집에서 해 먹기 힘드니 한 번씩 사 먹습니다",
          "original_form": "맛이 있고 집에서 해 먹기 힘드니 한 번씩 사 먹습니다",
          "speaker_id": "2",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100030704.1.15",
          "form": "수제비도 먹고 싶네요",
          "original_form": "수제비도 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100030704.1.16",
          "form": "우동도 먹고 싶네요",
          "original_form": "우동도 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100030704.1.17",
          "form": "거기 근처에 닭볶음탕도 맛있게 하는 곳이 있어요",
          "original_form": "거기 근처에 닭볶음탕도 맛있게 하는 곳이 있어요",
          "speaker_id": "2",
          "time": "20210904 11:36"
        }
      ]
