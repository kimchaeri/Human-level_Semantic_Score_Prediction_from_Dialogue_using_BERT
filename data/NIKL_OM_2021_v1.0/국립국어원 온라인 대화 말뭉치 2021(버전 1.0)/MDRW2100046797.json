      "utterance": [
        {
          "id": "MDRW2100046797.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211105 09:48"
        },
        {
          "id": "MDRW2100046797.1.2",
          "form": "뭐하고 계셨어요~?",
          "original_form": "뭐하고 계셨어요~?",
          "speaker_id": "2",
          "time": "20211105 10:03"
        },
        {
          "id": "MDRW2100046797.1.3",
          "form": "저는 출근했습니다",
          "original_form": "저는 출근했습니다",
          "speaker_id": "1",
          "time": "20211105 10:07"
        },
        {
          "id": "MDRW2100046797.1.4",
          "form": "name2님은 머하고계세요?",
          "original_form": "&name2&님은 머하고계세요?",
          "speaker_id": "1",
          "time": "20211105 10:07"
        },
        {
          "id": "MDRW2100046797.1.5",
          "form": "저도 출근이요 ㅠㅠ",
          "original_form": "저도 출근이요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211105 10:08"
        },
        {
          "id": "MDRW2100046797.1.6",
          "form": "오늘은 그래도 금요일이네요!",
          "original_form": "오늘은 그래도 금요일이네요!",
          "speaker_id": "2",
          "time": "20211105 10:08"
        },
        {
          "id": "MDRW2100046797.1.7",
          "form": "금요일은 넘 힘들어요 \r\n전 금요일이 일이 많거든요",
          "original_form": "금요일은 넘 힘들어요 \r\n전 금요일이 일이 많거든요",
          "speaker_id": "1",
          "time": "20211105 10:09"
        },
        {
          "id": "MDRW2100046797.1.8",
          "form": "오늘은 퇴근후 뭐하시나요",
          "original_form": "오늘은 퇴근후 뭐하시나요",
          "speaker_id": "1",
          "time": "20211105 10:09"
        },
        {
          "id": "MDRW2100046797.1.9",
          "form": "저는 내일 아침부터 놀러가야해서 오늘은 일찍 잘 거예요!!",
          "original_form": "저는 내일 아침부터 놀러가야해서 오늘은 일찍 잘 거예요!!",
          "speaker_id": "2",
          "time": "20211105 10:10"
        },
        {
          "id": "MDRW2100046797.1.10",
          "form": "금요일이 더 바쁘시구나 ㅠㅠ",
          "original_form": "금요일이 더 바쁘시구나 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211105 10:10"
        },
        {
          "id": "MDRW2100046797.1.11",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20211105 10:10"
        },
        {
          "id": "MDRW2100046797.1.12",
          "form": "오늘 힘든 날이니까 점심 꼭 맛있는 거 드세요!",
          "original_form": "오늘 힘든 날이니까 점심 꼭 맛있는 거 드세요!",
          "speaker_id": "2",
          "time": "20211105 10:16"
        },
        {
          "id": "MDRW2100046797.1.13",
          "form": "네네 추우니 뜨끈한 국물로 먹어야겠어요",
          "original_form": "네네 추우니 뜨끈한 국물로 먹어야겠어요",
          "speaker_id": "1",
          "time": "20211105 10:20"
        },
        {
          "id": "MDRW2100046797.1.14",
          "form": "name2님도 맛있는거 드세요",
          "original_form": "&name2&님도 맛있는거 드세요",
          "speaker_id": "1",
          "time": "20211105 10:20"
        },
        {
          "id": "MDRW2100046797.1.15",
          "form": "네 ㅎㅎ 오늘 쌀국수 추천드릴게요! ㅎㅎ",
          "original_form": "네 ㅎㅎ 오늘 쌀국수 추천드릴게요! ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 10:21"
        },
        {
          "id": "MDRW2100046797.1.16",
          "form": "쌀국수 맛있겠네요",
          "original_form": "쌀국수 맛있겠네요",
          "speaker_id": "1",
          "time": "20211105 10:33"
        },
        {
          "id": "MDRW2100046797.1.17",
          "form": "오늘 맛난 점심 드시고 즐거운 금요일 보내세요~!",
          "original_form": "오늘 맛난 점심 드시고 즐거운 금요일 보내세요~!",
