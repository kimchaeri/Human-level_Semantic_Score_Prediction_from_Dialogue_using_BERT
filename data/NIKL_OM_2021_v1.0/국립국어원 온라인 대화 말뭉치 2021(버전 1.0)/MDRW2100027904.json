      "utterance": [
        {
          "id": "MDRW2100027904.1.1",
          "form": "쌀국수집에서 반미...",
          "original_form": "쌀국수집에서 반미...",
          "speaker_id": "1",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027904.1.2",
          "form": "반미",
          "original_form": "반미",
          "speaker_id": "2",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027904.1.3",
          "form": "반미도 어찌보먄? 샌드위치이기때문에",
          "original_form": "반미도 어찌보먄? 샌드위치이기때문에",
          "speaker_id": "1",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027904.1.4",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027904.1.5",
          "form": "베트남식 샌드위치잖아",
          "original_form": "베트남식 샌드위치잖아",
          "speaker_id": "1",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027904.1.6",
          "form": "소프트바게트 개 맛잇다구",
          "original_form": "소프트바게트 개 맛잇다구",
          "speaker_id": "1",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027904.1.7",
          "form": "성심당의 반미 샌드위치",
          "original_form": "성심당의 반미 샌드위치",
          "speaker_id": "2",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027904.1.8",
          "form": "먹어보고싶다",
          "original_form": "먹어보고싶다",
          "speaker_id": "2",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027904.1.9",
          "form": "그건모르겟고 여기 시내 ㄹㅇ 로컬쌀국수집잇음",
          "original_form": "그건모르겟고 여기 시내 ㄹㅇ 로컬쌀국수집잇음",
          "speaker_id": "1",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027904.1.10",
          "form": "마쉿겧다",
          "original_form": "마쉿겧다",
          "speaker_id": "2",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100027904.1.11",
          "form": "예전에 베트남에서 먹었던 쌀국수",
          "original_form": "예전에 베트남에서 먹었던 쌀국수",
          "speaker_id": "2",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100027904.1.12",
          "form": "고명으로 올라가있던 떡튀김이 아직도 기억나오",
          "original_form": "고명으로 올라가있던 떡튀김이 아직도 기억나오",
          "speaker_id": "2",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100027904.1.13",
          "form": "완전 맛잏음",
          "original_form": "완전 맛잏음",
          "speaker_id": "1",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100027904.1.14",
          "form": "싸고 맛잇고",
          "original_form": "싸고 맛잇고",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027904.1.15",
          "form": "우리집에서 맨날 가기엔 조금거리가잇고",
          "original_form": "우리집에서 맨날 가기엔 조금거리가잇고",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027904.1.16",
          "form": "거리가 있고...ㅠㅠㅠㅠ",
          "original_form": "거리가 있고...ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027904.1.17",
          "form": "가까운게 최곤데",
          "original_form": "가까운게 최곤데",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027904.1.18",
          "form": "아무래더 시내니까용",
          "original_form": "아무래더 시내니까용",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027904.1.19",
          "form": "집 바로 앞 아님 다 먼 편이지",
          "original_form": "집 바로 앞 아님 다 먼 편이지",
          "speaker_id": "1",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027904.1.20",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027904.1.21",
          "form": "걸어서 15분 이상은 멀어요",
          "original_form": "걸어서 15분 이상은 멀어요",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027904.1.22",
          "form": "갑자기 성심당 샌드위치 먹고싶어졌어",
          "original_form": "갑자기 성심당 샌드위치 먹고싶어졌어",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027904.1.23",
          "form": "저번의 그거",
          "original_form": "저번의 그거",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027904.1.24",
          "form": "베트남.. 가본적 업음 고명떡튀김",
          "original_form": "베트남.. 가본적 업음 고명떡튀김",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027904.1.25",
          "form": "듣기만 해도 맛잇어보인다",
          "original_form": "듣기만 해도 맛잇어보인다",
          "speaker_id": "1",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027904.1.26",
          "form": "10년전인데 기억나면",
          "original_form": "10년전인데 기억나면",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027904.1.27",
          "form": "맛있었던거겠지?",
          "original_form": "맛있었던거겠지?",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027904.1.28",
          "form": "이미 늦었다!\r\n새우는 내가 다 먹었다",
          "original_form": "이미 늦었다!\r\n새우는 내가 다 먹었다",
