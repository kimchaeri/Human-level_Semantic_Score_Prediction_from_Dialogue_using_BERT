        },
        {
          "id": "MDRW2100013206.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013206.1.3",
          "form": "도마뱀 두마리요",
          "original_form": "도마뱀 두마리요",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013206.1.4",
          "form": "밀웜하고 밀웜 성충 거저리도요",
          "original_form": "밀웜하고 밀웜 성충 거저리도요",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013206.1.5",
          "form": "키우시나요?",
          "original_form": "키우시나요?",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013206.1.6",
          "form": "와 많네요!",
          "original_form": "와 많네요!",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013206.1.7",
          "form": "전 식물만ㅋㅋ",
          "original_form": "전 식물만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013206.1.8",
          "form": "아 어떤 식물요",
          "original_form": "아 어떤 식물요",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013206.1.9",
          "form": "전 똥손이라 다죽여요",
          "original_form": "전 똥손이라 다죽여요",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013206.1.10",
          "form": "지금 고구마 토마토는 키워요",
          "original_form": "지금 고구마 토마토는 키워요",
          "speaker_id": "1",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013206.1.11",
          "form": "저도 방토. ..ㅋㅋㅋ",
          "original_form": "저도 방토. ..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013206.1.12",
          "form": "아 대파도 키워요",
          "original_form": "아 대파도 키워요",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013206.1.13",
          "form": "파값 비싸졌대서",
          "original_form": "파값 비싸졌대서",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013206.1.14",
          "form": "오 저도 키웠었는데",
          "original_form": "오 저도 키웠었는데",
          "speaker_id": "1",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013206.1.15",
          "form": "요새 싸요",
          "original_form": "요새 싸요",
          "speaker_id": "1",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013206.1.16",
          "form": "겨울에 키웠어요 대파테크",
          "original_form": "겨울에 키웠어요 대파테크",
          "speaker_id": "1",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013206.1.17",
          "form": "한참 유행했죠",
          "original_form": "한참 유행했죠",
          "speaker_id": "1",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013206.1.18",
          "form": "전 실패 ㅋㅋ",
          "original_form": "전 실패 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013206.1.19",
          "form": "방룰토마토 안열려요",
          "original_form": "방룰토마토 안열려요",
          "speaker_id": "1",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013206.1.20",
          "form": "열렸어요?",
          "original_form": "열렸어요?",
          "speaker_id": "1",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013206.1.21",
          "form": "저 방토 한 8년 먹었는데ㅋㅋ",
          "original_form": "저 방토 한 8년 먹었는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013206.1.22",
          "form": "잘 커요 방토는!",
          "original_form": "잘 커요 방토는!",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013206.1.23",
          "form": "전 왜이러나요",
          "original_form": "전 왜이러나요",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013206.1.24",
          "form": "애숙제인데 클났어요",
          "original_form": "애숙제인데 클났어요",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013206.1.25",
          "form": "따서 인증샷 하라는데",
          "original_form": "따서 인증샷 하라는데",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013206.1.26",
          "form": "안열려요",
          "original_form": "안열려요",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013206.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013206.1.28",
          "form": "그~~ 자라긴 하는데",
          "original_form": "그~~ 자라긴 하는데",
          "speaker_id": "2",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013206.1.29",
          "form": "꽃이 안나요?",
          "original_form": "꽃이 안나요?",
          "speaker_id": "2",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013206.1.30",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013206.1.31",
          "form": "꽃이 있긴했는데 그후 열매가 안열려요",
          "original_form": "꽃이 있긴했는데 그후 열매가 안열려요",
          "speaker_id": "1",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013206.1.32",
          "form": "꽃끼리 수분 해줘도 안열리나요?",
          "original_form": "꽃끼리 수분 해줘도 안열리나요?",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013206.1.33",
          "form": "안해줬어요",
          "original_form": "안해줬어요",
          "speaker_id": "1",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013206.1.34",
          "form": "그래서 그런가요?",
          "original_form": "그래서 그런가요?",
          "speaker_id": "1",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013206.1.35",
          "form": "어쩌죠",
          "original_form": "어쩌죠",
          "speaker_id": "1",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013206.1.36",
          "form": "네 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013206.1.37",
          "form": "수분 해주셔야죠",
          "original_form": "수분 해주셔야죠",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013206.1.38",
          "form": "붓에 물 묻혀서",
          "original_form": "붓에 물 묻혀서",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013206.1.39",
          "form": "다른꽃에 슥슥",
          "original_form": "다른꽃에 슥슥",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013206.1.40",
          "form": "지금 늦은듯해요",
          "original_form": "지금 늦은듯해요",
          "speaker_id": "1",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013206.1.41",
          "form": "꽃이 졌어요 ㅜㅜ",
          "original_form": "꽃이 졌어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013206.1.42",
          "form": "ㅋㅋ또 꽃 나올꺼에요",
          "original_form": "ㅋㅋ또 꽃 나올꺼에요",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013206.1.43",
          "form": "또 꽃 나올거에요",
          "original_form": "또 꽃 나올거에요",
