      "utterance": [
        {
          "id": "MDRW2100043500.1.1",
          "form": "아녀 안먹어여",
          "original_form": "아녀 안먹어여",
          "speaker_id": "2",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043500.1.2",
          "form": "좀 배고프네",
          "original_form": "좀 배고프네",
          "speaker_id": "2",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043500.1.3",
          "form": "후라이라도 하나 먹자..",
          "original_form": "후라이라도 하나 먹자..",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043500.1.4",
          "form": "내가 너 대신 칼국수도 먹었어",
          "original_form": "내가 너 대신 칼국수도 먹었어",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043500.1.5",
          "form": "계란이 없어 ㅋㅋㅋㅋㅋ",
          "original_form": "계란이 없어 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043500.1.6",
          "form": "부럽다야",
          "original_form": "부럽다야",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043500.1.7",
          "form": "나도 칼국수면있는데",
          "original_form": "나도 칼국수면있는데",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043500.1.8",
          "form": "갈기자 칼국수 먹어줘야지...",
          "original_form": "갈기자 칼국수 먹어줘야지...",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043500.1.9",
          "form": "예?..이미 귀찮아서 누워있는 중 헤헤",
          "original_form": "예?..이미 귀찮아서 누워있는 중 헤헤",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043500.1.10",
          "form": "낼 불닭볶음면에 주먹밥 먹으려고",
          "original_form": "낼 불닭볶음면에 주먹밥 먹으려고",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043500.1.11",
          "form": "ㅎㅎ...후...난 라떼먹는중",
          "original_form": "ㅎㅎ...후...난 라떼먹는중",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043500.1.12",
          "form": "불닭 개조아",
          "original_form": "불닭 개조아",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043500.1.13",
          "form": "불닭 진짜 천재여",
          "original_form": "불닭 진짜 천재여",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043500.1.14",
          "form": "나 누가 불닭에 짜파게티 믹스해보래 존맛이래",
          "original_form": "나 누가 불닭에 짜파게티 믹스해보래 존맛이래",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043500.1.15",
          "form": "그거 불닭게티자나??",
          "original_form": "그거 불닭게티자나??",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043500.1.16",
          "form": "그래??",
          "original_form": "그래??",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043500.1.17",
          "form": "해먹어봄?",
          "original_form": "해먹어봄?",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043500.1.18",
          "form": "맛잘알 왜케 많아..",
          "original_form": "맛잘알 왜케 많아..",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043500.1.19",
          "form": "ㅇㅇㅇ 진짜맛있어 단맵이야",
          "original_form": "ㅇㅇㅇ 진짜맛있어 단맵이야",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043500.1.20",
          "form": "조아 먹어본다",
          "original_form": "조아 먹어본다",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043500.1.21",
          "form": "짱맛일거같아",
          "original_form": "짱맛일거같아",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043500.1.22",
          "form": "덜매워져서 너도 가능할듯",
          "original_form": "덜매워져서 너도 가능할듯",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043500.1.23",
          "form": "덜맵다해도 맵던데...",
          "original_form": "덜맵다해도 맵던데...",
          "speaker_id": "1",
          "time": "20210907 19:22"
        }
      ]
