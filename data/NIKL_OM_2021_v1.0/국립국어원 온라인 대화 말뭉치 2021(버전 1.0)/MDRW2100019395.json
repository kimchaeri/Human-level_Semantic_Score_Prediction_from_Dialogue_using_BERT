      "utterance": [
        {
          "id": "MDRW2100019395.1.1",
          "form": "좋아하는 과일 있으세요?",
          "original_form": "좋아하는 과일 있으세요?",
          "speaker_id": "2",
          "time": "20210827 19:04"
        },
        {
          "id": "MDRW2100019395.1.2",
          "form": "밥먹고 와서 답이 늦었어요 죄송해요 ㅋㅋㅋ",
          "original_form": "밥먹고 와서 답이 늦었어요 죄송해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:57"
        },
        {
          "id": "MDRW2100019395.1.3",
          "form": "대부분 좋아하는데 시큼하거나 단 과일을 좋아합니다",
          "original_form": "대부분 좋아하는데 시큼하거나 단 과일을 좋아합니다",
          "speaker_id": "1",
          "time": "20210827 19:57"
        },
        {
          "id": "MDRW2100019395.1.4",
          "form": "귤 패션후르츠 거봉 오렌지 딸기",
          "original_form": "귤 패션후르츠 거봉 오렌지 딸기",
          "speaker_id": "1",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019395.1.5",
          "form": "복숭아 좋아하세요?",
          "original_form": "복숭아 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019395.1.6",
          "form": "복숭아도 불론 좋아해요",
          "original_form": "복숭아도 불론 좋아해요",
          "speaker_id": "1",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100019395.1.7",
          "form": "딱복 ? 물복?",
          "original_form": "딱복 ? 물복?",
          "speaker_id": "1",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100019395.1.8",
          "form": "전 딱복이요!",
          "original_form": "전 딱복이요!",
          "speaker_id": "2",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019395.1.9",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019395.1.10",
          "form": "전 물복파인데 보관이 어려워서 오래 먹을거면 딱복사요",
          "original_form": "전 물복파인데 보관이 어려워서 오래 먹을거면 딱복사요",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019395.1.11",
          "form": "그럼 상온에 보관하시나요?",
          "original_form": "그럼 상온에 보관하시나요?",
          "speaker_id": "2",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019395.1.12",
          "form": "냉장고에 넣지만 물복은 금방 멍드니가요",
          "original_form": "냉장고에 넣지만 물복은 금방 멍드니가요",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019395.1.13",
          "form": "과일 중에 냉장고에 넣으면 맛없어지는 종류가 잇더라구요",
          "original_form": "과일 중에 냉장고에 넣으면 맛없어지는 종류가 잇더라구요",
          "speaker_id": "2",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019395.1.14",
          "form": "헐 복숭아는 냉장고에 넣음 안되나요?",
          "original_form": "헐 복숭아는 냉장고에 넣음 안되나요?",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019395.1.15",
          "form": "며칠전에 키위 사서 냉장고에 넣었는데 샀을 때보다 신맛이 강해져서 놀랐어요",
          "original_form": "며칠전에 키위 사서 냉장고에 넣었는데 샀을 때보다 신맛이 강해져서 놀랐어요",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019395.1.16",
          "form": "그건 모르겠어요 ㅋㅋㅋㅋ",
          "original_form": "그건 모르겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019395.1.17",
          "form": "와 키위는 더 셔진다니 몰랐어요",
          "original_form": "와 키위는 더 셔진다니 몰랐어요",
          "speaker_id": "1",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019395.1.18",
          "form": "밖에서 후숙해야겠네요",
          "original_form": "밖에서 후숙해야겠네요",
          "speaker_id": "1",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019395.1.19",
          "form": "그런거같아요",
          "original_form": "그런거같아요",
          "speaker_id": "2",
