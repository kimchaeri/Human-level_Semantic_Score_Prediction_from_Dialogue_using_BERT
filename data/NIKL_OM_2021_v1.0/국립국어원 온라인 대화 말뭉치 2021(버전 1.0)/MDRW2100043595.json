      "utterance": [
        {
          "id": "MDRW2100043595.1.1",
          "form": "반려동물이 있으신가요?",
          "original_form": "반려동물이 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043595.1.2",
          "form": "아니요 ㅋㅋㅋ",
          "original_form": "아니요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043595.1.3",
          "form": "키우세요?",
          "original_form": "키우세요?",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043595.1.4",
          "form": "아니옄ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니옄ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043595.1.5",
          "form": "키우고싶지만 불가능해서 ㅠ",
          "original_form": "키우고싶지만 불가능해서 ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043595.1.6",
          "form": "어떤 동물 좋아하세요?",
          "original_form": "어떤 동물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043595.1.7",
          "form": "저 코끼리요 ㅋㅋ",
          "original_form": "저 코끼리요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043595.1.8",
          "form": "그리구 호랑이..",
          "original_form": "그리구 호랑이..",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043595.1.9",
          "form": "으앗",
          "original_form": "으앗",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043595.1.10",
          "form": "다 키울수는 없네요 ㅎㅎㅎㅎ",
          "original_form": "다 키울수는 없네요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043595.1.11",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043595.1.12",
          "form": "다큐 이런 거 보면서 만족..",
          "original_form": "다큐 이런 거 보면서 만족..",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043595.1.13",
          "form": "표범두 좋아해요",
          "original_form": "표범두 좋아해요",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043595.1.14",
          "form": "오우.... . ...",
          "original_form": "오우.... . ...",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043595.1.15",
          "form": "맹수면서 멍청하구 약한 게 좋아요ㅠㅠ",
          "original_form": "맹수면서 멍청하구 약한 게 좋아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043595.1.16",
          "form": "키우고싶은 동물은요..?",
          "original_form": "키우고싶은 동물은요..?",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043595.1.17",
          "form": "ㅋㅋㅋㅋㅋ아악ㅋㅋㅋ못키우잖아욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아악ㅋㅋㅋ못키우잖아욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043595.1.18",
          "form": "정말 티비로만봐얔ㅋㅋㅋ",
          "original_form": "정말 티비로만봐얔ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043595.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043595.1.20",
          "form": "전에 어디서 표범 키우는 부자 봤는데 너무 부럽더라구요ㅠ",
          "original_form": "전에 어디서 표범 키우는 부자 봤는데 너무 부럽더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043595.1.21",
          "form": "보급형으로는 고양이? 근데 힘들 것 같아요",
          "original_form": "보급형으로는 고양이? 근데 힘들 것 같아요",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043595.1.22",
          "form": "그런건 외국에서 가능할거같아요",
          "original_form": "그런건 외국에서 가능할거같아요",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043595.1.23",
          "form": "왜요??",
          "original_form": "왜요??",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043595.1.24",
          "form": "털때문에???",
          "original_form": "털때문에???",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043595.1.25",
          "form": "네 털도 그렇구 사고치고 그런것도 잇구",
          "original_form": "네 털도 그렇구 사고치고 그런것도 잇구",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043595.1.26",
          "form": "알러지있으신가요?",
          "original_form": "알러지있으신가요?",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043595.1.27",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043595.1.28",
          "form": "제가 막상 키우면 다르겠지만... 무섭기두 해요 ㅋㅋ",
          "original_form": "제가 막상 키우면 다르겠지만... 무섭기두 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043595.1.29",
          "form": "아 그렇기도하죠 ㅎㅎ",
          "original_form": "아 그렇기도하죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:54"
        }
      ]
    }
