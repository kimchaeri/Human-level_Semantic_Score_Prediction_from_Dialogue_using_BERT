      "utterance": [
        {
          "id": "MDRW2100047325.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100047325.1.2",
          "form": "안녕하세요 비오는날좋아하시나요",
          "original_form": "안녕하세요 비오는날좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100047325.1.3",
          "form": "습해서 싫어요 님은요?",
          "original_form": "습해서 싫어요 님은요?",
          "speaker_id": "2",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100047325.1.4",
          "form": "저도요\n근데 코로나때문에 못나가니\n.가끔은 비가 확내리면 기분좋더라구요",
          "original_form": "저도요\n근데 코로나때문에 못나가니\n.가끔은 비가 확내리면 기분좋더라구요",
          "speaker_id": "1",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100047325.1.5",
          "form": "가을옷 꺼내셨나요?",
          "original_form": "가을옷 꺼내셨나요?",
          "speaker_id": "2",
          "time": "20210826 11:49"
        },
        {
          "id": "MDRW2100047325.1.6",
          "form": "아직요..벌써 꺼내셨어요?\n저도 여름옷 세탁좀 해야겠어요 ㅎ",
          "original_form": "아직요..벌써 꺼내셨어요?\n저도 여름옷 세탁좀 해야겠어요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100047325.1.7",
          "form": "좋아하는 계절 있으세요?",
          "original_form": "좋아하는 계절 있으세요?",
          "speaker_id": "2",
          "time": "20210826 11:51"
        },
        {
          "id": "MDRW2100047325.1.8",
          "form": "전 초여름요",
          "original_form": "전 초여름요",
          "speaker_id": "1",
          "time": "20210826 11:51"
        },
        {
          "id": "MDRW2100047325.1.9",
          "form": "날씨가 너무좋아요 나가고싶고 활력을 주는 날씨같아요",
          "original_form": "날씨가 너무좋아요 나가고싶고 활력을 주는 날씨같아요",
          "speaker_id": "1",
          "time": "20210826 11:52"
        },
        {
          "id": "MDRW2100047325.1.10",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210826 11:52"
        },
        {
          "id": "MDRW2100047325.1.11",
          "form": "저도 여름 좋아해요!",
          "original_form": "저도 여름 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 11:53"
        },
        {
          "id": "MDRW2100047325.1.12",
          "form": "겨울은 너무 게을러져요",
          "original_form": "겨울은 너무 게을러져요",
          "speaker_id": "1",
          "time": "20210826 11:54"
        },
        {
          "id": "MDRW2100047325.1.13",
          "form": "춥고 건조해서 싫어요ㅋㅋ",
          "original_form": "춥고 건조해서 싫어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:54"
        },
        {
          "id": "MDRW2100047325.1.14",
          "form": "맞아요 건조한것도 너무싫어요",
          "original_form": "맞아요 건조한것도 너무싫어요",
          "speaker_id": "1",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100047325.1.15",
          "form": "가습기 씻어놔야겠네요ㅎ",
          "original_form": "가습기 씻어놔야겠네요ㅎ",
          "speaker_id": "2",
