        {
          "id": "MDRW2100040240.1.1",
          "form": "날씨 엄청 좋은데 사시는 곳도 좋아요??",
          "original_form": "날씨 엄청 좋은데 사시는 곳도 좋아요??",
          "speaker_id": "1",
          "time": "20210904 14:44"
        },
        {
          "id": "MDRW2100040240.1.2",
          "form": "넹 조아요!!",
          "original_form": "넹 조아요!!",
          "speaker_id": "2",
          "time": "20210904 14:45"
        },
        {
          "id": "MDRW2100040240.1.3",
          "form": "놀러나왓어유",
          "original_form": "놀러나왓어유",
          "speaker_id": "2",
          "time": "20210904 14:45"
        },
        {
          "id": "MDRW2100040240.1.4",
          "form": "ㅋㅋ저도 한강ㅋㅋ",
          "original_form": "ㅋㅋ저도 한강ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:46"
        },
        {
          "id": "MDRW2100040240.1.5",
          "form": "야외신가요?",
          "original_form": "야외신가요?",
          "speaker_id": "1",
          "time": "20210904 14:46"
        },
        {
          "id": "MDRW2100040240.1.6",
          "form": "ㅋㅋㅋㅋ 북촌~~!!",
          "original_form": "ㅋㅋㅋㅋ 북촌~~!!",
          "speaker_id": "2",
          "time": "20210904 14:47"
        },
        {
          "id": "MDRW2100040240.1.7",
          "form": "한강 좋죵",
          "original_form": "한강 좋죵",
          "speaker_id": "2",
          "time": "20210904 14:47"
        },
        {
          "id": "MDRW2100040240.1.8",
          "form": "와 진짜 오늘 날씨에 딱이네요?",
          "original_form": "와 진짜 오늘 날씨에 딱이네요?",
          "speaker_id": "1",
          "time": "20210904 14:48"
        },
        {
          "id": "MDRW2100040240.1.9",
          "form": "햇빛 뜨겁죠?",
          "original_form": "햇빛 뜨겁죠?",
          "speaker_id": "1",
          "time": "20210904 14:48"
        },
        {
          "id": "MDRW2100040240.1.10",
          "form": "넨네 그늘은 진짜 딱조은",
          "original_form": "넨네 그늘은 진짜 딱조은",
          "speaker_id": "2",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100040240.1.11",
          "form": "하늘이뻐요",
          "original_form": "하늘이뻐요",
          "speaker_id": "2",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100040240.1.12",
          "form": "맞아요 그늘은 시원해요!",
          "original_form": "맞아요 그늘은 시원해요!",
          "speaker_id": "1",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100040240.1.13",
          "form": "하늘사진만 가득이네요",
          "original_form": "하늘사진만 가득이네요",
          "speaker_id": "1",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100040240.1.14",
          "form": "누워계신가요",
          "original_form": "누워계신가요",
          "speaker_id": "2",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100040240.1.15",
          "form": "ㅋㅋ 아뇨 카페 들어왔어요ㅋㅋ",
          "original_form": "ㅋㅋ 아뇨 카페 들어왔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100040240.1.16",
          "form": "좀있다 잔디밭가려고요",
          "original_form": "좀있다 잔디밭가려고요",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100040240.1.17",
          "form": "ㅋㅋ 한강뷰?",
          "original_form": "ㅋㅋ 한강뷰?",
          "speaker_id": "2",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100040240.1.18",
          "form": "머입으셧어요",
          "original_form": "머입으셧어요",
