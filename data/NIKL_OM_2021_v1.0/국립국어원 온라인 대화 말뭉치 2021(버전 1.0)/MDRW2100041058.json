      "utterance": [
        {
          "id": "MDRW2100041058.1.1",
          "form": "나도 최근에 가장 열심히 본 드라마가 압구정 백야인데 모름",
          "original_form": "나도 최근에 가장 열심히 본 드라마가 압구정 백야인데 모름",
          "speaker_id": "2",
          "time": "20210905 16:17"
        },
        {
          "id": "MDRW2100041058.1.2",
          "form": "박하나랑 남주밖에 기억 안나요",
          "original_form": "박하나랑 남주밖에 기억 안나요",
          "speaker_id": "1",
          "time": "20210905 16:17"
        },
        {
          "id": "MDRW2100041058.1.3",
          "form": "피고인에서도 나온거 보니 엑스트라엿나보다",
          "original_form": "피고인에서도 나온거 보니 엑스트라엿나보다",
          "speaker_id": "2",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100041058.1.4",
          "form": "이분도 열심히 살았네요",
          "original_form": "이분도 열심히 살았네요",
          "speaker_id": "1",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100041058.1.5",
          "form": "두번째남편에서 한기웅이 첫남편인가봐",
          "original_form": "두번째남편에서 한기웅이 첫남편인가봐",
          "speaker_id": "2",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100041058.1.6",
          "form": "두번째 남편이 차서원?",
          "original_form": "두번째 남편이 차서원?",
          "speaker_id": "1",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100041058.1.7",
          "form": "개천에서 용 난 케이스",
          "original_form": "개천에서 용 난 케이스",
          "speaker_id": "2",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100041058.1.8",
          "form": "누가요?",
          "original_form": "누가요?",
          "speaker_id": "1",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100041058.1.9",
          "form": "그렇지 차서원이 재벌 두번째 남편",
          "original_form": "그렇지 차서원이 재벌 두번째 남편",
          "speaker_id": "2",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100041058.1.10",
          "form": "엄현경이 성공했네",
          "original_form": "엄현경이 성공했네",
          "speaker_id": "1",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100041058.1.11",
          "form": "한기웅이 개천에서 용난 케이스인데 배신을 하나봐",
          "original_form": "한기웅이 개천에서 용난 케이스인데 배신을 하나봐",
          "speaker_id": "2",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100041058.1.12",
          "form": "아 글쿠나ㅋ",
          "original_form": "아 글쿠나ㅋ",
          "speaker_id": "1",
          "time": "20210905 16:20"
        },
        {
          "id": "MDRW2100041058.1.13",
          "form": "엄현경이 악역으로 확 뜨지 않았어",
          "original_form": "엄현경이 악역으로 확 뜨지 않았어",
          "speaker_id": "2",
          "time": "20210905 16:20"
        }
