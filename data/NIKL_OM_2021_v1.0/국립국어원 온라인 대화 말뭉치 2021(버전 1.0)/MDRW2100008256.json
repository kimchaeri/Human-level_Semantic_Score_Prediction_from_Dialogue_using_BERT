      "utterance": [
        {
          "id": "MDRW2100008256.1.1",
          "form": "닌텐도는 해보셨나요?",
          "original_form": "닌텐도는 해보셨나요?",
          "speaker_id": "2",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008256.1.2",
          "form": "닌텐도 진짜 예전에 해봤어요!",
          "original_form": "닌텐도 진짜 예전에 해봤어요!",
          "speaker_id": "1",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008256.1.3",
          "form": "링피트 사려다가 그때 출시되고 가격이 너무 플미 붙었어서",
          "original_form": "링피트 사려다가 그때 출시되고 가격이 너무 플미 붙었어서",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008256.1.4",
          "form": "해보셨나요?",
          "original_form": "해보셨나요?",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008256.1.5",
          "form": "아 저희집에 있어요!",
          "original_form": "아 저희집에 있어요!",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008256.1.6",
          "form": "가격 내려갔을때 쇽 사왔죠 ㅎㅎ",
          "original_form": "가격 내려갔을때 쇽 사왔죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008256.1.7",
          "form": "오!!자주하시나요?",
          "original_form": "오!!자주하시나요?",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008256.1.8",
          "form": "ㅋㅋㅋ사실 안한지 좀 됐어요",
          "original_form": "ㅋㅋㅋ사실 안한지 좀 됐어요",
          "speaker_id": "2",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100008256.1.9",
          "form": "헬스장 다니고 있어서 손이 안가더라고요",
          "original_form": "헬스장 다니고 있어서 손이 안가더라고요",
          "speaker_id": "2",
          "time": "20210822 15:17"
        },
        {
          "id": "MDRW2100008256.1.10",
          "form": "아 따로 운동 하시는구나",
          "original_form": "아 따로 운동 하시는구나",
          "speaker_id": "1",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008256.1.11",
          "form": "그럼 게임 위주로 하시는거세요?",
          "original_form": "그럼 게임 위주로 하시는거세요?",
          "speaker_id": "1",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008256.1.12",
          "form": "음 저는 좀",
          "original_form": "음 저는 좀",
          "speaker_id": "2",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008256.1.13",
          "form": "고전게임 좋아해서 ㅋㅋㅋㅋ",
          "original_form": "고전게임 좋아해서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008256.1.14",
          "form": "맨날 테트리스랑 뿌요뿌요해요",
          "original_form": "맨날 테트리스랑 뿌요뿌요해요",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008256.1.15",
          "form": "오 그런 게임칩?팩?도 있군요",
          "original_form": "오 그런 게임칩?팩?도 있군요",
          "speaker_id": "1",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008256.1.16",
          "form": "엥 만//져라 같은 이름 칩이요",
          "original_form": "엥 만//져라 같은 이름 칩이요",
          "speaker_id": "1",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008256.1.17",
          "form": "뿌요뿌요 완전 추억의 게임..",
          "original_form": "뿌요뿌요 완전 추억의 게임..",
          "speaker_id": "1",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008256.1.18",
          "form": "다른 ㄱ게임도 많은대",
          "original_form": "다른 ㄱ게임도 많은대",
          "speaker_id": "2",
          "time": "20210822 16:01"
        },
        {
          "id": "MDRW2100008256.1.19",
          "form": "그것만 플레이시간이 장난아니에요",
          "original_form": "그것만 플레이시간이 장난아니에요",
          "speaker_id": "2",
          "time": "20210822 16:01"
        },
        {
          "id": "MDRW2100008256.1.20",
          "form": "예전에  뭐해보샸나요?",
          "original_form": "예전에  뭐해보샸나요?",
          "speaker_id": "2",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100008256.1.21",
          "form": "전 만ㆍ져라 메이드인 와리오 이거랑 강아지 키우는 게임이었나?딱 그것만 해봤어요!",
          "original_form": "전 만ㆍ져라 메이드인 와리오 이거랑 강아지 키우는 게임이었나?딱 그것만 해봤어요!",
          "speaker_id": "1",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008256.1.22",
          "form": "친구집에 스위치 샀대서 나중에 가서 해보려고요!",
          "original_form": "친구집에 스위치 샀대서 나중에 가서 해보려고요!",
          "speaker_id": "1",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008256.1.23",
          "form": "와 그거 재밌었어요",
          "original_form": "와 그거 재밌었어요",
          "speaker_id": "2",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008256.1.24",
          "form": "진짜 추억이내요",
          "original_form": "진짜 추억이내요",
          "speaker_id": "2",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008256.1.25",
          "form": "만.져라 진짜 중독성 대박이죠 ㅋㅋㅋㅋㅋ",
          "original_form": "만.져라 진짜 중독성 대박이죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008256.1.26",
          "form": "네ㅋㅋㅋㅋ게임도 많고 이틀인가 삼일 빌려서 계속 그거만 했던 기억이",
          "original_form": "네ㅋㅋㅋㅋ게임도 많고 이틀인가 삼일 빌려서 계속 그거만 했던 기억이",
          "speaker_id": "1",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008256.1.27",
          "form": "그래서 나도 닌텐도 사달라고 했는데 그땐 안사주셔서 못했네요",
          "original_form": "그래서 나도 닌텐도 사달라고 했는데 그땐 안사주셔서 못했네요",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008256.1.28",
          "form": "저도 어릴때는 그랬내요 ㅠㅠ",
          "original_form": "저도 어릴때는 그랬내요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008256.1.29",
          "form": "아이의 자본으로는 살 수없는 ㅠㅠ",
          "original_form": "아이의 자본으로는 살 수없는 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:14"
        }
