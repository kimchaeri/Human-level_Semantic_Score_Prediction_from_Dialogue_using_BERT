      "utterance": [
        {
          "id": "MDRW2100036932.1.1",
          "form": "백신은 2차 맞으신 거에요?",
          "original_form": "백신은 2차 맞으신 거에요?",
          "speaker_id": "1",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100036932.1.2",
          "form": "아뇨ㅜ\r\n아직 1차요",
          "original_form": "아뇨ㅜ\r\n아직 1차요",
          "speaker_id": "2",
          "time": "20210831 15:16"
        },
        {
          "id": "MDRW2100036932.1.3",
          "form": "백신맞으셨어요?",
          "original_form": "백신맞으셨어요?",
          "speaker_id": "2",
          "time": "20210831 15:16"
        },
        {
          "id": "MDRW2100036932.1.4",
          "form": "전 담달에 1차 맞아요~",
          "original_form": "전 담달에 1차 맞아요~",
          "speaker_id": "1",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100036932.1.5",
          "form": "name2님은 담달에 2차  맞으시겠네요",
          "original_form": "&name2&님은 담달에 2차  맞으시겠네요",
          "speaker_id": "1",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100036932.1.6",
          "form": "화이자 맞으셨어요?",
          "original_form": "화이자 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100036932.1.7",
          "form": "네 화이자 맞았어요",
          "original_form": "네 화이자 맞았어요",
          "speaker_id": "2",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036932.1.8",
          "form": "팔이 많이 욱씬거리더라구요ㅜㅜ",
          "original_form": "팔이 많이 욱씬거리더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036932.1.9",
          "form": "확실이 팔쪽이 아프군요",
          "original_form": "확실이 팔쪽이 아프군요",
          "speaker_id": "1",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036932.1.10",
          "form": "전 화아지 아니면 모더나라던데.. 뭘 맞을진 모르겠네요",
          "original_form": "전 화아지 아니면 모더나라던데.. 뭘 맞을진 모르겠네요",
          "speaker_id": "1",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036932.1.11",
          "form": "어느걸 맞더라도 잘 맞으셨으면 좋겠네요~^^",
          "original_form": "어느걸 맞더라도 잘 맞으셨으면 좋겠네요~^^",
          "speaker_id": "2",
          "time": "20210831 16:03"
        },
        {
          "id": "MDRW2100036932.1.12",
          "form": "감사해요~",
          "original_form": "감사해요~",
          "speaker_id": "1",
          "time": "20210831 16:05"
        },
        {
          "id": "MDRW2100036932.1.13",
          "form": "백신 부작용 없이 우리 모두 잘 넘어갔으면 좋겠네요",
          "original_form": "백신 부작용 없이 우리 모두 잘 넘어갔으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210831 16:05"
        },
        {
          "id": "MDRW2100036932.1.14",
          "form": "건강을 위해 먹는 영양제 같은 건 있으세요?",
          "original_form": "건강을 위해 먹는 영양제 같은 건 있으세요?",
          "speaker_id": "1",
          "time": "20210831 16:05"
        },
        {
          "id": "MDRW2100036932.1.15",
          "form": "전 마그네슘이요",
          "original_form": "전 마그네슘이요",
          "speaker_id": "2",
          "time": "20210831 16:06"
        },
        {
          "id": "MDRW2100036932.1.16",
          "form": "원래안먹는데",
          "original_form": "원래안먹는데",
          "speaker_id": "2",
          "time": "20210831 16:06"
        },
        {
          "id": "MDRW2100036932.1.17",
          "form": "최근에 눈밑이 떨리길래ㅋ",
          "original_form": "최근에 눈밑이 떨리길래ㅋ",
          "speaker_id": "2",
          "time": "20210831 16:06"
        },
        {
          "id": "MDRW2100036932.1.18",
          "form": "오 저도 마그네슘",
          "original_form": "오 저도 마그네슘",
          "speaker_id": "1",
          "time": "20210831 16:06"
        },
        {
          "id": "MDRW2100036932.1.19",
          "form": "숙면에 좋다고해서",
          "original_form": "숙면에 좋다고해서",
          "speaker_id": "1",
          "time": "20210831 16:06"
        },
        {
          "id": "MDRW2100036932.1.20",
          "form": "비타민씨도 먹어요",
          "original_form": "비타민씨도 먹어요",
          "speaker_id": "1",
          "time": "20210831 16:07"
        },
        {
          "id": "MDRW2100036932.1.21",
          "form": "감기예방에 좋다고해서",
          "original_form": "감기예방에 좋다고해서",
          "speaker_id": "1",
          "time": "20210831 16:07"
        },
        {
          "id": "MDRW2100036932.1.22",
          "form": "마그네슘이 숙면에도 좋군요!",
          "original_form": "마그네슘이 숙면에도 좋군요!",
          "speaker_id": "2",
          "time": "20210831 16:09"
        },
        {
          "id": "MDRW2100036932.1.23",
          "form": "즐겨드시는 브랜드가 있으세요?",
          "original_form": "즐겨드시는 브랜드가 있으세요?",
          "speaker_id": "2",
          "time": "20210831 16:10"
        },
        {
          "id": "MDRW2100036932.1.24",
          "form": "저도 최근에 먹기 시작한거라 어디꺼가 좋고 그런거 모르고",
          "original_form": "저도 최근에 먹기 시작한거라 어디꺼가 좋고 그런거 모르고",
          "speaker_id": "1",
          "time": "20210831 16:11"
        },
        {
          "id": "MDRW2100036932.1.25",
          "form": "그냥 하나 사서 먹고 있어요.. 아마 종근당꺼 같은데",
          "original_form": "그냥 하나 사서 먹고 있어요.. 아마 종근당꺼 같은데",
          "speaker_id": "1",
          "time": "20210831 16:11"
        },
        {
          "id": "MDRW2100036932.1.26",
          "form": "아직  한통 다 안먹었어요",
          "original_form": "아직  한통 다 안먹었어요",
          "speaker_id": "1",
          "time": "20210831 16:11"
        },
        {
          "id": "MDRW2100036932.1.27",
          "form": "숙면은 취하셨나요?ㅎㅎ",
          "original_form": "숙면은 취하셨나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100036932.1.28",
          "form": "전 눈떨림은 확실히 없어지더라구요ㅎ",
          "original_form": "전 눈떨림은 확실히 없어지더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100036932.1.29",
          "form": "원래 자는데 숙면인지 아닌지 몰라서 먹고 있어요 ㅎ",
          "original_form": "원래 자는데 숙면인지 아닌지 몰라서 먹고 있어요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100036932.1.30",
          "form": "커피도 줄였구요",
          "original_form": "커피도 줄였구요",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100036932.1.31",
          "form": "맞아요 커피가 정말 카페인이 많더라구요",
          "original_form": "맞아요 커피가 정말 카페인이 많더라구요",
          "speaker_id": "2",
          "time": "20210831 17:33"
        }
      ]
