      "utterance": [
        {
          "id": "MDRW2100027814.1.1",
          "form": "요즘 집에서 어떤 반찬 해드세요?",
          "original_form": "요즘 집에서 어떤 반찬 해드세요?",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027814.1.2",
          "form": "얼마 전에는 겉절이를 했었어욤",
          "original_form": "얼마 전에는 겉절이를 했었어욤",
          "speaker_id": "2",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100027814.1.3",
          "form": "근데 제가 아니라",
          "original_form": "근데 제가 아니라",
          "speaker_id": "2",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100027814.1.4",
          "form": "엄마랑 아빠가 만들었어요 ㅋㅋㅋ!!!",
          "original_form": "엄마랑 아빠가 만들었어요 ㅋㅋㅋ!!!",
          "speaker_id": "2",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100027814.1.5",
          "form": "와 맛있겠네요 아버지가 집안일 잘 도와주시나봐요",
          "original_form": "와 맛있겠네요 아버지가 집안일 잘 도와주시나봐요",
          "speaker_id": "1",
          "time": "20210902 15:26"
        },
        {
          "id": "MDRW2100027814.1.6",
          "form": "네! 아버지도 식구니까",
          "original_form": "네! 아버지도 식구니까",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027814.1.7",
          "form": "도와준다는 개념보단 자기 일이라고 생각하고 계세요!",
          "original_form": "도와준다는 개념보단 자기 일이라고 생각하고 계세요!",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027814.1.8",
          "form": "와 너무 멋지세요 닮고싶은 분이네요",
          "original_form": "와 너무 멋지세요 닮고싶은 분이네요",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100027814.1.9",
          "form": "ㅎㅎ 감사합니다!!!",
          "original_form": "ㅎㅎ 감사합니다!!!",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100027814.1.10",
          "form": "name1님은 자매나 형제 있으신가요?!",
          "original_form": "&name1&님은 자매나 형제 있으신가요?!",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100027814.1.11",
          "form": "넵 제동생은 결혼해서 이번달에 조카 태어나요ㅎ",
          "original_form": "넵 제동생은 결혼해서 이번달에 조카 태어나요ㅎ",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100027814.1.12",
          "form": "오오 동생분이 먼저 결혼하셨나요?",
          "original_form": "오오 동생분이 먼저 결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100027814.1.13",
          "form": "여동생이신가요?!",
          "original_form": "여동생이신가요?!",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100027814.1.14",
          "form": "넵넵ㅋㅋ",
          "original_form": "넵넵ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100027814.1.15",
          "form": "그럼 더 애착 가시겠어요 ㅋㅋㅋㅋ",
          "original_form": "그럼 더 애착 가시겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100027814.1.16",
          "form": "여자형제가 낳은 조카가 더 애착이 간다더라고요!",
          "original_form": "여자형제가 낳은 조카가 더 애착이 간다더라고요!",
          "speaker_id": "2",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100027814.1.17",
          "form": "아 그래용?조카 있으세요?",
          "original_form": "아 그래용?조카 있으세요?",
          "speaker_id": "1",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100027814.1.18",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210903 08:47"
        },
        {
          "id": "MDRW2100027814.1.19",
          "form": "저 조카 3명이나 있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저 조카 3명이나 있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:47"
        },
        {
          "id": "MDRW2100027814.1.20",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:53"
        },
        {
          "id": "MDRW2100027814.1.21",
          "form": "근데 조카 하나는",
          "original_form": "근데 조카 하나는",
          "speaker_id": "2",
          "time": "20210903 08:54"
        },
        {
          "id": "MDRW2100027814.1.22",
          "form": "미국에 살고있어서",
          "original_form": "미국에 살고있어서",
          "speaker_id": "2",
          "time": "20210903 08:54"
        },
        {
          "id": "MDRW2100027814.1.23",
          "form": "자주 못본답니다ㅠ",
          "original_form": "자주 못본답니다ㅠ",
          "speaker_id": "2",
          "time": "20210903 08:54"
        },
        {
          "id": "MDRW2100027814.1.24",
          "form": "와 미국이라니 정말 머네요 조카가 생기면 막 챙겨주고싶어지나요?",
          "original_form": "와 미국이라니 정말 머네요 조카가 생기면 막 챙겨주고싶어지나요?",
          "speaker_id": "1",
          "time": "20210903 12:38"
        },
        {
          "id": "MDRW2100027814.1.25",
          "form": "네,, 뭐랄까요",
          "original_form": "네,, 뭐랄까요",
          "speaker_id": "2",
          "time": "20210903 14:04"
        },
        {
          "id": "MDRW2100027814.1.26",
          "form": "동생 같기도 하고",
          "original_form": "동생 같기도 하고",
          "speaker_id": "2",
          "time": "20210903 14:04"
        },
        {
          "id": "MDRW2100027814.1.27",
          "form": "저희 조카들이 착해서 그런 걸수도 있는데",
          "original_form": "저희 조카들이 착해서 그런 걸수도 있는데",
          "speaker_id": "2",
          "time": "20210903 14:05"
        },
        {
          "id": "MDRW2100027814.1.28",
          "form": "너무 사랑스러워요!",
          "original_form": "너무 사랑스러워요!",
