        {
          "id": "MDRW2100036334.1.1",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036334.1.2",
          "form": "자꾸 끊어서 죄송해요ㅠㅠ",
          "original_form": "자꾸 끊어서 죄송해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036334.1.3",
          "form": "어떤가요!?!?",
          "original_form": "어떤가요!?!?",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036334.1.4",
          "form": "아니에요 좋아요!!",
          "original_form": "아니에요 좋아요!!",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036334.1.5",
          "form": "바나나킥이 막걸리에 퐁당 빠진맛이랄까요?",
          "original_form": "바나나킥이 막걸리에 퐁당 빠진맛이랄까요?",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036334.1.6",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036334.1.7",
          "form": "조리퐁막걸리인가",
          "original_form": "조리퐁막걸리인가",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036334.1.8",
          "form": "엑 이게 뭐야 막걸리로 장난치네ㅡㅡ 했는데 먹어보고 오???",
          "original_form": "엑 이게 뭐야 막걸리로 장난치네ㅡㅡ 했는데 먹어보고 오???",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036334.1.9",
          "form": "안마셔봣는데 ㅋㅋㅋ",
          "original_form": "안마셔봣는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036334.1.10",
          "form": "죠리퐁이요?ㅋㅋㅋㅋ 아 근데 이것도 왜 맛있을것같져ㅋㅋㅋ",
          "original_form": "죠리퐁이요?ㅋㅋㅋㅋ 아 근데 이것도 왜 맛있을것같져ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100036334.1.11",
          "form": "전 도전안해봣어요 ㅋㅋㅋ",
          "original_form": "전 도전안해봣어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100036334.1.12",
          "form": "저 대학생때 순하리 유자가",
          "original_form": "저 대학생때 순하리 유자가",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100036334.1.13",
          "form": "처음 나왔었거든요",
          "original_form": "처음 나왔었거든요",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100036334.1.14",
          "form": "그때 과일소주가 한창 유행이었을때였어요",
          "original_form": "그때 과일소주가 한창 유행이었을때였어요",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100036334.1.15",
          "form": "혹시 민초소주드셔보셧어요??",
          "original_form": "혹시 민초소주드셔보셧어요??",
          "speaker_id": "2",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100036334.1.16",
          "form": "헉ㅋㅋㅋㅋㅋㅋ",
          "original_form": "헉ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100036334.1.17",
          "form": "그거 사진보고 합성아닌가 했는데",
          "original_form": "그거 사진보고 합성아닌가 했는데",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100036334.1.18",
          "form": "요구르트 막걸리 등등 ㅋㅋ많앗죠 ㅋㅋㅋ",
          "original_form": "요구르트 막걸리 등등 ㅋㅋ많앗죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100036334.1.19",
          "form": "실제에요??",
          "original_form": "실제에요??",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100036334.1.20",
          "form": "과일소주도",
          "original_form": "과일소주도",
          "speaker_id": "2",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100036334.1.21",
          "form": "네네 ㅋㅋㅋ 저도 안마셔봣어요 ㅋㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋ 저도 안마셔봣어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100036334.1.22",
          "form": "와... 아무리 민초단이지만 이건 선넘네욬ㅋㅋㅋㅋㅋ",
          "original_form": "와... 아무리 민초단이지만 이건 선넘네욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100036334.1.23",
          "form": "마트에서 못봣는데 ㅋㅋㅋ보이면 과연사야하나",
          "original_form": "마트에서 못봣는데 ㅋㅋㅋ보이면 과연사야하나",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100036334.1.24",
          "form": "고민이에요 ㅋㅋㅋㅋㅋ",
          "original_form": "고민이에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100036334.1.25",
          "form": "보이면 일단 한병쯤은 소장각?ㅋㅋㅋㅋ",
          "original_form": "보이면 일단 한병쯤은 소장각?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100036334.1.26",
          "form": "순하리가 무서운게",
          "original_form": "순하리가 무서운게",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100036334.1.27",
          "form": "오 안독하네? 오오 잘 넘어간다",
          "original_form": "오 안독하네? 오오 잘 넘어간다",
