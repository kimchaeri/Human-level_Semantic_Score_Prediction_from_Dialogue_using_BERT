      "utterance": [
        {
          "id": "MDRW2100013998.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013998.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013998.1.3",
          "form": "최근에 여행가신 적 있으세요?",
          "original_form": "최근에 여행가신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013998.1.4",
          "form": "올해는 없네요 작년에 여수갔었어요",
          "original_form": "올해는 없네요 작년에 여수갔었어요",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013998.1.5",
          "form": "여수 좋으셨겠어요!",
          "original_form": "여수 좋으셨겠어요!",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013998.1.6",
          "form": "어디 추천할만한 곳 있나요?",
          "original_form": "어디 추천할만한 곳 있나요?",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013998.1.7",
          "form": "근데 비가 너무많이와서 제대로 구경못햇어요",
          "original_form": "근데 비가 너무많이와서 제대로 구경못햇어요",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013998.1.8",
          "form": "워낙작아서 사실 하루면 다돌아요ㅋㅋ",
          "original_form": "워낙작아서 사실 하루면 다돌아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013998.1.9",
          "form": "예전에 엑스포 가보고 못가본거 같아요ㅋㅋㅋ",
          "original_form": "예전에 엑스포 가보고 못가본거 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013998.1.10",
          "form": "친가가 여수라 어릴때는 휴가때마다 낚시 갔었는데",
          "original_form": "친가가 여수라 어릴때는 휴가때마다 낚시 갔었는데",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013998.1.11",
          "form": "오랫만에 관광가니 새롭더라고요",
          "original_form": "오랫만에 관광가니 새롭더라고요",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013998.1.12",
          "form": "낚시도 할 수 있군요!",
          "original_form": "낚시도 할 수 있군요!",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100013998.1.13",
          "form": "여수바다에서 하신거에요?ㅎㅎ",
          "original_form": "여수바다에서 하신거에요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100013998.1.14",
          "form": "그쵸 그때는 딴데는 안가고 무조건 바다낚시만 하다 왔었어요",
          "original_form": "그쵸 그때는 딴데는 안가고 무조건 바다낚시만 하다 왔었어요",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013998.1.15",
          "form": "물고기 많이 잡히나요?",
          "original_form": "물고기 많이 잡히나요?",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100013998.1.16",
          "form": "네 고기잡고 성게 이런것도 잡고요ㅋㅋ",
          "original_form": "네 고기잡고 성게 이런것도 잡고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100013998.1.17",
          "form": "오 바다낚시 가보고 싶네요ㅋㅋ",
          "original_form": "오 바다낚시 가보고 싶네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100013998.1.18",
          "form": "요즘은 여행자체를 다니기가 힘드니 아쉽네요",
          "original_form": "요즘은 여행자체를 다니기가 힘드니 아쉽네요",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100013998.1.19",
          "form": "맞아요ㅜ 코로나만 끝나면 해외로 날아가고 싶네요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅜ 코로나만 끝나면 해외로 날아가고 싶네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100013998.1.20",
          "form": "해외여행 많이 다니셨어여?",
          "original_form": "해외여행 많이 다니셨어여?",
          "speaker_id": "2",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100013998.1.21",
          "form": "해외 여행 떠나고 싶어요ㅋㅋ",
          "original_form": "해외 여행 떠나고 싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:13"
        }
      ]
