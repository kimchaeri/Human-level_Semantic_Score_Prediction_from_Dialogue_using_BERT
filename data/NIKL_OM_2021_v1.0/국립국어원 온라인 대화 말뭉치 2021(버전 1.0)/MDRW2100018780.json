      "utterance": [
        {
          "id": "MDRW2100018780.1.1",
          "form": "데리 신작 게임 해볼래?",
          "original_form": "데리 신작 게임 해볼래?",
          "speaker_id": "1",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100018780.1.2",
          "form": "미정사건부나 신야근이나 원신이나",
          "original_form": "미정사건부나 신야근이나 원신이나",
          "speaker_id": "1",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100018780.1.3",
          "form": "엥 나쁜말이라니 넘 바쁘다 세개 다 깔아뒀어..... 정말이야...",
          "original_form": "엥 나쁜말이라니 넘 바쁘다 세개 다 깔아뒀어..... 정말이야...",
          "speaker_id": "2",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018780.1.4",
          "form": "플레이를 해야지",
          "original_form": "플레이를 해야지",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018780.1.5",
          "form": "원신 얼른 계정 열어서",
          "original_form": "원신 얼른 계정 열어서",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018780.1.6",
          "form": "같이 플레이하자",
          "original_form": "같이 플레이하자",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018780.1.7",
          "form": "핸드폰 볼 시간도 없네 리셀? 그런거 막 생각하다보니까 괜히 무서워져서",
          "original_form": "핸드폰 볼 시간도 없네 리셀? 그런거 막 생각하다보니까 괜히 무서워져서",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100018780.1.8",
          "form": "아 리세마라?",
          "original_form": "아 리세마라?",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100018780.1.9",
          "form": "오히려 지금처럼 제대로 플레이 할 시간 없을때 하는게 나아",
          "original_form": "오히려 지금처럼 제대로 플레이 할 시간 없을때 하는게 나아",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100018780.1.10",
          "form": "오토 모드로 켜두고",
          "original_form": "오토 모드로 켜두고",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100018780.1.11",
          "form": "휴대폰 엎어두면",
          "original_form": "휴대폰 엎어두면",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100018780.1.12",
          "form": "알아서 프롤로그 넘어가니까 ㅋㅋㅋ",
          "original_form": "알아서 프롤로그 넘어가니까 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100018780.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100018780.1.14",
          "form": "얼른 해봐 ㅋㅋㅋㅋㅋ",
          "original_form": "얼른 해봐 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100018780.1.15",
          "form": "이거 프롤로그 영상은 스킵 안돼서",
          "original_form": "이거 프롤로그 영상은 스킵 안돼서",
          "speaker_id": "1",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100018780.1.16",
          "form": "사람들이 시간 많이 걸린다고 싫어하는데",
          "original_form": "사람들이 시간 많이 걸린다고 싫어하는데",
          "speaker_id": "1",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100018780.1.17",
          "form": "폰 자주 못보면 걍 틀어두고 덮어두면 됨 ㅋㅋㅋ",
          "original_form": "폰 자주 못보면 걍 틀어두고 덮어두면 됨 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100018780.1.18",
          "form": "그럼 충전시켜두고 해야겠다 라고 쓰고 또 일했어",
          "original_form": "그럼 충전시켜두고 해야겠다 라고 쓰고 또 일했어",
          "speaker_id": "2",
