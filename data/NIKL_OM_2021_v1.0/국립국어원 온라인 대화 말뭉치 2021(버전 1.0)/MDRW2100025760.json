      "utterance": [
        {
          "id": "MDRW2100025760.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ나는 원두 가는게 너무좋음ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ나는 원두 가는게 너무좋음ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100025760.1.2",
          "form": "팔떨어질거같은데",
          "original_form": "팔떨어질거같은데",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100025760.1.3",
          "form": "재밌어",
          "original_form": "재밌어",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100025760.1.4",
          "form": "우리는 ㅅ9그거",
          "original_form": "우리는 ㅅ9그거",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100025760.1.5",
          "form": "가는기계있어",
          "original_form": "가는기계있어",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100025760.1.6",
          "form": "ㅋㅋㅋㅋㄱㅋㅋㅋ5초면 분쇜ㄱㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㄱㅋㅋㅋ5초면 분쇜ㄱㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100025760.1.7",
          "form": "그래서 핸드드립으로내려먹어",
          "original_form": "그래서 핸드드립으로내려먹어",
          "speaker_id": "1",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100025760.1.8",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ기계 사려다가 직접 가는게 갬성이지 이생각으로",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ기계 사려다가 직접 가는게 갬성이지 이생각으로",
          "speaker_id": "2",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100025760.1.9",
          "form": "팔에 고통만 줌",
          "original_form": "팔에 고통만 줌",
          "speaker_id": "2",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100025760.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100025760.1.11",
          "form": "근데그기계 안비쌀텐뎈ㄱㅋㅋㅋㅋ",
          "original_form": "근데그기계 안비쌀텐뎈ㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100025760.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ캡슐을 주로마시니께",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ캡슐을 주로마시니께",
          "speaker_id": "2",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100025760.1.13",
          "form": "별로 필요가 없겐 하더라곸ㅋㅋㅋ",
          "original_form": "별로 필요가 없겐 하더라곸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100025760.1.14",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100025760.1.15",
          "form": "우리는 생각보다 자주쓰더라고",
          "original_form": "우리는 생각보다 자주쓰더라고",
          "speaker_id": "1",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100025760.1.16",
          "form": "캡슐 한번사볼까...",
          "original_form": "캡슐 한번사볼까...",
          "speaker_id": "1",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100025760.1.17",
          "form": "나는 캡슐 추천ㅋㅋㅋ",
          "original_form": "나는 캡슐 추천ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100025760.1.18",
          "form": "넘나 편함",
          "original_form": "넘나 편함",
          "speaker_id": "2",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100025760.1.19",
          "form": "네스프레소가 호환이",
          "original_form": "네스프레소가 호환이",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100025760.1.20",
          "form": "잘되어있다던데",
          "original_form": "잘되어있다던데",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100025760.1.21",
          "form": "일리거사고싶은이마음",
          "original_form": "일리거사고싶은이마음",
          "speaker_id": "1",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100025760.1.22",
          "form": "일리 이뻐ㅠㅠㅠㅠㅠ이쁜데ㅜㅜ",
          "original_form": "일리 이뻐ㅠㅠㅠㅠㅠ이쁜데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025760.1.23",
          "form": "호환되는거 생각하면",
          "original_form": "호환되는거 생각하면",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025760.1.24",
          "form": "네스프레소가ㅇㅇㅇ",
          "original_form": "네스프레소가ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025760.1.25",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025760.1.26",
          "form": "일리가맛있다고하더라고",
          "original_form": "일리가맛있다고하더라고",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025760.1.27",
          "form": "근데일리만먹어야해서",
          "original_form": "근데일리만먹어야해서",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025760.1.28",
          "form": "그게불편해서",
          "original_form": "그게불편해서",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025760.1.29",
          "form": "ㅌㅋㅋㅋㅋㅋㅋ그니깤ㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋ그니깤ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025760.1.30",
          "form": "네사프레소는 대신 여러종류를",
          "original_form": "네사프레소는 대신 여러종류를",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025760.1.31",
          "form": "마실수있져",
          "original_form": "마실수있져",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025760.1.32",
          "form": "스벅도 맛있다고",
          "original_form": "스벅도 맛있다고",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025760.1.33",
          "form": "하더라고",
          "original_form": "하더라고",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025760.1.34",
          "form": "오홍",
          "original_form": "오홍",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025760.1.35",
          "form": "요새는 카페에서도 캡슐이 나오니께",
