      "utterance": [
        {
          "id": "MDRW2100037923.1.1",
          "form": "건강관리하세요?",
          "original_form": "건강관리하세요?",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100037923.1.2",
          "form": "인스턴트 음식 안먹으려고 노력해요",
          "original_form": "인스턴트 음식 안먹으려고 노력해요",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100037923.1.3",
          "form": "전 코로나로 살이 너무 쪄서..ㅠ",
          "original_form": "전 코로나로 살이 너무 쪄서..ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100037923.1.4",
          "form": "운동을 좀 해보려고 해요",
          "original_form": "운동을 좀 해보려고 해요",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100037923.1.5",
          "form": "네... 저도 살쪘어요...",
          "original_form": "네... 저도 살쪘어요...",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037923.1.6",
          "form": "빠지겠죠...",
          "original_form": "빠지겠죠...",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037923.1.7",
          "form": "운동을 하고 싶어도",
          "original_form": "운동을 하고 싶어도",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037923.1.8",
          "form": "센터가 문을 열었다 안열었다 하니ㅠ",
          "original_form": "센터가 문을 열었다 안열었다 하니ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037923.1.9",
          "form": "비타민제는 챙겨드시나요?",
          "original_form": "비타민제는 챙겨드시나요?",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100037923.1.10",
          "form": "비타민 오메가  칼슘은 꼭 챙겨먹으려고해요~",
          "original_form": "비타민 오메가  칼슘은 꼭 챙겨먹으려고해요~",
          "speaker_id": "1",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100037923.1.11",
          "form": "영양제 드시는 거 있으세요?",
          "original_form": "영양제 드시는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100037923.1.12",
          "form": "아뇨 있긴있는데 안먹어지네요",
          "original_form": "아뇨 있긴있는데 안먹어지네요",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037923.1.13",
          "form": "그렇죠?",
          "original_form": "그렇죠?",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100037923.1.14",
          "form": "저도 그래서 항상 눈에 띄는데다 가둬요",
          "original_form": "저도 그래서 항상 눈에 띄는데다 가둬요",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100037923.1.15",
          "form": "잘하고 계시네요",
          "original_form": "잘하고 계시네요",
          "speaker_id": "2",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100037923.1.16",
          "form": "그래야 겨우 먹는듯요",
          "original_form": "그래야 겨우 먹는듯요",
          "speaker_id": "2",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100037923.1.17",
          "form": "맞아요 제 몸은 제가 챙겨야ㅠㅋㅋㅋ",
          "original_form": "맞아요 제 몸은 제가 챙겨야ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100037923.1.18",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100037923.1.19",
          "form": "유일하게 할줄 아는게 수영이에요",
          "original_form": "유일하게 할줄 아는게 수영이에요",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037923.1.20",
          "form": "운동자주하세요?",
          "original_form": "운동자주하세요?",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037923.1.21",
          "form": "모두 수영장을 보고 싶어요~",
          "original_form": "모두 수영장을 보고 싶어요~",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100037923.1.22",
          "form": "항상 마음만 있죠;;",
          "original_form": "항상 마음만 있죠;;",
          "speaker_id": "1",
          "time": "20210901 21:31"
        }
