      "utterance": [
        {
          "id": "MDRW2100020616.1.1",
          "form": "오호 그렇군요",
          "original_form": "오호 그렇군요",
          "speaker_id": "2",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020616.1.2",
          "form": "사람마다 느끼는건 다른데 저희가족은 그랬어요",
          "original_form": "사람마다 느끼는건 다른데 저희가족은 그랬어요",
          "speaker_id": "1",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020616.1.3",
          "form": "비유를 들으니까 딱 알겠네요",
          "original_form": "비유를 들으니까 딱 알겠네요",
          "speaker_id": "2",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020616.1.4",
          "form": "맛에서는 냉동이랑 차이가없다고",
          "original_form": "맛에서는 냉동이랑 차이가없다고",
          "speaker_id": "1",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020616.1.5",
          "form": "당분간 안먹어봐도 되겠어요",
          "original_form": "당분간 안먹어봐도 되겠어요",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020616.1.6",
          "form": "수박 좋아하시면 수박주스 좋아하십니까",
          "original_form": "수박 좋아하시면 수박주스 좋아하십니까",
          "speaker_id": "1",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020616.1.7",
          "form": "망고빙수를 먹겠어요",
          "original_form": "망고빙수를 먹겠어요",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020616.1.8",
          "form": "아 여름 최애메뉴에요",
          "original_form": "아 여름 최애메뉴에요",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020616.1.9",
          "form": "저 아아만 먹는데",
          "original_form": "저 아아만 먹는데",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020616.1.10",
          "form": "저도 겁나좋아해요",
          "original_form": "저도 겁나좋아해요",
          "speaker_id": "1",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020616.1.11",
          "form": "여름만큼은 수박주스를 먹는답니나",
          "original_form": "여름만큼은 수박주스를 먹는답니나",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020616.1.12",
          "form": "근데 잘골라야해요",
          "original_form": "근데 잘골라야해요",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020616.1.13",
          "form": "가끔 수박시럽맛이 나는 곳이 있어요",
          "original_form": "가끔 수박시럽맛이 나는 곳이 있어요",
          "speaker_id": "2",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020616.1.14",
          "form": "맞아요 이상한데가면 물만 겁나넣고 갈아줘서",
          "original_form": "맞아요 이상한데가면 물만 겁나넣고 갈아줘서",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020616.1.15",
          "form": "시럽?",
          "original_form": "시럽?",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020616.1.16",
          "form": "빽다방은 안됩니다",
          "original_form": "빽다방은 안됩니다",
          "speaker_id": "2",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020616.1.17",
          "form": "헐 시럽쓰는 집이있구나",
          "original_form": "헐 시럽쓰는 집이있구나",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020616.1.18",
          "form": "피하겠습니다",
          "original_form": "피하겠습니다",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020616.1.19",
          "form": "시럽은 아니겠지만 시럽맛이 나요 ㅠ",
          "original_form": "시럽은 아니겠지만 시럽맛이 나요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:28"
        }
      ]
