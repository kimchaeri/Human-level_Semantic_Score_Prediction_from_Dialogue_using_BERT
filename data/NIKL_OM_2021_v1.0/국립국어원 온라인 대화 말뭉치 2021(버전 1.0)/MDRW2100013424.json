        },
        {
          "id": "MDRW2100013424.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013424.1.3",
          "form": "폰 게임 하시는거 잇으세여?",
          "original_form": "폰 게임 하시는거 잇으세여?",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013424.1.4",
          "form": "저 쿠키런 하다가 접구 그냥 타일깨는 플래시게임해요",
          "original_form": "저 쿠키런 하다가 접구 그냥 타일깨는 플래시게임해요",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013424.1.5",
          "form": "하시는 게임있으세요?",
          "original_form": "하시는 게임있으세요?",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013424.1.6",
          "form": "저는 꿈의 정원해여",
          "original_form": "저는 꿈의 정원해여",
          "speaker_id": "1",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013424.1.7",
          "form": "그거랑 리듬게임 같은거 조금?",
          "original_form": "그거랑 리듬게임 같은거 조금?",
          "speaker_id": "1",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013424.1.8",
          "form": "저도 꿈의정원했었는데ㅎㅎ",
          "original_form": "저도 꿈의정원했었는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013424.1.9",
          "form": "중독성이 크더라구여ㅋㅋ",
          "original_form": "중독성이 크더라구여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013424.1.10",
          "form": "마자요 ㅋㅋㅋㅋ 저 거의 5000레벨 깨고 있는ㄷ0",
          "original_form": "마자요 ㅋㅋㅋㅋ 저 거의 5000레벨 깨고 있는ㄷ0",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013424.1.11",
          "form": "슬슬 질리는 중이에요 ㅋㅋ",
          "original_form": "슬슬 질리는 중이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013424.1.12",
          "form": "ㅋㅋ레벨 올라가는줄도 모르고 계속 하게되더라구ㅎㅎ",
          "original_form": "ㅋㅋ레벨 올라가는줄도 모르고 계속 하게되더라구ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013424.1.13",
          "form": "리듬게임도 좋아해요ㅋㅋ",
          "original_form": "리듬게임도 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013424.1.14",
          "form": "저도요 저는 그 슈퍼스타 에스엠인가? 슈포스타 뭐시기 하다가",
          "original_form": "저도요 저는 그 슈퍼스타 에스엠인가? 슈포스타 뭐시기 하다가",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013424.1.15",
          "form": "모르는 노래가 너무 많아서",
          "original_form": "모르는 노래가 너무 많아서",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013424.1.16",
          "form": "안하고 있어요 ㅋㅋㅋ",
          "original_form": "안하고 있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013424.1.17",
          "form": "오 그거 너무 어렵더라구요..ㅠㅠ",
          "original_form": "오 그거 너무 어렵더라구요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013424.1.18",
          "form": "마자용 어려워요 ㅠㅠㅠ",
          "original_form": "마자용 어려워요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013424.1.19",
          "form": "오 잘하는데 하다가 미스가 나버려요ㅋㅋ",
          "original_form": "오 잘하는데 하다가 미스가 나버려요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013424.1.20",
          "form": "마자요 ㅋㅋㅋㅋㅋㅋㅋ 별 3개로 끝내고 싶은데 참 힘들더라고요",
          "original_form": "마자요 ㅋㅋㅋㅋㅋㅋㅋ 별 3개로 끝내고 싶은데 참 힘들더라고요",
