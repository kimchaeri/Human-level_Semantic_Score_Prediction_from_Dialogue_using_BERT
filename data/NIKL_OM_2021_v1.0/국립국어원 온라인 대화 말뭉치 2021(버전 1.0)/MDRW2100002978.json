          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002978.1.5",
          "form": "반갑습니다~^^",
          "original_form": "반갑습니다~^^",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002978.1.6",
          "form": "길거리에서 매일보는 그분들요?",
          "original_form": "길거리에서 매일보는 그분들요?",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002978.1.7",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002978.1.8",
          "form": "이 어플 처음깔았는데",
          "original_form": "이 어플 처음깔았는데",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002978.1.9",
          "form": "매우 신기방기",
          "original_form": "매우 신기방기",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002978.1.10",
          "form": "저희동네 많은데",
          "original_form": "저희동네 많은데",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002978.1.11",
          "form": "설마 그 도를 아십니까 하세요?",
          "original_form": "설마 그 도를 아십니까 하세요?",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002978.1.12",
          "form": "ㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002978.1.13",
          "form": "아니죠",
          "original_form": "아니죠",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002978.1.14",
          "form": "어디사시는데요",
          "original_form": "어디사시는데요",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002978.1.15",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002978.1.16",
          "form": "충남요",
          "original_form": "충남요",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002978.1.17",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002978.1.18",
          "form": "충남어디?",
          "original_form": "충남어디?",
          "speaker_id": "1",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002978.1.19",
          "form": "충남은 천주교의 고장이잖아요(깨알 종교주제)",
          "original_form": "충남은 천주교의 고장이잖아요(깨알 종교주제)",
          "speaker_id": "1",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002978.1.20",
          "form": "아 그런건 말하기가 ㅋㅋ",
          "original_form": "아 그런건 말하기가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002978.1.21",
          "form": "ㅋㅋㅋㅋㅋ말씀안하셔도 됩니다",
          "original_form": "ㅋㅋㅋㅋㅋ말씀안하셔도 됩니다",
          "speaker_id": "1",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002978.1.22",
          "form": "결혼하셨어요?",
          "original_form": "결혼하셨어요?",
          "speaker_id": "1",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002978.1.23",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002978.1.24",
          "form": "결혼할때 종교관도 중요한가요?",
          "original_form": "결혼할때 종교관도 중요한가요?",
          "speaker_id": "1",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002978.1.25",
          "form": "사람마다 다르죠",
          "original_form": "사람마다 다르죠",
          "speaker_id": "2",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002978.1.26",
          "form": "기독교 분들은 꼭 같이 다녀야 하는분 많던데요",
          "original_form": "기독교 분들은 꼭 같이 다녀야 하는분 많던데요",
          "speaker_id": "2",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002978.1.27",
          "form": "저도 결혼했는데",
          "original_form": "저도 결혼했는데",
          "speaker_id": "1",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002978.1.28",
          "form": "집사람이 기독교라서",
          "original_form": "집사람이 기독교라서",
          "speaker_id": "1",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002978.1.29",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002978.1.30",
          "form": "사실 저는 안맞춰주거든요ㅋㅋ",
          "original_form": "사실 저는 안맞춰주거든요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002978.1.31",
          "form": "같이 다니자고 하실텐데",
          "original_form": "같이 다니자고 하실텐데",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002978.1.32",
          "form": "철벽 방어 하시나 보네요 ㅋㅋ",
          "original_form": "철벽 방어 하시나 보네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002978.1.33",
          "form": "절대안가요",
          "original_form": "절대안가요",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002978.1.34",
          "form": "종교는 전 별로라 무교입니다",
          "original_form": "종교는 전 별로라 무교입니다",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002978.1.35",
          "form": "그조건으로 결혼했어요ㅋㅋ",
          "original_form": "그조건으로 결혼했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002978.1.36",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002978.1.37",
          "form": "나중에 모태신앙 어쩌구 할까봐",
          "original_form": "나중에 모태신앙 어쩌구 할까봐",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002978.1.38",
          "form": "강요할순없죠",
          "original_form": "강요할순없죠",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002978.1.39",
          "form": "종교는 자유니까",
          "original_form": "종교는 자유니까",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002978.1.40",
          "form": "싫은데 할순 없어요",
          "original_form": "싫은데 할순 없어요",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002978.1.41",
          "form": "주말에 가는게 전 젤 별로였어요",
          "original_form": "주말에 가는게 전 젤 별로였어요",
          "speaker_id": "2",
          "time": "20210820 17:22"
        }
      ]
