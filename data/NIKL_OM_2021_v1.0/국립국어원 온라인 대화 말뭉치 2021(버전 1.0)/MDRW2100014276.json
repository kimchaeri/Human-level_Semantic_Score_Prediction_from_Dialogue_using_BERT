      "utterance": [
        {
          "id": "MDRW2100014276.1.1",
          "form": "쇼핑 좋아하니?",
          "original_form": "쇼핑 좋아하니?",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014276.1.2",
          "form": "어머 name1~ 오랜만이다 야!! 잘 지냈니?? 나 쇼핑 좋아하지~",
          "original_form": "어머 &name1&~ 오랜만이다 야!! 잘 지냈니?? 나 쇼핑 좋아하지~",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014276.1.3",
          "form": "어제도 백화점가서 상품권으로 할인받고 가방 샀잖니~",
          "original_form": "어제도 백화점가서 상품권으로 할인받고 가방 샀잖니~",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014276.1.4",
          "form": "무슨색 가방이니?",
          "original_form": "무슨색 가방이니?",
          "speaker_id": "1",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014276.1.5",
          "form": "빨간색 가방~",
          "original_form": "빨간색 가방~",
          "speaker_id": "2",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014276.1.6",
          "form": "백팩이니??",
          "original_form": "백팩이니??",
          "speaker_id": "1",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014276.1.7",
          "form": "에코백이야 ㅎㅎ",
          "original_form": "에코백이야 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014276.1.8",
          "form": "맞아 요새 에코백 많이 들고 다니더라",
          "original_form": "맞아 요새 에코백 많이 들고 다니더라",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014276.1.9",
          "form": "나는 최근에 선풍기를 삿어",
          "original_form": "나는 최근에 선풍기를 삿어",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014276.1.10",
          "form": "직접 가기 귀찮아서 인터넷으로",
          "original_form": "직접 가기 귀찮아서 인터넷으로",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014276.1.11",
          "form": "와... 나는 인터넷으로 주문하면 불안하더라구",
          "original_form": "와... 나는 인터넷으로 주문하면 불안하더라구",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014276.1.12",
          "form": "그래서 난 꼭 내가 직접 보고사",
          "original_form": "그래서 난 꼭 내가 직접 보고사",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014276.1.13",
          "form": "맞아 인터넷이 뽑기운이 좀 있긴한데",
          "original_form": "맞아 인터넷이 뽑기운이 좀 있긴한데",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014276.1.14",
          "form": "후기 있는 제품으로 다 살펴보고 비교해서 사 ㅎㅎ",
          "original_form": "후기 있는 제품으로 다 살펴보고 비교해서 사 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014276.1.15",
          "form": "인터넷에서 사면 할인도 많이 되고 집으로 오니까 편해",
          "original_form": "인터넷에서 사면 할인도 많이 되고 집으로 오니까 편해",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014276.1.16",
          "form": "나는 예전에 인터넷으로 책 샀다가 무슨 고려시대에서 왔는지 다 찢어지고 너덜너덜해져서",
          "original_form": "나는 예전에 인터넷으로 책 샀다가 무슨 고려시대에서 왔는지 다 찢어지고 너덜너덜해져서",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014276.1.17",
          "form": "그 이후로는 인터넷으로 안 사",
          "original_form": "그 이후로는 인터넷으로 안 사",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014276.1.18",
          "form": "아이고 그건 반품을 해야지",
          "original_form": "아이고 그건 반품을 해야지",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014276.1.19",
          "form": "중고거래라 어쩔수가 없었어 ㅠㅠ",
          "original_form": "중고거래라 어쩔수가 없었어 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014276.1.20",
          "form": "아 중고면 어쩔수없지 ㅠㅠ",
          "original_form": "아 중고면 어쩔수없지 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014276.1.21",
          "form": "그럼 최근에 사고싶은 물건은 뭐야?",
          "original_form": "그럼 최근에 사고싶은 물건은 뭐야?",
          "speaker_id": "1",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014276.1.22",
          "form": "최근에는 의자를 사고 싶더라구",
          "original_form": "최근에는 의자를 사고 싶더라구",
          "speaker_id": "2",
          "time": "20210825 18:19"
        }
      ]
