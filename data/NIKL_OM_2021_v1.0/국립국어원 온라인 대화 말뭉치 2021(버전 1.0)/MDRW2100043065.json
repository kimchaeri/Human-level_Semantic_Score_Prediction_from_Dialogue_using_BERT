      "utterance": [
        {
          "id": "MDRW2100043065.1.1",
          "form": "요즘에도 옵치 자주해?",
          "original_form": "요즘에도 옵치 자주해?",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100043065.1.2",
          "form": "요즘은 안하지",
          "original_form": "요즘은 안하지",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043065.1.3",
          "form": "너도 안하는구나 ㅋㅋㅋㅋㅋ",
          "original_form": "너도 안하는구나 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043065.1.4",
          "form": "우리 몇년했지",
          "original_form": "우리 몇년했지",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043065.1.5",
          "form": "5년했을껄",
          "original_form": "5년했을껄",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043065.1.6",
          "form": "진짜 오래도 했다...",
          "original_form": "진짜 오래도 했다...",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043065.1.7",
          "form": "그러게 말이야 이 망겜을",
          "original_form": "그러게 말이야 이 망겜을",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043065.1.8",
          "form": "그니까...",
          "original_form": "그니까...",
          "speaker_id": "2",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043065.1.9",
          "form": "질문이나 해볼께",
          "original_form": "질문이나 해볼께",
          "speaker_id": "2",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043065.1.10",
          "form": "주캐는?",
          "original_form": "주캐는?",
          "speaker_id": "2",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043065.1.11",
          "form": "초반에 솔져 많이하다가 디바도 하고 자리야 제일많이 했지",
          "original_form": "초반에 솔져 많이하다가 디바도 하고 자리야 제일많이 했지",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043065.1.12",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043065.1.13",
          "form": "나는 초반에 아나 많이 하다가 로드호그 자주했는데",
          "original_form": "나는 초반에 아나 많이 하다가 로드호그 자주했는데",
          "speaker_id": "2",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043065.1.14",
          "form": "로드가 패치먹고 너무 약해져서",
          "original_form": "로드가 패치먹고 너무 약해져서",
          "speaker_id": "2",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043065.1.15",
          "form": "못해먹겠더라",
          "original_form": "못해먹겠더라",
          "speaker_id": "2",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043065.1.16",
          "form": "로드 궁게이지 되었던데...",
          "original_form": "로드 궁게이지 되었던데...",
          "speaker_id": "1",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043065.1.17",
          "form": "그니까...로드픽만해도 난리임",
          "original_form": "그니까...로드픽만해도 난리임",
          "speaker_id": "2",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043065.1.18",
          "form": "망겜수준....",
          "original_form": "망겜수준....",
          "speaker_id": "1",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100043065.1.19",
          "form": "탱커좀 버프해라",
          "original_form": "탱커좀 버프해라",
          "speaker_id": "1",
          "time": "20210907 13:50"
        }
      ]
