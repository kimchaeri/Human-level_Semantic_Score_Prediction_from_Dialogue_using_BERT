      "utterance": [
        {
          "id": "MDRW2100045055.1.1",
          "form": "좋아하시는 음식 있으세요?",
          "original_form": "좋아하시는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045055.1.2",
          "form": "네 저는 부대찌개 좋아해요",
          "original_form": "네 저는 부대찌개 좋아해요",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045055.1.3",
          "form": "부대찌개 맛있죠~",
          "original_form": "부대찌개 맛있죠~",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045055.1.4",
          "form": "음식도 잘하시나요?",
          "original_form": "음식도 잘하시나요?",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045055.1.5",
          "form": "저는 음식을 잘 못해서..ㅎ",
          "original_form": "저는 음식을 잘 못해서..ㅎ",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045055.1.6",
          "form": "집에 있을땐 간단히 차려먹는 편이에요",
          "original_form": "집에 있을땐 간단히 차려먹는 편이에요",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045055.1.7",
          "form": "배달음식 드시나요 그럼?",
          "original_form": "배달음식 드시나요 그럼?",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045055.1.8",
          "form": "아하 간단히 드시는군요",
          "original_form": "아하 간단히 드시는군요",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045055.1.9",
          "form": "밖에서는 아무래도 배달 시켜 먹는거 같아요!",
          "original_form": "밖에서는 아무래도 배달 시켜 먹는거 같아요!",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045055.1.10",
          "form": "좋아하는 배달음식 있으세용??",
          "original_form": "좋아하는 배달음식 있으세용??",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045055.1.11",
          "form": "저는 주로 치킨이나 마라탕 시켜 먹어요",
          "original_form": "저는 주로 치킨이나 마라탕 시켜 먹어요",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045055.1.12",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045055.1.13",
          "form": "저는뭔가 가족이랑 있을땐 치킨 피자 족발 시켜먹고",
          "original_form": "저는뭔가 가족이랑 있을땐 치킨 피자 족발 시켜먹고",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045055.1.14",
          "form": "오랜만에 족발이 먹고 싶네요",
          "original_form": "오랜만에 족발이 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045055.1.15",
          "form": "헉 저두용",
          "original_form": "헉 저두용",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045055.1.16",
          "form": "무뼈닭발도 좋아하시나요",
          "original_form": "무뼈닭발도 좋아하시나요",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045055.1.17",
          "form": "좋아하죠",
          "original_form": "좋아하죠",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045055.1.18",
          "form": "집 근처에 닭발맛집이 있어서",
          "original_form": "집 근처에 닭발맛집이 있어서",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045055.1.19",
          "form": "일년에 두 번은 가는 거 같아요",
          "original_form": "일년에 두 번은 가는 거 같아요",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045055.1.20",
          "form": "와아 부러워요",
          "original_form": "와아 부러워요",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045055.1.21",
          "form": "주먹밥이랑 닭발이랑 같이먹으면 진짜맛있죠",
          "original_form": "주먹밥이랑 닭발이랑 같이먹으면 진짜맛있죠",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045055.1.22",
          "form": "맞아요....거기 식혜가 있는데",
          "original_form": "맞아요....거기 식혜가 있는데",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045055.1.23",
          "form": "그거랑 먹으면 절대 안 질려요",
          "original_form": "그거랑 먹으면 절대 안 질려요",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045055.1.24",
          "form": "와앙 식혜도 맛있죠",
          "original_form": "와앙 식혜도 맛있죠",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045055.1.25",
          "form": "밥알 식감이 종이같긴한데 오랜만에 식혜마시고싶네요",
          "original_form": "밥알 식감이 종이같긴한데 오랜만에 식혜마시고싶네요",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045055.1.26",
          "form": "헉ㅋㅋㅋㅋㅋㅋㅋ밥알 식감 종이 같다는 말 너무 공감돼요",
          "original_form": "헉ㅋㅋㅋㅋㅋㅋㅋ밥알 식감 종이 같다는 말 너무 공감돼요",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045055.1.27",
          "form": "밥알 식감 종이같다는 거 너무 공감돼요 ㅋㅋㅋ",
          "original_form": "밥알 식감 종이같다는 거 너무 공감돼요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:53"
        }
      ]
