      "utterance": [
        {
          "id": "MDRW2100000724.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210819 18:44"
        },
        {
          "id": "MDRW2100000724.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210819 18:44"
        },
        {
          "id": "MDRW2100000724.1.3",
          "form": "운동 좋아하시나요..?",
          "original_form": "운동 좋아하시나요..?",
          "speaker_id": "2",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100000724.1.4",
          "form": "건강하신가요",
          "original_form": "건강하신가요",
          "speaker_id": "1",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100000724.1.5",
          "form": "좋아합니다",
          "original_form": "좋아합니다",
          "speaker_id": "1",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100000724.1.6",
          "form": "와..저는 하는 상상은 합니다..",
          "original_form": "와..저는 하는 상상은 합니다..",
          "speaker_id": "2",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100000724.1.7",
          "form": "사실 수영을 좋아하는데",
          "original_form": "사실 수영을 좋아하는데",
          "speaker_id": "2",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100000724.1.8",
          "form": "코로나로 못간지가 오래 됐네요.",
          "original_form": "코로나로 못간지가 오래 됐네요.",
          "speaker_id": "2",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100000724.1.9",
          "form": "어떤 운동 좋아하시나요?",
          "original_form": "어떤 운동 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100000724.1.10",
          "form": "저도 수영좋아합니다",
          "original_form": "저도 수영좋아합니다",
          "speaker_id": "1",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100000724.1.11",
          "form": "어렸을때 바닷가에서 자라서",
          "original_form": "어렸을때 바닷가에서 자라서",
          "speaker_id": "1",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100000724.1.12",
          "form": "잘합니다",
          "original_form": "잘합니다",
          "speaker_id": "1",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100000724.1.13",
          "form": "지금은 저도 코로스때문에 못하고있어요ㅜㅜ",
          "original_form": "지금은 저도 코로스때문에 못하고있어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100000724.1.14",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000724.1.15",
          "form": "코로나가 잠깐 멈칫 할때 수영 나가볼까!결심하면 기가막히게 거리두기 상향이 되더라구요...",
          "original_form": "코로나가 잠깐 멈칫 할때 수영 나가볼까!결심하면 기가막히게 거리두기 상향이 되더라구요...",
          "speaker_id": "2",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000724.1.16",
          "form": "인정합니다ㅜㅜ",
          "original_form": "인정합니다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100000724.1.17",
          "form": "요즘 코로나 때문에 살이쪄서 고민이에요",
          "original_form": "요즘 코로나 때문에 살이쪄서 고민이에요",
          "speaker_id": "1",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100000724.1.18",
          "form": "원래는 항상 마른체형이었는데",
          "original_form": "원래는 항상 마른체형이었는데",
          "speaker_id": "1",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000724.1.19",
          "form": "집에만 있으니 살이찌긴 찌네요",
          "original_form": "집에만 있으니 살이찌긴 찌네요",
          "speaker_id": "1",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000724.1.20",
          "form": "저는 코로나 직전부터 수영 그만두자마자 기가막히게 살이 붙기 시작해서 변명할 거리가 없네요ㅠㅠㅋㅋㅋ",
          "original_form": "저는 코로나 직전부터 수영 그만두자마자 기가막히게 살이 붙기 시작해서 변명할 거리가 없네요ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000724.1.21",
          "form": "코로나 핑게 대고 운동 안하고 있다가 최근에 그래도 저녁 산책 다니기 시작했어요.",
          "original_form": "코로나 핑게 대고 운동 안하고 있다가 최근에 그래도 저녁 산책 다니기 시작했어요.",
          "speaker_id": "2",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100000724.1.22",
          "form": "저도 운동안하고 있다가",
          "original_form": "저도 운동안하고 있다가",
          "speaker_id": "1",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100000724.1.23",
          "form": "스케이트보드 재밌어보여서 타고 다닐려구요",
          "original_form": "스케이트보드 재밌어보여서 타고 다닐려구요",
          "speaker_id": "1",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100000724.1.24",
          "form": "오오. 스케트보드 어떤가요?",
          "original_form": "오오. 스케트보드 어떤가요?",
          "speaker_id": "2",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000724.1.25",
          "form": "매번 동경 하지만 너무 무서워보여요...",
          "original_form": "매번 동경 하지만 너무 무서워보여요...",
          "speaker_id": "2",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000724.1.26",
          "form": "잘 못타는데",
          "original_form": "잘 못타는데",
          "speaker_id": "1",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100000724.1.27",
          "form": "스케이트 보드를 타면서 멋있게 입는걸 좋아해서ㅎㅎ",
          "original_form": "스케이트 보드를 타면서 멋있게 입는걸 좋아해서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100000724.1.28",
          "form": "그 맛에 탑니다",
          "original_form": "그 맛에 탑니다",
          "speaker_id": "1",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100000724.1.29",
          "form": "와우 멋좽이셨군요ㅋㅋ",
          "original_form": "와우 멋좽이셨군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000724.1.30",
          "form": "그래도 무슨 이유든 붙여서 운동하시는게 멋진 것 같아요.",
          "original_form": "그래도 무슨 이유든 붙여서 운동하시는게 멋진 것 같아요.",
          "speaker_id": "2",
          "time": "20210819 19:09"
        },
        {
          "id": "MDRW2100000724.1.31",
          "form": "저는 정말 슬라임이 되고 있어 걱정입니다...",
          "original_form": "저는 정말 슬라임이 되고 있어 걱정입니다...",
          "speaker_id": "2",
          "time": "20210819 19:09"
        },
        {
          "id": "MDRW2100000724.1.32",
          "form": "운동을ㅈ시작합시다",
          "original_form": "운동을ㅈ시작합시다",
          "speaker_id": "1",
          "time": "20210820 15:48"
        }
      ]
    }
