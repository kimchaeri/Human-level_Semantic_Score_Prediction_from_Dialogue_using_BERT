      "utterance": [
        {
          "id": "MDRW2100000557.1.1",
          "form": "코로나 끝나면 콘서트 가고 싶다",
          "original_form": "코로나 끝나면 콘서트 가고 싶다",
          "speaker_id": "1",
          "time": "20210819 16:51"
        },
        {
          "id": "MDRW2100000557.1.2",
          "form": "미스터트롯 콘서트는 아직 하나요?",
          "original_form": "미스터트롯 콘서트는 아직 하나요?",
          "speaker_id": "2",
          "time": "20210819 16:51"
        },
        {
          "id": "MDRW2100000557.1.3",
          "form": "콘서트는 미스터트롯 콘서트가 최고지",
          "original_form": "콘서트는 미스터트롯 콘서트가 최고지",
          "speaker_id": "1",
          "time": "20210819 16:51"
        },
        {
          "id": "MDRW2100000557.1.4",
          "form": "온통 4단계라 다 취소일 것 같은데",
          "original_form": "온통 4단계라 다 취소일 것 같은데",
          "speaker_id": "2",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000557.1.5",
          "form": "가수들도 많이 나오고 다들 가창력도 좋고 볼만하고",
          "original_form": "가수들도 많이 나오고 다들 가창력도 좋고 볼만하고",
          "speaker_id": "1",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000557.1.6",
          "form": "코로나가 끝나야 공연을 하겠지",
          "original_form": "코로나가 끝나야 공연을 하겠지",
          "speaker_id": "1",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000557.1.7",
          "form": "계약 기간도 얼마 안남았잖아요",
          "original_form": "계약 기간도 얼마 안남았잖아요",
          "speaker_id": "2",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000557.1.8",
          "form": "근데 무슨 첼로공연인가는 하던데",
          "original_form": "근데 무슨 첼로공연인가는 하던데",
          "speaker_id": "1",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000557.1.9",
          "form": "전시회들도 하는 것 같던데",
          "original_form": "전시회들도 하는 것 같던데",
          "speaker_id": "1",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000557.1.10",
          "form": "뮤지컬도 하는 거 봤어요 ㅋㅋ",
          "original_form": "뮤지컬도 하는 거 봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000557.1.11",
          "form": "조승우 나오는 건가 뮤지컬도 하던데",
          "original_form": "조승우 나오는 건가 뮤지컬도 하던데",
          "speaker_id": "1",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000557.1.12",
          "form": "근데 입장할 때 방역수칙이 좀 까다로워요",
          "original_form": "근데 입장할 때 방역수칙이 좀 까다로워요",
          "speaker_id": "2",
          "time": "20210819 16:53"
        },
        {
          "id": "MDRW2100000557.1.13",
          "form": "콘서트만 많이 안되는 것 같아 ㅠㅠ",
          "original_form": "콘서트만 많이 안되는 것 같아 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:53"
        },
        {
          "id": "MDRW2100000557.1.14",
          "form": "그래도 모처럼 뮤지컬은 재밌었습니다",
          "original_form": "그래도 모처럼 뮤지컬은 재밌었습니다",
          "speaker_id": "2",
          "time": "20210819 16:53"
        },
        {
          "id": "MDRW2100000557.1.15",
          "form": "방역수칙을 잘 지켜야 공연을 계속 하지",
          "original_form": "방역수칙을 잘 지켜야 공연을 계속 하지",
          "speaker_id": "1",
          "time": "20210819 16:53"
        },
        {
          "id": "MDRW2100000557.1.16",
          "form": "한 공연에 몇 천명씩 들어가는데",
          "original_form": "한 공연에 몇 천명씩 들어가는데",
          "speaker_id": "1",
          "time": "20210819 16:53"
        },
        {
          "id": "MDRW2100000557.1.17",
          "form": "영화관도 사람 조금씩 늘어나고 있어요",
          "original_form": "영화관도 사람 조금씩 늘어나고 있어요",
          "speaker_id": "2",
          "time": "20210819 16:53"
        },
        {
          "id": "MDRW2100000557.1.18",
          "form": "서로들 조심해야 공연도 다시 하고 그럴것 같아",
          "original_form": "서로들 조심해야 공연도 다시 하고 그럴것 같아",
          "speaker_id": "1",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000557.1.19",
          "form": "모가디슈 재미있습니다~",
          "original_form": "모가디슈 재미있습니다~",
          "speaker_id": "2",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000557.1.20",
          "form": "그래도 사람 없어서 예약하기는 쉽겠다",
          "original_form": "그래도 사람 없어서 예약하기는 쉽겠다",
          "speaker_id": "1",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000557.1.21",
          "form": "꼭 한 번 가서 보세요~^^",
          "original_form": "꼭 한 번 가서 보세요~^^",
          "speaker_id": "2",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000557.1.22",
          "form": "극장에도 좋은 자리 맡으려면",
          "original_form": "극장에도 좋은 자리 맡으려면",
          "speaker_id": "1",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000557.1.23",
          "form": "블랙위도우도 재미있었지안",
          "original_form": "블랙위도우도 재미있었지안",
          "speaker_id": "2",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000557.1.24",
          "form": "미리 예약하고 그랬어야 되는데",
          "original_form": "미리 예약하고 그랬어야 되는데",
          "speaker_id": "1",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000557.1.25",
          "form": "난 저번에 분노의질주가 재미있더라고",
          "original_form": "난 저번에 분노의질주가 재미있더라고",
          "speaker_id": "1",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000557.1.26",
          "form": "모기디슈가 좀 더 괜찮은거 같아요",
          "original_form": "모기디슈가 좀 더 괜찮은거 같아요",
          "speaker_id": "2",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000557.1.27",
          "form": "예약은 필요없슴다",
          "original_form": "예약은 필요없슴다",
          "speaker_id": "2",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000557.1.28",
          "form": "모가디슈가 최고지",
          "original_form": "모가디슈가 최고지",
          "speaker_id": "1",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000557.1.29",
          "form": "가면 자리 많아요 ㅋ",
          "original_form": "가면 자리 많아요 ㅋ",
          "speaker_id": "2",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000557.1.30",
          "form": "한국영화 잘 되라~~",
          "original_form": "한국영화 잘 되라~~",
