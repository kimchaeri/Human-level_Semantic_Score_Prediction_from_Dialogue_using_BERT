      "utterance": [
        {
          "id": "MDRW2100035156.1.1",
          "form": "안녕하세용 오늘 뭐 드셨어용?",
          "original_form": "안녕하세용 오늘 뭐 드셨어용?",
          "speaker_id": "1",
          "time": "20210829 08:39"
        },
        {
          "id": "MDRW2100035156.1.2",
          "form": "안녕하세요~ 아직 아침식사 전이에요 ㅠㅠ",
          "original_form": "안녕하세요~ 아직 아침식사 전이에요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 08:40"
        },
        {
          "id": "MDRW2100035156.1.3",
          "form": "아침식사 하셨나요??",
          "original_form": "아침식사 하셨나요??",
          "speaker_id": "2",
          "time": "20210829 08:41"
        },
        {
          "id": "MDRW2100035156.1.4",
          "form": "네네! 전 방금 카레 먹구왔어용ㅎㅎ",
          "original_form": "네네! 전 방금 카레 먹구왔어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 08:41"
        },
        {
          "id": "MDRW2100035156.1.5",
          "form": "뭐 드실예정이세용?",
          "original_form": "뭐 드실예정이세용?",
          "speaker_id": "1",
          "time": "20210829 08:41"
        },
        {
          "id": "MDRW2100035156.1.6",
          "form": "우와 카레 맛있으셨겠어요 김치랑 먹음 정말 맛있는데 ㅠㅠ",
          "original_form": "우와 카레 맛있으셨겠어요 김치랑 먹음 정말 맛있는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 08:43"
        },
        {
          "id": "MDRW2100035156.1.7",
          "form": "맞아용 ㅎㅎㅎ 고기 들어간 카레 좋아하세용?",
          "original_form": "맞아용 ㅎㅎㅎ 고기 들어간 카레 좋아하세용?",
          "speaker_id": "1",
          "time": "20210829 08:43"
        },
        {
          "id": "MDRW2100035156.1.8",
          "form": "네! 카레는 아무래도 고기 있어야한다고 생각해요",
          "original_form": "네! 카레는 아무래도 고기 있어야한다고 생각해요",
          "speaker_id": "2",
          "time": "20210829 08:44"
        },
        {
          "id": "MDRW2100035156.1.9",
          "form": "야채도 듬뿍넣구요 ㅎㅎ",
          "original_form": "야채도 듬뿍넣구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 08:44"
        },
        {
          "id": "MDRW2100035156.1.10",
          "form": "오 저랑 취향이 같으시네용ㅎㅎ",
          "original_form": "오 저랑 취향이 같으시네용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 08:44"
        },
        {
          "id": "MDRW2100035156.1.11",
          "form": "저는 방금 밥 먹었는데",
          "original_form": "저는 방금 밥 먹었는데",
          "speaker_id": "1",
          "time": "20210829 08:44"
        },
        {
          "id": "MDRW2100035156.1.12",
          "form": "점심엔 뭐먹을까 냉장고 보고있어요",
          "original_form": "점심엔 뭐먹을까 냉장고 보고있어요",
          "speaker_id": "1",
          "time": "20210829 08:45"
        },
        {
          "id": "MDRW2100035156.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 08:45"
        },
        {
          "id": "MDRW2100035156.1.14",
          "form": "그러게요ㅎㅎㅎ",
          "original_form": "그러게요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 08:47"
        },
        {
          "id": "MDRW2100035156.1.15",
          "form": "전 일단 아침으로 순두부라면이 먹고싶어요 \r\n요즘 꽂힌 것 같아요 ㅠㅠ",
          "original_form": "전 일단 아침으로 순두부라면이 먹고싶어요 \r\n요즘 꽂힌 것 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 08:47"
        },
        {
          "id": "MDRW2100035156.1.16",
          "form": "헉 요즘 유행하던데",
          "original_form": "헉 요즘 유행하던데",
          "speaker_id": "1",
          "time": "20210829 08:47"
        },
        {
          "id": "MDRW2100035156.1.17",
          "form": "열라면에 순두부 넣고 끓인거 맞죵?",
          "original_form": "열라면에 순두부 넣고 끓인거 맞죵?",
          "speaker_id": "1",
          "time": "20210829 08:47"
        },
        {
          "id": "MDRW2100035156.1.18",
          "form": "어제 점심으로도 먹었는데 너무 맛있더라구요.. 점심까지 못참겠는데 순두부를 사러가야해서 조금 귀찮네요",
          "original_form": "어제 점심으로도 먹었는데 너무 맛있더라구요.. 점심까지 못참겠는데 순두부를 사러가야해서 조금 귀찮네요",
          "speaker_id": "2",
          "time": "20210829 08:47"
        },
        {
          "id": "MDRW2100035156.1.19",
          "form": "네 맞아요! 열라면은 매워서 진라면으로 해서 먹어요~",
          "original_form": "네 맞아요! 열라면은 매워서 진라면으로 해서 먹어요~",
          "speaker_id": "2",
          "time": "20210829 08:48"
        },
        {
          "id": "MDRW2100035156.1.20",
          "form": "아 진짜요? 중독되셨나봐요ㅋㅋㅋㅋ",
          "original_form": "아 진짜요? 중독되셨나봐요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 08:48"
        },
        {
          "id": "MDRW2100035156.1.21",
          "form": "오호 저도 라면 좋아하는데",
          "original_form": "오호 저도 라면 좋아하는데",
          "speaker_id": "1",
          "time": "20210829 08:48"
        },
        {
          "id": "MDRW2100035156.1.22",
          "form": "만들어 먹어야겠어요ㅋㅋ",
          "original_form": "만들어 먹어야겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 08:48"
        },
        {
          "id": "MDRW2100035156.1.23",
          "form": "name1님 입맛에도 맞으셨음 좋겠어요 ㅎㅎ\r\n포인트는 후추에요!",
          "original_form": "&name1&님 입맛에도 맞으셨음 좋겠어요 ㅎㅎ\r\n포인트는 후추에요!",
          "speaker_id": "2",
          "time": "20210829 08:49"
        },
        {
          "id": "MDRW2100035156.1.24",
          "form": "아아 후추!",
          "original_form": "아아 후추!",
          "speaker_id": "1",
          "time": "20210829 08:49"
        },
        {
          "id": "MDRW2100035156.1.25",
          "form": "어제 까먹고 초반에 후추을 안넣었더니 심심하더라구요 ㅠㅠ",
          "original_form": "어제 까먹고 초반에 후추을 안넣었더니 심심하더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 08:49"
        },
        {
          "id": "MDRW2100035156.1.26",
          "form": "곡 기억할게용ㅋㅋㅋ",
          "original_form": "곡 기억할게용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 08:50"
        },
        {
          "id": "MDRW2100035156.1.27",
          "form": "요즘 그릭복숭아 유행이던데",
          "original_form": "요즘 그릭복숭아 유행이던데",
          "speaker_id": "1",
