      "utterance": [
        {
          "id": "MDRW2100016602.1.1",
          "form": "요즘 업무에 힘든 점은 없으시나여",
          "original_form": "요즘 업무에 힘든 점은 없으시나여",
          "speaker_id": "1",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016602.1.2",
          "form": "가을장마가너무힘들게해요",
          "original_form": "가을장마가너무힘들게해요",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016602.1.3",
          "form": "그리고지금은또너무덥네여",
          "original_form": "그리고지금은또너무덥네여",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016602.1.4",
          "form": "힘드신점없으싯가요",
          "original_form": "힘드신점없으싯가요",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016602.1.5",
          "form": "저는 뭐 그냥 코로나가 힘드네여",
          "original_form": "저는 뭐 그냥 코로나가 힘드네여",
          "speaker_id": "1",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016602.1.6",
          "form": "그외에 업무가 어렵진 않아요",
          "original_form": "그외에 업무가 어렵진 않아요",
          "speaker_id": "1",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016602.1.7",
          "form": "아 천직이예요역시",
          "original_form": "아 천직이예요역시",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016602.1.8",
          "form": "고등학교보다 수업 준비 부담도 덜하기도 하고",
          "original_form": "고등학교보다 수업 준비 부담도 덜하기도 하고",
          "speaker_id": "1",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016602.1.9",
          "form": "거기오래있어요",
          "original_form": "거기오래있어요",
          "speaker_id": "2",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016602.1.10",
          "form": "이쁘게바뀐것두좀누리구",
          "original_form": "이쁘게바뀐것두좀누리구",
          "speaker_id": "2",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016602.1.11",
          "form": "그랬으면 좋겠네요 만족도 최고의 직장",
          "original_form": "그랬으면 좋겠네요 만족도 최고의 직장",
          "speaker_id": "1",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016602.1.12",
          "form": "오늘 더워서 고생 많으시겠어요",
          "original_form": "오늘 더워서 고생 많으시겠어요",
          "speaker_id": "1",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016602.1.13",
          "form": "맞아요 고생좀했는데 아주머니가 근로장려금타서 음료수사줬어요",
          "original_form": "맞아요 고생좀했는데 아주머니가 근로장려금타서 음료수사줬어요",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100016602.1.14",
          "form": "부러웠어요",
          "original_form": "부러웠어요",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100016602.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100016602.1.16",
          "form": "왜 그분은 받는거에요",
          "original_form": "왜 그분은 받는거에요",
          "speaker_id": "1",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100016602.1.17",
          "form": "심지어 예상금액보다 더받으셨어요 부러워요..",
          "original_form": "심지어 예상금액보다 더받으셨어요 부러워요..",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100016602.1.18",
          "form": "될사람은 되네요...",
          "original_form": "될사람은 되네요...",
          "speaker_id": "1",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100016602.1.19",
          "form": "name2님도 근로 장려받아야 하는데...",
          "original_form": "&name2&님도 근로 장려받아야 하는데...",
          "speaker_id": "1",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100016602.1.20",
          "form": "음료수 비싼거 사주던가요",
          "original_form": "음료수 비싼거 사주던가요",
          "speaker_id": "1",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100016602.1.21",
          "form": "그냥제가자발적으로싼거시켰는데",
          "original_form": "그냥제가자발적으로싼거시켰는데",
          "speaker_id": "2",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100016602.1.22",
          "form": "흑당라떼시켰더니  흑당이랑우유밖에없더라고요\r\n커핀줄알았어요",
          "original_form": "흑당라떼시켰더니  흑당이랑우유밖에없더라고요\r\n커핀줄알았어요",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100016602.1.23",
          "form": "저런 비싼거 사먹어야져",
          "original_form": "저런 비싼거 사먹어야져",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100016602.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100016602.1.25",
          "form": "커피는안들어가나요했더니 돈더줘야한대요",
          "original_form": "커피는안들어가나요했더니 돈더줘야한대요",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100016602.1.26",
          "form": "근데 제앞사람이 아메리카노 사들고갔는데 남은게좀있디고 그거부어줬어요ㅋㄱㅋㅋㅋ",
          "original_form": "근데 제앞사람이 아메리카노 사들고갔는데 남은게좀있디고 그거부어줬어요ㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100016602.1.27",
          "form": "에이 한턱냈다는 그 소리 하지도 말라 그래요!",
          "original_form": "에이 한턱냈다는 그 소리 하지도 말라 그래요!",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100016602.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ악",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ악",
