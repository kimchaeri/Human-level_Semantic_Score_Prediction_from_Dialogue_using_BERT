      "utterance": [
        {
          "id": "MDRW2100005617.1.1",
          "form": "집에서 집안일 하세요?",
          "original_form": "집에서 집안일 하세요?",
          "speaker_id": "1",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100005617.1.2",
          "form": "청소랑 설거지 정도 해요",
          "original_form": "청소랑 설거지 정도 해요",
          "speaker_id": "2",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100005617.1.3",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100005617.1.4",
          "form": "저는... 가족이랑 사는데 거의 아무것도 안해요",
          "original_form": "저는... 가족이랑 사는데 거의 아무것도 안해요",
          "speaker_id": "1",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100005617.1.5",
          "form": "그래도 가끔 하신다면 주로 어떤 일을 하세요?",
          "original_form": "그래도 가끔 하신다면 주로 어떤 일을 하세요?",
          "speaker_id": "2",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100005617.1.6",
          "form": "씻고 난 후 대충 화장실 청소하는 정도...?",
          "original_form": "씻고 난 후 대충 화장실 청소하는 정도...?",
          "speaker_id": "1",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100005617.1.7",
          "form": "반성하게 되네요",
          "original_form": "반성하게 되네요",
          "speaker_id": "1",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100005617.1.8",
          "form": "저도 부모님이랑 살 때는 거의 안 했어요~",
          "original_form": "저도 부모님이랑 살 때는 거의 안 했어요~",
          "speaker_id": "2",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100005617.1.9",
          "form": "형제 있으세요?",
          "original_form": "형제 있으세요?",
          "speaker_id": "2",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100005617.1.10",
          "form": "남동생 하나 있어요 4살차이!!",
          "original_form": "남동생 하나 있어요 4살차이!!",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100005617.1.11",
          "form": "고등학생이에요",
          "original_form": "고등학생이에요",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100005617.1.12",
          "form": "헉 저도 5살 차이나는 남동생있어요",
          "original_form": "헉 저도 5살 차이나는 남동생있어요",
          "speaker_id": "2",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100005617.1.13",
          "form": "동생이랑 친하신가요?",
          "original_form": "동생이랑 친하신가요?",
          "speaker_id": "2",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100005617.1.14",
          "form": "안친해요 ㅋㅋㅋ",
          "original_form": "안친해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:09"
        },
        {
          "id": "MDRW2100005617.1.15",
          "form": "앜ㅋ 저도요",
          "original_form": "앜ㅋ 저도요",
          "speaker_id": "2",
          "time": "20210822 16:09"
        },
        {
          "id": "MDRW2100005617.1.16",
          "form": "별로 성격적으로 안맞아요  ㅋㅋㅋㅋ",
          "original_form": "별로 성격적으로 안맞아요  ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:09"
        },
        {
          "id": "MDRW2100005617.1.17",
          "form": "저도 서로 약간 한심해하는 스타일",
          "original_form": "저도 서로 약간 한심해하는 스타일",
          "speaker_id": "2",
          "time": "20210822 16:09"
        },
        {
          "id": "MDRW2100005617.1.18",
          "form": "안 챙기고 그냥 각자 지내요",
          "original_form": "안 챙기고 그냥 각자 지내요",
          "speaker_id": "2",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100005617.1.19",
          "form": "전 가족이라도 안맞으면 억지로 친해지려 노력하기 싫더라고요",
          "original_form": "전 가족이라도 안맞으면 억지로 친해지려 노력하기 싫더라고요",
          "speaker_id": "1",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100005617.1.20",
          "form": "어느정도 거리유지하는게 편해요",
          "original_form": "어느정도 거리유지하는게 편해요",
          "speaker_id": "1",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100005617.1.21",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
