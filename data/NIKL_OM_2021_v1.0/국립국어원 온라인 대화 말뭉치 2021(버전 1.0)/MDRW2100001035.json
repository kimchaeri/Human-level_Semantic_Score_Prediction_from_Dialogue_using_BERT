        },
        {
          "id": "MDRW2100001035.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210722 09:53"
        },
        {
          "id": "MDRW2100001035.1.3",
          "form": "ㄷ운동하세요?",
          "original_form": "ㄷ운동하세요?",
          "speaker_id": "2",
          "time": "20210722 09:53"
        },
        {
          "id": "MDRW2100001035.1.4",
          "form": "운동 잘 안해요",
          "original_form": "운동 잘 안해요",
          "speaker_id": "1",
          "time": "20210722 09:53"
        },
        {
          "id": "MDRW2100001035.1.5",
          "form": "해야되는데..",
          "original_form": "해야되는데..",
          "speaker_id": "1",
          "time": "20210722 09:53"
        },
        {
          "id": "MDRW2100001035.1.6",
          "form": "아하 뭐 걸리는게 있나요?",
          "original_form": "아하 뭐 걸리는게 있나요?",
          "speaker_id": "2",
          "time": "20210722 09:54"
        },
        {
          "id": "MDRW2100001035.1.7",
          "form": "귀찮음?",
          "original_form": "귀찮음?",
          "speaker_id": "2",
          "time": "20210722 09:54"
        },
        {
          "id": "MDRW2100001035.1.8",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "1",
          "time": "20210722 09:54"
        },
        {
          "id": "MDRW2100001035.1.9",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210722 09:54"
        },
        {
          "id": "MDRW2100001035.1.10",
          "form": "운동하시나요?",
          "original_form": "운동하시나요?",
          "speaker_id": "1",
          "time": "20210722 09:54"
        },
        {
          "id": "MDRW2100001035.1.11",
          "form": "회사 들어가기전에는 조금 했는데",
          "original_form": "회사 들어가기전에는 조금 했는데",
          "speaker_id": "2",
          "time": "20210722 09:54"
        },
        {
          "id": "MDRW2100001035.1.12",
          "form": "일과가 생기니 힘드네요 ㅋㅋ",
          "original_form": "일과가 생기니 힘드네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210722 09:55"
        },
        {
          "id": "MDRW2100001035.1.13",
          "form": "ㅋㅋㅋ비타민같은건 챙겨드세요?",
          "original_form": "ㅋㅋㅋ비타민같은건 챙겨드세요?",
          "speaker_id": "1",
          "time": "20210722 09:55"
        },
        {
          "id": "MDRW2100001035.1.14",
          "form": "비타민은 안먹고",
          "original_form": "비타민은 안먹고",
          "speaker_id": "2",
          "time": "20210722 09:55"
        },
        {
          "id": "MDRW2100001035.1.15",
          "form": "라토핏은 먹습니당",
          "original_form": "라토핏은 먹습니당",
          "speaker_id": "2",
          "time": "20210722 09:55"
        },
        {
          "id": "MDRW2100001035.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210722 09:55"
        },
        {
          "id": "MDRW2100001035.1.17",
          "form": "저는 약도 귀찮아서 잘안먹어요",
          "original_form": "저는 약도 귀찮아서 잘안먹어요",
          "speaker_id": "1",
          "time": "20210722 09:55"
        },
        {
          "id": "MDRW2100001035.1.18",
          "form": "먹어야되는데",
          "original_form": "먹어야되는데",
          "speaker_id": "1",
          "time": "20210722 09:56"
        },
        {
          "id": "MDRW2100001035.1.19",
          "form": "그럼 뭘 하시나요...",
          "original_form": "그럼 뭘 하시나요...",
          "speaker_id": "2",
          "time": "20210722 09:56"
        },
        {
          "id": "MDRW2100001035.1.20",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210722 09:56"
        },
        {
          "id": "MDRW2100001035.1.21",
          "form": "누워있습니다",
          "original_form": "누워있습니다",
          "speaker_id": "1",
          "time": "20210722 09:56"
        },
        {
          "id": "MDRW2100001035.1.22",
          "form": "휴식이 건강도 아주 중요합니다",
          "original_form": "휴식이 건강도 아주 중요합니다",
          "speaker_id": "1",
          "time": "20210722 09:56"
        },
        {
          "id": "MDRW2100001035.1.23",
          "form": "근데 뭐 운동 안해도 그렇죠?",
          "original_form": "근데 뭐 운동 안해도 그렇죠?",
          "speaker_id": "2",
          "time": "20210722 09:56"
        },
        {
          "id": "MDRW2100001035.1.24",
          "form": "식이요법하시면되지",
          "original_form": "식이요법하시면되지",
