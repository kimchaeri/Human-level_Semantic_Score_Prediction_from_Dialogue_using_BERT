          "speaker_id": "1",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100024103.1.6",
          "form": "방금 당근할려고 사진찍엇네요",
          "original_form": "방금 당근할려고 사진찍엇네요",
          "speaker_id": "2",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100024103.1.7",
          "form": "당근좋아하십니까?",
          "original_form": "당근좋아하십니까?",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100024103.1.8",
          "form": "오!!!",
          "original_form": "오!!!",
          "speaker_id": "1",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100024103.1.9",
          "form": "전 당근마켓 쇼핑 좋아합니다",
          "original_form": "전 당근마켓 쇼핑 좋아합니다",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100024103.1.10",
          "form": "뭐 파시게요???",
          "original_form": "뭐 파시게요???",
          "speaker_id": "1",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100024103.1.11",
          "form": "오... 얼마전에 이사했어서",
          "original_form": "오... 얼마전에 이사했어서",
          "speaker_id": "1",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100024103.1.12",
          "form": "네 식탁 바꿧서요",
          "original_form": "네 식탁 바꿧서요",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100024103.1.13",
          "form": "그때 많이 팔았어요",
          "original_form": "그때 많이 팔았어요",
          "speaker_id": "1",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100024103.1.14",
          "form": "오 저두",
          "original_form": "오 저두",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100024103.1.15",
          "form": "오~~~",
          "original_form": "오~~~",
          "speaker_id": "1",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100024103.1.16",
          "form": "이게 한번보면 계속 보게되더라구요",
          "original_form": "이게 한번보면 계속 보게되더라구요",
          "speaker_id": "1",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100024103.1.17",
          "form": "집을 샀습니다",
          "original_form": "집을 샀습니다",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100024103.1.18",
          "form": "당근으로요?",
          "original_form": "당근으로요?",
          "speaker_id": "1",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100024103.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100024103.1.20",
          "form": "강아지집..?",
          "original_form": "강아지집..?",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100024103.1.21",
          "form": "애기 집을 하나 삿어요",
          "original_form": "애기 집을 하나 삿어요",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100024103.1.22",
          "form": "아 사실 진짜 집을 삿서요 ㅋㅋㅋ",
          "original_form": "아 사실 진짜 집을 삿서요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100024103.1.23",
          "form": "애기집...이요??",
          "original_form": "애기집...이요??",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100024103.1.24",
          "form": "당근으로요 해서 드립생각이 안낫서요 ㅋㅋ",
          "original_form": "당근으로요 해서 드립생각이 안낫서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100024103.1.25",
          "form": "요즘엔 애기집도 있나요?",
          "original_form": "요즘엔 애기집도 있나요?",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100024103.1.26",
          "form": "음 있어요",
          "original_form": "음 있어요",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100024103.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100024103.1.28",
          "form": "머졍",
          "original_form": "머졍",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100024103.1.29",
          "form": "애기들 놀게 하는 플레이하우스 같은게 있어요",
          "original_form": "애기들 놀게 하는 플레이하우스 같은게 있어요",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024103.1.30",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024103.1.31",
          "form": "아@!!!!",
          "original_form": "아@!!!!",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024103.1.32",
          "form": "애기들들어가서 노는 거",
          "original_form": "애기들들어가서 노는 거",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024103.1.33",
          "form": "저희도 그런데서 놀아봤다 아닙니까?",
          "original_form": "저희도 그런데서 놀아봤다 아닙니까?",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024103.1.34",
          "form": "네 그거",
          "original_form": "네 그거",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024103.1.35",
          "form": "저는 땅파서 놀았습니다",
          "original_form": "저는 땅파서 놀았습니다",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024103.1.36",
          "form": "비싼건 못해봣네요 ㅋㅋ",
          "original_form": "비싼건 못해봣네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024103.1.37",
          "form": "후 오래되긴했는지 기억이 빨리안됫네요^^,;",
          "original_form": "후 오래되긴했는지 기억이 빨리안됫네요^^,;",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024103.1.38",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024103.1.39",
          "form": "자연이 최곱니다",
          "original_form": "자연이 최곱니다",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024103.1.40",
          "form": "땅거지 아시죠?",
          "original_form": "땅거지 아시죠?",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024103.1.41",
          "form": "아 쇼핑",
          "original_form": "아 쇼핑",
          "speaker_id": "2",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100024103.1.42",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
