      "utterance": [
        {
          "id": "MDRW2100043334.1.1",
          "form": "사인 가족이면 백만원??",
          "original_form": "사인 가족이면 백만원??",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100043334.1.2",
          "form": "백만원이요ㅋㅋㅋ",
          "original_form": "백만원이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100043334.1.3",
          "form": "옷도 한 벌 사야되겠네요ㅋㅋㅋㅋㅋ",
          "original_form": "옷도 한 벌 사야되겠네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100043334.1.4",
          "form": "그래옷도사고",
          "original_form": "그래옷도사고",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100043334.1.5",
          "form": "좋네 공짜돈",
          "original_form": "좋네 공짜돈",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100043334.1.6",
          "form": "돈 받을 때는 좋고",
          "original_form": "돈 받을 때는 좋고",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100043334.1.7",
          "form": "세금 나갈 때는 싫고",
          "original_form": "세금 나갈 때는 싫고",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100043334.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100043334.1.9",
          "form": "그러니까요ㅋㅋㅋㅋㅋ",
          "original_form": "그러니까요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100043334.1.10",
          "form": "이거주고",
          "original_form": "이거주고",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100043334.1.11",
          "form": "또 얼마나 걷어갈런지....",
          "original_form": "또 얼마나 걷어갈런지....",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043334.1.12",
          "form": "계속 내니까",
          "original_form": "계속 내니까",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043334.1.13",
          "form": "나라다 망하겠음",
          "original_form": "나라다 망하겠음",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043334.1.14",
          "form": "지금 태어나는 애들",
          "original_form": "지금 태어나는 애들",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043334.1.15",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043334.1.16",
          "form": "한 명당 빚이 일억이라잖아요;;",
          "original_form": "한 명당 빚이 일억이라잖아요;;",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043334.1.17",
          "form": "빚더미에 사는거지",
          "original_form": "빚더미에 사는거지",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043334.1.18",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043334.1.19",
          "form": "불쌍하다 정말ㅠ",
          "original_form": "불쌍하다 정말ㅠ",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043334.1.20",
          "form": "가난에서 벗어나기가 힘들다",
          "original_form": "가난에서 벗어나기가 힘들다",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043334.1.21",
          "form": "근데 요즘 복지 보면",
          "original_form": "근데 요즘 복지 보면",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043334.1.22",
          "form": "정말 가난하면은 오히려 더 나아요",
          "original_form": "정말 가난하면은 오히려 더 나아요",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043334.1.23",
          "form": "그러이",
          "original_form": "그러이",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043334.1.24",
          "form": "나라에서 다 주니까ㅋㅋㅋㅋㅋ",
          "original_form": "나라에서 다 주니까ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100043334.1.25",
          "form": "돈 진짜 많이 주잖아",
          "original_form": "돈 진짜 많이 주잖아",
          "speaker_id": "2",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100043334.1.26",
          "form": "우리 같은 사람이 힘든ㅋㅋㅋㅋㅋ",
          "original_form": "우리 같은 사람이 힘든ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100043334.1.27",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100043334.1.28",
          "form": "어중간하게 버는 사람들이",
          "original_form": "어중간하게 버는 사람들이",
          "speaker_id": "2",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100043334.1.29",
          "form": "제일 손해를 많이 본다니까",
          "original_form": "제일 손해를 많이 본다니까",
          "speaker_id": "2",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100043334.1.30",
          "form": "그러니까요 혜택은 다 못 보고",
          "original_form": "그러니까요 혜택은 다 못 보고",
          "speaker_id": "1",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100043334.1.31",
          "form": "우리는 지원금 같은 것도 못 받잖아",
          "original_form": "우리는 지원금 같은 것도 못 받잖아",
          "speaker_id": "2",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100043334.1.32",
          "form": "세금은 다 떼이고",
          "original_form": "세금은 다 떼이고",
          "speaker_id": "1",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100043334.1.33",
          "form": "그냥 내기만 계속 내는거야",
          "original_form": "그냥 내기만 계속 내는거야",
          "speaker_id": "2",
          "time": "20210907 17:56"
        }
      ]
    }
