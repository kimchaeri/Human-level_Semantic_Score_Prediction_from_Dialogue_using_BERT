      "utterance": [
        {
          "id": "MDRW2100008871.1.1",
          "form": "요새 비가 많이와요",
          "original_form": "요새 비가 많이와요",
          "speaker_id": "2",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008871.1.2",
          "form": "네 태풍도 온다고 하던데요",
          "original_form": "네 태풍도 온다고 하던데요",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008871.1.3",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008871.1.4",
          "form": "내일요?",
          "original_form": "내일요?",
          "speaker_id": "2",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008871.1.5",
          "form": "예보에는 그렇던데 큰태풍은 아닌가봐요",
          "original_form": "예보에는 그렇던데 큰태풍은 아닌가봐요",
          "speaker_id": "1",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008871.1.6",
          "form": "비만 온다고 하더라구요",
          "original_form": "비만 온다고 하더라구요",
          "speaker_id": "1",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008871.1.7",
          "form": "우산을 들고다녀야겠네요",
          "original_form": "우산을 들고다녀야겠네요",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008871.1.8",
          "form": "여름에는 별로 안오더니 가을다가오니까 비가 오네요",
          "original_form": "여름에는 별로 안오더니 가을다가오니까 비가 오네요",
          "speaker_id": "1",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008871.1.9",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008871.1.10",
          "form": "몰아서 오나봐요",
          "original_form": "몰아서 오나봐요",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008871.1.11",
          "form": "추워질거같기두 하구...",
          "original_form": "추워질거같기두 하구...",
          "speaker_id": "1",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008871.1.12",
          "form": "전 너무 더워서 좀 추워지면 좋겠어요",
          "original_form": "전 너무 더워서 좀 추워지면 좋겠어요",
          "speaker_id": "2",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008871.1.13",
          "form": "네 여름이 엄청 더웠죠ㅜㅜ",
          "original_form": "네 여름이 엄청 더웠죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008871.1.14",
          "form": "여름 겨울 뭐가 더 좋나요",
          "original_form": "여름 겨울 뭐가 더 좋나요",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008871.1.15",
          "form": "저는 겨울이 좋네요ㅎㅎ",
          "original_form": "저는 겨울이 좋네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008871.1.16",
          "form": "저도 겨울이 좋아요",
          "original_form": "저도 겨울이 좋아요",
          "speaker_id": "2",
