        },
        {
          "id": "MDRW2100018955.1.2",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210827 13:56"
        },
        {
          "id": "MDRW2100018955.1.3",
          "form": "그러면지방에서오는애들은",
          "original_form": "그러면지방에서오는애들은",
          "speaker_id": "1",
          "time": "20210827 13:56"
        },
        {
          "id": "MDRW2100018955.1.4",
          "form": "무조건자취야?",
          "original_form": "무조건자취야?",
          "speaker_id": "1",
          "time": "20210827 13:56"
        },
        {
          "id": "MDRW2100018955.1.5",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 13:56"
        },
        {
          "id": "MDRW2100018955.1.6",
          "form": "자취 말고는 방법X",
          "original_form": "자취 말고는 방법X",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018955.1.7",
          "form": "기숙사만들생각도없대?",
          "original_form": "기숙사만들생각도없대?",
          "speaker_id": "1",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018955.1.8",
          "form": "그럴 것 가같음 아마",
          "original_form": "그럴 것 가같음 아마",
          "speaker_id": "2",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018955.1.9",
          "form": "그냥 포기하고 다니려고",
          "original_form": "그냥 포기하고 다니려고",
          "speaker_id": "2",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018955.1.10",
          "form": "우리 대면 공지 떴다 드디어",
          "original_form": "우리 대면 공지 떴다 드디어",
          "speaker_id": "2",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018955.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018955.1.12",
          "form": "비대면",
          "original_form": "비대면",
          "speaker_id": "1",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018955.1.13",
          "form": "이래?",
          "original_form": "이래?",
          "speaker_id": "1",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018955.1.14",
          "form": "4단계까지는 비대면으로 하는데 낮아지면 격주로 하겠대",
          "original_form": "4단계까지는 비대면으로 하는데 낮아지면 격주로 하겠대",
          "speaker_id": "2",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018955.1.15",
          "form": "뭐임",
          "original_form": "뭐임",
          "speaker_id": "1",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018955.1.16",
          "form": "그러면9월중간에",
          "original_form": "그러면9월중간에",
          "speaker_id": "1",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018955.1.17",
          "form": "3단계되면",
          "original_form": "3단계되면",
          "speaker_id": "1",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100018955.1.18",
          "form": "학교가는거임?",
          "original_form": "학교가는거임?",
          "speaker_id": "1",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100018955.1.19",
          "form": "그럴 수도 있음 진심",
          "original_form": "그럴 수도 있음 진심",
          "speaker_id": "2",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100018955.1.20",
          "form": "아니 내가 코로나 4단계를 바라야만 하나",
          "original_form": "아니 내가 코로나 4단계를 바라야만 하나",
          "speaker_id": "2",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100018955.1.21",
          "form": "그러다또4단계올라가면",
          "original_form": "그러다또4단계올라가면",
          "speaker_id": "1",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100018955.1.22",
          "form": "다시대면하고?",
          "original_form": "다시대면하고?",
          "speaker_id": "1",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100018955.1.23",
          "form": "비대면",
          "original_form": "비대면",
          "speaker_id": "1",
          "time": "20210827 13:59"
        },
