      "utterance": [
        {
          "id": "MDRW2100008482.1.1",
          "form": "좋아하는 연예인 잇어요?",
          "original_form": "좋아하는 연예인 잇어요?",
          "speaker_id": "1",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100008482.1.2",
          "form": "저 아이돌 좋아해요",
          "original_form": "저 아이돌 좋아해요",
          "speaker_id": "2",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100008482.1.3",
          "form": "오 누구 좋아해여?",
          "original_form": "오 누구 좋아해여?",
          "speaker_id": "1",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100008482.1.4",
          "form": "레드벨벳이나 드림캐쳐 공원소녀 너무 많아서 못치겠네요 ㅋㄱㅋㄱㅋㄱ",
          "original_form": "레드벨벳이나 드림캐쳐 공원소녀 너무 많아서 못치겠네요 ㅋㄱㅋㄱㅋㄱ",
          "speaker_id": "2",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008482.1.5",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008482.1.6",
          "form": "공원소녀? 첨 들어봐요 !!",
          "original_form": "공원소녀? 첨 들어봐요 !!",
          "speaker_id": "1",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008482.1.7",
          "form": "공원소녀는 미야라는 멤버가 유명해요 미야 공중곡예사라고 치면 뜨는 영상이 엄청 인기에요",
          "original_form": "공원소녀는 미야라는 멤버가 유명해요 미야 공중곡예사라고 치면 뜨는 영상이 엄청 인기에요",
          "speaker_id": "2",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008482.1.8",
          "form": "오 한번 쳐볼게요",
          "original_form": "오 한번 쳐볼게요",
          "speaker_id": "1",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008482.1.9",
          "form": "넵 노래도 좋아요!",
          "original_form": "넵 노래도 좋아요!",
          "speaker_id": "2",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008482.1.10",
          "form": "오 되게",
          "original_form": "오 되게",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008482.1.11",
          "form": "매력젹이게 생겻어요",
          "original_form": "매력젹이게 생겻어요",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008482.1.12",
          "form": "걸크?!",
          "original_form": "걸크?!",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008482.1.13",
          "form": "약간 그런 쪽인것같아요",
          "original_form": "약간 그런 쪽인것같아요",
          "speaker_id": "2",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008482.1.14",
          "form": "오오!! 그렇군영",
          "original_form": "오오!! 그렇군영",
          "speaker_id": "1",
          "time": "20210822 16:54"
        },
        {
          "id": "MDRW2100008482.1.15",
          "form": "name1님은 좋아하시는 연예인 있으세요?",
          "original_form": "&name1&님은 좋아하시는 연예인 있으세요?",
          "speaker_id": "2",
          "time": "20210822 21:07"
        },
        {
          "id": "MDRW2100008482.1.16",
          "form": "전 래원이라고 하는 래퍼 좋아해요 ㅋㅋ",
          "original_form": "전 래원이라고 하는 래퍼 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:25"
        },
        {
          "id": "MDRW2100008482.1.17",
          "form": "아 래퍼 좋아하시는군요! 저도 아이돌그룹에서 래퍼포지션인 사람이 좋더라고요 ㅋㅋ",
          "original_form": "아 래퍼 좋아하시는군요! 저도 아이돌그룹에서 래퍼포지션인 사람이 좋더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100008482.1.18",
          "form": "아하 저는 래퍼인지 모르고 좋아햇다가 알고보니 래퍼엿어요 ㅋㅋ",
          "original_form": "아하 저는 래퍼인지 모르고 좋아햇다가 알고보니 래퍼엿어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100008482.1.19",
          "form": "그럴 때 있죠 ㅋㅋㅋㅋ 티비같은데서만 보면 뭐하는 사람인지 모를때가 있어가지고",
          "original_form": "그럴 때 있죠 ㅋㅋㅋㅋ 티비같은데서만 보면 뭐하는 사람인지 모를때가 있어가지고",
          "speaker_id": "2",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100008482.1.20",
          "form": "저는 유튜브에서 게스트로 출연한거 보고 맘에 들어 찾아보니 래퍼엿어요 ㅋㅋ",
          "original_form": "저는 유튜브에서 게스트로 출연한거 보고 맘에 들어 찾아보니 래퍼엿어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:33"
        },
        {
          "id": "MDRW2100008482.1.21",
          "form": "아 그렇군요 ㅋㅋㅋㅋㅋㅋㅋㄱ 유튜브 출연하는 연예인들 엄청 많아진 것 같아요",
          "original_form": "아 그렇군요 ㅋㅋㅋㅋㅋㅋㅋㄱ 유튜브 출연하는 연예인들 엄청 많아진 것 같아요",
          "speaker_id": "2",
          "time": "20210822 21:50"
        },
        {
          "id": "MDRW2100008482.1.22",
          "form": "맞아여 ㅋㅋㅋ 요즘 티비랑 유튜브의 경계가 많이 낮아진거같아요",
          "original_form": "맞아여 ㅋㅋㅋ 요즘 티비랑 유튜브의 경계가 많이 낮아진거같아요",
