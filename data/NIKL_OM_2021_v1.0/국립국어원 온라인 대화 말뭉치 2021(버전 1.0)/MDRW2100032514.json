      "utterance": [
        {
          "id": "MDRW2100032514.1.1",
          "form": "근데 밥먹고 걷는게 진짜 좋다던데",
          "original_form": "근데 밥먹고 걷는게 진짜 좋다던데",
          "speaker_id": "2",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100032514.1.2",
          "form": "너무 귀찮음",
          "original_form": "너무 귀찮음",
          "speaker_id": "2",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100032514.1.3",
          "form": "엥 진짜?",
          "original_form": "엥 진짜?",
          "speaker_id": "1",
          "time": "20210905 19:58"
        },
        {
          "id": "MDRW2100032514.1.4",
          "form": "밥먹고 운동하면 안좋은거 아니었어?",
          "original_form": "밥먹고 운동하면 안좋은거 아니었어?",
          "speaker_id": "1",
          "time": "20210905 19:58"
        },
        {
          "id": "MDRW2100032514.1.5",
          "form": "공복 유산소가 효과좋다고 알고있었는데ㅋㅋㅋㅋㅋㅋ",
          "original_form": "공복 유산소가 효과좋다고 알고있었는데ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:58"
        },
        {
          "id": "MDRW2100032514.1.6",
          "form": "공복 유산소도 좋은데",
          "original_form": "공복 유산소도 좋은데",
          "speaker_id": "2",
          "time": "20210905 19:58"
        },
        {
          "id": "MDRW2100032514.1.7",
          "form": "밥먹고 20분 이냐에 움직이는게 좋다고 하드라",
          "original_form": "밥먹고 20분 이냐에 움직이는게 좋다고 하드라",
          "speaker_id": "2",
          "time": "20210905 19:58"
        },
        {
          "id": "MDRW2100032514.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032514.1.9",
          "form": "옥키....",
          "original_form": "옥키....",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032514.1.10",
          "form": "뭐 누워있는것보다 안좋은게 어디있겠어...",
          "original_form": "뭐 누워있는것보다 안좋은게 어디있겠어...",
          "speaker_id": "2",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032514.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032514.1.12",
          "form": "ㅇㅈ",
          "original_form": "ㅇㅈ",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032514.1.13",
          "form": "난 밥먹고 바로 눕는데",
          "original_form": "난 밥먹고 바로 눕는데",
          "speaker_id": "2",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032514.1.14",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "2",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032514.1.15",
          "form": "내장들 쏘리~",
          "original_form": "내장들 쏘리~",
          "speaker_id": "2",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032514.1.16",
          "form": "아 맞다 그리고 그",
          "original_form": "아 맞다 그리고 그",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032514.1.17",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032514.1.18",
          "form": "허리에 제일 안좋은 자세가",
          "original_form": "허리에 제일 안좋은 자세가",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032514.1.19",
          "form": "앉아잇는거래ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앉아잇는거래ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032514.1.20",
          "form": "하루종일 앉아잇는 나",
          "original_form": "하루종일 앉아잇는 나",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032514.1.21",
          "form": "뼈맞음..",
          "original_form": "뼈맞음..",
          "speaker_id": "1",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100032514.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100032514.1.23",
          "form": "아니 앉아있는것도 안좋은데",
          "original_form": "아니 앉아있는것도 안좋은데",
          "speaker_id": "2",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100032514.1.24",
          "form": "똑바로 안 앉으니까",
          "original_form": "똑바로 안 앉으니까",
          "speaker_id": "2",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100032514.1.25",
          "form": "얼마나 더 안 좋을까",
          "original_form": "얼마나 더 안 좋을까",
          "speaker_id": "2",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100032514.1.26",
          "form": "맞음ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞음ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100032514.1.27",
          "form": "그 앉아있어도",
          "original_form": "그 앉아있어도",
          "speaker_id": "1",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100032514.1.28",
          "form": "복근에 힘주고 허리펴서 앉는건 괜찮고",
          "original_form": "복근에 힘주고 허리펴서 앉는건 괜찮고",
          "speaker_id": "1",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100032514.1.29",
          "form": "이제부터 누워서 공부 한다",
          "original_form": "이제부터 누워서 공부 한다",
          "speaker_id": "2",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100032514.1.30",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "2",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100032514.1.31",
          "form": "복근에 힘이여...?",
          "original_form": "복근에 힘이여...?",
          "speaker_id": "2",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100032514.1.32",
          "form": "겨우 누워있을 이유를 찾았는데",
          "original_form": "겨우 누워있을 이유를 찾았는데",
          "speaker_id": "2",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100032514.1.33",
          "form": "이렇게 또...",
          "original_form": "이렇게 또...",
          "speaker_id": "2",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100032514.1.34",
          "form": "힘.다.빼고",
          "original_form": "힘.다.빼고",
          "speaker_id": "1",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100032514.1.35",
          "form": "등받이에 기대는자세가 진짜안좋다는데",
          "original_form": "등받이에 기대는자세가 진짜안좋다는데",
          "speaker_id": "1",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100032514.1.36",
          "form": "나 ㄹㅇ 24시간 그자세라서",
          "original_form": "나 ㄹㅇ 24시간 그자세라서",
          "speaker_id": "1",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100032514.1.37",
          "form": "ㅎ.....",
          "original_form": "ㅎ.....",
          "speaker_id": "1",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100032514.1.38",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210905 20:02"
        }
      ]
