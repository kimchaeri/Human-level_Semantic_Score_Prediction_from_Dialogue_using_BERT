      "utterance": [
        {
          "id": "MDRW2100028230.1.1",
          "form": "오늘 D.P. 볼까 고민햇는데 탈영병잡는 얘기래",
          "original_form": "오늘 D.P. 볼까 고민햇는데 탈영병잡는 얘기래",
          "speaker_id": "2",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100028230.1.2",
          "form": "어디에서 볼 수 있는데?",
          "original_form": "어디에서 볼 수 있는데?",
          "speaker_id": "1",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100028230.1.3",
          "form": "넷플릭스에서 지금 시즌 1 6화까지 나왓어",
          "original_form": "넷플릭스에서 지금 시즌 1 6화까지 나왓어",
          "speaker_id": "2",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100028230.1.4",
          "form": "근데 정해인이 이등병인데",
          "original_form": "근데 정해인이 이등병인데",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028230.1.5",
          "form": "원래 D.P. 이등병 안시킨대",
          "original_form": "원래 D.P. 이등병 안시킨대",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028230.1.6",
          "form": "디피가 군인경찰이야?",
          "original_form": "디피가 군인경찰이야?",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028230.1.7",
          "form": "아니 그건 아니고 그냥 군인",
          "original_form": "아니 그건 아니고 그냥 군인",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028230.1.8",
          "form": "특별한 직급인거야?",
          "original_form": "특별한 직급인거야?",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028230.1.9",
          "form": "탈영병 잡는 군무이탈 체포조라고 잇는거래",
          "original_form": "탈영병 잡는 군무이탈 체포조라고 잇는거래",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028230.1.10",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028230.1.11",
          "form": "확실히 이등병 시키긴 좀 그렇다",
          "original_form": "확실히 이등병 시키긴 좀 그렇다",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028230.1.12",
          "form": "나는 이등병이 일등병보다 위인줄 알았는데",
          "original_form": "나는 이등병이 일등병보다 위인줄 알았는데",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028230.1.13",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028230.1.14",
          "form": "나도 그랬어",
          "original_form": "나도 그랬어",
          "speaker_id": "1",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028230.1.15",
          "form": "근데 이병->일병 이더라",
          "original_form": "근데 이병->일병 이더라",
          "speaker_id": "1",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028230.1.16",
          "form": "그러니까 숫자 순서대로인줄 알았는데",
          "original_form": "그러니까 숫자 순서대로인줄 알았는데",
          "speaker_id": "2",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100028230.1.17",
          "form": "가끔 군대 관련된거 볼때마다",
          "original_form": "가끔 군대 관련된거 볼때마다",
          "speaker_id": "1",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100028230.1.18",
          "form": "그 위에 직급들도 헷갈려",
          "original_form": "그 위에 직급들도 헷갈려",
          "speaker_id": "1",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100028230.1.19",
          "form": "쉽게 접하는 분야는 아니니까",
          "original_form": "쉽게 접하는 분야는 아니니까",
          "speaker_id": "2",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100028230.1.20",
          "form": "호기심 생겨서 오늘부터 보려고",
          "original_form": "호기심 생겨서 오늘부터 보려고",
          "speaker_id": "2",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100028230.1.21",
          "form": "보고 재미있으면 말해줘",
          "original_form": "보고 재미있으면 말해줘",
          "speaker_id": "1",
          "time": "20210902 18:35"
        },
        {
