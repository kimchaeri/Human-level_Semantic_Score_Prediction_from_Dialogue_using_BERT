      "utterance": [
        {
          "id": "MDRW2100019160.1.1",
          "form": "런데이 라고 어플이 있는데",
          "original_form": "런데이 라고 어플이 있는데",
          "speaker_id": "2",
          "time": "20210827 16:08"
        },
        {
          "id": "MDRW2100019160.1.2",
          "form": "인터벌로 걷다 달리다 할 수 있게 도와주는 어플이에요",
          "original_form": "인터벌로 걷다 달리다 할 수 있게 도와주는 어플이에요",
          "speaker_id": "2",
          "time": "20210827 16:08"
        },
        {
          "id": "MDRW2100019160.1.3",
          "form": "오 신기하네요",
          "original_form": "오 신기하네요",
          "speaker_id": "1",
          "time": "20210827 18:07"
        },
        {
          "id": "MDRW2100019160.1.4",
          "form": "디이어트 해보신 적 있나요?",
          "original_form": "디이어트 해보신 적 있나요?",
          "speaker_id": "2",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100019160.1.5",
          "form": "네 지금 하고 있어요",
          "original_form": "네 지금 하고 있어요",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019160.1.6",
          "form": "name2님은오?",
          "original_form": "&name2&님은오?",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019160.1.7",
          "form": "저는 두달 전까지 했어요",
          "original_form": "저는 두달 전까지 했어요",
          "speaker_id": "2",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019160.1.8",
          "form": "얼마나 되신건가요?",
          "original_form": "얼마나 되신건가요?",
          "speaker_id": "2",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019160.1.9",
          "form": "제대로한건 한달정도 됐어요",
          "original_form": "제대로한건 한달정도 됐어요",
          "speaker_id": "1",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019160.1.10",
          "form": "할만하신가요? 저는 8개월정도 햇어요",
          "original_form": "할만하신가요? 저는 8개월정도 햇어요",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019160.1.11",
          "form": "오.. 대단하세요",
          "original_form": "오.. 대단하세요",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019160.1.12",
          "form": "틈틈히 14키로 빼고 제대로 빼보려 하고 있어요",
          "original_form": "틈틈히 14키로 빼고 제대로 빼보려 하고 있어요",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019160.1.13",
          "form": "대박 14키로 대단하세요",
          "original_form": "대박 14키로 대단하세요",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019160.1.14",
          "form": "바디프로필도 찍으시나요?",
          "original_form": "바디프로필도 찍으시나요?",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019160.1.15",
          "form": "아직 20키로는 더 빼야해서 그때 도전해보고싶어요",
          "original_form": "아직 20키로는 더 빼야해서 그때 도전해보고싶어요",
          "speaker_id": "1",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019160.1.16",
          "form": "바디프로필 찍어보셨나요?",
          "original_form": "바디프로필 찍어보셨나요?",
          "speaker_id": "1",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019160.1.17",
          "form": "도저히못하겠어서 8개월만에 다이어트 끝냇어요ㅠ",
          "original_form": "도저히못하겠어서 8개월만에 다이어트 끝냇어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019160.1.18",
          "form": "앗ㅜㅠ 그래도 8개월하신거면 대단하세요",
          "original_form": "앗ㅜㅠ 그래도 8개월하신거면 대단하세요",
          "speaker_id": "1",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019160.1.19",
          "form": "한달도 힘든데..",
          "original_form": "한달도 힘든데..",
          "speaker_id": "1",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019160.1.20",
          "form": "나중에는 슬로어터로 가자는 생각으로 한끼만 식단하고 그랫어요",
          "original_form": "나중에는 슬로어터로 가자는 생각으로 한끼만 식단하고 그랫어요",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019160.1.21",
          "form": "일반식시작하니까 끊을수가없더라구요....ㅠ",
          "original_form": "일반식시작하니까 끊을수가없더라구요....ㅠ",
