      "utterance": [
        {
          "id": "MDRW2100038492.1.1",
          "form": "저는 홍시님 유튜브 보는데 재밌어요",
          "original_form": "저는 홍시님 유튜브 보는데 재밌어요",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100038492.1.2",
          "form": "오 그분은 어떤 컨텐츠 방송하세요?",
          "original_form": "오 그분은 어떤 컨텐츠 방송하세요?",
          "speaker_id": "2",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100038492.1.3",
          "form": "먹기만 하시던데요 ㅎㅎ 요리하고",
          "original_form": "먹기만 하시던데요 ㅎㅎ 요리하고",
          "speaker_id": "1",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100038492.1.4",
          "form": "자막이 쫌 웃겨요",
          "original_form": "자막이 쫌 웃겨요",
          "speaker_id": "1",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100038492.1.5",
          "form": "혼잔데 잘 챙겨드시더라구요",
          "original_form": "혼잔데 잘 챙겨드시더라구요",
          "speaker_id": "1",
          "time": "20210902 15:31"
        },
        {
          "id": "MDRW2100038492.1.6",
          "form": "그런분 부럽 ㅋㅋ 뭔가 덥업일치?ㅋㅋ",
          "original_form": "그런분 부럽 ㅋㅋ 뭔가 덥업일치?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100038492.1.7",
          "form": "최근 본 영화는 뭔가요?!",
          "original_form": "최근 본 영화는 뭔가요?!",
          "speaker_id": "1",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100038492.1.8",
          "form": "영화 블랙위도우 봤어요!",
          "original_form": "영화 블랙위도우 봤어요!",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038492.1.9",
          "form": "여성 히어로 멋지더라고요 ㅋㅋ",
          "original_form": "여성 히어로 멋지더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038492.1.10",
          "form": "오 저는 그느낌 원더우먼?",
          "original_form": "오 저는 그느낌 원더우먼?",
          "speaker_id": "1",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100038492.1.11",
          "form": "그거 잼썼어요",
          "original_form": "그거 잼썼어요",
          "speaker_id": "1",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100038492.1.12",
          "form": "그것도 함 봐야겠네요",
          "original_form": "그것도 함 봐야겠네요",
          "speaker_id": "1",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100038492.1.13",
          "form": "샹치는 어떨까요",
          "original_form": "샹치는 어떨까요",
          "speaker_id": "1",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100038492.1.14",
          "form": "샹치? 새로 개봉한 영화제목인가요?",
          "original_form": "샹치? 새로 개봉한 영화제목인가요?",
          "speaker_id": "2",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100038492.1.15",
          "form": "맞아요 마블영화래요",
          "original_form": "맞아요 마블영화래요",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100038492.1.16",
          "form": "오 그렇구나 마블 팬도 많더라고요",
          "original_form": "오 그렇구나 마블 팬도 많더라고요",
          "speaker_id": "2",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100038492.1.17",
          "form": "ㅇㅅㅇ 팬이신가요?",
          "original_form": "ㅇㅅㅇ 팬이신가요?",
          "speaker_id": "2",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100038492.1.18",
          "form": "왠만하면 재밌더라구요",
          "original_form": "왠만하면 재밌더라구요",
          "speaker_id": "1",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100038492.1.19",
          "form": "근데 너무 시리즈가 많긴 하더라구요",
          "original_form": "근데 너무 시리즈가 많긴 하더라구요",
          "speaker_id": "1",
          "time": "20210902 17:29"
        },
        {
          "id": "MDRW2100038492.1.20",
          "form": "저는 엄마랑 같이같는데 다행히 같이 재밌게 보고왔어요",
          "original_form": "저는 엄마랑 같이같는데 다행히 같이 재밌게 보고왔어요",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100038492.1.21",
          "form": "너무복잡하면 안되는데 걱정했거든요",
          "original_form": "너무복잡하면 안되는데 걱정했거든요",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100038492.1.22",
          "form": "다행이에요 ㅎㅎ",
          "original_form": "다행이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:36"
        }
      ]
