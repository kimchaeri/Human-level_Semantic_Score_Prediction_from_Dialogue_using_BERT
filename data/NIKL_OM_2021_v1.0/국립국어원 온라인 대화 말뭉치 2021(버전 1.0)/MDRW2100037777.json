      "utterance": [
        {
          "id": "MDRW2100037777.1.1",
          "form": "곧 저녁먹으러 갑니다",
          "original_form": "곧 저녁먹으러 갑니다",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037777.1.2",
          "form": "카레랑 같이 먹으려고 소세지도 굽고 있어요",
          "original_form": "카레랑 같이 먹으려고 소세지도 굽고 있어요",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037777.1.3",
          "form": "헉 환상의짝꿍",
          "original_form": "헉 환상의짝꿍",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037777.1.4",
          "form": "저는 저녁 아직 못먹었어요 맛있겠네요",
          "original_form": "저는 저녁 아직 못먹었어요 맛있겠네요",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037777.1.5",
          "form": "늦게 드시나요",
          "original_form": "늦게 드시나요",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037777.1.6",
          "form": "이제 금방 먹을거긴한데 할일이 남아서 미루는중입니닷",
          "original_form": "이제 금방 먹을거긴한데 할일이 남아서 미루는중입니닷",
          "speaker_id": "1",
          "time": "20210901 18:20"
        },
        {
          "id": "MDRW2100037777.1.7",
          "form": "요리 자주하세요?",
          "original_form": "요리 자주하세요?",
          "speaker_id": "1",
          "time": "20210901 18:20"
        },
        {
          "id": "MDRW2100037777.1.8",
          "form": "요리를 한다고 하기에는... 대부분이 레토르트라서...ㅋㅋ",
          "original_form": "요리를 한다고 하기에는... 대부분이 레토르트라서...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100037777.1.9",
          "form": "ㅋㅋㅋㅋㅋ그래도 스스로 챙겨드시는게..",
          "original_form": "ㅋㅋㅋㅋㅋ그래도 스스로 챙겨드시는게..",
          "speaker_id": "1",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100037777.1.10",
          "form": "대단하십니다",
          "original_form": "대단하십니다",
          "speaker_id": "1",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100037777.1.11",
          "form": "name1님도 직접 챙겨드세요... 사먹는거 보단 훨 나아요",
          "original_form": "&name1&님도 직접 챙겨드세요... 사먹는거 보단 훨 나아요",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100037777.1.12",
          "form": "그건 그래요 요즘 물가도 너무비싸고",
          "original_form": "그건 그래요 요즘 물가도 너무비싸고",
          "speaker_id": "1",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100037777.1.13",
          "form": "물가는 어쩔 수 없네요...",
          "original_form": "물가는 어쩔 수 없네요...",
          "speaker_id": "2",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100037777.1.14",
          "form": "그렇게 생각하면 사먹는건 어떻게 비교적으로 싼지 모르겠어요",
          "original_form": "그렇게 생각하면 사먹는건 어떻게 비교적으로 싼지 모르겠어요",
          "speaker_id": "2",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100037777.1.15",
          "form": "그르게요 ㅋㅋㅋㅋ 스스로를 삼시세끼 먹여살리는게 쉽지않군여",
          "original_form": "그르게요 ㅋㅋㅋㅋ 스스로를 삼시세끼 먹여살리는게 쉽지않군여",
          "speaker_id": "1",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100037777.1.16",
          "form": "하지만 한국인은 밥심이라 꼭 챙겨먹게 되는게 현실...",
          "original_form": "하지만 한국인은 밥심이라 꼭 챙겨먹게 되는게 현실...",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100037777.1.17",
          "form": "사는게 힘드니 먹는재미라도 맘껏 누려야해요 ㅋㅋㅋㅋ",
          "original_form": "사는게 힘드니 먹는재미라도 맘껏 누려야해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 08:52"
        },
        {
          "id": "MDRW2100037777.1.18",
          "form": "오늘 늦잠자서 아침을 못먹엇어요 ㅋ큐 아침드셧나요",
          "original_form": "오늘 늦잠자서 아침을 못먹엇어요 ㅋ큐 아침드셧나요",
          "speaker_id": "1",
          "time": "20210902 08:53"
        },
        {
          "id": "MDRW2100037777.1.19",
          "form": "허걱ㅠ",
          "original_form": "허걱ㅠ",
          "speaker_id": "2",
          "time": "20210902 08:53"
        },
        {
          "id": "MDRW2100037777.1.20",
          "form": "저는 어제 남은 카레 마저 먹었어요",
          "original_form": "저는 어제 남은 카레 마저 먹었어요",
          "speaker_id": "2",
          "time": "20210902 08:54"
        },
        {
          "id": "MDRW2100037777.1.21",
          "form": "오 카레 다 드셨나요",
          "original_form": "오 카레 다 드셨나요",
          "speaker_id": "1",
          "time": "20210902 08:55"
        },
        {
          "id": "MDRW2100037777.1.22",
          "form": "저는 한번에 한솥씩 해서 되게 오래먹어요",
          "original_form": "저는 한번에 한솥씩 해서 되게 오래먹어요",
          "speaker_id": "1",
          "time": "20210902 08:55"
        },
        {
          "id": "MDRW2100037777.1.23",
          "form": "한솥 했는데 가족들이랑 같이 먹어서",
          "original_form": "한솥 했는데 가족들이랑 같이 먹어서",
          "speaker_id": "2",
          "time": "20210902 08:56"
        },
        {
          "id": "MDRW2100037777.1.24",
          "form": "두끼면 동나요",
          "original_form": "두끼면 동나요",
          "speaker_id": "2",
          "time": "20210902 08:56"
        }
      ]
