      "utterance": [
        {
          "id": "MDRW2100002201.1.1",
          "form": "영양제 먹는거있나",
          "original_form": "영양제 먹는거있나",
          "speaker_id": "2",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002201.1.2",
          "form": "자세도 바르지않아서 허리도 좀 아프다. 너는 어때?",
          "original_form": "자세도 바르지않아서 허리도 좀 아프다. 너는 어때?",
          "speaker_id": "1",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002201.1.3",
          "form": "앞에 보낸거 나쁜말이래..",
          "original_form": "앞에 보낸거 나쁜말이래..",
          "speaker_id": "1",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002201.1.4",
          "form": "나는 비타민젤리 먹고 유산균 가끔!",
          "original_form": "나는 비타민젤리 먹고 유산균 가끔!",
          "speaker_id": "1",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002201.1.5",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002201.1.6",
          "form": "나는 아직 챙겨먹는게 없어",
          "original_form": "나는 아직 챙겨먹는게 없어",
          "speaker_id": "2",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002201.1.7",
          "form": "허리건강 중요하지!!",
          "original_form": "허리건강 중요하지!!",
          "speaker_id": "2",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002201.1.8",
          "form": "오래 앉아있으려면 미리미리 챙겨야해",
          "original_form": "오래 앉아있으려면 미리미리 챙겨야해",
          "speaker_id": "2",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002201.1.9",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002201.1.10",
          "form": "무릎건강도 챙겨야 해",
          "original_form": "무릎건강도 챙겨야 해",
          "speaker_id": "1",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002201.1.11",
          "form": "요새 가끔 무릎도 아프더라",
          "original_form": "요새 가끔 무릎도 아프더라",
          "speaker_id": "1",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002201.1.12",
          "form": "아 진짜 나 예전에 공부할때",
          "original_form": "아 진짜 나 예전에 공부할때",
          "speaker_id": "2",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002201.1.13",
          "form": "엄청 고생했잖아",
          "original_form": "엄청 고생했잖아",
          "speaker_id": "2",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002201.1.14",
          "form": "거북목도 안되게 조심해.",
          "original_form": "거북목도 안되게 조심해.",
          "speaker_id": "1",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002201.1.15",
          "form": "발받침대 쓰고있니",
          "original_form": "발받침대 쓰고있니",
          "speaker_id": "2",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002201.1.16",
          "form": "거북이가 되어 바다로 도ㄹ아가버리면 곤란하잖니.",
          "original_form": "거북이가 되어 바다로 도ㄹ아가버리면 곤란하잖니.",
          "speaker_id": "1",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002201.1.17",
          "form": "ㅠㅠ스트레칭 꼭 하자",
          "original_form": "ㅠㅠ스트레칭 꼭 하자",
          "speaker_id": "2",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002201.1.18",
          "form": "발받침대는 안써. 내 방에서는 책을 밟고있지만 오빠방에서는 책상다리 위에 발을 올리고 있어.",
          "original_form": "발받침대는 안써. 내 방에서는 책을 밟고있지만 오빠방에서는 책상다리 위에 발을 올리고 있어.",
          "speaker_id": "1",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002201.1.19",
          "form": "좋아. 건강을 위해 운동도 해보자.",
          "original_form": "좋아. 건강을 위해 운동도 해보자.",
          "speaker_id": "1",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002201.1.20",
          "form": "나는 다음주부터 운동을 할 거야.",
          "original_form": "나는 다음주부터 운동을 할 거야.",
          "speaker_id": "1",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002201.1.21",
          "form": "아 그럼 그게 발받침대지뭐",
          "original_form": "아 그럼 그게 발받침대지뭐",
          "speaker_id": "2",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002201.1.22",
          "form": "오 어떤운동?",
          "original_form": "오 어떤운동?",
          "speaker_id": "2",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002201.1.23",
          "form": "백신 대비 운동이야.",
          "original_form": "백신 대비 운동이야.",
          "speaker_id": "1",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002201.1.24",
          "form": "링피트지.",
          "original_form": "링피트지.",
          "speaker_id": "1",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002201.1.25",
          "form": "2차 맞기전 대비?",
          "original_form": "2차 맞기전 대비?",
          "speaker_id": "2",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002201.1.26",
          "form": "응 너무 아플까봐",
          "original_form": "응 너무 아플까봐",
          "speaker_id": "1",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002201.1.27",
          "form": "걱정이되네.",
          "original_form": "걱정이되네.",
          "speaker_id": "1",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002201.1.28",
          "form": "근데 건강하면 더 아프다는 소문도 있던데",
          "original_form": "근데 건강하면 더 아프다는 소문도 있던데",
          "speaker_id": "2",
          "time": "20210820 00:29"
        }
      ]
    }
