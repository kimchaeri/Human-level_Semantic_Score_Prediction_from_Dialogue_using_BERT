      "utterance": [
        {
          "id": "MDRW2100036259.1.1",
          "form": "언니 덕분에 잘샀옹 땡큐",
          "original_form": "언니 덕분에 잘샀옹 땡큐{emoji:♡}",
          "speaker_id": "2",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100036259.1.2",
          "form": "무슨 ㅋㅋㅋㅋ",
          "original_form": "무슨 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:57"
        },
        {
          "id": "MDRW2100036259.1.3",
          "form": "딴건또뭐햇어 사람들은 선물한다고 청도마니사더라 ㅋㅋ",
          "original_form": "딴건또뭐햇어 사람들은 선물한다고 청도마니사더라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:57"
        },
        {
          "id": "MDRW2100036259.1.4",
          "form": "엄마한테 아까 떡사려햇다가 안되서 보니 품절이야",
          "original_form": "엄마한테 아까 떡사려햇다가 안되서 보니 품절이야",
          "speaker_id": "1",
          "time": "20210830 20:57"
        },
        {
          "id": "MDRW2100036259.1.5",
          "form": "ㅋㅋ흠 뭐 사야 잘샀다 소문이 나지",
          "original_form": "ㅋㅋ흠 뭐 사야 잘샀다 소문이 나지",
          "speaker_id": "2",
          "time": "20210830 20:57"
        },
        {
          "id": "MDRW2100036259.1.6",
          "form": "내껏도 하나시켜려햇는데 품절이야 하니깐",
          "original_form": "내껏도 하나시켜려햇는데 품절이야 하니깐",
          "speaker_id": "1",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100036259.1.7",
          "form": "아 내꺼는시켯다고 ㅋㅋㅋ이럼 ㅡㅡ ㅋㅋㅋㅋ",
          "original_form": "아 내꺼는시켯다고 ㅋㅋㅋ이럼 ㅡㅡ ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100036259.1.8",
          "form": "언제오냐고...",
          "original_form": "언제오냐고...",
          "speaker_id": "1",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100036259.1.9",
          "form": "응 엄마는 먹을수잇오...",
          "original_form": "응 엄마는 먹을수잇오...",
          "speaker_id": "1",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100036259.1.10",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100036259.1.11",
          "form": "ㅋㅋ엄만 행복!",
          "original_form": "ㅋㅋ엄만 행복!",
          "speaker_id": "2",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100036259.1.12",
          "form": "중간에 메시지가 삭제되엇지만",
          "original_form": "중간에 메시지가 삭제되엇지만",
          "speaker_id": "1",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100036259.1.13",
          "form": "알겟쥬,",
          "original_form": "알겟쥬,",
          "speaker_id": "1",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100036259.1.14",
          "form": "응ㅋㅋㅋ알쥬알쥬",
          "original_form": "응ㅋㅋㅋ알쥬알쥬",
          "speaker_id": "2",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100036259.1.15",
          "form": "아스크림도 내가 9월3일 굼요일픽업으로할께하니",
          "original_form": "아스크림도 내가 9월3일 굼요일픽업으로할께하니",
          "speaker_id": "1",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100036259.1.16",
          "form": "엄마한테 떡도 살 수 있었는데 품절이래 하니",
          "original_form": "엄마한테 떡도 살 수 있었는데 품절이래 하니",
          "speaker_id": "2",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100036259.1.17",
          "form": "왜 더빨리안되? 제일빠른날로..해달라며...",
          "original_form": "왜 더빨리안되? 제일빠른날로..해달라며...",
          "speaker_id": "1",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100036259.1.18",
          "form": "그건 괜찮아~ 쏘쿨",
          "original_form": "그건 괜찮아~ 쏘쿨",
          "speaker_id": "2",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100036259.1.19",
          "form": "ㅋㅋㅋㅋㅋ앜ㅋㅋㅋ그래서 1일로 바꿨나요",
          "original_form": "ㅋㅋㅋㅋㅋ앜ㅋㅋㅋ그래서 1일로 바꿨나요",
          "speaker_id": "2",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100036259.1.20",
          "form": "넵 ㅋㅋㅋㅋ그래서 1일로 예약햇쥬",
          "original_form": "넵 ㅋㅋㅋㅋ그래서 1일로 예약햇쥬",
          "speaker_id": "1",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100036259.1.21",
          "form": "ㅋㅋㅋ울집은 금욜로 했당 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ울집은 금욜로 했당 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100036259.1.22",
          "form": "ㅋㅋㅋ 울엄마 한 3번이면..다먹을텐데..",
          "original_form": "ㅋㅋㅋ 울엄마 한 3번이면..다먹을텐데..",
          "speaker_id": "1",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100036259.1.23",
          "form": "그것도 자제해서일껄...",
          "original_form": "그것도 자제해서일껄...",
          "speaker_id": "1",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100036259.1.24",
          "form": "ㅋㅋㅋ진짜 배라 행사때마다 사드료야겠구만",
          "original_form": "ㅋㅋㅋ진짜 배라 행사때마다 사드료야겠구만",
          "speaker_id": "2",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100036259.1.25",
          "form": "응 ㅋㅋ그래서 파인트 행사할때도 물어봐 ㅋㅋㅋ",
          "original_form": "응 ㅋㅋ그래서 파인트 행사할때도 물어봐 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100036259.1.26",
          "form": "픽업행사같은거잇으면 ㅋㅋㅋ",
          "original_form": "픽업행사같은거잇으면 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100036259.1.27",
          "form": "부모님오셧으~?",
          "original_form": "부모님오셧으~?",
          "speaker_id": "1",
