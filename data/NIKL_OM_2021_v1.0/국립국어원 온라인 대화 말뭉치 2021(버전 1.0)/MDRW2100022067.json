      "utterance": [
        {
          "id": "MDRW2100022067.1.1",
          "form": "안녕하세요오오오",
          "original_form": "안녕하세요오오오",
          "speaker_id": "1",
          "time": "20210829 01:59"
        },
        {
          "id": "MDRW2100022067.1.2",
          "form": "거기도 비 오나요오",
          "original_form": "거기도 비 오나요오",
          "speaker_id": "1",
          "time": "20210829 01:59"
        },
        {
          "id": "MDRW2100022067.1.3",
          "form": "밤이 늦어 다들 주무시나 ㅠㅡㅠ..",
          "original_form": "밤이 늦어 다들 주무시나 ㅠㅡㅠ..",
          "speaker_id": "1",
          "time": "20210829 02:00"
        },
        {
          "id": "MDRW2100022067.1.4",
          "form": "아뇨 그쳤어요",
          "original_form": "아뇨 그쳤어요",
          "speaker_id": "2",
          "time": "20210829 02:00"
        },
        {
          "id": "MDRW2100022067.1.5",
          "form": "지금 비오나요?",
          "original_form": "지금 비오나요?",
          "speaker_id": "2",
          "time": "20210829 02:00"
        },
        {
          "id": "MDRW2100022067.1.6",
          "form": "오 안녕하세요",
          "original_form": "오 안녕하세요",
          "speaker_id": "1",
          "time": "20210829 02:00"
        },
        {
          "id": "MDRW2100022067.1.7",
          "form": "여긴 투둑투둑 떨어지네요 계속",
          "original_form": "여긴 투둑투둑 떨어지네요 계속",
          "speaker_id": "1",
          "time": "20210829 02:00"
        },
        {
          "id": "MDRW2100022067.1.8",
          "form": "시원하고 좋슴다 == ㅎ",
          "original_form": "시원하고 좋슴다 == ㅎ",
          "speaker_id": "1",
          "time": "20210829 02:00"
        },
        {
          "id": "MDRW2100022067.1.9",
          "form": "어디신데요?",
          "original_form": "어디신데요?",
          "speaker_id": "2",
          "time": "20210829 02:00"
        },
        {
          "id": "MDRW2100022067.1.10",
          "form": "서울이에요",
          "original_form": "서울이에요",
          "speaker_id": "1",
          "time": "20210829 02:00"
        },
        {
          "id": "MDRW2100022067.1.11",
          "form": "중부쪽",
          "original_form": "중부쪽",
          "speaker_id": "1",
          "time": "20210829 02:01"
        },
        {
          "id": "MDRW2100022067.1.12",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210829 02:01"
        },
        {
          "id": "MDRW2100022067.1.13",
          "form": "서울 지금 비내리는구나",
          "original_form": "서울 지금 비내리는구나",
          "speaker_id": "2",
          "time": "20210829 02:01"
        },
        {
          "id": "MDRW2100022067.1.14",
          "form": "거긴 비 안 오ㅡ.... 그쳤다고 하셨죠? 거긴 어느쪽이에요?",
          "original_form": "거긴 비 안 오ㅡ.... 그쳤다고 하셨죠? 거긴 어느쪽이에요?",
          "speaker_id": "1",
          "time": "20210829 02:01"
        },
        {
          "id": "MDRW2100022067.1.15",
          "form": "여긴 울산입니다",
          "original_form": "여긴 울산입니다",
          "speaker_id": "2",
          "time": "20210829 02:01"
        },
        {
          "id": "MDRW2100022067.1.16",
          "form": "와 완전 남쪽이구나",
          "original_form": "와 완전 남쪽이구나",
          "speaker_id": "1",
          "time": "20210829 02:01"
        },
        {
          "id": "MDRW2100022067.1.17",
          "form": "오늘 30도까지 올랐었습니다",
          "original_form": "오늘 30도까지 올랐었습니다",
          "speaker_id": "2",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022067.1.18",
          "form": "덥긴했는데",
          "original_form": "덥긴했는데",
          "speaker_id": "2",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022067.1.19",
          "form": "히이익...... 30도...... 여긴 낮에도 별로 안더웠어요;",
          "original_form": "히이익...... 30도...... 여긴 낮에도 별로 안더웠어요;",
          "speaker_id": "1",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022067.1.20",
          "form": "빨래 잘말라서 좋더라고요",
          "original_form": "빨래 잘말라서 좋더라고요",
          "speaker_id": "2",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022067.1.21",
          "form": "빨래는 잘 말라야죠. 빨래는 쨍쨍 내리쬐야죠.",
          "original_form": "빨래는 잘 말라야죠. 빨래는 쨍쨍 내리쬐야죠.",
          "speaker_id": "1",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022067.1.22",
          "form": "비가 와도 저는 건조기 돌리긴 하지만..",
          "original_form": "비가 와도 저는 건조기 돌리긴 하지만..",
          "speaker_id": "1",
          "time": "20210829 02:03"
        },
        {
          "id": "MDRW2100022067.1.23",
          "form": "이불 같은건 그래도 널어서 말려야 해요 흫",
          "original_form": "이불 같은건 그래도 널어서 말려야 해요 흫",
          "speaker_id": "1",
          "time": "20210829 02:03"
        },
        {
          "id": "MDRW2100022067.1.24",
          "form": "저도 수건이랑 티같은건 건조기 돌리는데",
          "original_form": "저도 수건이랑 티같은건 건조기 돌리는데",
          "speaker_id": "2",
          "time": "20210829 02:04"
        },
        {
          "id": "MDRW2100022067.1.25",
          "form": "줄어들 염려가 있는것들은",
          "original_form": "줄어들 염려가 있는것들은",
          "speaker_id": "2",
          "time": "20210829 02:04"
        },
        {
          "id": "MDRW2100022067.1.26",
          "form": "널어서 말려야하니까",
          "original_form": "널어서 말려야하니까",
          "speaker_id": "2",
          "time": "20210829 02:04"
        }
      ]
    }
  ]
