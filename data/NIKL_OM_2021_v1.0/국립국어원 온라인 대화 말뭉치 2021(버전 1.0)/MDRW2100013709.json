      "utterance": [
        {
          "id": "MDRW2100013709.1.1",
          "form": "최근에 산 물건이 뭐예요?",
          "original_form": "최근에 산 물건이 뭐예요?",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013709.1.2",
          "form": "저 하이닉스요.",
          "original_form": "저 하이닉스요.",
          "speaker_id": "1",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013709.1.3",
          "form": "핫싀...",
          "original_form": "핫싀...",
          "speaker_id": "1",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013709.1.4",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013709.1.5",
          "form": "하이닉스 혹시 꼭대기에서 사셨...",
          "original_form": "하이닉스 혹시 꼭대기에서 사셨...",
          "speaker_id": "2",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013709.1.6",
          "form": "앗 아뇨아뇨 10만 3000원에",
          "original_form": "앗 아뇨아뇨 10만 3000원에",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013709.1.7",
          "form": "아나 3500원인가",
          "original_form": "아나 3500원인가",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013709.1.8",
          "form": "암튼 그렇습니더",
          "original_form": "암튼 그렇습니더",
          "speaker_id": "1",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013709.1.9",
          "form": "name2님은 최근에 뭐사셨나요?",
          "original_form": "&name2&님은 최근에 뭐사셨나요?",
          "speaker_id": "1",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013709.1.10",
          "form": "저는 snp 500이요",
          "original_form": "저는 snp 500이요",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013709.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013709.1.12",
          "form": "아나 좋은거 사셨넼ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아나 좋은거 사셨넼ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013709.1.13",
          "form": "원래 꾸준히 투자하고 계셨어요?!",
          "original_form": "원래 꾸준히 투자하고 계셨어요?!",
          "speaker_id": "1",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013709.1.14",
          "form": "달러도?",
          "original_form": "달러도?",
          "speaker_id": "1",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013709.1.15",
          "form": "아뇨ㅋㅋㅋ 올해 처음 해봤는데",
          "original_form": "아뇨ㅋㅋㅋ 올해 처음 해봤는데",
          "speaker_id": "2",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013709.1.16",
          "form": "너무 오르니까 괜히 손해본느낌",
          "original_form": "너무 오르니까 괜히 손해본느낌",
          "speaker_id": "2",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013709.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013709.1.18",
          "form": "근데 그거는 암만 떨어져도 우상향하는 것 같더라고요",
          "original_form": "근데 그거는 암만 떨어져도 우상향하는 것 같더라고요",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013709.1.19",
          "form": "ㅡ저는 그 어도비...",
          "original_form": "ㅡ저는 그 어도비...",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013709.1.20",
          "form": "어도비 차트 보셨어요? 완전 피뢰침이더라고요",
          "original_form": "어도비 차트 보셨어요? 완전 피뢰침이더라고요",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013709.1.21",
          "form": "혹시 그 피뢰침이 내리꽂기 전에 사셨나요",
          "original_form": "혹시 그 피뢰침이 내리꽂기 전에 사셨나요",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013709.1.22",
          "form": "쓰읍..하나요 하나",
          "original_form": "쓰읍..하나요 하나",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013709.1.23",
          "form": "그 정찰병으로 보내는 거 있잖아요",
          "original_form": "그 정찰병으로 보내는 거 있잖아요",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013709.1.24",
          "form": "항상 그런애들이 피뢰침 되더라고요",
          "original_form": "항상 그런애들이 피뢰침 되더라고요",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013709.1.25",
          "form": "맨날 후회하는 것 : 더 살걸",
          "original_form": "맨날 후회하는 것 : 더 살걸",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013709.1.26",
          "form": "아 진짜 주식은 질병입니다",
          "original_form": "아 진짜 주식은 질병입니다",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013709.1.27",
          "form": "오른다: 더 못사서 후회",
          "original_form": "오른다: 더 못사서 후회",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013709.1.28",
          "form": "내림: 왜 샀을까",
          "original_form": "내림: 왜 샀을까",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013709.1.29",
          "form": "결국 후회",
          "original_form": "결국 후회",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013709.1.30",
          "form": "ㅇㄴ 결국 후회만이 남는군욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㄴ 결국 후회만이 남는군욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100013709.1.31",
          "form": "근데 진짜 작년..? 부터 주식 시작하는 사람들이 많더라고요 저도 물론 작년~이번년도 사이에 시작했지만요",
          "original_form": "근데 진짜 작년..? 부터 주식 시작하는 사람들이 많더라고요 저도 물론 작년~이번년도 사이에 시작했지만요",
