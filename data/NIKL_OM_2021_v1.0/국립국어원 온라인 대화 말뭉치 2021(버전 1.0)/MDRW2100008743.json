      "utterance": [
        {
          "id": "MDRW2100008743.1.1",
          "form": "최애 음식은?",
          "original_form": "최애 음식은?",
          "speaker_id": "2",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008743.1.2",
          "form": "저는 피자 좋아해요",
          "original_form": "저는 피자 좋아해요",
          "speaker_id": "2",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008743.1.3",
          "form": "빵도요 ㅎㅎ",
          "original_form": "빵도요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008743.1.4",
          "form": "전 고기가 최애음식ㅎ",
          "original_form": "전 고기가 최애음식ㅎ",
          "speaker_id": "1",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008743.1.5",
          "form": "저녁은 뭐드실거예요?",
          "original_form": "저녁은 뭐드실거예요?",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008743.1.6",
          "form": "저는 낮에 엄청 먹어서ㅜ",
          "original_form": "저는 낮에 엄청 먹어서ㅜ",
          "speaker_id": "2",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100008743.1.7",
          "form": "안먹으려고요",
          "original_form": "안먹으려고요",
          "speaker_id": "2",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100008743.1.8",
          "form": "그래도 식사는 하셔야죠^^",
          "original_form": "그래도 식사는 하셔야죠^^",
          "speaker_id": "1",
          "time": "20210822 18:12"
        },
        {
          "id": "MDRW2100008743.1.9",
          "form": "전 좀전에 먹어서 아스크림이나 먹을까봐요",
          "original_form": "전 좀전에 먹어서 아스크림이나 먹을까봐요",
          "speaker_id": "1",
          "time": "20210822 18:12"
        },
        {
          "id": "MDRW2100008743.1.10",
          "form": "방금전까지 먹었어요",
          "original_form": "방금전까지 먹었어요",
          "speaker_id": "2",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008743.1.11",
          "form": "저도 아이스크림 먹었어요",
          "original_form": "저도 아이스크림 먹었어요",
          "speaker_id": "2",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008743.1.12",
          "form": "옥동자 ㅋㅋ",
          "original_form": "옥동자 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008743.1.13",
          "form": "옥동자 맛있죠ㅎ",
          "original_form": "옥동자 맛있죠ㅎ",
          "speaker_id": "1",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008743.1.14",
          "form": "전 배스킨라빈스 사다놓은거있어서",
          "original_form": "전 배스킨라빈스 사다놓은거있어서",
          "speaker_id": "1",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008743.1.15",
          "form": "집앞에 무인 아이스크림 가게가 생겨서 또 한동안 마니 사먹었네여ㅜ",
          "original_form": "집앞에 무인 아이스크림 가게가 생겨서 또 한동안 마니 사먹었네여ㅜ",
          "speaker_id": "2",
          "time": "20210822 18:30"
        },
        {
          "id": "MDRW2100008743.1.16",
          "form": "무인아이스크림도 있어요?ㅎ",
          "original_form": "무인아이스크림도 있어요?ㅎ",
          "speaker_id": "1",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008743.1.17",
          "form": "직접사고 계산하는거예요?",
          "original_form": "직접사고 계산하는거예요?",
          "speaker_id": "1",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008743.1.18",
          "form": "네 그 홈플러스  무인계산대 처럼요",
          "original_form": "네 그 홈플러스  무인계산대 처럼요",
          "speaker_id": "2",
          "time": "20210822 18:37"
        },
        {
          "id": "MDRW2100008743.1.19",
          "form": "아 좋네요ㅎ 무슨아스크림이예요?",
          "original_form": "아 좋네요ㅎ 무슨아스크림이예요?",
          "speaker_id": "1",
          "time": "20210822 18:41"
        },
        {
          "id": "MDRW2100008743.1.20",
          "form": "콘은 아닐거같고",
          "original_form": "콘은 아닐거같고",
          "speaker_id": "1",
          "time": "20210822 18:41"
        },
        {
          "id": "MDRW2100008743.1.21",
          "form": "전문점은 뭐 웬만한 아이스크림은 다있어요",
          "original_form": "전문점은 뭐 웬만한 아이스크림은 다있어요",
          "speaker_id": "2",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008743.1.22",
          "form": "막대바 400원부터",
          "original_form": "막대바 400원부터",
          "speaker_id": "2",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008743.1.23",
          "form": "오 싸네요ㅎㅎ 전 빵또아 좋아하는데",
          "original_form": "오 싸네요ㅎㅎ 전 빵또아 좋아하는데",
          "speaker_id": "1",
          "time": "20210822 18:52"
        },
        {
          "id": "MDRW2100008743.1.24",
          "form": "계산안되면 계좌이체해도되고 ㅋㅋ",
          "original_form": "계산안되면 계좌이체해도되고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:56"
        },
        {
          "id": "MDRW2100008743.1.25",
          "form": "꼭 지나갈때마다 지갑열게됨 ㅋㅋ",
          "original_form": "꼭 지나갈때마다 지갑열게됨 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:56"
        },
        {
          "id": "MDRW2100008743.1.26",
          "form": "저녁은 그래서 안드시는거예요?ㅎ 그러다 야식드실텐데",
          "original_form": "저녁은 그래서 안드시는거예요?ㅎ 그러다 야식드실텐데",
          "speaker_id": "1",
          "time": "20210822 18:57"
        },
        {
          "id": "MDRW2100008743.1.27",
          "form": "지금 그래서 아아 하나샀어요 ㅋㄲ",
          "original_form": "지금 그래서 아아 하나샀어요 ㅋㄲ",
          "speaker_id": "2",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008743.1.28",
          "form": "빽다방 ㅋㅋ",
          "original_form": "빽다방 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008743.1.29",
          "form": "아아 좋아하시는군요ㅎ",
          "original_form": "아아 좋아하시는군요ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:04"
        },
        {
          "id": "MDRW2100008743.1.30",
          "form": "디저트도 하나 드시지ㅋ",
          "original_form": "디저트도 하나 드시지ㅋ",
          "speaker_id": "1",
          "time": "20210822 19:04"
        },
        {
          "id": "MDRW2100008743.1.31",
          "form": "케익먹고싶네요",
          "original_form": "케익먹고싶네요",
          "speaker_id": "1",
          "time": "20210822 19:04"
        },
        {
          "id": "MDRW2100008743.1.32",
          "form": "오늘 아이스크림사면서 과자도사서 5봉지나 먹엏어요",
          "original_form": "오늘 아이스크림사면서 과자도사서 5봉지나 먹엏어요",
          "speaker_id": "2",
          "time": "20210822 19:11"
        },
        {
          "id": "MDRW2100008743.1.33",
          "form": "먹었어요 ㅋㅋ",
          "original_form": "먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008743.1.34",
          "form": "와 쌀\r\n간식만 얼마나먹은건지",
          "original_form": "와 쌀\r\n간식만 얼마나먹은건지",
          "speaker_id": "2",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008743.1.35",
          "form": "헉 5봉지요?ㅋ",
          "original_form": "헉 5봉지요?ㅋ",
          "speaker_id": "1",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008743.1.36",
          "form": "싸다고",
          "original_form": "싸다고",
          "speaker_id": "2",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008743.1.37",
          "form": "아까 저녁안드신다면서요",
          "original_form": "아까 저녁안드신다면서요",
          "speaker_id": "1",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008743.1.38",
          "form": "샀는데",
          "original_form": "샀는데",
          "speaker_id": "2",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008743.1.39",
          "form": "밥은 아니니 저녁은 아니네요",
          "original_form": "밥은 아니니 저녁은 아니네요",
          "speaker_id": "1",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008743.1.40",
          "form": "세계과자 라서 싸서요",
          "original_form": "세계과자 라서 싸서요",
          "speaker_id": "2",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008743.1.41",
          "form": "ㅋㅋㅋ직전까지 드라마보면서 하나씩 까먹다보니",
          "original_form": "ㅋㅋㅋ직전까지 드라마보면서 하나씩 까먹다보니",
