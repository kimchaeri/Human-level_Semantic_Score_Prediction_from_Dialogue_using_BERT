      "utterance": [
        {
          "id": "MDRW2100022593.1.1",
          "form": "커뮤니티에서 빌런이라 부르는거",
          "original_form": "커뮤니티에서 빌런이라 부르는거",
          "speaker_id": "1",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100022593.1.2",
          "form": "뭔지 아세요?",
          "original_form": "뭔지 아세요?",
          "speaker_id": "1",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100022593.1.3",
          "form": "영화에서 부르는 빌런이랑 다른 의미인가요??",
          "original_form": "영화에서 부르는 빌런이랑 다른 의미인가요??",
          "speaker_id": "2",
          "time": "20210829 16:28"
        },
        {
          "id": "MDRW2100022593.1.4",
          "form": "같은 의미일걸요",
          "original_form": "같은 의미일걸요",
          "speaker_id": "1",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022593.1.5",
          "form": "약간 사회성 떨어지고 관종짓에",
          "original_form": "약간 사회성 떨어지고 관종짓에",
          "speaker_id": "1",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022593.1.6",
          "form": "등등 하는 사람요",
          "original_form": "등등 하는 사람요",
          "speaker_id": "1",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022593.1.7",
          "form": "어그로를 커뮤니티에서는 빌런이라 부르나 보네요 ㅎ",
          "original_form": "어그로를 커뮤니티에서는 빌런이라 부르나 보네요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100022593.1.8",
          "form": "네네 어그로요",
          "original_form": "네네 어그로요",
          "speaker_id": "1",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100022593.1.9",
          "form": "그게 커뮤니티 용어였나요?",
          "original_form": "그게 커뮤니티 용어였나요?",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022593.1.10",
          "form": "뉴스에서 많이 봐서 그냥 새로운 말인줄 알았어요",
          "original_form": "뉴스에서 많이 봐서 그냥 새로운 말인줄 알았어요",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022593.1.11",
          "form": "어그로요 빌런이요?",
          "original_form": "어그로요 빌런이요?",
          "speaker_id": "1",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022593.1.12",
          "form": "어그로요",
          "original_form": "어그로요",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022593.1.13",
          "form": "저도 잘 모르겠어요",
          "original_form": "저도 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022593.1.14",
          "form": "아 아냐고 물어보셔서 잘 아시나했어요",
          "original_form": "아 아냐고 물어보셔서 잘 아시나했어요",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022593.1.15",
          "form": "온라인이랑 실제사회랑",
          "original_form": "온라인이랑 실제사회랑",
          "speaker_id": "1",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022593.1.16",
          "form": "비슷헤보이기도해요",
          "original_form": "비슷헤보이기도해요",
          "speaker_id": "1",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022593.1.17",
          "form": "온라인 활동은 하는게 없어서 잘 모르겠어요",
