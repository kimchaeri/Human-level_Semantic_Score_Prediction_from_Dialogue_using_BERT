      "utterance": [
        {
          "id": "MDRW2100008869.1.1",
          "form": "식사는하셨나요?",
          "original_form": "식사는하셨나요?",
          "speaker_id": "1",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008869.1.2",
          "form": "네 초밥먹었어요 저녁 드셨어요?",
          "original_form": "네 초밥먹었어요 저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008869.1.3",
          "form": "저희는 아직인데ㅎㅎ 저는배불러서 패스하고",
          "original_form": "저희는 아직인데ㅎㅎ 저는배불러서 패스하고",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008869.1.4",
          "form": "다른식구들은 고기볶아주려구요",
          "original_form": "다른식구들은 고기볶아주려구요",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008869.1.5",
          "form": "아 맛있겠네요ㅠㅠ 꼬기",
          "original_form": "아 맛있겠네요ㅠㅠ 꼬기",
          "speaker_id": "2",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008869.1.6",
          "form": "근데 어디서 삼겹살을 굽는지 ㅠㅠ 삼겹냄새가나요ㅜㅋㅋ",
          "original_form": "근데 어디서 삼겹살을 굽는지 ㅠㅠ 삼겹냄새가나요ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008869.1.7",
          "form": "삼겹살이 더땡기는데 일단없으니 ㅠ 주물럭고기라도ㅜ",
          "original_form": "삼겹살이 더땡기는데 일단없으니 ㅠ 주물럭고기라도ㅜ",
          "speaker_id": "1",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008869.1.8",
          "form": "삼겹살ㅠㅠㅠㅠ배가 부른거 같았는데 아녔나봐요ㅎㅎ",
          "original_form": "삼겹살ㅠㅠㅠㅠ배가 부른거 같았는데 아녔나봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008869.1.9",
          "form": "ㅋㅋㄱㅋ 배는부르지만ㅋㅋ삼겹살은 ㅋㅋ 포기못해요",
          "original_form": "ㅋㅋㄱㅋ 배는부르지만ㅋㅋ삼겹살은 ㅋㅋ 포기못해요",
          "speaker_id": "1",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008869.1.10",
          "form": "근데없으니 ㅋㅋ냄새만 맡겠습니다ㅋ",
          "original_form": "근데없으니 ㅋㅋ냄새만 맡겠습니다ㅋ",
          "speaker_id": "1",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008869.1.11",
          "form": "저는 낼 먹어야겠네요ㅜㅜ 근데 이러다 배달시킬수도ㅋㅋ쿠ㅠㅠ",
          "original_form": "저는 낼 먹어야겠네요ㅜㅜ 근데 이러다 배달시킬수도ㅋㅋ쿠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008869.1.12",
          "form": "삼겹이요,ㅎㅎ?",
          "original_form": "삼겹이요,ㅎㅎ?",
          "speaker_id": "1",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008869.1.13",
          "form": "네 말들으니 너무 먹고싶네요ㅎㅎ",
          "original_form": "네 말들으니 너무 먹고싶네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008869.1.14",
          "form": "아 저도 조만간 삼겹을 먹어야ㅋㄱ",
          "original_form": "아 저도 조만간 삼겹을 먹어야ㅋㄱ",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008869.1.15",
          "form": "삼겹살은 먹어도 먹어도 안질려서ㅎㅎ",
          "original_form": "삼겹살은 먹어도 먹어도 안질려서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008869.1.16",
          "form": "삼겹살은 질릴수가 없어요",
          "original_form": "삼겹살은 질릴수가 없어요",
          "speaker_id": "1",
          "time": "20210822 23:02"
        }
