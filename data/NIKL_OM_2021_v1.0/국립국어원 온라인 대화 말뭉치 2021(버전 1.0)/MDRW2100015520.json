      "utterance": [
        {
          "id": "MDRW2100015520.1.1",
          "form": "혹시 무슨 직업 가지고 계세요?",
          "original_form": "혹시 무슨 직업 가지고 계세요?",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015520.1.2",
          "form": "언론사에서 일해요",
          "original_form": "언론사에서 일해요",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015520.1.3",
          "form": "name1님도 직장인이신가요",
          "original_form": "&name1&님도 직장인이신가요",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015520.1.4",
          "form": "와 멋지네요",
          "original_form": "와 멋지네요",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015520.1.5",
          "form": "네 직장인이예요",
          "original_form": "네 직장인이예요",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015520.1.6",
          "form": "몇년차세요?",
          "original_form": "몇년차세요?",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015520.1.7",
          "form": "6년정도 된거같아요",
          "original_form": "6년정도 된거같아요",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015520.1.8",
          "form": "오 한직장에 계속 계셨나요?",
          "original_form": "오 한직장에 계속 계셨나요?",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015520.1.9",
          "form": "아뇨 이리저리 세군데요",
          "original_form": "아뇨 이리저리 세군데요",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015520.1.10",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015520.1.11",
          "form": "두번째  직장이고 십년이 넘었네요",
          "original_form": "두번째  직장이고 십년이 넘었네요",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015520.1.12",
          "form": "이직 생각은 있으신가요",
          "original_form": "이직 생각은 있으신가요",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015520.1.13",
          "form": "이직 생각은 항상 마음에 품고살아요 ㅋㅋㅋ",
          "original_form": "이직 생각은 항상 마음에 품고살아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015520.1.14",
          "form": "십년인데 두번째라니",
          "original_form": "십년인데 두번째라니",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015520.1.15",
          "form": "좋은곳인가봐요",
          "original_form": "좋은곳인가봐요",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015520.1.16",
          "form": "사표를 가슴에 품고 살지요",
          "original_form": "사표를 가슴에 품고 살지요",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015520.1.17",
          "form": "게을러서 못 옮기는 거죠",
          "original_form": "게을러서 못 옮기는 거죠",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015520.1.18",
          "form": "사람 다 똑같아요ㅜㅜㅜ",
          "original_form": "사람 다 똑같아요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015520.1.19",
          "form": "그쵸 그 이직과정이 너무싫어요",
          "original_form": "그쵸 그 이직과정이 너무싫어요",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015520.1.20",
          "form": "적응할 것도 염려되고요",
          "original_form": "적응할 것도 염려되고요",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015520.1.21",
          "form": "맞아요ㅜㅜㅜ",
          "original_form": "맞아요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:29"
        }
      ]
