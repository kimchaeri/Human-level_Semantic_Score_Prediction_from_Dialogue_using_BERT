      "utterance": [
        {
          "id": "MDRW2100008533.1.1",
          "form": "잘 넘겼어요 언니 ㅋㅋ",
          "original_form": "잘 넘겼어요 언니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:26"
        },
        {
          "id": "MDRW2100008533.1.2",
          "form": "쇼핑은 쿠팡이져 ㅋㅋ",
          "original_form": "쇼핑은 쿠팡이져 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008533.1.3",
          "form": "요즘 핫딜이.잘 없어가 ㅋㅋ",
          "original_form": "요즘 핫딜이.잘 없어가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008533.1.4",
          "form": "위메프 포인트 환급도 없고",
          "original_form": "위메프 포인트 환급도 없고",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008533.1.5",
          "form": "슬프구만요~",
          "original_form": "슬프구만요~",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008533.1.6",
          "form": "나도 쿠팡와우",
          "original_form": "나도 쿠팡와우",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008533.1.7",
          "form": "요즘쿠팡만 주구장창",
          "original_form": "요즘쿠팡만 주구장창",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008533.1.8",
          "form": "매일 택배오는듯;;",
          "original_form": "매일 택배오는듯;;",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008533.1.9",
          "form": "쿠팡시키다 보니 다른택밴 기다리기 싫어가 안시키게됨 ㅋㅋㅋ",
          "original_form": "쿠팡시키다 보니 다른택밴 기다리기 싫어가 안시키게됨 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100008533.1.10",
          "form": "올리브영 세일허길래 싸다고 샀디 쿠팡이 훨 싸고~",
          "original_form": "올리브영 세일허길래 싸다고 샀디 쿠팡이 훨 싸고~",
          "speaker_id": "2",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100008533.1.11",
          "form": "ㅋㅋ 나도 로켓배송 땜에 못끊는다",
          "original_form": "ㅋㅋ 나도 로켓배송 땜에 못끊는다",
          "speaker_id": "1",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008533.1.12",
          "form": "2900원이 아깝지않다",
          "original_form": "2900원이 아깝지않다",
          "speaker_id": "1",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008533.1.13",
          "form": "맞죠?? 싸기도 엄청싸요~",
          "original_form": "맞죠?? 싸기도 엄청싸요~",
          "speaker_id": "2",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008533.1.14",
          "form": "양말하고 우유하고 시켜놨는데 ㅋㅋ",
          "original_form": "양말하고 우유하고 시켜놨는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008533.1.15",
          "form": "양말 늠 이쁨 ㅋㅋㅋ",
          "original_form": "양말 늠 이쁨 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008533.1.16",
          "form": "나도 양말사야되는데ㅋㅋ",
          "original_form": "나도 양말사야되는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008533.1.17",
          "form": "양말나라보더 쿠팡이 나아여 ㅋㅋㅋㅋ",
          "original_form": "양말나라보더 쿠팡이 나아여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008533.1.18",
          "form": "한번 빠니깐 고므 다터지고 ㅋㅋㅋ",
          "original_form": "한번 빠니깐 고므 다터지고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008533.1.19",
          "form": "가을맞이 꽃양말로~",
          "original_form": "가을맞이 꽃양말로~",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008533.1.20",
          "form": "나도 다터지가 사야됨ㅋㅋ",
          "original_form": "나도 다터지가 사야됨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008533.1.21",
          "form": "오늘 쇼핑은 양말ㅋ",
          "original_form": "오늘 쇼핑은 양말ㅋ",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008533.1.22",
          "form": "작은걸 사도 샤핑은 행복한것 ㅋㅋㅋ",
          "original_form": "작은걸 사도 샤핑은 행복한것 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008533.1.23",
          "form": "매일의 행복을위해 쇼핑",
          "original_form": "매일의 행복을위해 쇼핑",
          "speaker_id": "1",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008533.1.24",
          "form": "쌀도떨어짐",
          "original_form": "쌀도떨어짐",
          "speaker_id": "1",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008533.1.25",
          "form": "쌀 샀나?",
          "original_form": "쌀 샀나?",
          "speaker_id": "1",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008533.1.26",
          "form": "쌀 그때 하도 많이 사서 ㅋㅋㅋㅋ",
          "original_form": "쌀 그때 하도 많이 사서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:51"
        },
        {
          "id": "MDRW2100008533.1.27",
          "form": "쌀 이제 우리도 사긴사야하는데~",
          "original_form": "쌀 이제 우리도 사긴사야하는데~",
          "speaker_id": "2",
          "time": "20210822 19:51"
        },
        {
          "id": "MDRW2100008533.1.28",
          "form": "핫딜이 늠 뜸하군여 ㅋㅋㅋ",
          "original_form": "핫딜이 늠 뜸하군여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:51"
        },
        {
          "id": "MDRW2100008533.1.29",
          "form": "난 똑 떨어짐",
          "original_form": "난 똑 떨어짐",
          "speaker_id": "1",
          "time": "20210822 19:51"
        },
        {
          "id": "MDRW2100008533.1.30",
          "form": "당장 봐야겠다",
          "original_form": "당장 봐야겠다",
          "speaker_id": "1",
          "time": "20210822 19:51"
        },
        {
          "id": "MDRW2100008533.1.31",
          "form": "할인 안하면 속쓰린데",
          "original_form": "할인 안하면 속쓰린데",
          "speaker_id": "1",
          "time": "20210822 19:55"
        },
        {
          "id": "MDRW2100008533.1.32",
          "form": "아하~근데 은근 오케이가 쌀 싸던데여??",
          "original_form": "아하~근데 은근 오케이가 쌀 싸던데여??",
          "speaker_id": "2",
          "time": "20210822 19:57"
        },
        {
          "id": "MDRW2100008533.1.33",
          "form": "행사도 종종하고~",
          "original_form": "행사도 종종하고~",
          "speaker_id": "2",
          "time": "20210822 19:57"
        },
        {
          "id": "MDRW2100008533.1.34",
          "form": "오케이가봐야긋네 내일",
          "original_form": "오케이가봐야긋네 내일",
          "speaker_id": "1",
          "time": "20210822 20:02"
        }
      ]
