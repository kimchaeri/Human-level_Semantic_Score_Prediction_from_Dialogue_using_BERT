      "utterance": [
        {
          "id": "MDRW2100039413.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039413.1.2",
          "form": "안녕하세요!!!",
          "original_form": "안녕하세요!!!",
          "speaker_id": "2",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039413.1.3",
          "form": "학교!!! 저는 지금 강의듣고 있어요!!! 교수님 tmi가 너무 심해요 살려주세요ㅜ",
          "original_form": "학교!!! 저는 지금 강의듣고 있어요!!! 교수님 tmi가 너무 심해요 살려주세요ㅜ",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039413.1.4",
          "form": "저는 오늘 교수님이 줌링크를 안올려주셨는데 알고본이 교수님이 자체휴강을 하셨더라고요",
          "original_form": "저는 오늘 교수님이 줌링크를 안올려주셨는데 알고본이 교수님이 자체휴강을 하셨더라고요",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039413.1.5",
          "form": "뭐에 대한 수업 듣고 계세요?",
          "original_form": "뭐에 대한 수업 듣고 계세요?",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039413.1.6",
          "form": "교수님이 자체휴강을요?????",
          "original_form": "교수님이 자체휴강을요?????",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039413.1.7",
          "form": "저는 다문화관련 수업이요! 교양이에요",
          "original_form": "저는 다문화관련 수업이요! 교양이에요",
          "speaker_id": "2",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100039413.1.8",
          "form": "아니 30분이 되어도 공지없어서 전화했더니 휴강이라하셧대요",
          "original_form": "아니 30분이 되어도 공지없어서 전화했더니 휴강이라하셧대요",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039413.1.9",
          "form": "교양수업! 새내기이신가요",
          "original_form": "교양수업! 새내기이신가요",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039413.1.10",
          "form": "진짜 교수님 혼나야겠네요 무슨 그런 경우가 있대요...?",
          "original_form": "진짜 교수님 혼나야겠네요 무슨 그런 경우가 있대요...?",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039413.1.11",
          "form": "아니요 저 2학년이에요!",
          "original_form": "아니요 저 2학년이에요!",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039413.1.12",
          "form": "진짜 어이없죠? 심지어 오늘이 첫수업인 전필이었는데",
          "original_form": "진짜 어이없죠? 심지어 오늘이 첫수업인 전필이었는데",
          "speaker_id": "1",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100039413.1.13",
          "form": "저도 2학년이에요!",
          "original_form": "저도 2학년이에요!",
          "speaker_id": "1",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100039413.1.14",
          "form": "헐 전필을ㅋㅋ전필 교수님 왜 그러시는거죠ㅜㅜㅜ",
          "original_form": "헐 전필을ㅋㅋ전필 교수님 왜 그러시는거죠ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039413.1.15",
          "form": "저는 편입하려고 학점 모으는 중이에요!",
          "original_form": "저는 편입하려고 학점 모으는 중이에요!",
          "speaker_id": "2",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039413.1.16",
          "form": "그니까요 휴",
          "original_form": "그니까요 휴",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039413.1.17",
          "form": "저는 6년제라서 이제 4년반 남았네요",
          "original_form": "저는 6년제라서 이제 4년반 남았네요",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039413.1.18",
          "form": "6년제요????헉...",
          "original_form": "6년제요????헉...",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039413.1.19",
          "form": "졸업이 까마득하게 느껴져요 아직도 고등학생같구",
          "original_form": "졸업이 까마득하게 느껴져요 아직도 고등학생같구",
          "speaker_id": "1",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039413.1.20",
          "form": "대면수업 해보셨어요?",
          "original_form": "대면수업 해보셨어요?",
          "speaker_id": "1",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039413.1.21",
          "form": "전공 대면이에요!",
          "original_form": "전공 대면이에요!",
          "speaker_id": "2",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039413.1.22",
          "form": "지옥! 최악!",
          "original_form": "지옥! 최악!",
          "speaker_id": "2",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039413.1.23",
          "form": "앜ㅋㅋㅋ 많이 힘든 수업인가봐요",
          "original_form": "앜ㅋㅋㅋ 많이 힘든 수업인가봐요",
          "speaker_id": "1",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039413.1.24",
          "form": "저는 대면 한번밖에 못해봤어요",
          "original_form": "저는 대면 한번밖에 못해봤어요",
          "speaker_id": "1",
          "time": "20210903 18:03"
        },
        {
          "id": "MDRW2100039413.1.25",
          "form": "골프 교양수업이었는데.. 좋앗죠",
          "original_form": "골프 교양수업이었는데.. 좋앗죠",
          "speaker_id": "1",
          "time": "20210903 18:03"
        },
        {
          "id": "MDRW2100039413.1.26",
          "form": "네ㅜㅜ 전공이 예체능쪽이라",
          "original_form": "네ㅜㅜ 전공이 예체능쪽이라",
          "speaker_id": "2",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039413.1.27",
          "form": "골프... 저는 거기 청소하러 갔었어요ㅡㅜ",
          "original_form": "골프... 저는 거기 청소하러 갔었어요ㅡㅜ",
          "speaker_id": "2",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039413.1.28",
          "form": "헉 청소... 그렇구나...",
          "original_form": "헉 청소... 그렇구나...",
          "speaker_id": "1",
          "time": "20210903 18:06"
        },
        {
          "id": "MDRW2100039413.1.29",
          "form": "지금은 몇학점 들으시나용",
          "original_form": "지금은 몇학점 들으시나용",
          "speaker_id": "1",
          "time": "20210903 18:06"
        },
        {
          "id": "MDRW2100039413.1.30",
          "form": "저는 14학점요! 편입 생각중인지라",
          "original_form": "저는 14학점요! 편입 생각중인지라",
          "speaker_id": "2",
          "time": "20210903 18:07"
        }
      ]
