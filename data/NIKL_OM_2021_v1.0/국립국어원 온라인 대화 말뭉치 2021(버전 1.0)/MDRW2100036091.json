        },
        {
          "id": "MDRW2100036091.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 16:04"
        },
        {
          "id": "MDRW2100036091.1.3",
          "form": "피자 좋아하세요?",
          "original_form": "피자 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 16:04"
        },
        {
          "id": "MDRW2100036091.1.4",
          "form": "네 아주 좋아하네요",
          "original_form": "네 아주 좋아하네요",
          "speaker_id": "2",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100036091.1.5",
          "form": "오 무슨메뉴가 최애?",
          "original_form": "오 무슨메뉴가 최애?",
          "speaker_id": "1",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100036091.1.6",
          "form": "치즈 소세지 포테이토 페퍼로니 올리브 피자요",
          "original_form": "치즈 소세지 포테이토 페퍼로니 올리브 피자요",
          "speaker_id": "2",
          "time": "20210830 16:19"
        },
        {
          "id": "MDRW2100036091.1.7",
          "form": "헐.... 그걸 한번에 다??",
          "original_form": "헐.... 그걸 한번에 다??",
          "speaker_id": "1",
          "time": "20210830 16:22"
        },
        {
          "id": "MDRW2100036091.1.8",
          "form": "그럼 안되나요?",
          "original_form": "그럼 안되나요?",
          "speaker_id": "2",
          "time": "20210830 16:23"
        },
        {
          "id": "MDRW2100036091.1.9",
          "form": "오.... 야망 한가득이시네요.",
          "original_form": "오.... 야망 한가득이시네요.",
          "speaker_id": "1",
          "time": "20210830 16:23"
        },
        {
          "id": "MDRW2100036091.1.10",
          "form": "야망한가득",
          "original_form": "야망한가득",
          "speaker_id": "1",
          "time": "20210830 16:23"
        },
        {
          "id": "MDRW2100036091.1.11",
          "form": "욕망의 피자네여",
          "original_form": "욕망의 피자네여",
          "speaker_id": "1",
          "time": "20210830 16:23"
        },
        {
          "id": "MDRW2100036091.1.12",
          "form": "알아요",
          "original_form": "알아요",
          "speaker_id": "2",
          "time": "20210830 16:23"
        },
        {
          "id": "MDRW2100036091.1.13",
          "form": "페퍼로니는 넣어보셧야여?",
          "original_form": "페퍼로니는 넣어보셧야여?",
          "speaker_id": "1",
          "time": "20210830 16:23"
        },
        {
          "id": "MDRW2100036091.1.14",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210830 16:23"
        },
        {
          "id": "MDRW2100036091.1.15",
          "form": "피자는 보통 누가구워요?",
          "original_form": "피자는 보통 누가구워요?",
          "speaker_id": "1",
          "time": "20210830 16:23"
        },
        {
          "id": "MDRW2100036091.1.16",
          "form": "피자느 보통 누가구우ㅓ여?",
          "original_form": "피자느 보통 누가구우ㅓ여?",
          "speaker_id": "1",
          "time": "20210830 16:23"
        },
        {
          "id": "MDRW2100036091.1.17",
          "form": "비밀의 인물이요",
          "original_form": "비밀의 인물이요",
          "speaker_id": "2",
          "time": "20210830 16:23"
