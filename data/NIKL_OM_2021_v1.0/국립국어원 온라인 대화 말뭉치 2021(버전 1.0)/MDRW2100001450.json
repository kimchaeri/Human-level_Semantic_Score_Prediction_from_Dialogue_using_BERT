      "utterance": [
        {
          "id": "MDRW2100001450.1.1",
          "form": "name1님 오늘 저녁은 어떤 메뉴로 드세요? ㅎㅎ",
          "original_form": "&name1&님 오늘 저녁은 어떤 메뉴로 드세요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 15:15"
        },
        {
          "id": "MDRW2100001450.1.2",
          "form": "밥 김치 삼겹살 파채 마늘 등등",
          "original_form": "밥 김치 삼겹살 파채 마늘 등등",
          "speaker_id": "1",
          "time": "20211015 15:15"
        },
        {
          "id": "MDRW2100001450.1.3",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20211015 15:16"
        },
        {
          "id": "MDRW2100001450.1.4",
          "form": "중요한 건 삼겹살을 드신다는",
          "original_form": "중요한 건 삼겹살을 드신다는",
          "speaker_id": "2",
          "time": "20211015 15:16"
        },
        {
          "id": "MDRW2100001450.1.5",
          "form": "것이군요!",
          "original_form": "것이군요!",
          "speaker_id": "2",
          "time": "20211015 15:16"
        },
        {
          "id": "MDRW2100001450.1.6",
          "form": "네 맞습니다",
          "original_form": "네 맞습니다",
          "speaker_id": "1",
          "time": "20211015 15:16"
        },
        {
          "id": "MDRW2100001450.1.7",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20211015 15:17"
        },
        {
          "id": "MDRW2100001450.1.8",
          "form": "흑 너무 맛있겠어요",
          "original_form": "흑 너무 맛있겠어요",
          "speaker_id": "2",
          "time": "20211015 15:17"
        },
        {
          "id": "MDRW2100001450.1.9",
          "form": "벌써부터 배고픕니다",
          "original_form": "벌써부터 배고픕니다",
          "speaker_id": "1",
          "time": "20211015 15:17"
        },
        {
          "id": "MDRW2100001450.1.10",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20211015 15:17"
        },
        {
          "id": "MDRW2100001450.1.11",
          "form": "진짜 생각만해도 배고프네요 ㅎㅎ 김치도 같이 구우시나요 ㅎㅎ",
          "original_form": "진짜 생각만해도 배고프네요 ㅎㅎ 김치도 같이 구우시나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 15:17"
        },
        {
          "id": "MDRW2100001450.1.12",
          "form": "아뇨 집에서 먹을거라 김치는 못 구울거 같아요",
          "original_form": "아뇨 집에서 먹을거라 김치는 못 구울거 같아요",
          "speaker_id": "1",
          "time": "20211015 15:17"
        },
        {
          "id": "MDRW2100001450.1.13",
          "form": "대신 버섯, 양파, 마늘 정도는 구울 수 있을거 같아요",
          "original_form": "대신 버섯, 양파, 마늘 정도는 구울 수 있을거 같아요",
          "speaker_id": "1",
          "time": "20211015 15:17"
        },
        {
          "id": "MDRW2100001450.1.14",
          "form": "사실 김치 구우나 삼겹살 구우나 냄새나는건 똑같을거 같은데 말이죠",
          "original_form": "사실 김치 구우나 삼겹살 구우나 냄새나는건 똑같을거 같은데 말이죠",
          "speaker_id": "1",
          "time": "20211015 15:18"
        },
        {
          "id": "MDRW2100001450.1.15",
          "form": "그것만으로도 아주 맛있겠어요!",
          "original_form": "그것만으로도 아주 맛있겠어요!",
          "speaker_id": "2",
          "time": "20211015 15:18"
        },
        {
          "id": "MDRW2100001450.1.16",
          "form": "흠 근데 김치는 확실히",
          "original_form": "흠 근데 김치는 확실히",
          "speaker_id": "2",
          "time": "20211015 15:18"
        },
        {
          "id": "MDRW2100001450.1.17",
          "form": "기름에 굽고 나면 그 빨간 기름같은게 많이 튀어 있더라고요",
          "original_form": "기름에 굽고 나면 그 빨간 기름같은게 많이 튀어 있더라고요",
          "speaker_id": "2",
          "time": "20211015 15:18"
        },
        {
          "id": "MDRW2100001450.1.18",
          "form": "청소가 조금 귀찮지 않을까요..? ㅎㅎ",
          "original_form": "청소가 조금 귀찮지 않을까요..? ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 15:18"
        },
        {
          "id": "MDRW2100001450.1.19",
          "form": "네ㅋㅋ 저희는 후라이펜 뚜껑 있어서 그거 닫아 놓으면 조금 덜 튀어요",
          "original_form": "네ㅋㅋ 저희는 후라이펜 뚜껑 있어서 그거 닫아 놓으면 조금 덜 튀어요",
          "speaker_id": "1",
          "time": "20211015 15:19"
        },
        {
          "id": "MDRW2100001450.1.20",
          "form": "그렇군요..!",
          "original_form": "그렇군요..!",
          "speaker_id": "2",
          "time": "20211015 15:19"
        },
        {
          "id": "MDRW2100001450.1.21",
          "form": "점심 메뉴는 뭐 드셨었나요? ㅎㅎ",
          "original_form": "점심 메뉴는 뭐 드셨었나요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 15:19"
        },
        {
          "id": "MDRW2100001450.1.22",
          "form": "저는 회사다니는데",
          "original_form": "저는 회사다니는데",
          "speaker_id": "1",
          "time": "20211015 15:20"
        },
        {
          "id": "MDRW2100001450.1.23",
          "form": "다이어트 도시락 가지고 다녀서 그걸 먹었어요",
          "original_form": "다이어트 도시락 가지고 다녀서 그걸 먹었어요",
          "speaker_id": "1",
          "time": "20211015 15:20"
        },
        {
          "id": "MDRW2100001450.1.24",
          "form": "삼겹살...먹는다는건 못들은척 해주세요",
          "original_form": "삼겹살...먹는다는건 못들은척 해주세요",
          "speaker_id": "1",
          "time": "20211015 15:20"
        },
        {
          "id": "MDRW2100001450.1.25",
          "form": "다이어트 도시락 구성은 닭가슴살 사과 토마토 당근 등등입니다",
          "original_form": "다이어트 도시락 구성은 닭가슴살 사과 토마토 당근 등등입니다",
          "speaker_id": "1",
          "time": "20211015 15:21"
        },
        {
          "id": "MDRW2100001450.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ삼겹살도 다이어트에 중요한 단백질 아닌가요?! (뻔뻔)",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ삼겹살도 다이어트에 중요한 단백질 아닌가요?! (뻔뻔)",
          "speaker_id": "2",
          "time": "20211015 15:21"
        },
        {
          "id": "MDRW2100001450.1.27",
          "form": "점심에 일하시는데 심지어 닭가슴살이라니 ㅠㅠ 넘 대단하신 거 같아요",
          "original_form": "점심에 일하시는데 심지어 닭가슴살이라니 ㅠㅠ 넘 대단하신 거 같아요",
          "speaker_id": "2",
          "time": "20211015 15:22"
        },
        {
          "id": "MDRW2100001450.1.28",
          "form": "다이어트를 하시는 건가요?!",
          "original_form": "다이어트를 하시는 건가요?!",
          "speaker_id": "2",
          "time": "20211015 15:22"
        },
        {
          "id": "MDRW2100001450.1.29",
          "form": "다이어트 목적도 있고",
          "original_form": "다이어트 목적도 있고",
          "speaker_id": "1",
          "time": "20211015 15:22"
        },
        {
          "id": "MDRW2100001450.1.30",
          "form": "닭가슴살이 저렴해서(200g당 900원대) 저축 목적도 있어요",
          "original_form": "닭가슴살이 저렴해서(200g당 900원대) 저축 목적도 있어요",
          "speaker_id": "1",
          "time": "20211015 15:23"
        },
        {
          "id": "MDRW2100001450.1.31",
          "form": "와 천원도 안하는 건 정말 싸네요..! 그 가격이면 당장 반강제 다이어트식이 될 것 같아요 ㅋㅋㅋ",
          "original_form": "와 천원도 안하는 건 정말 싸네요..! 그 가격이면 당장 반강제 다이어트식이 될 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 15:25"
        }
      ]
