      "utterance": [
        {
          "id": "MDRW2100002391.1.1",
          "form": "밥해먹기가 제일 귀찮네요.",
          "original_form": "밥해먹기가 제일 귀찮네요.",
          "speaker_id": "1",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002391.1.2",
          "form": "저는 음식 하는것보다 설거지가 너무 귀찮은거같아요...",
          "original_form": "저는 음식 하는것보다 설거지가 너무 귀찮은거같아요...",
          "speaker_id": "2",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002391.1.3",
          "form": "사실 집안일은 재미가없어요...",
          "original_form": "사실 집안일은 재미가없어요...",
          "speaker_id": "2",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002391.1.4",
          "form": "설거지도 정말 귀찮아요.",
          "original_form": "설거지도 정말 귀찮아요.",
          "speaker_id": "1",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002391.1.5",
          "form": "치우는게 싫어요ㅜ",
          "original_form": "치우는게 싫어요ㅜ",
          "speaker_id": "1",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002391.1.6",
          "form": "ㅎ.ㅎ...저두요 집안일하려면",
          "original_form": "ㅎ.ㅎ...저두요 집안일하려면",
          "speaker_id": "2",
          "time": "20210820 11:06"
        },
        {
          "id": "MDRW2100002391.1.7",
          "form": "부지런해야하는거같아요",
          "original_form": "부지런해야하는거같아요",
          "speaker_id": "2",
          "time": "20210820 11:06"
        },
        {
          "id": "MDRW2100002391.1.8",
          "form": "어질지 않고 치우지 않는방법을 택했어요ㅎ",
          "original_form": "어질지 않고 치우지 않는방법을 택했어요ㅎ",
          "speaker_id": "1",
          "time": "20210820 11:07"
        },
        {
          "id": "MDRW2100002391.1.9",
          "form": "아..ㅋㄱㅋㅋㅋ 아주 좋은 방법이예요",
          "original_form": "아..ㅋㄱㅋㅋㅋ 아주 좋은 방법이예요",
          "speaker_id": "2",
          "time": "20210820 11:08"
        },
        {
          "id": "MDRW2100002391.1.10",
          "form": "저는 오늘 그릭요거트 만들어놓고 나와서 집가자마자 치워야해요..",
          "original_form": "저는 오늘 그릭요거트 만들어놓고 나와서 집가자마자 치워야해요..",
          "speaker_id": "2",
          "time": "20210820 11:09"
        },
        {
          "id": "MDRW2100002391.1.11",
          "form": "설거지랑..면보..",
          "original_form": "설거지랑..면보..",
          "speaker_id": "2",
          "time": "20210820 11:09"
        },
        {
          "id": "MDRW2100002391.1.12",
          "form": "면보 빨고 말리기까지 귀찮으신데 대단하셔요",
          "original_form": "면보 빨고 말리기까지 귀찮으신데 대단하셔요",
          "speaker_id": "1",
          "time": "20210820 11:14"
        },
        {
          "id": "MDRW2100002391.1.13",
          "form": "ㅎ.ㅎ..안하면 혼나요 부모님한테...",
          "original_form": "ㅎ.ㅎ..안하면 혼나요 부모님한테...",
          "speaker_id": "2",
          "time": "20210820 11:14"
        },
        {
          "id": "MDRW2100002391.1.14",
          "form": "요거트도직접 만드시고 능력자세요",
          "original_form": "요거트도직접 만드시고 능력자세요",
          "speaker_id": "1",
          "time": "20210820 11:50"
        },
        {
          "id": "MDRW2100002391.1.15",
          "form": "렌지에만 돌리면 되어서 그건 쉬워요!",
          "original_form": "렌지에만 돌리면 되어서 그건 쉬워요!",
          "speaker_id": "2",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100002391.1.16",
          "form": "나중에 좋아하시면 해보세요!",
          "original_form": "나중에 좋아하시면 해보세요!",
          "speaker_id": "2",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100002391.1.17",
          "form": "한번 검색해보고 시도해볼께요ㅎ",
          "original_form": "한번 검색해보고 시도해볼께요ㅎ",
          "speaker_id": "1",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100002391.1.18",
          "form": "넵! 생각보다 쉬울꺼예요!",
          "original_form": "넵! 생각보다 쉬울꺼예요!",
          "speaker_id": "2",
          "time": "20210820 11:53"
        },
        {
          "id": "MDRW2100002391.1.19",
          "form": "가족들도 좋아할꺼 같아요ㅎ",
          "original_form": "가족들도 좋아할꺼 같아요ㅎ",
          "speaker_id": "1",
          "time": "20210820 11:54"
        }
