      "utterance": [
        {
          "id": "MDRW2100000751.1.1",
          "form": "커뮤니티에서 정모 같은 거 해보신 적 있나요?",
          "original_form": "커뮤니티에서 정모 같은 거 해보신 적 있나요?",
          "speaker_id": "1",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100000751.1.2",
          "form": "아니요 커뮤니티 정모는 안해봤어요",
          "original_form": "아니요 커뮤니티 정모는 안해봤어요",
          "speaker_id": "2",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100000751.1.3",
          "form": "해보신거있으세요?",
          "original_form": "해보신거있으세요?",
          "speaker_id": "2",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100000751.1.4",
          "form": "아뇨 저도 없고 그냥 어릴 때",
          "original_form": "아뇨 저도 없고 그냥 어릴 때",
          "speaker_id": "1",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100000751.1.5",
          "form": "콘서트 간다고 만나서 논적은 있어여ㅋㅋㅋㅋㅋㅋ",
          "original_form": "콘서트 간다고 만나서 논적은 있어여ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100000751.1.6",
          "form": "아~맞아요 콘서트 갈 때 만나기도하죠",
          "original_form": "아~맞아요 콘서트 갈 때 만나기도하죠",
          "speaker_id": "2",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100000751.1.7",
          "form": "지금은 온라인커뮤니티 하시는거 없으세요?",
          "original_form": "지금은 온라인커뮤니티 하시는거 없으세요?",
          "speaker_id": "2",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100000751.1.8",
          "form": "네 지금 생각하면 어떻게 했나 싶어요 ㅎㅎ",
          "original_form": "네 지금 생각하면 어떻게 했나 싶어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100000751.1.9",
          "form": "지금도 여러곳 하고 있어요",
          "original_form": "지금도 여러곳 하고 있어요",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100000751.1.10",
          "form": "햇님님도 하시나요?",
          "original_form": "햇님님도 하시나요?",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100000751.1.11",
          "form": "네 저도 하고있어요",
          "original_form": "네 저도 하고있어요",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100000751.1.12",
          "form": "커뮤니티에서 얻을수있는 정보도 많고",
          "original_form": "커뮤니티에서 얻을수있는 정보도 많고",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100000751.1.13",
          "form": "일상얘기하면 재밌더라구요",
          "original_form": "일상얘기하면 재밌더라구요",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100000751.1.14",
          "form": "맞아요 남들 이야기도 좋고 제 일상 이야기도 좋고",
          "original_form": "맞아요 남들 이야기도 좋고 제 일상 이야기도 좋고",
          "speaker_id": "1",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100000751.1.15",
          "form": "정보도 교환하고 참 좋은 거 같아요",
          "original_form": "정보도 교환하고 참 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100000751.1.16",
          "form": "마자요 커뮤니티 안할때는 세상을 너무 모르고 산 느낌이랄까요ㅋㅋㅋ",
          "original_form": "마자요 커뮤니티 안할때는 세상을 너무 모르고 산 느낌이랄까요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100000751.1.17",
          "form": "좀더 세상에 눈이 떠지는거같아요",
          "original_form": "좀더 세상에 눈이 떠지는거같아요",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100000751.1.18",
          "form": "그러니까요 그 많은 정보들을 어떻게 모르고 살았나",
          "original_form": "그러니까요 그 많은 정보들을 어떻게 모르고 살았나",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100000751.1.19",
          "form": "지금 생각하면 신기해요",
          "original_form": "지금 생각하면 신기해요",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100000751.1.20",
          "form": "마자요 커뮤니티 좀 더 예전부터했더라면 좋았을걸 싶어요",
          "original_form": "마자요 커뮤니티 좀 더 예전부터했더라면 좋았을걸 싶어요",
          "speaker_id": "2",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100000751.1.21",
          "form": "이제는 없으면 못살거같아요",
          "original_form": "이제는 없으면 못살거같아요",
          "speaker_id": "2",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100000751.1.22",
          "form": "앗 저도요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗 저도요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100000751.1.23",
          "form": "이득인 부분이 아주 많아서 놓고싶지 않아요",
          "original_form": "이득인 부분이 아주 많아서 놓고싶지 않아요",
          "speaker_id": "1",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100000751.1.24",
          "form": "마자요ㅋㅋ\r\n커뮤니티 알려준 친구한테 너무 고마워요",
          "original_form": "마자요ㅋㅋ\r\n커뮤니티 알려준 친구한테 너무 고마워요",
