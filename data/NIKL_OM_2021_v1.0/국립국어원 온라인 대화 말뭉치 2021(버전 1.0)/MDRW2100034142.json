        },
        {
          "id": "MDRW2100034142.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100034142.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210828 20:35"
        },
        {
          "id": "MDRW2100034142.1.4",
          "form": "쇼핑 좋아하시나요?",
          "original_form": "쇼핑 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100034142.1.5",
          "form": "넹 좋아합니다",
          "original_form": "넹 좋아합니다",
          "speaker_id": "1",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100034142.1.6",
          "form": "주로 어떤 거 구매하세요~",
          "original_form": "주로 어떤 거 구매하세요~",
          "speaker_id": "2",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100034142.1.7",
          "form": "요즘 주로 먹을거 많이 쟁여요 ㅋㅋ",
          "original_form": "요즘 주로 먹을거 많이 쟁여요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100034142.1.8",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100034142.1.9",
          "form": "저도 군것질거리 많이 사는 것 같아요 ㅋㅋㅋ",
          "original_form": "저도 군것질거리 많이 사는 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100034142.1.10",
          "form": "주로 젤리를 쓸어 담고 있습니다 ㅎㅎ",
          "original_form": "주로 젤리를 쓸어 담고 있습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100034142.1.11",
          "form": "아 ㅋㅋㅋ 젤리 뭐좋아하세요",
          "original_form": "아 ㅋㅋㅋ 젤리 뭐좋아하세요",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100034142.1.12",
          "form": "트롤리 젤리 중에 햄버거랑 핫도그 젤리 좋아해요!",
          "original_form": "트롤리 젤리 중에 햄버거랑 핫도그 젤리 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100034142.1.13",
          "form": "젤리 좋아하세요?",
          "original_form": "젤리 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100034142.1.14",
          "form": "오 저도 트롤리 조아해요",
          "original_form": "오 저도 트롤리 조아해요",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100034142.1.15",
          "form": "전 사워? 그거 ㅋㅋㅋㅋㅍ",
          "original_form": "전 사워? 그거 ㅋㅋㅋㅋㅍ",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100034142.1.16",
          "form": "하얀갸뿌려있는거용",
          "original_form": "하얀갸뿌려있는거용",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100034142.1.17",
          "form": "잠 깨야 할 때 그거 먹으면 잠 확 깨죠 ㅎㅎ",
          "original_form": "잠 깨야 할 때 그거 먹으면 잠 확 깨죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100034142.1.18",
          "form": "가끔 입가심하기에도 좋은 것 같애요 ㅋㅋㅋㅋ",
          "original_form": "가끔 입가심하기에도 좋은 것 같애요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100034142.1.19",
          "form": "그죠 근데 입가심으로 안멈추고 ㅋㅋ",
          "original_form": "그죠 근데 입가심으로 안멈추고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100034142.1.20",
          "form": "다시 배부를 때까지 먹고 ㅋㅋㅋㅋㅋ",
          "original_form": "다시 배부를 때까지 먹고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100034142.1.21",
