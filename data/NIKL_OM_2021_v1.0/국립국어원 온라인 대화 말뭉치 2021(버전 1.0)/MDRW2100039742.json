      "utterance": [
        {
          "id": "MDRW2100039742.1.1",
          "form": "음 이상형이 어떻게 되시나요?",
          "original_form": "음 이상형이 어떻게 되시나요?",
          "speaker_id": "2",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100039742.1.2",
          "form": "이제와서 이상형이\r\n다 무슨 소용 있겠습니까",
          "original_form": "이제와서 이상형이\r\n다 무슨 소용 있겠습니까",
          "speaker_id": "1",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100039742.1.3",
          "form": "아니ㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜용...ㅠㅠㅠ...",
          "original_form": "아니ㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜용...ㅠㅠㅠ...",
          "speaker_id": "2",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100039742.1.4",
          "form": "저는 이미 돌아갈수없는 강을\r\n건넌 몸입니다",
          "original_form": "저는 이미 돌아갈수없는 강을\r\n건넌 몸입니다",
          "speaker_id": "1",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100039742.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100039742.1.6",
          "form": "헉 결혼하셨구낭~!!",
          "original_form": "헉 결혼하셨구낭~!!",
          "speaker_id": "2",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100039742.1.7",
          "form": "연애중이십니까?",
          "original_form": "연애중이십니까?",
          "speaker_id": "1",
          "time": "20210903 21:07"
        },
        {
          "id": "MDRW2100039742.1.8",
          "form": "아뇨...저는.... 취준생이라서...",
          "original_form": "아뇨...저는.... 취준생이라서...",
          "speaker_id": "2",
          "time": "20210903 21:07"
        },
        {
          "id": "MDRW2100039742.1.9",
          "form": "그리고 아이돌 좋아해요...ㅋㅋㅋㅋㅋㅋㅋㅋ...",
          "original_form": "그리고 아이돌 좋아해요...ㅋㅋㅋㅋㅋㅋㅋㅋ...",
          "speaker_id": "2",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100039742.1.10",
          "form": "아이돌 중 이상형에 가까운 분은?",
          "original_form": "아이돌 중 이상형에 가까운 분은?",
          "speaker_id": "1",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100039742.1.11",
          "form": "저....아실지 모르겠네요 엔시티 텐.... 세븐틴 윤정한.... 아시나요?",
          "original_form": "저....아실지 모르겠네요 엔시티 텐.... 세븐틴 윤정한.... 아시나요?",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100039742.1.12",
          "form": "부끄럽네요",
          "original_form": "부끄럽네요",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100039742.1.13",
          "form": "검색들어갑니다 ㅋㅋㅋ",
          "original_form": "검색들어갑니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039742.1.14",
          "form": "뉘집 아들들인지",
          "original_form": "뉘집 아들들인지",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039742.1.15",
          "form": "훌륭하게도 키우셨네요",
          "original_form": "훌륭하게도 키우셨네요",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039742.1.16",
          "form": "그쵸그쵸ㅋㅋㅋㅋㅋㅋ잘생겼죠... 제가 다 감사하네요",
          "original_form": "그쵸그쵸ㅋㅋㅋㅋㅋㅋ잘생겼죠... 제가 다 감사하네요",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100039742.1.17",
          "form": "결혼한지 몇년 되셨는지 여쭤봐도 될까용...",
          "original_form": "결혼한지 몇년 되셨는지 여쭤봐도 될까용...",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039742.1.18",
          "form": "ㅋㅋㅋㅋ제가 더 감사해요",
          "original_form": "ㅋㅋㅋㅋ제가 더 감사해요",
          "speaker_id": "1",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039742.1.19",
          "form": "안구정화 타임이었네염ㅋㅋ",
          "original_form": "안구정화 타임이었네염ㅋㅋ",
