      "utterance": [
        {
          "id": "MDRW2100027353.1.1",
          "form": "라면은",
          "original_form": "라면은",
          "speaker_id": "2",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027353.1.2",
          "form": "남이끓어주는라면이",
          "original_form": "남이끓어주는라면이",
          "speaker_id": "2",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027353.1.3",
          "form": "대존맛",
          "original_form": "대존맛",
          "speaker_id": "2",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027353.1.4",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027353.1.5",
          "form": "내가  끓이면 맛없어ㅜㅜ",
          "original_form": "내가  끓이면 맛없어ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027353.1.6",
          "form": "그거아세요",
          "original_form": "그거아세요",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027353.1.7",
          "form": "찬물일때 면스프 넣고",
          "original_form": "찬물일때 면스프 넣고",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027353.1.8",
          "form": "한번에 끓이면",
          "original_form": "한번에 끓이면",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027353.1.9",
          "form": "꼬들한라면완성된대요",
          "original_form": "꼬들한라면완성된대요",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027353.1.10",
          "form": "더 맛있나요",
          "original_form": "더 맛있나요",
          "speaker_id": "1",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027353.1.11",
          "form": "조리시간도 줄이고요",
          "original_form": "조리시간도 줄이고요",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027353.1.12",
          "form": "아 담에 먹을때 시도해봐야겠어요",
          "original_form": "아 담에 먹을때 시도해봐야겠어요",
          "speaker_id": "1",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027353.1.13",
          "form": "해봤는데 뜨거운물에 넣는거랑",
          "original_form": "해봤는데 뜨거운물에 넣는거랑",
          "speaker_id": "2",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027353.1.14",
          "form": "차이가 없더라구요",
          "original_form": "차이가 없더라구요",
          "speaker_id": "2",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027353.1.15",
          "form": "뭐죠?",
          "original_form": "뭐죠?",
          "speaker_id": "1",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027353.1.16",
          "form": "훨 편해요",
          "original_form": "훨 편해요",
          "speaker_id": "2",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027353.1.17",
          "form": "근데 더 꼬들한가요?",
          "original_form": "근데 더 꼬들한가요?",
          "speaker_id": "1",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027353.1.18",
          "form": "전퍼진라면좋아해서",
          "original_form": "전퍼진라면좋아해서",
          "speaker_id": "2",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027353.1.19",
          "form": "조금더오래끓였어요",
          "original_form": "조금더오래끓였어요",
          "speaker_id": "2",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027353.1.20",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027353.1.21",
          "form": "퍼진면 러버ㅋ",
          "original_form": "퍼진면 러버ㅋ",
          "speaker_id": "1",
          "time": "20210902 11:47"
        },
        {
          "id": "MDRW2100027353.1.22",
          "form": "라면에김치존맛",
          "original_form": "라면에김치존맛",
          "speaker_id": "2",
          "time": "20210902 11:47"
        },
        {
          "id": "MDRW2100027353.1.23",
          "form": "짜파게티에 파김치",
          "original_form": "짜파게티에 파김치",
          "speaker_id": "2",
          "time": "20210902 11:47"
        },
        {
          "id": "MDRW2100027353.1.24",
          "form": "대존맛",
          "original_form": "대존맛",
          "speaker_id": "2",
          "time": "20210902 11:47"
        },
        {
          "id": "MDRW2100027353.1.25",
          "form": "저녁드셨어요?",
          "original_form": "저녁드셨어요?",
          "speaker_id": "1",
