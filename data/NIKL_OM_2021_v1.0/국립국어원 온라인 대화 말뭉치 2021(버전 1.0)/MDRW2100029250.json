      "utterance": [
        {
          "id": "MDRW2100029250.1.1",
          "form": "가족과 함께 사나요?",
          "original_form": "가족과 함께 사나요?",
          "speaker_id": "2",
          "time": "20210903 00:55"
        },
        {
          "id": "MDRW2100029250.1.2",
          "form": "혼자요",
          "original_form": "혼자요",
          "speaker_id": "1",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029250.1.3",
          "form": "name2은요",
          "original_form": "&name2&은요",
          "speaker_id": "1",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029250.1.4",
          "form": "독립하셨나요?",
          "original_form": "독립하셨나요?",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029250.1.5",
          "form": "저는 가족과 삽니다",
          "original_form": "저는 가족과 삽니다",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029250.1.6",
          "form": "3명!",
          "original_form": "3명!",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029250.1.7",
          "form": "오호...그중..리더?",
          "original_form": "오호...그중..리더?",
          "speaker_id": "1",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029250.1.8",
          "form": "네 독립 ...인셈이죠",
          "original_form": "네 독립 ...인셈이죠",
          "speaker_id": "1",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029250.1.9",
          "form": "네 제가 가장입니다",
          "original_form": "네 제가 가장입니다",
          "speaker_id": "2",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029250.1.10",
          "form": "쉽지않습니다",
          "original_form": "쉽지않습니다",
          "speaker_id": "2",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029250.1.11",
          "form": "혼자가 좋습닏ㄴ",
          "original_form": "혼자가 좋습닏ㄴ",
          "speaker_id": "2",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029250.1.12",
          "form": "아이고 그럼 집안일 머하세요",
          "original_form": "아이고 그럼 집안일 머하세요",
          "speaker_id": "1",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100029250.1.13",
          "form": "다하져",
          "original_form": "다하져",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029250.1.14",
          "form": "빨래",
          "original_form": "빨래",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029250.1.15",
          "form": "설겆이",
          "original_form": "설겆이",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029250.1.16",
          "form": "수채청소",
          "original_form": "수채청소",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029250.1.17",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029250.1.18",
          "form": "안하는걸 찾아야됩니다 ㅋㅋ",
          "original_form": "안하는걸 찾아야됩니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029250.1.19",
          "form": "육아도 잘하시나요",
          "original_form": "육아도 잘하시나요",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029250.1.20",
          "form": "잘하져 ㅋㄱㅅ",
          "original_form": "잘하져 ㅋㄱㅅ",
          "speaker_id": "2",
          "time": "20210903 01:33"
        },
        {
          "id": "MDRW2100029250.1.21",
          "form": "매일 목욕같이해요 ㅋㅋ",
          "original_form": "매일 목욕같이해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:33"
        },
        {
          "id": "MDRW2100029250.1.22",
          "form": "ㅋㅋㅋㅋㅋ 소주 드시고 ?",
          "original_form": "ㅋㅋㅋㅋㅋ 소주 드시고 ?",
          "speaker_id": "1",
          "time": "20210903 01:33"
        },
        {
          "id": "MDRW2100029250.1.23",
          "form": "소주먹어도 해요 ㅋㅋㅋㅋ",
          "original_form": "소주먹어도 해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100029250.1.24",
          "form": "독립하시니까 어떠신가요?",
          "original_form": "독립하시니까 어떠신가요?",
          "speaker_id": "2",
          "time": "20210903 10:21"
        },
        {
          "id": "MDRW2100029250.1.25",
          "form": "저요...걍 청소하고 밥먹고설거지하고 할게많죠   ..",
          "original_form": "저요...걍 청소하고 밥먹고설거지하고 할게많죠   ..",
          "speaker_id": "1",
          "time": "20210903 10:22"
        },
        {
          "id": "MDRW2100029250.1.26",
          "form": "혼자사는건조아요",
          "original_form": "혼자사는건조아요",
          "speaker_id": "1",
          "time": "20210903 10:22"
        },
        {
          "id": "MDRW2100029250.1.27",
          "form": "여자친구는요?",
          "original_form": "여자친구는요?",
          "speaker_id": "2",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100029250.1.28",
          "form": "여친은 있어야져!",
          "original_form": "여친은 있어야져!",
          "speaker_id": "2",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100029250.1.29",
          "form": "저 여잔뎅 ! ㅋㅋㅋㅋ",
          "original_form": "저 여잔뎅 ! ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029250.1.30",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029250.1.31",
          "form": "죄송 ㅋㅋㅋ",
          "original_form": "죄송 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029250.1.32",
          "form": "혼자가 좋죠",
          "original_form": "혼자가 좋죠",
          "speaker_id": "2",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029250.1.33",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029250.1.34",
          "form": "질문수정 남친은 있나요?",
          "original_form": "질문수정 남친은 있나요?",
          "speaker_id": "2",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029250.1.35",
          "form": "그거도업네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그거도업네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100029250.1.36",
          "form": "왜 남자분들에게 기회를 안주십니까 ㅋㅋ",
          "original_form": "왜 남자분들에게 기회를 안주십니까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100029250.1.37",
          "form": "남자가없던대요  ..",
          "original_form": "남자가없던대요  ..",
          "speaker_id": "1",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100029250.1.38",
          "form": "말잘듣는남자",
          "original_form": "말잘듣는남자",
          "speaker_id": "1",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100029250.1.39",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100029250.1.40",
          "form": "성향",
          "original_form": "성향",
          "speaker_id": "2",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100029250.1.41",
          "form": "쌘캐신가보네요",
          "original_form": "쌘캐신가보네요",
          "speaker_id": "2",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100029250.1.42",
          "form": "훌륭합니다",
          "original_form": "훌륭합니다",
          "speaker_id": "2",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100029250.1.43",
          "form": "자기 주관 뚜렸한 여자 매력터지죠 ㅎㅎ",
          "original_form": "자기 주관 뚜렸한 여자 매력터지죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100029250.1.44",
          "form": "ㅋㅋㅋㅋ 받아줄사람이없네요 아빠말고",
          "original_form": "ㅋㅋㅋㅋ 받아줄사람이없네요 아빠말고",
