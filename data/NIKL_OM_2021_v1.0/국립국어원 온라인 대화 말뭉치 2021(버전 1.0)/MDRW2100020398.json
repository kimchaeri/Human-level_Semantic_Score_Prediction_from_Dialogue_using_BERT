        },
        {
          "id": "MDRW2100020398.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020398.1.3",
          "form": "현재 일하는 직종에서의 평균급여?!",
          "original_form": "현재 일하는 직종에서의 평균급여?!",
          "speaker_id": "1",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020398.1.4",
          "form": "가 저는 최저라서",
          "original_form": "가 저는 최저라서",
          "speaker_id": "1",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020398.1.5",
          "form": "일단목표가 200이에요 ,,,",
          "original_form": "일단목표가 200이에요 ,,,",
          "speaker_id": "1",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020398.1.6",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210828 02:48"
        },
        {
          "id": "MDRW2100020398.1.7",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "2",
          "time": "20210828 02:48"
        },
        {
          "id": "MDRW2100020398.1.8",
          "form": "그 국가에서 해주는",
          "original_form": "그 국가에서 해주는",
          "speaker_id": "2",
          "time": "20210828 02:48"
        },
        {
          "id": "MDRW2100020398.1.9",
          "form": "수업?",
          "original_form": "수업?",
          "speaker_id": "2",
          "time": "20210828 02:48"
        },
        {
          "id": "MDRW2100020398.1.10",
          "form": "아 제가 아이티로 옮겼다고 했잖아요",
          "original_form": "아 제가 아이티로 옮겼다고 했잖아요",
          "speaker_id": "2",
          "time": "20210828 02:48"
        },
        {
          "id": "MDRW2100020398.1.11",
          "form": "내일배움카드?",
          "original_form": "내일배움카드?",
          "speaker_id": "1",
          "time": "20210828 02:48"
        },
        {
          "id": "MDRW2100020398.1.12",
          "form": "오 네네 그런거?!?!",
          "original_form": "오 네네 그런거?!?!",
          "speaker_id": "2",
          "time": "20210828 02:48"
        },
        {
          "id": "MDRW2100020398.1.13",
          "form": "근데 여기보면 문과출신도 많거든요?!",
          "original_form": "근데 여기보면 문과출신도 많거든요?!",
          "speaker_id": "2",
          "time": "20210828 02:49"
        },
        {
          "id": "MDRW2100020398.1.14",
          "form": "보면 컴퓨터학원에서 배우고",
          "original_form": "보면 컴퓨터학원에서 배우고",
          "speaker_id": "2",
          "time": "20210828 02:49"
        },
        {
          "id": "MDRW2100020398.1.15",
          "form": "거기서 연결도 해주고 하는거같더라구요",
          "original_form": "거기서 연결도 해주고 하는거같더라구요",
          "speaker_id": "2",
          "time": "20210828 02:49"
        },
        {
          "id": "MDRW2100020398.1.16",
          "form": "와 좋다",
          "original_form": "와 좋다",
          "speaker_id": "1",
          "time": "20210828 02:49"
        },
        {
          "id": "MDRW2100020398.1.17",
          "form": "여기는 지금 인력난이에요",
          "original_form": "여기는 지금 인력난이에요",
          "speaker_id": "2",
          "time": "20210828 02:49"
        },
        {
          "id": "MDRW2100020398.1.18",
          "form": "사람없어서 못구해요",
          "original_form": "사람없어서 못구해요",
          "speaker_id": "2",
          "time": "20210828 02:49"
        },
        {
          "id": "MDRW2100020398.1.19",
          "form": "코딩 프로그래밍 이런건가요?!",
          "original_form": "코딩 프로그래밍 이런건가요?!",
          "speaker_id": "1",
          "time": "20210828 02:49"
        },
        {
          "id": "MDRW2100020398.1.20",
          "form": "함 알아보세요!!",
          "original_form": "함 알아보세요!!",
          "speaker_id": "2",
          "time": "20210828 02:49"
        },
        {
          "id": "MDRW2100020398.1.21",
          "form": "완전 블루오션이네요",
          "original_form": "완전 블루오션이네요",
          "speaker_id": "1",
          "time": "20210828 02:50"
        },
        {
          "id": "MDRW2100020398.1.22",
          "form": "네네 !! 근데 물론 뭐 카카오 이런급 전혀아니구요",
          "original_form": "네네 !! 근데 물론 뭐 카카오 이런급 전혀아니구요",
          "speaker_id": "2",
          "time": "20210828 02:50"
        },
        {
          "id": "MDRW2100020398.1.23",
          "form": "제 친구는 자바하는데 저한테 자바배우라고 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "제 친구는 자바하는데 저한테 자바배우라고 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:50"
        },
        {
          "id": "MDRW2100020398.1.24",
          "form": "무조건 연봉 3천은 기본으로 생각하실수 있어요 비전공자라도",
          "original_form": "무조건 연봉 3천은 기본으로 생각하실수 있어요 비전공자라도",
          "speaker_id": "2",
          "time": "20210828 02:50"
        },
        {
          "id": "MDRW2100020398.1.25",
          "form": "혼자하시긴 힘들테니까",
          "original_form": "혼자하시긴 힘들테니까",
          "speaker_id": "2",
          "time": "20210828 02:50"
        },
        {
          "id": "MDRW2100020398.1.26",
          "form": "솔깃,,군침,,싸악",
          "original_form": "솔깃,,군침,,싸악",
          "speaker_id": "1",
          "time": "20210828 02:50"
        },
        {
          "id": "MDRW2100020398.1.27",
          "form": "저는 학원추천이요!!",
          "original_form": "저는 학원추천이요!!",
          "speaker_id": "2",
          "time": "20210828 02:50"
        },
        {
          "id": "MDRW2100020398.1.28",
          "form": "그것도 국비로 할수도 있다고하더라구요",
          "original_form": "그것도 국비로 할수도 있다고하더라구요",
          "speaker_id": "2",
          "time": "20210828 02:51"
        },
        {
          "id": "MDRW2100020398.1.29",
          "form": "감사해요 ! 진짜 찾아볼게요 진짜",
          "original_form": "감사해요 ! 진짜 찾아볼게요 진짜",
          "speaker_id": "1",
          "time": "20210828 02:51"
        },
        {
          "id": "MDRW2100020398.1.30",
          "form": "지금 실업급여 받으면서 4~5달 쉴수있어서 적기에요",
          "original_form": "지금 실업급여 받으면서 4~5달 쉴수있어서 적기에요",
          "speaker_id": "1",
          "time": "20210828 02:51"
        },
        {
          "id": "MDRW2100020398.1.31",
          "form": "네네 하지만 아까말씀드렸듯이 좀...쓰리디같아요 ㅜㅜ",
          "original_form": "네네 하지만 아까말씀드렸듯이 좀...쓰리디같아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 02:51"
        },
        {
          "id": "MDRW2100020398.1.32",
          "form": "그쵸 ,, 엄청 힘들다고 하더라고요",
          "original_form": "그쵸 ,, 엄청 힘들다고 하더라고요",
          "speaker_id": "1",
          "time": "20210828 02:51"
        },
        {
          "id": "MDRW2100020398.1.33",
          "form": "이럴거면 예전회사 왜때려쳤나 싶고요ㅠㅠ",
          "original_form": "이럴거면 예전회사 왜때려쳤나 싶고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 02:51"
        },
        {
          "id": "MDRW2100020398.1.34",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 02:51"
        },
        {
          "id": "MDRW2100020398.1.35",
          "form": "진짜 연봉.. 일의 강도.. 다..",
          "original_form": "진짜 연봉.. 일의 강도.. 다..",
          "speaker_id": "2",
          "time": "20210828 02:52"
        },
        {
          "id": "MDRW2100020398.1.36",
          "form": "화공쪽으로 다시 돌아가실 생각은 없으신거에요?!",
          "original_form": "화공쪽으로 다시 돌아가실 생각은 없으신거에요?!",
          "speaker_id": "1",
          "time": "20210828 02:52"
        },
        {
          "id": "MDRW2100020398.1.37",
          "form": "음.. 생각해보고 있긴해요!!",
          "original_form": "음.. 생각해보고 있긴해요!!",
          "speaker_id": "2",
          "time": "20210828 02:52"
        },
        {
          "id": "MDRW2100020398.1.38",
          "form": "근데 전혀 알아보고있지않아요",
          "original_form": "근데 전혀 알아보고있지않아요",
          "speaker_id": "2",
          "time": "20210828 02:52"
        },
        {
          "id": "MDRW2100020398.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ게을러서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ게을러서",
          "speaker_id": "2",
