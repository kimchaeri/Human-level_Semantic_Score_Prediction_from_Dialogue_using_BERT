      "utterance": [
        {
          "id": "MDRW2100009068.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 20:55"
        },
        {
          "id": "MDRW2100009068.1.2",
          "form": "여행 다니는거좋아하시나요?",
          "original_form": "여행 다니는거좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 20:55"
        },
        {
          "id": "MDRW2100009068.1.3",
          "form": "네~^^코로나로 못가서 좀 답답하네요",
          "original_form": "네~^^코로나로 못가서 좀 답답하네요",
          "speaker_id": "2",
          "time": "20210822 20:56"
        },
        {
          "id": "MDRW2100009068.1.4",
          "form": "코로나가 성행하니까 전엔 제주도도 갔었는데",
          "original_form": "코로나가 성행하니까 전엔 제주도도 갔었는데",
          "speaker_id": "1",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009068.1.5",
          "form": "요샌 좀 집이더 편해진것같아요",
          "original_form": "요샌 좀 집이더 편해진것같아요",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009068.1.6",
          "form": "제친구는 여행을 못가니까 아예 제주도로 이사를갔어요ㅎㅎ",
          "original_form": "제친구는 여행을 못가니까 아예 제주도로 이사를갔어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009068.1.7",
          "form": "친구분이 부럽군요. 올해는 여행 안가셨나요?",
          "original_form": "친구분이 부럽군요. 올해는 여행 안가셨나요?",
          "speaker_id": "2",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009068.1.8",
          "form": "저는 출장만가고 ㅋㅋ 못갔네요 name2님은요?",
          "original_form": "저는 출장만가고 ㅋㅋ 못갔네요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009068.1.9",
          "form": "전 아이가 있어서 물놀이장 노래를 불러서 한적한 곳으로 물놀이 다녀왔어요",
          "original_form": "전 아이가 있어서 물놀이장 노래를 불러서 한적한 곳으로 물놀이 다녀왔어요",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009068.1.10",
          "form": "아유 아이가 있으면 갑갑하시겠어요",
          "original_form": "아유 아이가 있으면 갑갑하시겠어요",
          "speaker_id": "1",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009068.1.11",
          "form": "물놀이 좋았겠네요 부럽당ㅋㅋ",
          "original_form": "물놀이 좋았겠네요 부럽당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009068.1.12",
          "form": "국내여행 어디가 젤좋으셨나요 추천좀해주세요",
          "original_form": "국내여행 어디가 젤좋으셨나요 추천좀해주세요",
          "speaker_id": "1",
          "time": "20210822 21:33"
        },
        {
          "id": "MDRW2100009068.1.13",
          "form": "나중에라도 가보게요 ㅎㅎ",
          "original_form": "나중에라도 가보게요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:33"
        },
        {
          "id": "MDRW2100009068.1.14",
          "form": "친구랑 부산여행 갔었는데 부산도 볼게 엄청 많더라고요",
          "original_form": "친구랑 부산여행 갔었는데 부산도 볼게 엄청 많더라고요",
          "speaker_id": "2",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009068.1.15",
          "form": "애 생기시면 변산반도 격포 해수욕장 추천드려요^^",
          "original_form": "애 생기시면 변산반도 격포 해수욕장 추천드려요^^",
          "speaker_id": "2",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009068.1.16",
          "form": "오오 좋은곳 추천 감사합니다",
          "original_form": "오오 좋은곳 추천 감사합니다",
          "speaker_id": "1",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100009068.1.17",
          "form": "전라북도네요",
          "original_form": "전라북도네요",
          "speaker_id": "1",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100009068.1.18",
          "form": "넘 멀지만 꼭 가보겠습니다",
          "original_form": "넘 멀지만 꼭 가보겠습니다",
          "speaker_id": "1",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100009068.1.19",
          "form": "네~ 저도 2시간 가량 가야하지만 여기가 사람이 적어서 좋더라고요",
          "original_form": "네~ 저도 2시간 가량 가야하지만 여기가 사람이 적어서 좋더라고요",
          "speaker_id": "2",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100009068.1.20",
          "form": "아 그런 포인트가",
          "original_form": "아 그런 포인트가",
          "speaker_id": "1",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009068.1.21",
          "form": "저는 부산사람이라 다른해수욕장 잘 안가게되던데",
          "original_form": "저는 부산사람이라 다른해수욕장 잘 안가게되던데",
          "speaker_id": "1",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009068.1.22",
          "form": "변산반도를 꼭 가봐야겠어요",
          "original_form": "변산반도를 꼭 가봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009068.1.23",
          "form": "앗!!! 저 최근에 해운대 갔었는데 사람이 진짜 엄청나게 많던데요",
          "original_form": "앗!!! 저 최근에 해운대 갔었는데 사람이 진짜 엄청나게 많던데요",
          "speaker_id": "2",
          "time": "20210822 21:50"
        },
        {
          "id": "MDRW2100009068.1.24",
          "form": "제가 부산사람에게 부산 여행을 추천했군요 이런..",
          "original_form": "제가 부산사람에게 부산 여행을 추천했군요 이런..",
          "speaker_id": "2",
          "time": "20210822 21:50"
        },
        {
          "id": "MDRW2100009068.1.25",
          "form": "아니에요 저도 간지 넘 오래돼서 몰라용 ㅋㅋ",
          "original_form": "아니에요 저도 간지 넘 오래돼서 몰라용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100009068.1.26",
          "form": "해운대 해수욕장 닫았는데도 사람많은가보네요ㅜㅜ",
          "original_form": "해운대 해수욕장 닫았는데도 사람많은가보네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100009068.1.27",
          "form": "부산이 공원도 예쁘고 동백섬도 좋고 여기서 일이년 살았음 좋겠다 싶더라고요",
          "original_form": "부산이 공원도 예쁘고 동백섬도 좋고 여기서 일이년 살았음 좋겠다 싶더라고요",
          "speaker_id": "2",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100009068.1.28",
          "form": "도시인데 조금만 나가면 바다도 보이고",
          "original_form": "도시인데 조금만 나가면 바다도 보이고",
          "speaker_id": "2",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100009068.1.29",
          "form": "인프라가좋은데 사람은 서울보다 적고 자연경관도 좋은동네에요",
          "original_form": "인프라가좋은데 사람은 서울보다 적고 자연경관도 좋은동네에요",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009068.1.30",
          "form": "일자리가 없는거 빼고",
          "original_form": "일자리가 없는거 빼고",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009068.1.31",
          "form": "두둥!",
          "original_form": "두둥!",
          "speaker_id": "1",
          "time": "20210822 22:09"
        }
      ]
