      "utterance": [
        {
          "id": "MDRW2100042879.1.1",
          "form": "편의점 샌드위치 먹었어용",
          "original_form": "편의점 샌드위치 먹었어용",
          "speaker_id": "2",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042879.1.2",
          "form": "잘했어용",
          "original_form": "잘했어용",
          "speaker_id": "1",
          "time": "20210907 10:43"
        },
        {
          "id": "MDRW2100042879.1.3",
          "form": "전 시리얼먹었어요",
          "original_form": "전 시리얼먹었어요",
          "speaker_id": "1",
          "time": "20210907 10:43"
        },
        {
          "id": "MDRW2100042879.1.4",
          "form": "저 방금 떡도 먹었어요 어떤 분 딸이 cpa합격했다고 떡 돌렸대요",
          "original_form": "저 방금 떡도 먹었어요 어떤 분 딸이 cpa합격했다고 떡 돌렸대요",
          "speaker_id": "2",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100042879.1.5",
          "form": "와우.. 부럽네요",
          "original_form": "와우.. 부럽네요",
          "speaker_id": "1",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100042879.1.6",
          "form": "떡돌릴만하다",
          "original_form": "떡돌릴만하다",
          "speaker_id": "1",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100042879.1.7",
          "form": "그니깐요  ㅋㅋㅋㅋ 근데 이게 무슨떡인지 몰겠어요",
          "original_form": "그니깐요  ㅋㅋㅋㅋ 근데 이게 무슨떡인지 몰겠어요",
          "speaker_id": "2",
          "time": "20210907 10:48"
        },
        {
          "id": "MDRW2100042879.1.8",
          "form": "약간 떡에 공기가 많이 들어간 느낌인데",
          "original_form": "약간 떡에 공기가 많이 들어간 느낌인데",
          "speaker_id": "2",
          "time": "20210907 10:49"
        },
        {
          "id": "MDRW2100042879.1.9",
          "form": "안에 팥앙금들어있어요",
          "original_form": "안에 팥앙금들어있어요",
          "speaker_id": "2",
          "time": "20210907 10:49"
        },
        {
          "id": "MDRW2100042879.1.10",
          "form": "오... 떡고가 좋아할 스타일이네요",
          "original_form": "오... 떡고가 좋아할 스타일이네요",
          "speaker_id": "1",
          "time": "20210907 10:49"
        },
        {
          "id": "MDRW2100042879.1.11",
          "form": "아 바람떡인가보다",
          "original_form": "아 바람떡인가보다",
          "speaker_id": "1",
          "time": "20210907 10:49"
        },
        {
          "id": "MDRW2100042879.1.12",
          "form": "아!! 와 이름이랑 딱이네요 ㅋㅋㅋㅋㅋ바람떡ㅋㅋㅋㅋ 공기반 떡반ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아!! 와 이름이랑 딱이네요 ㅋㅋㅋㅋㅋ바람떡ㅋㅋㅋㅋ 공기반 떡반ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:49"
        },
        {
          "id": "MDRW2100042879.1.13",
          "form": "그쵸 ㅋㅋㅋ 공기반 떡반 ㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋ 공기반 떡반 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:52"
        },
        {
          "id": "MDRW2100042879.1.14",
          "form": "근데 떡고는 떡도 유기농 거리면서 유난떨며 먹을것같네요",
          "original_form": "근데 떡고는 떡도 유기농 거리면서 유난떨며 먹을것같네요",
          "speaker_id": "1",
          "time": "20210907 10:52"
        },
        {
          "id": "MDRW2100042879.1.15",
          "form": "맞아요 오래 건강히 살 거 같아요 ㅎㅎ",
          "original_form": "맞아요 오래 건강히 살 거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042879.1.16",
          "form": "오늘 점심메뉴는 뭐에용?",
          "original_form": "오늘 점심메뉴는 뭐에용?",
          "speaker_id": "1",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042879.1.17",
          "form": "근데 제왑 구내식당 진짜 함 가보고 싶네요",
          "original_form": "근데 제왑 구내식당 진짜 함 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042879.1.18",
          "form": "저두요ㅋㅋㅋㅋㅋ궁금해요",
          "original_form": "저두요ㅋㅋㅋㅋㅋ궁금해요",
          "speaker_id": "1",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042879.1.19",
          "form": "밥 얼마나 좋은걸로 잘나올지 ㅋㅋㅋ",
          "original_form": "밥 얼마나 좋은걸로 잘나올지 ㅋㅋㅋ",
