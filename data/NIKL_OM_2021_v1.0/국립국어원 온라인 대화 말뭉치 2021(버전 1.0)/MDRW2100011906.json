        },
        {
          "id": "MDRW2100011906.1.3",
          "form": "나중에 직관가고싶어요",
          "original_form": "나중에 직관가고싶어요",
          "speaker_id": "2",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011906.1.4",
          "form": "헉 저도요",
          "original_form": "헉 저도요",
          "speaker_id": "1",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011906.1.5",
          "form": "김연경 선수 경기 한번 꼭 보고싶은데",
          "original_form": "김연경 선수 경기 한번 꼭 보고싶은데",
          "speaker_id": "1",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011906.1.6",
          "form": "해외리그 가시는거 아쉬워요",
          "original_form": "해외리그 가시는거 아쉬워요",
          "speaker_id": "1",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011906.1.7",
          "form": "ㅠㅠㅠ그니까요",
          "original_form": "ㅠㅠㅠ그니까요",
          "speaker_id": "2",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100011906.1.8",
          "form": "왜늦게입덕해서..",
          "original_form": "왜늦게입덕해서..",
          "speaker_id": "2",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100011906.1.9",
          "form": "김연경선수는 아쉽게 됐지만 김희진 선수 경기는 꼭 가보려구요!",
          "original_form": "김연경선수는 아쉽게 됐지만 김희진 선수 경기는 꼭 가보려구요!",
          "speaker_id": "1",
          "time": "20210824 17:29"
        },
        {
