      "utterance": [
        {
          "id": "MDRW2100040630.1.1",
          "form": "그니까요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040630.1.2",
          "form": "경험해보고 싶은데 체력이 딸려서 될까 모르겠어요 ㅋㅋㅋㅋ",
          "original_form": "경험해보고 싶은데 체력이 딸려서 될까 모르겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040630.1.3",
          "form": "체력이 안되서 못갈거같아요 ㅋㄱㅋ",
          "original_form": "체력이 안되서 못갈거같아요 ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040630.1.4",
          "form": "뛰면서 보기에는",
          "original_form": "뛰면서 보기에는",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040630.1.5",
          "form": "저질체력이라",
          "original_form": "저질체력이라",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040630.1.6",
          "form": "ㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100040630.1.7",
          "form": "근데 콘서트는 티켓팅 치열하다는데",
          "original_form": "근데 콘서트는 티켓팅 치열하다는데",
          "speaker_id": "1",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100040630.1.8",
          "form": "다 성공하신거예요?",
          "original_form": "다 성공하신거예요?",
          "speaker_id": "1",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100040630.1.9",
          "form": "ㅋㅋㅋㅋ 저도 체력 생각하면 ㅠㅠㅋㅋㅋㅋ큐",
          "original_form": "ㅋㅋㅋㅋ 저도 체력 생각하면 ㅠㅠㅋㅋㅋㅋ큐",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100040630.1.10",
          "form": "티켓팅 진짜 토나와요..",
          "original_form": "티켓팅 진짜 토나와요..",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100040630.1.11",
          "form": "근데 가고싶으니까 해야조 ㅠㅠㅠㅠ",
          "original_form": "근데 가고싶으니까 해야조 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100040630.1.12",
          "form": "다 성공하긴 하는데",
          "original_form": "다 성공하긴 하는데",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100040630.1.13",
          "form": "와 대단하네요",
          "original_form": "와 대단하네요",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040630.1.14",
          "form": "친구들 싹다 같이해요",
          "original_form": "친구들 싹다 같이해요",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040630.1.15",
          "form": "혼자는 답없어요",
          "original_form": "혼자는 답없어요",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040630.1.16",
          "form": "제아이디랑 비번 뿌리고",
          "original_form": "제아이디랑 비번 뿌리고",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040630.1.17",
          "form": "친구들은 부탁해서 같이하는거예요?",
          "original_form": "친구들은 부탁해서 같이하는거예요?",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040630.1.18",
          "form": "아 ㅋㄱㅋㄱㄱㅋ",
          "original_form": "아 ㅋㄱㅋㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040630.1.19",
          "form": "잡는 사람 밥 산다",
          "original_form": "잡는 사람 밥 산다",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040630.1.20",
          "form": "이렇게하는거죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이렇게하는거죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040630.1.21",
          "form": "저처럼 손느리면",
          "original_form": "저처럼 손느리면",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040630.1.22",
          "form": "제가 잡은적이 대부분이긴한데",
          "original_form": "제가 잡은적이 대부분이긴한데",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040630.1.23",
          "form": "엄두도 못내겠네요",
          "original_form": "엄두도 못내겠네요",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040630.1.24",
          "form": "그래듀 있어야 그나마 마음이 편해요",
          "original_form": "그래듀 있어야 그나마 마음이 편해요",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040630.1.25",
          "form": "아니에요 완전 운이에요",
          "original_form": "아니에요 완전 운이에요",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040630.1.26",
          "form": "속도랑은 거의 상관 없는데",
          "original_form": "속도랑은 거의 상관 없는데",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040630.1.27",
          "form": "워낙 사람이 몰라니까 로딩이 계속 되거든요",
          "original_form": "워낙 사람이 몰라니까 로딩이 계속 되거든요",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040630.1.28",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040630.1.29",
          "form": "네 같은 공간에서 같은 와이파이로 해도",
          "original_form": "네 같은 공간에서 같은 와이파이로 해도",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040630.1.30",
          "form": "아예 안들어가지는게 있고 들어가지는게 있고 그래요",
          "original_form": "아예 안들어가지는게 있고 들어가지는게 있고 그래요",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040630.1.31",
          "form": "치열하네요ㄷㄷㄷ",
          "original_form": "치열하네요ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210904 23:29"
        }
      ]
    }
