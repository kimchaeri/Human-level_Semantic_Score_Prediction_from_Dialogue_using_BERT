      "utterance": [
        {
          "id": "MDRW2100013024.1.1",
          "form": "결혼하셧거나 연애중이신가요?",
          "original_form": "결혼하셧거나 연애중이신가요?",
          "speaker_id": "1",
          "time": "20210825 11:12"
        },
        {
          "id": "MDRW2100013024.1.2",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20210825 11:12"
        },
        {
          "id": "MDRW2100013024.1.3",
          "form": "저는 결혼했어요",
          "original_form": "저는 결혼했어요",
          "speaker_id": "2",
          "time": "20210825 11:12"
        },
        {
          "id": "MDRW2100013024.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 11:12"
        },
        {
          "id": "MDRW2100013024.1.5",
          "form": "저는 솔로입니다 !!",
          "original_form": "저는 솔로입니다 !!",
          "speaker_id": "1",
          "time": "20210825 11:13"
        },
        {
          "id": "MDRW2100013024.1.6",
          "form": "결혼한지 얼마나 되셧어요",
          "original_form": "결혼한지 얼마나 되셧어요",
          "speaker_id": "1",
          "time": "20210825 11:13"
        },
        {
          "id": "MDRW2100013024.1.7",
          "form": "오 빛이나는 쏠로",
          "original_form": "오 빛이나는 쏠로{emoji:♡}",
          "speaker_id": "2",
          "time": "20210825 11:13"
        },
        {
          "id": "MDRW2100013024.1.8",
          "form": "저는 올해로 7년차에요:)",
          "original_form": "저는 올해로 7년차에요:)",
          "speaker_id": "2",
          "time": "20210825 11:13"
        },
        {
          "id": "MDRW2100013024.1.9",
          "form": "name1님은 자만추세요?",
          "original_form": "&name1&님은 자만추세요?",
          "speaker_id": "2",
          "time": "20210825 11:13"
        },
        {
          "id": "MDRW2100013024.1.10",
          "form": "넵 !! 근데 요즘 코로나라서 만나기가 꺼려지네요 ㅜㅜ",
          "original_form": "넵 !! 근데 요즘 코로나라서 만나기가 꺼려지네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 11:14"
        },
        {
          "id": "MDRW2100013024.1.11",
          "form": "연애 오래 하시다가 하신건가요?",
          "original_form": "연애 오래 하시다가 하신건가요?",
          "speaker_id": "1",
          "time": "20210825 11:14"
        },
        {
          "id": "MDRW2100013024.1.12",
          "form": "그쵸 ㅜㅜ 코시국이라 참...",
          "original_form": "그쵸 ㅜㅜ 코시국이라 참...",
          "speaker_id": "2",
          "time": "20210825 11:15"
        },
        {
          "id": "MDRW2100013024.1.13",
          "form": "연애는 1년 조금 안되게하고\r\n결혼했어요:)",
          "original_form": "연애는 1년 조금 안되게하고\r\n결혼했어요:)",
          "speaker_id": "2",
          "time": "20210825 11:15"
        },
        {
          "id": "MDRW2100013024.1.14",
          "form": "오오 소개팅으로 만나셨나요??",
          "original_form": "오오 소개팅으로 만나셨나요??",
          "speaker_id": "1",
          "time": "20210825 11:16"
        },
        {
          "id": "MDRW2100013024.1.15",
          "form": "아뇽ㅋㅋ 저는 자만추입니다ㅋㅋ",
          "original_form": "아뇽ㅋㅋ 저는 자만추입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:16"
        },
        {
          "id": "MDRW2100013024.1.16",
          "form": "소개팅은 한번도 안해보셧나요??",
          "original_form": "소개팅은 한번도 안해보셧나요??",
          "speaker_id": "1",
          "time": "20210825 11:16"
        },
        {
          "id": "MDRW2100013024.1.17",
          "form": "대학동기인데 썸타다 군대가고\r\n안뇽빠빠이 되었다가 \r\n20대 후반에 다시 재회ㅋㅋ",
          "original_form": "대학동기인데 썸타다 군대가고\r\n안뇽빠빠이 되었다가 \r\n20대 후반에 다시 재회ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:17"
        },
        {
          "id": "MDRW2100013024.1.18",
          "form": "소개팅 당연 해봤죠ㅋㅋ",
          "original_form": "소개팅 당연 해봤죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:17"
        },
        {
          "id": "MDRW2100013024.1.19",
          "form": "소개팅으로 만나서 사귄적도 있지만\r\n자만추가 더 좋더라구요",
          "original_form": "소개팅으로 만나서 사귄적도 있지만\r\n자만추가 더 좋더라구요",
          "speaker_id": "2",
          "time": "20210825 11:17"
        },
        {
          "id": "MDRW2100013024.1.20",
          "form": "우아 완전 인연이네요!",
          "original_form": "우아 완전 인연이네요!",
          "speaker_id": "1",
          "time": "20210825 11:19"
        },
        {
          "id": "MDRW2100013024.1.21",
          "form": "몇살때 결혼 하신거에용?",
          "original_form": "몇살때 결혼 하신거에용?",
          "speaker_id": "1",
          "time": "20210825 11:19"
        },
        {
          "id": "MDRW2100013024.1.22",
          "form": "ㅋㅋ28살때 만나서\r\n29살에 결혼했어요ㅋㅋ",
          "original_form": "ㅋㅋ28살때 만나서\r\n29살에 결혼했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:20"
        },
        {
          "id": "MDRW2100013024.1.23",
          "form": "우아 저도 결혼 빨리 하고싶어요!!",
