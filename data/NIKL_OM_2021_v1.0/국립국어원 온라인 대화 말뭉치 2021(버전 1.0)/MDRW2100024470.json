      "utterance": [
        {
          "id": "MDRW2100024470.1.1",
          "form": "혹시 반려동물키우시나요?",
          "original_form": "혹시 반려동물키우시나요?",
          "speaker_id": "1",
          "time": "20210831 10:56"
        },
        {
          "id": "MDRW2100024470.1.2",
          "form": "아니요 반려동물 키우지는 않고",
          "original_form": "아니요 반려동물 키우지는 않고",
          "speaker_id": "2",
          "time": "20210831 11:08"
        },
        {
          "id": "MDRW2100024470.1.3",
          "form": "고양이 좋아해요",
          "original_form": "고양이 좋아해요",
          "speaker_id": "2",
          "time": "20210831 11:08"
        },
        {
          "id": "MDRW2100024470.1.4",
          "form": "거북이 키우려고 생각하고 있어요",
          "original_form": "거북이 키우려고 생각하고 있어요",
          "speaker_id": "2",
          "time": "20210831 11:08"
        },
        {
          "id": "MDRW2100024470.1.5",
          "form": "거북! 실제로 키우기에는 고양이 보다 쉬울것 같은데",
          "original_form": "거북! 실제로 키우기에는 고양이 보다 쉬울것 같은데",
          "speaker_id": "1",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100024470.1.6",
          "form": "네 ㅎㅎ 아이가 계속 무언가를 키우고 싶어해서",
          "original_form": "네 ㅎㅎ 아이가 계속 무언가를 키우고 싶어해서",
          "speaker_id": "2",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100024470.1.7",
          "form": "일단 손 덜가는 거북이부터 키워보자고",
          "original_form": "일단 손 덜가는 거북이부터 키워보자고",
          "speaker_id": "2",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100024470.1.8",
          "form": "계속 고양이 키우자고 하는데 아직 힘들어서요 ㅠ",
          "original_form": "계속 고양이 키우자고 하는데 아직 힘들어서요 ㅠ",
          "speaker_id": "2",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100024470.1.9",
          "form": "반려동물 있으세요??",
          "original_form": "반려동물 있으세요??",
          "speaker_id": "2",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100024470.1.10",
          "form": "전 퇴근이 늦어서 많이 신경을 못쓰겠더라구요",
          "original_form": "전 퇴근이 늦어서 많이 신경을 못쓰겠더라구요",
          "speaker_id": "1",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100024470.1.11",
          "form": "그래서 유튭렌선으로 만족하고 있어요ㅎㅎ",
          "original_form": "그래서 유튭렌선으로 만족하고 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100024470.1.12",
          "form": "ㅎㅎ 렌선좋네요 ㅎㅎ",
          "original_form": "ㅎㅎ 렌선좋네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 12:42"
        },
        {
          "id": "MDRW2100024470.1.13",
          "form": "어떤 동물 좋아하세요??",
          "original_form": "어떤 동물 좋아하세요??",
          "speaker_id": "2",
          "time": "20210831 12:42"
        },
        {
          "id": "MDRW2100024470.1.14",
          "form": "개를 제일 좋아하는데 유튜브는 고양이보고 있네요ㅋㅋ",
          "original_form": "개를 제일 좋아하는데 유튜브는 고양이보고 있네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:42"
        },
        {
          "id": "MDRW2100024470.1.15",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ 개냥이 키우고 싶어요",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ 개냥이 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210831 12:46"
        },
        {
          "id": "MDRW2100024470.1.16",
          "form": "너무 도도한 고양이보다는 ㅎㅎ",
          "original_form": "너무 도도한 고양이보다는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 12:46"
        },
        {
          "id": "MDRW2100024470.1.17",
          "form": "개냥이 최고죠!",
          "original_form": "개냥이 최고죠!",
          "speaker_id": "1",
          "time": "20210831 12:46"
        },
        {
          "id": "MDRW2100024470.1.18",
          "form": "네네 개냥이 ^^",
          "original_form": "네네 개냥이 ^^",
          "speaker_id": "2",
          "time": "20210831 12:48"
        },
        {
          "id": "MDRW2100024470.1.19",
          "form": "털 빠짐을 감수하고",
          "original_form": "털 빠짐을 감수하고",
          "speaker_id": "2",
          "time": "20210831 12:48"
        },
        {
          "id": "MDRW2100024470.1.20",
          "form": "개냥이를 키우고 싶은 마음이 하루에도 몇번씩 드네요",
          "original_form": "개냥이를 키우고 싶은 마음이 하루에도 몇번씩 드네요",
          "speaker_id": "2",
          "time": "20210831 12:49"
        },
        {
          "id": "MDRW2100024470.1.21",
          "form": "털이 엄청 빠지더라구요",
          "original_form": "털이 엄청 빠지더라구요",
          "speaker_id": "1",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100024470.1.22",
          "form": "네 털 때문에 고민이예요",
          "original_form": "네 털 때문에 고민이예요",
          "speaker_id": "2",
          "time": "20210831 13:24"
        },
        {
          "id": "MDRW2100024470.1.23",
          "form": "한번도 반려동물을 키워본적이 없어서",
          "original_form": "한번도 반려동물을 키워본적이 없어서",
          "speaker_id": "2",
          "time": "20210831 13:25"
        },
        {
          "id": "MDRW2100024470.1.24",
          "form": "잘키울수 있을까 걱정도 되고",
          "original_form": "잘키울수 있을까 걱정도 되고",
          "speaker_id": "2",
          "time": "20210831 13:25"
        },
        {
          "id": "MDRW2100024470.1.25",
          "form": "유튭으로 보는데 키우는건 정말 큰 용기가 필요하더라구요",
          "original_form": "유튭으로 보는데 키우는건 정말 큰 용기가 필요하더라구요",
          "speaker_id": "1",
          "time": "20210831 13:26"
        },
        {
          "id": "MDRW2100024470.1.26",
          "form": "돈도 그렇구요",
          "original_form": "돈도 그렇구요",
          "speaker_id": "1",
          "time": "20210831 13:26"
        }
      ]
