      "utterance": [
        {
          "id": "MDRW2100019770.1.1",
          "form": "종교있으신가요?",
          "original_form": "종교있으신가요?",
          "speaker_id": "1",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100019770.1.2",
          "form": "저는 기독교예요 종교 있으세요?",
          "original_form": "저는 기독교예요 종교 있으세요?",
          "speaker_id": "2",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100019770.1.3",
          "form": "아 저는 천주교 세례를받긴했는데",
          "original_form": "아 저는 천주교 세례를받긴했는데",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100019770.1.4",
          "form": "재미로 성당다닌달까..",
          "original_form": "재미로 성당다닌달까..",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100019770.1.5",
          "form": "성가대 노래부르러다녔더든요",
          "original_form": "성가대 노래부르러다녔더든요",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100019770.1.6",
          "form": "와 신기해요!",
          "original_form": "와 신기해요!",
          "speaker_id": "2",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100019770.1.7",
          "form": "그럼....종교가...취미?",
          "original_form": "그럼....종교가...취미?",
          "speaker_id": "2",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100019770.1.8",
          "form": "약간...그런건가요?!ㅋㅋㅎㅎ",
          "original_form": "약간...그런건가요?!ㅋㅋㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019770.1.9",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019770.1.10",
          "form": "노래만 부르러갔어요 화음넣고하는게 잼써서요",
          "original_form": "노래만 부르러갔어요 화음넣고하는게 잼써서요",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019770.1.11",
          "form": "모태신앙이세요?",
          "original_form": "모태신앙이세요?",
          "speaker_id": "1",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019770.1.12",
          "form": "네! 저 모태신앙이에요!ㅋㅋㅋ",
          "original_form": "네! 저 모태신앙이에요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019770.1.13",
          "form": "성가대 화음 넣는 거 넘 신기해요",
          "original_form": "성가대 화음 넣는 거 넘 신기해요",
          "speaker_id": "2",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019770.1.14",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019770.1.15",
          "form": "부같은거 안하셧나요?",
          "original_form": "부같은거 안하셧나요?",
          "speaker_id": "1",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100019770.1.16",
          "form": "전례부이런거 있지않나요~",
          "original_form": "전례부이런거 있지않나요~",
          "speaker_id": "1",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100019770.1.17",
          "form": "네! 그런 부가 따로 없었어요!!ㅋㅋㅋ",
          "original_form": "네! 그런 부가 따로 없었어요!!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100019770.1.18",
          "form": "있어도 몰랐을 거 같아요 너무 가볍게 다녀서 ㅋㅋ",
          "original_form": "있어도 몰랐을 거 같아요 너무 가볍게 다녀서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100019770.1.19",
          "form": "오오 그렇군요",
          "original_form": "오오 그렇군요",
          "speaker_id": "1",
          "time": "20210828 12:52"
        },
        {
          "id": "MDRW2100019770.1.20",
          "form": "미사만드리는건가요?",
          "original_form": "미사만드리는건가요?",
          "speaker_id": "1",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100019770.1.21",
          "form": "네 예배요!ㅋㅋㅋㅎㅎ 그런 부서가 있는 걸 알려주셔서 알았네요...신기!",
          "original_form": "네 예배요!ㅋㅋㅋㅎㅎ 그런 부서가 있는 걸 알려주셔서 알았네요...신기!",
          "speaker_id": "2",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100019770.1.22",
          "form": "오오그렇군요",
          "original_form": "오오그렇군요",
          "speaker_id": "1",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100019770.1.23",
          "form": "그럼언제예배가요?",
          "original_form": "그럼언제예배가요?",
          "speaker_id": "1",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100019770.1.24",
          "form": "일요일 예배인데 코로나이후에는 한 번도 안갔어요",
          "original_form": "일요일 예배인데 코로나이후에는 한 번도 안갔어요",
          "speaker_id": "2",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100019770.1.25",
          "form": "못가겠더라고요ㅡㅜㅋㅋㅋ",
          "original_form": "못가겠더라고요ㅡㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100019770.1.26",
          "form": "아하그초",
          "original_form": "아하그초",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100019770.1.27",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100019770.1.28",
          "form": "저도 안갔어요",
          "original_form": "저도 안갔어요",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100019770.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100019770.1.30",
          "form": "안가는게 최고죠..",
          "original_form": "안가는게 최고죠..",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100019770.1.31",
          "form": "맞아요!ㅠㅠ 어디서 확진자가 나올지 모르기때문에요ㅠㅠ",
