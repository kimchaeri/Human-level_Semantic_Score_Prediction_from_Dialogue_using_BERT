          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100006219.1.3",
          "form": "이번편은좀재미없어요",
          "original_form": "이번편은좀재미없어요",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100006219.1.4",
          "form": "안봐야지",
          "original_form": "안봐야지",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006219.1.5",
          "form": "고마워요",
          "original_form": "고마워요",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006219.1.6",
          "form": "아직안봤군요",
          "original_form": "아직안봤군요",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006219.1.7",
          "form": "오늘본줄알았어요",
          "original_form": "오늘본줄알았어요",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006219.1.8",
          "form": "전 치사하게",
          "original_form": "전 치사하게",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006219.1.9",
          "form": "혼자 안봐요",
          "original_form": "혼자 안봐요",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006219.1.10",
          "form": "누가 술취해서 곯아떨어졌어요",
          "original_form": "누가 술취해서 곯아떨어졌어요",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006219.1.11",
          "form": "그래서혼자봤어요",
          "original_form": "그래서혼자봤어요",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100006219.1.12",
          "form": "치사대마왕이시군요?",
          "original_form": "치사대마왕이시군요?",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100006219.1.13",
          "form": "술취하지않도록하세요",
          "original_form": "술취하지않도록하세요",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100006219.1.14",
          "form": "런닝맨은 안하나요",
          "original_form": "런닝맨은 안하나요",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100006219.1.15",
          "form": "아맞다",
          "original_form": "아맞다",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100006219.1.16",
          "form": "근데 이따 다른거봐야돼요",
          "original_form": "근데 이따 다른거봐야돼요",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100006219.1.17",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006219.1.18",
          "form": "런닝맨혼자봐도돼요?",
          "original_form": "런닝맨혼자봐도돼요?",
          "speaker_id": "2",
          "time": "20210907 22:01"
