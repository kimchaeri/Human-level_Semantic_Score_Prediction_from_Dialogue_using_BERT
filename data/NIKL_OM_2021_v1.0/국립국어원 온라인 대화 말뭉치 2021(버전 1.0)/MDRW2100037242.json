        },
        {
          "id": "MDRW2100037242.1.2",
          "form": "카푸치노 좋아해?",
          "original_form": "카푸치노 좋아해?",
          "speaker_id": "1",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100037242.1.3",
          "form": "별로 안 마셔봤어",
          "original_form": "별로 안 마셔봤어",
          "speaker_id": "2",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100037242.1.4",
          "form": "시나몬가루는?",
          "original_form": "시나몬가루는?",
          "speaker_id": "1",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100037242.1.5",
          "form": "시나몬은 좋아",
          "original_form": "시나몬은 좋아",
          "speaker_id": "2",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037242.1.6",
          "form": "카푸치노에 뿌려먹으면 꿀맛",
          "original_form": "카푸치노에 뿌려먹으면 꿀맛",
          "speaker_id": "1",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037242.1.7",
          "form": "폴바셋 카푸치노로맛있어",
          "original_form": "폴바셋 카푸치노로맛있어",
          "speaker_id": "1",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037242.1.8",
          "form": "그게 그거구나",
          "original_form": "그게 그거구나",
          "speaker_id": "2",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100037242.1.9",
          "form": "폴바셋꺼 먹어봤어?",
          "original_form": "폴바셋꺼 먹어봤어?",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100037242.1.10",
          "form": "아니 안 먹어 봤네",
          "original_form": "아니 안 먹어 봤네",
          "speaker_id": "2",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100037242.1.11",
          "form": "폴바셋은 우유가맛있어",
          "original_form": "폴바셋은 우유가맛있어",
          "speaker_id": "1",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037242.1.12",
          "form": "소화 잘 되는 우유가 있는 거 알아",
          "original_form": "소화 잘 되는 우유가 있는 거 알아",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037242.1.13",
          "form": "아이스크림라떼도맛있어",
          "original_form": "아이스크림라떼도맛있어",
          "speaker_id": "1",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037242.1.14",
          "form": "그게 제일 맛있는 거 같아",
          "original_form": "그게 제일 맛있는 거 같아",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037242.1.15",
          "form": "폴바셋이 가까이있음좋겠다",
          "original_form": "폴바셋이 가까이있음좋겠다",
          "speaker_id": "1",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100037242.1.16",
          "form": "내 집 근처는 있는데",
          "original_form": "내 집 근처는 있는데",
          "speaker_id": "2",
