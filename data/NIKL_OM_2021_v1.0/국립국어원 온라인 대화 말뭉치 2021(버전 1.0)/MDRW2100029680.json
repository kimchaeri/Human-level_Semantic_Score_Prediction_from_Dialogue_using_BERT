      "utterance": [
        {
          "id": "MDRW2100029680.1.1",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "1",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029680.1.2",
          "form": "아뇨 직장인요",
          "original_form": "아뇨 직장인요",
          "speaker_id": "2",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029680.1.3",
          "form": "오 멋있어요",
          "original_form": "오 멋있어요",
          "speaker_id": "1",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029680.1.4",
          "form": "전 대학생이에요",
          "original_form": "전 대학생이에요",
          "speaker_id": "1",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029680.1.5",
          "form": "학교라..",
          "original_form": "학교라..",
          "speaker_id": "2",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029680.1.6",
          "form": "오 무슨전공인가요",
          "original_form": "오 무슨전공인가요",
          "speaker_id": "2",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029680.1.7",
          "form": "저는 애니메이션이요",
          "original_form": "저는 애니메이션이요",
          "speaker_id": "2",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029680.1.8",
          "form": "화학이요!",
          "original_form": "화학이요!",
          "speaker_id": "1",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029680.1.9",
          "form": "와 멋있어요",
          "original_form": "와 멋있어요",
          "speaker_id": "1",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029680.1.10",
          "form": "영상제작이나 그림 그리시는건가요?",
          "original_form": "영상제작이나 그림 그리시는건가요?",
          "speaker_id": "1",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029680.1.11",
          "form": "화학이라니 벌써 어렵군요",
          "original_form": "화학이라니 벌써 어렵군요",
          "speaker_id": "2",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029680.1.12",
          "form": "네 직업이 그래요^^",
          "original_form": "네 직업이 그래요^^",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029680.1.13",
          "form": "이모티콘도 만들고요",
          "original_form": "이모티콘도 만들고요",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029680.1.14",
          "form": "대학생이시니 좋군요!",
          "original_form": "대학생이시니 좋군요!",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029680.1.15",
          "form": "와 이모티콘!!!",
          "original_form": "와 이모티콘!!!",
          "speaker_id": "1",
          "time": "20210903 11:49"
        },
        {
          "id": "MDRW2100029680.1.16",
          "form": "막학년이라 진로에 대한 고민이 많습니다ㅠ",
          "original_form": "막학년이라 진로에 대한 고민이 많습니다ㅠ",
          "speaker_id": "1",
          "time": "20210903 11:49"
        },
        {
          "id": "MDRW2100029680.1.17",
          "form": "아 맞아요 생각하는직업있나요",
          "original_form": "아 맞아요 생각하는직업있나요",
          "speaker_id": "2",
          "time": "20210903 11:49"
        },
        {
          "id": "MDRW2100029680.1.18",
          "form": "저도 고민많이했어요",
          "original_form": "저도 고민많이했어요",
          "speaker_id": "2",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100029680.1.19",
          "form": "반도체소재 연구직하고싶은데",
          "original_form": "반도체소재 연구직하고싶은데",
          "speaker_id": "1",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100029680.1.20",
          "form": "취준할지 대학원갈지 고민중이에요",
          "original_form": "취준할지 대학원갈지 고민중이에요",
          "speaker_id": "1",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100029680.1.21",
          "form": "화학이니 딱이네요",
          "original_form": "화학이니 딱이네요",
          "speaker_id": "2",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100029680.1.22",
          "form": "배움에 뜻이있다면 진학도",
          "original_form": "배움에 뜻이있다면 진학도",
          "speaker_id": "2",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100029680.1.23",
          "form": "저는 공부보단 일이었어요",
          "original_form": "저는 공부보단 일이었어요",
          "speaker_id": "2",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100029680.1.24",
          "form": "배움에 뜻이 있긴한데 대학원이 배움만보고 가긴 힘든곳이더라고요",
          "original_form": "배움에 뜻이 있긴한데 대학원이 배움만보고 가긴 힘든곳이더라고요",
          "speaker_id": "1",
          "time": "20210903 11:59"
        },
        {
          "id": "MDRW2100029680.1.25",
          "form": "굉장히 폐쇄적이에요...",
          "original_form": "굉장히 폐쇄적이에요...",
          "speaker_id": "1",
