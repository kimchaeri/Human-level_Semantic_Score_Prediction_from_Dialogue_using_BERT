      "utterance": [
        {
          "id": "MDRW2100020355.1.1",
          "form": "name2님 운동하는거 좋아하세요?",
          "original_form": "&name2&님 운동하는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020355.1.2",
          "form": "저 운동진짜안해서 몸이 안좋아요",
          "original_form": "저 운동진짜안해서 몸이 안좋아요",
          "speaker_id": "2",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020355.1.3",
          "form": "요즘 운동시작했어요",
          "original_form": "요즘 운동시작했어요",
          "speaker_id": "2",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020355.1.4",
          "form": "공원걷고뛰고하는데",
          "original_form": "공원걷고뛰고하는데",
          "speaker_id": "2",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020355.1.5",
          "form": "비가와서 일주일째 안가는중,,,",
          "original_form": "비가와서 일주일째 안가는중,,,",
          "speaker_id": "2",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020355.1.6",
          "form": "오!! 혹시 런데이..?",
          "original_form": "오!! 혹시 런데이..?",
          "speaker_id": "1",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020355.1.7",
          "form": "아 그것도 고민했는데",
          "original_form": "아 그것도 고민했는데",
          "speaker_id": "2",
          "time": "20210828 01:59"
        },
        {
          "id": "MDRW2100020355.1.8",
          "form": "하루해보니까 아직 그것도 못할몸이더라고요",
          "original_form": "하루해보니까 아직 그것도 못할몸이더라고요",
          "speaker_id": "2",
          "time": "20210828 01:59"
        },
        {
          "id": "MDRW2100020355.1.9",
          "form": "한곡을 못뛰어요,,,",
          "original_form": "한곡을 못뛰어요,,,",
          "speaker_id": "2",
          "time": "20210828 01:59"
        },
        {
          "id": "MDRW2100020355.1.10",
          "form": "런데이하세요???",
          "original_form": "런데이하세요???",
          "speaker_id": "2",
          "time": "20210828 01:59"
        },
        {
          "id": "MDRW2100020355.1.11",
          "form": "아이구ㅠㅠ 체력이 많이 약하신가부다",
          "original_form": "아이구ㅠㅠ 체력이 많이 약하신가부다",
          "speaker_id": "1",
          "time": "20210828 01:59"
        },
        {
          "id": "MDRW2100020355.1.12",
          "form": "아 저도 예전에 했었어요!",
          "original_form": "아 저도 예전에 했었어요!",
          "speaker_id": "1",
          "time": "20210828 01:59"
        },
        {
          "id": "MDRW2100020355.1.13",
          "form": "처음에는 30초뛰는거도 죽을거같았는데 나중되면 몇분도 뛰어지긴 하더라구요",
          "original_form": "처음에는 30초뛰는거도 죽을거같았는데 나중되면 몇분도 뛰어지긴 하더라구요",
          "speaker_id": "1",
          "time": "20210828 01:59"
        },
        {
          "id": "MDRW2100020355.1.14",
          "form": "물론 뛰고나면 죽을거같지만요^_^",
          "original_form": "물론 뛰고나면 죽을거같지만요^_^",
          "speaker_id": "1",
          "time": "20210828 02:00"
        },
        {
          "id": "MDRW2100020355.1.15",
          "form": "ㅋㅋㄱㅋㄱㄱㅋㅋㄱㄱ한달채우셨어요??",
          "original_form": "ㅋㅋㄱㅋㄱㄱㅋㅋㄱㄱ한달채우셨어요??",
          "speaker_id": "2",
          "time": "20210828 02:00"
        },
        {
          "id": "MDRW2100020355.1.16",
          "form": "8주 프로그램으로 기억하는데 5주, 6주쯤에서 때려쳤던거같아요",
          "original_form": "8주 프로그램으로 기억하는데 5주, 6주쯤에서 때려쳤던거같아요",
          "speaker_id": "1",
          "time": "20210828 02:01"
        },
        {
          "id": "MDRW2100020355.1.17",
          "form": "늦봄에 시작해서 여름을 만나버려서^_^",
          "original_form": "늦봄에 시작해서 여름을 만나버려서^_^",
          "speaker_id": "1",
          "time": "20210828 02:01"
        },
        {
          "id": "MDRW2100020355.1.18",
          "form": "와그래도 엄청많이하셨네요!",
          "original_form": "와그래도 엄청많이하셨네요!",
          "speaker_id": "2",
          "time": "20210828 02:01"
        },
        {
          "id": "MDRW2100020355.1.19",
          "form": "올여름은 못할 날씨,,,",
          "original_form": "올여름은 못할 날씨,,,",
          "speaker_id": "2",
          "time": "20210828 02:01"
        },
        {
          "id": "MDRW2100020355.1.20",
          "form": "대학땐진짜 미용목적 운동이었는데",
          "original_form": "대학땐진짜 미용목적 운동이었는데",
          "speaker_id": "2",
          "time": "20210828 02:02"
        },
        {
          "id": "MDRW2100020355.1.21",
          "form": "여러번 시도했는데 아직도 끝까지는 못해봤네요ㅋㅋㅋㅋ",
          "original_form": "여러번 시도했는데 아직도 끝까지는 못해봤네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:02"
        },
        {
          "id": "MDRW2100020355.1.22",
          "form": "이젠 그저건강목적이에요",
          "original_form": "이젠 그저건강목적이에요",
          "speaker_id": "2",
          "time": "20210828 02:02"
        },
        {
          "id": "MDRW2100020355.1.23",
          "form": "맞아요! 살기위해서ㅠㅠ",
          "original_form": "맞아요! 살기위해서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 02:02"
        },
        {
          "id": "MDRW2100020355.1.24",
          "form": "체력 회복되시면 런데이 도전해보는거 추천드립니당",
          "original_form": "체력 회복되시면 런데이 도전해보는거 추천드립니당",
          "speaker_id": "1",
          "time": "20210828 02:02"
        },
        {
          "id": "MDRW2100020355.1.25",
          "form": "이게 은근 성취감이 있더라고요??",
          "original_form": "이게 은근 성취감이 있더라고요??",
          "speaker_id": "1",
          "time": "20210828 02:02"
        },
        {
          "id": "MDRW2100020355.1.26",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "2",
          "time": "20210828 02:02"
        },
        {
          "id": "MDRW2100020355.1.27",
          "form": "5바퀴를도는 나",
          "original_form": "5바퀴를도는 나",
          "speaker_id": "2",
          "time": "20210828 02:03"
        },
        {
          "id": "MDRW2100020355.1.28",
          "form": "매일 나와서 나와의약속을 지키는 멋진나",
          "original_form": "매일 나와서 나와의약속을 지키는 멋진나",
          "speaker_id": "2",
          "time": "20210828 02:03"
        },
        {
          "id": "MDRW2100020355.1.29",
          "form": "공원에서 뛰는사람보면 뭔가 멋져보이잖아요ㅋㅋㅋㅋ",
          "original_form": "공원에서 뛰는사람보면 뭔가 멋져보이잖아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:03"
        }
