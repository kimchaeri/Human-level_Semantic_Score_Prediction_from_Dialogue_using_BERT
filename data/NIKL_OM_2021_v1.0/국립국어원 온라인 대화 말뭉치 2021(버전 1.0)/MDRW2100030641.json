      "utterance": [
        {
          "id": "MDRW2100030641.1.1",
          "form": "자취하시나요?",
          "original_form": "자취하시나요?",
          "speaker_id": "2",
          "time": "20210904 10:55"
        },
        {
          "id": "MDRW2100030641.1.2",
          "form": "어머니랑 둘이살아요",
          "original_form": "어머니랑 둘이살아요",
          "speaker_id": "1",
          "time": "20210904 10:56"
        },
        {
          "id": "MDRW2100030641.1.3",
          "form": "자취하고싶네요",
          "original_form": "자취하고싶네요",
          "speaker_id": "1",
          "time": "20210904 10:56"
        },
        {
          "id": "MDRW2100030641.1.4",
          "form": "프리덤",
          "original_form": "프리덤",
          "speaker_id": "1",
          "time": "20210904 10:57"
        },
        {
          "id": "MDRW2100030641.1.5",
          "form": "아항 회사는 집에서 가까우신가요?",
          "original_form": "아항 회사는 집에서 가까우신가요?",
          "speaker_id": "2",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100030641.1.6",
          "form": "지하철로 2정거장이네요",
          "original_form": "지하철로 2정거장이네요",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030641.1.7",
          "form": "오히려 운동하는곳이 멀어요ㄷㄷ",
          "original_form": "오히려 운동하는곳이 멀어요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030641.1.8",
          "form": "헐 운동을 멀리가세요?",
          "original_form": "헐 운동을 멀리가세요?",
          "speaker_id": "2",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030641.1.9",
          "form": "집이 합정인데 운동을 관악구가서 하네요ㅋㅋ",
          "original_form": "집이 합정인데 운동을 관악구가서 하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030641.1.10",
          "form": "헉 어떤 운동이시길래 그렇게 멀리가세요?",
          "original_form": "헉 어떤 운동이시길래 그렇게 멀리가세요?",
          "speaker_id": "2",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030641.1.11",
          "form": "검도해요ㅋㅋ",
          "original_form": "검도해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030641.1.12",
          "form": "아항ㅋㅋ집에서 집안일은 많이 하시나요?",
          "original_form": "아항ㅋㅋ집에서 집안일은 많이 하시나요?",
          "speaker_id": "2",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100030641.1.13",
          "form": "요즘 어무이가 아파서 거진 제가 다하네묘",
          "original_form": "요즘 어무이가 아파서 거진 제가 다하네묘",
          "speaker_id": "1",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100030641.1.14",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100030641.1.15",
          "form": "에궁 ㅜㅜ 그래도 그동안은 어머님께서 다 해주시던거니까",
          "original_form": "에궁 ㅜㅜ 그래도 그동안은 어머님께서 다 해주시던거니까",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100030641.1.16",
          "form": "그렇게 생각하면 또 마음아프더라구요",
          "original_form": "그렇게 생각하면 또 마음아프더라구요",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100030641.1.17",
          "form": "하지만 용돈과 재테크를 많이 해주고있어서",
          "original_form": "하지만 용돈과 재테크를 많이 해주고있어서",
          "speaker_id": "1",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030641.1.18",
          "form": "당당합니다",
