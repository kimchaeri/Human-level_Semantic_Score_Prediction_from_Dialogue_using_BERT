      "utterance": [
        {
          "id": "MDRW2100022404.1.1",
          "form": "요즘 사람들은 유투브 많이 보죠?",
          "original_form": "요즘 사람들은 유투브 많이 보죠?",
          "speaker_id": "2",
          "time": "20210829 13:34"
        },
        {
          "id": "MDRW2100022404.1.2",
          "form": "저는 유튜브보다는",
          "original_form": "저는 유튜브보다는",
          "speaker_id": "1",
          "time": "20210829 13:35"
        },
        {
          "id": "MDRW2100022404.1.3",
          "form": "티비프로 좋더라구요",
          "original_form": "티비프로 좋더라구요",
          "speaker_id": "1",
          "time": "20210829 13:35"
        },
        {
          "id": "MDRW2100022404.1.4",
          "form": "즐겨보는 프로그램있으신가요",
          "original_form": "즐겨보는 프로그램있으신가요",
          "speaker_id": "1",
          "time": "20210829 13:35"
        },
        {
          "id": "MDRW2100022404.1.5",
          "form": "그렇구나 저도 티비 주로 봐요",
          "original_form": "그렇구나 저도 티비 주로 봐요",
          "speaker_id": "2",
          "time": "20210829 13:36"
        },
        {
          "id": "MDRW2100022404.1.6",
          "form": "트롯 경연대회 위주로 봅니다",
          "original_form": "트롯 경연대회 위주로 봅니다",
          "speaker_id": "2",
          "time": "20210829 13:36"
        },
        {
          "id": "MDRW2100022404.1.7",
          "form": "요즘 트로트 너무 좋더라구요",
          "original_form": "요즘 트로트 너무 좋더라구요",
          "speaker_id": "1",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100022404.1.8",
          "form": "좋아하는 트로트 가수 있으신가요",
          "original_form": "좋아하는 트로트 가수 있으신가요",
          "speaker_id": "1",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100022404.1.9",
          "form": "음악 감상을 좋아해서 특정 가수보다는 두루두루 듣습니다",
          "original_form": "음악 감상을 좋아해서 특정 가수보다는 두루두루 듣습니다",
          "speaker_id": "2",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100022404.1.10",
          "form": "두루두루 들으시는구나",
          "original_form": "두루두루 들으시는구나",
          "speaker_id": "1",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100022404.1.11",
          "form": "어떤 노래가 좋던가요",
          "original_form": "어떤 노래가 좋던가요",
          "speaker_id": "1",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100022404.1.12",
          "form": "트로트를 제일 좋아하고",
          "original_form": "트로트를 제일 좋아하고",
          "speaker_id": "2",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022404.1.13",
          "form": "불후의명곡이나 복면가왕 프로 매주 봅니다",
          "original_form": "불후의명곡이나 복면가왕 프로 매주 봅니다",
          "speaker_id": "2",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022404.1.14",
          "form": "음악관련프로를 좋아하시는군요",
          "original_form": "음악관련프로를 좋아하시는군요",
          "speaker_id": "1",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022404.1.15",
          "form": "이번주는 어떤 곡이 인상적이었나요",
          "original_form": "이번주는 어떤 곡이 인상적이었나요",
          "speaker_id": "1",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022404.1.16",
          "form": "어제 불후의 명곡에 전인권 나오더군요",
          "original_form": "어제 불후의 명곡에 전인권 나오더군요",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022404.1.17",
          "form": "전인권씨가요?",
          "original_form": "전인권씨가요?",
          "speaker_id": "1",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022404.1.18",
          "form": "젊었을때 들었던 곡들이 반가웠어요",
          "original_form": "젊었을때 들었던 곡들이 반가웠어요",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022404.1.19",
          "form": "전설로 나왔어요",
          "original_form": "전설로 나왔어요",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022404.1.20",
          "form": "짱이네요",
          "original_form": "짱이네요",
          "speaker_id": "1",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022404.1.21",
          "form": "전인권씨 노래 중 어떤거 좋아하세요",
          "original_form": "전인권씨 노래 중 어떤거 좋아하세요",
          "speaker_id": "1",
          "time": "20210829 13:48"
        }
