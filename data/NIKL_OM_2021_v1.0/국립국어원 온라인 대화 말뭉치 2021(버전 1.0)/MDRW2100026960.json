      "utterance": [
        {
          "id": "MDRW2100026960.1.1",
          "form": "언제 주무십니까 ㅋㅋ",
          "original_form": "언제 주무십니까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:54"
        },
        {
          "id": "MDRW2100026960.1.2",
          "form": "저 이제 자려구요ㅎ",
          "original_form": "저 이제 자려구요ㅎ",
          "speaker_id": "1",
          "time": "20210902 01:55"
        },
        {
          "id": "MDRW2100026960.1.3",
          "form": "안정적 백신을 위하여!",
          "original_form": "안정적 백신을 위하여!",
          "speaker_id": "1",
          "time": "20210902 01:55"
        },
        {
          "id": "MDRW2100026960.1.4",
          "form": "네 오늘 즐거웠어요",
          "original_form": "네 오늘 즐거웠어요",
          "speaker_id": "2",
          "time": "20210902 01:55"
        },
        {
          "id": "MDRW2100026960.1.5",
          "form": "name2도 굿나잇!",
          "original_form": "&name2&도 굿나잇!",
          "speaker_id": "1",
          "time": "20210902 01:55"
        },
        {
          "id": "MDRW2100026960.1.6",
          "form": "푹 쉬시고 내일 또 놀아요",
          "original_form": "푹 쉬시고 내일 또 놀아요",
          "speaker_id": "2",
          "time": "20210902 01:55"
        },
        {
          "id": "MDRW2100026960.1.7",
          "form": "굿밤!!",
          "original_form": "굿밤!!",
          "speaker_id": "2",
          "time": "20210902 01:55"
        },
        {
          "id": "MDRW2100026960.1.8",
          "form": "건투를 빌어주세요",
          "original_form": "건투를 빌어주세요",
          "speaker_id": "1",
          "time": "20210902 01:55"
        },
        {
          "id": "MDRW2100026960.1.9",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210902 01:55"
        },
        {
          "id": "MDRW2100026960.1.10",
          "form": "화이팅!!!",
          "original_form": "화이팅!!!",
          "speaker_id": "2",
          "time": "20210902 01:56"
        },
        {
          "id": "MDRW2100026960.1.11",
          "form": "일어나세요 용사여",
          "original_form": "일어나세요 용사여",
          "speaker_id": "2",
          "time": "20210902 08:57"
        },
        {
          "id": "MDRW2100026960.1.12",
          "form": "아니ㅋ 절 뭘로 보시죠!",
          "original_form": "아니ㅋ 절 뭘로 보시죠!",
          "speaker_id": "1",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100026960.1.13",
          "form": "이미 강아지 산책 마치고 백신 맞으러 갈 준비한다구요ㅎ",
          "original_form": "이미 강아지 산책 마치고 백신 맞으러 갈 준비한다구요ㅎ",
          "speaker_id": "1",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100026960.1.14",
          "form": "그러고나면 라이언 역장님을 겟해야죠ㅎㅎ",
          "original_form": "그러고나면 라이언 역장님을 겟해야죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100026960.1.15",
          "form": "그 생각보다 되게 부지런하시네요^^",
          "original_form": "그 생각보다 되게 부지런하시네요^^",
          "speaker_id": "2",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100026960.1.16",
          "form": "ㅋㅋ 정말 절 뭘로 보신 거죠",
          "original_form": "ㅋㅋ 정말 절 뭘로 보신 거죠",
          "speaker_id": "1",
          "time": "20210902 09:52"
        },
        {
          "id": "MDRW2100026960.1.17",
          "form": "그 머리 좋은데 착실하지는 않은 느낌이신데 ㅋㅋ",
          "original_form": "그 머리 좋은데 착실하지는 않은 느낌이신데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:54"
        },
        {
          "id": "MDRW2100026960.1.18",
          "form": "앞은 인사치레고 뒤가 진심이잖아욧!",
          "original_form": "앞은 인사치레고 뒤가 진심이잖아욧!",
          "speaker_id": "1",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100026960.1.19",
          "form": "착실하지는 않은데 머리는 좋으실거 같애요",
          "original_form": "착실하지는 않은데 머리는 좋으실거 같애요",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100026960.1.20",
          "form": "알뜰하게 쇼핑하시고",
          "original_form": "알뜰하게 쇼핑하시고",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100026960.1.21",
          "form": "현모양처 느낌이에요",
          "original_form": "현모양처 느낌이에요",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100026960.1.22",
          "form": "ㅎㅎ 와 어쩜 한마디 한마디가 이렇게 가식적이실 수가!",
          "original_form": "ㅎㅎ 와 어쩜 한마디 한마디가 이렇게 가식적이실 수가!",
          "speaker_id": "1",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100026960.1.23",
          "form": "저한테 라이언 역장님을 영업한게 누구신가요!",
          "original_form": "저한테 라이언 역장님을 영업한게 누구신가요!",
