      "utterance": [
        {
          "id": "MDRW2100041830.1.1",
          "form": "최근에 산 거 있으세요?",
          "original_form": "최근에 산 거 있으세요?",
          "speaker_id": "2",
          "time": "20210906 16:36"
        },
        {
          "id": "MDRW2100041830.1.2",
          "form": "티셔츠요ㅎㅎ",
          "original_form": "티셔츠요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100041830.1.3",
          "form": "앗 긴팔인가요",
          "original_form": "앗 긴팔인가요",
          "speaker_id": "2",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041830.1.4",
          "form": "날이 애매해서 반팔샀어요ㅎ 더 선선해지면 자켓 안에도 입을려구요ㅎ",
          "original_form": "날이 애매해서 반팔샀어요ㅎ 더 선선해지면 자켓 안에도 입을려구요ㅎ",
          "speaker_id": "1",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100041830.1.5",
          "form": "저는가디건샀어요ㅎㅎ",
          "original_form": "저는가디건샀어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:10"
        },
        {
          "id": "MDRW2100041830.1.6",
          "form": "요새세일하길래",
          "original_form": "요새세일하길래",
          "speaker_id": "2",
          "time": "20210906 17:10"
        },
        {
          "id": "MDRW2100041830.1.7",
          "form": "옷장 열어보니 작년 가을엔 내가 뭘 입고 다녔나...싶어요ㅋㅋ",
          "original_form": "옷장 열어보니 작년 가을엔 내가 뭘 입고 다녔나...싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100041830.1.8",
          "form": "ㅎㅎ맞아요",
          "original_form": "ㅎㅎ맞아요",
          "speaker_id": "2",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041830.1.9",
          "form": "당근해보신적있으세요?",
          "original_form": "당근해보신적있으세요?",
          "speaker_id": "2",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041830.1.10",
          "form": "아뇨 아직 해본적 없어요ㅎㅎ 님은요?",
          "original_form": "아뇨 아직 해본적 없어요ㅎㅎ 님은요?",
          "speaker_id": "1",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100041830.1.11",
          "form": "ㅎㅎ저는팔려고올려봤는데",
          "original_form": "ㅎㅎ저는팔려고올려봤는데",
          "speaker_id": "2",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100041830.1.12",
          "form": "안팔렸어욪ㅋㅋㅋ",
          "original_form": "안팔렸어욪ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100041830.1.13",
          "form": "뭐 올리셨는데요?ㅎㅎ",
          "original_form": "뭐 올리셨는데요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041830.1.14",
          "form": "헤이카카오라고",
          "original_form": "헤이카카오라고",
          "speaker_id": "2",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041830.1.15",
          "form": "ai스피커?요런거요ㅎㅎ",
          "original_form": "ai스피커?요런거요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041830.1.16",
          "form": "새거라올렸는데..",
          "original_form": "새거라올렸는데..",
          "speaker_id": "2",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041830.1.17",
          "form": "게시글 안지우면 언젠가는 팔리지않을까요ㅎㅎㅎ",
          "original_form": "게시글 안지우면 언젠가는 팔리지않을까요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041830.1.18",
          "form": "ㅎㅎㅎ앗 당근을지워버렸는데",
          "original_form": "ㅎㅎㅎ앗 당근을지워버렸는데",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041830.1.19",
          "form": "언제한번들어가봐야겠어요",
          "original_form": "언제한번들어가봐야겠어요",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041830.1.20",
          "form": "팔릴때까지 존버ㅋㅋㅋ",
          "original_form": "팔릴때까지 존버ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041830.1.21",
          "form": "ㅎㅎㅎㅎ가격을낮췄어야했나봐요",
          "original_form": "ㅎㅎㅎㅎ가격을낮췄어야했나봐요",
