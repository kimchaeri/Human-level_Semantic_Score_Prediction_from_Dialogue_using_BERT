      "utterance": [
        {
          "id": "MDRW2100045692.1.1",
          "form": "앗 마지막 못 봤어요",
          "original_form": "앗 마지막 못 봤어요",
          "speaker_id": "2",
          "time": "20210908 05:30"
        },
        {
          "id": "MDRW2100045692.1.2",
          "form": "넘 호로록 지나가브림",
          "original_form": "넘 호로록 지나가브림",
          "speaker_id": "2",
          "time": "20210908 05:30"
        },
        {
          "id": "MDRW2100045692.1.3",
          "form": "점심메뉴 출제 가능하신가요?",
          "original_form": "점심메뉴 출제 가능하신가요?",
          "speaker_id": "1",
          "time": "20210908 05:30"
        },
        {
          "id": "MDRW2100045692.1.4",
          "form": "점심메뉴 으으음",
          "original_form": "점심메뉴 으으음",
          "speaker_id": "2",
          "time": "20210908 05:30"
        },
        {
          "id": "MDRW2100045692.1.5",
          "form": "네 기억 났어요!",
          "original_form": "네 기억 났어요!",
          "speaker_id": "2",
          "time": "20210908 05:30"
        },
        {
          "id": "MDRW2100045692.1.6",
          "form": "맞촤보시죠!!!!",
          "original_form": "맞촤보시죠!!!!",
          "speaker_id": "2",
          "time": "20210908 05:30"
        },
        {
          "id": "MDRW2100045692.1.7",
          "form": "양식인가요?",
          "original_form": "양식인가요?",
          "speaker_id": "1",
          "time": "20210908 05:31"
        },
        {
          "id": "MDRW2100045692.1.8",
          "form": "아니요! 양식 아닙니다",
          "original_form": "아니요! 양식 아닙니다",
          "speaker_id": "2",
          "time": "20210908 05:31"
        },
        {
          "id": "MDRW2100045692.1.9",
          "form": "양식이라기보단 전세계적으로 오래 전부터 먹어왔어요",
          "original_form": "양식이라기보단 전세계적으로 오래 전부터 먹어왔어요",
          "speaker_id": "2",
          "time": "20210908 05:32"
        },
        {
          "id": "MDRW2100045692.1.10",
          "form": "음 요구르트같은걸까요",
          "original_form": "음 요구르트같은걸까요",
          "speaker_id": "1",
          "time": "20210908 05:33"
        },
        {
          "id": "MDRW2100045692.1.11",
          "form": "유제품이 들어가나요?",
          "original_form": "유제품이 들어가나요?",
          "speaker_id": "1",
          "time": "20210908 05:33"
        },
        {
          "id": "MDRW2100045692.1.12",
          "form": "아니요! 액체가 아니예요",
          "original_form": "아니요! 액체가 아니예요",
          "speaker_id": "2",
          "time": "20210908 05:33"
        },
        {
          "id": "MDRW2100045692.1.13",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210908 05:33"
        },
        {
          "id": "MDRW2100045692.1.14",
          "form": "쌀이 들어가나요?",
          "original_form": "쌀이 들어가나요?",
          "speaker_id": "1",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045692.1.15",
          "form": "아니요! 쌀도 안 들어가요",
          "original_form": "아니요! 쌀도 안 들어가요",
          "speaker_id": "2",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045692.1.16",
          "form": "열라 심플한 음식이예요",
          "original_form": "열라 심플한 음식이예요",
          "speaker_id": "2",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045692.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ심플하다고 하니까 더 미궁이에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ심플하다고 하니까 더 미궁이에요",
          "speaker_id": "1",
          "time": "20210908 05:35"
        },
        {
          "id": "MDRW2100045692.1.18",
          "form": "과일인가요?",
          "original_form": "과일인가요?",
          "speaker_id": "1",
          "time": "20210908 05:36"
        },
        {
          "id": "MDRW2100045692.1.19",
          "form": "아니요! 과일은 아니고 채소입니다",
          "original_form": "아니요! 과일은 아니고 채소입니다",
          "speaker_id": "2",
          "time": "20210908 05:37"
        },
        {
          "id": "MDRW2100045692.1.20",
          "form": "채소...식사가 될만한 채소",
          "original_form": "채소...식사가 될만한 채소",
          "speaker_id": "1",
          "time": "20210908 05:37"
        },
        {
          "id": "MDRW2100045692.1.21",
          "form": "고구마인가요?",
          "original_form": "고구마인가요?",
          "speaker_id": "1",
          "time": "20210908 05:37"
        },
        {
          "id": "MDRW2100045692.1.22",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210908 05:38"
        },
        {
          "id": "MDRW2100045692.1.23",
          "form": "비슷해요",
          "original_form": "비슷해요",
          "speaker_id": "2",
          "time": "20210908 05:38"
        },
        {
          "id": "MDRW2100045692.1.24",
          "form": "단호박?",
          "original_form": "단호박?",
          "speaker_id": "1",
          "time": "20210908 05:39"
        },
        {
          "id": "MDRW2100045692.1.25",
          "form": "거의 다 왔어요!",
          "original_form": "거의 다 왔어요!",
          "speaker_id": "2",
          "time": "20210908 05:39"
        },
        {
          "id": "MDRW2100045692.1.26",
          "form": "단호박 아닙네다",
          "original_form": "단호박 아닙네다",
          "speaker_id": "2",
          "time": "20210908 05:39"
        },
        {
          "id": "MDRW2100045692.1.27",
          "form": "고구마 친구",
