        },
        {
          "id": "MDRW2100032579.1.3",
          "form": "응 백신맞고 뭐?",
          "original_form": "응 백신맞고 뭐?",
          "speaker_id": "1",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100032579.1.4",
          "form": "가을에 많이 놀러다니자고",
          "original_form": "가을에 많이 놀러다니자고",
          "speaker_id": "2",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100032579.1.5",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100032579.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅇㅋㅇㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅇㅋㅇㅋ",
          "speaker_id": "1",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100032579.1.7",
          "form": "올해는",
          "original_form": "올해는",
          "speaker_id": "1",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100032579.1.8",
          "form": "여름이 진짜 금방 지나간듯",
          "original_form": "여름이 진짜 금방 지나간듯",
          "speaker_id": "1",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100032579.1.9",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032579.1.10",
          "form": "원래 한 9월 중순은 돼야 이런날씨였던거 같은데..",
          "original_form": "원래 한 9월 중순은 돼야 이런날씨였던거 같은데..",
          "speaker_id": "1",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032579.1.11",
          "form": "핑크뮬리 보러가자",
          "original_form": "핑크뮬리 보러가자",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032579.1.12",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032579.1.13",
          "form": "핑크뮬리가 뭐야?",
          "original_form": "핑크뮬리가 뭐야?",
          "speaker_id": "1",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032579.1.14",
          "form": "엄청 역대급 더위라고 하던데",
          "original_form": "엄청 역대급 더위라고 하던데",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032579.1.15",
          "form": "별로 안 더웠던것 같아",
          "original_form": "별로 안 더웠던것 같아",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032579.1.16",
          "form": "그니까ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032579.1.17",
          "form": "3~4주 바짝 덥고 끝나버림",
