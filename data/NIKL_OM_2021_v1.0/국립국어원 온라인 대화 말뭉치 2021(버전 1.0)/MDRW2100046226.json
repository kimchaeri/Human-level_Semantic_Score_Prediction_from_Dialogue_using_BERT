        {
          "id": "MDRW2100046226.1.1",
          "form": "아직 성공하지못했어요",
          "original_form": "아직 성공하지못했어요",
          "speaker_id": "2",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046226.1.2",
          "form": "키운지 얼마되셨나요?",
          "original_form": "키운지 얼마되셨나요?",
          "speaker_id": "1",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046226.1.3",
          "form": "아직 한달도 안됏어요",
          "original_form": "아직 한달도 안됏어요",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046226.1.4",
          "form": "헉 정말 얼마안됐네요",
          "original_form": "헉 정말 얼마안됐네요",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046226.1.5",
          "form": "네 전애키우고 2년만이내요",
          "original_form": "네 전애키우고 2년만이내요",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046226.1.6",
          "form": "오랜만에 키우게되신거네요",
          "original_form": "오랜만에 키우게되신거네요",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046226.1.7",
          "form": "네 동생이 키우고싶어해서",
          "original_form": "네 동생이 키우고싶어해서",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046226.1.8",
          "form": "동생분도 동물 좋아하시나봐요",
          "original_form": "동생분도 동물 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046226.1.9",
          "form": "아무래도 그런편이죠",
          "original_form": "아무래도 그런편이죠",
          "speaker_id": "2",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046226.1.10",
          "form": "점점 햄스터랑 친해지실거에요",
          "original_form": "점점 햄스터랑 친해지실거에요",
