      "utterance": [
        {
          "id": "MDRW2100013851.1.1",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "1",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013851.1.2",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013851.1.3",
          "form": "아니요 아까 남은 쭈꾸미에 밥비벼먹었어요\n점심으로..",
          "original_form": "아니요 아까 남은 쭈꾸미에 밥비벼먹었어요\n점심으로..",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013851.1.4",
          "form": "점심뭐드셨어요?",
          "original_form": "점심뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013851.1.5",
          "form": "짜장밥이요",
          "original_form": "짜장밥이요",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013851.1.6",
          "form": "맛나셨겠네요",
          "original_form": "맛나셨겠네요",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013851.1.7",
          "form": "저녁은요??",
          "original_form": "저녁은요??",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013851.1.8",
          "form": "메뉴?",
          "original_form": "메뉴?",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013851.1.9",
          "form": "고민중이요",
          "original_form": "고민중이요",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013851.1.10",
          "form": "저는 어제 끓인 소고기무국먹을려구요",
          "original_form": "저는 어제 끓인 소고기무국먹을려구요",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013851.1.11",
          "form": "쭈꾸미 좋아하세요?",
          "original_form": "쭈꾸미 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013851.1.12",
          "form": "네좋아해요",
          "original_form": "네좋아해요",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013851.1.13",
          "form": "얼마전 낭만쭈꾸미 포장해온거 남아서",
          "original_form": "얼마전 낭만쭈꾸미 포장해온거 남아서",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013851.1.14",
          "form": "너무맛나요 그집..",
          "original_form": "너무맛나요 그집..",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013851.1.15",
          "form": "낭쭈 맛집이죠",
          "original_form": "낭쭈 맛집이죠",
          "speaker_id": "1",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013851.1.16",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013851.1.17",
          "form": "맛집 다니세요?",
          "original_form": "맛집 다니세요?",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013851.1.18",
          "form": "먹는것도 일이에요\n포장메뉴 괜찮은거있나요",
          "original_form": "먹는것도 일이에요\n포장메뉴 괜찮은거있나요",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013851.1.19",
          "form": "다니는거좋아하는데 코로나때문에 ㅜ",
          "original_form": "다니는거좋아하는데 코로나때문에 ㅜ",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013851.1.20",
          "form": "맛집 추천해주세요",
          "original_form": "맛집 추천해주세요",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013851.1.21",
          "form": "삼겹살 곱창 그런거좋아해요 전\n뭐좋아하세요?",
          "original_form": "삼겹살 곱창 그런거좋아해요 전\n뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 16:24"
        }
      ]
