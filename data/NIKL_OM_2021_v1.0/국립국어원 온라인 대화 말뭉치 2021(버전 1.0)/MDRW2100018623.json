      "utterance": [
        {
          "id": "MDRW2100018623.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 09:14"
        },
        {
          "id": "MDRW2100018623.1.2",
          "form": "태풍도 지나가나봐요 비가 안오네요",
          "original_form": "태풍도 지나가나봐요 비가 안오네요",
          "speaker_id": "2",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100018623.1.3",
          "form": "그러게요! 하늘이 너무 파랗고 맑아요!",
          "original_form": "그러게요! 하늘이 너무 파랗고 맑아요!",
          "speaker_id": "1",
          "time": "20210827 18:19"
        },
        {
          "id": "MDRW2100018623.1.4",
          "form": "이렇다 밤에 쏟아지는건 아니겠죠?",
          "original_form": "이렇다 밤에 쏟아지는건 아니겠죠?",
          "speaker_id": "1",
          "time": "20210827 18:20"
        },
        {
          "id": "MDRW2100018623.1.5",
          "form": "저는 오늘 강원도 왔는데 날이 참 좋더라고요",
          "original_form": "저는 오늘 강원도 왔는데 날이 참 좋더라고요",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100018623.1.6",
          "form": "여행 가셨나요?",
          "original_form": "여행 가셨나요?",
          "speaker_id": "1",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100018623.1.7",
          "form": "강원도는 빨리 추워진다던데 가을 옷 챙겨가셨나요 ㅎㅎ",
          "original_form": "강원도는 빨리 추워진다던데 가을 옷 챙겨가셨나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100018623.1.8",
          "form": "네 그런데 원주는 영서라 그런지 그정도는 아니더라고요",
          "original_form": "네 그런데 원주는 영서라 그런지 그정도는 아니더라고요",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100018623.1.9",
          "form": "원주에 가셨군요",
          "original_form": "원주에 가셨군요",
          "speaker_id": "1",
          "time": "20210829 11:34"
        },
        {
          "id": "MDRW2100018623.1.10",
          "form": "여기는 또 비가 오려는지 구름이 모이네요..",
          "original_form": "여기는 또 비가 오려는지 구름이 모이네요..",
          "speaker_id": "1",
          "time": "20210829 11:34"
        },
        {
          "id": "MDRW2100018623.1.11",
          "form": "비 좀 그만 왔으면...",
          "original_form": "비 좀 그만 왔으면...",
          "speaker_id": "1",
          "time": "20210829 11:34"
        },
        {
          "id": "MDRW2100018623.1.12",
          "form": "아 저는 비가 생각보다 안오나보다 했는데 지역별로 다르네요",
          "original_form": "아 저는 비가 생각보다 안오나보다 했는데 지역별로 다르네요",
          "speaker_id": "2",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100018623.1.13",
          "form": "그러게요 이 작은 나라에 ㅎㅎ",
          "original_form": "그러게요 이 작은 나라에 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100018623.1.14",
          "form": "땅도 작은데 신기하네요",
          "original_form": "땅도 작은데 신기하네요",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100018623.1.15",
          "form": "비가 오기 시작했나요",
          "original_form": "비가 오기 시작했나요",
          "speaker_id": "2",
          "time": "20210829 12:58"
        },
        {
          "id": "MDRW2100018623.1.16",
          "form": "여기도 약간 흐려지고 있긴해요",
          "original_form": "여기도 약간 흐려지고 있긴해요",
          "speaker_id": "2",
          "time": "20210829 12:58"
        },
        {
          "id": "MDRW2100018623.1.17",
          "form": "왔다가 지금은 잠시 또 멎었어요",
          "original_form": "왔다가 지금은 잠시 또 멎었어요",
          "speaker_id": "1",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100018623.1.18",
          "form": "조금씩 왔다 그쳤다 할 것 같아요",
          "original_form": "조금씩 왔다 그쳤다 할 것 같아요",
          "speaker_id": "1",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100018623.1.19",
          "form": "이러다 가을 없이 겨울 올것같아요",
          "original_form": "이러다 가을 없이 겨울 올것같아요",
          "speaker_id": "2",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100018623.1.20",
          "form": "저는 개인적으로 겨울을 엄청 좋아해서 그것도 좋을 것 같아요 ㅎㅎ",
          "original_form": "저는 개인적으로 겨울을 엄청 좋아해서 그것도 좋을 것 같아요 ㅎㅎ",
          "speaker_id": "1",
