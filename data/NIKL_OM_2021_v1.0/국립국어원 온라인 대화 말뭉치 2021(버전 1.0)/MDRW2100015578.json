      "utterance": [
        {
          "id": "MDRW2100015578.1.1",
          "form": "아직 배송이 안와서 몰라요...",
          "original_form": "아직 배송이 안와서 몰라요...",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015578.1.2",
          "form": "혹시 읽기쉬운? 자기계발서 추천해줄수 있나요??",
          "original_form": "혹시 읽기쉬운? 자기계발서 추천해줄수 있나요??",
          "speaker_id": "1",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015578.1.3",
          "form": "자기계발서 중에 어떤 거요?",
          "original_form": "자기계발서 중에 어떤 거요?",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015578.1.4",
          "form": "아 그냥 미래발전?!",
          "original_form": "아 그냥 미래발전?!",
          "speaker_id": "1",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015578.1.5",
          "form": "아.. 딱히 떠오르는 게 없네요 ㅜㅜ",
          "original_form": "아.. 딱히 떠오르는 게 없네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015578.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ요즘 그런게 대세같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ요즘 그런게 대세같아요",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015578.1.7",
          "form": "이상하게 전 그런 책 보면 제 맘 속에 답정너가 있나봐요",
          "original_form": "이상하게 전 그런 책 보면 제 맘 속에 답정너가 있나봐요",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015578.1.8",
          "form": "뭐지 푸그림 있고",
          "original_form": "뭐지 푸그림 있고",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015578.1.9",
          "form": "푸라서 다행이야? 괜찮아?",
          "original_form": "푸라서 다행이야? 괜찮아?",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015578.1.10",
          "form": "그감성",
          "original_form": "그감성",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015578.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015578.1.12",
          "form": "ㅋㅋㅋ 귀엽다",
          "original_form": "ㅋㅋㅋ 귀엽다",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015578.1.13",
          "form": "답정너요?",
          "original_form": "답정너요?",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015578.1.14",
          "form": "네네 뭔가 아무리 좋은 말 해줘도 맘에 와닿지 않으면..",
          "original_form": "네네 뭔가 아무리 좋은 말 해줘도 맘에 와닿지 않으면..",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015578.1.15",
          "form": "실행하지 않는다..",
          "original_form": "실행하지 않는다..",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015578.1.16",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015578.1.17",
          "form": "그런거있어요",
          "original_form": "그런거있어요",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015578.1.18",
          "form": "약간 반발심 솟는?",
          "original_form": "약간 반발심 솟는?",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015578.1.19",
          "form": "요새 마음 치유 이런 쪽 읽어보는데",
          "original_form": "요새 마음 치유 이런 쪽 읽어보는데",
          "speaker_id": "2",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015578.1.20",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015578.1.21",
          "form": "와.. 무슨 중국 고대학자 나와서.. 좀 지루했어요",
          "original_form": "와.. 무슨 중국 고대학자 나와서.. 좀 지루했어요",
          "speaker_id": "2",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015578.1.22",
          "form": "네 그런분야좋죠",
          "original_form": "네 그런분야좋죠",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015578.1.23",
          "form": "채근담이니 장자니 너무 옛스러운 그런 내용 ㅜㅜ",
          "original_form": "채근담이니 장자니 너무 옛스러운 그런 내용 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015578.1.24",
          "form": "아 그런거 어려워요 ㅠㅠ",
          "original_form": "아 그런거 어려워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:39"
        }
      ]
