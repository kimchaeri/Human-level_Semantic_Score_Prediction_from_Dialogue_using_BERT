      "utterance": [
        {
          "id": "MDRW2100006848.1.1",
          "form": "다이어트 하시나요",
          "original_form": "다이어트 하시나요",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100006848.1.2",
          "form": "다요트는 매일 ㅎㅎ",
          "original_form": "다요트는 매일 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006848.1.3",
          "form": "어떤식으로요?",
          "original_form": "어떤식으로요?",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006848.1.4",
          "form": "주2회 운동해요",
          "original_form": "주2회 운동해요",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100006848.1.5",
          "form": "식단조절이 어려워요",
          "original_form": "식단조절이 어려워요",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100006848.1.6",
          "form": "그렇죠 식단조절도 하시나요?",
          "original_form": "그렇죠 식단조절도 하시나요?",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100006848.1.7",
          "form": "나름 하려고",
          "original_form": "나름 하려고",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006848.1.8",
          "form": "저녁은 닭가슴살 먹기도해요",
          "original_form": "저녁은 닭가슴살 먹기도해요",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006848.1.9",
          "form": "와 노력중이시군요",
          "original_form": "와 노력중이시군요",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006848.1.10",
          "form": "근데 주말엔 허리띠풀고 먹어요 ㅋ",
          "original_form": "근데 주말엔 허리띠풀고 먹어요 ㅋ",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006848.1.11",
          "form": "ㅎㅎ 치팅데이같은던가요",
          "original_form": "ㅎㅎ 치팅데이같은던가요",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100006848.1.12",
          "form": "안먹을수가 없어요",
          "original_form": "안먹을수가 없어요",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100006848.1.13",
          "form": "외식도하고 주말에 신랑오면 찌개도하고 하니",
          "original_form": "외식도하고 주말에 신랑오면 찌개도하고 하니",
          "speaker_id": "2",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100006848.1.14",
          "form": "그렇죠 다이어트 하신지는 얼마나 되셨나요",
          "original_form": "그렇죠 다이어트 하신지는 얼마나 되셨나요",
          "speaker_id": "1",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100006848.1.15",
          "form": "저는 매일 맘만먹어요 ㅋㅋ",
          "original_form": "저는 매일 맘만먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100006848.1.16",
          "form": "그러시군요 쉽지않어요 다이어트",
          "original_form": "그러시군요 쉽지않어요 다이어트",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
