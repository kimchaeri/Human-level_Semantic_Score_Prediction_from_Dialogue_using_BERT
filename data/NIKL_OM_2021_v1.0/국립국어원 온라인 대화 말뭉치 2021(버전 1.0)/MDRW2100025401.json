      "utterance": [
        {
          "id": "MDRW2100025401.1.1",
          "form": "점심 메뉴는 정하셨나요?",
          "original_form": "점심 메뉴는 정하셨나요?",
          "speaker_id": "1",
          "time": "20210901 10:59"
        },
        {
          "id": "MDRW2100025401.1.2",
          "form": "전 회사 식당에서 먹어서 정할수가 없어요ㅋㅋ",
          "original_form": "전 회사 식당에서 먹어서 정할수가 없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100025401.1.3",
          "form": "오 구내식당이군요",
          "original_form": "오 구내식당이군요",
          "speaker_id": "1",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025401.1.4",
          "form": "그럼 아침은 드세요?",
          "original_form": "그럼 아침은 드세요?",
          "speaker_id": "1",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025401.1.5",
          "form": "출근 전에요",
          "original_form": "출근 전에요",
          "speaker_id": "1",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025401.1.6",
          "form": "안먹어요ㅋㅋㅋ 중고딩 이후로",
          "original_form": "안먹어요ㅋㅋㅋ 중고딩 이후로",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025401.1.7",
          "form": "아침 일찍 나갔어야 했어서",
          "original_form": "아침 일찍 나갔어야 했어서",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025401.1.8",
          "form": "자연스레 안먹게됐는데",
          "original_form": "자연스레 안먹게됐는데",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025401.1.9",
          "form": "이게 지금보니 간헐적 단식이더라구요?",
          "original_form": "이게 지금보니 간헐적 단식이더라구요?",
          "speaker_id": "2",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025401.1.10",
          "form": "그러네요.. 속은 괜찮으세요?",
          "original_form": "그러네요.. 속은 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025401.1.11",
          "form": "넵ㅋㅋ 오히려 9시 이전에 먹으면",
          "original_form": "넵ㅋㅋ 오히려 9시 이전에 먹으면",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025401.1.12",
          "form": "배아파요..",
          "original_form": "배아파요..",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025401.1.13",
          "form": "아항.. 전 간헐적 단식한다고",
          "original_form": "아항.. 전 간헐적 단식한다고",
          "speaker_id": "1",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025401.1.14",
          "form": "아침 거르고 커피로 배채우다가",
          "original_form": "아침 거르고 커피로 배채우다가",
          "speaker_id": "1",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025401.1.15",
          "form": "위가 난리나서",
          "original_form": "위가 난리나서",
          "speaker_id": "1",
          "time": "20210901 11:18"
        },
        {
          "id": "MDRW2100025401.1.16",
          "form": "약먹고",
          "original_form": "약먹고",
          "speaker_id": "1",
          "time": "20210901 11:18"
        },
        {
          "id": "MDRW2100025401.1.17",
          "form": "그뒤로 아침 챙겨먹어요..ㅎㅎ",
          "original_form": "그뒤로 아침 챙겨먹어요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 11:18"
        },
        {
          "id": "MDRW2100025401.1.18",
          "form": "빈속에 커피 최악이에요ㅠ",
          "original_form": "빈속에 커피 최악이에요ㅠ",
          "speaker_id": "2",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025401.1.19",
          "form": "맞아요.. 것도 모르고 한 일년했따가 속만 버려죠..",
          "original_form": "맞아요.. 것도 모르고 한 일년했따가 속만 버려죠..",
          "speaker_id": "1",
          "time": "20210901 11:22"
        },
        {
          "id": "MDRW2100025401.1.20",
          "form": "그래서 출근해서 커피 되도록 마시지 않을려고 합니다",
          "original_form": "그래서 출근해서 커피 되도록 마시지 않을려고 합니다",
          "speaker_id": "2",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025401.1.21",
          "form": "체감되드라구요 빈속커피",
          "original_form": "체감되드라구요 빈속커피",
          "speaker_id": "2",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025401.1.22",
          "form": "몸이 거부하는 느낌?",
          "original_form": "몸이 거부하는 느낌?",
          "speaker_id": "2",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025401.1.23",
          "form": "요새 안먹다 어쩌다 마시니까 그러더라구요.. 근대 매일 마실땐 좋았어요..ㅎㅎ 이상도 모르겠고",
          "original_form": "요새 안먹다 어쩌다 마시니까 그러더라구요.. 근대 매일 마실땐 좋았어요..ㅎㅎ 이상도 모르겠고",
          "speaker_id": "1",
          "time": "20210901 11:38"
        },
        {
          "id": "MDRW2100025401.1.24",
          "form": "저녁은 집에서 드시나요?",
          "original_form": "저녁은 집에서 드시나요?",
          "speaker_id": "1",
          "time": "20210901 11:38"
        },
        {
          "id": "MDRW2100025401.1.25",
          "form": "네 거진 집에서 먹어요",
          "original_form": "네 거진 집에서 먹어요",
          "speaker_id": "2",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025401.1.26",
          "form": "(회사 밥이 맛이 없어요..)",
          "original_form": "(회사 밥이 맛이 없어요..)",
          "speaker_id": "2",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025401.1.27",
          "form": "이런.. 점심마다 꼭 먹어야 하는데 맛이 없다니 고역이겠어요",
          "original_form": "이런.. 점심마다 꼭 먹어야 하는데 맛이 없다니 고역이겠어요",
          "speaker_id": "1",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025401.1.28",
          "form": "저녁 집에서 드실 때는 시켜먹나요? 해먹나요?",
          "original_form": "저녁 집에서 드실 때는 시켜먹나요? 해먹나요?",
          "speaker_id": "1",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025401.1.29",
          "form": "이럴거면 돈으로 주지 싶긴한데",
          "original_form": "이럴거면 돈으로 주지 싶긴한데",
          "speaker_id": "2",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025401.1.30",
          "form": "5:5 인듯요",
          "original_form": "5:5 인듯요",
          "speaker_id": "2",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025401.1.31",
          "form": "치우기 귀찮은 날엔 시켜먹고",
          "original_form": "치우기 귀찮은 날엔 시켜먹고",
          "speaker_id": "2",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025401.1.32",
          "form": "배달앱 쓰세요?",
          "original_form": "배달앱 쓰세요?",
          "speaker_id": "1",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025401.1.33",
          "form": "네네 요기요 주로 써요",
          "original_form": "네네 요기요 주로 써요",
          "speaker_id": "2",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025401.1.34",
          "form": "그렇구나.. 전 배달앱이 없어요",
          "original_form": "그렇구나.. 전 배달앱이 없어요",
          "speaker_id": "1",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025401.1.35",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025401.1.36",
          "form": "다 해드세요??",
          "original_form": "다 해드세요??",
          "speaker_id": "2",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025401.1.37",
          "form": "주로 해먹고",
          "original_form": "주로 해먹고",
          "speaker_id": "1",
          "time": "20210901 11:55"
        },
        {
          "id": "MDRW2100025401.1.38",
          "form": "가끔 가게에서 픽업해서 먹고",
          "original_form": "가끔 가게에서 픽업해서 먹고",
          "speaker_id": "1",
          "time": "20210901 11:55"
        },
        {
          "id": "MDRW2100025401.1.39",
          "form": "배달시키진 않아요",
          "original_form": "배달시키진 않아요",
          "speaker_id": "1",
          "time": "20210901 11:55"
        },
        {
          "id": "MDRW2100025401.1.40",
          "form": "아아 픽업하시는구나",
          "original_form": "아아 픽업하시는구나",
          "speaker_id": "2",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100025401.1.41",
          "form": "저는 할인 받아먹으려고ㅋㅋ",
          "original_form": "저는 할인 받아먹으려고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100025401.1.42",
          "form": "저도 할인 이야기 들을때마다 앱을 깔아야하나 고민이라니까요",
          "original_form": "저도 할인 이야기 들을때마다 앱을 깔아야하나 고민이라니까요",
          "speaker_id": "1",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025401.1.43",
          "form": "왠지 손해보는 느낌",
          "original_form": "왠지 손해보는 느낌",
          "speaker_id": "1",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025401.1.44",
          "form": "ㅋㅋ얘들은 어떻게 버티나 싶어요",
          "original_form": "ㅋㅋ얘들은 어떻게 버티나 싶어요",
          "speaker_id": "2",
          "time": "20210901 13:04"
        }
      ]
    }
  ]
