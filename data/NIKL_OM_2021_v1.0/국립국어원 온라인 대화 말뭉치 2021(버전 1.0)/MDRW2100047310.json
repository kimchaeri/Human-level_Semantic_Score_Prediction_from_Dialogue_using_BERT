      "utterance": [
        {
          "id": "MDRW2100047310.1.1",
          "form": "저는 왠만하믄 팔아요 ㅋ",
          "original_form": "저는 왠만하믄 팔아요 ㅋ",
          "speaker_id": "2",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100047310.1.2",
          "form": "내 ㅎㅎ저도 그래야겠어요",
          "original_form": "내 ㅎㅎ저도 그래야겠어요",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100047310.1.3",
          "form": "제가 요새 팔아보니",
          "original_form": "제가 요새 팔아보니",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100047310.1.4",
          "form": "이게 돈이돼요",
          "original_form": "이게 돈이돼요",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100047310.1.5",
          "form": "신랑은 이런것도 팔리냐고 할정도예요 ㅎ",
          "original_form": "신랑은 이런것도 팔리냐고 할정도예요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100047310.1.6",
          "form": "예전엔 왜버렸지 후회중요",
          "original_form": "예전엔 왜버렸지 후회중요",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100047310.1.7",
          "form": "오 정말요",
          "original_form": "오 정말요",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100047310.1.8",
          "form": "뭐가 팔려요?",
          "original_form": "뭐가 팔려요?",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100047310.1.9",
          "form": "옷도 모아서 20키로 이상되면 키로당으로 팔아요",
          "original_form": "옷도 모아서 20키로 이상되면 키로당으로 팔아요",
          "speaker_id": "2",
          "time": "20210826 10:54"
        },
        {
          "id": "MDRW2100047310.1.10",
          "form": "아 한번 저도 팔아봤어요",
          "original_form": "아 한번 저도 팔아봤어요",
          "speaker_id": "1",
          "time": "20210826 10:54"
        },
        {
          "id": "MDRW2100047310.1.11",
          "form": "후라이팬 도 드리니 돈이 ㅋㅋ좀 나오던데요",
          "original_form": "후라이팬 도 드리니 돈이 ㅋㅋ좀 나오던데요",
          "speaker_id": "1",
          "time": "20210826 10:54"
        },
        {
          "id": "MDRW2100047310.1.12",
          "form": "옷도 괜히 버렸어요 그렇게 팔걸 ㅋㅋ",
          "original_form": "옷도 괜히 버렸어요 그렇게 팔걸 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100047310.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100047310.1.14",
          "form": "많이 받으셨어요?",
          "original_form": "많이 받으셨어요?",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100047310.1.15",
          "form": "저도 후라이팬 드릴라고 남겨놨어요",
          "original_form": "저도 후라이팬 드릴라고 남겨놨어요",
          "speaker_id": "2",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100047310.1.16",
          "form": "오 후라이팬이 짭절해요",
          "original_form": "오 후라이팬이 짭절해요",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100047310.1.17",
          "form": "집에 저동차 장난감이 안팔려요 ㅡㅡ",
          "original_form": "집에 저동차 장난감이 안팔려요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100047310.1.18",
          "form": "장난감은 잘팔리던데 ㅎ",
          "original_form": "장난감은 잘팔리던데 ㅎ",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100047310.1.19",
          "form": "나름 올드 토이인데 ㅋㅋ",
          "original_form": "나름 올드 토이인데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100047310.1.20",
          "form": "2000년초 ㅋㅋ",
          "original_form": "2000년초 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100047310.1.21",
          "form": "헐값에 팔아야겠어요",
          "original_form": "헐값에 팔아야겠어요",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100047310.1.22",
          "form": "아...안찾는 종류인가ㅎㅎ",
          "original_form": "아...안찾는 종류인가ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100047310.1.23",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100047310.1.24",
          "form": "오래되면 비싼가했는데 ㅋㅋ",
          "original_form": "오래되면 비싼가했는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100047310.1.25",
          "form": "좋다 말았어요",
          "original_form": "좋다 말았어요",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100047310.1.26",
          "form": "장난감은 신상들이 잘나와서",
          "original_form": "장난감은 신상들이 잘나와서",
          "speaker_id": "2",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100047310.1.27",
          "form": "바로바로  처분해야하더라구요",
          "original_form": "바로바로  처분해야하더라구요",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100047310.1.28",
          "form": "그렇죠 ㅜㅜ",
          "original_form": "그렇죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100047310.1.29",
          "form": "저는 일좀하고 올께용~",
          "original_form": "저는 일좀하고 올께용~",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100047310.1.30",
          "form": "제가 몰랐어요",
          "original_form": "제가 몰랐어요",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100047310.1.31",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100047310.1.32",
          "form": "같이 잠드셔도 ㅎㅎ",
          "original_form": "같이 잠드셔도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100047310.1.33",
          "form": "어쩔수 없는 시간이죠 다 챙기고 나면",
          "original_form": "어쩔수 없는 시간이죠 다 챙기고 나면",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100047310.1.34",
          "form": "어서 씻고",
          "original_form": "어서 씻고",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100047310.1.35",
          "form": "쉬다 되면 오세요 ㅎㅎ",
          "original_form": "쉬다 되면 오세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:32"
        }
      ]
