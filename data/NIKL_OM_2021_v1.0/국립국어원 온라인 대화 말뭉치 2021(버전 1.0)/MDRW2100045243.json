      "utterance": [
        {
          "id": "MDRW2100045243.1.1",
          "form": "저녁 뭐 드셧어용",
          "original_form": "저녁 뭐 드셧어용",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045243.1.2",
          "form": "냉면먹었네요",
          "original_form": "냉면먹었네요",
          "speaker_id": "2",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045243.1.3",
          "form": "어떤거 드셨어요?",
          "original_form": "어떤거 드셨어요?",
          "speaker_id": "2",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045243.1.4",
          "form": "오 저는 간단히 볶음밥 먹었어요 ㅋㅋㅋ",
          "original_form": "오 저는 간단히 볶음밥 먹었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045243.1.5",
          "form": "배달 자주 드세요?",
          "original_form": "배달 자주 드세요?",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045243.1.6",
          "form": "자주는 아닌데 일주일에 한번정도는 먹는 것 같아요",
          "original_form": "자주는 아닌데 일주일에 한번정도는 먹는 것 같아요",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045243.1.7",
          "form": "자주드시나요?",
          "original_form": "자주드시나요?",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045243.1.8",
          "form": "저도 일주일에 한번정도??",
          "original_form": "저도 일주일에 한번정도??",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045243.1.9",
          "form": "주로 포장을 자주해요 ㅋㅋ",
          "original_form": "주로 포장을 자주해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045243.1.10",
          "form": "오 어떤 음식 주로드시나요?",
          "original_form": "오 어떤 음식 주로드시나요?",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045243.1.11",
          "form": "저는 분식집 자주 이용해요!!",
          "original_form": "저는 분식집 자주 이용해요!!",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045243.1.12",
          "form": "김밥천국 같은?",
          "original_form": "김밥천국 같은?",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045243.1.13",
          "form": "오 맛있죠 메뉴도 많구요ㅎㅎ",
          "original_form": "오 맛있죠 메뉴도 많구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045243.1.14",
          "form": "네네!! 분식집가면 어떤거 시키세요",
          "original_form": "네네!! 분식집가면 어떤거 시키세요",
          "speaker_id": "1",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100045243.1.15",
          "form": "김밥 라볶이 돈까스 요런거요!!",
          "original_form": "김밥 라볶이 돈까스 요런거요!!",
          "speaker_id": "2",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045243.1.16",
          "form": "맛있어요 ㅎㅎ",
          "original_form": "맛있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045243.1.17",
          "form": "오오 저도요 !! 돈까스 !!!",
          "original_form": "오오 저도요 !! 돈까스 !!!",
          "speaker_id": "1",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045243.1.18",
          "form": "치즈가 좋아요 ㅠㅠ",
          "original_form": "치즈가 좋아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045243.1.19",
          "form": "맛있죠!! 약간 고급지진 않은 맛인데",
          "original_form": "맛있죠!! 약간 고급지진 않은 맛인데",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045243.1.20",
          "form": "그런 돈까스 넘 맛잇지 않나요 ... ㅜㅜ",
          "original_form": "그런 돈까스 넘 맛잇지 않나요 ... ㅜㅜ",
