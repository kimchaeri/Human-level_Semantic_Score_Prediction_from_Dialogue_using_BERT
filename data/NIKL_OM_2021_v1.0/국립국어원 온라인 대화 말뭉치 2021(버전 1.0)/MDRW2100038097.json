      "utterance": [
        {
          "id": "MDRW2100038097.1.1",
          "form": "나 요즘 왜 배가 자주아플까?",
          "original_form": "나 요즘 왜 배가 자주아플까?",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038097.1.2",
          "form": "음 나는 자주 안깎는거같아",
          "original_form": "음 나는 자주 안깎는거같아",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038097.1.3",
          "form": "나는 미신때문에 주로 밤에 깎아",
          "original_form": "나는 미신때문에 주로 밤에 깎아",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038097.1.4",
          "form": "그렇구나 나는 너무 자주 길어서 물어봤어",
          "original_form": "그렇구나 나는 너무 자주 길어서 물어봤어",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038097.1.5",
          "form": "배는 왜아픈거야?",
          "original_form": "배는 왜아픈거야?",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038097.1.6",
          "form": "내 손톱을 먹은 쥐가 내 분신이 된다는 미신 알지?",
          "original_form": "내 손톱을 먹은 쥐가 내 분신이 된다는 미신 알지?",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038097.1.7",
          "form": "나도 모르겠어",
          "original_form": "나도 모르겠어",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038097.1.8",
          "form": "알지 그래서 손톱관리를 잘 해야해",
          "original_form": "알지 그래서 손톱관리를 잘 해야해",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038097.1.9",
          "form": "매운 걸 먹어서 그론골까m",
          "original_form": "매운 걸 먹어서 그론골까m",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038097.1.10",
          "form": "그만먹고 좋은 음식을 먹는건어때",
          "original_form": "그만먹고 좋은 음식을 먹는건어때",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038097.1.11",
          "form": "맞아. 난 요즘 손톱이 약해졌어",
          "original_form": "맞아. 난 요즘 손톱이 약해졌어",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038097.1.12",
          "form": "영양이 부족한가봐",
          "original_form": "영양이 부족한가봐",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038097.1.13",
          "form": "양배추가 위에 좋대",
          "original_form": "양배추가 위에 좋대",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038097.1.14",
          "form": "몸이 전채적으로 비실비실하내",
          "original_form": "몸이 전채적으로 비실비실하내",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038097.1.15",
          "form": "나는 꺗잎이 먹고싶어",
          "original_form": "나는 꺗잎이 먹고싶어",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038097.1.16",
          "form": "ㅠㅠ툭치면 쓰러지겠다 얘",
          "original_form": "ㅠㅠ툭치면 쓰러지겠다 얘",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038097.1.17",
          "form": "깻잎맛있겠다",
          "original_form": "깻잎맛있겠다",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038097.1.18",
          "form": "절대아냐",
          "original_form": "절대아냐",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038097.1.19",
          "form": "안심해.",
          "original_form": "안심해.",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038097.1.20",
          "form": "다행이지..?",
          "original_form": "다행이지..?",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038097.1.21",
          "form": "깻잎반찬 맛있어",
          "original_form": "깻잎반찬 맛있어",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038097.1.22",
          "form": "알지. 깻잎은 다 맛있어",
          "original_form": "알지. 깻잎은 다 맛있어",
