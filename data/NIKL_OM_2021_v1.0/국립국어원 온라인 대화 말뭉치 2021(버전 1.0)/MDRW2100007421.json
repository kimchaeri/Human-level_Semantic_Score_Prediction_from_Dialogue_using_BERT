      "utterance": [
        {
          "id": "MDRW2100007421.1.1",
          "form": "싸이하셨나요?",
          "original_form": "싸이하셨나요?",
          "speaker_id": "1",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007421.1.2",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007421.1.3",
          "form": "많이했죠",
          "original_form": "많이했죠",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007421.1.4",
          "form": "친구들하고",
          "original_form": "친구들하고",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007421.1.5",
          "form": "우리다이어리 많이썻는데",
          "original_form": "우리다이어리 많이썻는데",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007421.1.6",
          "form": "오... 도토리도 사셨나요?",
          "original_form": "오... 도토리도 사셨나요?",
          "speaker_id": "1",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007421.1.7",
          "form": "그것도 부활되나요",
          "original_form": "그것도 부활되나요",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007421.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007421.1.9",
          "form": "도토리는 안삿어요",
          "original_form": "도토리는 안삿어요",
          "speaker_id": "2",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007421.1.10",
          "form": "사이버 재화를 구매하는데에 좀 인색해서",
          "original_form": "사이버 재화를 구매하는데에 좀 인색해서",
          "speaker_id": "2",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007421.1.11",
          "form": "도토리 누구한테 선물하셧나요",
          "original_form": "도토리 누구한테 선물하셧나요",
          "speaker_id": "2",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007421.1.12",
          "form": "정...씨 얼짱이요",
          "original_form": "정...씨 얼짱이요",
          "speaker_id": "1",
          "time": "20210822 02:02"
        },
        {
          "id": "MDRW2100007421.1.13",
          "form": "근데 저는 싸이월드보다는 좀 뒷세대라고 생각해서 스스로",
          "original_form": "근데 저는 싸이월드보다는 좀 뒷세대라고 생각해서 스스로",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007421.1.14",
          "form": "그분싸이월드도",
          "original_form": "그분싸이월드도",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007421.1.15",
          "form": "부활할까요",
          "original_form": "부활할까요",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007421.1.16",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007421.1.17",
          "form": "그러면 어느세대에요",
          "original_form": "그러면 어느세대에요",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007421.1.18",
          "form": "당시에 집에 컴퓨터가 없었어요",
          "original_form": "당시에 집에 컴퓨터가 없었어요",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007421.1.19",
          "form": "아 그럴 수 잇다",
          "original_form": "아 그럴 수 잇다",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007421.1.20",
          "form": "저는 네이버 블로그 세대예요!",
          "original_form": "저는 네이버 블로그 세대예요!",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007421.1.21",
          "form": "싸이월드 부활하면",
          "original_form": "싸이월드 부활하면",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007421.1.22",
          "form": "하면?",
          "original_form": "하면?",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007421.1.23",
          "form": "일촌으로 잃은 친구들을 좀 보겠네요",
          "original_form": "일촌으로 잃은 친구들을 좀 보겠네요",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007421.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007421.1.25",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007421.1.26",
          "form": "한명 남앗어요...",
          "original_form": "한명 남앗어요...",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007421.1.27",
          "form": "일촌중에서...",
          "original_form": "일촌중에서...",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007421.1.28",
          "form": "일촌이 기억나세요?",
          "original_form": "일촌이 기억나세요?",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007421.1.29",
          "form": "그때도",
          "original_form": "그때도",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007421.1.30",
          "form": "소수정예로 싸이해서",
          "original_form": "소수정예로 싸이해서",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007421.1.31",
          "form": "몇명없엇거든요",
          "original_form": "몇명없엇거든요",
          "speaker_id": "2",
          "time": "20210822 02:06"
        }
      ]
    }
  ]
