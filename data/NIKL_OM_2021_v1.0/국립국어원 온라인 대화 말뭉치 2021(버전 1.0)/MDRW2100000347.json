      "utterance": [
        {
          "id": "MDRW2100000347.1.1",
          "form": "안녕하세요 ㅎ",
          "original_form": "안녕하세요 ㅎ",
          "speaker_id": "1",
          "time": "20210819 15:27"
        },
        {
          "id": "MDRW2100000347.1.2",
          "form": "네. 안녕하세용",
          "original_form": "네. 안녕하세용",
          "speaker_id": "2",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000347.1.3",
          "form": "집안일 너무 힘들지 않아요? ㅠ",
          "original_form": "집안일 너무 힘들지 않아요? ㅠ",
          "speaker_id": "1",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000347.1.4",
          "form": "ㅠㅠ해도티도 안나고ㅠ",
          "original_form": "ㅠㅠ해도티도 안나고ㅠ",
          "speaker_id": "2",
          "time": "20210819 15:36"
        },
        {
          "id": "MDRW2100000347.1.5",
          "form": "가족들이랑 같이살아요? 자취해요?",
          "original_form": "가족들이랑 같이살아요? 자취해요?",
          "speaker_id": "1",
          "time": "20210819 15:36"
        },
        {
          "id": "MDRW2100000347.1.6",
          "form": "가족들이랑 살아용",
          "original_form": "가족들이랑 살아용",
          "speaker_id": "2",
          "time": "20210819 15:37"
        },
        {
          "id": "MDRW2100000347.1.7",
          "form": "저도 가족들이랑 사는데 엄마랑 빨래때매 맨날싸워요 ㅠㅠㅠㅋㅋㅋ 집안일 너무스트레스받아요",
          "original_form": "저도 가족들이랑 사는데 엄마랑 빨래때매 맨날싸워요 ㅠㅠㅠㅋㅋㅋ 집안일 너무스트레스받아요",
          "speaker_id": "1",
          "time": "20210819 15:38"
        },
        {
          "id": "MDRW2100000347.1.8",
          "form": "ㅋㅋㅋ저는 결혼해서 집안일하는데 정말ㅋㅋ티도안나고 힘들고그래요ㅠ",
          "original_form": "ㅋㅋㅋ저는 결혼해서 집안일하는데 정말ㅋㅋ티도안나고 힘들고그래요ㅠ",
          "speaker_id": "2",
          "time": "20210819 15:42"
        },
        {
          "id": "MDRW2100000347.1.9",
          "form": "아 결혼하면 진짜 집안일 많이하죠 ㅠㅠㅠㅠ",
          "original_form": "아 결혼하면 진짜 집안일 많이하죠 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 15:47"
        },
        {
          "id": "MDRW2100000347.1.10",
          "form": "일찍 결혼하고싶은데 집안일때매 살짝 망설여지기도 하는거같아요",
          "original_form": "일찍 결혼하고싶은데 집안일때매 살짝 망설여지기도 하는거같아요",
          "speaker_id": "1",
          "time": "20210819 15:47"
        },
        {
          "id": "MDRW2100000347.1.11",
          "form": "맞아요ㅠ정말 누가알아주지도 않으니ㅜㅠ요새는가사도 분담을 정말로 해야되용",
          "original_form": "맞아요ㅠ정말 누가알아주지도 않으니ㅜㅠ요새는가사도 분담을 정말로 해야되용",
          "speaker_id": "2",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000347.1.12",
          "form": "저는 나중에 로봇청소기 하나 사려구요 ㅋㅋㅋㅋ",
          "original_form": "저는 나중에 로봇청소기 하나 사려구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000347.1.13",
          "form": "청소 자주못해줄거같아서",
          "original_form": "청소 자주못해줄거같아서",
          "speaker_id": "1",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000347.1.14",
          "form": "로봇청소기좋죠ㅋㅋ그거는 바닥에 근데 물건이 없을때 유용 한듯해요",
          "original_form": "로봇청소기좋죠ㅋㅋ그거는 바닥에 근데 물건이 없을때 유용 한듯해요",
          "speaker_id": "2",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000347.1.15",
          "form": "아~ 하긴 바닥에 뭐가 많으면 다 피해가겠구나",
          "original_form": "아~ 하긴 바닥에 뭐가 많으면 다 피해가겠구나",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000347.1.16",
          "form": "네ㅠ결국은 그걸 치우는것도 일이죠ㅠㅠ",
          "original_form": "네ㅠ결국은 그걸 치우는것도 일이죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000347.1.17",
          "form": "뭐가됏든 다 일이네요 ㅋㅋㅋㅋ 나중에 돈벌어서 가사도우미 쓰는게 좋겠어요, 돈을 진짜많이벌어야하는게 함정이네",
          "original_form": "뭐가됏든 다 일이네요 ㅋㅋㅋㅋ 나중에 돈벌어서 가사도우미 쓰는게 좋겠어요, 돈을 진짜많이벌어야하는게 함정이네",
          "speaker_id": "1",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000347.1.18",
          "form": "그게 문제에요ㅋㅋ",
          "original_form": "그게 문제에요ㅋㅋ",
