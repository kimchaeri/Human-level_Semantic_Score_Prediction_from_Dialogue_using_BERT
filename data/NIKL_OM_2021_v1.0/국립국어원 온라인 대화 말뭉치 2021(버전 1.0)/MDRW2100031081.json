      "utterance": [
        {
          "id": "MDRW2100031081.1.1",
          "form": "난 고민중이야 간단하게 먹고싶은데",
          "original_form": "난 고민중이야 간단하게 먹고싶은데",
          "speaker_id": "2",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100031081.1.2",
          "form": "쌀 면 빵 중에 뭐 먹지",
          "original_form": "쌀 면 빵 중에 뭐 먹지",
          "speaker_id": "2",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100031081.1.3",
          "form": "과일이랑 삶은계란이 좋다더라",
          "original_form": "과일이랑 삶은계란이 좋다더라",
          "speaker_id": "1",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100031081.1.4",
          "form": "빵도 맛있지",
          "original_form": "빵도 맛있지",
          "speaker_id": "1",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100031081.1.5",
          "form": "삶은계란이랑 토마토 먹을까",
          "original_form": "삶은계란이랑 토마토 먹을까",
          "speaker_id": "2",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100031081.1.6",
          "form": "잘 구운 토스트 먹고싶다",
          "original_form": "잘 구운 토스트 먹고싶다",
          "speaker_id": "1",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100031081.1.7",
          "form": "나도!",
          "original_form": "나도!",
          "speaker_id": "2",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100031081.1.8",
          "form": "토달볶2잖아",
          "original_form": "토달볶2잖아",
          "speaker_id": "1",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100031081.1.9",
          "form": "근데 건강식이긴하다",
          "original_form": "근데 건강식이긴하다",
          "speaker_id": "1",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100031081.1.10",
          "form": "ㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ정말 너무해",
          "original_form": "ㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ정말 너무해",
          "speaker_id": "2",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100031081.1.11",
          "form": "토달볶 박해를 멈춰라 우우",
          "original_form": "토달볶 박해를 멈춰라 우우",
          "speaker_id": "2",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100031081.1.12",
          "form": "너 주먹밥을",
          "original_form": "너 주먹밥을",
          "speaker_id": "1",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100031081.1.13",
          "form": "참깨에 돌돌 굴린거 먹어봤냐",
          "original_form": "참깨에 돌돌 굴린거 먹어봤냐",
          "speaker_id": "1",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100031081.1.14",
          "form": "헐 대박 맛있겠다 아니 파래김은",
          "original_form": "헐 대박 맛있겠다 아니 파래김은",
          "speaker_id": "2",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100031081.1.15",
          "form": "굴려봤는데",
          "original_form": "굴려봤는데",
          "speaker_id": "2",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100031081.1.16",
          "form": "맛있단다 제법",
          "original_form": "맛있단다 제법",
          "speaker_id": "1",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100031081.1.17",
          "form": "깨에 2차로 굴려본 적은 없어",
          "original_form": "깨에 2차로 굴려본 적은 없어",
          "speaker_id": "2",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100031081.1.18",
          "form": "진짜 고소하겠다 침나와",
          "original_form": "진짜 고소하겠다 침나와",
          "speaker_id": "2",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100031081.1.19",
          "form": "주먹밥안에 땡초랑 오뎅볶은거 넣어도",
          "original_form": "주먹밥안에 땡초랑 오뎅볶은거 넣어도",
          "speaker_id": "1",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100031081.1.20",
          "form": "맛있다",
          "original_form": "맛있다",
          "speaker_id": "1",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100031081.1.21",
          "form": "헐 너 매운거 잘 먹니",
          "original_form": "헐 너 매운거 잘 먹니",
          "speaker_id": "2",
          "time": "20210904 16:28"
        },
        {
          "id": "MDRW2100031081.1.22",
          "form": "땡초 안 매워?",
          "original_form": "땡초 안 매워?",
          "speaker_id": "2",
          "time": "20210904 16:28"
        },
        {
          "id": "MDRW2100031081.1.23",
          "form": "조금만 넣으면 왠지 괜찮아",
          "original_form": "조금만 넣으면 왠지 괜찮아",
          "speaker_id": "1",
          "time": "20210904 16:28"
        },
        {
          "id": "MDRW2100031081.1.24",
          "form": "점점 혓바닥이 강해지고 있구나",
          "original_form": "점점 혓바닥이 강해지고 있구나",
          "speaker_id": "2",
          "time": "20210904 16:28"
        }
      ]
    }
