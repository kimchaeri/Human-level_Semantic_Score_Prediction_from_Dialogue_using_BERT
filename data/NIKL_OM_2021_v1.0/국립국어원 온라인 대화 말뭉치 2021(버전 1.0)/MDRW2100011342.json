      "utterance": [
        {
          "id": "MDRW2100011342.1.1",
          "form": "스포츠좋아하세요?",
          "original_form": "스포츠좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 10:09"
        },
        {
          "id": "MDRW2100011342.1.2",
          "form": "보는것만요ㅎㅎ",
          "original_form": "보는것만요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 10:30"
        },
        {
          "id": "MDRW2100011342.1.3",
          "form": "가구님은?",
          "original_form": "가구님은?",
          "speaker_id": "2",
          "time": "20210825 10:30"
        },
        {
          "id": "MDRW2100011342.1.4",
          "form": "저도 보는것 좋,아해요",
          "original_form": "저도 보는것 좋,아해요",
          "speaker_id": "1",
          "time": "20210825 11:17"
        },
        {
          "id": "MDRW2100011342.1.5",
          "form": "어떤스포츠요?",
          "original_form": "어떤스포츠요?",
          "speaker_id": "2",
          "time": "20210825 11:49"
        },
        {
          "id": "MDRW2100011342.1.6",
          "form": "전 올림픽 월드컵좋아하는데",
          "original_form": "전 올림픽 월드컵좋아하는데",
          "speaker_id": "2",
          "time": "20210825 11:49"
        },
        {
          "id": "MDRW2100011342.1.7",
          "form": "오 저도 둘다좋아해요",
          "original_form": "오 저도 둘다좋아해요",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100011342.1.8",
          "form": "올림픽 재밌죠ㅎ 어릴때부터 이건 거의 다챙겨봐요",
          "original_form": "올림픽 재밌죠ㅎ 어릴때부터 이건 거의 다챙겨봐요",
          "speaker_id": "2",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100011342.1.9",
          "form": "마자요 월드컵도",
          "original_form": "마자요 월드컵도",
          "speaker_id": "1",
          "time": "20210825 12:43"
        },
        {
          "id": "MDRW2100011342.1.10",
          "form": "야구나 축구 경기장도 가시나요",
          "original_form": "야구나 축구 경기장도 가시나요",
          "speaker_id": "2",
          "time": "20210825 12:46"
        },
        {
          "id": "MDRW2100011342.1.11",
          "form": "네네 직관가봤어요",
          "original_form": "네네 직관가봤어요",
          "speaker_id": "1",
          "time": "20210825 12:48"
        },
        {
          "id": "MDRW2100011342.1.12",
          "form": "재밌어요? 야구?",
          "original_form": "재밌어요? 야구?",
          "speaker_id": "2",
          "time": "20210825 12:48"
        },
        {
          "id": "MDRW2100011342.1.13",
          "form": "맥주응원 재밌어요ㅎㅎ",
          "original_form": "맥주응원 재밌어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:49"
        },
        {
          "id": "MDRW2100011342.1.14",
          "form": "농구장은 어릴때 가봤는데",
          "original_form": "농구장은 어릴때 가봤는데",
          "speaker_id": "2",
          "time": "20210825 12:50"
        },
        {
          "id": "MDRW2100011342.1.15",
          "form": "농구장도 재밈죠?",
          "original_form": "농구장도 재밈죠?",
          "speaker_id": "1",
          "time": "20210825 12:55"
        }
