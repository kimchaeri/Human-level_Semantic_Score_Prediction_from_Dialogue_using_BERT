      "utterance": [
        {
          "id": "MDRW2100034369.1.1",
          "form": "마자요 츄  많이 나오죠",
          "original_form": "마자요 츄  많이 나오죠",
          "speaker_id": "2",
          "time": "20210828 00:44"
        },
        {
          "id": "MDRW2100034369.1.2",
          "form": "그럼 신곡나온지 좀 됐나요?",
          "original_form": "그럼 신곡나온지 좀 됐나요?",
          "speaker_id": "1",
          "time": "20210828 00:44"
        },
        {
          "id": "MDRW2100034369.1.3",
          "form": "최근에 나왔었는데",
          "original_form": "최근에 나왔었는데",
          "speaker_id": "2",
          "time": "20210828 00:44"
        },
        {
          "id": "MDRW2100034369.1.4",
          "form": "코로나땜에 활동 일찍 끝냈더라구요",
          "original_form": "코로나땜에 활동 일찍 끝냈더라구요",
          "speaker_id": "2",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100034369.1.5",
          "form": "아 ㅠㅠ 망할 코로나..",
          "original_form": "아 ㅠㅠ 망할 코로나..",
          "speaker_id": "1",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100034369.1.6",
          "form": "요새 백댄서들도 마스크쓰더라고요",
          "original_form": "요새 백댄서들도 마스크쓰더라고요",
          "speaker_id": "1",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100034369.1.7",
          "form": "그니까요 숨차겠어요ㅠ\r\n퀸덤말고는 뭐 들으세요?",
          "original_form": "그니까요 숨차겠어요ㅠ\r\n퀸덤말고는 뭐 들으세요?",
          "speaker_id": "2",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100034369.1.8",
          "form": "음 넥스트레벨이요",
          "original_form": "음 넥스트레벨이요",
          "speaker_id": "1",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100034369.1.9",
          "form": "넥스트레벌도 좋죠",
          "original_form": "넥스트레벌도 좋죠",
          "speaker_id": "2",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100034369.1.10",
          "form": "중독성 장난 아니더라구요",
          "original_form": "중독성 장난 아니더라구요",
          "speaker_id": "2",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100034369.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100034369.1.12",
          "form": "sm쪽이시군요!",
          "original_form": "sm쪽이시군요!",
          "speaker_id": "2",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100034369.1.13",
          "form": "전 처음에 좀 난해한 부분이 있어서 ㅋㅋㅋ",
          "original_form": "전 처음에 좀 난해한 부분이 있어서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100034369.1.14",
          "form": "네 ㅋㅋㅋ 제가사실 에스엠 아이돌 팬이라..",
          "original_form": "네 ㅋㅋㅋ 제가사실 에스엠 아이돌 팬이라..",
          "speaker_id": "1",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100034369.1.15",
          "form": "마자요ㅋㅋ저두요",
          "original_form": "마자요ㅋㅋ저두요",
          "speaker_id": "2",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100034369.1.16",
          "form": "생각해보니 요새듣는 노래가 다 스엠이네요",
          "original_form": "생각해보니 요새듣는 노래가 다 스엠이네요",
          "speaker_id": "1",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100034369.1.17",
          "form": "이게 소속사 따라서 듣는거같아요ㅋㅋ",
          "original_form": "이게 소속사 따라서 듣는거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:47"
        },
