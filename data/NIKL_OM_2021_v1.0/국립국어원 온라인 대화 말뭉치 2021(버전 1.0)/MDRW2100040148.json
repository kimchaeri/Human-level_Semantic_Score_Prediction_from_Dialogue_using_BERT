      "utterance": [
        {
          "id": "MDRW2100040148.1.1",
          "form": "저희는 3키로에요 몇키로에요?",
          "original_form": "저희는 3키로에요 몇키로에요?",
          "speaker_id": "1",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100040148.1.2",
          "form": "와 너무 작겠네요 ㅜㅜ",
          "original_form": "와 너무 작겠네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100040148.1.3",
          "form": "저는 7키로요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 7키로요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100040148.1.4",
          "form": "들고다님 사람애 안고있는기분",
          "original_form": "들고다님 사람애 안고있는기분",
          "speaker_id": "2",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100040148.1.5",
          "form": "와 각각 이요?",
          "original_form": "와 각각 이요?",
          "speaker_id": "1",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100040148.1.6",
          "form": "넹 ㅜㅜ",
          "original_form": "넹 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100040148.1.7",
          "form": "코숏이라 떡대가 큰건지 ㅎㅎ",
          "original_form": "코숏이라 떡대가 큰건지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100040148.1.8",
          "form": "와우 밥은 뭐 먹여요",
          "original_form": "와우 밥은 뭐 먹여요",
          "speaker_id": "1",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100040148.1.9",
          "form": "무려 다이어트 사료요",
          "original_form": "무려 다이어트 사료요",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100040148.1.10",
          "form": "살이안빠지는 느낌 ㅜㅜ",
          "original_form": "살이안빠지는 느낌 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040148.1.11",
          "form": "오오 저희도 췌장 사료 먹여요",
          "original_form": "오오 저희도 췌장 사료 먹여요",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040148.1.12",
          "form": "병원 처방 사료 그냥 먹여도 된다고 해서",
          "original_form": "병원 처방 사료 그냥 먹여도 된다고 해서",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040148.1.13",
          "form": "힐스껀가요?",
          "original_form": "힐스껀가요?",
          "speaker_id": "2",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100040148.1.14",
          "form": "췌장에 문제가 있었는지...",
          "original_form": "췌장에 문제가 있었는지...",
          "speaker_id": "2",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100040148.1.15",
          "form": "네네 힐스거에요",
          "original_form": "네네 힐스거에요",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040148.1.16",
          "form": "췌장염이 살짝 왔어서 먹이는데",
          "original_form": "췌장염이 살짝 왔어서 먹이는데",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040148.1.17",
          "form": "계속 먹여도 괜찮다네요",
          "original_form": "계속 먹여도 괜찮다네요",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040148.1.18",
          "form": "어디거 먹이세요",
          "original_form": "어디거 먹이세요",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040148.1.19",
          "form": "저도 힐스 다이어트사료에요",
          "original_form": "저도 힐스 다이어트사료에요",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040148.1.20",
          "form": "근데 변기덩을 싸요 ㅜㅜ",
          "original_form": "근데 변기덩을 싸요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040148.1.21",
          "form": "변비",
          "original_form": "변비",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040148.1.22",
          "form": "다이어트 사료라 그런걸까요",
          "original_form": "다이어트 사료라 그런걸까요",
          "speaker_id": "2",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100040148.1.23",
          "form": "오오 힐스세요",
          "original_form": "오오 힐스세요",
          "speaker_id": "1",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100040148.1.24",
          "form": "아마도그럴거같아요ㅠㅠ",
          "original_form": "아마도그럴거같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 12:49"
        },
        {
          "id": "MDRW2100040148.1.25",
          "form": "음슈량을 늘려보세요ㅠㅠ",
          "original_form": "음슈량을 늘려보세요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 12:49"
        },
        {
          "id": "MDRW2100040148.1.26",
          "form": "캔간식 줄때 물을 섞어쥬는데 ㅜㅜ 그래도 부족한가바요",
          "original_form": "캔간식 줄때 물을 섞어쥬는데 ㅜㅜ 그래도 부족한가바요",
          "speaker_id": "2",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100040148.1.27",
          "form": "췌장염 증상이 있었나요?",
          "original_form": "췌장염 증상이 있었나요?",
          "speaker_id": "2",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100040148.1.28",
          "form": "네 살짝 있었어요 자주 토해서ㅠㅠ",
          "original_form": "네 살짝 있었어요 자주 토해서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100040148.1.29",
          "form": "검사결과는 문제없어요",
          "original_form": "검사결과는 문제없어요",
          "speaker_id": "1",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100040148.1.30",
          "form": "다행이에요 ㅠㅠ 고양이는 원래 토하는 애들이라 ㄷㄷ",
          "original_form": "다행이에요 ㅠㅠ 고양이는 원래 토하는 애들이라 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210905 09:32"
        },
        {
          "id": "MDRW2100040148.1.31",
          "form": "자주토하면 꼭 병원가봐야겠네요",
          "original_form": "자주토하면 꼭 병원가봐야겠네요",
          "speaker_id": "2",
          "time": "20210905 09:32"
        },
        {
          "id": "MDRW2100040148.1.32",
          "form": "아 자주 그런다면서요 헤어볼이랑 ㅠㅠ",
          "original_form": "아 자주 그런다면서요 헤어볼이랑 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 09:33"
        }
      ]
    }
