        {
          "id": "MDRW2100036788.1.1",
          "form": "햄버거 좋아하세요?",
          "original_form": "햄버거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100036788.1.2",
          "form": "네 맥도날드 ㅎㅎ 핫스파이스 ㅎㅎ 자주먹어요 ㅎㅎ",
          "original_form": "네 맥도날드 ㅎㅎ 핫스파이스 ㅎㅎ 자주먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:31"
        },
        {
          "id": "MDRW2100036788.1.3",
          "form": "핫스파이시 ㅎㅎ",
          "original_form": "핫스파이시 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:32"
        },
        {
          "id": "MDRW2100036788.1.4",
          "form": "아 상하이 어쩌고 그거 말씀이시죠?",
          "original_form": "아 상하이 어쩌고 그거 말씀이시죠?",
          "speaker_id": "2",
          "time": "20210831 12:33"
        },
        {
          "id": "MDRW2100036788.1.5",
          "form": "맞아요 그거 ㅋㅋ",
          "original_form": "맞아요 그거 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:33"
        },
        {
          "id": "MDRW2100036788.1.6",
          "form": "예전에 그거 엄청 자주 먹었어요",
          "original_form": "예전에 그거 엄청 자주 먹었어요",
          "speaker_id": "2",
          "time": "20210831 12:34"
        },
        {
          "id": "MDRW2100036788.1.7",
          "form": "상하이요",
          "original_form": "상하이요",
          "speaker_id": "2",
          "time": "20210831 12:34"
        },
        {
          "id": "MDRW2100036788.1.8",
          "form": "맛있어요 닭도두툼하고",
          "original_form": "맛있어요 닭도두툼하고",
          "speaker_id": "1",
          "time": "20210831 12:36"
        },
        {
          "id": "MDRW2100036788.1.9",
          "form": "약간 매운맛나는거도 맛있구요",
          "original_form": "약간 매운맛나는거도 맛있구요",
          "speaker_id": "2",
          "time": "20210831 12:37"
        },
        {
          "id": "MDRW2100036788.1.10",
          "form": "맞아요 ㅎㅎ 매콤한게 좋아요",
          "original_form": "맞아요 ㅎㅎ 매콤한게 좋아요",
          "speaker_id": "1",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100036788.1.11",
          "form": "맥날 슈비버거 드셔보셨어요?",
          "original_form": "맥날 슈비버거 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210831 14:59"
        },
        {
          "id": "MDRW2100036788.1.12",
          "form": "네 ㅎㅎ 먹어봤어요 한번 인가",
          "original_form": "네 ㅎㅎ 먹어봤어요 한번 인가",
          "speaker_id": "1",
          "time": "20210831 15:00"
        },
        {
          "id": "MDRW2100036788.1.13",
          "form": "슈비버거 맛있더라구요 고기랑 새우 다 있어서좋아요 ㅋㅋ",
          "original_form": "슈비버거 맛있더라구요 고기랑 새우 다 있어서좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:03"
        },
        {
          "id": "MDRW2100036788.1.14",
          "form": "새우도 두툼하고",
          "original_form": "새우도 두툼하고",
          "speaker_id": "1",
          "time": "20210831 15:11"
        },
        {
          "id": "MDRW2100036788.1.15",
          "form": "네 새우가 생각보다 크더라구요",
