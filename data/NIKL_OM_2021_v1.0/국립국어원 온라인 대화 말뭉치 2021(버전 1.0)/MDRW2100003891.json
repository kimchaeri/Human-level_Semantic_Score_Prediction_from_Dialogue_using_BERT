      "utterance": [
        {
          "id": "MDRW2100003891.1.1",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 13:20"
        },
        {
          "id": "MDRW2100003891.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210821 13:21"
        },
        {
          "id": "MDRW2100003891.1.3",
          "form": "어떤 커뮤니티 하세요?",
          "original_form": "어떤 커뮤니티 하세요?",
          "speaker_id": "2",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003891.1.4",
          "form": "네이버 카페",
          "original_form": "네이버 카페",
          "speaker_id": "1",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003891.1.5",
          "form": "블로그요",
          "original_form": "블로그요",
          "speaker_id": "1",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003891.1.6",
          "form": "거기서 주식과 장남감 광물 지역카페",
          "original_form": "거기서 주식과 장남감 광물 지역카페",
          "speaker_id": "1",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003891.1.7",
          "form": "활동해요",
          "original_form": "활동해요",
          "speaker_id": "1",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003891.1.8",
          "form": "어디 활동 하세요?",
          "original_form": "어디 활동 하세요?",
          "speaker_id": "1",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003891.1.9",
          "form": "전 다음 카페도 하고 블로그도 해요",
          "original_form": "전 다음 카페도 하고 블로그도 해요",
          "speaker_id": "2",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003891.1.10",
          "form": "아 저도 카페랑 블로그가 친숙하니 좋더라구요",
          "original_form": "아 저도 카페랑 블로그가 친숙하니 좋더라구요",
          "speaker_id": "1",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003891.1.11",
          "form": "정보 찾거나 주식 공부 목적이요",
          "original_form": "정보 찾거나 주식 공부 목적이요",
          "speaker_id": "2",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003891.1.12",
          "form": "예전엔 정모도 했는데 요새 안해서 아쉬워요",
          "original_form": "예전엔 정모도 했는데 요새 안해서 아쉬워요",
          "speaker_id": "1",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003891.1.13",
          "form": "다른 sns는 불편해요",
          "original_form": "다른 sns는 불편해요",
          "speaker_id": "2",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003891.1.14",
          "form": "코로나가 모임의 형태를 다 바꿨어요",
          "original_form": "코로나가 모임의 형태를 다 바꿨어요",
          "speaker_id": "1",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003891.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003891.1.16",
          "form": "소통이 원활하지 않아요",
          "original_form": "소통이 원활하지 않아요",
          "speaker_id": "1",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003891.1.17",
          "form": "아하 정모도 나가셨나요? 전 그냥 온라인 활동만 했거든요",
          "original_form": "아하 정모도 나가셨나요? 전 그냥 온라인 활동만 했거든요",
          "speaker_id": "2",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003891.1.18",
          "form": "예전에 갔어요 ㅎㅎ밥먹고 수다 잼났어요",
          "original_form": "예전에 갔어요 ㅎㅎ밥먹고 수다 잼났어요",
          "speaker_id": "1",
          "time": "20210821 13:26"
        },
        {
          "id": "MDRW2100003891.1.19",
          "form": "저도 주로 온라인이죠",
          "original_form": "저도 주로 온라인이죠",
          "speaker_id": "1",
          "time": "20210821 13:26"
        },
        {
          "id": "MDRW2100003891.1.20",
          "form": "중고나라서 물건도 팔고 주식블로그에선 주식 정보보고",
          "original_form": "중고나라서 물건도 팔고 주식블로그에선 주식 정보보고",
          "speaker_id": "1",
          "time": "20210821 13:27"
        },
        {
          "id": "MDRW2100003891.1.21",
          "form": "도움 많이돼요",
          "original_form": "도움 많이돼요",
          "speaker_id": "1",
          "time": "20210821 13:27"
        },
        {
          "id": "MDRW2100003891.1.22",
          "form": "무슨 카페예요?",
          "original_form": "무슨 카페예요?",
          "speaker_id": "1",
          "time": "20210821 13:27"
        },
        {
          "id": "MDRW2100003891.1.23",
          "form": "요즘은 정모같은게 어려운 세상인것 같아요",
          "original_form": "요즘은 정모같은게 어려운 세상인것 같아요",
          "speaker_id": "2",
          "time": "20210821 13:27"
        },
        {
          "id": "MDRW2100003891.1.24",
          "form": "맞아요 글로는 손도 느리고 안좋아요",
          "original_form": "맞아요 글로는 손도 느리고 안좋아요",
          "speaker_id": "1",
          "time": "20210821 13:28"
        },
        {
          "id": "MDRW2100003891.1.25",
          "form": "그냥 여초 카페들이나 주식 관련 블로그 봐요",
          "original_form": "그냥 여초 카페들이나 주식 관련 블로그 봐요",
          "speaker_id": "2",
          "time": "20210821 13:28"
        },
        {
          "id": "MDRW2100003891.1.26",
          "form": "아 저도 주식은 블로그봐요 ㅎㅎ",
          "original_form": "아 저도 주식은 블로그봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 13:30"
        },
        {
          "id": "MDRW2100003891.1.27",
          "form": "아무래도 정보나 공부하려면 필요해요",
          "original_form": "아무래도 정보나 공부하려면 필요해요",
          "speaker_id": "2",
          "time": "20210821 13:32"
        },
        {
          "id": "MDRW2100003891.1.28",
          "form": "네 그래야 주식도하지 막하다 마이너수 났어요",
          "original_form": "네 그래야 주식도하지 막하다 마이너수 났어요",
          "speaker_id": "1",
          "time": "20210821 13:33"
        },
        {
          "id": "MDRW2100003891.1.29",
          "form": "전 블로그 보고 공모주해요",
          "original_form": "전 블로그 보고 공모주해요",
          "speaker_id": "1",
          "time": "20210821 13:34"
        },
        {
          "id": "MDRW2100003891.1.30",
          "form": "카페는 정보랑 장난감 발매정보 봐요",
          "original_form": "카페는 정보랑 장난감 발매정보 봐요",
          "speaker_id": "1",
          "time": "20210821 13:34"
        }
      ]
