      "utterance": [
        {
          "id": "MDRW2100045185.1.1",
          "form": "올해는 여기저기서 다 초당옥수수 맛내나봐요 ㅋㅋㅋ",
          "original_form": "올해는 여기저기서 다 초당옥수수 맛내나봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045185.1.2",
          "form": "유행인가봐요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "유행인가봐요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045185.1.3",
          "form": "작년엔 한창 샤인머스캣이더니 ㅋㅋㅋ",
          "original_form": "작년엔 한창 샤인머스캣이더니 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045185.1.4",
          "form": "다음엔 무슨맛이 유행할지 궁금하네요",
          "original_form": "다음엔 무슨맛이 유행할지 궁금하네요",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045185.1.5",
          "form": "그쵸 ㅋㅋㅋ 되도록 안겹치는 쪽일거같은데",
          "original_form": "그쵸 ㅋㅋㅋ 되도록 안겹치는 쪽일거같은데",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045185.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ기대되네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ기대되네요",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045185.1.7",
          "form": "음식은 뭐 좋아하세요?",
          "original_form": "음식은 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045185.1.8",
          "form": "음식 크게 가리는거 없어요!",
          "original_form": "음식 크게 가리는거 없어요!",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045185.1.9",
          "form": "초밥 좋아해요 ㅋㅋㅋ",
          "original_form": "초밥 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045185.1.10",
          "form": "무슨초밥 제일 좋아하세요?",
          "original_form": "무슨초밥 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045185.1.11",
          "form": "저 단새우초밥이요!",
          "original_form": "저 단새우초밥이요!",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045185.1.12",
          "form": "어떤 음식 좋아하시나요?",
          "original_form": "어떤 음식 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045185.1.13",
          "form": "앗 저는 생새우초밥좋아해요 ㅋㅋㅋㅋ",
          "original_form": "앗 저는 생새우초밥좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045185.1.14",
          "form": "생새우 초밥도 맛있죠 ㅠ",
          "original_form": "생새우 초밥도 맛있죠 ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045185.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ광어초밥도 좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ광어초밥도 좋아해요",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045185.1.16",
          "form": "광어 저 지느러미쪽 좋아해요! ㅋㅋㅋ",
          "original_form": "광어 저 지느러미쪽 좋아해요! ㅋㅋㅋ",
          "speaker_id": "1",
