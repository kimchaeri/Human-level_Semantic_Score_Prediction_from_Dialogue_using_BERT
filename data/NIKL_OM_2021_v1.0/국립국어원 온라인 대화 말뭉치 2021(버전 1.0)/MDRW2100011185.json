      "utterance": [
        {
          "id": "MDRW2100011185.1.1",
          "form": "출근 잘하셨나요",
          "original_form": "출근 잘하셨나요",
          "speaker_id": "2",
          "time": "20210824 11:00"
        },
        {
          "id": "MDRW2100011185.1.2",
          "form": "날씨 어떤가요 ㅎㅎ",
          "original_form": "날씨 어떤가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:01"
        },
        {
          "id": "MDRW2100011185.1.3",
          "form": "네 아침출근길에 비살짝오던데",
          "original_form": "네 아침출근길에 비살짝오던데",
          "speaker_id": "1",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100011185.1.4",
          "form": "많이는 안오더라구요",
          "original_form": "많이는 안오더라구요",
          "speaker_id": "1",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100011185.1.5",
          "form": "퇴근길은 어떠셨나요ㅎ",
          "original_form": "퇴근길은 어떠셨나요ㅎ",
          "speaker_id": "1",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100011185.1.6",
          "form": "ㅎㅎ 아직 침실이에요",
          "original_form": "ㅎㅎ 아직 침실이에요",
          "speaker_id": "2",
          "time": "20210824 11:08"
        },
        {
          "id": "MDRW2100011185.1.7",
          "form": "이제 짐 싸서 가려구요",
          "original_form": "이제 짐 싸서 가려구요",
          "speaker_id": "2",
          "time": "20210824 11:08"
        },
        {
          "id": "MDRW2100011185.1.8",
          "form": "아 이제퇴근이군요ㅎ 피곤하시겠어요",
          "original_form": "아 이제퇴근이군요ㅎ 피곤하시겠어요",
          "speaker_id": "1",
          "time": "20210824 11:09"
        },
        {
          "id": "MDRW2100011185.1.9",
          "form": "비많이오나요?거긴?",
          "original_form": "비많이오나요?거긴?",
          "speaker_id": "1",
          "time": "20210824 11:09"
        },
        {
          "id": "MDRW2100011185.1.10",
          "form": "조금씩 내리기 시작했어요 ㅎㅎ",
          "original_form": "조금씩 내리기 시작했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100011185.1.11",
          "form": "거기는요?",
          "original_form": "거기는요?",
          "speaker_id": "2",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100011185.1.12",
          "form": "여기도 조금오는거같아요",
          "original_form": "여기도 조금오는거같아요",
          "speaker_id": "1",
          "time": "20210824 11:34"
        },
        {
          "id": "MDRW2100011185.1.13",
          "form": "태풍온다는데 여긴 영향없나봐요",
          "original_form": "태풍온다는데 여긴 영향없나봐요",
          "speaker_id": "1",
          "time": "20210824 11:36"
        },
        {
          "id": "MDRW2100011185.1.14",
          "form": "어우 꽤 오는데요ㅎㅎ",
          "original_form": "어우 꽤 오는데요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100011185.1.15",
          "form": "네 밥먹고오니 많이오네요ㅎ",
          "original_form": "네 밥먹고오니 많이오네요ㅎ",
          "speaker_id": "1",
          "time": "20210824 12:49"
        },
        {
          "id": "MDRW2100011185.1.16",
          "form": "밑에 지방은 심하다던데ㅠ",
          "original_form": "밑에 지방은 심하다던데ㅠ",
          "speaker_id": "2",
          "time": "20210824 12:59"
        },
        {
          "id": "MDRW2100011185.1.17",
          "form": "수원도 좀많이온대요",
          "original_form": "수원도 좀많이온대요",
          "speaker_id": "1",
          "time": "20210824 13:03"
        },
        {
          "id": "MDRW2100011185.1.18",
          "form": "태풍지나간건지 오고있는건지",
          "original_form": "태풍지나간건지 오고있는건지",
          "speaker_id": "1",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100011185.1.19",
          "form": "지금 비행기도 결항이고 난리네요",
          "original_form": "지금 비행기도 결항이고 난리네요",
          "speaker_id": "2",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011185.1.20",
          "form": "아 그래요? 제주도 뱅기 끊어달래서 담주끊었는데",
          "original_form": "아 그래요? 제주도 뱅기 끊어달래서 담주끊었는데",
          "speaker_id": "1",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011185.1.21",
          "form": "다음주에는 진정되지 않을까요 ㅎㅎ",
          "original_form": "다음주에는 진정되지 않을까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011185.1.22",
          "form": "날씨보고 끊으라한거니 괜찮겠죠",
          "original_form": "날씨보고 끊으라한거니 괜찮겠죠",
          "speaker_id": "1",
          "time": "20210824 14:23"
        }
      ]
