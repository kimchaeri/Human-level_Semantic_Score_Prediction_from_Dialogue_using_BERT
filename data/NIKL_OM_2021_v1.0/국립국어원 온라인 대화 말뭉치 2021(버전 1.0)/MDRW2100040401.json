      "utterance": [
        {
          "id": "MDRW2100040401.1.1",
          "form": "나도 케밥은 그냥그래 ㅋㅋ",
          "original_form": "나도 케밥은 그냥그래 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100040401.1.2",
          "form": "ㅋㅋ타코는 그 바삭바삭함이랑 과카몰리? 그 소스가 맛있는듯ㅋㅋㅋ",
          "original_form": "ㅋㅋ타코는 그 바삭바삭함이랑 과카몰리? 그 소스가 맛있는듯ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100040401.1.3",
          "form": "마자마자 ㅋㅋㅋ그무침같은거 ㅌㅋㅋㅋ",
          "original_form": "마자마자 ㅋㅋㅋ그무침같은거 ㅌㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100040401.1.4",
          "form": "고수도 같이먹어야하는ㄷ0",
          "original_form": "고수도 같이먹어야하는ㄷ0",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100040401.1.5",
          "form": "ㅋㅋㅋㅋ고수파 등장",
          "original_form": "ㅋㅋㅋㅋ고수파 등장",
          "speaker_id": "1",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100040401.1.6",
          "form": "근데 나도 한 2년밖에안됨",
          "original_form": "근데 나도 한 2년밖에안됨",
          "speaker_id": "2",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100040401.1.7",
          "form": "먹은지 ㅋㅋㅋ",
          "original_form": "먹은지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100040401.1.8",
          "form": "바질은 먹나유?",
          "original_form": "바질은 먹나유?",
          "speaker_id": "2",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100040401.1.9",
          "form": "바질은 먹쥬!ㅋㅋㅋㅋ",
          "original_form": "바질은 먹쥬!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100040401.1.10",
          "form": "바질이랑 비슷해?",
          "original_form": "바질이랑 비슷해?",
          "speaker_id": "1",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100040401.1.11",
          "form": "그냥 ㅋㅋ비슷은 아니구",
          "original_form": "그냥 ㅋㅋ비슷은 아니구",
          "speaker_id": "2",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100040401.1.12",
          "form": "향이나니깐 ㅋㅋㅋ",
          "original_form": "향이나니깐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100040401.1.13",
          "form": "ㅋㅋㅋ바질은 괜찮은데",
          "original_form": "ㅋㅋㅋ바질은 괜찮은데",
          "speaker_id": "1",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100040401.1.14",
          "form": "난 근데 미나리도 잘안먹음ㅋㅋㅋ",
          "original_form": "난 근데 미나리도 잘안먹음ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:18"
        },
        {
          "id": "MDRW2100040401.1.15",
          "form": "나도 잘먹진않앗는데",
          "original_form": "나도 잘먹진않앗는데",
          "speaker_id": "2",
          "time": "20210904 18:18"
        },
        {
          "id": "MDRW2100040401.1.16",
          "form": "미나리삼겹살먹고 ㅋㅋㅋㅋ이제 잘먹읍 ㅋㅋㅋㅋ",
          "original_form": "미나리삼겹살먹고 ㅋㅋㅋㅋ이제 잘먹읍 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:18"
        },
        {
          "id": "MDRW2100040401.1.17",
          "form": "ㅋㅋㅋ아 맞아 그건 좀 괜찮지 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ아 맞아 그건 좀 괜찮지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:18"
        },
        {
          "id": "MDRW2100040401.1.18",
          "form": "ㅋㅋㅋ구워먹어도맛잇고",
          "original_form": "ㅋㅋㅋ구워먹어도맛잇고",
          "speaker_id": "2",
          "time": "20210904 18:19"
        },
        {
          "id": "MDRW2100040401.1.19",
          "form": "나는 산초도 싫어해 ㅋㅋㅋ",
          "original_form": "나는 산초도 싫어해 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:19"
        },
        {
          "id": "MDRW2100040401.1.20",
          "form": "또 ㅋㅋ그냥싸먹어도",
          "original_form": "또 ㅋㅋ그냥싸먹어도",
          "speaker_id": "2",
          "time": "20210904 18:19"
        },
        {
          "id": "MDRW2100040401.1.21",
          "form": "난 산초를 제대로먹어본적없는거같아",
          "original_form": "난 산초를 제대로먹어본적없는거같아",
          "speaker_id": "2",
          "time": "20210904 18:19"
        },
        {
          "id": "MDRW2100040401.1.22",
          "form": "ㅋㅋㅋ삼겹살 덕분에 미나리가 묻힘",
          "original_form": "ㅋㅋㅋ삼겹살 덕분에 미나리가 묻힘",
          "speaker_id": "1",
          "time": "20210904 18:19"
        },
        {
          "id": "MDRW2100040401.1.23",
          "form": "근데 전에 마라탕새로운집에선 산초가 쌔던데 그게 산초인가봐 ㅋㅋㅋ",
          "original_form": "근데 전에 마라탕새로운집에선 산초가 쌔던데 그게 산초인가봐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:19"
        },
        {
          "id": "MDRW2100040401.1.24",
          "form": "난 누가 산초 장아찌? 줬는거 먹어봤는데 영 아니더라 ㅋㅋㅋ",
          "original_form": "난 누가 산초 장아찌? 줬는거 먹어봤는데 영 아니더라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100040401.1.25",
          "form": "그것두 잎파리여?",
          "original_form": "그것두 잎파리여?",
          "speaker_id": "2",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100040401.1.26",
          "form": "제피가루? 그 추어탕에 뿌려먹는것도 별로여 ㅋㅋ",
          "original_form": "제피가루? 그 추어탕에 뿌려먹는것도 별로여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100040401.1.27",
          "form": "나두 안먹어봄 !!",
          "original_form": "나두 안먹어봄 !!",
          "speaker_id": "2",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100040401.1.28",
          "form": "열매같이 생겼던데 동글동글",
          "original_form": "열매같이 생겼던데 동글동글",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100040401.1.29",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100040401.1.30",
          "form": "내일은.너가.요리사해봐",
          "original_form": "내일은.너가.요리사해봐",
          "speaker_id": "2",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100040401.1.31",
