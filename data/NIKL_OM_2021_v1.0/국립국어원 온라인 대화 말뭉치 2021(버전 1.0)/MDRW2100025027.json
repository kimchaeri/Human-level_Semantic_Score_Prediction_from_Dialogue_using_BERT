      "utterance": [
        {
          "id": "MDRW2100025027.1.1",
          "form": "김치찌개에 그거도 들어가는군요..",
          "original_form": "김치찌개에 그거도 들어가는군요..",
          "speaker_id": "2",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100025027.1.2",
          "form": "레드페퍼파우더ㄷ넣가",
          "original_form": "레드페퍼파우더ㄷ넣가",
          "speaker_id": "1",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100025027.1.3",
          "form": "ㅇ정말 재료를 필터링하면 어쩌자는건지",
          "original_form": "ㅇ정말 재료를 필터링하면 어쩌자는건지",
          "speaker_id": "1",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100025027.1.4",
          "form": "그리고 푹끓이셔야해요",
          "original_form": "그리고 푹끓이셔야해요",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100025027.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100025027.1.6",
          "form": "찌개는원래 오래 끓여야해서",
          "original_form": "찌개는원래 오래 끓여야해서",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100025027.1.7",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100025027.1.8",
          "form": "이마트에",
          "original_form": "이마트에",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100025027.1.9",
          "form": "꼭 성공하시길",
          "original_form": "꼭 성공하시길",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100025027.1.10",
          "form": "김치찌개 밀키트 맛있는 거 있었는데",
          "original_form": "김치찌개 밀키트 맛있는 거 있었는데",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100025027.1.11",
          "form": "단종됐더라구요",
          "original_form": "단종됐더라구요",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100025027.1.12",
          "form": "정용진 정말..후....",
          "original_form": "정용진 정말..후....",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100025027.1.13",
          "form": "요새 이마트 밀키트 진짜많던데",
          "original_form": "요새 이마트 밀키트 진짜많던데",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100025027.1.14",
          "form": "트레이더스에ㅅ파는",
          "original_form": "트레이더스에ㅅ파는",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100025027.1.15",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100025027.1.16",
          "form": "부대찌개 밀키트 진짜크더라구요",
          "original_form": "부대찌개 밀키트 진짜크더라구요",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100025027.1.17",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100025027.1.18",
          "form": "그거 만사천원인ㄱ하는데 그거 사면",
          "original_form": "그거 만사천원인ㄱ하는데 그거 사면",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100025027.1.19",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100025027.1.20",
          "form": "5명도먹어요",
          "original_form": "5명도먹어요",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100025027.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100025027.1.22",
          "form": "대식가신데!",
          "original_form": "대식가신데!",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100025027.1.23",
          "form": "맛도 그럭저럭괜찮더라구요",
          "original_form": "맛도 그럭저럭괜찮더라구요",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100025027.1.24",
          "form": "5명이나먹을수있다니!",
          "original_form": "5명이나먹을수있다니!",
          "speaker_id": "2",
          "time": "20210831 22:30"
        }
      ]
