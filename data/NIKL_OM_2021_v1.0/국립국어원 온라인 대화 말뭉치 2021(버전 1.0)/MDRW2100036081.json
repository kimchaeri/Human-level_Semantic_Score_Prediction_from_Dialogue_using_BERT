        {
          "id": "MDRW2100036081.1.1",
          "form": "ㄱㄱㄱㅋㄱㅋㄱ거기도 난리군요",
          "original_form": "ㄱㄱㄱㅋㄱㅋㄱ거기도 난리군요",
