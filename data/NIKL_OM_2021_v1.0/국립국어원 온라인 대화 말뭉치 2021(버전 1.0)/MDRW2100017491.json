      "utterance": [
        {
          "id": "MDRW2100017491.1.1",
          "form": "어떤 스포츠 좋아하셔요",
          "original_form": "어떤 스포츠 좋아하셔요",
          "speaker_id": "1",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017491.1.2",
          "form": "배구요",
          "original_form": "배구요",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017491.1.3",
          "form": "아 직관하러 가세요?",
          "original_form": "아 직관하러 가세요?",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017491.1.4",
          "form": "이번에 올림픽보고 너무 배구에 흥미가 생겼어요",
          "original_form": "이번에 올림픽보고 너무 배구에 흥미가 생겼어요",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017491.1.5",
          "form": "직관 해보고 싶네요",
          "original_form": "직관 해보고 싶네요",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017491.1.6",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017491.1.7",
          "form": "김연경 너무 멋진 사람 ㅠㅠ",
          "original_form": "김연경 너무 멋진 사람 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017491.1.8",
          "form": "응원하는 팀 있으세요?",
          "original_form": "응원하는 팀 있으세요?",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017491.1.9",
          "form": "사실 배구는 올림픽에서 본 게 다라서 배구팀은 잘 모르고",
          "original_form": "사실 배구는 올림픽에서 본 게 다라서 배구팀은 잘 모르고",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017491.1.10",
          "form": "응원하면서 보던 스포츠는 야구밖에 없어서",
          "original_form": "응원하면서 보던 스포츠는 야구밖에 없어서",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017491.1.11",
          "form": "오 야구  어느팀이요?",
          "original_form": "오 야구  어느팀이요?",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017491.1.12",
          "form": "지역이 부산이라 롯데요ㅋㅋㅋㅋ",
          "original_form": "지역이 부산이라 롯데요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017491.1.13",
          "form": "직관해보셨나요?",
          "original_form": "직관해보셨나요?",
          "speaker_id": "2",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017491.1.14",
          "form": "네네 사직구장 종종 가서 맥주마시며 응원했었는데",
          "original_form": "네네 사직구장 종종 가서 맥주마시며 응원했었는데",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017491.1.15",
          "form": "코로나 이후로는 잘 못가네요",
          "original_form": "코로나 이후로는 잘 못가네요",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017491.1.16",
          "form": "요즘은 무관중이죠?",
          "original_form": "요즘은 무관중이죠?",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017491.1.17",
          "form": "그런 것 같더라구요",
          "original_form": "그런 것 같더라구요",
          "speaker_id": "1",
          "time": "20210826 19:55"
        }
      ]
