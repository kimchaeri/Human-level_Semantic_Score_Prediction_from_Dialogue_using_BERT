      "utterance": [
        {
          "id": "MDRW2100030396.1.1",
          "form": "하얀 로냐프 강 기억하시나요",
          "original_form": "하얀 로냐프 강 기억하시나요",
          "speaker_id": "2",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100030396.1.2",
          "form": "그때 책을 참 많이 읽었네요",
          "original_form": "그때 책을 참 많이 읽었네요",
          "speaker_id": "1",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100030396.1.3",
          "form": "공부는 안 하고",
          "original_form": "공부는 안 하고",
          "speaker_id": "1",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100030396.1.4",
          "form": "다시 읽어 보고 싶네요",
          "original_form": "다시 읽어 보고 싶네요",
          "speaker_id": "2",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100030396.1.5",
          "form": "당연히 기억하지요 가즈나이트도",
          "original_form": "당연히 기억하지요 가즈나이트도",
          "speaker_id": "1",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030396.1.6",
          "form": "드래곤 라자도 재밌게 읽었죠",
          "original_form": "드래곤 라자도 재밌게 읽었죠",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030396.1.7",
          "form": "다시 보면 더 좋을 것 같아요",
          "original_form": "다시 보면 더 좋을 것 같아요",
          "speaker_id": "1",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030396.1.8",
          "form": "정말 재밌었어요",
          "original_form": "정말 재밌었어요",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030396.1.9",
          "form": "세월의 돌 이었나요",
          "original_form": "세월의 돌 이었나요",
          "speaker_id": "1",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030396.1.10",
          "form": "요즘에는 없겠죠",
          "original_form": "요즘에는 없겠죠",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030396.1.11",
          "form": "세월의 돌도 정말 재미있었어요",
          "original_form": "세월의 돌도 정말 재미있었어요",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030396.1.12",
          "form": "뭔가 엄청 많이 읽었어요",
          "original_form": "뭔가 엄청 많이 읽었어요",
          "speaker_id": "1",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030396.1.13",
          "form": "퇴마록 또 재밌었고",
          "original_form": "퇴마록 또 재밌었고",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030396.1.14",
          "form": "드래곤 라자",
          "original_form": "드래곤 라자",
          "speaker_id": "1",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030396.1.15",
          "form": "명작들이 많았네요",
          "original_form": "명작들이 많았네요",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030396.1.16",
          "form": "드래곤 라자 매번 기다렸던 기억이 나네요",
