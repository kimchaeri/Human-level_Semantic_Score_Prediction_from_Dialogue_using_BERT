      "utterance": [
        {
          "id": "MDRW2100016772.1.1",
          "form": "그냥 부쳐도 맛있어요",
          "original_form": "그냥 부쳐도 맛있어요",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016772.1.2",
          "form": "피자치즈 냉동시켜놓으면 편해요ㅎ",
          "original_form": "피자치즈 냉동시켜놓으면 편해요ㅎ",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016772.1.3",
          "form": "시간 지났을때 껍질과 분리되는 것도 없고",
          "original_form": "시간 지났을때 껍질과 분리되는 것도 없고",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016772.1.4",
          "form": "오그렇군요 저도 애호박 되게 좋아해요",
          "original_form": "오그렇군요 저도 애호박 되게 좋아해요",
          "speaker_id": "1",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016772.1.5",
          "form": "호박은 뭘해 먹어도 맛있는 것 같아요ㅎ",
          "original_form": "호박은 뭘해 먹어도 맛있는 것 같아요ㅎ",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016772.1.6",
          "form": "전집 가도 호박부터 손이 가요",
          "original_form": "전집 가도 호박부터 손이 가요",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016772.1.7",
          "form": "맞아요 ㅎㅎ달달해서",
          "original_form": "맞아요 ㅎㅎ달달해서",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016772.1.8",
          "form": "늙은호박?전도 맛있잖아요 호박은 다 맛있네요",
          "original_form": "늙은호박?전도 맛있잖아요 호박은 다 맛있네요",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016772.1.9",
          "form": "맞아요!!!단호박찜도 좋고",
          "original_form": "맞아요!!!단호박찜도 좋고",
          "speaker_id": "2",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100016772.1.10",
          "form": "노각으로는 주로 어떻게 요리하세요?",
          "original_form": "노각으로는 주로 어떻게 요리하세요?",
          "speaker_id": "2",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016772.1.11",
          "form": "제가 해본적은 없지만 무쳐먹었던 기억이..노각있으세요?",
          "original_form": "제가 해본적은 없지만 무쳐먹었던 기억이..노각있으세요?",
          "speaker_id": "1",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016772.1.12",
          "form": "네;; 최근 어떤분이 주셨어요",
          "original_form": "네;; 최근 어떤분이 주셨어요",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016772.1.13",
          "form": "채쳐서 무쳐먹는 거 얘기하시는 거죠?",
          "original_form": "채쳐서 무쳐먹는 거 얘기하시는 거죠?",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016772.1.14",
          "form": "잘 모르겠어요ㅋㅋㅋ",
          "original_form": "잘 모르겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016772.1.15",
          "form": "빨간 양념이었는데..",
          "original_form": "빨간 양념이었는데..",
          "speaker_id": "1",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016772.1.16",
          "form": "어; 내가 아는건 색이 하얀데...",
          "original_form": "어; 내가 아는건 색이 하얀데...",
          "speaker_id": "2",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100016772.1.17",
          "form": "빨간양녕도 궁금하네요",
          "original_form": "빨간양녕도 궁금하네요",
          "speaker_id": "2",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100016772.1.18",
          "form": "네 식감도 좋고 맛있더라구요 !",
          "original_form": "네 식감도 좋고 맛있더라구요 !",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016772.1.19",
          "form": "아 이거군요! 검색해보니 알겠어요",
          "original_form": "아 이거군요! 검색해보니 알겠어요",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016772.1.20",
          "form": "수박 흰테두리로도 비슷하게 해먹던 거군요",
          "original_form": "수박 흰테두리로도 비슷하게 해먹던 거군요",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016772.1.21",
          "form": "그쵸ㅎㅎ전 그거만 먹어봤어요",
          "original_form": "그쵸ㅎㅎ전 그거만 먹어봤어요",
          "speaker_id": "1",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100016772.1.22",
          "form": "아 수박으로도 해먹더라구요",
          "original_form": "아 수박으로도 해먹더라구요",
          "speaker_id": "1",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100016772.1.23",
          "form": "저도 딴거랑 헷갈린듯ㅎ",
          "original_form": "저도 딴거랑 헷갈린듯ㅎ",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100016772.1.24",
          "form": "제가 아는 노각요리도 이거네요",
          "original_form": "제가 아는 노각요리도 이거네요",
          "speaker_id": "2",
          "time": "20210826 15:26"
        }
      ]
    }
