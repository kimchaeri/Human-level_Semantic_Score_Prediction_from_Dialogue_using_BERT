      "utterance": [
        {
          "id": "MDRW2100025518.1.1",
          "form": "우산쓰고 가면 되지 ㅋㅋ",
          "original_form": "우산쓰고 가면 되지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100025518.1.2",
          "form": "비 넘 많이 오는디ㅋㅋ",
          "original_form": "비 넘 많이 오는디ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:32"
        },
        {
          "id": "MDRW2100025518.1.3",
          "form": "지금 갈거 아니라서 ㅋㅋ",
          "original_form": "지금 갈거 아니라서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100025518.1.4",
          "form": "하긴 저녁  메뉴이니ㅋㅋㅋ",
          "original_form": "하긴 저녁  메뉴이니ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025518.1.5",
          "form": "점심 먹었니",
          "original_form": "점심 먹었니",
          "speaker_id": "2",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100025518.1.6",
          "form": "아점? 갈치구이해서ㅋㅋ 넌?",
          "original_form": "아점? 갈치구이해서ㅋㅋ 넌?",
          "speaker_id": "1",
          "time": "20210901 13:16"
        },
        {
          "id": "MDRW2100025518.1.7",
          "form": "라면 먹을까 생각중ㅋㅋ",
          "original_form": "라면 먹을까 생각중ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100025518.1.8",
          "form": "라면 하니까 앵그리너구리 먹고싶네ㅎㅎ",
          "original_form": "라면 하니까 앵그리너구리 먹고싶네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:30"
        },
        {
          "id": "MDRW2100025518.1.9",
          "form": "만사 귀찮네 꼭 집에 없는 게 먹고싶고 ㅋㅋㅋ",
          "original_form": "만사 귀찮네 꼭 집에 없는 게 먹고싶고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:35"
        },
        {
          "id": "MDRW2100025518.1.10",
          "form": "청개구리 심보구먼ㅋㅋㅋ",
          "original_form": "청개구리 심보구먼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:38"
        },
        {
          "id": "MDRW2100025518.1.11",
          "form": "김치우동 먹고싶네",
          "original_form": "김치우동 먹고싶네",
          "speaker_id": "2",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025518.1.12",
          "form": "비오니 뜨끈한 궁물이 땡기는구나ㅎㅎ",
          "original_form": "비오니 뜨끈한 궁물이 땡기는구나ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100025518.1.13",
          "form": "어묵이 없어",
          "original_form": "어묵이 없어",
          "speaker_id": "2",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025518.1.14",
          "form": "어제부터 먹고싶었는데 김치우동 크흡",
          "original_form": "어제부터 먹고싶었는데 김치우동 크흡",
          "speaker_id": "2",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025518.1.15",
          "form": "투다리.. 간이역...언제 먹어봤나",
          "original_form": "투다리.. 간이역...언제 먹어봤나",
