        },
        {
          "id": "MDRW2100045201.1.2",
          "form": "본거야",
          "original_form": "본거야",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045201.1.3",
          "form": "아니 몰아서 볼려고",
          "original_form": "아니 몰아서 볼려고",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045201.1.4",
          "form": "재밌나봐",
          "original_form": "재밌나봐",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045201.1.5",
          "form": "티비엔꺼",
          "original_form": "티비엔꺼",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045201.1.6",
          "form": "티비엔이 드라마 많이 한다",
          "original_form": "티비엔이 드라마 많이 한다",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045201.1.7",
          "form": "응 거기다 특이한것도",
          "original_form": "응 거기다 특이한것도",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045201.1.8",
          "form": "이게 지성 나오는거 맞지",
          "original_form": "이게 지성 나오는거 맞지",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045201.1.9",
          "form": "엉 김민정이랑 같이",
          "original_form": "엉 김민정이랑 같이",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045201.1.10",
          "form": "김민정 여전히 이쁘더라",
          "original_form": "김민정 여전히 이쁘더라",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045201.1.11",
          "form": "생각보다 나이가 있을껀데 아직 동안",
          "original_form": "생각보다 나이가 있을껀데 아직 동안",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045201.1.12",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045201.1.13",
          "form": "지성이 연기를 잘하더라",
          "original_form": "지성이 연기를 잘하더라",
