      "utterance": [
        {
          "id": "MDRW2100023150.1.1",
          "form": "네.. 오랜만에 비 안와서 그런지..",
          "original_form": "네.. 오랜만에 비 안와서 그런지..",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100023150.1.2",
          "form": "사람 바글바글..",
          "original_form": "사람 바글바글..",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023150.1.3",
          "form": "네ㅋㅋㅋㅋ 그럴 거 같아요",
          "original_form": "네ㅋㅋㅋㅋ 그럴 거 같아요",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023150.1.4",
          "form": "날도 시원하고",
          "original_form": "날도 시원하고",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023150.1.5",
          "form": "운동하기 딱이죠",
          "original_form": "운동하기 딱이죠",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023150.1.6",
          "form": "또 다음주 비 예보라.. 다시 강제 집콕하겟어요..",
          "original_form": "또 다음주 비 예보라.. 다시 강제 집콕하겟어요..",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023150.1.7",
          "form": "ㅠ 아 비 정말 싫어요...ㅡ",
          "original_form": "ㅠ 아 비 정말 싫어요...ㅡ",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023150.1.8",
          "form": "전 경기도라 그나마 나은데.. \n중부이남은 엄청 난듯...",
          "original_form": "전 경기도라 그나마 나은데.. \n중부이남은 엄청 난듯...",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023150.1.9",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100023150.1.10",
          "form": "엄청.. 난리죠",
          "original_form": "엄청.. 난리죠",
          "speaker_id": "2",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100023150.1.11",
          "form": "날씨 민감한 일하시나요?",
          "original_form": "날씨 민감한 일하시나요?",
          "speaker_id": "1",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023150.1.12",
          "form": "그던 아니에요 ㅠ",
          "original_form": "그던 아니에요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:10"
        },
        {
          "id": "MDRW2100023150.1.13",
          "form": "근데 가는 아침이 너무 싫죠",
          "original_form": "근데 가는 아침이 너무 싫죠",
          "speaker_id": "2",
          "time": "20210830 00:11"
        },
        {
          "id": "MDRW2100023150.1.14",
          "form": "요즘 출근할땐 제법 시원하죠?",
          "original_form": "요즘 출근할땐 제법 시원하죠?",
          "speaker_id": "1",
          "time": "20210830 00:14"
        },
        {
          "id": "MDRW2100023150.1.15",
          "form": "시원한데 비가와서",
          "original_form": "시원한데 비가와서",
          "speaker_id": "2",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023150.1.16",
          "form": "애매해요 ㅠㅠ",
          "original_form": "애매해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023150.1.17",
          "form": "대중교통 이용할땐 비오면 진짜 에바긴하됴.",
          "original_form": "대중교통 이용할땐 비오면 진짜 에바긴하됴.",
          "speaker_id": "1",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023150.1.18",
          "form": "넵 ㅠ 전 걸어가서",
          "original_form": "넵 ㅠ 전 걸어가서",
          "speaker_id": "2",
          "time": "20210830 00:42"
        },
        {
          "id": "MDRW2100023150.1.19",
          "form": "더 그래여..최악이져..",
          "original_form": "더 그래여..최악이져..",
          "speaker_id": "2",
          "time": "20210830 00:42"
        },
        {
          "id": "MDRW2100023150.1.20",
          "form": "아.. 도보시면.. 비오면 신발도 다젖고..",
          "original_form": "아.. 도보시면.. 비오면 신발도 다젖고..",
          "speaker_id": "1",
          "time": "20210830 00:44"
        },
        {
          "id": "MDRW2100023150.1.21",
          "form": "ㅋㅋㅋㅋㅋㅠ 네 헬게이트에여",
          "original_form": "ㅋㅋㅋㅋㅋㅠ 네 헬게이트에여",
          "speaker_id": "2",
          "time": "20210830 00:44"
        }
      ]
