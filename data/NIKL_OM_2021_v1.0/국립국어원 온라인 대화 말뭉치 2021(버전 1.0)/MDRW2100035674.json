      "utterance": [
        {
          "id": "MDRW2100035674.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035674.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210831 20:50"
        },
        {
          "id": "MDRW2100035674.1.3",
          "form": "저녁 식사 하셨나요!",
          "original_form": "저녁 식사 하셨나요!",
          "speaker_id": "2",
          "time": "20210831 20:50"
        },
        {
          "id": "MDRW2100035674.1.4",
          "form": "네 저녁 드셨나요?",
          "original_form": "네 저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210831 20:52"
        },
        {
          "id": "MDRW2100035674.1.5",
          "form": "네 전 닭고기에 야채 곁들여서 구워먹었답니다ㅎㅎ",
          "original_form": "네 전 닭고기에 야채 곁들여서 구워먹었답니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100035674.1.6",
          "form": "name1님은 어떤 걸 드셨나요?",
          "original_form": "&name1&님은 어떤 걸 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100035674.1.7",
          "form": "전 떡볶이 먹었어요",
          "original_form": "전 떡볶이 먹었어요",
          "speaker_id": "1",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100035674.1.8",
          "form": "직접 만들어드셨나요?",
          "original_form": "직접 만들어드셨나요?",
          "speaker_id": "1",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100035674.1.9",
          "form": "닭 한마리를 사서 손질한 건 아니고 닭안심만 따로 나오는 걸 사서 구워먹는데 엄청 간편하더라구요",
          "original_form": "닭 한마리를 사서 손질한 건 아니고 닭안심만 따로 나오는 걸 사서 구워먹는데 엄청 간편하더라구요",
          "speaker_id": "2",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100035674.1.10",
          "form": "떡볶이는 만들어드셨나요?",
          "original_form": "떡볶이는 만들어드셨나요?",
          "speaker_id": "2",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100035674.1.11",
          "form": "저는 냉동된거 해동해서 먹었어요",
          "original_form": "저는 냉동된거 해동해서 먹었어요",
          "speaker_id": "1",
          "time": "20210831 21:01"
        },
        {
          "id": "MDRW2100035674.1.12",
          "form": "좋아하시는 음식 있으신가요?",
          "original_form": "좋아하시는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210831 21:01"
        },
        {
          "id": "MDRW2100035674.1.13",
          "form": "요즘은 굴소스볶음밥이 그렇게 맛있더라구요",
          "original_form": "요즘은 굴소스볶음밥이 그렇게 맛있더라구요",
          "speaker_id": "2",
          "time": "20210831 21:02"
        },
        {
          "id": "MDRW2100035674.1.14",
          "form": "가장 좋아하는 음식 어떤 건가요?",
          "original_form": "가장 좋아하는 음식 어떤 건가요?",
          "speaker_id": "2",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100035674.1.15",
          "form": "저는 떡볶이요",
          "original_form": "저는 떡볶이요",
          "speaker_id": "1",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100035674.1.16",
          "form": "ㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:04"
        },
        {
          "id": "MDRW2100035674.1.17",
          "form": "젤 좋아하시는 거 드셨군요",
          "original_form": "젤 좋아하시는 거 드셨군요",
          "speaker_id": "2",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100035674.1.18",
          "form": "매운 걸 좋아하시나요?",
          "original_form": "매운 걸 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100035674.1.19",
          "form": "매운거 좋아는 하는데 잘 못먹어요\r\nname2님은요?",
          "original_form": "매운거 좋아는 하는데 잘 못먹어요\r\n&name2&님은요?",
          "speaker_id": "1",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100035674.1.20",
          "form": "매운걸 좋아하고 잘 먹기도 하는데, 늘 먹고 나서 돈고의 심판을 받아요",
          "original_form": "매운걸 좋아하고 잘 먹기도 하는데, 늘 먹고 나서 돈고의 심판을 받아요",
          "speaker_id": "2",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100035674.1.21",
          "form": "앗!!그렇군요",
          "original_form": "앗!!그렇군요",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100035674.1.22",
          "form": "별로 야채 별로 안좋아해요",
          "original_form": "별로 야채 별로 안좋아해요",
          "speaker_id": "1",
          "time": "20210831 21:10"
        }
      ]
