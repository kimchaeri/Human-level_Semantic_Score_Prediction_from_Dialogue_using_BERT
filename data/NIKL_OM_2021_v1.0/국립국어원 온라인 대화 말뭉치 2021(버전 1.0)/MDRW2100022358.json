      "utterance": [
        {
          "id": "MDRW2100022358.1.1",
          "form": "너 책 자주보냐",
          "original_form": "너 책 자주보냐",
          "speaker_id": "2",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100022358.1.2",
          "form": "음 예전에는 소설 뚝딱뚝딱 읽었는데",
          "original_form": "음 예전에는 소설 뚝딱뚝딱 읽었는데",
          "speaker_id": "1",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100022358.1.3",
          "form": "요즘은 정보성 도서 읽느라",
          "original_form": "요즘은 정보성 도서 읽느라",
          "speaker_id": "1",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100022358.1.4",
          "form": "한권 한권 시간이 걸려서",
          "original_form": "한권 한권 시간이 걸려서",
          "speaker_id": "1",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022358.1.5",
          "form": "난 최근에 보건교사안은영읽었음",
          "original_form": "난 최근에 보건교사안은영읽었음",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022358.1.6",
          "form": "자주 못 봄",
          "original_form": "자주 못 봄",
          "speaker_id": "1",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022358.1.7",
          "form": "정보성도서 니가",
          "original_form": "정보성도서 니가",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022358.1.8",
          "form": "어때",
          "original_form": "어때",
          "speaker_id": "1",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022358.1.9",
          "form": "재밌니",
          "original_form": "재밌니",
          "speaker_id": "1",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022358.1.10",
          "form": "정리하는거 보고있어",
          "original_form": "정리하는거 보고있어",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022358.1.11",
          "form": "어??",
          "original_form": "어??",
          "speaker_id": "1",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022358.1.12",
          "form": "어떤거 말하는거지",
          "original_form": "어떤거 말하는거지",
          "speaker_id": "1",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022358.1.13",
          "form": "안은영 재밌더라",
          "original_form": "안은영 재밌더라",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022358.1.14",
          "form": "유성애로 끝나서 아쉽긴한데",
          "original_form": "유성애로 끝나서 아쉽긴한데",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022358.1.15",
          "form": "경제용어",
          "original_form": "경제용어",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022358.1.16",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022358.1.17",
          "form": "ㅎㅎㅎㅎㅎㅎ뿌듯하구만",
          "original_form": "ㅎㅎㅎㅎㅎㅎ뿌듯하구만",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022358.1.18",
          "form": "안은영 유성애니 떼잉",
          "original_form": "안은영 유성애니 떼잉",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022358.1.19",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022358.1.20",
          "form": "세계관이 특히 재밋ㄷㅎ얻",
          "original_form": "세계관이 특히 재밋ㄷㅎ얻",
          "speaker_id": "2",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022358.1.21",
          "form": "주인공이 째밌다고",
          "original_form": "주인공이 째밌다고",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022358.1.22",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022358.1.23",
          "form": "위시리스트에 넣어두고",
          "original_form": "위시리스트에 넣어두고",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022358.1.24",
          "form": "아직 안 읽고 있는데",
          "original_form": "아직 안 읽고 있는데",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022358.1.25",
          "form": "시간내서 봐야겠는걸?",
          "original_form": "시간내서 봐야겠는걸?",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022358.1.26",
          "form": "그거 드라마도 독특하다고",
          "original_form": "그거 드라마도 독특하다고",
          "speaker_id": "1",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022358.1.27",
          "form": "사람들이 호평했던 걸로 기억해",
          "original_form": "사람들이 호평했던 걸로 기억해",
          "speaker_id": "1",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022358.1.28",
          "form": "근데 난 드라마보다",
          "original_form": "근데 난 드라마보다",
          "speaker_id": "1",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022358.1.29",
          "form": "보건교사 안은영 쓴 작가가",
          "original_form": "보건교사 안은영 쓴 작가가",
          "speaker_id": "2",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022358.1.30",
          "form": "원작 책을 좋아해서 책 읽어보려구",
          "original_form": "원작 책을 좋아해서 책 읽어보려구",
          "speaker_id": "1",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022358.1.31",
          "form": "쓴 다른책도 꽤 재밌어",
          "original_form": "쓴 다른책도 꽤 재밌어",
          "speaker_id": "2",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022358.1.32",
          "form": "정세랑 작가였나",
          "original_form": "정세랑 작가였나",
          "speaker_id": "1",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022358.1.33",
          "form": "ㄷ맞아",
          "original_form": "ㄷ맞아",
          "speaker_id": "2",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022358.1.34",
          "form": "피프티피플이라는 책도",
          "original_form": "피프티피플이라는 책도",
          "speaker_id": "2",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022358.1.35",
          "form": "재밌어",
          "original_form": "재밌어",
          "speaker_id": "2",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022358.1.36",
          "form": "사이버도서관에 있나",
          "original_form": "사이버도서관에 있나",
          "speaker_id": "1",
          "time": "20210829 13:06"
        },
        {
          "id": "MDRW2100022358.1.37",
          "form": "찾아볼까 잠만",
          "original_form": "찾아볼까 잠만",
          "speaker_id": "1",
          "time": "20210829 13:06"
        },
        {
          "id": "MDRW2100022358.1.38",
          "form": "있을지도",
          "original_form": "있을지도",
          "speaker_id": "2",
          "time": "20210829 13:06"
        },
        {
          "id": "MDRW2100022358.1.39",
          "form": "이런.. 없다",
          "original_form": "이런.. 없다",
          "speaker_id": "1",
          "time": "20210829 13:06"
        },
        {
          "id": "MDRW2100022358.1.40",
          "form": "안타깝다",
          "original_form": "안타깝다",
          "speaker_id": "1",
          "time": "20210829 13:06"
        },
        {
          "id": "MDRW2100022358.1.41",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "2",
          "time": "20210829 13:07"
        },
        {
          "id": "MDRW2100022358.1.42",
          "form": "나 폰이 고장나서 자꾸",
          "original_form": "나 폰이 고장나서 자꾸",
          "speaker_id": "2",
          "time": "20210829 13:07"
        }
      ]
    }
