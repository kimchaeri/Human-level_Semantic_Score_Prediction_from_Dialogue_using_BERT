          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007050.1.3",
          "form": "배송비만 들고 있어요",
          "original_form": "배송비만 들고 있어요",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007050.1.4",
          "form": "저는 음.. 중고로 많이 사요",
          "original_form": "저는 음.. 중고로 많이 사요",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007050.1.5",
          "form": "전 아직 중고거래는 안해봤어요",
          "original_form": "전 아직 중고거래는 안해봤어요",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007050.1.6",
          "form": "네 요샌 뭐 사고싶으세요",
          "original_form": "네 요샌 뭐 사고싶으세요",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007050.1.7",
          "form": "옷이요 님은요",
          "original_form": "옷이요 님은요",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007050.1.8",
          "form": "음 저는 자동차를 바꾸고 싶어요",
          "original_form": "음 저는 자동차를 바꾸고 싶어요",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007050.1.9",
          "form": "자동차는 있으시죠?",
          "original_form": "자동차는 있으시죠?",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007050.1.10",
          "form": "중고 알아보고 있어요",
          "original_form": "중고 알아보고 있어요",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007050.1.11",
          "form": "아하. bmw좋아요\r\nbus metro walk",
          "original_form": "아하. bmw좋아요\r\nbus metro walk",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007050.1.12",
          "form": "네 물론 좋죠 전 suv 사고싶어요",
          "original_form": "네 물론 좋죠 전 suv 사고싶어요",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007050.1.13",
          "form": "오호.....요즘 대세 에슈비",
          "original_form": "오호.....요즘 대세 에슈비",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007050.1.14",
          "form": "지비육공 나와요",
          "original_form": "지비육공 나와요",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007050.1.15",
          "form": "아파트도 쇼핑하고 싶어요",
          "original_form": "아파트도 쇼핑하고 싶어요",
