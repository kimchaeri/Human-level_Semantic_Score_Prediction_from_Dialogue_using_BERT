      "utterance": [
        {
          "id": "MDRW2100030326.1.1",
          "form": "감사해요",
          "original_form": "감사해요",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030326.1.2",
          "form": "저는 내일 감자탕 먹어야 돼요",
          "original_form": "저는 내일 감자탕 먹어야 돼요",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030326.1.3",
          "form": "며칠째죠?",
          "original_form": "며칠째죠?",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030326.1.4",
          "form": "아직 많이 남았어요",
          "original_form": "아직 많이 남았어요",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030326.1.5",
          "form": "얼마나요",
          "original_form": "얼마나요",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030326.1.6",
          "form": "솥에 끓이셨나요?",
          "original_form": "솥에 끓이셨나요?",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030326.1.7",
          "form": "5일정도요ㅜㅜ",
          "original_form": "5일정도요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030326.1.8",
          "form": "아오 ㅠㅠ",
          "original_form": "아오 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030326.1.9",
          "form": "한 솥 양껏 끓였어요",
          "original_form": "한 솥 양껏 끓였어요",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030326.1.10",
          "form": "너무많아요",
          "original_form": "너무많아요",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030326.1.11",
          "form": "이제 이틀만 먹으면 돼요",
          "original_form": "이제 이틀만 먹으면 돼요",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030326.1.12",
          "form": "혼자먹나요?",
          "original_form": "혼자먹나요?",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030326.1.13",
          "form": "잘 먹어야 또 만들어 줘요",
          "original_form": "잘 먹어야 또 만들어 줘요",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030326.1.14",
          "form": "아 저런",
          "original_form": "아 저런",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030326.1.15",
          "form": "남편은 질렸대요",
          "original_form": "남편은 질렸대요",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030326.1.16",
          "form": "그런느낌인가요",
          "original_form": "그런느낌인가요",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030326.1.17",
          "form": "아까우니까 먹어야죠",
          "original_form": "아까우니까 먹어야죠",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030326.1.18",
          "form": "맛은있지요?",
          "original_form": "맛은있지요?",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030326.1.19",
          "form": "버리면 아깝죠",
          "original_form": "버리면 아깝죠",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030326.1.20",
          "form": "많이데우면 안짜나요",
          "original_form": "많이데우면 안짜나요",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030326.1.21",
          "form": "당연히 맛있지요",
          "original_form": "당연히 맛있지요",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030326.1.22",
          "form": "물넣고 데우나요",
          "original_form": "물넣고 데우나요",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030326.1.23",
          "form": "물을 추가해요",
          "original_form": "물을 추가해요",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030326.1.24",
          "form": "제배속에 버려주시죠",
          "original_form": "제배속에 버려주시죠",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030326.1.25",
          "form": "김밥 재료 샀어요",
          "original_form": "김밥 재료 샀어요",
          "speaker_id": "1",
          "time": "20210903 23:50"
        }
