      "utterance": [
        {
          "id": "MDRW2100009197.1.1",
          "form": "겜 하시는거 있나요??",
          "original_form": "겜 하시는거 있나요??",
          "speaker_id": "1",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009197.1.2",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009197.1.3",
          "form": "돈스타브 사랑하죠",
          "original_form": "돈스타브 사랑하죠",
          "speaker_id": "2",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009197.1.4",
          "form": "당신은...",
          "original_form": "당신은...",
          "speaker_id": "2",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009197.1.5",
          "form": "그 느구죠 젤다?",
          "original_form": "그 느구죠 젤다?",
          "speaker_id": "2",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009197.1.6",
          "form": "링크거든요",
          "original_form": "링크거든요",
          "speaker_id": "1",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009197.1.7",
          "form": "일부러그랬어요",
          "original_form": "일부러그랬어요",
          "speaker_id": "2",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009197.1.8",
          "form": "야숨 새로운 시리즈를 애타게 기다리고있어요",
          "original_form": "야숨 새로운 시리즈를 애타게 기다리고있어요",
          "speaker_id": "1",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009197.1.9",
          "form": "화나네",
          "original_form": "화나네",
          "speaker_id": "1",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009197.1.10",
          "form": "재밌다",
          "original_form": "재밌다",
          "speaker_id": "2",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009197.1.11",
          "form": "뭐가 젤 재밌나요",
          "original_form": "뭐가 젤 재밌나요",
          "speaker_id": "2",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009197.1.12",
          "form": "저도 돈만있음 하고싶은",
          "original_form": "저도 돈만있음 하고싶은",
          "speaker_id": "2",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009197.1.13",
          "form": "젤다 시리즈중에서요?",
          "original_form": "젤다 시리즈중에서요?",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009197.1.14",
          "form": "사실 전 야숨이랑 꿈섬밖에 안해봐서",
          "original_form": "사실 전 야숨이랑 꿈섬밖에 안해봐서",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009197.1.15",
          "form": "찐젤다팬이라 할수없음... 야숨팬",
          "original_form": "찐젤다팬이라 할수없음... 야숨팬",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009197.1.16",
          "form": "야생의 숨결 자체가 재밌어요 스위치 있는 사람 모두에게 추천하는중",
          "original_form": "야생의 숨결 자체가 재밌어요 스위치 있는 사람 모두에게 추천하는중",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009197.1.17",
          "form": "아 웃기다",
          "original_form": "아 웃기다",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009197.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009197.1.19",
          "form": "좋아요 이런건",
          "original_form": "좋아요 이런건",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009197.1.20",
          "form": "많이하는게",
          "original_form": "많이하는게",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009197.1.21",
          "form": "시작하고 15분만에 재미없고 어렵다며 접었던 친구도",
          "original_form": "시작하고 15분만에 재미없고 어렵다며 접었던 친구도",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009197.1.22",
          "form": "재밋더라고요 대게",
          "original_form": "재밋더라고요 대게",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009197.1.23",
          "form": "한달뒤 플레이타임 100시간을 캡쳐해 보내주는",
          "original_form": "한달뒤 플레이타임 100시간을 캡쳐해 보내주는",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009197.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009197.1.25",
          "form": "와 이거 찐이네요",
          "original_form": "와 이거 찐이네요",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009197.1.26",
          "form": "근데 좀 비싸던데",
          "original_form": "근데 좀 비싸던데",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009197.1.27",
          "form": "큰맘먹고사야해요",
          "original_form": "큰맘먹고사야해요",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009197.1.28",
          "form": "ㅠ 맞죠... 4만얼만가",
          "original_form": "ㅠ 맞죠... 4만얼만가",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009197.1.29",
          "form": "7만인가...",
          "original_form": "7만인가...",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009197.1.30",
          "form": "저랑 뿌요뿌요해요",
          "original_form": "저랑 뿌요뿌요해요",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009197.1.31",
          "form": "가물가물",
          "original_form": "가물가물",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009197.1.32",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009197.1.33",
          "form": "7만대엿던듯 ㅠㅠ",
          "original_form": "7만대엿던듯 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009197.1.34",
          "form": "뿌요뿌요테트리스?",
          "original_form": "뿌요뿌요테트리스?",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009197.1.35",
          "form": "그냥 테트리스는 무료로 있길래 가끔하고있어요",
          "original_form": "그냥 테트리스는 무료로 있길래 가끔하고있어요",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009197.1.36",
          "form": "네 뿌요쁘요 테트리스요",
          "original_form": "네 뿌요쁘요 테트리스요",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009197.1.37",
          "form": "pc랑 싸울때",
          "original_form": "pc랑 싸울때",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009197.1.38",
          "form": "진짜 잘해서 킹받아요",
          "original_form": "진짜 잘해서 킹받아요",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009197.1.39",
          "form": "킹받아요가 된다니",
          "original_form": "킹받아요가 된다니",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009197.1.40",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009197.1.41",
          "form": "왕받아요",
          "original_form": "왕받아요",
          "speaker_id": "1",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009197.1.42",
          "form": "그냥 테트리스랑 뭐가다른가요??",
          "original_form": "그냥 테트리스랑 뭐가다른가요??",
          "speaker_id": "1",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009197.1.43",
          "form": "뿌요뿌요 안해보셨어요?",
          "original_form": "뿌요뿌요 안해보셨어요?",
          "speaker_id": "2",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009197.1.44",
          "form": "그것도 할수잇고",
          "original_form": "그것도 할수잇고",
          "speaker_id": "2",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009197.1.45",
          "form": "레벨진짜 많는데",
          "original_form": "레벨진짜 많는데",
          "speaker_id": "2",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009197.1.46",
          "form": "...고수예요 진짜",
          "original_form": "...고수예요 진짜",
          "speaker_id": "2",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009197.1.47",
          "form": "아...뿌요뿌요안해봤어요",
          "original_form": "아...뿌요뿌요안해봤어요",
          "speaker_id": "1",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009197.1.48",
          "form": "제가 많은게임들을 좋아하는데",
          "original_form": "제가 많은게임들을 좋아하는데",
          "speaker_id": "1",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009197.1.49",
          "form": "오락실게임류를 잘 못해요...",
          "original_form": "오락실게임류를 잘 못해요...",
          "speaker_id": "1",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009197.1.50",
          "form": "이럴수가..",
          "original_form": "이럴수가..",
          "speaker_id": "2",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009197.1.51",
          "form": "저는 오직 고전뿐인데..",
          "original_form": "저는 오직 고전뿐인데..",
          "speaker_id": "2",
          "time": "20210822 22:17"
        }
      ]
    }
  ]
