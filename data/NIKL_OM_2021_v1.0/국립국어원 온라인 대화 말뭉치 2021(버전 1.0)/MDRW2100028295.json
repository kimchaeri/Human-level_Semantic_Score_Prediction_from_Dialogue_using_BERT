      "utterance": [
        {
          "id": "MDRW2100028295.1.1",
          "form": "저한창 오버워치 할 떄 들어갔는데",
          "original_form": "저한창 오버워치 할 떄 들어갔는데",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100028295.1.2",
          "form": "같이 피시방가나여?",
          "original_form": "같이 피시방가나여?",
          "speaker_id": "1",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100028295.1.3",
          "form": "자리야로 팟지 먹어서 스타벅스카드 받았던 기억이 나네요 ㅎㅎ",
          "original_form": "자리야로 팟지 먹어서 스타벅스카드 받았던 기억이 나네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100028295.1.4",
          "form": "정모를 피씨방에서 해요 ㅋㅋ",
          "original_form": "정모를 피씨방에서 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100028295.1.5",
          "form": "되게 좋았던 동아리...",
          "original_form": "되게 좋았던 동아리...",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028295.1.6",
          "form": "우와 ㅋㅋㅋㅋㅋㅋ진짜 피씨방가는거였군여",
          "original_form": "우와 ㅋㅋㅋㅋㅋㅋ진짜 피씨방가는거였군여",
          "speaker_id": "1",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028295.1.7",
          "form": "저희 있는 데는 여대였어서",
          "original_form": "저희 있는 데는 여대였어서",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028295.1.8",
          "form": "저도 그런데를 먼저했었더라면...",
          "original_form": "저도 그런데를 먼저했었더라면...",
          "speaker_id": "1",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028295.1.9",
          "form": "완전 화목했어요 ㅋㅋ",
          "original_form": "완전 화목했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028295.1.10",
          "form": "넘부럽네여",
          "original_form": "넘부럽네여",
          "speaker_id": "1",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028295.1.11",
          "form": "겜친사귀기가 은근히 어렵지 않나요...",
          "original_form": "겜친사귀기가 은근히 어렵지 않나요...",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028295.1.12",
          "form": "맞아요 그래서 팀플레이로하는게임 저 못해요ㅜㅜ",
          "original_form": "맞아요 그래서 팀플레이로하는게임 저 못해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028295.1.13",
          "form": "제친구들 게임하지도않고ㅜ온라인에 모르는사람구하기도 좀그렇고",
          "original_form": "제친구들 게임하지도않고ㅜ온라인에 모르는사람구하기도 좀그렇고",
          "speaker_id": "1",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028295.1.14",
          "form": "아~~ 맞아요 팀으로 하는 건 혼자하면 화나는데",
          "original_form": "아~~ 맞아요 팀으로 하는 건 혼자하면 화나는데",
          "speaker_id": "2",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100028295.1.15",
          "form": "같이 할 친구 없이 버티기 힘들죠",
          "original_form": "같이 할 친구 없이 버티기 힘들죠",
          "speaker_id": "2",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100028295.1.16",
          "form": "네 그래서 오버워치도 빨리접었어요",
          "original_form": "네 그래서 오버워치도 빨리접었어요",
          "speaker_id": "1",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100028295.1.17",
          "form": "저 사실 오버워치에 거의 중독되었던 수준이라",
          "original_form": "저 사실 오버워치에 거의 중독되었던 수준이라",
          "speaker_id": "2",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100028295.1.18",
          "form": "1학년때 학점 말도 못해요",
          "original_form": "1학년때 학점 말도 못해요",
          "speaker_id": "2",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100028295.1.19",
          "form": "학교생활..피폐 그 자체",
          "original_form": "학교생활..피폐 그 자체",
          "speaker_id": "2",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100028295.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ원래 1학년은 점수생각안하는학년아닌가여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ원래 1학년은 점수생각안하는학년아닌가여",
          "speaker_id": "1",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100028295.1.21",
          "form": "전 그이후로도 쭉...",
          "original_form": "전 그이후로도 쭉...",
