      "utterance": [
        {
          "id": "MDRW2100026797.1.1",
          "form": "해야하는데 쉽지가 않아요",
          "original_form": "해야하는데 쉽지가 않아요",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026797.1.2",
          "form": "다이어트 해보셨나요?",
          "original_form": "다이어트 해보셨나요?",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026797.1.3",
          "form": "어우마니해봣죠. .",
          "original_form": "어우마니해봣죠. .",
          "speaker_id": "1",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026797.1.4",
          "form": "거의성공적이지 않구요 ㅋㅋㅋ",
          "original_form": "거의성공적이지 않구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026797.1.5",
          "form": "좋은거 추천 부탁드려요",
          "original_form": "좋은거 추천 부탁드려요",
          "speaker_id": "2",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100026797.1.6",
          "form": "ㅋㅋㅋ 다이어트 음식이영 ?",
          "original_form": "ㅋㅋㅋ 다이어트 음식이영 ?",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026797.1.7",
          "form": "그냥 ... 뭘먹든 적게먹는게 답인듯",
          "original_form": "그냥 ... 뭘먹든 적게먹는게 답인듯",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026797.1.8",
          "form": "진리..",
          "original_form": "진리..",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026797.1.9",
          "form": "아 우문현답이네요",
          "original_form": "아 우문현답이네요",
          "speaker_id": "2",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026797.1.10",
          "form": "그게 너무어려워요",
          "original_form": "그게 너무어려워요",
          "speaker_id": "2",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026797.1.11",
          "form": "그져 ㅋㅋㅋㅋ 그래서 저는 ...진짜 스트레스 받고 우울해서 입맛없었을때...",
          "original_form": "그져 ㅋㅋㅋㅋ 그래서 저는 ...진짜 스트레스 받고 우울해서 입맛없었을때...",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100026797.1.12",
          "form": "케익을 먹든 뭘먹든 조금씩만 먹으니",
          "original_form": "케익을 먹든 뭘먹든 조금씩만 먹으니",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100026797.1.13",
          "form": "닭가슴살 먹으며 칼로리 맞추는거보다 훨 잘빠짐..",
          "original_form": "닭가슴살 먹으며 칼로리 맞추는거보다 훨 잘빠짐..",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100026797.1.14",
          "form": "케익이 닭가슴살보다 더 효과적이네요ㅎ",
          "original_form": "케익이 닭가슴살보다 더 효과적이네요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100026797.1.15",
          "form": "저는 요즘 에그타르트가 너무 맛나여",
          "original_form": "저는 요즘 에그타르트가 너무 맛나여",
          "speaker_id": "2",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026797.1.16",
          "form": "에타 ㅜㅜ근데 에타 맛집 찾기 어려워용",
          "original_form": "에타 ㅜㅜ근데 에타 맛집 찾기 어려워용",
          "speaker_id": "1",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100026797.1.17",
          "form": "사실 웬만하면 맛잇지만",
          "original_form": "사실 웬만하면 맛잇지만",
          "speaker_id": "1",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100026797.1.18",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100026797.1.19",
          "form": "에타먹구 운동해야죠 ㅎㅎㅎㅎ",
          "original_form": "에타먹구 운동해야죠 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026797.1.20",
          "form": "무슨운동 하세요?",
          "original_form": "무슨운동 하세요?",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026797.1.21",
          "form": "ㅋㅋㅋ 검도해요 요즘",
          "original_form": "ㅋㅋㅋ 검도해요 요즘",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026797.1.22",
          "form": "대박 멋져요",
          "original_form": "대박 멋져요",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026797.1.23",
          "form": "ㅋㅋㅋㅋ 2주됏어요",
          "original_form": "ㅋㅋㅋㅋ 2주됏어요",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026797.1.24",
          "form": "왕초보",
          "original_form": "왕초보",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026797.1.25",
          "form": "냄새는 안나요?",
          "original_form": "냄새는 안나요?",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026797.1.26",
          "form": "보호장비",
          "original_form": "보호장비",
          "speaker_id": "2",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026797.1.27",
          "form": "아아! ㅋㅋ 아직 안써요",
          "original_form": "아아! ㅋㅋ 아직 안써요",
          "speaker_id": "1",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026797.1.28",
          "form": "초보라..",
          "original_form": "초보라..",
          "speaker_id": "1",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026797.1.29",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "2",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026797.1.30",
          "form": "추천할만 한가요?",
          "original_form": "추천할만 한가요?",
          "speaker_id": "2",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026797.1.31",
          "form": "전 완전 ㅋㅋㅋ",
          "original_form": "전 완전 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026797.1.32",
          "form": "잼써요!",
          "original_form": "잼써요!",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026797.1.33",
          "form": "운동하시나요",
          "original_form": "운동하시나요",
          "speaker_id": "1",
          "time": "20210902 00:35"
        }
      ]
    }
