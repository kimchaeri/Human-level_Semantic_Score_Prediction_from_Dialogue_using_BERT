      "utterance": [
        {
          "id": "MDRW2100019713.1.1",
          "form": "디앤서 주식책 읽엇어요 ㅋㅋ",
          "original_form": "디앤서 주식책 읽엇어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019713.1.2",
          "form": "우와 주식허시나요",
          "original_form": "우와 주식허시나요",
          "speaker_id": "1",
          "time": "20210827 21:35"
        },
        {
          "id": "MDRW2100019713.1.3",
          "form": "저는 한강 채식주의자",
          "original_form": "저는 한강 채식주의자",
          "speaker_id": "1",
          "time": "20210827 21:35"
        },
        {
          "id": "MDRW2100019713.1.4",
          "form": "주식 재밋어요 ㅋㅋ",
          "original_form": "주식 재밋어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019713.1.5",
          "form": "채식에 관한 내용인가바여",
          "original_form": "채식에 관한 내용인가바여",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019713.1.6",
          "form": "좀 폭력적이고 기괴한 내용이에요",
          "original_form": "좀 폭력적이고 기괴한 내용이에요",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019713.1.7",
          "form": "ㅡ저도 주식 책 봐야하는데",
          "original_form": "ㅡ저도 주식 책 봐야하는데",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019713.1.8",
          "form": "오 제취향이에요",
          "original_form": "오 제취향이에요",
          "speaker_id": "2",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019713.1.9",
          "form": "어두운 내용 좋아해요",
          "original_form": "어두운 내용 좋아해요",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019713.1.10",
          "form": "맨부커상 받았대요",
          "original_form": "맨부커상 받았대요",
          "speaker_id": "1",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019713.1.11",
          "form": "잔인한것두잘봄",
          "original_form": "잔인한것두잘봄",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019713.1.12",
          "form": "오 진짜요 그옴 럼 추천",
          "original_form": "오 진짜요 그옴 럼 추천",
          "speaker_id": "1",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019713.1.13",
          "form": "안그래두 네이버에 찜해놧어요",
          "original_form": "안그래두 네이버에 찜해놧어요",
          "speaker_id": "2",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019713.1.14",
          "form": "오오 전 좀 기괴했지만 문학상 받았대서 한번 봤어요",
          "original_form": "오오 전 좀 기괴했지만 문학상 받았대서 한번 봤어요",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019713.1.15",
          "form": "기괴한거 완전 잘봐요ㅋ",
          "original_form": "기괴한거 완전 잘봐요ㅋ",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019713.1.16",
          "form": "디앤서도 완전 강추 에요",
          "original_form": "디앤서도 완전 강추 에요",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019713.1.17",
          "form": "오 디앤서 메모",
          "original_form": "오 디앤서 메모",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019713.1.18",
          "form": "저는 에세이 좋아하는데 우리나라 여성 헤지펀드 입성 내용 이에요",
          "original_form": "저는 에세이 좋아하는데 우리나라 여성 헤지펀드 입성 내용 이에요",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019713.1.19",
          "form": "우와 헤지펀드!!",
          "original_form": "우와 헤지펀드!!",
