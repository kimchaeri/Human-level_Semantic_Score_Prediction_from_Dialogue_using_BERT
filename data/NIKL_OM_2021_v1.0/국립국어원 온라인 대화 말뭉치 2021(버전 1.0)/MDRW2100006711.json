      "utterance": [
        {
          "id": "MDRW2100006711.1.1",
          "form": "오늘 날씨 어때요?",
          "original_form": "오늘 날씨 어때요?",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100006711.1.2",
          "form": "비는 그쳤어요",
          "original_form": "비는 그쳤어요",
          "speaker_id": "2",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100006711.1.3",
          "form": "여기도요 근데 엄청 춥네요 바람이",
          "original_form": "여기도요 근데 엄청 춥네요 바람이",
          "speaker_id": "1",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100006711.1.4",
          "form": "바람불고 추워졌어요",
          "original_form": "바람불고 추워졌어요",
          "speaker_id": "2",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100006711.1.5",
          "form": "금방 겨울이 올것 같은 날씨네요",
          "original_form": "금방 겨울이 올것 같은 날씨네요",
          "speaker_id": "2",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100006711.1.6",
          "form": "집도 추워서 난방을 해야 할것 같아요",
          "original_form": "집도 추워서 난방을 해야 할것 같아요",
          "speaker_id": "2",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100006711.1.7",
          "form": "그렇죠 에어컨 커버 이제 씌웠는데...ㅎㅎ",
          "original_form": "그렇죠 에어컨 커버 이제 씌웠는데...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100006711.1.8",
          "form": "전 선풍기도 다 들여놓았어요",
          "original_form": "전 선풍기도 다 들여놓았어요",
          "speaker_id": "2",
          "time": "20210908 07:31"
        },
        {
          "id": "MDRW2100006711.1.9",
          "form": "아 부지런하세요",
          "original_form": "아 부지런하세요",
          "speaker_id": "1",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100006711.1.10",
          "form": "이젠 따뜻한 난방류를 꺼내놔야 겠어요",
          "original_form": "이젠 따뜻한 난방류를 꺼내놔야 겠어요",
          "speaker_id": "2",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100006711.1.11",
          "form": "어떤거 사용하세요?",
          "original_form": "어떤거 사용하세요?",
          "speaker_id": "1",
          "time": "20210908 08:08"
        },
        {
          "id": "MDRW2100006711.1.12",
          "form": "옥매트 ,옥돌마루,전기히터 를 사용해요",
          "original_form": "옥매트 ,옥돌마루,전기히터 를 사용해요",
          "speaker_id": "2",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100006711.1.13",
          "form": "많네요 온열기구..저희는 아이들때문도 그렇고 없어요 ㅎ",
          "original_form": "많네요 온열기구..저희는 아이들때문도 그렇고 없어요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100006711.1.14",
          "form": "아 ! 옥매트는 아이들도 괜찮지 않나요",
          "original_form": "아 ! 옥매트는 아이들도 괜찮지 않나요",
          "speaker_id": "2",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100006711.1.15",
          "form": "네 근데 관리도 힘들도 부피도 크구요.ㅡㅠ",
