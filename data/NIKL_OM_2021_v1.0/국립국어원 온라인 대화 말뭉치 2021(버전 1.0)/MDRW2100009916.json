      "utterance": [
        {
          "id": "MDRW2100009916.1.1",
          "form": "하는 커뮤니티 있어?",
          "original_form": "하는 커뮤니티 있어?",
          "speaker_id": "1",
          "time": "20210823 11:40"
        },
        {
          "id": "MDRW2100009916.1.2",
          "form": "인스타그램",
          "original_form": "인스타그램",
          "speaker_id": "2",
          "time": "20210823 11:40"
        },
        {
          "id": "MDRW2100009916.1.3",
          "form": "name1이는?",
          "original_form": "&name1&이는?",
          "speaker_id": "2",
          "time": "20210823 11:40"
        },
        {
          "id": "MDRW2100009916.1.4",
          "form": "나는 다음카페!",
          "original_form": "나는 다음카페!",
          "speaker_id": "1",
          "time": "20210823 14:38"
        },
        {
          "id": "MDRW2100009916.1.5",
          "form": "다음 카페는 어때 재밌어 ?",
          "original_form": "다음 카페는 어때 재밌어 ?",
          "speaker_id": "2",
          "time": "20210823 14:38"
        },
        {
          "id": "MDRW2100009916.1.6",
          "form": "카카오톡 쪽 보면 인기 글 같은 거 보이는 거 같던데",
          "original_form": "카카오톡 쪽 보면 인기 글 같은 거 보이는 거 같던데",
          "speaker_id": "2",
          "time": "20210823 14:38"
        },
        {
          "id": "MDRW2100009916.1.7",
          "form": "응응 재밌어",
          "original_form": "응응 재밌어",
          "speaker_id": "1",
          "time": "20210823 14:40"
        },
        {
          "id": "MDRW2100009916.1.8",
          "form": "어떤 게 가장 재밌니",
          "original_form": "어떤 게 가장 재밌니",
          "speaker_id": "2",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100009916.1.9",
          "form": "인스타그램은 어떤 걸 보는 거야?",
          "original_form": "인스타그램은 어떤 걸 보는 거야?",
          "speaker_id": "1",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100009916.1.10",
          "form": "페이스북 해봤어 ?",
          "original_form": "페이스북 해봤어 ?",
          "speaker_id": "2",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100009916.1.11",
          "form": "페이스북 하고 비슷한데 좀 더 깔끔한 느낌이라고 생각하면 돼",
          "original_form": "페이스북 하고 비슷한데 좀 더 깔끔한 느낌이라고 생각하면 돼",
          "speaker_id": "2",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100009916.1.12",
          "form": "다음카페 좋은 점은",
          "original_form": "다음카페 좋은 점은",
          "speaker_id": "1",
          "time": "20210823 14:42"
        },
        {
          "id": "MDRW2100009916.1.13",
          "form": "비슷한 생각을 가진 사람끼리 모여서 댓글로 대화를 나눌 수 있다는 거야",
          "original_form": "비슷한 생각을 가진 사람끼리 모여서 댓글로 대화를 나눌 수 있다는 거야",
          "speaker_id": "1",
          "time": "20210823 14:42"
        },
        {
          "id": "MDRW2100009916.1.14",
          "form": "익명이 보장 되니까 재밌어",
          "original_form": "익명이 보장 되니까 재밌어",
          "speaker_id": "1",
          "time": "20210823 14:42"
        },
        {
          "id": "MDRW2100009916.1.15",
          "form": "너는 어떤 주제로 대화를 나눠 ?",
          "original_form": "너는 어떤 주제로 대화를 나눠 ?",
          "speaker_id": "2",
          "time": "20210823 14:42"
        },
        {
          "id": "MDRW2100009916.1.16",
          "form": "음 내가 관심 가지는 모든 것들",
          "original_form": "음 내가 관심 가지는 모든 것들",
          "speaker_id": "1",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100009916.1.17",
          "form": "독서라든지",
          "original_form": "독서라든지",
          "speaker_id": "1",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100009916.1.18",
          "form": "식물키우기라든지",
          "original_form": "식물키우기라든지",
          "speaker_id": "1",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100009916.1.19",
          "form": "식물 키우기 식물 어떤 거 키워",
          "original_form": "식물 키우기 식물 어떤 거 키워",
          "speaker_id": "2",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100009916.1.20",
          "form": "다육이 키웠었어ㅋㅋㅋㅋ",
          "original_form": "다육이 키웠었어ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100009916.1.21",
          "form": "오.. 어땠어",
          "original_form": "오.. 어땠어",
          "speaker_id": "2",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100009916.1.22",
          "form": "그냥 잘 자랐어 글고 내가 해준 행동들이 식물한테 좋은 거였다는 것도 깨달았었고",
          "original_form": "그냥 잘 자랐어 글고 내가 해준 행동들이 식물한테 좋은 거였다는 것도 깨달았었고",
          "speaker_id": "1",
          "time": "20210823 14:44"
        },
        {
          "id": "MDRW2100009916.1.23",
          "form": "마음에 안정이 온다던지 그런걸 공유하는건가",
          "original_form": "마음에 안정이 온다던지 그런걸 공유하는건가",
          "speaker_id": "2",
          "time": "20210823 14:44"
        },
        {
          "id": "MDRW2100009916.1.24",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210823 14:44"
        },
        {
          "id": "MDRW2100009916.1.25",
          "form": "죽은 잎 떼어주고 그랬거든",
          "original_form": "죽은 잎 떼어주고 그랬거든",
          "speaker_id": "1",
          "time": "20210823 14:44"
        },
        {
          "id": "MDRW2100009916.1.26",
          "form": "그치 좋은 점 얘기하거나",
          "original_form": "그치 좋은 점 얘기하거나",
          "speaker_id": "1",
          "time": "20210823 14:44"
        }
      ]
    }
