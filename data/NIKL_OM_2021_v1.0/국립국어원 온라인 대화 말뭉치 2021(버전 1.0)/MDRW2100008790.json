        },
        {
          "id": "MDRW2100008790.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210822 18:41"
        },
        {
          "id": "MDRW2100008790.1.3",
          "form": "그쪽 날씨 어때요?",
          "original_form": "그쪽 날씨 어때요?",
          "speaker_id": "2",
          "time": "20210822 18:41"
        },
        {
          "id": "MDRW2100008790.1.4",
          "form": "장마라서 비가 엄청 쏟아져요ㅜㅜ",
          "original_form": "장마라서 비가 엄청 쏟아져요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100008790.1.5",
          "form": "헉 고생이 많으시겠어요 ㅠㅠ",
          "original_form": "헉 고생이 많으시겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 19:43"
        },
        {
          "id": "MDRW2100008790.1.6",
          "form": "지금도 심해요?",
          "original_form": "지금도 심해요?",
          "speaker_id": "2",
          "time": "20210824 19:43"
        },
        {
          "id": "MDRW2100008790.1.7",
          "form": "지금은 좀 잠잠해졌어요!!",
          "original_form": "지금은 좀 잠잠해졌어요!!",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100008790.1.8",
          "form": "거기는 어때요?",
          "original_form": "거기는 어때요?",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100008790.1.9",
          "form": "잠깐잠깐 내렸다 마네요!!",
          "original_form": "잠깐잠깐 내렸다 마네요!!",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100008790.1.10",
          "form": "우산이 무거운..ㅎㅎ",
          "original_form": "우산이 무거운..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100008790.1.11",
          "form": "빨리 여름이 끝났으면 좋겠어요ㅜ",
          "original_form": "빨리 여름이 끝났으면 좋겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100008790.1.12",
          "form": "장마랑 더위 너무 싫어요ㅋㅋ",
          "original_form": "장마랑 더위 너무 싫어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100008790.1.13",
          "form": "아아 저도 너무 싫어해요 ㅠㅠ",
          "original_form": "아아 저도 너무 싫어해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100008790.1.14",
          "form": "가을이 너무 반가운거있죠",
          "original_form": "가을이 너무 반가운거있죠",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100008790.1.15",
          "form": "가을 너무좋아요ㅎㅎ",
          "original_form": "가을 너무좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100008790.1.16",
          "form": "옷도 예쁜게 많고ㅎㅎ",
          "original_form": "옷도 예쁜게 많고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100008790.1.17",
          "form": "맞아요 !",
          "original_form": "맞아요 !",
          "speaker_id": "2",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100008790.1.18",
          "form": "가을에는 하늘하늘~",
          "original_form": "가을에는 하늘하늘~",
          "speaker_id": "2",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100008790.1.19",
          "form": "그리고 코트의 계절이죠ㅎㅎ",
          "original_form": "그리고 코트의 계절이죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100008790.1.20",
          "form": "코트 미리 사놔서 빨리 입고 나가고 싶어용",
          "original_form": "코트 미리 사놔서 빨리 입고 나가고 싶어용",
          "speaker_id": "1",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100008790.1.21",
          "form": "이번 장마 지나면 딱이겠네요!",
          "original_form": "이번 장마 지나면 딱이겠네요!",
          "speaker_id": "2",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100008790.1.22",
          "form": "언제 겨울올지 몰라서 후딱 꺼내입어야해요 ㅎㅎ",
          "original_form": "언제 겨울올지 몰라서 후딱 꺼내입어야해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100008790.1.23",
          "form": "비가 쫌 그치나싶었는데",
          "original_form": "비가 쫌 그치나싶었는데",
          "speaker_id": "1",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100008790.1.24",
          "form": "또 장마라네요ㅜㅜ",
          "original_form": "또 장마라네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100008790.1.25",
