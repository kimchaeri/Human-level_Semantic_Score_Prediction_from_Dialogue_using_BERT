          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026694.1.12",
          "form": "가격이...",
          "original_form": "가격이...",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026694.1.13",
