      "utterance": [
        {
          "id": "MDRW2100026062.1.1",
          "form": "너 있는 곳도 비가 오니?",
          "original_form": "너 있는 곳도 비가 오니?",
          "speaker_id": "1",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100026062.1.2",
          "form": "여기는 비 너무  많이 와ㅜㅜ",
          "original_form": "여기는 비 너무  많이 와ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100026062.1.3",
          "form": "방금 전까지만 해도 엄청 내렸는데",
          "original_form": "방금 전까지만 해도 엄청 내렸는데",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026062.1.4",
          "form": "다시 멈췄어..!",
          "original_form": "다시 멈췄어..!",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026062.1.5",
          "form": "뭐 이런 날씨가 다 있을까",
          "original_form": "뭐 이런 날씨가 다 있을까",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026062.1.6",
          "form": "그러니까ㅠㅠㅠ",
          "original_form": "그러니까ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026062.1.7",
          "form": "요즘 날씨 좀 이상한 거 같아...",
          "original_form": "요즘 날씨 좀 이상한 거 같아...",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026062.1.8",
          "form": "오늘은 좀 덥던데",
          "original_form": "오늘은 좀 덥던데",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026062.1.9",
          "form": "우린 멈췄다가 다시 막 쏟아져",
          "original_form": "우린 멈췄다가 다시 막 쏟아져",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026062.1.10",
          "form": "갑자기 비 왕창 내리더니",
          "original_form": "갑자기 비 왕창 내리더니",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026062.1.11",
          "form": "아직 장마가 안 끝났나봐",
          "original_form": "아직 장마가 안 끝났나봐",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026062.1.12",
          "form": "맞아 맞아",
          "original_form": "맞아 맞아",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026062.1.13",
          "form": "목요일까지 계속 비온다고 하던데",
          "original_form": "목요일까지 계속 비온다고 하던데",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026062.1.14",
          "form": "아 정말?",
          "original_form": "아 정말?",
          "speaker_id": "2",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100026062.1.15",
          "form": "내일도 추적추적하게 보내겠다...",
          "original_form": "내일도 추적추적하게 보내겠다...",
          "speaker_id": "2",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100026062.1.16",
          "form": "빨래해야 하는데 비가와서 계속 미루는 중 ㅠㅠ",
          "original_form": "빨래해야 하는데 비가와서 계속 미루는 중 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100026062.1.17",
          "form": "저번주에도 장마라서 겨우 빨래했는데ㅠㅠ",
          "original_form": "저번주에도 장마라서 겨우 빨래했는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 19:31"
        },
        {
          "id": "MDRW2100026062.1.18",
          "form": "헐~ 귀찮겠다",
          "original_form": "헐~ 귀찮겠다",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026062.1.19",
          "form": "나 저번에 비 때문에 빨래 거뒀다가... 제대로 안피고 아무대나 둬서 쭈굴해져서 다림질한다고 엄청 고생했어",
          "original_form": "나 저번에 비 때문에 빨래 거뒀다가... 제대로 안피고 아무대나 둬서 쭈굴해져서 다림질한다고 엄청 고생했어",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026062.1.20",
          "form": "아아 뭔지 알아 그거",
          "original_form": "아아 뭔지 알아 그거",
          "speaker_id": "1",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100026062.1.21",
          "form": "특히 면종류 완전 쭈굴쭈굴해지잖아",
          "original_form": "특히 면종류 완전 쭈굴쭈굴해지잖아",
          "speaker_id": "1",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100026062.1.22",
          "form": "맞아... 그래서 엄마한테 엄청 혼났어 ㅋㅋ",
          "original_form": "맞아... 그래서 엄마한테 엄청 혼났어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:34"
        }
      ]
    }
