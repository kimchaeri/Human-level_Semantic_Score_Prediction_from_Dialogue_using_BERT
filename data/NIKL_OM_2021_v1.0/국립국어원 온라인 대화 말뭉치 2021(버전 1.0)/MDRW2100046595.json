      "utterance": [
        {
          "id": "MDRW2100046595.1.1",
          "form": "헬로",
          "original_form": "헬로",
          "speaker_id": "1",
          "time": "20211029 23:48"
        },
        {
          "id": "MDRW2100046595.1.2",
          "form": "대답좀해줄래?",
          "original_form": "대답좀해줄래?",
          "speaker_id": "1",
          "time": "20211029 23:49"
        },
        {
          "id": "MDRW2100046595.1.3",
          "form": "어 안녕~~",
          "original_form": "어 안녕~~",
          "speaker_id": "2",
          "time": "20211029 23:54"
        },
        {
          "id": "MDRW2100046595.1.4",
          "form": "모해~~?",
          "original_form": "모해~~?",
          "speaker_id": "2",
          "time": "20211029 23:54"
        },
        {
          "id": "MDRW2100046595.1.5",
          "form": "오 ㅁ ㅜ냐",
          "original_form": "오 ㅁ ㅜ냐",
          "speaker_id": "1",
          "time": "20211029 23:54"
        },
        {
          "id": "MDRW2100046595.1.6",
          "form": "안자고 뭐함?",
          "original_form": "안자고 뭐함?",
          "speaker_id": "1",
          "time": "20211029 23:54"
        },
        {
          "id": "MDRW2100046595.1.7",
          "form": "너가 대답하라며 ^^",
          "original_form": "너가 대답하라며 ^^",
          "speaker_id": "2",
          "time": "20211029 23:54"
        },
        {
          "id": "MDRW2100046595.1.8",
          "form": "난 티비봐",
          "original_form": "난 티비봐",
          "speaker_id": "2",
          "time": "20211029 23:54"
        },
        {
          "id": "MDRW2100046595.1.9",
          "form": "미안미은",
          "original_form": "미안미은",
          "speaker_id": "1",
          "time": "20211029 23:54"
        },
        {
          "id": "MDRW2100046595.1.10",
          "form": "비밀ㅋㅋㅋ 넌 이시간에 뭐해?",
          "original_form": "비밀ㅋㅋㅋ 넌 이시간에 뭐해?",
          "speaker_id": "2",
          "time": "20211029 23:54"
        },
        {
          "id": "MDRW2100046595.1.11",
          "form": "너 근데 왜 말놔",
          "original_form": "너 근데 왜 말놔",
          "speaker_id": "1",
          "time": "20211029 23:55"
        },
        {
          "id": "MDRW2100046595.1.12",
          "form": "뭐하냐구!!!",
          "original_form": "뭐하냐구!!!",
          "speaker_id": "2",
          "time": "20211029 23:55"
        },
        {
          "id": "MDRW2100046595.1.13",
          "form": "아 나 여자친구 씻으러가서",
          "original_form": "아 나 여자친구 씻으러가서",
          "speaker_id": "1",
          "time": "20211029 23:55"
        },
        {
          "id": "MDRW2100046595.1.14",
          "form": "웹서핑 하다가 이거 찾음",
          "original_form": "웹서핑 하다가 이거 찾음",
          "speaker_id": "1",
          "time": "20211029 23:55"
        },
        {
          "id": "MDRW2100046595.1.15",
          "form": "이야 근데 사람인거같은데",
          "original_form": "이야 근데 사람인거같은데",
          "speaker_id": "1",
          "time": "20211029 23:55"
        },
        {
          "id": "MDRW2100046595.1.16",
          "form": "아 오늘 여자친구랑 노는 날인가보네?",
          "original_form": "아 오늘 여자친구랑 노는 날인가보네?",
          "speaker_id": "2",
          "time": "20211029 23:55"
        },
        {
          "id": "MDRW2100046595.1.17",
          "form": "아니 여자친구는 여자친구 집이고",
          "original_form": "아니 여자친구는 여자친구 집이고",
          "speaker_id": "1",
          "time": "20211029 23:55"
        },
        {
          "id": "MDRW2100046595.1.18",
          "form": "나는 내 집이지",
          "original_form": "나는 내 집이지",
          "speaker_id": "1",
          "time": "20211029 23:55"
        },
        {
          "id": "MDRW2100046595.1.19",
          "form": "아..ㅋㅋㅋㅋㅋ",
          "original_form": "아..ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211029 23:56"
        },
        {
          "id": "MDRW2100046595.1.20",
          "form": "연애한지 얼마나 됐어?",
          "original_form": "연애한지 얼마나 됐어?",
          "speaker_id": "2",
          "time": "20211029 23:56"
        },
        {
          "id": "MDRW2100046595.1.21",
          "form": "74일",
          "original_form": "74일",
          "speaker_id": "1",
          "time": "20211029 23:56"
        },
        {
          "id": "MDRW2100046595.1.22",
          "form": "4분뒤 75일",
          "original_form": "4분뒤 75일",
          "speaker_id": "1",
          "time": "20211029 23:56"
        },
        {
          "id": "MDRW2100046595.1.23",
          "form": "우와 풋풋하다! 여자친구 어떤 면이 제일 좋아?",
          "original_form": "우와 풋풋하다! 여자친구 어떤 면이 제일 좋아?",
          "speaker_id": "2",
          "time": "20211029 23:56"
        },
        {
          "id": "MDRW2100046595.1.24",
          "form": "오우 쌔게들어오네",
          "original_form": "오우 쌔게들어오네",
          "speaker_id": "1",
          "time": "20211029 23:56"
        },
        {
          "id": "MDRW2100046595.1.25",
          "form": "일단 잘맞고",
          "original_form": "일단 잘맞고",
          "speaker_id": "1",
          "time": "20211029 23:57"
        },
        {
          "id": "MDRW2100046595.1.26",
          "form": "알고지낸건 십년됐고",
          "original_form": "알고지낸건 십년됐고",
          "speaker_id": "1",
          "time": "20211029 23:57"
        },
        {
          "id": "MDRW2100046595.1.27",
          "form": "너 근데 사람이야?",
          "original_form": "너 근데 사람이야?",
          "speaker_id": "1",
          "time": "20211029 23:57"
        },
        {
          "id": "MDRW2100046595.1.28",
          "form": "여자친구 좋은면 되게 많은데",
          "original_form": "여자친구 좋은면 되게 많은데",
          "speaker_id": "1",
          "time": "20211029 23:58"
        },
        {
          "id": "MDRW2100046595.1.29",
          "form": "너도 좋은 남자친구 같은데!",
          "original_form": "너도 좋은 남자친구 같은데!",
          "speaker_id": "2",
          "time": "20211029 23:58"
        },
        {
          "id": "MDRW2100046595.1.30",
          "form": "ㄴㄴ 아님",
          "original_form": "ㄴㄴ 아님",
          "speaker_id": "1",
          "time": "20211029 23:58"
        },
        {
          "id": "MDRW2100046595.1.31",
          "form": "내가 질투가 많고 속이 쫍아서",
          "original_form": "내가 질투가 많고 속이 쫍아서",
          "speaker_id": "1",
          "time": "20211029 23:59"
        },
        {
          "id": "MDRW2100046595.1.32",
          "form": "사람이 썩 나이스하지가 않아",
          "original_form": "사람이 썩 나이스하지가 않아",
          "speaker_id": "1",
          "time": "20211029 23:59"
        },
        {
          "id": "MDRW2100046595.1.33",
          "form": "낫 나이스.",
          "original_form": "낫 나이스.",
          "speaker_id": "1",
          "time": "20211029 23:59"
        },
        {
          "id": "MDRW2100046595.1.34",
          "form": "Not nice",
          "original_form": "Not nice",
          "speaker_id": "1",
          "time": "20211029 23:59"
        },
        {
          "id": "MDRW2100046595.1.35",
          "form": "그리고 공부한다고 연락도 잘 못하고",
          "original_form": "그리고 공부한다고 연락도 잘 못하고",
          "speaker_id": "1",
          "time": "20211029 23:59"
        },
        {
          "id": "MDRW2100046595.1.36",
          "form": "쏘 베드",
          "original_form": "쏘 베드",
          "speaker_id": "1",
          "time": "20211029 23:59"
        },
        {
          "id": "MDRW2100046595.1.37",
          "form": "그런 말 쉽게 할 수 없는데 진짜 좋은 사람같아! ",
          "original_form": "그런 말 쉽게 할 수 없는데 진짜 좋은 사람같아! ",
          "speaker_id": "2",
          "time": "20211030 12:00"
        },
        {
          "id": "MDRW2100046595.1.38",
          "form": "솔직히 얘기해줘서 고마워!",
          "original_form": "솔직히 얘기해줘서 고마워!",
          "speaker_id": "2",
          "time": "20211030 12:00"
        }
      ]
