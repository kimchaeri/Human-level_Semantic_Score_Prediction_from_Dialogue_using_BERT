      "utterance": [
        {
          "id": "MDRW2100011561.1.1",
          "form": "가끔해먹기도하고 인스턴트먹기도하고",
          "original_form": "가끔해먹기도하고 인스턴트먹기도하고",
          "speaker_id": "2",
          "time": "20210824 13:56"
        },
        {
          "id": "MDRW2100011561.1.2",
          "form": "고기자주구워먹어요ㅎ",
          "original_form": "고기자주구워먹어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 13:56"
        },
        {
          "id": "MDRW2100011561.1.3",
          "form": "요리좋아하세요?",
          "original_form": "요리좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 13:57"
        },
        {
          "id": "MDRW2100011561.1.4",
          "form": "요리 자주해요~",
          "original_form": "요리 자주해요~",
          "speaker_id": "1",
          "time": "20210824 13:57"
        },
        {
          "id": "MDRW2100011561.1.5",
          "form": "어떤고기 좋아하세요",
          "original_form": "어떤고기 좋아하세요",
          "speaker_id": "1",
          "time": "20210824 13:57"
        },
        {
          "id": "MDRW2100011561.1.6",
          "form": "어떤요리요?",
          "original_form": "어떤요리요?",
          "speaker_id": "2",
          "time": "20210824 13:57"
        },
        {
          "id": "MDRW2100011561.1.7",
          "form": "전 고기는 다ㅋ",
          "original_form": "전 고기는 다ㅋ",
          "speaker_id": "2",
          "time": "20210824 13:57"
        },
        {
          "id": "MDRW2100011561.1.8",
          "form": "돼지 닭 소 중에 어떤게 좋으세요?",
          "original_form": "돼지 닭 소 중에 어떤게 좋으세요?",
          "speaker_id": "1",
          "time": "20210824 13:59"
        },
        {
          "id": "MDRW2100011561.1.9",
          "form": "전 소 돼지 닭 순이요ㅎ",
          "original_form": "전 소 돼지 닭 순이요ㅎ",
          "speaker_id": "2",
          "time": "20210824 13:59"
        },
        {
          "id": "MDRW2100011561.1.10",
          "form": "저는 소고기 맛있는지 모르겠더라고요ㅎㅎ",
          "original_form": "저는 소고기 맛있는지 모르겠더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:59"
        },
        {
          "id": "MDRW2100011561.1.11",
          "form": "돼지고기가 제일 맛있어요",
          "original_form": "돼지고기가 제일 맛있어요",
          "speaker_id": "1",
          "time": "20210824 13:59"
        },
        {
          "id": "MDRW2100011561.1.12",
          "form": "아 마블링있는건 살살녹는데ㅎㅎ",
          "original_form": "아 마블링있는건 살살녹는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 14:00"
        },
        {
          "id": "MDRW2100011561.1.13",
          "form": "삼겹살 맛있죠",
          "original_form": "삼겹살 맛있죠",
          "speaker_id": "2",
          "time": "20210824 14:00"
        },
        {
          "id": "MDRW2100011561.1.14",
          "form": "김치구워서 냠냠",
          "original_form": "김치구워서 냠냠",
          "speaker_id": "2",
          "time": "20210824 14:00"
        },
        {
          "id": "MDRW2100011561.1.15",
          "form": "저는 목살이랑 사태 자주먹어요",
          "original_form": "저는 목살이랑 사태 자주먹어요",
          "speaker_id": "1",
          "time": "20210824 14:01"
        },
        {
          "id": "MDRW2100011561.1.16",
          "form": "아 기름진거 안좋아하시는군요",
          "original_form": "아 기름진거 안좋아하시는군요",
          "speaker_id": "2",
          "time": "20210824 14:02"
        },
        {
          "id": "MDRW2100011561.1.17",
          "form": "네 정답ㅎㅎㅎ",
          "original_form": "네 정답ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:02"
        },
        {
          "id": "MDRW2100011561.1.18",
          "form": "사태는 장조림할때먹나요?",
          "original_form": "사태는 장조림할때먹나요?",
          "speaker_id": "2",
          "time": "20210824 14:02"
        },
        {
          "id": "MDRW2100011561.1.19",
          "form": "사태는 삶아먹거나",
          "original_form": "사태는 삶아먹거나",
          "speaker_id": "1",
          "time": "20210824 14:03"
        },
        {
          "id": "MDRW2100011561.1.20",
          "form": "볶아먹어요",
          "original_form": "볶아먹어요",
          "speaker_id": "1",
          "time": "20210824 14:03"
        },
        {
          "id": "MDRW2100011561.1.21",
          "form": "식감이 쫄깃쫄깃 합니다",
          "original_form": "식감이 쫄깃쫄깃 합니다",
          "speaker_id": "1",
          "time": "20210824 14:03"
        },
        {
          "id": "MDRW2100011561.1.22",
          "form": "사태사본적이 없네요ㅎ 맨날 삼겹이만^^",
          "original_form": "사태사본적이 없네요ㅎ 맨날 삼겹이만^^",
          "speaker_id": "2",
          "time": "20210824 14:04"
        },
        {
