      "utterance": [
        {
          "id": "MDRW2100015337.1.1",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015337.1.2",
          "form": "간단하게 버터에 식빵 한장 구워먹었어요",
          "original_form": "간단하게 버터에 식빵 한장 구워먹었어요",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015337.1.3",
          "form": "간단하게 드셨네여~ 야식땡기셨다길래 거하게 드신줄 ㅋㅋㅋ",
          "original_form": "간단하게 드셨네여~ 야식땡기셨다길래 거하게 드신줄 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015337.1.4",
          "form": "넘 소박하자나요~ 간식으로 정정..ㅋㅋㅋ",
          "original_form": "넘 소박하자나요~ 간식으로 정정..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015337.1.5",
          "form": "자주 먹는건 아니다보니까요... ㅎㅎ",
          "original_form": "자주 먹는건 아니다보니까요... ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015337.1.6",
          "form": "어떤 날에는 식빵 한조각이 한끼가 되기도 하니 야식 아닐까요",
          "original_form": "어떤 날에는 식빵 한조각이 한끼가 되기도 하니 야식 아닐까요",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015337.1.7",
          "form": "그런가열 ㅎㅎ",
          "original_form": "그런가열 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015337.1.8",
          "form": "최근 어떤 과일드셨어요?",
          "original_form": "최근 어떤 과일드셨어요?",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015337.1.9",
          "form": "참외 먹었어요 엄청 달더라구요",
          "original_form": "참외 먹었어요 엄청 달더라구요",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015337.1.10",
          "form": "아 참외맛있겠다~~",
          "original_form": "아 참외맛있겠다~~",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015337.1.11",
          "form": "저는 이번여름에 복숭아를 원없이 먹는것 같아요 ㅎㅎ",
          "original_form": "저는 이번여름에 복숭아를 원없이 먹는것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015337.1.12",
          "form": "헐 저는 이번 여름에 복숭아 안먹었나봐요",
          "original_form": "헐 저는 이번 여름에 복숭아 안먹었나봐요",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015337.1.13",
          "form": "물복 딱복 중에 뭐 좋아하세요?",
          "original_form": "물복 딱복 중에 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015337.1.14",
          "form": "둘다 좋아하는데 요즘은 계속 물복먹고 있네여ㅋㅋ",
          "original_form": "둘다 좋아하는데 요즘은 계속 물복먹고 있네여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015337.1.15",
          "form": "복숭아가 좋은게 향부터 달아서ㅜㅜ",
          "original_form": "복숭아가 좋은게 향부터 달아서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015337.1.16",
          "form": "맞아요 껍질 깔때부터 향이 퍼지죠",
          "original_form": "맞아요 껍질 깔때부터 향이 퍼지죠",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015337.1.17",
          "form": "촉감이나 식감도 보들보들하구요 ㅎㅎ",
          "original_form": "촉감이나 식감도 보들보들하구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015337.1.18",
          "form": "복숭아가 참 좋죠 복숭아 닮았다고 하면 칭찬이기도 하잖아요",
          "original_form": "복숭아가 참 좋죠 복숭아 닮았다고 하면 칭찬이기도 하잖아요",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015337.1.19",
          "form": "ㅋㅋㅋㅋㅋ그런가요",
          "original_form": "ㅋㅋㅋㅋㅋ그런가요",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015337.1.20",
          "form": "하긴 좋은의미로 쓰이긴하쥬...",
          "original_form": "하긴 좋은의미로 쓰이긴하쥬...",
          "speaker_id": "2",
