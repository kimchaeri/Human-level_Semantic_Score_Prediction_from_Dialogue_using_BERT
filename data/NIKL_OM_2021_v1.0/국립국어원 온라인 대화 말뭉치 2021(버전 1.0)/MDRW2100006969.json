        },
        {
          "id": "MDRW2100006969.1.3",
          "form": "name1님은 학생이세요?",
          "original_form": "&name1&님은 학생이세요?",
          "speaker_id": "2",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006969.1.4",
          "form": "앗 아니요!",
          "original_form": "앗 아니요!",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006969.1.5",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006969.1.6",
          "form": "저도 학생은 아닙니다:)",
          "original_form": "저도 학생은 아닙니다:)",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006969.1.7",
          "form": "그럼 name1님은 학교다니실때",
          "original_form": "그럼 &name1&님은 학교다니실때",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006969.1.8",
          "form": "어떤 동아리 셨어요?",
          "original_form": "어떤 동아리 셨어요?",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006969.1.9",
          "form": "아 저는 과 안에 있는 연극부 했었어요 ㅎㅎ",
          "original_form": "아 저는 과 안에 있는 연극부 했었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006969.1.10",
          "form": "우와:) 연극부셨군용",
          "original_form": "우와:) 연극부셨군용",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006969.1.11",
          "form": "네 어떤거 하셨나요?",
          "original_form": "네 어떤거 하셨나요?",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006969.1.12",
          "form": "저는... ㅋㅋㅋ 기독교 동아리였어요",
          "original_form": "저는... ㅋㅋㅋ 기독교 동아리였어요",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006969.1.13",
          "form": "아 그러셨구나 ㅋㅋ 저희 학교에도 있었는데 그럼 CCM도 부르고 그러나용?",
          "original_form": "아 그러셨구나 ㅋㅋ 저희 학교에도 있었는데 그럼 CCM도 부르고 그러나용?",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006969.1.14",
          "form": "네:) 채플이 있어서 채플에서는\r\nccm도 부르고 했었어영",
          "original_form": "네:) 채플이 있어서 채플에서는\r\nccm도 부르고 했었어영",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006969.1.15",
          "form": "전 과 동아리였어갖고 인간관계가 넓어지진 않았거든요 전체동아리는 그래서 좀 부럽더라고요 ㅋㅋ",
          "original_form": "전 과 동아리였어갖고 인간관계가 넓어지진 않았거든요 전체동아리는 그래서 좀 부럽더라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006969.1.16",
          "form": "대신에 넓지는 않아도\r\n더 돈독한건 있겠네요:)",
          "original_form": "대신에 넓지는 않아도\r\n더 돈독한건 있겠네요:)",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006969.1.17",
          "form": "네 맞아요 굳이 자기소개 안해도 되고요 ㅋㅋㅋ",
          "original_form": "네 맞아요 굳이 자기소개 안해도 되고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006969.1.18",
          "form": "전공이 어떻게 되세요?",
          "original_form": "전공이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006969.1.19",
          "form": "ㅋㅋ저는 중국어요ㅋㅋ",
          "original_form": "ㅋㅋ저는 중국어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100006969.1.20",
          "form": "니취팔로마는 넣어두세요 ㅋㅋ",
          "original_form": "니취팔로마는 넣어두세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100006969.1.21",
          "form": "엄머머 저는 일본언데 ㅋㅋㅋㅋㅋ",
          "original_form": "엄머머 저는 일본언데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006969.1.22",
          "form": "너무 공감가요 하...",
          "original_form": "너무 공감가요 하...",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006969.1.23",
          "form": "ㅋㅋ뭔지 아시죠?ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ뭔지 아시죠?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006969.1.24",
          "form": "첨보는 사람인데 갑자기 말 해보라 그러고 ㅋㅋㅋ",
          "original_form": "첨보는 사람인데 갑자기 말 해보라 그러고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100006969.1.25",
          "form": "진짜 매번 왜 부끄러움은 제 몫인걸까요ㅋ",
          "original_form": "진짜 매번 왜 부끄러움은 제 몫인걸까요ㅋ",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100006969.1.26",
          "form": "싸우자는건가? 이렇게 ㅋㅋㅋㅋ",
          "original_form": "싸우자는건가? 이렇게 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100006969.1.27",
          "form": "ㅋㅋㅋ진짜 거르고 싶어여",
          "original_form": "ㅋㅋㅋ진짜 거르고 싶어여",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006969.1.28",
          "form": "초면에 그러는 사람들ㅋㅋㅈ",
          "original_form": "초면에 그러는 사람들ㅋㅋㅈ",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006969.1.29",
          "form": "진짜 다 그러나봐요 ㅋㅋㅋ 중국어 저도 배우고 싶은데 제 안에선 외국어가 일본어로 박혀서 새로운 외국어 배우기가 쉽지않네요",
