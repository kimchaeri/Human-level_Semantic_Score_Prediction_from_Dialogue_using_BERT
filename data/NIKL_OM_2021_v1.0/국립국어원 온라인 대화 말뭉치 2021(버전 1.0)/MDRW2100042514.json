      "utterance": [
        {
          "id": "MDRW2100042514.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042514.1.2",
          "form": "여행 좋아하죠",
          "original_form": "여행 좋아하죠",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042514.1.3",
          "form": "여름에 휴가 다녀오셨나요?",
          "original_form": "여름에 휴가 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042514.1.4",
          "form": "아 네 저는 6월에 사람없을때(?) 제주 다녀왔습니다 ㅎㅎ",
          "original_form": "아 네 저는 6월에 사람없을때(?) 제주 다녀왔습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042514.1.5",
          "form": "다녀오셨나요?",
          "original_form": "다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042514.1.6",
          "form": "어? 저도 7월에 제주도 갔어요",
          "original_form": "어? 저도 7월에 제주도 갔어요",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042514.1.7",
          "form": "어디쪽 가셨나요?",
          "original_form": "어디쪽 가셨나요?",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042514.1.8",
          "form": "아 정말요? ㅋㅋ저는 그때 겉으로 해서 반시계 방향으로 돌았어요! 사려니숲길갔다가 크게 성산 중문 애월 갔었네요",
          "original_form": "아 정말요? ㅋㅋ저는 그때 겉으로 해서 반시계 방향으로 돌았어요! 사려니숲길갔다가 크게 성산 중문 애월 갔었네요",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100042514.1.9",
          "form": "어느쪽으로 가셧어요?:)",
          "original_form": "어느쪽으로 가셧어요?:)",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100042514.1.10",
          "form": "전 김녕쪽에서만 있었어요",
          "original_form": "전 김녕쪽에서만 있었어요",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042514.1.11",
          "form": "가서 바다속에서만 놀았거든요ㅋㅋㅋㅋ",
          "original_form": "가서 바다속에서만 놀았거든요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042514.1.12",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042514.1.13",
          "form": "ㅎㅎ제주하면 바다죠",
          "original_form": "ㅎㅎ제주하면 바다죠",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042514.1.14",
          "form": "저는 같이 간 친구가 물을 별로 안 좋아하기도 하고해서 ㅎㅎ",
          "original_form": "저는 같이 간 친구가 물을 별로 안 좋아하기도 하고해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042514.1.15",
          "form": "뽀송뽀송하게 다녀왔습니다",
          "original_form": "뽀송뽀송하게 다녀왔습니다",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042514.1.16",
          "form": "저희는 애초에 바다에서 놀다오는걸로 계획했어요!!",
          "original_form": "저희는 애초에 바다에서 놀다오는걸로 계획했어요!!",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042514.1.17",
          "form": "그러면 눈으로 보는 관광 위주로 하셨겠네요?",
          "original_form": "그러면 눈으로 보는 관광 위주로 하셨겠네요?",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042514.1.18",
          "form": "ㅎㅎ네네 제일 많이 젖은게",
          "original_form": "ㅎㅎ네네 제일 많이 젖은게",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042514.1.19",
          "form": "돌고래보는 요트투어 하면서였던것같아요",
          "original_form": "돌고래보는 요트투어 하면서였던것같아요",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042514.1.20",
          "form": "ㅋㅋ성산일출봉..도 등산으로 쳐주신다면..등산도 한걸로...",
          "original_form": "ㅋㅋ성산일출봉..도 등산으로 쳐주신다면..등산도 한걸로...",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042514.1.21",
          "form": "우와 돌고래 가까이서 보셨나요??",
          "original_form": "우와 돌고래 가까이서 보셨나요??",
          "speaker_id": "2",
          "time": "20210906 23:09"
        },
        {
          "id": "MDRW2100042514.1.22",
          "form": "네! 그때 운이 좋아서 정말 가까이에서 볼 수 있었습니다",
          "original_form": "네! 그때 운이 좋아서 정말 가까이에서 볼 수 있었습니다",
          "speaker_id": "1",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042514.1.23",
          "form": "ㅎㅎ못해도 50-60 마리는 본 것 같아여",
          "original_form": "ㅎㅎ못해도 50-60 마리는 본 것 같아여",
          "speaker_id": "1",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042514.1.24",
          "form": "배 멀미 안하시면 추천드립니당!",
          "original_form": "배 멀미 안하시면 추천드립니당!",
          "speaker_id": "1",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100042514.1.25",
          "form": "헉 그렇게나 많이요???",
          "original_form": "헉 그렇게나 많이요???",
          "speaker_id": "2",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100042514.1.26",
          "form": "대박이네요!",
          "original_form": "대박이네요!",
          "speaker_id": "2",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100042514.1.27",
          "form": "한두마리 볼 수 있는줄 알았는데",
          "original_form": "한두마리 볼 수 있는줄 알았는데",
          "speaker_id": "2",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042514.1.28",
          "form": "네네 돌고개는 그룹으로 다니는데 개체가 많은가봐요 ㅎㅎ",
          "original_form": "네네 돌고개는 그룹으로 다니는데 개체가 많은가봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042514.1.29",
          "form": "배가 지나가면서 파도를 만드는데 돌고래가 그거 타고 놀려고 배 옆으로 가까이 오기때문에",
          "original_form": "배가 지나가면서 파도를 만드는데 돌고래가 그거 타고 놀려고 배 옆으로 가까이 오기때문에",
          "speaker_id": "1",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042514.1.30",
          "form": "상당히 가까이서 보실 수 있어요 :)",
          "original_form": "상당히 가까이서 보실 수 있어요 :)",
          "speaker_id": "1",
          "time": "20210906 23:13"
        }
      ]
