      "utterance": [
        {
          "id": "MDRW2100011040.1.1",
          "form": "너 메타버스에 대해서 들어봤어",
          "original_form": "너 메타버스에 대해서 들어봤어",
          "speaker_id": "1",
          "time": "20210823 23:59"
        },
        {
          "id": "MDRW2100011040.1.2",
          "form": "어 나 들어봤어",
          "original_form": "어 나 들어봤어",
          "speaker_id": "2",
          "time": "20210823 23:59"
        },
        {
          "id": "MDRW2100011040.1.3",
          "form": "요즘 뉴스에서 많이 떠들더라.",
          "original_form": "요즘 뉴스에서 많이 떠들더라.",
          "speaker_id": "2",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100011040.1.4",
          "form": "근데 나는 메타버스와 아직 많이 엮이지 않은 거 같아.",
          "original_form": "근데 나는 메타버스와 아직 많이 엮이지 않은 거 같아.",
          "speaker_id": "2",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100011040.1.5",
          "form": "나도 아직 직접해본적이없오",
          "original_form": "나도 아직 직접해본적이없오",
          "speaker_id": "1",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100011040.1.6",
          "form": "근데 엄청 유행이라고하더라",
          "original_form": "근데 엄청 유행이라고하더라",
          "speaker_id": "1",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100011040.1.7",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "2",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100011040.1.8",
          "form": "사실 한번 해보고싶긴해",
          "original_form": "사실 한번 해보고싶긴해",
          "speaker_id": "1",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100011040.1.9",
          "form": "메타버스 관련주들도 뜨더라",
          "original_form": "메타버스 관련주들도 뜨더라",
          "speaker_id": "2",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100011040.1.10",
          "form": "코인도 많이 뜨고.",
          "original_form": "코인도 많이 뜨고.",
          "speaker_id": "2",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100011040.1.11",
          "form": "나는 그런 데 정신을 쏟지 않기로 했어.",
          "original_form": "나는 그런 데 정신을 쏟지 않기로 했어.",
          "speaker_id": "2",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011040.1.12",
          "form": "맞아 나 조금 혹했잖아",
          "original_form": "맞아 나 조금 혹했잖아",
          "speaker_id": "1",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011040.1.13",
          "form": "내가 너무 깊이 빠질 거 같기도 해",
          "original_form": "내가 너무 깊이 빠질 거 같기도 해",
          "speaker_id": "2",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011040.1.14",
          "form": "조심해!",
          "original_form": "조심해!",
          "speaker_id": "2",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011040.1.15",
          "form": "근데 내가 찾아보고난 다음날 바로 떨어졌어",
          "original_form": "근데 내가 찾아보고난 다음날 바로 떨어졌어",
          "speaker_id": "1",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011040.1.16",
          "form": "안목이 없구나.",
          "original_form": "안목이 없구나.",
          "speaker_id": "2",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011040.1.17",
          "form": "안사길 너무 잘했다 생각했지",
          "original_form": "안사길 너무 잘했다 생각했지",
          "speaker_id": "1",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011040.1.18",
          "form": "아냐. 지금이 초창기니까. 가능성 있을 수뎌 있어.",
          "original_form": "아냐. 지금이 초창기니까. 가능성 있을 수뎌 있어.",
          "speaker_id": "2",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011040.1.19",
          "form": "근데 메타버스 관련된 코인도 있다는게",
          "original_form": "근데 메타버스 관련된 코인도 있다는게",
          "speaker_id": "1",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011040.1.20",
          "form": "너무 신기해",
          "original_form": "너무 신기해",
          "speaker_id": "1",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011040.1.21",
          "form": "맞아. 난 코인이 너무 신기해",
          "original_form": "맞아. 난 코인이 너무 신기해",
          "speaker_id": "2",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011040.1.22",
          "form": "알수없는 원천을 믿는구나 다들.",
          "original_form": "알수없는 원천을 믿는구나 다들.",
