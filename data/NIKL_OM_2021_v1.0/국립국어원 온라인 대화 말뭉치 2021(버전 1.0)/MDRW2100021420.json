      "utterance": [
        {
          "id": "MDRW2100021420.1.1",
          "form": "저는 못 먹어봤어요 아직",
          "original_form": "저는 못 먹어봤어요 아직",
          "speaker_id": "1",
          "time": "20210828 18:46"
        },
        {
          "id": "MDRW2100021420.1.2",
          "form": "헉 닭발은 호불호가 많이 갈리긴하죠ㅜ",
          "original_form": "헉 닭발은 호불호가 많이 갈리긴하죠ㅜ",
          "speaker_id": "2",
          "time": "20210828 18:50"
        },
        {
          "id": "MDRW2100021420.1.3",
          "form": "넵 전 닭발 홍어 이런 호불호갈리는건 대부분 불호더라구요ㅠㅠ",
          "original_form": "넵 전 닭발 홍어 이런 호불호갈리는건 대부분 불호더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 18:51"
        },
        {
          "id": "MDRW2100021420.1.4",
          "form": "저도 홍어는 못먹어요ㅜㅜㅋㅋ 곱창도...",
          "original_form": "저도 홍어는 못먹어요ㅜㅜㅋㅋ 곱창도...",
          "speaker_id": "2",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100021420.1.5",
          "form": "저랑 비슷하시네요 ㅋㅋㅋ",
          "original_form": "저랑 비슷하시네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100021420.1.6",
          "form": "홍어는 젊은사람들 거의 불호같아요",
          "original_form": "홍어는 젊은사람들 거의 불호같아요",
          "speaker_id": "1",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100021420.1.7",
          "form": "딱 한입 먹어봤는데 씹다가 뱉었어요 ......",
          "original_form": "딱 한입 먹어봤는데 씹다가 뱉었어요 ......",
          "speaker_id": "2",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100021420.1.8",
          "form": "충격적",
          "original_form": "충격적",
          "speaker_id": "2",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100021420.1.9",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021420.1.10",
          "form": "어떤가요??!!",
          "original_form": "어떤가요??!!",
          "speaker_id": "1",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021420.1.11",
          "form": "물컹!!!!하고 꼬리꼬리해요",
          "original_form": "물컹!!!!하고 꼬리꼬리해요",
          "speaker_id": "2",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021420.1.12",
          "form": "물컹한 식감을 좋아하는분들은 좋아할것같아요",
          "original_form": "물컹한 식감을 좋아하는분들은 좋아할것같아요",
          "speaker_id": "2",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021420.1.13",
          "form": "흠 회좋아하는사람도요?!",
          "original_form": "흠 회좋아하는사람도요?!",
          "speaker_id": "1",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021420.1.14",
          "form": "아 꼬린거 좋아하는분도요...",
          "original_form": "아 꼬린거 좋아하는분도요...",
          "speaker_id": "2",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100021420.1.15",
          "form": "두리안같은거..",
          "original_form": "두리안같은거..",
          "speaker_id": "2",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100021420.1.16",
          "form": "헉 두리안 냄새도 못맡아봤는데",
          "original_form": "헉 두리안 냄새도 못맡아봤는데",
          "speaker_id": "1",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100021420.1.17",
          "form": "갑자기 홍어가 더 싫어졌네요 ㅋㅋㅋㅋ",
          "original_form": "갑자기 홍어가 더 싫어졌네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100021420.1.18",
          "form": "ㅋㅋㅋ안드시는게 좋을것같네요",
          "original_form": "ㅋㅋㅋ안드시는게 좋을것같네요",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100021420.1.19",
          "form": "호불호 끝판왕인듯",
          "original_form": "호불호 끝판왕인듯",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021420.1.20",
          "form": "앜ㅋㅋㅋㅋ 그러게요",
          "original_form": "앜ㅋㅋㅋㅋ 그러게요",
