      "utterance": [
        {
          "id": "MDRW2100045190.1.1",
          "form": "나 이번에 한복 점프수트 샀는데 너무 만족스러워ㅠㅜ 근데 지금 입기엔 너무 추워서 여름에 입으려고 장롱 속에서 고이 모셔있엉",
          "original_form": "나 이번에 한복 점프수트 샀는데 너무 만족스러워ㅠㅜ 근데 지금 입기엔 너무 추워서 여름에 입으려고 장롱 속에서 고이 모셔있엉",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045190.1.2",
          "form": "허얼 이제 가을인데",
          "original_form": "허얼 이제 가을인데",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045190.1.3",
          "form": "이제막가을인데 추워?",
          "original_form": "이제막가을인데 추워?",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045190.1.4",
          "form": "엉ㅠㅜ 나는 더위보다 추위를 잘 타가지고 장판 틀어놓고 자",
          "original_form": "엉ㅠㅜ 나는 더위보다 추위를 잘 타가지고 장판 틀어놓고 자",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045190.1.5",
          "form": "나 5월까지도 장판 틀어놓고 잤었는데",
          "original_form": "나 5월까지도 장판 틀어놓고 잤었는데",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045190.1.6",
          "form": "9월달에 다시 키고 자넹..",
          "original_form": "9월달에 다시 키고 자넹..",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045190.1.7",
          "form": "헐 그땐 여름 아니니",
          "original_form": "헐 그땐 여름 아니니",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045190.1.8",
          "form": "그렇긴한데 5월달도 좀 춥더라고ㅠ",
          "original_form": "그렇긴한데 5월달도 좀 춥더라고ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045190.1.9",
          "form": "글구나 장판 근데 안망가졌어?",
          "original_form": "글구나 장판 근데 안망가졌어?",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045190.1.10",
          "form": "저번에 보니까 장판 싸게팔던데",
          "original_form": "저번에 보니까 장판 싸게팔던데",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045190.1.11",
          "form": "안망가졌징",
          "original_form": "안망가졌징",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045190.1.12",
          "form": "이제 한 3년 쓰고 있는걸?",
          "original_form": "이제 한 3년 쓰고 있는걸?",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045190.1.13",
          "form": "아아 나도슬슬 추워지니까 겨울옷 장만해야하나",
          "original_form": "아아 나도슬슬 추워지니까 겨울옷 장만해야하나",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045190.1.14",
          "form": "이번에 또 자라에서 세일하니까 자라 노려도 될 거 같아",
          "original_form": "이번에 또 자라에서 세일하니까 자라 노려도 될 거 같아",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045190.1.15",
          "form": "맞다 나 이번에 또 다이어리 살거야!",
          "original_form": "맞다 나 이번에 또 다이어리 살거야!",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045190.1.16",
          "form": "허럴",
          "original_form": "허럴",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045190.1.17",
          "form": "어디서?",
          "original_form": "어디서?",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045190.1.18",
          "form": "그 다이어리는 덕후 용도로 쓸거거든",
          "original_form": "그 다이어리는 덕후 용도로 쓸거거든",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045190.1.19",
          "form": "텀블벅이라는 후원하는곳이 있는데 거기서 사려공",
          "original_form": "텀블벅이라는 후원하는곳이 있는데 거기서 사려공",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045190.1.20",
          "form": "오 너 후원도 하는구나",
          "original_form": "오 너 후원도 하는구나",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045190.1.21",
          "form": "대단하구먼",
          "original_form": "대단하구먼",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045190.1.22",
          "form": "근데 후원이 뭔가 뭐랄까 그냥 이름만 후원이고 거의 구매와 같아",
          "original_form": "근데 후원이 뭔가 뭐랄까 그냥 이름만 후원이고 거의 구매와 같아",
          "speaker_id": "1",
          "time": "20210908 01:21"
        }
      ]
    }
