      "utterance": [
        {
          "id": "MDRW2100015682.1.1",
          "form": "여름의 마무리가 너무 축축하네요",
          "original_form": "여름의 마무리가 너무 축축하네요",
          "speaker_id": "1",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015682.1.2",
          "form": "그러게요 덥고 습하네요",
          "original_form": "그러게요 덥고 습하네요",
          "speaker_id": "2",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015682.1.3",
          "form": "비가 장마 처럼 오네요",
          "original_form": "비가 장마 처럼 오네요",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015682.1.4",
          "form": "비오나요?",
          "original_form": "비오나요?",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015682.1.5",
          "form": "여긴그치긴했는데 가을장마인가봐요",
          "original_form": "여긴그치긴했는데 가을장마인가봐요",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015682.1.6",
          "form": "긍까요.. 최근 가을장마 계속이네요",
          "original_form": "긍까요.. 최근 가을장마 계속이네요",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015682.1.7",
          "form": "기후가 변한건지",
          "original_form": "기후가 변한건지",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015682.1.8",
          "form": "그니까요 비오는거 싫은데",
          "original_form": "그니까요 비오는거 싫은데",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015682.1.9",
          "form": "빨리가을왔음 좋겠어요",
          "original_form": "빨리가을왔음 좋겠어요",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015682.1.10",
          "form": "여름 다 지나서 에어컨 틀고있어요",
          "original_form": "여름 다 지나서 에어컨 틀고있어요",
          "speaker_id": "1",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015682.1.11",
          "form": "8월초에 장마끝났다고 좋아했는데",
          "original_form": "8월초에 장마끝났다고 좋아했는데",
          "speaker_id": "1",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015682.1.12",
          "form": "저도 지금 선풍기 에어컨둘다요ㅎ",
          "original_form": "저도 지금 선풍기 에어컨둘다요ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015682.1.13",
          "form": "둘다는 춥지 않아요? ㅋㅋㅋ",
          "original_form": "둘다는 춥지 않아요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015682.1.14",
          "form": "씻고왔더니 덥네요 ㅎ",
          "original_form": "씻고왔더니 덥네요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015682.1.15",
          "form": "저녘이 습도가 높아서 샤워해도 찝찝하죠..",
          "original_form": "저녘이 습도가 높아서 샤워해도 찝찝하죠..",
          "speaker_id": "1",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015682.1.16",
          "form": "저도 낮에는 그냥 버티다가.. 밤에 풀가동",
          "original_form": "저도 낮에는 그냥 버티다가.. 밤에 풀가동",
