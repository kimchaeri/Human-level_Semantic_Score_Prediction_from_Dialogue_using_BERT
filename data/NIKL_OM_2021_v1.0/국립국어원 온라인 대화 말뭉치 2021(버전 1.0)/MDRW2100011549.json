      "utterance": [
        {
          "id": "MDRW2100011549.1.1",
          "form": "최근에 구매한 물건있으세요",
          "original_form": "최근에 구매한 물건있으세요",
          "speaker_id": "1",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011549.1.2",
          "form": "네 후드집업 하나 샀는데 오는중이요",
          "original_form": "네 후드집업 하나 샀는데 오는중이요",
          "speaker_id": "2",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011549.1.3",
          "form": "곧 가을이니까 자주 입으시겠네요",
          "original_form": "곧 가을이니까 자주 입으시겠네요",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011549.1.4",
          "form": "잘안입는 옷이긴한데 적립금쓰느라 하나 사봤어요ㅎㅎ",
          "original_form": "잘안입는 옷이긴한데 적립금쓰느라 하나 사봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011549.1.5",
          "form": "중고거래 해보신적있나요",
          "original_form": "중고거래 해보신적있나요",
          "speaker_id": "1",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011549.1.6",
          "form": "당근마켓 종종 이용해요ㅎ",
          "original_form": "당근마켓 종종 이용해요ㅎ",
          "speaker_id": "2",
          "time": "20210824 13:52"
        },
        {
          "id": "MDRW2100011549.1.7",
          "form": "옥돔님은요?",
          "original_form": "옥돔님은요?",
          "speaker_id": "2",
          "time": "20210824 13:52"
        },
        {
          "id": "MDRW2100011549.1.8",
          "form": "저도 당근 자주해요~~",
          "original_form": "저도 당근 자주해요~~",
          "speaker_id": "1",
          "time": "20210824 13:52"
        },
        {
          "id": "MDRW2100011549.1.9",
          "form": "최근에 갤럭시 버즈 팔았네요",
          "original_form": "최근에 갤럭시 버즈 팔았네요",
          "speaker_id": "1",
          "time": "20210824 13:52"
        },
        {
          "id": "MDRW2100011549.1.10",
          "form": "오ㅎ얼마에 파셨어요?",
          "original_form": "오ㅎ얼마에 파셨어요?",
          "speaker_id": "2",
          "time": "20210824 13:53"
        },
        {
          "id": "MDRW2100011549.1.11",
          "form": "버즈 플러스 핑크색이고 7만 5천원에 팔았어요",
          "original_form": "버즈 플러스 핑크색이고 7만 5천원에 팔았어요",
          "speaker_id": "1",
          "time": "20210824 13:54"
        },
        {
          "id": "MDRW2100011549.1.12",
          "form": "어디서 선물받으신거예요?",
          "original_form": "어디서 선물받으신거예요?",
          "speaker_id": "2",
          "time": "20210824 13:54"
        },
        {
          "id": "MDRW2100011549.1.13",
          "form": "아님 쓰시던거?",
          "original_form": "아님 쓰시던거?",
          "speaker_id": "2",
          "time": "20210824 13:55"
        },
        {
          "id": "MDRW2100011549.1.14",
          "form": "사은품으로 받았어요",
          "original_form": "사은품으로 받았어요",
          "speaker_id": "1",
          "time": "20210824 13:55"
        },
        {
          "id": "MDRW2100011549.1.15",
          "form": "좋네요 그런건 받아본적이 없어서",
          "original_form": "좋네요 그런건 받아본적이 없어서",
          "speaker_id": "2",
          "time": "20210824 13:55"
        },
        {
          "id": "MDRW2100011549.1.16",
          "form": "전자기기라서 잘팔리더라고요",
          "original_form": "전자기기라서 잘팔리더라고요",
