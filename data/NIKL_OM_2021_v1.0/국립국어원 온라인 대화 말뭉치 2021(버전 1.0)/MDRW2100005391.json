      "utterance": [
        {
          "id": "MDRW2100005391.1.1",
          "form": "아는형님 보시나요?",
          "original_form": "아는형님 보시나요?",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005391.1.2",
          "form": "아는형님봐요ㅎ",
          "original_form": "아는형님봐요ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005391.1.3",
          "form": "거기 출연자중에 누가가장좋으세요?ㅎ",
          "original_form": "거기 출연자중에 누가가장좋으세요?ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005391.1.4",
          "form": "고정멤버는 다 그냥그런데 게스트땜에 가끔봐요ㅎ",
          "original_form": "고정멤버는 다 그냥그런데 게스트땜에 가끔봐요ㅎ",
          "speaker_id": "1",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005391.1.5",
          "form": "아까 펜싱 4인방나오길래",
          "original_form": "아까 펜싱 4인방나오길래",
          "speaker_id": "1",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005391.1.6",
          "form": "펜싱4인방 멋있죠정말ㅎㅎ",
          "original_form": "펜싱4인방 멋있죠정말ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005391.1.7",
          "form": "실력도있지 잘생겼지ㅎ 날렵도 하고ㅎㄹ",
          "original_form": "실력도있지 잘생겼지ㅎ 날렵도 하고ㅎㄹ",
          "speaker_id": "2",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005391.1.8",
          "form": "그니까요ㅎ 오상욱선수 귀여워요",
          "original_form": "그니까요ㅎ 오상욱선수 귀여워요",
          "speaker_id": "1",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005391.1.9",
          "form": "전에 사장님귀는 당나귀귀에서 봤어요ㅎ",
          "original_form": "전에 사장님귀는 당나귀귀에서 봤어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005391.1.10",
          "form": "tv 프로그램에서 올림픽 금메달리스트나와서 봤는데 너무 재밌고 멋있더라구요ㅎ",
          "original_form": "tv 프로그램에서 올림픽 금메달리스트나와서 봤는데 너무 재밌고 멋있더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005391.1.11",
          "form": "여기저기 선수들 많이나오더라구요",
          "original_form": "여기저기 선수들 많이나오더라구요",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005391.1.12",
          "form": "유퀴즈에도 나오고",
          "original_form": "유퀴즈에도 나오고",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005391.1.13",
          "form": "선수들을 누려도되죠ㅎ 그동안 고생했는데ㅎㅎ",
          "original_form": "선수들을 누려도되죠ㅎ 그동안 고생했는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005391.1.14",
          "form": "금전적으로도 그렇고 자랑해야죠ㅎ 정말 고생했잖아요ㅎ",
          "original_form": "금전적으로도 그렇고 자랑해야죠ㅎ 정말 고생했잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005391.1.15",
          "form": "김준호 선수는 소속사도 계약했대요",
          "original_form": "김준호 선수는 소속사도 계약했대요",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005391.1.16",
          "form": "당분간 계속 나왔으면 좋겟네요ㄹ",
          "original_form": "당분간 계속 나왔으면 좋겟네요ㄹ",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005391.1.17",
          "form": "연예계 관심있나봐요",
          "original_form": "연예계 관심있나봐요",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005391.1.18",
          "form": "우와ㅎㅎ 연예인이네요ㅎㅎ",
          "original_form": "우와ㅎㅎ 연예인이네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005391.1.19",
          "form": "아이돌 느낌이 있긴해요",
          "original_form": "아이돌 느낌이 있긴해요",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005391.1.20",
          "form": "영웅재중닮은듯",
          "original_form": "영웅재중닮은듯",
          "speaker_id": "1",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005391.1.21",
          "form": "맞아요ㅎㅎ 실력도 있으니ㅎ",
          "original_form": "맞아요ㅎㅎ 실력도 있으니ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005391.1.22",
          "form": "다음올림픽때도 기대되요ㅎ",
          "original_form": "다음올림픽때도 기대되요ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005391.1.23",
          "form": "부럽긴해요ㅎ 그렇게 잘생기고 실력도 좋으면ㅎㅎ",
          "original_form": "부럽긴해요ㅎ 그렇게 잘생기고 실력도 좋으면ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005391.1.24",
          "form": "곧 동계올림픽 하지않나요?",
          "original_form": "곧 동계올림픽 하지않나요?",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005391.1.25",
          "form": "내년이라는거같던데",
          "original_form": "내년이라는거같던데",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005391.1.26",
          "form": "벌써요?ㅎ 아맞네ㅎ 2년주기니깐 올해끝났으니 내년이내요ㅎ",
          "original_form": "벌써요?ㅎ 아맞네ㅎ 2년주기니깐 올해끝났으니 내년이내요ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005391.1.27",
          "form": "동계올림픽은 스켈레톤 선수 누구지 이름이생각안나네요ㅎㅎ",
          "original_form": "동계올림픽은 스켈레톤 선수 누구지 이름이생각안나네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005391.1.28",
          "form": "바로 하나봐요 2월에",
          "original_form": "바로 하나봐요 2월에",
          "speaker_id": "1",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005391.1.29",
          "form": "컬링이요?",
          "original_form": "컬링이요?",
          "speaker_id": "1",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005391.1.30",
          "form": "아 아이언맨ㅋ",
          "original_form": "아 아이언맨ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005391.1.31",
          "form": "컬링도 잘했죠ㅎ 영미!영미!",
          "original_form": "컬링도 잘했죠ㅎ 영미!영미!",
          "speaker_id": "2",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005391.1.32",
          "form": "윤성빈선수",
          "original_form": "윤성빈선수",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005391.1.33",
          "form": "아이언맨ㅅㄹㄹ",
          "original_form": "아이언맨ㅅㄹㄹ",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005391.1.34",
          "form": "맞아요ㅎㅎ 세계1위ㅅㅎ",
          "original_form": "맞아요ㅎㅎ 세계1위ㅅㅎ",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005391.1.35",
          "form": "저도 그선수 좋아요ㅎ",
          "original_form": "저도 그선수 좋아요ㅎ",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005391.1.36",
          "form": "스키점프장 갔다왔어요ㅎㅎ",
          "original_form": "스키점프장 갔다왔어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005391.1.37",
          "form": "평창가서ㅎㄹ",
          "original_form": "평창가서ㅎㄹ",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005391.1.38",
          "form": "오ㅎ 직관하신거예요?",
          "original_form": "오ㅎ 직관하신거예요?",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005391.1.39",
          "form": "아니요ㅎㅎ 다끝나고요ㅎ",
          "original_form": "아니요ㅎㅎ 다끝나고요ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005391.1.40",
          "form": "직관은 못했어요ㅠ 일이있어서ㅍ",
          "original_form": "직관은 못했어요ㅠ 일이있어서ㅍ",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005391.1.41",
          "form": "아ㅎㅎ",
          "original_form": "아ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005391.1.42",
          "form": "얼마전에 어쩌다농구인지 거기나왔는데",
          "original_form": "얼마전에 어쩌다농구인지 거기나왔는데",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005391.1.43",
          "form": "예능많이 보시네요ㅎㅎ",
          "original_form": "예능많이 보시네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005391.1.44",
          "form": "집에있음 티비달고 살아요",
          "original_form": "집에있음 티비달고 살아요",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005391.1.45",
          "form": "tv 프로그램 뭐뭐 보세요?ㅎ",
          "original_form": "tv 프로그램 뭐뭐 보세요?ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005391.1.46",
          "form": "주로 예능봐요ㅎ 유퀴즈 전참시 나혼자산다",
          "original_form": "주로 예능봐요ㅎ 유퀴즈 전참시 나혼자산다",
          "speaker_id": "1",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005391.1.47",
          "form": "꼬꼬무 좋아했는데 10월에 다시한대요",
          "original_form": "꼬꼬무 좋아했는데 10월에 다시한대요",
          "speaker_id": "1",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005391.1.48",
          "form": "ㅎㅎ 예능저도 엄청좋아해요ㅎㅎ",
          "original_form": "ㅎㅎ 예능저도 엄청좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005391.1.49",
          "form": "전 식스센스 도시어부 당나귀귀 런닝맨 등등 재밌는거 위주로 봐요ㅎㅎ",
          "original_form": "전 식스센스 도시어부 당나귀귀 런닝맨 등등 재밌는거 위주로 봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005391.1.50",
          "form": "웃고있으면 너무 재밌더라구여ㅎ",
          "original_form": "웃고있으면 너무 재밌더라구여ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005391.1.51",
          "form": "그냥 보다보면 고민은 사라지니 티비의 장점이죠",
          "original_form": "그냥 보다보면 고민은 사라지니 티비의 장점이죠",
          "speaker_id": "1",
          "time": "20210821 23:44"
        }
      ]
