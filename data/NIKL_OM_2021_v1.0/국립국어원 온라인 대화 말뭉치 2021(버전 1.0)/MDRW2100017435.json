      "utterance": [
        {
          "id": "MDRW2100017435.1.1",
          "form": "name2님~ 오랜만이네요",
          "original_form": "&name2&님~ 오랜만이네요",
          "speaker_id": "1",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100017435.1.2",
          "form": "점심은 드셨나요?",
          "original_form": "점심은 드셨나요?",
          "speaker_id": "1",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100017435.1.3",
          "form": "안녕하세유",
          "original_form": "안녕하세유",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100017435.1.4",
          "form": "넹 방금 ㅋㅋㅋ",
          "original_form": "넹 방금 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100017435.1.5",
          "form": "무엇을 드셨어요?",
          "original_form": "무엇을 드셨어요?",
          "speaker_id": "1",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100017435.1.6",
          "form": "솥밥이요",
          "original_form": "솥밥이요",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100017435.1.7",
          "form": "17000",
          "original_form": "17000",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100017435.1.8",
          "form": "비싸다",
          "original_form": "비싸다",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100017435.1.9",
          "form": "오 솥밥 맛있었겠네요",
          "original_form": "오 솥밥 맛있었겠네요",
          "speaker_id": "1",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100017435.1.10",
          "form": "가성비 별루",
          "original_form": "가성비 별루",
          "speaker_id": "2",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100017435.1.11",
          "form": "솥밥이 가격이 꽤 나가긴 하죠",
          "original_form": "솥밥이 가격이 꽤 나가긴 하죠",
          "speaker_id": "1",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100017435.1.12",
          "form": "name1님은용",
          "original_form": "&name1&님은용",
          "speaker_id": "2",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100017435.1.13",
          "form": "저는 집에서 왕 삼치 구워먹었어요",
          "original_form": "저는 집에서 왕 삼치 구워먹었어요",
          "speaker_id": "1",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100017435.1.14",
          "form": "우와 직접 ?",
          "original_form": "우와 직접 ?",
          "speaker_id": "2",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100017435.1.15",
          "form": "어..  부모님이 구워주시긴 했는데",
          "original_form": "어..  부모님이 구워주시긴 했는데",
          "speaker_id": "1",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100017435.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 집밥 ~~~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 집밥 ~~~",
          "speaker_id": "2",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100017435.1.17",
          "form": "반찬은요",
          "original_form": "반찬은요",
          "speaker_id": "2",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100017435.1.18",
          "form": "엄청 크고 두꺼워서 한참 구웠어요",
          "original_form": "엄청 크고 두꺼워서 한참 구웠어요",
          "speaker_id": "1",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100017435.1.19",
          "form": "삼치가 크고 싱싱해서 사실 다른 반찬은 필요없더라고요",
          "original_form": "삼치가 크고 싱싱해서 사실 다른 반찬은 필요없더라고요",
          "speaker_id": "1",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100017435.1.20",
          "form": "삼치랑 김치랑 밥 먹었어요",
          "original_form": "삼치랑 김치랑 밥 먹었어요",
          "speaker_id": "1",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100017435.1.21",
          "form": "우왕 김치 맛잇을거같아요",
          "original_form": "우왕 김치 맛잇을거같아요",
          "speaker_id": "2",
          "time": "20210904 13:23"
        },
        {
          "id": "MDRW2100017435.1.22",
          "form": "네 이번에 산 김치가 잘 익어서",
          "original_form": "네 이번에 산 김치가 잘 익어서",
          "speaker_id": "1",
          "time": "20210904 13:23"
        },
        {
          "id": "MDRW2100017435.1.23",
          "form": "맛있게 먹고 있어요",
          "original_form": "맛있게 먹고 있어요",
          "speaker_id": "1",
          "time": "20210904 13:23"
        },
        {
          "id": "MDRW2100017435.1.24",
          "form": "크..사셧구나 어디서요",
          "original_form": "크..사셧구나 어디서요",
          "speaker_id": "2",
          "time": "20210904 13:23"
        },
        {
          "id": "MDRW2100017435.1.25",
          "form": "도미솔 김치라고",
          "original_form": "도미솔 김치라고",
          "speaker_id": "1",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100017435.1.26",
          "form": "전에 홈쇼핑서 샀다가 맛있어서 주기적으로 인터넷으로 시켜 먹어요",
          "original_form": "전에 홈쇼핑서 샀다가 맛있어서 주기적으로 인터넷으로 시켜 먹어요",
          "speaker_id": "1",
          "time": "20210904 13:24"
        }
      ]
