        },
        {
          "id": "MDRW2100010310.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210823 16:40"
        },
        {
          "id": "MDRW2100010310.1.3",
          "form": "이상형있으신가요?",
          "original_form": "이상형있으신가요?",
          "speaker_id": "1",
          "time": "20210823 16:44"
        },
        {
          "id": "MDRW2100010310.1.4",
          "form": "예의 바른 사람 좋아해요! 저를 존중해줬으면 하고요!",
          "original_form": "예의 바른 사람 좋아해요! 저를 존중해줬으면 하고요!",
          "speaker_id": "2",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010310.1.5",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010310.1.6",
          "form": "맞아요 저도",
          "original_form": "맞아요 저도",
          "speaker_id": "1",
          "time": "20210823 16:54"
        },
        {
          "id": "MDRW2100010310.1.7",
          "form": "내적인게 진짜 중요하다생각해요~",
          "original_form": "내적인게 진짜 중요하다생각해요~",
          "speaker_id": "1",
          "time": "20210823 16:54"
        },
        {
          "id": "MDRW2100010310.1.8",
          "form": "지금 연애중이신가요?",
          "original_form": "지금 연애중이신가요?",
          "speaker_id": "2",
          "time": "20210823 17:01"
        },
        {
          "id": "MDRW2100010310.1.9",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "1",
          "time": "20210823 17:04"
        },
        {
          "id": "MDRW2100010310.1.10",
          "form": "연애중이신가요?",
          "original_form": "연애중이신가요?",
          "speaker_id": "1",
          "time": "20210823 17:04"
        },
        {
          "id": "MDRW2100010310.1.11",
          "form": "저도 지금은 연애 안 하고 있어요",
          "original_form": "저도 지금은 연애 안 하고 있어요",
          "speaker_id": "2",
          "time": "20210823 17:05"
        },
        {
          "id": "MDRW2100010310.1.12",
          "form": "사실 요즘은 연애할 여유도 없어요ㅜ",
          "original_form": "사실 요즘은 연애할 여유도 없어요ㅜ",
          "speaker_id": "2",
          "time": "20210823 17:06"
        },
        {
          "id": "MDRW2100010310.1.13",
          "form": "별로 막 하고 싶다는 생각도 안 드네요",
          "original_form": "별로 막 하고 싶다는 생각도 안 드네요",
          "speaker_id": "2",
          "time": "20210823 17:06"
        },
        {
          "id": "MDRW2100010310.1.14",
          "form": "맞아요 대부분 그런생각들많더라고요ㅋㅋㅋ",
          "original_form": "맞아요 대부분 그런생각들많더라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010310.1.15",
          "form": "딱히괜찮은 사람도 없고",
          "original_form": "딱히괜찮은 사람도 없고",
          "speaker_id": "1",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010310.1.16",
          "form": "맞아요 제 시간이랑 막 쏟을 만큼 괜찮은 사람이 없는 거 같아요",
          "original_form": "맞아요 제 시간이랑 막 쏟을 만큼 괜찮은 사람이 없는 거 같아요",
          "speaker_id": "2",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010310.1.17",
          "form": "저는 요즘 딱히 외로움도 안 느껴요ㅋㅋㅋ",
          "original_form": "저는 요즘 딱히 외로움도 안 느껴요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010310.1.18",
          "form": "ㅋㅋㅋ 좋죠",
          "original_form": "ㅋㅋㅋ 좋죠",
          "speaker_id": "1",
          "time": "20210823 17:35"
        },
        {
          "id": "MDRW2100010310.1.19",
          "form": "결혼생각은 있나요?",
          "original_form": "결혼생각은 있나요?",
          "speaker_id": "1",
          "time": "20210823 17:36"
        },
        {
          "id": "MDRW2100010310.1.20",
          "form": "요즘은 딱히 안 드는 거 같아요",
          "original_form": "요즘은 딱히 안 드는 거 같아요",
          "speaker_id": "2",
          "time": "20210823 17:36"
        },
        {
          "id": "MDRW2100010310.1.21",
          "form": "연애든 결혼이든 의무적으로 하기는 싫어서요",
          "original_form": "연애든 결혼이든 의무적으로 하기는 싫어서요",
          "speaker_id": "2",
          "time": "20210823 17:37"
        }
      ]
