        {
          "id": "MDRW2100035101.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035101.1.2",
          "form": "내 친구가 강아지를 키우더라",
          "original_form": "내 친구가 강아지를 키우더라",
          "speaker_id": "1",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035101.1.3",
          "form": "무순 종을 키우니",
          "original_form": "무순 종을 키우니",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035101.1.4",
          "form": "작은 요크셔테리어야",
          "original_form": "작은 요크셔테리어야",
          "speaker_id": "1",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035101.1.5",
          "form": "요크셔테리어 귀엽겠다",
          "original_form": "요크셔테리어 귀엽겠다",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035101.1.6",
          "form": "이름은 name3이야",
          "original_form": "이름은 &name3&이야",
          "speaker_id": "1",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035101.1.7",
          "form": "수컷이 안 커지니",
          "original_form": "수컷이 안 커지니",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035101.1.8",
          "form": "수컷이니 암컷이니",
          "original_form": "수컷이니 암컷이니",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035101.1.9",
          "form": "수컷이야 그리고 성격이 특이해",
          "original_form": "수컷이야 그리고 성격이 특이해",
          "speaker_id": "1",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100035101.1.10",
          "form": "성격이 별로 안 좋나 보네",
          "original_form": "성격이 별로 안 좋나 보네",
          "speaker_id": "2",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100035101.1.11",
          "form": "아니야 겁이 엄청 많아",
          "original_form": "아니야 겁이 엄청 많아",
          "speaker_id": "1",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100035101.1.12",
          "form": "아아 소심한 거구나",
          "original_form": "아아 소심한 거구나",
          "speaker_id": "2",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100035101.1.13",
          "form": "그래서오히려 귀엽기도 하더라",
          "original_form": "그래서오히려 귀엽기도 하더라",
          "speaker_id": "1",
          "time": "20210829 02:42"
        },
        {
          "id": "MDRW2100035101.1.14",
          "form": "직접 본 적도 있어",
          "original_form": "직접 본 적도 있어",
          "speaker_id": "2",
          "time": "20210829 02:42"
        },
