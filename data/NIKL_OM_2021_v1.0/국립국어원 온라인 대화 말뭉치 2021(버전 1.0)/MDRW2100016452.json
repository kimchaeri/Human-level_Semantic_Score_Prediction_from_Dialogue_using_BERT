      "utterance": [
        {
          "id": "MDRW2100016452.1.1",
          "form": "겜 좋아하시나여",
          "original_form": "겜 좋아하시나여",
          "speaker_id": "1",
          "time": "20210826 13:25"
        },
        {
          "id": "MDRW2100016452.1.2",
          "form": "어느정도 좋아해요",
          "original_form": "어느정도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016452.1.3",
          "form": "어떤거 하시나요",
          "original_form": "어떤거 하시나요",
          "speaker_id": "1",
          "time": "20210826 15:03"
        },
        {
          "id": "MDRW2100016452.1.4",
          "form": "모바일게임 위주로 해요 꿈의집이라고 아세요?",
          "original_form": "모바일게임 위주로 해요 꿈의집이라고 아세요?",
          "speaker_id": "2",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100016452.1.5",
          "form": "오 처음들어봤어요",
          "original_form": "오 처음들어봤어요",
          "speaker_id": "1",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016452.1.6",
          "form": "은근히 광고 많이 해서 광고로 보셨을 수도 있어요ㅋㅋ",
          "original_form": "은근히 광고 많이 해서 광고로 보셨을 수도 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016452.1.7",
          "form": "어쨌든 꽤 재밌어요 중독성있구요",
          "original_form": "어쨌든 꽤 재밌어요 중독성있구요",
          "speaker_id": "2",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016452.1.8",
          "form": "name1은 무슨게임하세요?",
          "original_form": "&name1&은 무슨게임하세요?",
          "speaker_id": "2",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016452.1.9",
          "form": "피파온라인이요!",
          "original_form": "피파온라인이요!",
          "speaker_id": "1",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016452.1.10",
          "form": "오 그거 어렵지않아요?",
          "original_form": "오 그거 어렵지않아요?",
          "speaker_id": "2",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016452.1.11",
          "form": "봤을 때는 퀄리티도 높고 재밌어보이는더",
          "original_form": "봤을 때는 퀄리티도 높고 재밌어보이는더",
          "speaker_id": "2",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100016452.1.12",
          "form": "축구룰을 잘 몰라서 어려워보여요",
          "original_form": "축구룰을 잘 몰라서 어려워보여요",
          "speaker_id": "2",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100016452.1.13",
          "form": "맞아요 축구좋아하셔야ㅎㅎ",
          "original_form": "맞아요 축구좋아하셔야ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016452.1.14",
          "form": "피씨게임은",
          "original_form": "피씨게임은",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016452.1.15",
          "form": "최근에 심즈 외에 해본게 없네요...",
          "original_form": "최근에 심즈 외에 해본게 없네요...",
          "speaker_id": "2",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100016452.1.16",
          "form": "오 심즈 저도 예전에 해봤어요",
          "original_form": "오 심즈 저도 예전에 해봤어요",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100016452.1.17",
          "form": "ㅋㅋ심즈 재밌죠?",
          "original_form": "ㅋㅋ심즈 재밌죠?",
          "speaker_id": "2",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100016452.1.18",
          "form": "네너 추억의게임이네요",
          "original_form": "네너 추억의게임이네요",
          "speaker_id": "1",
