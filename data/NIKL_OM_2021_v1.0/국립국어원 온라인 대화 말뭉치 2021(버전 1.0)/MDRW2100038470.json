        {
          "id": "MDRW2100038470.1.1",
          "form": "우리 시장에 파는 핫도그 넘 맛있옹~",
          "original_form": "우리 시장에 파는 핫도그 넘 맛있옹~",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100038470.1.2",
          "form": "그러게 별맛 없는데",
          "original_form": "그러게 별맛 없는데",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100038470.1.3",
          "form": "ㅇㅇ 별거없는데 맛남 ㅋ",
          "original_form": "ㅇㅇ 별거없는데 맛남 ㅋ",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100038470.1.4",
          "form": "그래 근데 가격을",
          "original_form": "그래 근데 가격을",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100038470.1.5",
          "form": "아 맞다 ㅡㅠ",
          "original_form": "아 맞다 ㅡㅠ",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100038470.1.6",
          "form": "야금야금 올리더라",
          "original_form": "야금야금 올리더라",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100038470.1.7",
          "form": "장사가 잘 되니까 그러나보다",
          "original_form": "장사가 잘 되니까 그러나보다",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100038470.1.8",
          "form": "그래도 그러면 안 되지",
          "original_form": "그래도 그러면 안 되지",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100038470.1.9",
          "form": "그럼 안사먹어줘야하는데..",
          "original_form": "그럼 안사먹어줘야하는데..",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
