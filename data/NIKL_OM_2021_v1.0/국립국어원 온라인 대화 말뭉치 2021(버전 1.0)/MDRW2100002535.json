      "utterance": [
        {
          "id": "MDRW2100002535.1.1",
          "form": "염색을 하시나요?",
          "original_form": "염색을 하시나요?",
          "speaker_id": "1",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002535.1.2",
          "form": "뿌염이 귀찮아서 염색끊은지 몇년됐어요",
          "original_form": "뿌염이 귀찮아서 염색끊은지 몇년됐어요",
          "speaker_id": "2",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002535.1.3",
          "form": "이전에 했던 색깔 중에 만족스러운 색은 뭐였나요?",
          "original_form": "이전에 했던 색깔 중에 만족스러운 색은 뭐였나요?",
          "speaker_id": "1",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002535.1.4",
          "form": "항상 무난한색으로만해서",
          "original_form": "항상 무난한색으로만해서",
          "speaker_id": "2",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002535.1.5",
          "form": "그나마 밝은갈색?",
          "original_form": "그나마 밝은갈색?",
          "speaker_id": "2",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002535.1.6",
          "form": "갈색 종류 많이 하셨구나",
          "original_form": "갈색 종류 많이 하셨구나",
          "speaker_id": "1",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002535.1.7",
          "form": "저는 탈색도 염색도 즐겨해요",
          "original_form": "저는 탈색도 염색도 즐겨해요",
          "speaker_id": "1",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002535.1.8",
          "form": "저도 탈색해보고싶었는데",
          "original_form": "저도 탈색해보고싶었는데",
          "speaker_id": "2",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002535.1.9",
          "form": "뿌리올라오는거맞춰서 매번 탈색하기엔",
          "original_form": "뿌리올라오는거맞춰서 매번 탈색하기엔",
          "speaker_id": "2",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002535.1.10",
          "form": "제가너무 게을럿어요ㅠㅋㅋ",
          "original_form": "제가너무 게을럿어요ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002535.1.11",
          "form": "맞아요.. 힘들죠",
          "original_form": "맞아요.. 힘들죠",
          "speaker_id": "1",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002535.1.12",
          "form": "진짜 부지런해야 할 수 있는 거 같아요",
          "original_form": "진짜 부지런해야 할 수 있는 거 같아요",
          "speaker_id": "1",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002535.1.13",
          "form": "그래도 탈색하면",
          "original_form": "그래도 탈색하면",
          "speaker_id": "2",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002535.1.14",
          "form": "예쁜색 많이할수있어서 부럽더라구요",
          "original_form": "예쁜색 많이할수있어서 부럽더라구요",
          "speaker_id": "2",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002535.1.15",
          "form": "name1님은 어떤색이 제일 맘에드셨나요?",
          "original_form": "&name1&님은 어떤색이 제일 맘에드셨나요?",
          "speaker_id": "2",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002535.1.16",
          "form": "다양한 색을 할 수 있다는 장점 되게 좋아요",
          "original_form": "다양한 색을 할 수 있다는 장점 되게 좋아요",
          "speaker_id": "1",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002535.1.17",
          "form": "저 분홍색 마음에 들었어요",
          "original_form": "저 분홍색 마음에 들었어요",
          "speaker_id": "1",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002535.1.18",
          "form": "한창 유행 탈 때 했어서 그 때 반짝 하긴 했는데",
          "original_form": "한창 유행 탈 때 했어서 그 때 반짝 하긴 했는데",
          "speaker_id": "1",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002535.1.19",
          "form": "꽤 마음에 들더라구요",
          "original_form": "꽤 마음에 들더라구요",
          "speaker_id": "1",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002535.1.20",
          "form": "지나다니면서 분홍색하신분들 부러웠는데",
          "original_form": "지나다니면서 분홍색하신분들 부러웠는데",
          "speaker_id": "2",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002535.1.21",
          "form": "그게 name1님이셨군용",
          "original_form": "그게 &name1&님이셨군용",
          "speaker_id": "2",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002535.1.22",
          "form": "앗ㅎㅎㅎ hihihi님은 살면서 꼭 한 번 해보고싶었다 하는 색 있나요?",
          "original_form": "앗ㅎㅎㅎ hihihi님은 살면서 꼭 한 번 해보고싶었다 하는 색 있나요?",
          "speaker_id": "1",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002535.1.23",
          "form": "옴브레하는거보면서",
          "original_form": "옴브레하는거보면서",
          "speaker_id": "2",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002535.1.24",
          "form": "다싱염색할까 마음이 흔들렸어요ㅋㅋ",
          "original_form": "다싱염색할까 마음이 흔들렸어요ㅋㅋ",
