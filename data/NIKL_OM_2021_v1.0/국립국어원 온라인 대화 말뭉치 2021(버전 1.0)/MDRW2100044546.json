      "utterance": [
        {
          "id": "MDRW2100044546.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044546.1.2",
          "form": "배송이 오길 기다리고걔신 상품이 잇으새요?",
          "original_form": "배송이 오길 기다리고걔신 상품이 잇으새요?",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044546.1.3",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044546.1.4",
          "form": "네. 있어요",
          "original_form": "네. 있어요",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044546.1.5",
          "form": "뭔데요??",
          "original_form": "뭔데요??",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044546.1.6",
          "form": "파인애플주스요.",
          "original_form": "파인애플주스요.",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044546.1.7",
          "form": "마시려고요?",
          "original_form": "마시려고요?",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044546.1.8",
          "form": "아니요. 냉동해서 아이스크림처럼먹던가\r\n빵반죽에 넣으려고요",
          "original_form": "아니요. 냉동해서 아이스크림처럼먹던가\r\n빵반죽에 넣으려고요",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044546.1.9",
          "form": "오!!베이킹 하실줄아시나봐요",
          "original_form": "오!!베이킹 하실줄아시나봐요",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044546.1.10",
          "form": "네 할줄아랑요",
          "original_form": "네 할줄아랑요",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044546.1.11",
          "form": "무슨 요리하시랴고요?",
          "original_form": "무슨 요리하시랴고요?",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044546.1.12",
          "form": "저는 하와이안롤이라는 빵을 만들어보려고요",
          "original_form": "저는 하와이안롤이라는 빵을 만들어보려고요",
          "speaker_id": "2",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044546.1.13",
          "form": "오 처음들어봐요",
          "original_form": "오 처음들어봐요",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044546.1.14",
          "form": "마카롱여사님깨서 알려주신 레시피인데\r\n만들어보게되었어요",
          "original_form": "마카롱여사님깨서 알려주신 레시피인데\r\n만들어보게되었어요",
