        {
          "id": "MDRW2100039821.1.1",
          "form": "어떤 날씨를 좋아하느시나요",
          "original_form": "어떤 날씨를 좋아하느시나요",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039821.1.2",
          "form": "name2씨?",
          "original_form": "&name2&씨?",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039821.1.3",
          "form": "비오는날",
          "original_form": "비오는날",
          "speaker_id": "2",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039821.1.4",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039821.1.5",
          "form": "왜죠",
          "original_form": "왜죠",
          "speaker_id": "1",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039821.1.6",
          "form": "저는 찝찝해...",
          "original_form": "저는 찝찝해...",
          "speaker_id": "1",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039821.1.7",
          "form": "별로야...",
          "original_form": "별로야...",
          "speaker_id": "1",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039821.1.8",
          "form": "찝찝한건 역시 별로긴 하지만? 선선하고? 분위기도 좋고",
          "original_form": "찝찝한건 역시 별로긴 하지만? 선선하고? 분위기도 좋고",
          "speaker_id": "2",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039821.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039821.1.10",
          "form": "맞아 분위기 좋져",
          "original_form": "맞아 분위기 좋져",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039821.1.11",
          "form": "나는 적당히 완벽한 날씨",
          "original_form": "나는 적당히 완벽한 날씨",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039821.1.12",
          "form": "무서운거 보기 딱 좋은날",
          "original_form": "무서운거 보기 딱 좋은날",
          "speaker_id": "2",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039821.1.13",
          "form": "알죠?",
          "original_form": "알죠?",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039821.1.14",
          "form": "모순적이다",
          "original_form": "모순적이다",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039821.1.15",
          "form": "적당히 완벽하다니",
          "original_form": "적당히 완벽하다니",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039821.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나는 무서운거 싫어 그런 날 싫어..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나는 무서운거 싫어 그런 날 싫어..",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039821.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039821.1.18",
          "form": "모순적이라니",
          "original_form": "모순적이라니",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039821.1.19",
          "form": "적당히 완벽한 날이",
          "original_form": "적당히 완벽한 날이",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039821.1.20",
          "form": "쉬운게 아냐",
          "original_form": "쉬운게 아냐",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039821.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ적당하지않아!!",
