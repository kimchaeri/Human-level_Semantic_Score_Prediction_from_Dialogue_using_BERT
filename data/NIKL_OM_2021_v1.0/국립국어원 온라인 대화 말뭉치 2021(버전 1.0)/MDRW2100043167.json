      "utterance": [
        {
          "id": "MDRW2100043167.1.1",
          "form": "타이레놀먹었나",
          "original_form": "타이레놀먹었나",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100043167.1.2",
          "form": "먹었다 식은땀이 나더라",
          "original_form": "먹었다 식은땀이 나더라",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043167.1.3",
          "form": "난 먹었는데 효과가없노",
          "original_form": "난 먹었는데 효과가없노",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043167.1.4",
          "form": "열나는게아니라서그런가",
          "original_form": "열나는게아니라서그런가",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043167.1.5",
          "form": "열나는게 아니라서 그런가보다ㅜ",
          "original_form": "열나는게 아니라서 그런가보다ㅜ",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043167.1.6",
          "form": "어 그런가보다",
          "original_form": "어 그런가보다",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043167.1.7",
          "form": "몸은 그래서 낮보다 어떤데?",
          "original_form": "몸은 그래서 낮보다 어떤데?",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043167.1.8",
          "form": "밥먹고 약먹으니 괜찮은거같긴하다 일단은",
          "original_form": "밥먹고 약먹으니 괜찮은거같긴하다 일단은",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043167.1.9",
          "form": "타이레놀이 열에만 좋은게 아닌가보다",
          "original_form": "타이레놀이 열에만 좋은게 아닌가보다",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043167.1.10",
          "form": "안서방은 괜찮나?",
          "original_form": "안서방은 괜찮나?",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043167.1.11",
          "form": "안서방은 피곤하다하지 미쓱거리고",
          "original_form": "안서방은 피곤하다하지 미쓱거리고",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043167.1.12",
          "form": "내일되면 더 아플텐데ㅜ",
          "original_form": "내일되면 더 아플텐데ㅜ",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043167.1.13",
          "form": "낼은 쉬니까",
          "original_form": "낼은 쉬니까",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043167.1.14",
          "form": "재택이라 편하네",
          "original_form": "재택이라 편하네",
