      "utterance": [
        {
          "id": "MDRW2100006322.1.1",
          "form": "이번 명절은 몇인까지인가요?",
          "original_form": "이번 명절은 몇인까지인가요?",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100006322.1.2",
          "form": "모르겠어욕ㅋ",
          "original_form": "모르겠어욕ㅋ",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100006322.1.3",
          "form": "안만날거니까 인원확인안했어요ㅜㅜ",
          "original_form": "안만날거니까 인원확인안했어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006322.1.4",
          "form": "지난 명절엔 시댁 안가서 좋았는데요",
          "original_form": "지난 명절엔 시댁 안가서 좋았는데요",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006322.1.5",
          "form": "아! ㅋㅋ 중요하네요ㅜ",
          "original_form": "아! ㅋㅋ 중요하네요ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100006322.1.6",
          "form": "지난번엔 5인이라 저 빼고 갔거든요 ㅋㅋㅋ",
          "original_form": "지난번엔 5인이라 저 빼고 갔거든요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100006322.1.7",
          "form": "아ㅋㅋ",
          "original_form": "아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100006322.1.8",
          "form": "백신때매 아프다고하는건 너무 티나죠...",
          "original_form": "백신때매 아프다고하는건 너무 티나죠...",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100006322.1.9",
          "form": "네 이미 맞은지 넘 오래됐어요 ㅋㅋㅋㅋ",
          "original_form": "네 이미 맞은지 넘 오래됐어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100006322.1.10",
          "form": "부스터샷 혼자 맞았다고ㅋㅈㅋ",
          "original_form": "부스터샷 혼자 맞았다고ㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100006322.1.11",
          "form": "그럴까요?? 정말 격하게 혼자있고 싶어요",
          "original_form": "그럴까요?? 정말 격하게 혼자있고 싶어요",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100006322.1.12",
          "form": "급 배탈!",
          "original_form": "급 배탈!",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100006322.1.13",
          "form": "아 김밥... 식중독....ㅜ",
          "original_form": "아 김밥... 식중독....ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100006322.1.14",
          "form": "그럴까요? 그냥 이동금지 명령을 제발 내려주셨으면 좋겠어요",
          "original_form": "그럴까요? 그냥 이동금지 명령을 제발 내려주셨으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100006322.1.15",
          "form": "그러니까요!!! 아오!!!",
          "original_form": "그러니까요!!! 아오!!!",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006322.1.16",
          "form": "확진자동선!!!",
          "original_form": "확진자동선!!!",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006322.1.17",
          "form": "겹쳤다고하세요!",
          "original_form": "겹쳤다고하세요!",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006322.1.18",
          "form": "아이고 모르겠어요. 근데 추석 방역대책은.언제 발표한거죠?",
          "original_form": "아이고 모르겠어요. 근데 추석 방역대책은.언제 발표한거죠?",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006322.1.19",
          "form": "어젠가.??",
          "original_form": "어젠가.??",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006322.1.20",
