      "utterance": [
        {
          "id": "MDRW2100017008.1.1",
          "form": "읽은거 아냐?",
          "original_form": "읽은거 아냐?",
          "speaker_id": "2",
          "time": "20210826 16:50"
        },
        {
          "id": "MDRW2100017008.1.2",
          "form": "제목이 기억안남",
          "original_form": "제목이 기억안남",
          "speaker_id": "1",
          "time": "20210826 16:51"
        },
        {
          "id": "MDRW2100017008.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:51"
        },
        {
          "id": "MDRW2100017008.1.4",
          "form": "이게 스토리가 있다기 보단",
          "original_form": "이게 스토리가 있다기 보단",
          "speaker_id": "1",
          "time": "20210826 16:51"
        },
        {
          "id": "MDRW2100017008.1.5",
          "form": "마음의 병을 형상화한 거임",
          "original_form": "마음의 병을 형상화한 거임",
          "speaker_id": "1",
          "time": "20210826 16:51"
        },
        {
          "id": "MDRW2100017008.1.6",
          "form": "애들용이 아닌데",
          "original_form": "애들용이 아닌데",
          "speaker_id": "2",
          "time": "20210826 16:51"
        },
        {
          "id": "MDRW2100017008.1.7",
          "form": "ㅇㅇ아무래도..",
          "original_form": "ㅇㅇ아무래도..",
          "speaker_id": "1",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100017008.1.8",
          "form": "근데 그림 진짜 예쁨",
          "original_form": "근데 그림 진짜 예쁨",
          "speaker_id": "1",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100017008.1.9",
          "form": "나중에 보야조",
          "original_form": "나중에 보야조",
          "speaker_id": "2",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100017008.1.10",
          "form": "집가서 찾아봄ㅋㅋㅋ",
          "original_form": "집가서 찾아봄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017008.1.11",
          "form": "조오아",
          "original_form": "조오아",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017008.1.12",
          "form": "아 키큰애가 리디셀렉 해서",
          "original_form": "아 키큰애가 리디셀렉 해서",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017008.1.13",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017008.1.14",
          "form": "ㅋㅋㅋ잘 안읽어서 체험도 안해봤는데",
          "original_form": "ㅋㅋㅋ잘 안읽어서 체험도 안해봤는데",
          "speaker_id": "1",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100017008.1.15",
          "form": "나중에 하면",
          "original_form": "나중에 하면",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100017008.1.16",
          "form": "눈물을 마시는 새",
          "original_form": "눈물을 마시는 새",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100017008.1.17",
          "form": "잊지마",
          "original_form": "잊지마",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100017008.1.18",
          "form": "그래 알겠어ㅋㅋㅋㅋ",
          "original_form": "그래 알겠어ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
