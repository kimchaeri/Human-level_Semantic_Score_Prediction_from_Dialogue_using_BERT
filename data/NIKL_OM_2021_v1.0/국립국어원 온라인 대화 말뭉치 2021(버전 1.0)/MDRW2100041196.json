        },
        {
          "id": "MDRW2100041196.1.2",
          "form": "웃기노 ㅋㅋㅋ",
          "original_form": "웃기노 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100041196.1.3",
          "form": "ㅋㅋㅋㅋㅋㄱ 그렇지 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㄱ 그렇지 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210905 19:58"
        },
        {
          "id": "MDRW2100041196.1.4",
          "form": "원래 4주뒤 였는데",
          "original_form": "원래 4주뒤 였는데",
          "speaker_id": "2",
          "time": "20210905 19:58"
        },
        {
          "id": "MDRW2100041196.1.5",
          "form": "6주로 밀려가지고",
          "original_form": "6주로 밀려가지고",
          "speaker_id": "2",
          "time": "20210905 19:58"
        },
        {
          "id": "MDRW2100041196.1.6",
          "form": "니도 6주뒤에 2차맞나??",
          "original_form": "니도 6주뒤에 2차맞나??",
          "speaker_id": "2",
          "time": "20210905 19:58"
        },
        {
          "id": "MDRW2100041196.1.7",
          "form": "맞어 맞어 ㅋㅋ",
          "original_form": "맞어 맞어 ㅋㅋ",
