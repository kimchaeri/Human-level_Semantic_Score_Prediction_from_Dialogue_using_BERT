        {
          "id": "MDRW2100020766.1.1",
          "form": "지인 중 군대 가있는 분이 계신가요?",
          "original_form": "지인 중 군대 가있는 분이 계신가요?",
          "speaker_id": "1",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100020766.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100020766.1.3",
          "form": "이제 다 제대를 했어요",
          "original_form": "이제 다 제대를 했어요",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100020766.1.4",
          "form": "제주변은",
          "original_form": "제주변은",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100020766.1.5",
          "form": "name1님은 군대지인이 있으신가요?",
          "original_form": "&name1&님은 군대지인이 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100020766.1.6",
          "form": "제 주변에는 지금 둘 정도 가있어요",
          "original_form": "제 주변에는 지금 둘 정도 가있어요",
          "speaker_id": "1",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100020766.1.7",
          "form": "피엑스 부탁을",
          "original_form": "피엑스 부탁을",
          "speaker_id": "2",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100020766.1.8",
          "form": "하세요!!",
          "original_form": "하세요!!",
          "speaker_id": "2",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100020766.1.9",
          "form": "그때가 적기입니다",
          "original_form": "그때가 적기입니다",
          "speaker_id": "2",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100020766.1.10",
          "form": "어떤 물품이 좋을까요???",
          "original_form": "어떤 물품이 좋을까요???",
          "speaker_id": "1",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100020766.1.11",
          "form": "저는 닥터지선크림이랑 듀이트리 크림 사달라고했어요",
          "original_form": "저는 닥터지선크림이랑 듀이트리 크림 사달라고했어요",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100020766.1.12",
          "form": "하겐다즈도 싸대요",
          "original_form": "하겐다즈도 싸대요",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100020766.1.13",
          "form": "정말요??? 하겐다즈도 싸다니....이건 정말 부탁해야겠네요",
          "original_form": "정말요??? 하겐다즈도 싸다니....이건 정말 부탁해야겠네요",
          "speaker_id": "1",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100020766.1.14",
          "form": "하겐다즈도 싸게 판다는걸 저는 얼마전에 알아서",
          "original_form": "하겐다즈도 싸게 판다는걸 저는 얼마전에 알아서",
          "speaker_id": "2",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100020766.1.15",
          "form": "즐기지못했어요",
          "original_form": "즐기지못했어요",
          "speaker_id": "2",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100020766.1.16",
          "form": "아이고ㅠㅠㅠ 저는 아직 기회가 있네요..! 동생도 아직 안 갔구요ㅋㅋ",
          "original_form": "아이고ㅠㅠㅠ 저는 아직 기회가 있네요..! 동생도 아직 안 갔구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100020766.1.17",
          "form": "부러워요!!",
          "original_form": "부러워요!!",
          "speaker_id": "2",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100020766.1.18",
          "form": "그 로카티셔츠도 갖고싶어요",
          "original_form": "그 로카티셔츠도 갖고싶어요",
          "speaker_id": "2",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100020766.1.19",
          "form": "그 티셔츠도 괜찮다고 하더라구요",
          "original_form": "그 티셔츠도 괜찮다고 하더라구요",
          "speaker_id": "1",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100020766.1.20",
          "form": "런데이나가면",
          "original_form": "런데이나가면",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100020766.1.21",
          "form": "그거입은사람 많아요",
          "original_form": "그거입은사람 많아요",
