      "utterance": [
        {
          "id": "MDRW2100034324.1.1",
          "form": "아르바이트 해보셨어요?",
          "original_form": "아르바이트 해보셨어요?",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100034324.1.2",
          "form": "네 대학교때 많이  했죠",
          "original_form": "네 대학교때 많이  했죠",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100034324.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100034324.1.4",
          "form": "저두요 ㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100034324.1.5",
          "form": "보통 서빙 많이하는거같아요",
          "original_form": "보통 서빙 많이하는거같아요",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034324.1.6",
          "form": "저는 서빙은 한번도 안해봤는데",
          "original_form": "저는 서빙은 한번도 안해봤는데",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034324.1.7",
          "form": "재밌나요ㅋㅋ",
          "original_form": "재밌나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034324.1.8",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034324.1.9",
          "form": "하 저는 일은..",
          "original_form": "하 저는 일은..",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034324.1.10",
          "form": "다재미없어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "다재미없어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034324.1.11",
          "form": "아 그쵸 재밌진않죠ㅋㅋ",
          "original_form": "아 그쵸 재밌진않죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034324.1.12",
          "form": "응대하는게 어려울거같아요",
          "original_form": "응대하는게 어려울거같아요",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034324.1.13",
          "form": "맞아요 ㅜㅜ",
          "original_form": "맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100034324.1.14",
          "form": "저는 본죽이랑 치킨집 햇엇는데",
          "original_form": "저는 본죽이랑 치킨집 햇엇는데",
          "speaker_id": "1",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100034324.1.15",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100034324.1.16",
          "form": "하필 사람많은 강남 치킨집이라 힘들엇어요 ㅠ",
          "original_form": "하필 사람많은 강남 치킨집이라 힘들엇어요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100034324.1.17",
          "form": "와..장난안였겠네요",
          "original_form": "와..장난안였겠네요",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100034324.1.18",
          "form": "본죽도 손님 많지않나요?",
          "original_form": "본죽도 손님 많지않나요?",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100034324.1.19",
          "form": "맞아여 동네 죽집이엇는데",
          "original_form": "맞아여 동네 죽집이엇는데",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100034324.1.20",
          "form": "포장손님이 대부분인데 막 주문하고",
          "original_form": "포장손님이 대부분인데 막 주문하고",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100034324.1.21",
          "form": "슈퍼갓다오시고 그러니까 저희는 번호표도 없엇거든요 ㅠ",
          "original_form": "슈퍼갓다오시고 그러니까 저희는 번호표도 없엇거든요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100034324.1.22",
          "form": "뭐가뭔지 엄청 헷갈렸어요",
          "original_form": "뭐가뭔지 엄청 헷갈렸어요",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034324.1.23",
          "form": "아..진짜 고난도 알바네요",
          "original_form": "아..진짜 고난도 알바네요",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034324.1.24",
          "form": "네 ㅠㅠ 골치더라구여 그게",
          "original_form": "네 ㅠㅠ 골치더라구여 그게",
          "speaker_id": "1",
          "time": "20210827 23:32"
        }
      ]
