      "utterance": [
        {
          "id": "MDRW2100017058.1.1",
          "form": "로맨스 아닌 장르는",
          "original_form": "로맨스 아닌 장르는",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017058.1.2",
          "form": "벨은 리디북스",
          "original_form": "벨은 리디북스",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017058.1.3",
          "form": "판소나 무협은 문피아가",
          "original_form": "판소나 무협은 문피아가",
          "speaker_id": "2",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100017058.1.4",
          "form": "대세라",
          "original_form": "대세라",
          "speaker_id": "2",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100017058.1.5",
          "form": "카카오 안해도 괜찮지 않나",
          "original_form": "카카오 안해도 괜찮지 않나",
          "speaker_id": "2",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100017058.1.6",
          "form": "그건 그렇지",
          "original_form": "그건 그렇지",
          "speaker_id": "1",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100017058.1.7",
          "form": "근데 탐라 추천받은거 다 카카오라ㅋㅋㅋㅋ",
          "original_form": "근데 탐라 추천받은거 다 카카오라ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017058.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017058.1.9",
          "form": "카카오가 장르 소설 요즘 많이 차지했지",
          "original_form": "카카오가 장르 소설 요즘 많이 차지했지",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017058.1.10",
          "form": "웹툰도 많아지는거 같구",
          "original_form": "웹툰도 많아지는거 같구",
          "speaker_id": "1",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100017058.1.11",
          "form": "요즘 애들은 확실히 책을 잘 안 읽어",
          "original_form": "요즘 애들은 확실히 책을 잘 안 읽어",
          "speaker_id": "2",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100017058.1.12",
          "form": "책말고 할게 많긴 하지",
          "original_form": "책말고 할게 많긴 하지",
          "speaker_id": "1",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100017058.1.13",
          "form": "하지만 난 책이 더 좋아",
          "original_form": "하지만 난 책이 더 좋아",
          "speaker_id": "2",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017058.1.14",
          "form": "나더 퇴사하고 책이나 읽고싶다",
          "original_form": "나더 퇴사하고 책이나 읽고싶다",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017058.1.15",
          "form": "좋은 아이디어",
          "original_form": "좋은 아이디어",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017058.1.16",
          "form": "컴 그만 볼래ㅋㅋㅋㅋ",
          "original_form": "컴 그만 볼래ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017058.1.17",
          "form": "눈물을 마시는 새 잊지 말아요",
          "original_form": "눈물을 마시는 새 잊지 말아요",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017058.1.18",
          "form": "알겠엉ㅋㅋㅋ 보면 얘기함",
          "original_form": "알겠엉ㅋㅋㅋ 보면 얘기함",
          "speaker_id": "1",
          "time": "20210826 17:23"
        },
