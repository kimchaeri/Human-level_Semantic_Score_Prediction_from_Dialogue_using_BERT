      "utterance": [
        {
          "id": "MDRW2100019369.1.1",
          "form": "어디꺼?",
          "original_form": "어디꺼?",
          "speaker_id": "2",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019369.1.2",
          "form": "한만두 일반만두랑 얇은피 김치만두!!",
          "original_form": "한만두 일반만두랑 얇은피 김치만두!!",
          "speaker_id": "1",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019369.1.3",
          "form": "한만두 저번에 먹어본거같아",
          "original_form": "한만두 저번에 먹어본거같아",
          "speaker_id": "2",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100019369.1.4",
          "form": "김치만두로ㅋ",
          "original_form": "김치만두로ㅋ",
          "speaker_id": "2",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100019369.1.5",
          "form": "김치맛 찐하게 잘나고 맛 괜찮았던거같아",
          "original_form": "김치맛 찐하게 잘나고 맛 괜찮았던거같아",
          "speaker_id": "2",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100019369.1.6",
          "form": "한만두 난 매운갈비가 괜찮더라",
          "original_form": "한만두 난 매운갈비가 괜찮더라",
          "speaker_id": "1",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100019369.1.7",
          "form": "그냥 갈비만두는 느끼하고",
          "original_form": "그냥 갈비만두는 느끼하고",
          "speaker_id": "1",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019369.1.8",
          "form": "난 피 얇은 만두가 좋아\r\n밀가루맛 안나는ㅎ",
          "original_form": "난 피 얇은 만두가 좋아\r\n밀가루맛 안나는ㅎ",
          "speaker_id": "2",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019369.1.9",
          "form": "맞다",
          "original_form": "맞다",
          "speaker_id": "2",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019369.1.10",
          "form": "한만두에 매운갈비 인기더라",
          "original_form": "한만두에 매운갈비 인기더라",
          "speaker_id": "2",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019369.1.11",
          "form": "한만두 김치만두는 먹어봤을건데 기억이안나네",
          "original_form": "한만두 김치만두는 먹어봤을건데 기억이안나네",
          "speaker_id": "1",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019369.1.12",
          "form": "매운갈비 만두는 울 엄마도 괜찮다고 하시더라ㅎ",
          "original_form": "매운갈비 만두는 울 엄마도 괜찮다고 하시더라ㅎ",
          "speaker_id": "2",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019369.1.13",
          "form": "ㅇㅇ근데 달달한거 싫어하는사람은 안맞을듯",
          "original_form": "ㅇㅇ근데 달달한거 싫어하는사람은 안맞을듯",
          "speaker_id": "1",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100019369.1.14",
          "form": "저번에 할인?할때 샀던거 같은데\r\n다음에도 할인하면 매운갈비 사야지",
          "original_form": "저번에 할인?할때 샀던거 같은데\r\n다음에도 할인하면 매운갈비 사야지",
          "speaker_id": "2",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100019369.1.15",
          "form": "ㅇㅇ 그냥 갈비만두보단 훨 나아",
          "original_form": "ㅇㅇ 그냥 갈비만두보단 훨 나아",
          "speaker_id": "1",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100019369.1.16",
          "form": "달달매콤\r\n고기맛이라 괜찮긴했어ㅎ",
          "original_form": "달달매콤\r\n고기맛이라 괜찮긴했어ㅎ",
          "speaker_id": "2",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100019369.1.17",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "2",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100019369.1.18",
          "form": "그냥 갈비만두는 진짜 단맛만두라",
          "original_form": "그냥 갈비만두는 진짜 단맛만두라",
          "speaker_id": "2",
          "time": "20210827 18:32"
        },
        {
          "id": "MDRW2100019369.1.19",
          "form": "물려",
          "original_form": "물려",
          "speaker_id": "2",
          "time": "20210827 18:32"
        },
        {
          "id": "MDRW2100019369.1.20",
          "form": "ㅇㅇ 금방 물리더라",
          "original_form": "ㅇㅇ 금방 물리더라",
          "speaker_id": "1",
          "time": "20210827 18:32"
        },
        {
          "id": "MDRW2100019369.1.21",
          "form": "얇은피만두는",
          "original_form": "얇은피만두는",
          "speaker_id": "1",
          "time": "20210827 18:32"
        },
        {
          "id": "MDRW2100019369.1.22",
          "form": "응 매운갈비만두 도입잘한듯",
          "original_form": "응 매운갈비만두 도입잘한듯",
          "speaker_id": "2",
          "time": "20210827 18:32"
        },
        {
          "id": "MDRW2100019369.1.23",
          "form": "깻잎이랑 땡초만두가 맛있어",
          "original_form": "깻잎이랑 땡초만두가 맛있어",
          "speaker_id": "1",
          "time": "20210827 18:32"
        },
        {
          "id": "MDRW2100019369.1.24",
          "form": "김치만두가 최애인 사람도 많던데 난 그냥 그렇더라",
          "original_form": "김치만두가 최애인 사람도 많던데 난 그냥 그렇더라",
          "speaker_id": "1",
