      "utterance": [
        {
          "id": "MDRW2100045089.1.1",
          "form": "요즘 좀 추워졋다 그치",
          "original_form": "요즘 좀 추워졋다 그치",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045089.1.2",
          "form": "맞아ㅠㅠ 오늘 비가 와서 더 추운거 같아",
          "original_form": "맞아ㅠㅠ 오늘 비가 와서 더 추운거 같아",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045089.1.3",
          "form": "이제 가을이오나봥",
          "original_form": "이제 가을이오나봥",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045089.1.4",
          "form": "윽.. 맞아..",
          "original_form": "윽.. 맞아..",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045089.1.5",
          "form": "근데 가을하니까 생각난건데",
          "original_form": "근데 가을하니까 생각난건데",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045089.1.6",
          "form": "가을 모기가 너무 독하다...",
          "original_form": "가을 모기가 너무 독하다...",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045089.1.7",
          "form": "ㅋㅋㅋㅋ 그래?? 나는 잘 안물던데",
          "original_form": "ㅋㅋㅋㅋ 그래?? 나는 잘 안물던데",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045089.1.8",
          "form": "다 수컷모긴가봐",
          "original_form": "다 수컷모긴가봐",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045089.1.9",
          "form": "그래? 부럽다ㅠㅠ 난 한마리한테 10방 이상은 물린거 같아",
          "original_form": "그래? 부럽다ㅠㅠ 난 한마리한테 10방 이상은 물린거 같아",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045089.1.10",
          "form": "그것도 나혼자만...",
          "original_form": "그것도 나혼자만...",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045089.1.11",
          "form": "대박..",
          "original_form": "대박..",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045089.1.12",
          "form": "비가오는데 모기가그리많나?",
          "original_form": "비가오는데 모기가그리많나?",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045089.1.13",
          "form": "내일도비온대",
          "original_form": "내일도비온대",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045089.1.14",
          "form": "비가 와도 모기는 있나봐",
          "original_form": "비가 와도 모기는 있나봐",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045089.1.15",
          "form": "내일도?",
          "original_form": "내일도?",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045089.1.16",
          "form": "뒤늦은 장마인가...",
          "original_form": "뒤늦은 장마인가...",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045089.1.17",
          "form": "갑자기 날씨가 바뀌었네",
          "original_form": "갑자기 날씨가 바뀌었네",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045089.1.18",
          "form": "내일도비왓었는데 아닌거로바꼇네",
          "original_form": "내일도비왓었는데 아닌거로바꼇네",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045089.1.19",
          "form": "엥? 그래?",
          "original_form": "엥? 그래?",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045089.1.20",
          "form": "요즘 날씨 계속 바뀌네...",
          "original_form": "요즘 날씨 계속 바뀌네...",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045089.1.21",
          "form": "실시간으로 바뀌나보다",
          "original_form": "실시간으로 바뀌나보다",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045089.1.22",
          "form": "옷을모입어야할지모르겟넹",
          "original_form": "옷을모입어야할지모르겟넹",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045089.1.23",
          "form": "맞아 요즘 날씨가 이상해서 반팔에 겉옷 챙기는거 밖에 못하겠더라ㅠ",
          "original_form": "맞아 요즘 날씨가 이상해서 반팔에 겉옷 챙기는거 밖에 못하겠더라ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045089.1.24",
          "form": "겉옷 가지고 다니면 걸리적 거려서 불편한데",
          "original_form": "겉옷 가지고 다니면 걸리적 거려서 불편한데",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045089.1.25",
          "form": "나도오늘 나시원피스에 긴팔 가디건 입었어",
          "original_form": "나도오늘 나시원피스에 긴팔 가디건 입었어",
          "speaker_id": "1",
          "time": "20210908 00:56"
        }
      ]
    }
  ]
