      "utterance": [
        {
          "id": "MDRW2100039416.1.1",
          "form": "그래? ㅎㅎㅎㅎㅎ다행이다",
          "original_form": "그래? ㅎㅎㅎㅎㅎ다행이다",
          "speaker_id": "2",
          "time": "20210903 17:23"
        },
        {
          "id": "MDRW2100039416.1.2",
          "form": "선천적으로 다리 튼튼했거든",
          "original_form": "선천적으로 다리 튼튼했거든",
          "speaker_id": "2",
          "time": "20210903 17:23"
        },
        {
          "id": "MDRW2100039416.1.3",
          "form": "신기하다 다리근육이 많은편이구나",
          "original_form": "신기하다 다리근육이 많은편이구나",
          "speaker_id": "1",
          "time": "20210903 17:23"
        },
        {
          "id": "MDRW2100039416.1.4",
          "form": "아니 그냥 명상하는 겸 아무것도 없이 마스크만 끼고 올라",
          "original_form": "아니 그냥 명상하는 겸 아무것도 없이 마스크만 끼고 올라",
          "speaker_id": "2",
          "time": "20210903 17:23"
        },
        {
          "id": "MDRW2100039416.1.5",
          "form": "늙으면 허리근육도 중요하대",
          "original_form": "늙으면 허리근육도 중요하대",
          "speaker_id": "1",
          "time": "20210903 17:23"
        },
        {
          "id": "MDRW2100039416.1.6",
          "form": "허리는 그닥 안 튼튼한데 이런",
          "original_form": "허리는 그닥 안 튼튼한데 이런",
          "speaker_id": "2",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039416.1.7",
          "form": "숨이 많이 찰텐대 대단해",
          "original_form": "숨이 많이 찰텐대 대단해",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039416.1.8",
          "form": "허리근육이 뒤에 있다보니까",
          "original_form": "허리근육이 뒤에 있다보니까",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039416.1.9",
          "form": "현대인이 뒷근육을 안써서",
          "original_form": "현대인이 뒷근육을 안써서",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039416.1.10",
          "form": "후후 고마버",
          "original_form": "후후 고마버",
          "speaker_id": "2",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039416.1.11",
          "form": "문제가 생기는거래",
          "original_form": "문제가 생기는거래",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039416.1.12",
          "form": "그래? 책상에 계속 앉아있어서 그런가?",
          "original_form": "그래? 책상에 계속 앉아있어서 그런가?",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039416.1.13",
          "form": "그런가봐 그래서 헬스가서 뒷근육 운동하면",
          "original_form": "그런가봐 그래서 헬스가서 뒷근육 운동하면",
          "speaker_id": "1",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039416.1.14",
          "form": "엄청 힘들더라",
          "original_form": "엄청 힘들더라",
          "speaker_id": "1",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039416.1.15",
          "form": "뒷근육 부실한데 어떻게 키워야할까?",
          "original_form": "뒷근육 부실한데 어떻게 키워야할까?",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039416.1.16",
          "form": "무슨 운동으로 해 넌?",
          "original_form": "무슨 운동으로 해 넌?",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039416.1.17",
          "form": "헬스장가서 레그컬 누워서 하는거랑 등운동같은거나",
          "original_form": "헬스장가서 레그컬 누워서 하는거랑 등운동같은거나",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039416.1.18",
          "form": "등등 이름을 잘모르겠네",
          "original_form": "등등 이름을 잘모르겠네",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039416.1.19",
          "form": "홈트의 단점이 무게를 칠수없어서",
          "original_form": "홈트의 단점이 무게를 칠수없어서",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039416.1.20",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039416.1.21",
          "form": "근력운동은 무게를 치는게 중요하니까",
          "original_form": "근력운동은 무게를 치는게 중요하니까",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039416.1.22",
          "form": "아 맞아 집에서 하면 도구가 없어서 무게를 못 치더라고",
          "original_form": "아 맞아 집에서 하면 도구가 없어서 무게를 못 치더라고",
          "speaker_id": "2",
          "time": "20210903 17:27"
        }
