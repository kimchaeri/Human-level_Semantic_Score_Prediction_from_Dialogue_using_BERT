      "utterance": [
        {
          "id": "MDRW2100031925.1.1",
          "form": "아침 드셨나요",
          "original_form": "아침 드셨나요",
          "speaker_id": "1",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031925.1.2",
          "form": "이제 먹으려구여 ㅋㅋ",
          "original_form": "이제 먹으려구여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100031925.1.3",
          "form": "일어난지 얼마 안되가지고ㅋ",
          "original_form": "일어난지 얼마 안되가지고ㅋ",
          "speaker_id": "2",
          "time": "20210905 08:52"
        },
        {
          "id": "MDRW2100031925.1.4",
          "form": "뭐 드셨나요",
          "original_form": "뭐 드셨나요",
          "speaker_id": "1",
          "time": "20210905 13:27"
        },
        {
          "id": "MDRW2100031925.1.5",
          "form": "점심은 라면이요",
          "original_form": "점심은 라면이요",
          "speaker_id": "2",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100031925.1.6",
          "form": "맛있겠네요 어떤 라면 드시나요",
          "original_form": "맛있겠네요 어떤 라면 드시나요",
          "speaker_id": "1",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100031925.1.7",
          "form": "저는 너구리 좋아해요 ㅎㅎ",
          "original_form": "저는 너구리 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100031925.1.8",
          "form": "얼큰해서",
          "original_form": "얼큰해서",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100031925.1.9",
          "form": "너구리 맛있죠 오동통면도 드셔보셨나요",
          "original_form": "너구리 맛있죠 오동통면도 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100031925.1.10",
          "form": "오동통면은 컵라면으로요",
          "original_form": "오동통면은 컵라면으로요",
          "speaker_id": "2",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100031925.1.11",
          "form": "비슷한 맛인가요?",
          "original_form": "비슷한 맛인가요?",
          "speaker_id": "1",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100031925.1.12",
          "form": "네 비슷한데 너구리가 조금더 매운?",
          "original_form": "네 비슷한데 너구리가 조금더 매운?",
          "speaker_id": "2",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100031925.1.13",
          "form": "그렇군요 저는 안먹어봐서 궁금했네요",
          "original_form": "그렇군요 저는 안먹어봐서 궁금했네요",
          "speaker_id": "1",
          "time": "20210905 13:59"
        },
        {
          "id": "MDRW2100031925.1.14",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "2",
          "time": "20210905 14:02"
        },
        {
          "id": "MDRW2100031925.1.15",
          "form": "네 떡국 먹었습니다 듕이님은요?",
          "original_form": "네 떡국 먹었습니다 듕이님은요?",
          "speaker_id": "1",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100031925.1.16",
          "form": "식사는 뭐하셨어요",
          "original_form": "식사는 뭐하셨어요",
          "speaker_id": "1",
