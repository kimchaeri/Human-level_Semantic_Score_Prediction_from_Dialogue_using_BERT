      "utterance": [
        {
          "id": "MDRW2100010221.1.1",
          "form": "나는 아마 이사하면서 다 버린 것 같야",
          "original_form": "나는 아마 이사하면서 다 버린 것 같야",
          "speaker_id": "1",
          "time": "20210823 15:49"
        },
        {
          "id": "MDRW2100010221.1.2",
          "form": "너 쿠키런 해봣니?",
          "original_form": "너 쿠키런 해봣니?",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010221.1.3",
          "form": "해봤지",
          "original_form": "해봤지",
          "speaker_id": "2",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010221.1.4",
          "form": "쿠키들이 정말 귀여워",
          "original_form": "쿠키들이 정말 귀여워",
          "speaker_id": "2",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010221.1.5",
          "form": "너는 어떤 쿠키를 제일 좋아하니?",
          "original_form": "너는 어떤 쿠키를 제일 좋아하니?",
          "speaker_id": "2",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010221.1.6",
          "form": "맞아 혹시 좋아하는 쿠키 있어?",
          "original_form": "맞아 혹시 좋아하는 쿠키 있어?",
          "speaker_id": "1",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010221.1.7",
          "form": "앗 통했메",
          "original_form": "앗 통했메",
          "speaker_id": "2",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010221.1.8",
          "form": "ㅋㅋㅋㅋ나는 달빛술사쿠키 좋아해",
          "original_form": "ㅋㅋㅋㅋ나는 달빛술사쿠키 좋아해",
          "speaker_id": "1",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010221.1.9",
          "form": "나는 바다요정쿠키도 좋아하고 달 타고다니는 애도 좋아해",
          "original_form": "나는 바다요정쿠키도 좋아하고 달 타고다니는 애도 좋아해",
          "speaker_id": "2",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010221.1.10",
          "form": "너무 예쁘고 귀엽더라",
          "original_form": "너무 예쁘고 귀엽더라",
          "speaker_id": "1",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010221.1.11",
          "form": "아맞아 달빛술사",
          "original_form": "아맞아 달빛술사",
          "speaker_id": "2",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010221.1.12",
          "form": "맞아 걔가 달빛술사야",
          "original_form": "맞아 걔가 달빛술사야",
          "speaker_id": "1",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010221.1.13",
          "form": "맞아 그 효과가 정말 예쁘지 않니",
          "original_form": "맞아 그 효과가 정말 예쁘지 않니",
          "speaker_id": "2",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010221.1.14",
          "form": "나는 그전에는 벚꽃도 좋아했어",
          "original_form": "나는 그전에는 벚꽃도 좋아했어",
          "speaker_id": "2",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010221.1.15",
          "form": "요즘에 쿠키런 하니?",
          "original_form": "요즘에 쿠키런 하니?",
          "speaker_id": "2",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010221.1.16",
          "form": "벛꽃쿠키는 분홍색이고 너무 귀엽더라",
          "original_form": "벛꽃쿠키는 분홍색이고 너무 귀엽더라",
          "speaker_id": "1",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010221.1.17",
          "form": "아니 나 얼마전까지 쿠킹덤 했었어",
          "original_form": "아니 나 얼마전까지 쿠킹덤 했었어",
          "speaker_id": "1",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010221.1.18",
          "form": "쿠키런킹덤은 해본적있니?",
          "original_form": "쿠키런킹덤은 해본적있니?",
          "speaker_id": "1",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010221.1.19",
          "form": "나는 네가 말한김에 생각나서 조만간 다시 찾아볼 거 같아.",
          "original_form": "나는 네가 말한김에 생각나서 조만간 다시 찾아볼 거 같아.",
          "speaker_id": "2",
          "time": "20210823 16:31"
        },
        {
          "id": "MDRW2100010221.1.20",
          "form": "쿠킹덤은 해본적 없어",
          "original_form": "쿠킹덤은 해본적 없어",
          "speaker_id": "2",
          "time": "20210823 16:31"
        },
        {
          "id": "MDRW2100010221.1.21",
          "form": "그런 게엠은 즐기지 않는단다.",
          "original_form": "그런 게엠은 즐기지 않는단다.",
          "speaker_id": "2",
          "time": "20210823 16:31"
        },
        {
          "id": "MDRW2100010221.1.22",
          "form": "아직하니 그거?",
          "original_form": "아직하니 그거?",
          "speaker_id": "2",
          "time": "20210823 16:31"
        },
        {
          "id": "MDRW2100010221.1.23",
          "form": "지금은 안해!",
          "original_form": "지금은 안해!",
          "speaker_id": "1",
          "time": "20210823 16:47"
        },
        {
          "id": "MDRW2100010221.1.24",
          "form": "하지만 나중에 너도 한번 해보렴",
          "original_form": "하지만 나중에 너도 한번 해보렴",
          "speaker_id": "1",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100010221.1.25",
          "form": "왕국 꾸미기에 꽤 진심이 될 수도 있어",
          "original_form": "왕국 꾸미기에 꽤 진심이 될 수도 있어",
          "speaker_id": "1",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100010221.1.26",
          "form": "옛날에 내가하던 스타듀벨리라는 게임은 기억나니?",
          "original_form": "옛날에 내가하던 스타듀벨리라는 게임은 기억나니?",
          "speaker_id": "1",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100010221.1.27",
          "form": "기억나.",
          "original_form": "기억나.",
          "speaker_id": "2",
          "time": "20210823 17:14"
        },
        {
          "id": "MDRW2100010221.1.28",
          "form": "아무래도 나는 닌텐도 어서와요같은, 아니면 심슨마을?같은 게임은 나랑 안맞는듯해.",
          "original_form": "아무래도 나는 닌텐도 어서와요같은, 아니면 심슨마을?같은 게임은 나랑 안맞는듯해.",
          "speaker_id": "2",
          "time": "20210823 17:15"
        }
      ]
    }
