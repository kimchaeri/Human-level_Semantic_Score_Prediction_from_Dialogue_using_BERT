      "utterance": [
        {
          "id": "MDRW2100005093.1.1",
          "form": "새로 나온 프로그램인가요?",
          "original_form": "새로 나온 프로그램인가요?",
          "speaker_id": "2",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005093.1.2",
          "form": "잠시만요 검색해서 보고올게요",
          "original_form": "잠시만요 검색해서 보고올게요",
          "speaker_id": "2",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005093.1.3",
          "form": "지금 방영중인 예능인데",
          "original_form": "지금 방영중인 예능인데",
          "speaker_id": "1",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005093.1.4",
          "form": "이별 위기의 커플들이",
          "original_form": "이별 위기의 커플들이",
          "speaker_id": "1",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005093.1.5",
          "form": "짝을 바꿔서 데이트하는 예능이에요",
          "original_form": "짝을 바꿔서 데이트하는 예능이에요",
          "speaker_id": "1",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005093.1.6",
          "form": "헉 저 저거 예고? 봤어요",
          "original_form": "헉 저 저거 예고? 봤어요",
          "speaker_id": "2",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005093.1.7",
          "form": "매콤하던데..",
          "original_form": "매콤하던데..",
          "speaker_id": "2",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005093.1.8",
          "form": "환승연애도 그렇고 요새 전애인? 애인들이랑 나오는 프로가 많네요",
          "original_form": "환승연애도 그렇고 요새 전애인? 애인들이랑 나오는 프로가 많네요",
          "speaker_id": "2",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005093.1.9",
          "form": "근데 이개 환승연애보다 마라맛인거같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "근데 이개 환승연애보다 마라맛인거같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005093.1.10",
          "form": "저는 보면서 그냥",
          "original_form": "저는 보면서 그냥",
          "speaker_id": "1",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005093.1.11",
          "form": "이거 커플깨려고 작정했나..?",
          "original_form": "이거 커플깨려고 작정했나..?",
          "speaker_id": "1",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005093.1.12",
          "form": "그냥 헤어져.. 이생각 밖에 안하게되는",
          "original_form": "그냥 헤어져.. 이생각 밖에 안하게되는",
          "speaker_id": "1",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005093.1.13",
          "form": "다른 사람이랑 연애하면서 설래하는 장면들도 나오던데",
          "original_form": "다른 사람이랑 연애하면서 설래하는 장면들도 나오던데",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005093.1.14",
          "form": "만약 안헤어지고 나가서도 지금 방영하는 중애 헤어질거같아요 ㅋㅋㅋ",
          "original_form": "만약 안헤어지고 나가서도 지금 방영하는 중애 헤어질거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005093.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㄱ어우 설명만 들어도 자극적이네..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱ어우 설명만 들어도 자극적이네..",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005093.1.16",
          "form": "지금 촬영 끝났으면 인스타 같은곳에 계속 사귀고 있는지 아닌지 알 수도 있겠어요!",
          "original_form": "지금 촬영 끝났으면 인스타 같은곳에 계속 사귀고 있는지 아닌지 알 수도 있겠어요!",
          "speaker_id": "2",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005093.1.17",
          "form": "저는 근데 구애인이랑은 방송 못나올거같은..",
          "original_form": "저는 근데 구애인이랑은 방송 못나올거같은..",
          "speaker_id": "2",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005093.1.18",
          "form": "딱 일주일 합숙하는거거든요",
          "original_form": "딱 일주일 합숙하는거거든요",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005093.1.19",
          "form": "근데 32부작인가?",
          "original_form": "근데 32부작인가?",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005093.1.20",
          "form": "진짜 길어서 ㅋㅋㅋ 분량 제대로 뽑아먹는거 같더라고요",
          "original_form": "진짜 길어서 ㅋㅋㅋ 분량 제대로 뽑아먹는거 같더라고요",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005093.1.21",
          "form": "와우 32부작..? 뭔 하루종일 따라다닌거 편집없이 내보내는 수준이네요ㅠㅋㅋㅋㅋ",
          "original_form": "와우 32부작..? 뭔 하루종일 따라다닌거 편집없이 내보내는 수준이네요ㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005093.1.22",
          "form": "휴.. 구애인이랑? 진짜 끔찍하네요",
          "original_form": "휴.. 구애인이랑? 진짜 끔찍하네요",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005093.1.23",
          "form": "그래도 촬영을 제주도에서 하던데",
          "original_form": "그래도 촬영을 제주도에서 하던데",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005093.1.24",
          "form": "그건 너무 부러웠어요",
          "original_form": "그건 너무 부러웠어요",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005093.1.25",
          "form": "연락하기도 전에 차단박혔을듯한..구애인ㅎ",
          "original_form": "연락하기도 전에 차단박혔을듯한..구애인ㅎ",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005093.1.26",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005093.1.27",
          "form": "숙소도 좋죠?",
          "original_form": "숙소도 좋죠?",
          "speaker_id": "2",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005093.1.28",
          "form": "대박좋아요",
          "original_form": "대박좋아요",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005093.1.29",
          "form": "얼굴팔리지만 돈받고..제주도에서.. 구애인 만나기냐..",
          "original_form": "얼굴팔리지만 돈받고..제주도에서.. 구애인 만나기냐..",
          "speaker_id": "2",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005093.1.30",
