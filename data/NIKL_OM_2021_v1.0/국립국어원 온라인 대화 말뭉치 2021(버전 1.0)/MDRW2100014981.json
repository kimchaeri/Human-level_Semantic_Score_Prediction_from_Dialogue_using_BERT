        {
          "id": "MDRW2100014981.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014981.1.23",
          "form": "왜요 고모가 유가네 하시나요?",
          "original_form": "왜요 고모가 유가네 하시나요?",
          "speaker_id": "2",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014981.1.24",
          "form": "유가네가면",
          "original_form": "유가네가면",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014981.1.25",
          "form": "우동사리 추가",
          "original_form": "우동사리 추가",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014981.1.26",
          "form": "아니요 고모 가정주부세요",
          "original_form": "아니요 고모 가정주부세요",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014981.1.27",
          "form": "앗 제가 성급했네요",
