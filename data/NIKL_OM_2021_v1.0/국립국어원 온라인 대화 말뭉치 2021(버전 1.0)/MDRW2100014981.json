      "utterance": [
        {
          "id": "MDRW2100014981.1.1",
          "form": "저녁드셨나요",
          "original_form": "저녁드셨나요",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014981.1.2",
          "form": "네네 name1님도 드셨나요",
          "original_form": "네네 &name1&님도 드셨나요",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014981.1.3",
          "form": "어떤거 드셨죠",
          "original_form": "어떤거 드셨죠",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014981.1.4",
          "form": "전 닭갈비 먹었어요",
          "original_form": "전 닭갈비 먹었어요",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014981.1.5",
          "form": "거의 분위기 지금 취조",
          "original_form": "거의 분위기 지금 취조",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014981.1.6",
          "form": "뭐 드셨나요",
          "original_form": "뭐 드셨나요",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014981.1.7",
          "form": "닭갈비요? 닭갈비 사리 넣어드셨나요 그게 찐인데",
          "original_form": "닭갈비요? 닭갈비 사리 넣어드셨나요 그게 찐인데",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014981.1.8",
          "form": "저 숯불갈비 덮밥이요",
          "original_form": "저 숯불갈비 덮밥이요",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014981.1.9",
          "form": "아 안타깝게도 집에서 먹어서ㅈ사리가 없었어요",
          "original_form": "아 안타깝게도 집에서 먹어서ㅈ사리가 없었어요",
          "speaker_id": "1",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014981.1.10",
          "form": "숯불갈비 맛있었겠다",
          "original_form": "숯불갈비 맛있었겠다",
          "speaker_id": "1",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014981.1.11",
          "form": "잠만",
          "original_form": "잠만",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014981.1.12",
          "form": "만들어드셨나요",
          "original_form": "만들어드셨나요",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014981.1.13",
          "form": "!!!!",
          "original_form": "!!!!",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014981.1.14",
          "form": "당근 사드신줄알았는데",
          "original_form": "당근 사드신줄알았는데",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014981.1.15",
          "form": "만들어..드셨다..?",
          "original_form": "만들어..드셨다..?",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014981.1.16",
          "form": "고모가 보내준 냉동있어서 그거 해먹었어요",
          "original_form": "고모가 보내준 냉동있어서 그거 해먹었어요",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014981.1.17",
          "form": "사먹기엔 집에 있는 닭갈비가 아른거려.",
          "original_form": "사먹기엔 집에 있는 닭갈비가 아른거려.",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014981.1.18",
          "form": "사먹지 못했어요ㅋㅋㅋㄲㅋ",
          "original_form": "사먹지 못했어요ㅋㅋㅋㄲㅋ",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014981.1.19",
          "form": "유가네 좋아해요?",
          "original_form": "유가네 좋아해요?",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014981.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014981.1.21",
          "form": "헉",
          "original_form": "헉",
