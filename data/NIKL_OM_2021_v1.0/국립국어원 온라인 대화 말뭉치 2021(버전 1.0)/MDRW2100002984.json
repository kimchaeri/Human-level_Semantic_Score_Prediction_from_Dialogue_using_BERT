      "utterance": [
        {
          "id": "MDRW2100002984.1.1",
          "form": "오늘도 숙제 열심히 했지?",
          "original_form": "오늘도 숙제 열심히 했지?",
          "speaker_id": "2",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002984.1.2",
          "form": "그럼그럼",
          "original_form": "그럼그럼",
          "speaker_id": "1",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002984.1.3",
          "form": "저녁에 회랑 라면 먹으니 좋아하겠네 ㅎㅎ",
          "original_form": "저녁에 회랑 라면 먹으니 좋아하겠네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002984.1.4",
          "form": "그렇지 ㅎㅎ",
          "original_form": "그렇지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002984.1.5",
          "form": "방학동안 열심히 했으니 주말엔 좀 쉬게 해줘야겠네",
          "original_form": "방학동안 열심히 했으니 주말엔 좀 쉬게 해줘야겠네",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002984.1.6",
          "form": "ㅎㅎ 매일 열심히 해야 실력이 늘지",
          "original_form": "ㅎㅎ 매일 열심히 해야 실력이 늘지",
          "speaker_id": "1",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002984.1.7",
          "form": "쉬는 시간도 필요해 ㅎ",
          "original_form": "쉬는 시간도 필요해 ㅎ",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002984.1.8",
          "form": "그건 그런데 매일 해야 하는 것들도 있으니깐",
          "original_form": "그건 그런데 매일 해야 하는 것들도 있으니깐",
          "speaker_id": "1",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002984.1.9",
          "form": "ㅎㅎ 하루정도는 쉬어도 되지",
          "original_form": "ㅎㅎ 하루정도는 쉬어도 되지",
          "speaker_id": "2",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002984.1.10",
          "form": "ㅇㅇ 알았옹 ㅎㅎ",
          "original_form": "ㅇㅇ 알았옹 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002984.1.11",
          "form": "저녁에 해리포터 보고 싶댕",
          "original_form": "저녁에 해리포터 보고 싶댕",
          "speaker_id": "1",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002984.1.12",
          "form": "ㅎㅎ 작은 아들?",
          "original_form": "ㅎㅎ 작은 아들?",
          "speaker_id": "2",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002984.1.13",
          "form": "ㅇㅇ요즘 해리포터에 푹 빠졌엉 ㅋㅋ",
          "original_form": "ㅇㅇ요즘 해리포터에 푹 빠졌엉 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002984.1.14",
          "form": "ㅎㅎ 보여줘",
          "original_form": "ㅎㅎ 보여줘",
          "speaker_id": "2",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002984.1.15",
          "form": "애들은 개학 못하고 온라인 수업 할 것 같아",
