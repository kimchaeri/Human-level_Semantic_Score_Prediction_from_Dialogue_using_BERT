      "utterance": [
        {
          "id": "MDRW2100027656.1.1",
          "form": "즐겨보는 프로그램 있으세요?",
          "original_form": "즐겨보는 프로그램 있으세요?",
          "speaker_id": "1",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027656.1.2",
          "form": "요즘 드라마봐요\n갯마을차차차",
          "original_form": "요즘 드라마봐요\n갯마을차차차",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027656.1.3",
          "form": "저는 재미나게 본게 보쌈이였어요 지금은 끝났지만요",
          "original_form": "저는 재미나게 본게 보쌈이였어요 지금은 끝났지만요",
          "speaker_id": "1",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027656.1.4",
          "form": "그게뭔가요",
          "original_form": "그게뭔가요",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027656.1.5",
          "form": "드라마예요 tvn에서 했었어요",
          "original_form": "드라마예요 tvn에서 했었어요",
          "speaker_id": "1",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027656.1.6",
          "form": "정말요? 언제한거에요?",
          "original_form": "정말요? 언제한거에요?",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027656.1.7",
          "form": "이미 종영했는데 3월달인가했나 가물가물하네요",
          "original_form": "이미 종영했는데 3월달인가했나 가물가물하네요",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027656.1.8",
          "form": "엄마랑 같이 재미나게 봤었어요 사극이였어요",
          "original_form": "엄마랑 같이 재미나게 봤었어요 사극이였어요",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027656.1.9",
          "form": "아 사극이요 주인공에 누구였죠",
          "original_form": "아 사극이요 주인공에 누구였죠",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027656.1.10",
          "form": "정일우씨였어요 여자주인공은 유리였어요",
          "original_form": "정일우씨였어요 여자주인공은 유리였어요",
          "speaker_id": "1",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027656.1.11",
          "form": "소녀시대라고 그 멤버중 하나인데 연기를 엄청 잘했어요",
          "original_form": "소녀시대라고 그 멤버중 하나인데 연기를 엄청 잘했어요",
          "speaker_id": "1",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027656.1.12",
          "form": "아 네 지나가다 봤어요",
          "original_form": "아 네 지나가다 봤어요",
          "speaker_id": "2",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027656.1.13",
          "form": "넷플에서 봐야겠어요",
          "original_form": "넷플에서 봐야겠어요",
          "speaker_id": "2",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027656.1.14",
          "form": "넷플릭스에 있는지는 모르겠네요",
          "original_form": "넷플릭스에 있는지는 모르겠네요",
          "speaker_id": "1",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027656.1.15",
          "form": "티비엔은 거의있긴하던데..",
          "original_form": "티비엔은 거의있긴하던데..",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027656.1.16",
          "form": "넷플 보시는군요",
          "original_form": "넷플 보시는군요",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027656.1.17",
          "form": "유재석은 정말대단한거같아요 나오는프로 다 잼나요",
          "original_form": "유재석은 정말대단한거같아요 나오는프로 다 잼나요",
          "speaker_id": "2",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027656.1.18",
          "form": "넷플에서 주로 무엇을 보시나요?",
          "original_form": "넷플에서 주로 무엇을 보시나요?",
