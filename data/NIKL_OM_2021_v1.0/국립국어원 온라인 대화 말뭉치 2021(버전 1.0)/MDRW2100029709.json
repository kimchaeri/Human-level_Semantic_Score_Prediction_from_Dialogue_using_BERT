      "utterance": [
        {
          "id": "MDRW2100029709.1.1",
          "form": "안녕하세요~!",
          "original_form": "안녕하세요~!",
          "speaker_id": "1",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100029709.1.2",
          "form": "sns 많이 하세요?",
          "original_form": "sns 많이 하세요?",
          "speaker_id": "2",
          "time": "20210903 12:10"
        },
        {
          "id": "MDRW2100029709.1.3",
          "form": "아뇨! 자주는 안하는데 최근에 인스타 시작했어요",
          "original_form": "아뇨! 자주는 안하는데 최근에 인스타 시작했어요",
          "speaker_id": "1",
          "time": "20210903 12:20"
        },
        {
          "id": "MDRW2100029709.1.4",
          "form": "인스타로 뭐하세요? 일상용?",
          "original_form": "인스타로 뭐하세요? 일상용?",
          "speaker_id": "2",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100029709.1.5",
          "form": "아직 낯설어서 따로 활동한적은 없고 친구들 스토리 눈팅만 해요",
          "original_form": "아직 낯설어서 따로 활동한적은 없고 친구들 스토리 눈팅만 해요",
          "speaker_id": "1",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029709.1.6",
          "form": "ㅋㄴ님은 에센에스 하시나요!",
          "original_form": "ㅋㄴ님은 에센에스 하시나요!",
          "speaker_id": "1",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029709.1.7",
          "form": "전 트위터랑 인스타 해요",
          "original_form": "전 트위터랑 인스타 해요",
          "speaker_id": "2",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100029709.1.8",
          "form": "헉 그렇구나 트위터 재밌나요?",
          "original_form": "헉 그렇구나 트위터 재밌나요?",
          "speaker_id": "1",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100029709.1.9",
          "form": "재밌어요ㅋㅋ",
          "original_form": "재밌어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:47"
        },
        {
          "id": "MDRW2100029709.1.10",
          "form": "같은거 좋아하는 사람끼리 모여서요",
          "original_form": "같은거 좋아하는 사람끼리 모여서요",
          "speaker_id": "2",
          "time": "20210903 12:47"
        },
        {
          "id": "MDRW2100029709.1.11",
          "form": "다음에 한번 해보고싶네요",
          "original_form": "다음에 한번 해보고싶네요",
          "speaker_id": "1",
          "time": "20210903 13:33"
        },
        {
          "id": "MDRW2100029709.1.12",
          "form": "일단 인스타먼저 적응하고ㅠ",
          "original_form": "일단 인스타먼저 적응하고ㅠ",
          "speaker_id": "1",
          "time": "20210903 13:33"
        },
        {
          "id": "MDRW2100029709.1.13",
          "form": "sns 너무어려워요",
          "original_form": "sns 너무어려워요",
          "speaker_id": "1",
          "time": "20210903 13:34"
        },
        {
          "id": "MDRW2100029709.1.14",
          "form": "처음 하시면 적응하기 좀 힘들죠ㅠ",
          "original_form": "처음 하시면 적응하기 좀 힘들죠ㅠ",
          "speaker_id": "2",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100029709.1.15",
          "form": "그래도 하나 잘하시면 대부분 비슷해서 다 쉽게 하실거예요",
          "original_form": "그래도 하나 잘하시면 대부분 비슷해서 다 쉽게 하실거예요",
          "speaker_id": "2",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100029709.1.16",
          "form": "ㅋㅋㅋ좋아요 열심히 함해볼게요",
          "original_form": "ㅋㅋㅋ좋아요 열심히 함해볼게요",
          "speaker_id": "1",
          "time": "20210903 15:58"
        },
        {
          "id": "MDRW2100029709.1.17",
          "form": "트위터 시작하시고 주제 겹치면 맞팔해요",
          "original_form": "트위터 시작하시고 주제 겹치면 맞팔해요",
          "speaker_id": "2",
          "time": "20210903 16:03"
        },
        {
          "id": "MDRW2100029709.1.18",
          "form": "와 좋아요 관심있으신 분야?가 어디쪽이신데요?",
          "original_form": "와 좋아요 관심있으신 분야?가 어디쪽이신데요?",
          "speaker_id": "1",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100029709.1.19",
          "form": "아이돌 덕질용 계정 있고",
          "original_form": "아이돌 덕질용 계정 있고",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029709.1.20",
          "form": "이스포츠 응원하는 계정도 있어요",
          "original_form": "이스포츠 응원하는 계정도 있어요",
          "speaker_id": "2",
          "time": "20210903 16:05"
        }
