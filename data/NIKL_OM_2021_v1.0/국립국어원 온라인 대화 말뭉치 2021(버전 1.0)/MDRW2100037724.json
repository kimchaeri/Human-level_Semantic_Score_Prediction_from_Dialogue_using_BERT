      "utterance": [
        {
          "id": "MDRW2100037724.1.1",
          "form": "오늘 저녁은 뭐 먹노??",
          "original_form": "오늘 저녁은 뭐 먹노??",
          "speaker_id": "2",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037724.1.2",
          "form": "그러게 언니 ㅋㅋ 땡기는게없다ㅋ",
          "original_form": "그러게 언니 ㅋㅋ 땡기는게없다ㅋ",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100037724.1.3",
          "form": "우짜지ㅋㅋ",
          "original_form": "우짜지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100037724.1.4",
          "form": "언니는,?",
          "original_form": "언니는,?",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037724.1.5",
          "form": "우리는 어제 먹다 남은 삼계탕",
          "original_form": "우리는 어제 먹다 남은 삼계탕",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037724.1.6",
          "form": "생선구워서 줄려고",
          "original_form": "생선구워서 줄려고",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037724.1.7",
          "form": "오 반찬좋구만ㅋㅋ",
          "original_form": "오 반찬좋구만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037724.1.8",
          "form": "나는 곰국있는데 솔직히 내가 안땡기네ㅋㅋ",
          "original_form": "나는 곰국있는데 솔직히 내가 안땡기네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037724.1.9",
          "form": "오늘 신랑 늦는다해서",
          "original_form": "오늘 신랑 늦는다해서",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037724.1.10",
          "form": "애들 위주로",
          "original_form": "애들 위주로",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037724.1.11",
          "form": "해주려고",
          "original_form": "해주려고",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037724.1.12",
          "form": "우리도 오늘은 신랑 늦는다해서",
          "original_form": "우리도 오늘은 신랑 늦는다해서",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100037724.1.13",
          "form": "애들만 줘야지",
          "original_form": "애들만 줘야지",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100037724.1.14",
          "form": "오오 좋구만",
          "original_form": "오오 좋구만",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100037724.1.15",
          "form": "일찍재우려고",
          "original_form": "일찍재우려고",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100037724.1.16",
          "form": "오늘은ㅋ",
          "original_form": "오늘은ㅋ",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100037724.1.17",
          "form": "와 어제 11시에자니깐 피곤ㅋ",
          "original_form": "와 어제 11시에자니깐 피곤ㅋ",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100037724.1.18",
          "form": "그래 같이 일찍 육퇴해보자",
          "original_form": "그래 같이 일찍 육퇴해보자",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100037724.1.19",
          "form": "나도 눈에 뭐 날라는지 눈이 뻐근하네",
          "original_form": "나도 눈에 뭐 날라는지 눈이 뻐근하네",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100037724.1.20",
          "form": "아고 피곤해서 카나보다ㅜㅜ",
          "original_form": "아고 피곤해서 카나보다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100037724.1.21",
          "form": "엉 난 피곤하면 눈에 나타남",
          "original_form": "엉 난 피곤하면 눈에 나타남",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100037724.1.22",
          "form": "생성 굽고 있다",
          "original_form": "생성 굽고 있다",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100037724.1.23",
          "form": "생선",
          "original_form": "생선",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100037724.1.24",
          "form": "맞다맞어ㅋㅋ",
          "original_form": "맞다맞어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100037724.1.25",
          "form": "생선좋다ㅋ",
          "original_form": "생선좋다ㅋ",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100037724.1.26",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100037724.1.27",
          "form": "빨리 저녁 먹고",
          "original_form": "빨리 저녁 먹고",
          "speaker_id": "2",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100037724.1.28",
          "form": "일찍 퇴근하자",
          "original_form": "일찍 퇴근하자",
          "speaker_id": "2",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100037724.1.29",
          "form": "언니 홧팅ㅋㅋ",
          "original_form": "언니 홧팅ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100037724.1.30",
