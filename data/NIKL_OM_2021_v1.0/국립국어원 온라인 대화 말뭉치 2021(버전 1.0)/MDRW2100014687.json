      "utterance": [
        {
          "id": "MDRW2100014687.1.1",
          "form": "싱크홀 재밌게봤어요",
          "original_form": "싱크홀 재밌게봤어요",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100014687.1.2",
          "form": "누두누구 나와요?",
          "original_form": "누두누구 나와요?",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100014687.1.3",
          "form": "차승원 이광수 김성균 나와요ㅎ 코메디입니다",
          "original_form": "차승원 이광수 김성균 나와요ㅎ 코메디입니다",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100014687.1.4",
          "form": "아 코메디예요?",
          "original_form": "아 코메디예요?",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100014687.1.5",
          "form": "재난물이라 코메디일 줄 생각도 못했어요ㅋㅋ",
          "original_form": "재난물이라 코메디일 줄 생각도 못했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100014687.1.6",
          "form": "코메디면 극장 가서 볼만하죠 관객분들 반응도 괜찮던가요?",
          "original_form": "코메디면 극장 가서 볼만하죠 관객분들 반응도 괜찮던가요?",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100014687.1.7",
          "form": "네 불편한 장면 없이 가볍게 보기좋았어요",
          "original_form": "네 불편한 장면 없이 가볍게 보기좋았어요",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100014687.1.8",
          "form": "와 관객동원도 꽤 했네요",
          "original_form": "와 관객동원도 꽤 했네요",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100014687.1.9",
          "form": "저는 여름이라 음악영화가 보고싶은데 요즘에 개봉한 게 마땅히 없네요ㅋㅋ",
          "original_form": "저는 여름이라 음악영화가 보고싶은데 요즘에 개봉한 게 마땅히 없네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100014687.1.10",
          "form": "광복절연휴였나 그게있어서 관객동원잘된것같더라구요",
          "original_form": "광복절연휴였나 그게있어서 관객동원잘된것같더라구요",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100014687.1.11",
          "form": "이미 본 영화중에 괜찮은거 있음 추천해주세요",
          "original_form": "이미 본 영화중에 괜찮은거 있음 추천해주세요",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100014687.1.12",
          "form": "최근에는 모가디슈요",
          "original_form": "최근에는 모가디슈요",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100014687.1.13",
          "form": "안정적으로 재밌게봤어요",
          "original_form": "안정적으로 재밌게봤어요",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100014687.1.14",
          "form": "근데 요즘에 보자면 실제상황이 오버랩될 것 같아요ㅠㅠ",
          "original_form": "근데 요즘에 보자면 실제상황이 오버랩될 것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100014687.1.15",
          "form": "모가디슈도 히트쳤더라구요",
          "original_form": "모가디슈도 히트쳤더라구요",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100014687.1.16",
          "form": "줄거리는 모르는데 그런영화군요",
          "original_form": "줄거리는 모르는데 그런영화군요",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100014687.1.17",
          "form": "네 내전 일어난 나라에서 탈출하는 영화인데요",
          "original_form": "네 내전 일어난 나라에서 탈출하는 영화인데요",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100014687.1.18",
          "form": "아프가니스탄 상황이랑 공교롭게 겹치는 부분이 있네요",
          "original_form": "아프가니스탄 상황이랑 공교롭게 겹치는 부분이 있네요",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100014687.1.19",
          "form": "배우는 누가나오나요",
          "original_form": "배우는 누가나오나요",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100014687.1.20",
          "form": "조인성 김윤석 허준호요!",
          "original_form": "조인성 김윤석 허준호요!",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100014687.1.21",
          "form": "+류승완 감독",
          "original_form": "+류승완 감독",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100014687.1.22",
          "form": "잘 될 수밖에 없는 조합이었네요ㅋㅋ",
          "original_form": "잘 될 수밖에 없는 조합이었네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100014687.1.23",
          "form": "아~류승완이 완전 각잡고 만든거라던데",
          "original_form": "아~류승완이 완전 각잡고 만든거라던데",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100014687.1.24",
          "form": "보증된 영화네요",
          "original_form": "보증된 영화네요",
          "speaker_id": "2",
          "time": "20210825 22:29"
        }
      ]
    }
  ]
