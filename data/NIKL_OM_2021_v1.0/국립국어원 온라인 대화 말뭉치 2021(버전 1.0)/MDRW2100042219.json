      "utterance": [
        {
          "id": "MDRW2100042219.1.1",
          "form": "그럼 아침은 안먹고 나가시는 건가요",
          "original_form": "그럼 아침은 안먹고 나가시는 건가요",
          "speaker_id": "1",
          "time": "20210906 20:41"
        },
        {
          "id": "MDRW2100042219.1.2",
          "form": "아니면 간단히라도 먹는건가요?",
          "original_form": "아니면 간단히라도 먹는건가요?",
          "speaker_id": "1",
          "time": "20210906 20:41"
        },
        {
          "id": "MDRW2100042219.1.3",
          "form": "간단히 먹을 빵이나 야채 과일있으면 먹고 출근해요!",
          "original_form": "간단히 먹을 빵이나 야채 과일있으면 먹고 출근해요!",
          "speaker_id": "2",
          "time": "20210907 07:19"
        },
        {
          "id": "MDRW2100042219.1.4",
          "form": "사실집안일 쉬운것만골라하고 있네요",
          "original_form": "사실집안일 쉬운것만골라하고 있네요",
          "speaker_id": "2",
          "time": "20210907 07:19"
        },
        {
          "id": "MDRW2100042219.1.5",
          "form": "집안일은 해도해도 티가 안나고 매일 같은 일 반복하는거라 사실 쉬운 집안일이란 없는 거 같아요",
          "original_form": "집안일은 해도해도 티가 안나고 매일 같은 일 반복하는거라 사실 쉬운 집안일이란 없는 거 같아요",
          "speaker_id": "1",
          "time": "20210907 08:27"
        },
        {
          "id": "MDRW2100042219.1.6",
          "form": "그쵸 진짜 일상이긴하죠",
          "original_form": "그쵸 진짜 일상이긴하죠",
          "speaker_id": "2",
          "time": "20210907 08:28"
        },
        {
          "id": "MDRW2100042219.1.7",
          "form": "청소랑 음식이 제일 힘든것같아요",
          "original_form": "청소랑 음식이 제일 힘든것같아요",
          "speaker_id": "2",
          "time": "20210907 08:29"
        },
        {
          "id": "MDRW2100042219.1.8",
          "form": "육아가 최상급이겠죠",
          "original_form": "육아가 최상급이겠죠",
          "speaker_id": "2",
          "time": "20210907 08:29"
        },
        {
          "id": "MDRW2100042219.1.9",
          "form": "육아는 집안일과 따로  봐야 한다고 생각해요",
          "original_form": "육아는 집안일과 따로  봐야 한다고 생각해요",
          "speaker_id": "1",
          "time": "20210907 08:30"
        },
        {
          "id": "MDRW2100042219.1.10",
          "form": "진짜 육아는 참 힘들죠",
          "original_form": "진짜 육아는 참 힘들죠",
          "speaker_id": "1",
          "time": "20210907 08:30"
        },
        {
          "id": "MDRW2100042219.1.11",
          "form": "다들 그러더라고요ㅠ 육아도 젊을때 체력 좋을때하는게 좋은것같아요",
          "original_form": "다들 그러더라고요ㅠ 육아도 젊을때 체력 좋을때하는게 좋은것같아요",
          "speaker_id": "2",
          "time": "20210907 08:34"
        },
        {
          "id": "MDRW2100042219.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210907 08:36"
        },
        {
          "id": "MDRW2100042219.1.13",
          "form": "아이들의 에너지는 참 따라잡기 힘들죠",
          "original_form": "아이들의 에너지는 참 따라잡기 힘들죠",
          "speaker_id": "1",
          "time": "20210907 08:36"
        },
        {
          "id": "MDRW2100042219.1.14",
          "form": "그래도 힘든만큼 보람있지않아요?",
          "original_form": "그래도 힘든만큼 보람있지않아요?",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042219.1.15",
          "form": "어떠세요",
          "original_form": "어떠세요",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042219.1.16",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042219.1.17",
          "form": "요즘 딩크족도 많잖아요",
          "original_form": "요즘 딩크족도 많잖아요",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042219.1.18",
          "form": "그 보람이 힘듦을 이기죠",
          "original_form": "그 보람이 힘듦을 이기죠",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042219.1.19",
          "form": "너무 힘든데 너무 사랑스러워서 얘없었으면 어떘을까 싶죠",
          "original_form": "너무 힘든데 너무 사랑스러워서 얘없었으면 어떘을까 싶죠",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042219.1.20",
          "form": "그 맛에 애기들 낳고 하나봐요",
          "original_form": "그 맛에 애기들 낳고 하나봐요",
          "speaker_id": "2",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042219.1.21",
          "form": "저도 회사에 아이있으신분들많은데",
          "original_form": "저도 회사에 아이있으신분들많은데",
          "speaker_id": "2",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042219.1.22",
          "form": "확실히 대화주제가",
          "original_form": "확실히 대화주제가",
          "speaker_id": "2",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042219.1.23",
          "form": "주로 아이들얘기하세요",
          "original_form": "주로 아이들얘기하세요",
          "speaker_id": "2",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042219.1.24",
          "form": "어쩔 수 없어요 최고의 관심사거든요",
          "original_form": "어쩔 수 없어요 최고의 관심사거든요",
          "speaker_id": "1",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042219.1.25",
          "form": "그렇지만 딩크는 딩크대로 만족스럴운 삶을 살수 있다고 봐요",
          "original_form": "그렇지만 딩크는 딩크대로 만족스럴운 삶을 살수 있다고 봐요",
          "speaker_id": "1",
          "time": "20210907 09:51"
        },
        {
          "id": "MDRW2100042219.1.26",
          "form": "전 딸이 결혼해도 아이 안낳고 살았으면 좋겠어요",
          "original_form": "전 딸이 결혼해도 아이 안낳고 살았으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210907 09:51"
        },
        {
          "id": "MDRW2100042219.1.27",
          "form": "그쵸 특히나 어리면 더 그렇죠",
          "original_form": "그쵸 특히나 어리면 더 그렇죠",
          "speaker_id": "2",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042219.1.28",
          "form": "왜 그렇게 생각하세요?ㅎㅎ또 애 다키우면 다르지않을까요",
          "original_form": "왜 그렇게 생각하세요?ㅎㅎ또 애 다키우면 다르지않을까요",
          "speaker_id": "2",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042219.1.29",
          "form": "자기 밥벌이할때가되면요",
          "original_form": "자기 밥벌이할때가되면요",
          "speaker_id": "2",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042219.1.30",
          "form": "딸이라서 그런가봐요",
          "original_form": "딸이라서 그런가봐요",
          "speaker_id": "1",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100042219.1.31",
          "form": "제가 딸만 둬서",
          "original_form": "제가 딸만 둬서",
          "speaker_id": "1",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100042219.1.32",
          "form": "아들이면 장가가서 손주 낳길 바랬을지도요 ㅎㅎㅎ",
          "original_form": "아들이면 장가가서 손주 낳길 바랬을지도요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100042219.1.33",
          "form": "맞아요 그랬을지도 몰라요",
          "original_form": "맞아요 그랬을지도 몰라요",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100042219.1.34",
          "form": "근데 자식이 주는 힘이 대단하긴한것같아요",
          "original_form": "근데 자식이 주는 힘이 대단하긴한것같아요",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
