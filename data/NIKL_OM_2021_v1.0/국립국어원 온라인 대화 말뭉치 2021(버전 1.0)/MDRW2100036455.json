      "utterance": [
        {
          "id": "MDRW2100036455.1.1",
          "form": "음 이미 줬는데...",
          "original_form": "음 이미 줬는데...",
          "speaker_id": "2",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100036455.1.2",
          "form": "너도 누가 집등이하는지 아는거야?",
          "original_form": "너도 누가 집등이하는지 아는거야?",
          "speaker_id": "2",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100036455.1.3",
          "form": "응 니가 저번에 얘기해줬어",
          "original_form": "응 니가 저번에 얘기해줬어",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036455.1.4",
          "form": "집들이 선물로 휴지가 제일 무난하지?",
          "original_form": "집들이 선물로 휴지가 제일 무난하지?",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036455.1.5",
          "form": "그친구 완전 자취의 고수라며",
          "original_form": "그친구 완전 자취의 고수라며",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036455.1.6",
          "form": "식용유나 올리브유같은건 어때?",
          "original_form": "식용유나 올리브유같은건 어때?",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036455.1.7",
          "form": "아 정말? 아냐",
          "original_form": "아 정말? 아냐",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036455.1.8",
          "form": "응응 아무래도 언젠간 쓸 꺼니까",
          "original_form": "응응 아무래도 언젠간 쓸 꺼니까",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036455.1.9",
          "form": "그친구는 자취핵초보라...",
          "original_form": "그친구는 자취핵초보라...",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036455.1.10",
          "form": "다 알려줘야해",
          "original_form": "다 알려줘야해",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036455.1.11",
          "form": "식용유도 너무 좋지 요리할때 기본이잖아??",
          "original_form": "식용유도 너무 좋지 요리할때 기본이잖아??",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036455.1.12",
          "form": "청소용품을 많이 사는 친구야",
          "original_form": "청소용품을 많이 사는 친구야",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036455.1.13",
          "form": "대체 청소를 언제하는진 모르겠지만",
          "original_form": "대체 청소를 언제하는진 모르겠지만",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036455.1.14",
          "form": "그럼 청소용품을 선물로 주는 건 어때??",
          "original_form": "그럼 청소용품을 선물로 주는 건 어때??",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036455.1.15",
          "form": "일단 용품은 많이 사더라!!",
          "original_form": "일단 용품은 많이 사더라!!",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036455.1.16",
          "form": "이 미 줬 다 구 .",
          "original_form": "이 미 줬 다 구 .",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100036455.1.17",
          "form": "나는 휴지를 줬어",
          "original_form": "나는 휴지를 줬어",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100036455.1.18",
          "form": "미용티슈 부드러울 거 같아.",
          "original_form": "미용티슈 부드러울 거 같아.",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100036455.1.19",
          "form": "또주는것도 친구를 기쁘게 항 것 같아",
          "original_form": "또주는것도 친구를 기쁘게 항 것 같아",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100036455.1.20",
          "form": "휴지값이 금값이더라",
          "original_form": "휴지값이 금값이더라",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100036455.1.21",
          "form": "너 돈 좀 있니?",
          "original_form": "너 돈 좀 있니?",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100036455.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ정말?? 몰랐어 ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ정말?? 몰랐어 ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100036455.1.23",
          "form": "어라",
          "original_form": "어라",
          "speaker_id": "2",
