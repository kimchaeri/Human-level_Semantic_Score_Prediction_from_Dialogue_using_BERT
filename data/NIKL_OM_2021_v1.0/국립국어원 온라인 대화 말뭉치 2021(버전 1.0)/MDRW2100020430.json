      "utterance": [
        {
          "id": "MDRW2100020430.1.1",
          "form": "어제 뭐 드셨어요?",
          "original_form": "어제 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 05:25"
        },
        {
          "id": "MDRW2100020430.1.2",
          "form": "저 어제 식당 메뉴가 너무 맘에 들었는데",
          "original_form": "저 어제 식당 메뉴가 너무 맘에 들었는데",
          "speaker_id": "2",
          "time": "20210828 05:26"
        },
        {
          "id": "MDRW2100020430.1.3",
          "form": "반각 오븐구이라고",
          "original_form": "반각 오븐구이라고",
          "speaker_id": "2",
          "time": "20210828 05:26"
        },
        {
          "id": "MDRW2100020430.1.4",
          "form": "오 식당 이름이 뭐에용?",
          "original_form": "오 식당 이름이 뭐에용?",
          "speaker_id": "1",
          "time": "20210828 05:26"
        },
        {
          "id": "MDRW2100020430.1.5",
          "form": "반각이 뭐에용?",
          "original_form": "반각이 뭐에용?",
          "speaker_id": "1",
          "time": "20210828 05:26"
        },
        {
          "id": "MDRW2100020430.1.6",
          "form": "닭 반마리 오븐에 구워서 ㅋㅋ",
          "original_form": "닭 반마리 오븐에 구워서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:26"
        },
        {
          "id": "MDRW2100020430.1.7",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210828 05:26"
        },
        {
          "id": "MDRW2100020430.1.8",
          "form": "중식인가용?",
          "original_form": "중식인가용?",
          "speaker_id": "1",
          "time": "20210828 05:26"
        },
        {
          "id": "MDRW2100020430.1.9",
          "form": "식당 그냥 직장 식당이요",
          "original_form": "식당 그냥 직장 식당이요",
          "speaker_id": "2",
          "time": "20210828 05:26"
        },
        {
          "id": "MDRW2100020430.1.10",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210828 05:26"
        },
        {
          "id": "MDRW2100020430.1.11",
          "form": "옛날 통닭 느낌이었어요",
          "original_form": "옛날 통닭 느낌이었어요",
          "speaker_id": "2",
          "time": "20210828 05:26"
        },
        {
          "id": "MDRW2100020430.1.12",
          "form": "반각 오븐구이",
          "original_form": "반각 오븐구이",
          "speaker_id": "1",
          "time": "20210828 05:26"
        },
        {
          "id": "MDRW2100020430.1.13",
          "form": "와 맛있겠다",
          "original_form": "와 맛있겠다",
          "speaker_id": "1",
          "time": "20210828 05:27"
        },
        {
          "id": "MDRW2100020430.1.14",
          "form": "비올때 옛날통닭 먹으면 엄청 맛있어요",
          "original_form": "비올때 옛날통닭 먹으면 엄청 맛있어요",
          "speaker_id": "1",
          "time": "20210828 05:27"
        },
        {
          "id": "MDRW2100020430.1.15",
          "form": "엄청 커다래서 하루종일 배불렀어요",
          "original_form": "엄청 커다래서 하루종일 배불렀어요",
          "speaker_id": "2",
          "time": "20210828 05:27"
        },
        {
          "id": "MDRW2100020430.1.16",
          "form": "근데 집에서 치킨 시켜먹자해서",
          "original_form": "근데 집에서 치킨 시켜먹자해서",
          "speaker_id": "2",
          "time": "20210828 05:27"
        },
        {
          "id": "MDRW2100020430.1.17",
          "form": "우와ㅠㅠ 푸짐했었나봐요",
          "original_form": "우와ㅠㅠ 푸짐했었나봐요",
          "speaker_id": "1",
          "time": "20210828 05:27"
        },
        {
          "id": "MDRW2100020430.1.18",
          "form": "닭 물려서 전 치킨을 안먹었어요 ㅋㅋㅋ",
          "original_form": "닭 물려서 전 치킨을 안먹었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:27"
        },
        {
          "id": "MDRW2100020430.1.19",
          "form": "치킨 무슨치킨 좋아하세요?",
          "original_form": "치킨 무슨치킨 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 05:27"
        },
        {
          "id": "MDRW2100020430.1.20",
          "form": "아ㅋㅋㅋㅋ 그렇죠 하루종일 닭은 좀",
          "original_form": "아ㅋㅋㅋㅋ 그렇죠 하루종일 닭은 좀",
          "speaker_id": "1",
          "time": "20210828 05:27"
        },
        {
          "id": "MDRW2100020430.1.21",
          "form": "교촌 허니콤보요",
          "original_form": "교촌 허니콤보요",
          "speaker_id": "2",
          "time": "20210828 05:27"
        },
        {
          "id": "MDRW2100020430.1.22",
          "form": "저도 허니콤보 좋아해용",
          "original_form": "저도 허니콤보 좋아해용",
          "speaker_id": "1",
          "time": "20210828 05:28"
        },
        {
          "id": "MDRW2100020430.1.23",
          "form": "닭이 작아서 좀 슬프지만...ㅋㅋ",
          "original_form": "닭이 작아서 좀 슬프지만...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:28"
        }
      ]
