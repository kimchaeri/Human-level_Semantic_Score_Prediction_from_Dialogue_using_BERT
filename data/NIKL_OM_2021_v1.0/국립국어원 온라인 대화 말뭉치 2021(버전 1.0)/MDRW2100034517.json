      "utterance": [
        {
          "id": "MDRW2100034517.1.1",
          "form": "응~여자가 애 낳기싫어해~",
          "original_form": "응~여자가 애 낳기싫어해~",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034517.1.2",
          "form": "신랑 회사동생은 낳고싶어하는데~",
          "original_form": "신랑 회사동생은 낳고싶어하는데~",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034517.1.3",
          "form": "애는 지날수록 생각이 바뀌니깐. 나중에 그 대리석바닥이",
          "original_form": "애는 지날수록 생각이 바뀌니깐. 나중에 그 대리석바닥이",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034517.1.4",
          "form": "매트바닥이될수있다",
          "original_form": "매트바닥이될수있다",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034517.1.5",
          "form": "집도 그 여자가",
          "original_form": "집도 그 여자가",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034517.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034517.1.7",
          "form": "자기 친구들은 다 대구에 사는데 ㅋㅋㅋ",
          "original_form": "자기 친구들은 다 대구에 사는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034517.1.8",
          "form": "이렇게 또ㅋㅋ",
          "original_form": "이렇게 또ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034517.1.9",
          "form": "밀리기 싫다고",
          "original_form": "밀리기 싫다고",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034517.1.10",
          "form": "리모델링이라도 해야된다고;;",
          "original_form": "리모델링이라도 해야된다고;;",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034517.1.11",
          "form": "허걱 ㅋㅋㅋ밀리기싫대",
          "original_form": "허걱 ㅋㅋㅋ밀리기싫대",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034517.1.12",
          "form": "웃겨ㅋㅋ",
          "original_form": "웃겨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034517.1.13",
          "form": "신랑이 4천이면 차를사지 캄서 ㅋㅋㅋ",
          "original_form": "신랑이 4천이면 차를사지 캄서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100034517.1.14",
          "form": "여자 좀 독특하더라..",
          "original_form": "여자 좀 독특하더라..",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100034517.1.15",
          "form": "ㅋㅋㅋ그러게 ㅋ 자존심이 꽤 쎈가보다",
          "original_form": "ㅋㅋㅋ그러게 ㅋ 자존심이 꽤 쎈가보다",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100034517.1.16",
          "form": "친구들한테 밀리기싫다는거보니",
          "original_form": "친구들한테 밀리기싫다는거보니",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100034517.1.17",
          "form": "그라이~울신랑이 어이없어서ㅋㅋㅋㅋ",
          "original_form": "그라이~울신랑이 어이없어서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100034517.1.18",
          "form": "근데..화장실이.늠 예쁘더라 ㅋㅋㅋㅋ",
          "original_form": "근데..화장실이.늠 예쁘더라 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100034517.1.19",
          "form": "ㅋㅋㅋ보고싶노 그마이 이뻤나보네ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ보고싶노 그마이 이뻤나보네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100034517.1.20",
          "form": "난 리모델링1도 관심없었는데 보니 또 하곱 ㅋㅋㅋ그치만 돈이 없다ㅜㅋㅋㅋ",
          "original_form": "난 리모델링1도 관심없었는데 보니 또 하곱 ㅋㅋㅋ그치만 돈이 없다ㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034517.1.21",
          "form": "두찌 언넝낮잠재우고 문구사에 준비물 사러갈랬디 와이래.안자너;;",
          "original_form": "두찌 언넝낮잠재우고 문구사에 준비물 사러갈랬디 와이래.안자너;;",
          "speaker_id": "2",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100034517.1.22",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100034517.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ맞다맞다ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞다맞다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100034517.1.24",
          "form": "나는 이틀전에 문구사에 다녀옴",
          "original_form": "나는 이틀전에 문구사에 다녀옴",
          "speaker_id": "1",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100034517.1.25",
          "form": "2만원 플렉스하고왔다",
          "original_form": "2만원 플렉스하고왔다",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100034517.1.26",
