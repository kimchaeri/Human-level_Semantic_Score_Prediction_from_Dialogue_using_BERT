      "utterance": [
        {
          "id": "MDRW2100039753.1.1",
          "form": "헉 진짜? 고민 많이 하더니",
          "original_form": "헉 진짜? 고민 많이 하더니",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039753.1.2",
          "form": "드디어 샀냐",
          "original_form": "드디어 샀냐",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039753.1.3",
          "form": "아 그때 그거아니야",
          "original_form": "아 그때 그거아니야",
          "speaker_id": "1",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100039753.1.4",
          "form": "이번에 소라색 가디건",
          "original_form": "이번에 소라색 가디건",
          "speaker_id": "1",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100039753.1.5",
          "form": "보고 한눈에 반해서",
          "original_form": "보고 한눈에 반해서",
          "speaker_id": "1",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100039753.1.6",
          "form": "아 전에 그게 아니구나",
          "original_form": "아 전에 그게 아니구나",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039753.1.7",
          "form": "이전에 본 베이지색 말히는 줄 알았어",
          "original_form": "이전에 본 베이지색 말히는 줄 알았어",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039753.1.8",
          "form": "그건 베이직해서 좋긴한대",
          "original_form": "그건 베이직해서 좋긴한대",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039753.1.9",
          "form": "요즘 좀더 날렵해보이는 라아이",
          "original_form": "요즘 좀더 날렵해보이는 라아이",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039753.1.10",
          "form": "라인이 좋더라고",
          "original_form": "라인이 좋더라고",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039753.1.11",
          "form": "근데 문제는 너무 인기가 많아서",
          "original_form": "근데 문제는 너무 인기가 많아서",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039753.1.12",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039753.1.13",
          "form": "10월 30일에 순차배송이래\r\n돌은거 아니냐",
          "original_form": "10월 30일에 순차배송이래\r\n돌은거 아니냐",
          "speaker_id": "1",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100039753.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100039753.1.15",
          "form": "겨울에 오겠는데",
          "original_form": "겨울에 오겠는데",
          "speaker_id": "2",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039753.1.16",
          "form": "어떻게 생겼는지 궁금하다",
          "original_form": "어떻게 생겼는지 궁금하다",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039753.1.17",
          "form": "민무늬 가디건이야?",
          "original_form": "민무늬 가디건이야?",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039753.1.18",
          "form": "엄청 현란한 짜임으로 돼있어",
          "original_form": "엄청 현란한 짜임으로 돼있어",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039753.1.19",
          "form": "그래서 막상오면 한번입고 패딩입겠어",
          "original_form": "그래서 막상오면 한번입고 패딩입겠어",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039753.1.20",
          "form": "그런 느낌알지",
          "original_form": "그런 느낌알지",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039753.1.21",
          "form": "ㅌㅋㅌㅋㅋㅋㅋㅋ배송때문에?",
          "original_form": "ㅌㅋㅌㅋㅋㅋㅋㅋ배송때문에?",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039753.1.22",
          "form": "짜임 화려하면 멋지겠다",
          "original_form": "짜임 화려하면 멋지겠다",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039753.1.23",
          "form": "실물 구경하고싶어",
          "original_form": "실물 구경하고싶어",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039753.1.24",
          "form": "응 근데 주문취소를 못할정도로",
          "original_form": "응 근데 주문취소를 못할정도로",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039753.1.25",
          "form": "제품사진 봤다!",
          "original_form": "제품사진 봤다!",
          "speaker_id": "2",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039753.1.26",
          "form": "취소를 못할 정도로?",
          "original_form": "취소를 못할 정도로?",
          "speaker_id": "2",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039753.1.27",
          "form": "마음에 들더라고",
          "original_form": "마음에 들더라고",
          "speaker_id": "1",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039753.1.28",
          "form": "귀여워",
          "original_form": "귀여워",
          "speaker_id": "2",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039753.1.29",
          "form": "다이어트좀 했다고 입을수있는 의상의 폭이 넓어진게",
          "original_form": "다이어트좀 했다고 입을수있는 의상의 폭이 넓어진게",
          "speaker_id": "1",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039753.1.30",
          "form": "참 다양한 생각이 들게해",
          "original_form": "참 다양한 생각이 들게해",
          "speaker_id": "1",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039753.1.31",
          "form": "그러게 제작되는 옷 선택지가",
          "original_form": "그러게 제작되는 옷 선택지가",
          "speaker_id": "2",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039753.1.32",
          "form": "저 짜임이랑 투웨이인거랑 색상",
          "original_form": "저 짜임이랑 투웨이인거랑 색상",
          "speaker_id": "1",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039753.1.33",
          "form": "폭이 좁아서 원",
          "original_form": "폭이 좁아서 원",
          "speaker_id": "2",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039753.1.34",
          "form": "3가지 포인트가 마음에 들었어",
          "original_form": "3가지 포인트가 마음에 들었어",
          "speaker_id": "1",
          "time": "20210903 21:42"
        }
      ]
    }
  ]
