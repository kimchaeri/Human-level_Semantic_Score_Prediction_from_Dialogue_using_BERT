      "utterance": [
        {
          "id": "MDRW2100010167.1.1",
          "form": "맞아요 부모님이 계셔서 그나마 일도 해요",
          "original_form": "맞아요 부모님이 계셔서 그나마 일도 해요",
          "speaker_id": "1",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010167.1.2",
          "form": "코로나 끝나야 엄마도 아이도 맘편히 일하고 다닐텐데",
          "original_form": "코로나 끝나야 엄마도 아이도 맘편히 일하고 다닐텐데",
          "speaker_id": "2",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010167.1.3",
          "form": "요새 육아 힘들어요",
          "original_form": "요새 육아 힘들어요",
          "speaker_id": "2",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010167.1.4",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010167.1.5",
          "form": "저희 동생도 엄마가 봐줘서 일해요",
          "original_form": "저희 동생도 엄마가 봐줘서 일해요",
          "speaker_id": "2",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010167.1.6",
          "form": "돌아가면 저녁은 어떻게 하세요?",
          "original_form": "돌아가면 저녁은 어떻게 하세요?",
          "speaker_id": "2",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010167.1.7",
          "form": "힘드셔서 차리기 싫으시죠",
          "original_form": "힘드셔서 차리기 싫으시죠",
          "speaker_id": "2",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010167.1.8",
          "form": "전 방학과 코로나때 애들 밥 챙기기가 너무 힘들더라구요",
          "original_form": "전 방학과 코로나때 애들 밥 챙기기가 너무 힘들더라구요",
          "speaker_id": "2",
          "time": "20210823 15:01"
        },
        {
          "id": "MDRW2100010167.1.9",
          "form": "네 배달반찬이나 부모님 반찬으로 주로 먹죠",
          "original_form": "네 배달반찬이나 부모님 반찬으로 주로 먹죠",
          "speaker_id": "1",
          "time": "20210823 15:01"
        },
        {
          "id": "MDRW2100010167.1.10",
          "form": "메뉴 선정이 너무 오래 집에있으니",
          "original_form": "메뉴 선정이 너무 오래 집에있으니",
          "speaker_id": "2",
          "time": "20210823 15:01"
        },
        {
          "id": "MDRW2100010167.1.11",
          "form": "아 정말 부모님 감사하네요",
          "original_form": "아 정말 부모님 감사하네요",
          "speaker_id": "2",
          "time": "20210823 15:01"
        },
        {
          "id": "MDRW2100010167.1.12",
          "form": "방학때 진짜 힘들어요 육아 음식 가사 ㅎㅎ",
          "original_form": "방학때 진짜 힘들어요 육아 음식 가사 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 15:01"
        },
        {
          "id": "MDRW2100010167.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 15:01"
        },
        {
          "id": "MDRW2100010167.1.14",
          "form": "재택근무하심 젛을텐데",
          "original_form": "재택근무하심 젛을텐데",
          "speaker_id": "2",
          "time": "20210823 15:02"
        },
        {
          "id": "MDRW2100010167.1.15",
          "form": "좋을",
          "original_form": "좋을",
          "speaker_id": "2",
          "time": "20210823 15:02"
        },
        {
          "id": "MDRW2100010167.1.16",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210823 15:02"
        },
        {
          "id": "MDRW2100010167.1.17",
          "form": "근데 또 엄마가 집에 있으면 일도 육아도 같이하다보면 지쳐요",
          "original_form": "근데 또 엄마가 집에 있으면 일도 육아도 같이하다보면 지쳐요",
          "speaker_id": "2",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010167.1.18",
          "form": "저는 아직 아이가 어린데",
          "original_form": "저는 아직 아이가 어린데",
          "speaker_id": "1",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010167.1.19",
          "form": "일하며 아이 봐야해서 집중이 안돼요",
          "original_form": "일하며 아이 봐야해서 집중이 안돼요",
          "speaker_id": "2",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010167.1.20",
          "form": "육아는 끝도없고",
          "original_form": "육아는 끝도없고",
          "speaker_id": "1",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010167.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010167.1.22",
          "form": "그럼요 다 일일이 해줘야하고",
          "original_form": "그럼요 다 일일이 해줘야하고",
          "speaker_id": "2",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010167.1.23",
          "form": "많은 감정을 느끼게 되더라고요 ㅋㅋ",
          "original_form": "많은 감정을 느끼게 되더라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010167.1.24",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010167.1.25",
          "form": "저도 아이 하나 낳을걸 했었어요",
          "original_form": "저도 아이 하나 낳을걸 했었어요",
          "speaker_id": "2",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010167.1.26",
          "form": "그래서 둘이상 낳고 싶었는데 그릇이 안되는지 외동이 되었어요",
          "original_form": "그래서 둘이상 낳고 싶었는데 그릇이 안되는지 외동이 되었어요",
          "speaker_id": "1",
          "time": "20210823 15:04"
        },
        {
          "id": "MDRW2100010167.1.27",
          "form": "2명은 2배 3명은 3배로 힘들어요",
          "original_form": "2명은 2배 3명은 3배로 힘들어요",
          "speaker_id": "2",
          "time": "20210823 15:04"
        },
        {
          "id": "MDRW2100010167.1.28",
          "form": "아휴 잘하셨어요",
          "original_form": "아휴 잘하셨어요",
          "speaker_id": "2",
          "time": "20210823 15:04"
        },
        {
          "id": "MDRW2100010167.1.29",
          "form": "그래서 여전히 두아이가족보면 부러워요",
          "original_form": "그래서 여전히 두아이가족보면 부러워요",
          "speaker_id": "1",
          "time": "20210823 15:04"
        },
        {
          "id": "MDRW2100010167.1.30",
          "form": "다복한 느낌",
          "original_form": "다복한 느낌",
          "speaker_id": "1",
          "time": "20210823 15:04"
        },
        {
          "id": "MDRW2100010167.1.31",
          "form": "근데 육아는 현실이라",
          "original_form": "근데 육아는 현실이라",
          "speaker_id": "2",
          "time": "20210823 15:04"
        },
        {
          "id": "MDRW2100010167.1.32",
          "form": "남이라 좋은 거예요 ㅋㅋ",
          "original_form": "남이라 좋은 거예요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:04"
        },
        {
          "id": "MDRW2100010167.1.33",
          "form": "외동 괜찮아요 ㅎㅎ",
          "original_form": "외동 괜찮아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 15:04"
        },
        {
          "id": "MDRW2100010167.1.34",
          "form": "그분은 외동 부러워 하실거예요 ㅎㅎ",
          "original_form": "그분은 외동 부러워 하실거예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010167.1.35",
          "form": "혹시 갖더라도 늦게는 안좋더라구요",
          "original_form": "혹시 갖더라도 늦게는 안좋더라구요",
          "speaker_id": "2",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010167.1.36",
          "form": "나이차이 많이 나면 서로 대면대면해요",
          "original_form": "나이차이 많이 나면 서로 대면대면해요",
          "speaker_id": "2",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010167.1.37",
          "form": "저는 두살터울 동성아이를 갖고싶었지만",
          "original_form": "저는 두살터울 동성아이를 갖고싶었지만",
          "speaker_id": "1",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010167.1.38",
          "form": "코시국도 그렇고 과감히 포기 ㅠ",
          "original_form": "코시국도 그렇고 과감히 포기 ㅠ",
          "speaker_id": "1",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010167.1.39",
          "form": "그게 좋아요",
          "original_form": "그게 좋아요",
          "speaker_id": "2",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100010167.1.40",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100010167.1.41",
          "form": "그렇게 안되면 포기가 낫죠",
          "original_form": "그렇게 안되면 포기가 낫죠",
          "speaker_id": "2",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100010167.1.42",
          "form": "괜히 나이차이나면 같이 안놀아요",
          "original_form": "괜히 나이차이나면 같이 안놀아요",
          "speaker_id": "2",
          "time": "20210823 15:07"
        }
      ]
    }
