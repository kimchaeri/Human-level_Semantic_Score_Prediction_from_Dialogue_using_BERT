      "utterance": [
        {
          "id": "MDRW2100038778.1.1",
          "form": "암튼,, 오트밀을 머겄더니",
          "original_form": "암튼,, 오트밀을 머겄더니",
          "speaker_id": "1",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100038778.1.2",
          "form": "눈이 퀭ㅎ진 기분이군요",
          "original_form": "눈이 퀭ㅎ진 기분이군요",
          "speaker_id": "1",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100038778.1.3",
          "form": "ㅋㅋㅋㅋ 이제 그만 오트밀에서 벗어나세요ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ 이제 그만 오트밀에서 벗어나세요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100038778.1.4",
          "form": "내일 아침으론 뭘 드실 건가요?!",
          "original_form": "내일 아침으론 뭘 드실 건가요?!",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100038778.1.5",
          "form": "내일 먹을 맛있는 걸 생각하며 오트밀을 이겨냅시다",
          "original_form": "내일 먹을 맛있는 걸 생각하며 오트밀을 이겨냅시다",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038778.1.6",
          "form": "아침은 시리얼이요",
          "original_form": "아침은 시리얼이요",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038778.1.7",
          "form": "ㅋㅋㅋㅋㅋ 아침은... 아무래도... 간단하게 먹게 되죠",
          "original_form": "ㅋㅋㅋㅋㅋ 아침은... 아무래도... 간단하게 먹게 되죠",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038778.1.8",
          "form": "저는 어제 홀케이크를 사와서 그거랑 커피랑 먹을까 싶어요",
          "original_form": "저는 어제 홀케이크를 사와서 그거랑 커피랑 먹을까 싶어요",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038778.1.9",
          "form": "아 정말 밥 챙겨먹기가 아침엔 힘드니까요",
          "original_form": "아 정말 밥 챙겨먹기가 아침엔 힘드니까요",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038778.1.10",
          "form": "어떤 케이크를 사셨나요?",
          "original_form": "어떤 케이크를 사셨나요?",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038778.1.11",
          "form": "빠바 순수우유케이크요",
          "original_form": "빠바 순수우유케이크요",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100038778.1.12",
          "form": "생크림케이크를 제일 좋아합니다ㅎㅎ",
          "original_form": "생크림케이크를 제일 좋아합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038778.1.13",
          "form": "아이스아메리카노랑 먹음 꿀맛이예요",
          "original_form": "아이스아메리카노랑 먹음 꿀맛이예요",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038778.1.14",
          "form": "1호인가요?2호?",
          "original_form": "1호인가요?2호?",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038778.1.15",
          "form": "순수우유케이크는 사이즈가 하나밖에 없어요.",
          "original_form": "순수우유케이크는 사이즈가 하나밖에 없어요.",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038778.1.16",
          "form": "글서 1호인지 2호인진 잘 모르겠어요",
          "original_form": "글서 1호인지 2호인진 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100038778.1.17",
          "form": "그리 크진 않습니다.",
          "original_form": "그리 크진 않습니다.",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100038778.1.18",
          "form": ".그럼 1호일것 같네요",
          "original_form": ".그럼 1호일것 같네요",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100038778.1.19",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100038778.1.20",
          "form": "저도 먹고싶네요,,,",
          "original_form": "저도 먹고싶네요,,,",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038778.1.21",
          "form": "조각케이크로 파는 곳이 있으면 좋겠는데",
          "original_form": "조각케이크로 파는 곳이 있으면 좋겠는데",
          "speaker_id": "2",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100038778.1.22",
          "form": "없어서 맨날 홀케이크로 먹어요...",
          "original_form": "없어서 맨날 홀케이크로 먹어요...",
          "speaker_id": "2",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100038778.1.23",
          "form": "그리고 가게에 없을 때도 많음ㅠㅠ",
          "original_form": "그리고 가게에 없을 때도 많음ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100038778.1.24",
          "form": "제가 반 먹으면 될텐데 ",
          "original_form": "제가 반 먹으면 될텐데 ",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100038778.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100038778.1.26",
          "form": "물리적 거리가 좀 있네요 랜선상이니",
          "original_form": "물리적 거리가 좀 있네요 랜선상이니",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100038778.1.27",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100038778.1.28",
          "form": "도넛 구독도 둘이 하나 시켜서 나눠먹음 좋을 텐데",
          "original_form": "도넛 구독도 둘이 하나 시켜서 나눠먹음 좋을 텐데",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100038778.1.29",
          "form": "거리가 머네요ㅠㅠ",
          "original_form": "거리가 머네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:26"
        }
      ]
