      "utterance": [
        {
          "id": "MDRW2100040406.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100040406.1.2",
          "form": "오늘 날씨가 좋았었나요?",
          "original_form": "오늘 날씨가 좋았었나요?",
          "speaker_id": "1",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100040406.1.3",
          "form": "ㅋㅋㅋ네 안녕하세용",
          "original_form": "ㅋㅋㅋ네 안녕하세용",
          "speaker_id": "2",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100040406.1.4",
          "form": "네! 여기는 맑고 화창해요",
          "original_form": "네! 여기는 맑고 화창해요",
          "speaker_id": "2",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100040406.1.5",
          "form": "name1님 계신곳은요?",
          "original_form": "&name1&님 계신곳은요?",
          "speaker_id": "2",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100040406.1.6",
          "form": "여기도 오늘 맑은 날씨였어요\r\n날씨가 맑으니 좋더라구요!",
          "original_form": "여기도 오늘 맑은 날씨였어요\r\n날씨가 맑으니 좋더라구요!",
          "speaker_id": "1",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100040406.1.7",
          "form": "쌀쌀하진 않으세요?",
          "original_form": "쌀쌀하진 않으세요?",
          "speaker_id": "2",
          "time": "20210904 18:27"
        },
        {
          "id": "MDRW2100040406.1.8",
          "form": "햇빛이 쨍쨍해서인지 낮에는 살짝 덥게도 느껴졌는데, 오후 늦어지니 쌀쌀해지네요",
          "original_form": "햇빛이 쨍쨍해서인지 낮에는 살짝 덥게도 느껴졌는데, 오후 늦어지니 쌀쌀해지네요",
          "speaker_id": "1",
          "time": "20210904 18:28"
        },
        {
          "id": "MDRW2100040406.1.9",
          "form": "맞아요 아무리 낮에 더워도 이제는 해지면 금방 쌀쌀해지더라구요",
          "original_form": "맞아요 아무리 낮에 더워도 이제는 해지면 금방 쌀쌀해지더라구요",
          "speaker_id": "2",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100040406.1.10",
          "form": "저 어제 극세사 이불 꺼내고 싶었어요..",
          "original_form": "저 어제 극세사 이불 꺼내고 싶었어요..",
          "speaker_id": "2",
          "time": "20210904 18:30"
        },
        {
          "id": "MDRW2100040406.1.11",
          "form": "맞아요! 밤에는 춥다고 느껴지기도 하더라구요",
          "original_form": "맞아요! 밤에는 춥다고 느껴지기도 하더라구요",
          "speaker_id": "1",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100040406.1.12",
          "form": "이제 좀 있으면 극세사 꺼내도 될거 같아요",
          "original_form": "이제 좀 있으면 극세사 꺼내도 될거 같아요",
          "speaker_id": "1",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100040406.1.13",
          "form": "처서 지나면 가을이라더니 어른들 말씀 틀린게 없어요...",
          "original_form": "처서 지나면 가을이라더니 어른들 말씀 틀린게 없어요...",
          "speaker_id": "2",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100040406.1.14",
          "form": "넘 추워요 ㅋㅋㅋㅋㅋ",
          "original_form": "넘 추워요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100040406.1.15",
          "form": "맞아요ㅋㅋㅋ절기는 너무 신기한거 같아요",
          "original_form": "맞아요ㅋㅋㅋ절기는 너무 신기한거 같아요",
          "speaker_id": "1",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100040406.1.16",
          "form": "그래도 아직은 안된다! 하면서 버티고 있는데 이러다 감기 걸릴지도 모르겠어요 ㅋㅋㅋㅋ",
          "original_form": "그래도 아직은 안된다! 하면서 버티고 있는데 이러다 감기 걸릴지도 모르겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:33"
        },
        {
          "id": "MDRW2100040406.1.17",
          "form": "안 맞을거 같은데도 신기하게 절기에 맞춰서 딱 날씨가 변화하더라구요",
          "original_form": "안 맞을거 같은데도 신기하게 절기에 맞춰서 딱 날씨가 변화하더라구요",
          "speaker_id": "1",
          "time": "20210904 18:33"
        },
        {
          "id": "MDRW2100040406.1.18",
          "form": "그니까요 참 신기해요",
          "original_form": "그니까요 참 신기해요",
          "speaker_id": "2",
          "time": "20210904 18:36"
        },
        {
          "id": "MDRW2100040406.1.19",
          "form": "날이 쌀쌀해서 그런가 미세먼지도 없고 너무 좋아요",
          "original_form": "날이 쌀쌀해서 그런가 미세먼지도 없고 너무 좋아요",
          "speaker_id": "2",
          "time": "20210904 18:36"
        },
        {
          "id": "MDRW2100040406.1.20",
          "form": "거기도 그런가요?",
          "original_form": "거기도 그런가요?",
          "speaker_id": "2",
          "time": "20210904 18:37"
        },
        {
          "id": "MDRW2100040406.1.21",
          "form": "네! 여기도 그래요",
          "original_form": "네! 여기도 그래요",
          "speaker_id": "1",
          "time": "20210904 18:49"
        },
        {
          "id": "MDRW2100040406.1.22",
          "form": "여기도 그래요ㅎㅎ",
          "original_form": "여기도 그래요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 18:49"
        },
        {
          "id": "MDRW2100040406.1.23",
          "form": "계절 중 어느 계절을 가장 좋아하세요?",
          "original_form": "계절 중 어느 계절을 가장 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 18:50"
        }
      ]
    }
