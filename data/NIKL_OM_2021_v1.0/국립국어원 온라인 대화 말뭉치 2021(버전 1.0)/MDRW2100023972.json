      "utterance": [
        {
          "id": "MDRW2100023972.1.1",
          "form": "저희는 그냥 학교로고 박혀있었는뎈ㅋㅋㄱ",
          "original_form": "저희는 그냥 학교로고 박혀있었는뎈ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023972.1.2",
          "form": "체육복에 ..ㅜㅜ",
          "original_form": "체육복에 ..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023972.1.3",
          "form": "앗 ㅋㅋ 차라리 그게 더 나은 것 같아요",
          "original_form": "앗 ㅋㅋ 차라리 그게 더 나은 것 같아요",
          "speaker_id": "2",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100023972.1.4",
          "form": "저도 코파 로고를 별로 안좋아해서",
          "original_form": "저도 코파 로고를 별로 안좋아해서",
          "speaker_id": "2",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100023972.1.5",
          "form": "아니 ㅋㅋㅋㅋ 카파 로고요...",
          "original_form": "아니 ㅋㅋㅋㅋ 카파 로고요...",
          "speaker_id": "2",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100023972.1.6",
          "form": "코팤ㅋㅋㄱ아 한끗차이로 ㅋㅋㄱㅋ",
          "original_form": "코팤ㅋㅋㄱ아 한끗차이로 ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210830 20:49"
        },
        {
          "id": "MDRW2100023972.1.7",
          "form": "하긴 로고도 간지나는 나이키가 있어야 이쁘죠",
          "original_form": "하긴 로고도 간지나는 나이키가 있어야 이쁘죠",
          "speaker_id": "1",
          "time": "20210830 20:50"
        },
        {
          "id": "MDRW2100023972.1.8",
          "form": "학교로고는 걍... 학생이구나하고 넘어가는 ㅋㅋㅋ",
          "original_form": "학교로고는 걍... 학생이구나하고 넘어가는 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:50"
        },
        {
          "id": "MDRW2100023972.1.9",
          "form": "맞아요 ㅜㅜ",
          "original_form": "맞아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100023972.1.10",
          "form": "저는 어제 그 드디어 기다리고 기다리던 네이버멤버십데이여가지고",
          "original_form": "저는 어제 그 드디어 기다리고 기다리던 네이버멤버십데이여가지고",
          "speaker_id": "2",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100023972.1.11",
          "form": "잠도 안자고 장바구니 열심히 담았네요 ㅋㅋ",
          "original_form": "잠도 안자고 장바구니 열심히 담았네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100023972.1.12",
          "form": "앗 혹시 열심히 30만원 채우셨나요 ㅋㅋㅋ !",
          "original_form": "앗 혹시 열심히 30만원 채우셨나요 ㅋㅋㅋ !",
          "speaker_id": "1",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100023972.1.13",
          "form": "아 20만원이욬ㅋ !",
          "original_form": "아 20만원이욬ㅋ !",
          "speaker_id": "1",
          "time": "20210830 20:54"
        },
        {
          "id": "MDRW2100023972.1.14",
          "form": "ㅋㅋㅋ아 20만원 그거혹시 카드 그거요?",
          "original_form": "ㅋㅋㅋ아 20만원 그거혹시 카드 그거요?",
          "speaker_id": "2",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023972.1.15",
          "form": "네네 ㅋㅋㅋㅋ !",
          "original_form": "네네 ㅋㅋㅋㅋ !",
          "speaker_id": "1",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023972.1.16",
          "form": "아 저그거 채울려고 했는데 안했어요..",
          "original_form": "아 저그거 채울려고 했는데 안했어요..",
          "speaker_id": "2",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023972.1.17",
          "form": "생각보다 살 게 없더라구요;;",
          "original_form": "생각보다 살 게 없더라구요;;",
          "speaker_id": "2",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023972.1.18",
          "form": "저두욬ㅋㅋㄱ 채우다가 포기했어요ㅎㅎ",
          "original_form": "저두욬ㅋㅋㄱ 채우다가 포기했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100023972.1.19",
          "form": "그냥 장바구니에 담아둔것만 샀어요",
          "original_form": "그냥 장바구니에 담아둔것만 샀어요",
          "speaker_id": "1",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100023972.1.20",
          "form": "젤 중요한건 맴버십을 9월 30일까지 유지해야된다는 조건",
          "original_form": "젤 중요한건 맴버십을 9월 30일까지 유지해야된다는 조건",
          "speaker_id": "2",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100023972.1.21",
          "form": "제가 봤을 땐 다음달부턴 축소하거나 아예 멤버십플러스데이 안할 것 같아요",
          "original_form": "제가 봤을 땐 다음달부턴 축소하거나 아예 멤버십플러스데이 안할 것 같아요",
          "speaker_id": "2",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100023972.1.22",
          "form": "그게 뭐 한달에 한번씩은 꼭 한다고 적혀져있다던더",
          "original_form": "그게 뭐 한달에 한번씩은 꼭 한다고 적혀져있다던더",
          "speaker_id": "1",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023972.1.23",
          "form": "축소하거나 멘트 은근슬쩍 지울거같아요 ㅋㄲ",
          "original_form": "축소하거나 멘트 은근슬쩍 지울거같아요 ㅋㄲ",
          "speaker_id": "1",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023972.1.24",
          "form": "진짜ㅠㅠ 다들 기다렸다는듯이 호다닥 구매햇는지",
          "original_form": "진짜ㅠㅠ 다들 기다렸다는듯이 호다닥 구매햇는지",
          "speaker_id": "2",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100023972.1.25",
          "form": "저도 벼르고 있던거 바로 품절 됐더라구요 ㅋㅋ",
          "original_form": "저도 벼르고 있던거 바로 품절 됐더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100023972.1.26",
          "form": "아니 근데 막날에 하는거 너무해욬ㅋㅋㅋ",
          "original_form": "아니 근데 막날에 하는거 너무해욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100023972.1.27",
          "form": "그냥 안하는줄알고 산 사람도 많더라구요??",
