        {
          "id": "MDRW2100028051.1.3",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028051.1.4",
          "form": "박제된 cc 기록을 지워야 해",
          "original_form": "박제된 cc 기록을 지워야 해",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028051.1.5",
          "form": "ㅎㅎㅎㅎㅎ 빨리 지우자",
          "original_form": "ㅎㅎㅎㅎㅎ 빨리 지우자",
          "speaker_id": "2",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028051.1.6",
          "form": "너는 싸이 기록 다 지웠니?",
          "original_form": "너는 싸이 기록 다 지웠니?",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028051.1.7",
          "form": "나는 싸이 거의 안했어",
          "original_form": "나는 싸이 거의 안했어",
          "speaker_id": "2",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028051.1.8",
          "form": "넘나 다행이여",
          "original_form": "넘나 다행이여",
          "speaker_id": "2",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028051.1.9",
          "form": "정말 다행이다",
          "original_form": "정말 다행이다",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028051.1.10",
          "form": "현명해",
          "original_form": "현명해",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028051.1.11",
          "form": "나의 귀차니즘이 날 살렸어",
          "original_form": "나의 귀차니즘이 날 살렸어",
          "speaker_id": "2",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028051.1.12",
          "form": "본받아야지",
          "original_form": "본받아야지",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028051.1.13",
          "form": "페이스북도 안 함",
          "original_form": "페이스북도 안 함",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028051.1.14",
          "form": "쓸데없이 부지런하면 안돼",
          "original_form": "쓸데없이 부지런하면 안돼",
          "speaker_id": "2",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028051.1.15",
          "form": "인스타 해?",
          "original_form": "인스타 해?",
          "speaker_id": "1",
          "time": "20210902 18:13"
        },
        {
