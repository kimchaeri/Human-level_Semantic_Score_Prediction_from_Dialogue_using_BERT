      "utterance": [
        {
          "id": "MDRW2100028630.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028630.1.2",
          "form": "안녕하세오 ㅎㅎ",
          "original_form": "안녕하세오 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028630.1.3",
          "form": "이상형이 뭔가요?",
          "original_form": "이상형이 뭔가요?",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028630.1.4",
          "form": "저는 키크고 손큰 사람을 좋아합니다",
          "original_form": "저는 키크고 손큰 사람을 좋아합니다",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028630.1.5",
          "form": "전 키크고",
          "original_form": "전 키크고",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028630.1.6",
          "form": "잘생긴사람이요",
          "original_form": "잘생긴사람이요",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028630.1.7",
          "form": "키큰건 똑같군요",
          "original_form": "키큰건 똑같군요",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028630.1.8",
          "form": "연애할때 가장 중요한게 뭔가요?",
          "original_form": "연애할때 가장 중요한게 뭔가요?",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028630.1.9",
          "form": "착한사람이요 말도 잘통하고",
          "original_form": "착한사람이요 말도 잘통하고",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028630.1.10",
          "form": "그렁 사람은 흔해지 않지만 ㅜㅜ",
          "original_form": "그렁 사람은 흔해지 않지만 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028630.1.11",
          "form": "말 잘통하는거 좋져! 이게 참 힘든거 같아요",
          "original_form": "말 잘통하는거 좋져! 이게 참 힘든거 같아요",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028630.1.12",
          "form": "연애 중이신가요?",
          "original_form": "연애 중이신가요?",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028630.1.13",
          "form": "아니요 안하고 있어요",
          "original_form": "아니요 안하고 있어요",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028630.1.14",
          "form": "하고 계세요?!",
          "original_form": "하고 계세요?!",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028630.1.15",
          "form": "저는 하고 있습니다",
          "original_form": "저는 하고 있습니다",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028630.1.16",
          "form": "5년차 되었어요",
          "original_form": "5년차 되었어요",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028630.1.17",
          "form": "헉 되게 오래되었군요!!",
          "original_form": "헉 되게 오래되었군요!!",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028630.1.18",
          "form": "네 어찌하다보니 5년이 되었어요",
          "original_form": "네 어찌하다보니 5년이 되었어요",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028630.1.19",
          "form": "좋나요,??",
          "original_form": "좋나요,??",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028630.1.20",
          "form": "결혼 생각 있으신가요?",
          "original_form": "결혼 생각 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028630.1.21",
          "form": "좋긴 좋은거 같슴니다 내편이 있다고 생각하면 맘이 편해요ㅎ",
          "original_form": "좋긴 좋은거 같슴니다 내편이 있다고 생각하면 맘이 편해요ㅎ",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028630.1.22",
          "form": "전 결혼 생각 없어요",
          "original_form": "전 결혼 생각 없어요",
          "speaker_id": "2",
          "time": "20210902 21:25"
        }
      ]
