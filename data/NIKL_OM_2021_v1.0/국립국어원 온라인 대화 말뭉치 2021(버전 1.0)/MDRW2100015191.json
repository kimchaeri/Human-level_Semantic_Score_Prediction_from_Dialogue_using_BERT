      "utterance": [
        {
          "id": "MDRW2100015191.1.1",
          "form": "전 풀빵 진짜 좋아하거든요",
          "original_form": "전 풀빵 진짜 좋아하거든요",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015191.1.2",
          "form": "이제 풀빵 파는곳 찾기 너무 어려워서 아쉬워요ㅜㅜ",
          "original_form": "이제 풀빵 파는곳 찾기 너무 어려워서 아쉬워요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015191.1.3",
          "form": "그쵸 저희지역도 그래요",
          "original_form": "그쵸 저희지역도 그래요",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015191.1.4",
          "form": "풀빵이 최곤데....",
          "original_form": "풀빵이 최곤데....",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015191.1.5",
          "form": "붕어빵은 슈붕파세요 팥붕파세요?",
          "original_form": "붕어빵은 슈붕파세요 팥붕파세요?",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015191.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ전 슈붕이요!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ전 슈붕이요!!",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015191.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015191.1.8",
          "form": "저도요!!",
          "original_form": "저도요!!",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015191.1.9",
          "form": "슈붕이 최고예요",
          "original_form": "슈붕이 최고예요",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015191.1.10",
          "form": "ㅋㅋㅋ맞아요 전 팥은 팥알갱이 없는 고운 단팥이 좋은데 붕어빵 팥은 통팥이 꽤 많아서 슈붕파에요~~",
          "original_form": "ㅋㅋㅋ맞아요 전 팥은 팥알갱이 없는 고운 단팥이 좋은데 붕어빵 팥은 통팥이 꽤 많아서 슈붕파에요~~",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015191.1.11",
          "form": "슈크림도 달콤하고 맛있고요!",
          "original_form": "슈크림도 달콤하고 맛있고요!",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015191.1.12",
          "form": "먹고 싶어졌어요",
          "original_form": "먹고 싶어졌어요",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015191.1.13",
          "form": "사실 붕어빵보다 잉어빵이 훨씬 맛있는데 아시나요?",
          "original_form": "사실 붕어빵보다 잉어빵이 훨씬 맛있는데 아시나요?",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015191.1.14",
          "form": "헉 진짜요?",
          "original_form": "헉 진짜요?",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015191.1.15",
          "form": "사실 잘 몰랐어요!!",
          "original_form": "사실 잘 몰랐어요!!",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015191.1.16",
          "form": "어떤점이 다르죠....?!",
          "original_form": "어떤점이 다르죠....?!",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015191.1.17",
          "form": "반죽이 달라요!!",
          "original_form": "반죽이 달라요!!",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015191.1.18",
          "form": "붕어빵은 흐물떵해요",
          "original_form": "붕어빵은 흐물떵해요",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015191.1.19",
          "form": "바삭하고 맛있는 놈이 잉어빵이에요",
          "original_form": "바삭하고 맛있는 놈이 잉어빵이에요",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015191.1.20",
          "form": "헉 그렇구나!!!",
          "original_form": "헉 그렇구나!!!",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015191.1.21",
          "form": "바삭하면 확실히 더 맛있겠어요!!!!!",
          "original_form": "바삭하면 확실히 더 맛있겠어요!!!!!",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015191.1.22",
          "form": "근데 요즘 잉어빵이 많이 없는것같아요ㅠㅡㅠ",
          "original_form": "근데 요즘 잉어빵이 많이 없는것같아요ㅠㅡㅠ",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015191.1.23",
          "form": "맞아요 아무래도 코로나 때문에 길거리음식이 잘 안 팔리나봐요",
          "original_form": "맞아요 아무래도 코로나 때문에 길거리음식이 잘 안 팔리나봐요",
          "speaker_id": "1",
          "time": "20210826 07:24"
        },
        {
          "id": "MDRW2100015191.1.24",
          "form": "그러게용ㅠ길거리음식 먹은지 오래된것 같아요~",
          "original_form": "그러게용ㅠ길거리음식 먹은지 오래된것 같아요~",
          "speaker_id": "2",
          "time": "20210826 09:18"
        },
        {
          "id": "MDRW2100015191.1.25",
          "form": "ㅋㅋㅋ갑자기 옛날에 분식집에서 파는 떡꼬치 먹고싶어졌어요",
          "original_form": "ㅋㅋㅋ갑자기 옛날에 분식집에서 파는 떡꼬치 먹고싶어졌어요",
          "speaker_id": "2",
          "time": "20210826 09:19"
        },
        {
          "id": "MDRW2100015191.1.26",
          "form": "저도 떡꼬치 완전 좋아해요!!",
          "original_form": "저도 떡꼬치 완전 좋아해요!!",
          "speaker_id": "1",
          "time": "20210826 09:37"
        },
        {
          "id": "MDRW2100015191.1.27",
          "form": "집 앞에 있는 조그만 분식집 떡꼬치 맛있어서 자주 사먹었었어요",
          "original_form": "집 앞에 있는 조그만 분식집 떡꼬치 맛있어서 자주 사먹었었어요",
          "speaker_id": "1",
          "time": "20210826 09:38"
        },
        {
          "id": "MDRW2100015191.1.28",
          "form": "ㅋㅋㅋㅋㅋ매콤달달하고 너무 맛있죠!!ㅎ떡꼬치는 그런 분식집에서만 팔아서 요즘은 보기힘드네요ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ매콤달달하고 너무 맛있죠!!ㅎ떡꼬치는 그런 분식집에서만 팔아서 요즘은 보기힘드네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 09:38"
        },
        {
          "id": "MDRW2100015191.1.29",
          "form": "맞아요 그런 게 진짜 맛있는건데",
          "original_form": "맞아요 그런 게 진짜 맛있는건데",
          "speaker_id": "1",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100015191.1.30",
          "form": "아침은 드셨어요?",
          "original_form": "아침은 드셨어요?",
          "speaker_id": "1",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100015191.1.31",
          "form": "전 아침은 잘 안먹는편이라서 안먹었어요!\r\n근데 요즘은 챙겨먹어야하나 고민중이에요ㅋㅋㄲㅋ뭔가 배고프네요",
          "original_form": "전 아침은 잘 안먹는편이라서 안먹었어요!\r\n근데 요즘은 챙겨먹어야하나 고민중이에요ㅋㅋㄲㅋ뭔가 배고프네요",
