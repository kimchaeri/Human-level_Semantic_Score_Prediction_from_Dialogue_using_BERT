      "utterance": [
        {
          "id": "MDRW2100042007.1.1",
          "form": "요즘 대면하나요?",
          "original_form": "요즘 대면하나요?",
          "speaker_id": "2",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100042007.1.2",
          "form": "아니요 비대면해요",
          "original_form": "아니요 비대면해요",
          "speaker_id": "1",
          "time": "20210906 18:35"
        },
        {
          "id": "MDRW2100042007.1.3",
          "form": "거리두기 단계에 따라 대면을 한다고 해요",
          "original_form": "거리두기 단계에 따라 대면을 한다고 해요",
          "speaker_id": "1",
          "time": "20210906 18:35"
        },
        {
          "id": "MDRW2100042007.1.4",
          "form": "아쉽기도하고 좋기도하죠?",
          "original_form": "아쉽기도하고 좋기도하죠?",
          "speaker_id": "2",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100042007.1.5",
          "form": "네 학교에 안 가서 좋은데 비대면이라서 불편하기도 해요",
          "original_form": "네 학교에 안 가서 좋은데 비대면이라서 불편하기도 해요",
          "speaker_id": "1",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100042007.1.6",
          "form": "맞아요 마주보고 소통할게 있는데요ㅜ",
          "original_form": "맞아요 마주보고 소통할게 있는데요ㅜ",
          "speaker_id": "2",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100042007.1.7",
          "form": "동아리 하세요??",
          "original_form": "동아리 하세요??",
          "speaker_id": "2",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100042007.1.8",
          "form": "아니요 저는 안 들어갔어요 고민중이에요",
          "original_form": "아니요 저는 안 들어갔어요 고민중이에요",
          "speaker_id": "1",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100042007.1.9",
          "form": "대면못하니까 애매할거같아요",
          "original_form": "대면못하니까 애매할거같아요",
          "speaker_id": "2",
          "time": "20210906 18:46"
        },
        {
          "id": "MDRW2100042007.1.10",
          "form": "맞아요 주변에 동아리 하는 친구들 보면 동아리 들어가면 행사 때 잠깐만 나온다고 하더라고요",
          "original_form": "맞아요 주변에 동아리 하는 친구들 보면 동아리 들어가면 행사 때 잠깐만 나온다고 하더라고요",
          "speaker_id": "1",
          "time": "20210906 18:47"
        },
        {
          "id": "MDRW2100042007.1.11",
          "form": "제 친구는 가입하고 반년도 안되어 그만뒀어요 내년엔 대면가능할거같으니 천천히 결정하세요!",
          "original_form": "제 친구는 가입하고 반년도 안되어 그만뒀어요 내년엔 대면가능할거같으니 천천히 결정하세요!",
          "speaker_id": "2",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100042007.1.12",
          "form": "그래야겠어요! 천천히 해도 되니까요",
          "original_form": "그래야겠어요! 천천히 해도 되니까요",
          "speaker_id": "1",
          "time": "20210906 18:51"
        },
        {
          "id": "MDRW2100042007.1.13",
          "form": "시간표도 의미가 없겠네요ㅋㅋ 전 공강만드느라ㅋㅋ",
          "original_form": "시간표도 의미가 없겠네요ㅋㅋ 전 공강만드느라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:01"
        },
        {
          "id": "MDRW2100042007.1.14",
          "form": "맞아요 시간표 만들어도 녹화된 강의가 더 많아서 의미가 크게 없는거 같아요",
          "original_form": "맞아요 시간표 만들어도 녹화된 강의가 더 많아서 의미가 크게 없는거 같아요",
          "speaker_id": "1",
          "time": "20210906 19:55"
        },
        {
          "id": "MDRW2100042007.1.15",
          "form": "ㅜㅜ 진짜 코로나때문에 속상하네요!",
          "original_form": "ㅜㅜ 진짜 코로나때문에 속상하네요!",
          "speaker_id": "2",
          "time": "20210906 19:55"
        }
      ]
