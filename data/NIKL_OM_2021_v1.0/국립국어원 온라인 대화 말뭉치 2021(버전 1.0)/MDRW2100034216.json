      "utterance": [
        {
          "id": "MDRW2100034216.1.1",
          "form": "뭐 사고싶은 물건 있으새요?",
          "original_form": "뭐 사고싶은 물건 있으새요?",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100034216.1.2",
          "form": "플스 5랑 소파사고 싶어요",
          "original_form": "플스 5랑 소파사고 싶어요",
          "speaker_id": "1",
          "time": "20210828 09:43"
        },
        {
          "id": "MDRW2100034216.1.3",
          "form": "사고싶은 물건 있으세요?",
          "original_form": "사고싶은 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210828 09:43"
        },
        {
          "id": "MDRW2100034216.1.4",
          "form": "저는 갤럭시 워치요 ㅠㅠ",
          "original_form": "저는 갤럭시 워치요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100034216.1.5",
          "form": "너무 탐나더라고요",
          "original_form": "너무 탐나더라고요",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100034216.1.6",
          "form": "게임 좋아하새요m",
          "original_form": "게임 좋아하새요m",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100034216.1.7",
          "form": "아 갤럭시 워치! 저두요!",
          "original_form": "아 갤럭시 워치! 저두요!",
          "speaker_id": "1",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100034216.1.8",
          "form": "너무 기능이 좋고 디자인이 이뻐요 ㅠㅠ",
          "original_form": "너무 기능이 좋고 디자인이 이뻐요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100034216.1.9",
          "form": "네 게임 좋아해요!",
          "original_form": "네 게임 좋아해요!",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100034216.1.10",
          "form": "그쵸그쵸 ㅠㅠ 인바디 젤수있다는 것도 넘 혹하고..",
          "original_form": "그쵸그쵸 ㅠㅠ 인바디 젤수있다는 것도 넘 혹하고..",
          "speaker_id": "2",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100034216.1.11",
          "form": "오 최근에 산 게임 있어요?",
          "original_form": "오 최근에 산 게임 있어요?",
          "speaker_id": "2",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100034216.1.12",
          "form": "그쵸 인바디 진짜 혹해요",
          "original_form": "그쵸 인바디 진짜 혹해요",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100034216.1.13",
          "form": "최근에 동숲 샀어요",
          "original_form": "최근에 동숲 샀어요",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100034216.1.14",
          "form": "젤다야숨이랑요",
          "original_form": "젤다야숨이랑요",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100034216.1.15",
          "form": "오 야슘 재밌다 들었는데",
          "original_form": "오 야슘 재밌다 들었는데",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100034216.1.16",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100034216.1.17",
          "form": "오픈월드게임 좋아하시면 무조건 추천이요",
          "original_form": "오픈월드게임 좋아하시면 무조건 추천이요",
          "speaker_id": "1",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100034216.1.18",
          "form": "어려울까봐 걱정되는데 괜찮을가요 ㅠ",
          "original_form": "어려울까봐 걱정되는데 괜찮을가요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034216.1.19",
          "form": "컨트롤 못하시면 못하시는대로 진행가능해요",
          "original_form": "컨트롤 못하시면 못하시는대로 진행가능해요",
          "speaker_id": "1",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100034216.1.20",
          "form": "그게 야숨의 장점인거 같아요",
          "original_form": "그게 야숨의 장점인거 같아요",
          "speaker_id": "1",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100034216.1.21",
          "form": "오오 재밌겠네요",
          "original_form": "오오 재밌겠네요",
          "speaker_id": "2",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100034216.1.22",
          "form": "저도 함 플레이 해봐야겠어요",
          "original_form": "저도 함 플레이 해봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100034216.1.23",
          "form": "타이틀류는 중고로 사시는거 추천해요",
          "original_form": "타이틀류는 중고로 사시는거 추천해요",
          "speaker_id": "1",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100034216.1.24",
          "form": "다시 되팔기도 좋구요 야숨은 가격방어가 좋아서...",
          "original_form": "다시 되팔기도 좋구요 야숨은 가격방어가 좋아서...",
          "speaker_id": "1",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100034216.1.25",
          "form": "오 완전 꿀팁이네요",
          "original_form": "오 완전 꿀팁이네요",
          "speaker_id": "2",
          "time": "20210828 17:12"
        }
      ]
    }
  ]
