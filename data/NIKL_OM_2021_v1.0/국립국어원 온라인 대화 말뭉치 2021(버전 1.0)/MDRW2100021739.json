      "utterance": [
        {
          "id": "MDRW2100021739.1.1",
          "form": "쓸모없던 거 음..",
          "original_form": "쓸모없던 거 음..",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021739.1.2",
          "form": "쓸모없는건 다 덕심에 산것들이 쓸모없지 않을까요",
          "original_form": "쓸모없는건 다 덕심에 산것들이 쓸모없지 않을까요",
          "speaker_id": "2",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021739.1.3",
          "form": "앨범같은거 사실 필요없는데 사고..",
          "original_form": "앨범같은거 사실 필요없는데 사고..",
          "speaker_id": "2",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021739.1.4",
          "form": "ㅋㅋㅋㅋ앨벜 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ앨벜 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021739.1.5",
          "form": "누구 덕질 하셨어요?",
          "original_form": "누구 덕질 하셨어요?",
          "speaker_id": "1",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021739.1.6",
          "form": "막 덕질 심하게 한건 아닌데",
          "original_form": "막 덕질 심하게 한건 아닌데",
          "speaker_id": "2",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021739.1.7",
          "form": "은지원이 멋져서 젝키 앨범 몇번 샀는데",
          "original_form": "은지원이 멋져서 젝키 앨범 몇번 샀는데",
          "speaker_id": "2",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021739.1.8",
          "form": "다 부질없어여 ㅋㅋㅋㅋ",
          "original_form": "다 부질없어여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021739.1.9",
          "form": "어머 은지원 ㅋㅋㅋ",
          "original_form": "어머 은지원 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021739.1.10",
          "form": "지원이엉아 뭔가 진짜 지니어스 같아요",
          "original_form": "지원이엉아 뭔가 진짜 지니어스 같아요",
          "speaker_id": "1",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021739.1.11",
          "form": "네ㅋㅋㅋ 또 그룹할땐 카리스마 엄청나고",
          "original_form": "네ㅋㅋㅋ 또 그룹할땐 카리스마 엄청나고",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021739.1.12",
          "form": "진짜 매력 넘쳐서 덕질할만도 해요 ㅋㅋㅋ",
          "original_form": "진짜 매력 넘쳐서 덕질할만도 해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021739.1.13",
          "form": "지금 그 앨범들 어떻게 하셨어요? 요새 씨디플레이어도 없을텐데",
          "original_form": "지금 그 앨범들 어떻게 하셨어요? 요새 씨디플레이어도 없을텐데",
          "speaker_id": "1",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021739.1.14",
          "form": "씨디들은 보관하고 앨범은 다 버렸어요ㅋㅋㅋ",
          "original_form": "씨디들은 보관하고 앨범은 다 버렸어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021739.1.15",
          "form": "미니멀리스트가 되겠다며 한참 방을 비웠거든요",
          "original_form": "미니멀리스트가 되겠다며 한참 방을 비웠거든요",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021739.1.16",
          "form": "오마이갓 ㅋㅋㅋㅋ",
          "original_form": "오마이갓 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021739.1.17",
          "form": "그런거 중고거래하면 비싸게 팔 수 있지 않아요?",
          "original_form": "그런거 중고거래하면 비싸게 팔 수 있지 않아요?",
          "speaker_id": "1",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021739.1.18",
          "form": "살 사람이 있을까요?ㅋㅋㅋㅋ 없을 것 같은데 ㅋㅋㅋ",
          "original_form": "살 사람이 있을까요?ㅋㅋㅋㅋ 없을 것 같은데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021739.1.19",
          "form": "님은 쓸모없는 물건있어요??",
          "original_form": "님은 쓸모없는 물건있어요??",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021739.1.20",
          "form": "저 미니고데기요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 미니고데기요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021739.1.21",
          "form": "이유도 말해드릴까요 ㅋㅋㅋㅋ",
          "original_form": "이유도 말해드릴까요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021739.1.22",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "2",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021739.1.23",
          "form": "제가 고데기 사용할 줄 몰라요..",
          "original_form": "제가 고데기 사용할 줄 몰라요..",
          "speaker_id": "1",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021739.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021739.1.25",
          "form": "거울 보고 하는게 왜 이리 어려운거죠?ㅜㅜ 여자들 대단해요",
          "original_form": "거울 보고 하는게 왜 이리 어려운거죠?ㅜㅜ 여자들 대단해요",
          "speaker_id": "1",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021739.1.26",
          "form": "유튜브 보면 되져 왜",
          "original_form": "유튜브 보면 되져 왜",
          "speaker_id": "2",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021739.1.27",
          "form": "머리로는 알겠는데 몸이 말을 안들어요",
          "original_form": "머리로는 알겠는데 몸이 말을 안들어요",
          "speaker_id": "1",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021739.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021739.1.29",
          "form": "저도 결국은 고데기 안하고 걍 롤 말고 마는데",
          "original_form": "저도 결국은 고데기 안하고 걍 롤 말고 마는데",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021739.1.30",
          "form": "난 볼륨 넣을라했는데 까치집 만들고..ㅠㅠ",
          "original_form": "난 볼륨 넣을라했는데 까치집 만들고..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021739.1.31",
          "form": "롤 비싸요?",
          "original_form": "롤 비싸요?",
          "speaker_id": "1",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021739.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021739.1.33",
          "form": "아니욬ㅋㅋㅋㅋ",
          "original_form": "아니욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021739.1.34",
