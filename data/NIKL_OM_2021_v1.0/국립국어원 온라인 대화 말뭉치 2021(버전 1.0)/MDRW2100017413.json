      "utterance": [
        {
          "id": "MDRW2100017413.1.1",
          "form": "한국은 부동산이 망하면 나라가 망하고",
          "original_form": "한국은 부동산이 망하면 나라가 망하고",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017413.1.2",
          "form": "미국은 주식이 망하면 나라가 망한다는",
          "original_form": "미국은 주식이 망하면 나라가 망한다는",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017413.1.3",
          "form": "경제학적인 헛소리가 있거든요",
          "original_form": "경제학적인 헛소리가 있거든요",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017413.1.4",
          "form": "서로 그쪽이 진심인거라서",
          "original_form": "서로 그쪽이 진심인거라서",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017413.1.5",
          "form": "ㅋㅋ진짜 이 얘길 왜 하고있지",
          "original_form": "ㅋㅋ진짜 이 얘길 왜 하고있지",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017413.1.6",
          "form": "이상하네",
          "original_form": "이상하네",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017413.1.7",
          "form": "아무튼 은행 이율은 리스크가 없지만 매년 손해를 보고 넣는거다~ 이정도",
          "original_form": "아무튼 은행 이율은 리스크가 없지만 매년 손해를 보고 넣는거다~ 이정도",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017413.1.8",
          "form": "경제 얘기 부모님이랑 하세요?",
          "original_form": "경제 얘기 부모님이랑 하세요?",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017413.1.9",
          "form": "아뇨 전혀안해요 ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 전혀안해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100017413.1.10",
          "form": "친구랑도거의안하고",
          "original_form": "친구랑도거의안하고",
          "speaker_id": "1",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100017413.1.11",
          "form": "첨이네요",
          "original_form": "첨이네요",
          "speaker_id": "1",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017413.1.12",
          "form": "신기하다 엄청 장아시네",
          "original_form": "신기하다 엄청 장아시네",
          "speaker_id": "1",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017413.1.13",
          "form": "잘아시네",
          "original_form": "잘아시네",
          "speaker_id": "1",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017413.1.14",
          "form": "나름 경제학과? ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나름 경제학과? ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017413.1.15",
          "form": "근데 친구들도 몇명하고만 이런 얘기를 하지",
          "original_form": "근데 친구들도 몇명하고만 이런 얘기를 하지",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017413.1.16",
          "form": "회사 형들하고 합니다 대부분",
          "original_form": "회사 형들하고 합니다 대부분",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017413.1.17",
          "form": "난 뭐라도 해서 편하게 살고 싶은데",
          "original_form": "난 뭐라도 해서 편하게 살고 싶은데",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017413.1.18",
          "form": "안그런가봐요",
          "original_form": "안그런가봐요",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017413.1.19",
          "form": "이것도 뭐 마음이 맞아야 하는거지 하고",
          "original_form": "이것도 뭐 마음이 맞아야 하는거지 하고",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017413.1.20",
          "form": "그래도 봤으면 하는 정보들은 톡방에 던져주긴하는데",
          "original_form": "그래도 봤으면 하는 정보들은 톡방에 던져주긴하는데",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017413.1.21",
          "form": "몇이나 볼지",
          "original_form": "몇이나 볼지",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017413.1.22",
          "form": "절 꼰대라 생각할수도ㅋㅋ",
          "original_form": "절 꼰대라 생각할수도ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017413.1.23",
          "form": "저한테 뿌려주세요 앞으로 ㅋ",
          "original_form": "저한테 뿌려주세요 앞으로 ㅋ",
          "speaker_id": "1",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017413.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017413.1.25",
          "form": "도박은 하지마시구요",
          "original_form": "도박은 하지마시구요",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017413.1.26",
          "form": "가치투자!",
          "original_form": "가치투자!",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017413.1.27",
          "form": "버핏형이 항상 외치는ㅋㅋ",
          "original_form": "버핏형이 항상 외치는ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017413.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100017413.1.29",
          "form": "쫄본데 무슨 도박이에요 ㅋㅋㅋㅋ절대못해",
          "original_form": "쫄본데 무슨 도박이에요 ㅋㅋㅋㅋ절대못해",
          "speaker_id": "1",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100017413.1.30",
          "form": "로또 몇번 사보셨어요?",
          "original_form": "로또 몇번 사보셨어요?",
          "speaker_id": "2",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017413.1.31",
          "form": "로또는...",
          "original_form": "로또는...",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017413.1.32",
          "form": "달에한번은해요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "달에한번은해요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017413.1.33",
          "form": "그러네 로또가잇옷네",
          "original_form": "그러네 로또가잇옷네",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017413.1.34",
          "form": "어허.. 도박사시네",
          "original_form": "어허.. 도박사시네",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100017413.1.35",
          "form": "생각난김에 내일하나사야겟어요 ㅎ",
          "original_form": "생각난김에 내일하나사야겟어요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100017413.1.36",
          "form": "ㅋㅋㅋㅋ아니... 이걸",
          "original_form": "ㅋㅋㅋㅋ아니... 이걸",
          "speaker_id": "2",
          "time": "20210827 08:32"
        },
        {
          "id": "MDRW2100017413.1.37",
          "form": "전 로또 살면서 5번?",
          "original_form": "전 로또 살면서 5번?",
          "speaker_id": "2",
          "time": "20210827 08:33"
        },
        {
          "id": "MDRW2100017413.1.38",
          "form": "사봤네요",
          "original_form": "사봤네요",
          "speaker_id": "2",
          "time": "20210827 08:33"
        },
        {
          "id": "MDRW2100017413.1.39",
          "form": "헥?",
          "original_form": "헥?",
          "speaker_id": "1",
          "time": "20210827 08:38"
        },
        {
          "id": "MDRW2100017413.1.40",
          "form": "저는가끔 5만원되드라구여 ㅋㅋㅋㅋ",
          "original_form": "저는가끔 5만원되드라구여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:38"
        },
        {
          "id": "MDRW2100017413.1.41",
          "form": "아 또비오네",
          "original_form": "아 또비오네",
          "speaker_id": "1",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100017413.1.42",
          "form": "크 꿈과 희망을 사시네",
          "original_form": "크 꿈과 희망을 사시네",
          "speaker_id": "2",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100017413.1.43",
          "form": "윗동네는 이번주 내내 비와요",
          "original_form": "윗동네는 이번주 내내 비와요",
          "speaker_id": "2",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100017413.1.44",
          "form": "짜증",
          "original_form": "짜증",
          "speaker_id": "2",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100017413.1.45",
          "form": "출근길 넘빡셈",
          "original_form": "출근길 넘빡셈",
          "speaker_id": "2",
