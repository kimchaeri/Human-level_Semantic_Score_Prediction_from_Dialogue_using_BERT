      "utterance": [
        {
          "id": "MDRW2100026595.1.1",
          "form": "연어솥밥만 할까요..",
          "original_form": "연어솥밥만 할까요..",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026595.1.2",
          "form": "그에 비함 이 부대찌개는 카스트의 밑바닥이죠",
          "original_form": "그에 비함 이 부대찌개는 카스트의 밑바닥이죠",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026595.1.3",
          "form": "카스트 ㅎㅎ",
          "original_form": "카스트 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026595.1.4",
          "form": "어휘구사력이 상당하시네요",
          "original_form": "어휘구사력이 상당하시네요",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026595.1.5",
          "form": "혹시 레크레이션학과 나오셨나요?",
          "original_form": "혹시 레크레이션학과 나오셨나요?",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026595.1.6",
          "form": "제가 위장끈이 길어서요ㅎ",
          "original_form": "제가 위장끈이 길어서요ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026595.1.7",
          "form": "아니  절 뭘로 보시구 ㅂㄷㅂㄷ",
          "original_form": "아니  절 뭘로 보시구 ㅂㄷㅂㄷ",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026595.1.8",
          "form": "고소할 겁니다",
          "original_form": "고소할 겁니다",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026595.1.9",
          "form": "말씀을 잘하셔서 ㅋㅋ",
          "original_form": "말씀을 잘하셔서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026595.1.10",
          "form": "레크레이션 학과 무시하시는건가요?",
          "original_form": "레크레이션 학과 무시하시는건가요?",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026595.1.11",
          "form": "! 레크리에이션 학과 나온 분만 제게 돌을 던져욧!",
          "original_form": "! 레크리에이션 학과 나온 분만 제게 돌을 던져욧!",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026595.1.12",
          "form": "제가 제일 좋아하는 사람이 렠크레이션 학과 나와ㅅ습니다",
          "original_form": "제가 제일 좋아하는 사람이 렠크레이션 학과 나와ㅅ습니다",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026595.1.13",
          "form": "그 이름은 이수근!",
          "original_form": "그 이름은 이수근!",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026595.1.14",
          "form": "아 무슨과 나오셧는지 물어봐도되요?",
          "original_form": "아 무슨과 나오셧는지 물어봐도되요?",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026595.1.15",
          "form": "ㅋㅋㅋ 이상형인가요",
          "original_form": "ㅋㅋㅋ 이상형인가요",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026595.1.16",
          "form": "문송해서 안됩니다!",
          "original_form": "문송해서 안됩니다!",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026595.1.17",
          "form": "문송 ㅎㅎ",
          "original_form": "문송 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026595.1.18",
          "form": "전 이송",
          "original_form": "전 이송",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026595.1.19",
          "form": "아 그럼 그말 아시겠네요ㅎ",
          "original_form": "아 그럼 그말 아시겠네요ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026595.1.20",
          "form": "이과 다 죽어버렸으면ㅎㅎ",
          "original_form": "이과 다 죽어버렸으면ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026595.1.21",
          "form": "님도 그말 들으신적 있쥬",
          "original_form": "님도 그말 들으신적 있쥬",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026595.1.22",
          "form": "아녀 제주변은 천사만있어서",
          "original_form": "아녀 제주변은 천사만있어서",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026595.1.23",
          "form": "아니 아내분도 그런적이 없다구요?",
          "original_form": "아니 아내분도 그런적이 없다구요?",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026595.1.24",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026595.1.25",
          "form": "죽어까진 아니고 ㅋㅋ",
          "original_form": "죽어까진 아니고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026595.1.26",
          "form": "자꾸 논리적으로 말하지말래여",
          "original_form": "자꾸 논리적으로 말하지말래여",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026595.1.27",
          "form": "인과관계를 말하지말라고합니다",
          "original_form": "인과관계를 말하지말라고합니다",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026595.1.28",
          "form": "밥이나 먹으래요",
          "original_form": "밥이나 먹으래요",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026595.1.29",
          "form": "왜 때문인가요",
          "original_form": "왜 때문인가요",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026595.1.30",
          "form": "ㅎㅎ 아내가 정말 고운말만 쓰네요",
          "original_form": "ㅎㅎ 아내가 정말 고운말만 쓰네요",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026595.1.31",
          "form": "하하하",
          "original_form": "하하하",
          "speaker_id": "2",
          "time": "20210901 23:18"
        }
      ]
    }
  ]
