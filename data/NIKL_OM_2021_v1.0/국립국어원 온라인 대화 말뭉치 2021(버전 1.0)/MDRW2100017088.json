      "utterance": [
        {
          "id": "MDRW2100017088.1.1",
          "form": "name2님 안녕하세요~",
          "original_form": "&name2&님 안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017088.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017088.1.3",
          "form": "요즘 재택근무하시나요?",
          "original_form": "요즘 재택근무하시나요?",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017088.1.4",
          "form": "저는 지금 백수라서 알바구하고 있어요 ㅠㅠ",
          "original_form": "저는 지금 백수라서 알바구하고 있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017088.1.5",
          "form": "으악! name2님~ 제가 먼저 말걸어놓고 사라져서!",
          "original_form": "으악! &name2&님~ 제가 먼저 말걸어놓고 사라져서!",
          "speaker_id": "1",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100017088.1.6",
          "form": "배고파서 밥 먹으러 갔었어요",
          "original_form": "배고파서 밥 먹으러 갔었어요",
          "speaker_id": "1",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100017088.1.7",
          "form": "그러시구나 금방 좋은 일 찾으실 거에요",
          "original_form": "그러시구나 금방 좋은 일 찾으실 거에요",
          "speaker_id": "1",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100017088.1.8",
          "form": "네 ㅋㅋㅋ 저도 저녁먹었어요! 그래서 일단 저는 뭐라도하려고 알바나 계약직자리에 이력서내보고있는 중이에요",
          "original_form": "네 ㅋㅋㅋ 저도 저녁먹었어요! 그래서 일단 저는 뭐라도하려고 알바나 계약직자리에 이력서내보고있는 중이에요",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017088.1.9",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017088.1.10",
          "form": "제 백수 시절을 돌이켜보니까",
          "original_form": "제 백수 시절을 돌이켜보니까",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017088.1.11",
          "form": "자존감이 제일 중요한 것 같아요",
          "original_form": "자존감이 제일 중요한 것 같아요",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017088.1.12",
          "form": "난 잘하고 있다",
          "original_form": "난 잘하고 있다",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017088.1.13",
          "form": "이력서 떨어질 수도 있지",
          "original_form": "이력서 떨어질 수도 있지",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017088.1.14",
          "form": "잘했다",
          "original_form": "잘했다",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017088.1.15",
          "form": "이렇게 많이 칭찬해주세요",
          "original_form": "이렇게 많이 칭찬해주세요",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017088.1.16",
          "form": "ㅠㅠ 좋은말 감사해요",
          "original_form": "ㅠㅠ 좋은말 감사해요",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017088.1.17",
          "form": "알바붙으면 자격증공부하려구요 물론 지금도 하고있지만요 ㅋㅋㅋ",
          "original_form": "알바붙으면 자격증공부하려구요 물론 지금도 하고있지만요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017088.1.18",
          "form": "그래요 ㅎㅎ",
          "original_form": "그래요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017088.1.19",
          "form": "무슨 자격증 공부하세요?",
          "original_form": "무슨 자격증 공부하세요?",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017088.1.20",
          "form": "전산회계자격증공부해요",
          "original_form": "전산회계자격증공부해요",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017088.1.21",
          "form": "오 어려운 거 하시네요",
          "original_form": "오 어려운 거 하시네요",
          "speaker_id": "1",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100017088.1.22",
          "form": "비전공자도 할만하다해서 하는데 생각보다 쉽진않더라구요 돈계산에 약해서 ㅋㅋ",
          "original_form": "비전공자도 할만하다해서 하는데 생각보다 쉽진않더라구요 돈계산에 약해서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017088.1.23",
          "form": "꾸준히 하면 되겠죠 ㅎㅎ",
          "original_form": "꾸준히 하면 되겠죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017088.1.24",
          "form": "제 친구도 회계공부하는데 반갑네요",
          "original_form": "제 친구도 회계공부하는데 반갑네요",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017088.1.25",
          "form": "네! ㅋㅋㅋ 더 많이 보면 될것같아요",
          "original_form": "네! ㅋㅋㅋ 더 많이 보면 될것같아요",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017088.1.26",
          "form": "오 ㅎㅎ 고생이 많으세요",
          "original_form": "오 ㅎㅎ 고생이 많으세요",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017088.1.27",
          "form": "스터디카페 다니세요?",
          "original_form": "스터디카페 다니세요?",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017088.1.28",
          "form": "지금은 코로나때문에 자주는 안가고 예전엔 자주 갔어요 요즘은 집중안될때 가끔가요",
          "original_form": "지금은 코로나때문에 자주는 안가고 예전엔 자주 갔어요 요즘은 집중안될때 가끔가요",
          "speaker_id": "2",
          "time": "20210826 21:17"
        }
      ]
    }
