      "utterance": [
        {
          "id": "MDRW2100030479.1.1",
          "form": "그리고... 혹시",
          "original_form": "그리고... 혹시",
          "speaker_id": "1",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100030479.1.2",
          "form": "요즘 모기들은",
          "original_form": "요즘 모기들은",
          "speaker_id": "1",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100030479.1.3",
          "form": "소리도 안내는거 아시나요?",
          "original_form": "소리도 안내는거 아시나요?",
          "speaker_id": "1",
          "time": "20210904 01:51"
        },
        {
          "id": "MDRW2100030479.1.4",
          "form": "앗ㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:51"
        },
        {
          "id": "MDRW2100030479.1.5",
          "form": "맞아요 소리소문없이",
          "original_form": "맞아요 소리소문없이",
          "speaker_id": "2",
          "time": "20210904 01:51"
        },
        {
          "id": "MDRW2100030479.1.6",
          "form": "잔뜩 물리더라구요",
          "original_form": "잔뜩 물리더라구요",
          "speaker_id": "2",
          "time": "20210904 01:51"
        },
        {
          "id": "MDRW2100030479.1.7",
          "form": "그쵸!!!!",
          "original_form": "그쵸!!!!",
          "speaker_id": "1",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100030479.1.8",
          "form": "아니 가을모기가 그런가?",
          "original_form": "아니 가을모기가 그런가?",
          "speaker_id": "1",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100030479.1.9",
          "form": "작년에도 느꼈거든요.",
          "original_form": "작년에도 느꼈거든요.",
          "speaker_id": "1",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100030479.1.10",
          "form": "모기들이 아주 응큼해요!",
          "original_form": "모기들이 아주 응큼해요!",
          "speaker_id": "1",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100030479.1.11",
          "form": "앜ㅋㅋㅋㅋㅋ맞아요",
          "original_form": "앜ㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100030479.1.12",
          "form": "물린부위도",
          "original_form": "물린부위도",
          "speaker_id": "2",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100030479.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100030479.1.14",
          "form": "엄천",
          "original_form": "엄천",
          "speaker_id": "1",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100030479.1.15",
          "form": "엄청 붓지 않나요...?",
          "original_form": "엄청 붓지 않나요...?",
          "speaker_id": "1",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100030479.1.16",
          "form": "아니다 근데 여름모기보다는 가을모기가",
          "original_form": "아니다 근데 여름모기보다는 가을모기가",
          "speaker_id": "2",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100030479.1.17",
          "form": "빨리 가라앉지않나요?",
          "original_form": "빨리 가라앉지않나요?",
          "speaker_id": "2",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100030479.1.18",
          "form": "붓기는 엄청 붓긴하더라구요ㅋㅋㅋ",
          "original_form": "붓기는 엄청 붓긴하더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100030479.1.19",
          "form": "그런 것 같기도 하고요....? 으아아",
          "original_form": "그런 것 같기도 하고요....? 으아아",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030479.1.20",
          "form": "저는 제일 무서울 때가",
          "original_form": "저는 제일 무서울 때가",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030479.1.21",
          "form": "청바지도 뚫고 물렸을 때,,,",
          "original_form": "청바지도 뚫고 물렸을 때,,,",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030479.1.22",
          "form": "헉 그러기도하나요? ㄷㄷ",
          "original_form": "헉 그러기도하나요? ㄷㄷ",
          "speaker_id": "2",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030479.1.23",
          "form": "네,,",
          "original_form": "네,,",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030479.1.24",
          "form": "진짜 굳센 모기네요..",
          "original_form": "진짜 굳센 모기네요..",
          "speaker_id": "2",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030479.1.25",
          "form": "아 제 기분탓일수도 있지만...",
          "original_form": "아 제 기분탓일수도 있지만...",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030479.1.26",
          "form": "분명히 옷이 가린 부위인데!!!",
          "original_form": "분명히 옷이 가린 부위인데!!!",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030479.1.27",
          "form": "양말을 뚫린적은 있는 것 같아요",
          "original_form": "양말을 뚫린적은 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030479.1.28",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100030479.1.29",
          "form": "근데 산같은 곳이면 이해하는데 왜 카페같은 곳에서도",
          "original_form": "근데 산같은 곳이면 이해하는데 왜 카페같은 곳에서도",
          "speaker_id": "2",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100030479.1.30",
          "form": "또 나쁜말이라고 하네요 ",
          "original_form": "또 나쁜말이라고 하네요 ",
          "speaker_id": "1",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100030479.1.31",
          "form": "막 물리는걸까요",
          "original_form": "막 물리는걸까요",
          "speaker_id": "2",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100030479.1.32",
          "form": "아 그니까요!!!",
          "original_form": "아 그니까요!!!",
          "speaker_id": "1",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100030479.1.33",
          "form": "엥ㅜ..",
          "original_form": "엥ㅜ..",
          "speaker_id": "2",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100030479.1.34",
          "form": "정말 미스테리예요...",
          "original_form": "정말 미스테리예요...",
          "speaker_id": "1",
          "time": "20210904 01:55"
        }
      ]
    }
