      "utterance": [
        {
          "id": "MDRW2100023136.1.1",
          "form": "근데 마사지건도 친구가 사놓고 안쓰길랴",
          "original_form": "근데 마사지건도 친구가 사놓고 안쓰길랴",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100023136.1.2",
          "form": "어머머 네네",
          "original_form": "어머머 네네",
          "speaker_id": "1",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100023136.1.3",
          "form": "저것도 마찬가지구나하죠",
          "original_form": "저것도 마찬가지구나하죠",
          "speaker_id": "2",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023136.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023136.1.5",
          "form": "아 ㅋㅋ 바로 창고행이구나",
          "original_form": "아 ㅋㅋ 바로 창고행이구나",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023136.1.6",
          "form": "그럼 안사야겠네요 마사지건은 ㅎㅎ",
          "original_form": "그럼 안사야겠네요 마사지건은 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023136.1.7",
          "form": "그냥 한번씩 안마의자 가서 누워잇는게 젤 편하겠던데요",
          "original_form": "그냥 한번씩 안마의자 가서 누워잇는게 젤 편하겠던데요",
          "speaker_id": "2",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023136.1.8",
          "form": "혹시 그럼 저주파 마시지기기는요?",
          "original_form": "혹시 그럼 저주파 마시지기기는요?",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023136.1.9",
          "form": "그건 써보긴 했는게 막 특별히 좋은진 모르겟어요",
          "original_form": "그건 써보긴 했는게 막 특별히 좋은진 모르겟어요",
          "speaker_id": "2",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023136.1.10",
          "form": "아 ㅠㅠ 뭐야 그럼 바이럴 마케팅인가보다",
          "original_form": "아 ㅠㅠ 뭐야 그럼 바이럴 마케팅인가보다",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023136.1.11",
          "form": "EMS마사지기기에요 아ㅣ니면 저주파?",
          "original_form": "EMS마사지기기에요 아ㅣ니면 저주파?",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023136.1.12",
          "form": "저주파였어요 의료용",
          "original_form": "저주파였어요 의료용",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023136.1.13",
          "form": "오 의료기기인데도 별로였어요?",
          "original_form": "오 의료기기인데도 별로였어요?",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023136.1.14",
          "form": "ems는 안써봣어요",
          "original_form": "ems는 안써봣어요",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023136.1.15",
          "form": "제가 찾아보니까 의료기기 아닌 것들 대다수더라구요",
          "original_form": "제가 찾아보니까 의료기기 아닌 것들 대다수더라구요",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023136.1.16",
          "form": "구냥 노인들한텐 좋은가",
          "original_form": "구냥 노인들한텐 좋은가",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023136.1.17",
          "form": "울 엄마는 시원하다고 그러시던데 ㅜㅜㅜ",
          "original_form": "울 엄마는 시원하다고 그러시던데 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023136.1.18",
          "form": "젊은이한테는 크게 효능이 없는거 같은 ㅋㅋㅋ",
          "original_form": "젊은이한테는 크게 효능이 없는거 같은 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023136.1.19",
          "form": "제 아는 동생이 그거 배에다 붙이면 복근 생긴데요...",
          "original_form": "제 아는 동생이 그거 배에다 붙이면 복근 생긴데요...",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023136.1.20",
          "form": "그래서 살가말까 고민 했어요",
          "original_form": "그래서 살가말까 고민 했어요",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023136.1.21",
          "form": "앜ㅋㅋㅋㅋ진짜요ㅋㅋㅋㅋ해볼걸 그랫나ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ진짜요ㅋㅋㅋㅋ해볼걸 그랫나ㅋㅋㅋ",
