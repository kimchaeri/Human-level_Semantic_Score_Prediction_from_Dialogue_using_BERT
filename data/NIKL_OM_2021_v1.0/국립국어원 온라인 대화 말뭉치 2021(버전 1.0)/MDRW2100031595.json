      "utterance": [
        {
          "id": "MDRW2100031595.1.1",
          "form": "ㅠㅠㅠ그쵸",
          "original_form": "ㅠㅠㅠ그쵸",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100031595.1.2",
          "form": "진짜 왜 욕먹는지 이햐 안갔어요",
          "original_form": "진짜 왜 욕먹는지 이햐 안갔어요",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100031595.1.3",
          "form": "연기 잘하고 완전 찰떡이구만..",
          "original_form": "연기 잘하고 완전 찰떡이구만..",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100031595.1.4",
          "form": "저도요 진짜 김태리님 ,,,",
          "original_form": "저도요 진짜 김태리님 ,,,",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100031595.1.5",
          "form": "뽀야미,,너무 좋아요",
          "original_form": "뽀야미,,너무 좋아요",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100031595.1.6",
          "form": "리틀포레스트도 보고 미스터 션샤인도 재밌게 봤어요 ㅠㅠ",
          "original_form": "리틀포레스트도 보고 미스터 션샤인도 재밌게 봤어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:02"
        },
        {
          "id": "MDRW2100031595.1.7",
          "form": "으앗 저도요 ㅠ! 저 리틀포레스트가 제 인생 영화거든요",
          "original_form": "으앗 저도요 ㅠ! 저 리틀포레스트가 제 인생 영화거든요",
          "speaker_id": "1",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100031595.1.8",
          "form": "영화에서 나오는 음식 다 먹어보고 싶지 않나요?",
          "original_form": "영화에서 나오는 음식 다 먹어보고 싶지 않나요?",
          "speaker_id": "1",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100031595.1.9",
          "form": "어머 !!><",
          "original_form": "어머 !!><",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100031595.1.10",
          "form": "진짜 다 해먹고 싶어요",
          "original_form": "진짜 다 해먹고 싶어요",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100031595.1.11",
          "form": "분위기가 진짜 최고예요 ㅠㅠㅠㅠㅠ",
          "original_form": "분위기가 진짜 최고예요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100031595.1.12",
          "form": "아 그니까요! 저 야채 별로 안 좋아하는데듀 배추잔은 너무 먹고 싶더라고요!!",
          "original_form": "아 그니까요! 저 야채 별로 안 좋아하는데듀 배추잔은 너무 먹고 싶더라고요!!",
          "speaker_id": "1",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100031595.1.13",
          "form": "ㅠㅠㅠ진짜 음식 색깔도 너무 예쁘더라구요",
          "original_form": "ㅠㅠㅠ진짜 음식 색깔도 너무 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031595.1.14",
          "form": "못하는 음식도 없고 뭐든 척척,,",
          "original_form": "못하는 음식도 없고 뭐든 척척,,",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031595.1.15",
          "form": "부러웠어요 진짜",
          "original_form": "부러웠어요 진짜",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031595.1.16",
          "form": "그니까요! 글고 손이 야무져서? 더 맛나 보리는 것 같앙ㅅ",
          "original_form": "그니까요! 글고 손이 야무져서? 더 맛나 보리는 것 같앙ㅅ",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031595.1.17",
          "form": "강아지도 너무 귀엽지 않나요@",
          "original_form": "강아지도 너무 귀엽지 않나요@",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031595.1.18",
          "form": "오구..",
          "original_form": "오구..",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031595.1.19",
          "form": "마자요마자요!!ㅠㅠㅠ",
          "original_form": "마자요마자요!!ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031595.1.20",
          "form": "오구,,ㅠㅠ",
          "original_form": "오구,,ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031595.1.21",
          "form": "크는게 보이더라구요 ㅋㅋ큐ㅠ귀여웡",
          "original_form": "크는게 보이더라구요 ㅋㅋ큐ㅠ귀여웡",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031595.1.22",
          "form": "그니까요 ㅋㅋㅋㅋㅋ 애기 때 모습 넘 귀여워요",
          "original_form": "그니까요 ㅋㅋㅋㅋㅋ 애기 때 모습 넘 귀여워요",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031595.1.23",
          "form": "글구 그 진기주 배우? 도 너무 귀여운 것 같아요",
          "original_form": "글구 그 진기주 배우? 도 너무 귀여운 것 같아요",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031595.1.24",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031595.1.25",
          "form": "마자요 저는 기주님 스펙보고 또한번 놀랐어요",
          "original_form": "마자요 저는 기주님 스펙보고 또한번 놀랐어요",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031595.1.26",
          "form": "삼성의 딸 ㄷㄷ",
          "original_form": "삼성의 딸 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031595.1.27",
          "form": "헐 그러니까요... 저였음... 배우 도전을 할 수 있었을까",
          "original_form": "헐 그러니까요... 저였음... 배우 도전을 할 수 있었을까",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031595.1.28",
          "form": "싶었다니까요...ㅁ",
          "original_form": "싶었다니까요...ㅁ",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031595.1.29",
          "form": "기자도 했다고 하지 않았나요?",
          "original_form": "기자도 했다고 하지 않았나요?",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031595.1.30",
          "form": "저두요,, 모델도 하셨대요",
          "original_form": "저두요,, 모델도 하셨대요",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031595.1.31",
          "form": "기자도 하시고,,",
          "original_form": "기자도 하시고,,",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031595.1.32",
          "form": "더 대단해보여요",
          "original_form": "더 대단해보여요",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031595.1.33",
          "form": "그니까요!!!!",
          "original_form": "그니까요!!!!",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031595.1.34",
          "form": "그래서 연기도 잘하나 싶은..!!!",
          "original_form": "그래서 연기도 잘하나 싶은..!!!",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031595.1.35",
          "form": "보보경심 보샸나요@?",
          "original_form": "보보경심 보샸나요@?",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031595.1.36",
          "form": "거기도 나왔던 거 같은데",
          "original_form": "거기도 나왔던 거 같은데",
          "speaker_id": "1",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100031595.1.37",
          "form": "보보경심에 기주님이 나왔나요??",
          "original_form": "보보경심에 기주님이 나왔나요??",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031595.1.38",
          "form": "그 아이유 하녀가 기주님 아녔..느요?!?!?!",
          "original_form": "그 아이유 하녀가 기주님 아녔..느요?!?!?!",
          "speaker_id": "1",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031595.1.39",
          "form": "헐어어엉ㄹ",
          "original_form": "헐어어엉ㄹ",
          "speaker_id": "2",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031595.1.40",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031595.1.41",
          "form": "이준기랑 강하늘만 생각났어요ㅋㅋㅋ",
          "original_form": "이준기랑 강하늘만 생각났어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031595.1.42",
          "form": "보보경심 보면서 진짜 엄청 울었는데",
          "original_form": "보보경심 보면서 진짜 엄청 울었는데",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031595.1.43",
          "form": "저 올해 보보경심 봣거든요?",
          "original_form": "저 올해 보보경심 봣거든요?",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031595.1.44",
          "form": "거짓말 안 하고 3일내내 울었어요",
          "original_form": "거짓말 안 하고 3일내내 울었어요",
          "speaker_id": "1",
          "time": "20210904 23:27"
        }
