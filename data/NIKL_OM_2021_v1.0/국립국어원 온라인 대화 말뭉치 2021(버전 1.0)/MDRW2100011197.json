        },
        {
          "id": "MDRW2100011197.1.3",
          "form": "학생이새요?",
          "original_form": "학생이새요?",
          "speaker_id": "2",
          "time": "20210824 03:52"
        },
        {
          "id": "MDRW2100011197.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100011197.1.5",
          "form": "퐁퐁님은요",
          "original_form": "퐁퐁님은요",
          "speaker_id": "1",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100011197.1.6",
          "form": "대학생이에요",
          "original_form": "대학생이에요",
          "speaker_id": "2",
          "time": "20210825 19:01"
        },
        {
          "id": "MDRW2100011197.1.7",
          "form": "개강 했나요?",
          "original_form": "개강 했나요?",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100011197.1.8",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100011197.1.9",
          "form": "일주일 남았어요 ㅠ",
          "original_form": "일주일 남았어요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100011197.1.10",
          "form": "동아리 활동 하시나요?",
          "original_form": "동아리 활동 하시나요?",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100011197.1.11",
          "form": "아니여 ㅠ 할걸 그랬어요",
          "original_form": "아니여 ㅠ 할걸 그랬어요",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100011197.1.12",
          "form": "이제와서 아까워요",
          "original_form": "이제와서 아까워요",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100011197.1.13",
          "form": "수강 신청은 어땠나요?",
          "original_form": "수강 신청은 어땠나요?",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100011197.1.14",
          "form": "전공필수 노잼과목만 들어야해서",
          "original_form": "전공필수 노잼과목만 들어야해서",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100011197.1.15",
          "form": "다른 애들 다끝나고 12시에 일어나서 했어요",
          "original_form": "다른 애들 다끝나고 12시에 일어나서 했어요",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100011197.1.16",
          "form": "학교생활 만족하시나요?",
          "original_form": "학교생활 만족하시나요?",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100011197.1.17",
          "form": "그럭저럭 좋아요",
          "original_form": "그럭저럭 좋아요",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100011197.1.18",
          "form": "교수님은 좀 싫고..",
          "original_form": "교수님은 좀 싫고..",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100011197.1.19",
