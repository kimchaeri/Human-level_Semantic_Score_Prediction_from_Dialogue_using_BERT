      "utterance": [
        {
          "id": "MDRW2100020560.1.1",
          "form": "저두요 왜 그걸 몰랐지?",
          "original_form": "저두요 왜 그걸 몰랐지?",
          "speaker_id": "2",
          "time": "20210828 09:46"
        },
        {
          "id": "MDRW2100020560.1.2",
          "form": "ㅋㅋ 그니까요",
          "original_form": "ㅋㅋ 그니까요",
          "speaker_id": "1",
          "time": "20210828 09:46"
        },
        {
          "id": "MDRW2100020560.1.3",
          "form": "소세지 하나를 썬거라는 생각을 못했어요ㅋㅋㅋ",
          "original_form": "소세지 하나를 썬거라는 생각을 못했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:46"
        },
        {
          "id": "MDRW2100020560.1.4",
          "form": "어떤빵 좋아하세요",
          "original_form": "어떤빵 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 09:46"
        },
        {
          "id": "MDRW2100020560.1.5",
          "form": "저는 버터프레첼 좋아합니다",
          "original_form": "저는 버터프레첼 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 09:47"
        },
        {
          "id": "MDRW2100020560.1.6",
          "form": "스콘이나",
          "original_form": "스콘이나",
          "speaker_id": "2",
          "time": "20210828 09:47"
        },
        {
          "id": "MDRW2100020560.1.7",
          "form": "치아버터나",
          "original_form": "치아버터나",
          "speaker_id": "2",
          "time": "20210828 09:47"
        },
        {
          "id": "MDRW2100020560.1.8",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210828 09:47"
        },
        {
          "id": "MDRW2100020560.1.9",
          "form": "너무 맛있어 약간",
          "original_form": "너무 맛있어 약간",
          "speaker_id": "1",
          "time": "20210828 09:47"
        },
        {
          "id": "MDRW2100020560.1.10",
          "form": "약간 목이 막히는 빵이요",
          "original_form": "약간 목이 막히는 빵이요",
          "speaker_id": "2",
          "time": "20210828 09:47"
        },
        {
          "id": "MDRW2100020560.1.11",
          "form": "풍미가 깊은..",
          "original_form": "풍미가 깊은..",
          "speaker_id": "1",
          "time": "20210828 09:47"
        },
        {
          "id": "MDRW2100020560.1.12",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "1",
          "time": "20210828 09:47"
        },
        {
          "id": "MDRW2100020560.1.13",
          "form": "후후",
          "original_form": "후후",
          "speaker_id": "2",
          "time": "20210828 09:47"
        },
        {
          "id": "MDRW2100020560.1.14",
          "form": "저 어제 파리바게트 갔거든요",
          "original_form": "저 어제 파리바게트 갔거든요",
          "speaker_id": "1",
          "time": "20210828 09:47"
        },
        {
          "id": "MDRW2100020560.1.15",
          "form": "요기요할인해서",
          "original_form": "요기요할인해서",
          "speaker_id": "1",
          "time": "20210828 09:47"
        },
        {
          "id": "MDRW2100020560.1.16",
          "form": "제가 빵사들고 집에가면 다들 뭐 이런 빵을 사오냐고 하죠",
          "original_form": "제가 빵사들고 집에가면 다들 뭐 이런 빵을 사오냐고 하죠",
          "speaker_id": "2",
          "time": "20210828 09:48"
        },
        {
          "id": "MDRW2100020560.1.17",
          "form": "아 맞아요 어제 할인했죠",
          "original_form": "아 맞아요 어제 할인했죠",
          "speaker_id": "2",
          "time": "20210828 09:48"
        },
        {
          "id": "MDRW2100020560.1.18",
          "form": "ㅋㅋㅋ 아닌데 다 맛있는건데",
          "original_form": "ㅋㅋㅋ 아닌데 다 맛있는건데",
          "speaker_id": "1",
          "time": "20210828 09:48"
        },
        {
          "id": "MDRW2100020560.1.19",
          "form": "왜 그러셨지",
          "original_form": "왜 그러셨지",
          "speaker_id": "1",
          "time": "20210828 09:48"
        },
        {
          "id": "MDRW2100020560.1.20",
          "form": "그니까요 ㅠ 많이 골라오셨나요",
          "original_form": "그니까요 ㅠ 많이 골라오셨나요",
          "speaker_id": "2",
          "time": "20210828 09:48"
        },
        {
          "id": "MDRW2100020560.1.21",
          "form": "진짜 오랜만에 빵 원없이 담은거같아요",
          "original_form": "진짜 오랜만에 빵 원없이 담은거같아요",
          "speaker_id": "1",
          "time": "20210828 09:48"
        }
      ]
