        {
          "id": "MDRW2100031784.1.1",
          "form": "중식은 좋아하세요?",
          "original_form": "중식은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100031784.1.2",
          "form": "네 종종먹어요",
          "original_form": "네 종종먹어요",
          "speaker_id": "2",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100031784.1.3",
          "form": "짜장면 ㅡ짬뽕",
          "original_form": "짜장면 ㅡ짬뽕",
          "speaker_id": "1",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100031784.1.4",
          "form": "짬뽕 좋아해요",
          "original_form": "짬뽕 좋아해요",
          "speaker_id": "2",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100031784.1.5",
          "form": "오 매운거잘드시나요?!",
          "original_form": "오 매운거잘드시나요?!",
          "speaker_id": "1",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100031784.1.6",
          "form": "너무 맵지만 않으면 먹는것같아요",
          "original_form": "너무 맵지만 않으면 먹는것같아요",
          "speaker_id": "2",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100031784.1.7",
          "form": "탕수육도 좋아하시나요?",
          "original_form": "탕수육도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100031784.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100031784.1.9",
          "form": "냉면은",
          "original_form": "냉면은",
          "speaker_id": "1",
          "time": "20210905 00:26"
        },
        {
          "id": "MDRW2100031784.1.10",
          "form": "물냉 ㅡ비냉",
          "original_form": "물냉 ㅡ비냉",
          "speaker_id": "1",
          "time": "20210905 00:26"
        },
