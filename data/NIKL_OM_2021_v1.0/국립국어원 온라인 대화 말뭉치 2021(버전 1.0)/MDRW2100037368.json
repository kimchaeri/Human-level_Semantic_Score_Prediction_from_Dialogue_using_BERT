      "utterance": [
        {
          "id": "MDRW2100037368.1.1",
          "form": "맞아요!!! 그리고 그 이광수 특유의",
          "original_form": "맞아요!!! 그리고 그 이광수 특유의",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037368.1.2",
          "form": "그 눈 돈...?",
          "original_form": "그 눈 돈...?",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037368.1.3",
          "form": "행동이 너무 웃걍7",
          "original_form": "행동이 너무 웃걍7",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037368.1.4",
          "form": "아 ㅋㅋㄱㄱ",
          "original_form": "아 ㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037368.1.5",
          "form": "맞아욬ㅋㄱㄱㅋㄱ",
          "original_form": "맞아욬ㅋㄱㄱㅋㄱ",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037368.1.6",
          "form": "진짜 제가 본 사람중에",
          "original_form": "진짜 제가 본 사람중에",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037368.1.7",
          "form": "젤 웃긴 사람이에여 ㅋㄱㅋ",
          "original_form": "젤 웃긴 사람이에여 ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037368.1.8",
          "form": "그 막 유튜브 클립으로도 모여있잖아요",
          "original_form": "그 막 유튜브 클립으로도 모여있잖아요",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037368.1.9",
          "form": "그거 볼때마다 울듯이 웃어요 ㅠ.^",
          "original_form": "그거 볼때마다 울듯이 웃어요 ㅠ.^",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037368.1.10",
          "form": "아 마자옄ㅋㅋㅋ",
          "original_form": "아 마자옄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037368.1.11",
          "form": "유튜브도 자주 보시는군요 !",
          "original_form": "유튜브도 자주 보시는군요 !",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037368.1.12",
          "form": "네네 저 완전 유튜브에 푹 빠져 살아요",
          "original_form": "네네 저 완전 유튜브에 푹 빠져 살아요",
          "speaker_id": "2",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100037368.1.13",
          "form": "name1님은요!",
          "original_form": "&name1&님은요!",
          "speaker_id": "2",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100037368.1.14",
          "form": "저두 하루의 절반이",
          "original_form": "저두 하루의 절반이",
          "speaker_id": "1",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100037368.1.15",
          "form": "유튭이에요!",
          "original_form": "유튭이에요!",
          "speaker_id": "1",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100037368.1.16",
          "form": "오 주로 뭐뭐 보세요옹",
          "original_form": "오 주로 뭐뭐 보세요옹",
          "speaker_id": "2",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100037368.1.17",
          "form": "전 직캠도 보구",
          "original_form": "전 직캠도 보구",
          "speaker_id": "1",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100037368.1.18",
          "form": "먹방도 보구..",
          "original_form": "먹방도 보구..",
          "speaker_id": "1",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100037368.1.19",
          "form": "5분 순삭도 가끔 봐요 ㅋㅋㅋ",
          "original_form": "5분 순삭도 가끔 봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100037368.1.20",
          "form": "교차편집도 보구요",
          "original_form": "교차편집도 보구요",
          "speaker_id": "1",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100037368.1.21",
          "form": "헐헐 5분 순삭 저도 봐요",
          "original_form": "헐헐 5분 순삭 저도 봐요",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037368.1.22",
          "form": "그거 막 하이킥 해주잖아요!",
          "original_form": "그거 막 하이킥 해주잖아요!",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037368.1.23",
          "form": "마자요 !!!",
          "original_form": "마자요 !!!",
          "speaker_id": "1",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037368.1.24",
          "form": "하이킥 재밌자나요 ㅜㅜ",
          "original_form": "하이킥 재밌자나요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100037368.1.25",
          "form": "하이킥 시리즈 중에 뭐 제일 좋아하세요!?",
          "original_form": "하이킥 시리즈 중에 뭐 제일 좋아하세요!?",
          "speaker_id": "2",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100037368.1.26",
          "form": "전 웃긴건 거침없이 , 자주본건 지붕뚫고 입니다 !",
          "original_form": "전 웃긴건 거침없이 , 자주본건 지붕뚫고 입니다 !",
