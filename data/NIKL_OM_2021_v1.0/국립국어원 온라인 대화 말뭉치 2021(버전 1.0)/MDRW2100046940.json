        },
        {
          "id": "MDRW2100046940.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211108 13:18"
        },
        {
          "id": "MDRW2100046940.1.3",
          "form": "운동하시는거 좋아하시나요?",
          "original_form": "운동하시는거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20211108 13:18"
        },
        {
          "id": "MDRW2100046940.1.4",
          "form": "운동은 별로 안좋아해요",
          "original_form": "운동은 별로 안좋아해요",
          "speaker_id": "1",
          "time": "20211108 13:22"
        },
        {
          "id": "MDRW2100046940.1.5",
          "form": "ㅎ 사실 저두^^",
          "original_form": "ㅎ 사실 저두^^",
          "speaker_id": "2",
          "time": "20211108 13:24"
        },
        {
          "id": "MDRW2100046940.1.6",
          "form": "운동 좋아하고싶은데",
          "original_form": "운동 좋아하고싶은데",
          "speaker_id": "2",
          "time": "20211108 13:24"
        },
        {
          "id": "MDRW2100046940.1.7",
          "form": "정말 어렵네요..",
          "original_form": "정말 어렵네요..",
          "speaker_id": "2",
          "time": "20211108 13:25"
        },
        {
          "id": "MDRW2100046940.1.8",
          "form": "요즘은 코로나때문에 더욱 운동하기가 힘든거같아요",
          "original_form": "요즘은 코로나때문에 더욱 운동하기가 힘든거같아요",
          "speaker_id": "1",
          "time": "20211108 13:27"
        },
        {
          "id": "MDRW2100046940.1.9",
          "form": "정말요 ㅠㅠ",
          "original_form": "정말요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046940.1.10",
          "form": "걷기랑 스텝퍼..",
          "original_form": "걷기랑 스텝퍼..",
          "speaker_id": "2",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046940.1.11",
          "form": "가끔 엘레베이터 안타고",
          "original_form": "가끔 엘레베이터 안타고",
          "speaker_id": "2",
          "time": "20211108 13:36"
        },
        {
          "id": "MDRW2100046940.1.12",
          "form": "계단 올라가기",
          "original_form": "계단 올라가기",
          "speaker_id": "2",
          "time": "20211108 13:37"
        },
        {
          "id": "MDRW2100046940.1.13",
          "form": "이정도만 하네요",
          "original_form": "이정도만 하네요",
          "speaker_id": "2",
          "time": "20211108 13:37"
        },
        {
          "id": "MDRW2100046940.1.14",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20211108 13:37"
        },
        {
          "id": "MDRW2100046940.1.15",
          "form": "전 계단도 잘 안올라가요",
          "original_form": "전 계단도 잘 안올라가요",
          "speaker_id": "1",
          "time": "20211108 14:17"
        },
        {
          "id": "MDRW2100046940.1.16",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20211108 14:17"
        },
        {
          "id": "MDRW2100046940.1.17",
          "form": "숨쉬기운동만해요",
          "original_form": "숨쉬기운동만해요",
          "speaker_id": "1",
          "time": "20211108 14:17"
        },
        {
          "id": "MDRW2100046940.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 14:20"
        },
        {
          "id": "MDRW2100046940.1.19",
          "form": "스포츠 보는 건 좋아하세욤?",
          "original_form": "스포츠 보는 건 좋아하세욤?",
          "speaker_id": "2",
          "time": "20211108 14:20"
        },
        {
          "id": "MDRW2100046940.1.20",
          "form": "스포츠도 잘안봐요",
          "original_form": "스포츠도 잘안봐요",
          "speaker_id": "1",
          "time": "20211108 14:22"
        },
        {
          "id": "MDRW2100046940.1.21",
          "form": "올림픽도 안봐요 ㅋ",
          "original_form": "올림픽도 안봐요 ㅋ",
          "speaker_id": "1",
          "time": "20211108 14:22"
        },
        {
          "id": "MDRW2100046940.1.22",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20211108 14:25"
        },
        {
          "id": "MDRW2100046940.1.23",
          "form": "이번에 올림픽도 안보셨나요?",
          "original_form": "이번에 올림픽도 안보셨나요?",
          "speaker_id": "2",
          "time": "20211108 14:25"
        },
        {
          "id": "MDRW2100046940.1.24",
          "form": "네 실황중개는 안보고 다끝나고 결과만 뉴스로 봤어요",
          "original_form": "네 실황중개는 안보고 다끝나고 결과만 뉴스로 봤어요",
          "speaker_id": "1",
          "time": "20211108 14:27"
        },
        {
          "id": "MDRW2100046940.1.25",
          "form": "호오",
          "original_form": "호오",
          "speaker_id": "2",
          "time": "20211108 14:28"
        },
        {
          "id": "MDRW2100046940.1.26",
          "form": "저도 사실 막 엄청 챙겨보는 스타일은 아닌데",
          "original_form": "저도 사실 막 엄청 챙겨보는 스타일은 아닌데",
          "speaker_id": "2",
          "time": "20211108 14:28"
        },
        {
          "id": "MDRW2100046940.1.27",
          "form": "여자배구는 진짜 재밌었는데 ㅋㅋㅋㅋ",
          "original_form": "여자배구는 진짜 재밌었는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 14:28"
        },
        {
          "id": "MDRW2100046940.1.28",
          "form": "터키전이 진짜 재밌었는데",
          "original_form": "터키전이 진짜 재밌었는데",
          "speaker_id": "2",
          "time": "20211108 14:28"
        },
        {
          "id": "MDRW2100046940.1.29",
          "form": "시간 나면 한 번 봐보세요 ㅎㅎ",
          "original_form": "시간 나면 한 번 봐보세요 ㅎㅎ",
