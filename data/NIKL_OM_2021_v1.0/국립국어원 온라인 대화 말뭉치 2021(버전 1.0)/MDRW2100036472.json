      "utterance": [
        {
          "id": "MDRW2100036472.1.1",
          "form": "어제오늘 날씨가 정말 좋아요!!",
          "original_form": "어제오늘 날씨가 정말 좋아요!!",
          "speaker_id": "1",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100036472.1.2",
          "form": "맞아요 비도 안오고 날씨도 그렇게까진 안추워요",
          "original_form": "맞아요 비도 안오고 날씨도 그렇게까진 안추워요",
          "speaker_id": "2",
          "time": "20210904 19:48"
        },
        {
          "id": "MDRW2100036472.1.3",
          "form": "네 하늘도 정말 파랗고",
          "original_form": "네 하늘도 정말 파랗고",
          "speaker_id": "1",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100036472.1.4",
          "form": "구름도 뭉게뭉게 예쁘고",
          "original_form": "구름도 뭉게뭉게 예쁘고",
          "speaker_id": "1",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100036472.1.5",
          "form": "그러니까요 name1님은 좋아하시는 계절 있으신가요?",
          "original_form": "그러니까요 &name1&님은 좋아하시는 계절 있으신가요?",
          "speaker_id": "2",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100036472.1.6",
          "form": "저는 봄 좋아했는데",
          "original_form": "저는 봄 좋아했는데",
          "speaker_id": "1",
          "time": "20210904 19:50"
        },
        {
          "id": "MDRW2100036472.1.7",
          "form": "요즘은 가을도 좋아요ㅎㅎ",
          "original_form": "요즘은 가을도 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 19:50"
        },
        {
          "id": "MDRW2100036472.1.8",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210904 19:50"
        },
        {
          "id": "MDRW2100036472.1.9",
          "form": "저는 겨울이요",
          "original_form": "저는 겨울이요",
          "speaker_id": "2",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100036472.1.10",
          "form": "오 겨울을 좋아하시는 이유가 있나요?",
          "original_form": "오 겨울을 좋아하시는 이유가 있나요?",
          "speaker_id": "1",
          "time": "20210904 19:52"
        },
        {
          "id": "MDRW2100036472.1.11",
          "form": "저는 땀안나고 벌레없고 먼지많이 없어서요",
          "original_form": "저는 땀안나고 벌레없고 먼지많이 없어서요",
          "speaker_id": "2",
          "time": "20210904 19:53"
        },
        {
          "id": "MDRW2100036472.1.12",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210904 19:53"
        },
        {
          "id": "MDRW2100036472.1.13",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210904 19:54"
        },
        {
          "id": "MDRW2100036472.1.14",
          "form": "저는 밖에 걸어다니기 좋은 기온",
          "original_form": "저는 밖에 걸어다니기 좋은 기온",
          "speaker_id": "1",
          "time": "20210904 19:54"
        },
        {
          "id": "MDRW2100036472.1.15",
          "form": "봄꽃이나 단풍도 예쁘고요",
          "original_form": "봄꽃이나 단풍도 예쁘고요",
          "speaker_id": "1",
          "time": "20210904 19:55"
        },
        {
          "id": "MDRW2100036472.1.16",
          "form": "계절 좋으실 때 산책하시는거 좋아하시나봐요",
          "original_form": "계절 좋으실 때 산책하시는거 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210904 19:56"
        },
        {
          "id": "MDRW2100036472.1.17",
          "form": "네 저녁즈음 선선한 바람 맞으며 걷는 것 좋아해요ㅎㅎ",
          "original_form": "네 저녁즈음 선선한 바람 맞으며 걷는 것 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 19:57"
        },
        {
          "id": "MDRW2100036472.1.18",
          "form": "그럼 추석때쯤? 날씨 좋아하실 것 같아요",
          "original_form": "그럼 추석때쯤? 날씨 좋아하실 것 같아요",
          "speaker_id": "2",
          "time": "20210904 19:59"
        },
        {
          "id": "MDRW2100036472.1.19",
          "form": "네 추석 즈음의 날씨 좋아해요",
          "original_form": "네 추석 즈음의 날씨 좋아해요",
          "speaker_id": "1",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100036472.1.20",
          "form": "살짝 쌀쌀한가 싶은데",
          "original_form": "살짝 쌀쌀한가 싶은데",
          "speaker_id": "1",
