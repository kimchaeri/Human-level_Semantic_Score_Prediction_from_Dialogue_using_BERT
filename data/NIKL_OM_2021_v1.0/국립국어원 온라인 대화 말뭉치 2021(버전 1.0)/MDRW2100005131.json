      "utterance": [
        {
          "id": "MDRW2100005131.1.1",
          "form": "지금 학생이세요??",
          "original_form": "지금 학생이세요??",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005131.1.2",
          "form": "네네 대학생이에요!",
          "original_form": "네네 대학생이에요!",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005131.1.3",
          "form": "우와 무슨 과인지 물어봐도 되나요?",
          "original_form": "우와 무슨 과인지 물어봐도 되나요?",
          "speaker_id": "1",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005131.1.4",
          "form": "로봇학부!입니당",
          "original_form": "로봇학부!입니당",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005131.1.5",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "1",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005131.1.6",
          "form": "뭐 배워요???",
          "original_form": "뭐 배워요???",
          "speaker_id": "1",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005131.1.7",
          "form": "지금은 1학년이라",
          "original_form": "지금은 1학년이라",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005131.1.8",
          "form": "아직 기초 과목 배우고",
          "original_form": "아직 기초 과목 배우고",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005131.1.9",
          "form": "코딩 정도만 하고 있어요",
          "original_form": "코딩 정도만 하고 있어요",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005131.1.10",
          "form": "가끔 납땜하고,,",
          "original_form": "가끔 납땜하고,,",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005131.1.11",
          "form": "저도 이과여가지고 되게 신기해요!!!",
          "original_form": "저도 이과여가지고 되게 신기해요!!!",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005131.1.12",
          "form": "오오 전공이 어느쪽이신가요??",
          "original_form": "오오 전공이 어느쪽이신가요??",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005131.1.13",
          "form": "저는 고딩이에요!",
          "original_form": "저는 고딩이에요!",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005131.1.14",
          "form": "오 그렇군요!! 공대는 도망치세요!!",
          "original_form": "오 그렇군요!! 공대는 도망치세요!!",
          "speaker_id": "2",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005131.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005131.1.16",
          "form": "아..아!!",
          "original_form": "아..아!!",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005131.1.17",
          "form": "근데 이과는 공대말고 없자나요....",
          "original_form": "근데 이과는 공대말고 없자나요....",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005131.1.18",
          "form": "그건 맞아요ㅠㅠ",
          "original_form": "그건 맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005131.1.19",
          "form": "그리고 공대가 쎄긴 하죠...",
          "original_form": "그리고 공대가 쎄긴 하죠...",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005131.1.20",
          "form": "하지만 그만큼 힘들다는 점...",
          "original_form": "하지만 그만큼 힘들다는 점...{emoji:☆}",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005131.1.21",
          "form": "남들 다 놀러다니는 거 인스타로 올릴 때",
          "original_form": "남들 다 놀러다니는 거 인스타로 올릴 때",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005131.1.22",
          "form": "저는 쉬는시간에 짬내서 그거 확인하고",
          "original_form": "저는 쉬는시간에 짬내서 그거 확인하고",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005131.1.23",
          "form": "새벽까지 맨날 뭐 만들고 있어요",
          "original_form": "새벽까지 맨날 뭐 만들고 있어요",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005131.1.24",
          "form": "크흡",
          "original_form": "크흡",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005131.1.25",
          "form": "세상.. 이과는 과로사한다는 말이 괜히있는게 아니네요...",
          "original_form": "세상.. 이과는 과로사한다는 말이 괜히있는게 아니네요...",
          "speaker_id": "1",
          "time": "20210821 21:02"
