      "utterance": [
        {
          "id": "MDRW2100047565.1.1",
          "form": "완전 맛잘알이시네여!!",
          "original_form": "완전 맛잘알이시네여!!",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100047565.1.2",
          "form": "먹는 걸 좋아해요ㅎㅎ",
          "original_form": "먹는 걸 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100047565.1.3",
          "form": "거기 유자소스랑 먹음 정말 맛있어요 ㅋㅋㅋㅋ 강추합니당",
          "original_form": "거기 유자소스랑 먹음 정말 맛있어요 ㅋㅋㅋㅋ 강추합니당",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100047565.1.4",
          "form": "맛집 탐방도 좋아하고",
          "original_form": "맛집 탐방도 좋아하고",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100047565.1.5",
          "form": "오오~~~",
          "original_form": "오오~~~",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100047565.1.6",
          "form": "저는 맛집을 찾아가는 편은 아니거든요!",
          "original_form": "저는 맛집을 찾아가는 편은 아니거든요!",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100047565.1.7",
          "form": "추천하는 곳 있으세요??!!!",
          "original_form": "추천하는 곳 있으세요??!!!",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100047565.1.8",
          "form": "음 그때그때 찾아보고 가는데",
          "original_form": "음 그때그때 찾아보고 가는데",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100047565.1.9",
          "form": "한동안 서울 연남동 쪽 식당 많이 갔어요",
          "original_form": "한동안 서울 연남동 쪽 식당 많이 갔어요",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100047565.1.10",
          "form": "유명하고 맛있는건 다 그쪽에 있는것 같아요ㅜㅜ 연남동 이태원...",
          "original_form": "유명하고 맛있는건 다 그쪽에 있는것 같아요ㅜㅜ 연남동 이태원...",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100047565.1.11",
          "form": "제 생활반경과 너무 먼 ㅋㅋㅋ",
          "original_form": "제 생활반경과 너무 먼 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100047565.1.12",
          "form": "아무래도 사람들이 많이 모이는 곳에 맛집들도 많죠",
          "original_form": "아무래도 사람들이 많이 모이는 곳에 맛집들도 많죠",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100047565.1.13",
          "form": "저도 가깝지는 않은 지역인데",
          "original_form": "저도 가깝지는 않은 지역인데",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100047565.1.14",
          "form": "주말에 나들이겸 다녀오고 그랬어요",
          "original_form": "주말에 나들이겸 다녀오고 그랬어요",
          "speaker_id": "2",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100047565.1.15",
          "form": "그랬군여! 거기서 먹었던 것 중에 어떤게 제일 맛있으셨어요?",
          "original_form": "그랬군여! 거기서 먹었던 것 중에 어떤게 제일 맛있으셨어요?",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100047565.1.16",
          "form": "미쁘동이라고 해산물덮밥집도 맛있었고",
          "original_form": "미쁘동이라고 해산물덮밥집도 맛있었고",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100047565.1.17",
          "form": "포가라는 중국집 깐풍만두도 맛있었어요",
          "original_form": "포가라는 중국집 깐풍만두도 맛있었어요",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100047565.1.18",
          "form": "오~~ 중식 많이 드셨네요!!",
          "original_form": "오~~ 중식 많이 드셨네요!!",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100047565.1.19",
          "form": "깐풍만두 완전 끌리는데요?!?!",
          "original_form": "깐풍만두 완전 끌리는데요?!?!",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100047565.1.20",
          "form": "미쁘동은 일식계열이예요ㅎㅎ",
          "original_form": "미쁘동은 일식계열이예요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100047565.1.21",
          "form": "네 깐풍만두가 아주 바삭하니 맛있었어요",
          "original_form": "네 깐풍만두가 아주 바삭하니 맛있었어요",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100047565.1.22",
          "form": "오~ 그렇군요! 일본식덮밥 ㅎㅎ",
          "original_form": "오~ 그렇군요! 일본식덮밥 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100047565.1.23",
          "form": "꺄ㅜㅡ ㅋㅋㅋㅋㅋ",
          "original_form": "꺄ㅜㅡ ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100047565.1.24",
          "form": "너무 맛있겠다....",
          "original_form": "너무 맛있겠다....",
