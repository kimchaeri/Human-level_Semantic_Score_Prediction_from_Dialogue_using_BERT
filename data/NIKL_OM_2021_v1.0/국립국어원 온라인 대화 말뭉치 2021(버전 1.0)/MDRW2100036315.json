      "utterance": [
        {
          "id": "MDRW2100036315.1.1",
          "form": "어떤 피자를 드셨나요?",
          "original_form": "어떤 피자를 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100036315.1.2",
          "form": "저녁 뭐 드셨나요",
          "original_form": "저녁 뭐 드셨나요",
          "speaker_id": "2",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100036315.1.3",
          "form": "저도 피자를 좋ㅇ해요",
          "original_form": "저도 피자를 좋ㅇ해요",
          "speaker_id": "1",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100036315.1.4",
          "form": "저는 포테이토피자랑 불고기 피자요!",
          "original_form": "저는 포테이토피자랑 불고기 피자요!",
          "speaker_id": "2",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100036315.1.5",
          "form": "제가 좋아하구 그리고 모두가 조 ㅎ아하는 무난한 메뉴네요",
          "original_form": "제가 좋아하구 그리고 모두가 조 ㅎ아하는 무난한 메뉴네요",
          "speaker_id": "1",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100036315.1.6",
          "form": "전 가지전 먹었어요 ㅋㅋ",
          "original_form": "전 가지전 먹었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100036315.1.7",
          "form": "그쵸 전 포테이토 피자 엄청 좋아해요",
          "original_form": "그쵸 전 포테이토 피자 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100036315.1.8",
          "form": "우와 가지전 맛있으셨겠네요",
          "original_form": "우와 가지전 맛있으셨겠네요",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100036315.1.9",
          "form": "보송보송 푹신한 식감이었어요",
          "original_form": "보송보송 푹신한 식감이었어요",
          "speaker_id": "1",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100036315.1.10",
          "form": "저도 포테이토 좋아해요 담백해서 좋아요",
          "original_form": "저도 포테이토 좋아해요 담백해서 좋아요",
          "speaker_id": "1",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100036315.1.11",
          "form": "제 기준 가지는 맛없어보이는 색인데 너무 맛있어요ㅋㅋㅌ",
          "original_form": "제 기준 가지는 맛없어보이는 색인데 너무 맛있어요ㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100036315.1.12",
          "form": "전 볶거나 튀긴거 좋아해요",
          "original_form": "전 볶거나 튀긴거 좋아해요",
          "speaker_id": "1",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100036315.1.13",
          "form": "그리고 파란색은 보편적으로 식욕을 떨어드리는 색이에여 ㅋㅋ",
          "original_form": "그리고 파란색은 보편적으로 식욕을 떨어드리는 색이에여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100036315.1.14",
          "form": "전 나물도 좋아하고 볶거나 튀긴 것도 좋아해요",
          "original_form": "전 나물도 좋아하고 볶거나 튀긴 것도 좋아해요",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100036315.1.15",
          "form": "어향가지 좋아해용",
          "original_form": "어향가지 좋아해용",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100036315.1.16",
          "form": "전 가지나물은 촉감때매 잘 못먹어요 목구멍서넘어가는 느낌이 싫어요 ㅠㅠ",
          "original_form": "전 가지나물은 촉감때매 잘 못먹어요 목구멍서넘어가는 느낌이 싫어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100036315.1.17",
          "form": "물컹한 느낌?",
          "original_form": "물컹한 느낌?",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100036315.1.18",
          "form": "네 콧물같아요",
          "original_form": "네 콧물같아요",
          "speaker_id": "1",
          "time": "20210830 21:21"
        }
      ]
