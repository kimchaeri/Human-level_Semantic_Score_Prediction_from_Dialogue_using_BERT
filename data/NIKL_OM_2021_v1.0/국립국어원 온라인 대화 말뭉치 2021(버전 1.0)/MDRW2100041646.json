      "utterance": [
        {
          "id": "MDRW2100041646.1.1",
          "form": "직장 다니시나요?",
          "original_form": "직장 다니시나요?",
          "speaker_id": "1",
          "time": "20210906 13:48"
        },
        {
          "id": "MDRW2100041646.1.2",
          "form": "지금은 그냥 알바중이예요ㅎㅎ  직장다니시나용",
          "original_form": "지금은 그냥 알바중이예요ㅎㅎ  직장다니시나용",
          "speaker_id": "2",
          "time": "20210906 13:50"
        },
        {
          "id": "MDRW2100041646.1.3",
          "form": "넵 저는 직장다녀요",
          "original_form": "넵 저는 직장다녀요",
          "speaker_id": "1",
          "time": "20210906 13:50"
        },
        {
          "id": "MDRW2100041646.1.4",
          "form": "어떤 알바하세요?",
          "original_form": "어떤 알바하세요?",
          "speaker_id": "1",
          "time": "20210906 13:50"
        },
        {
          "id": "MDRW2100041646.1.5",
          "form": "공공근로해요..ㅋㅋㅋㅋ",
          "original_form": "공공근로해요..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100041646.1.6",
          "form": "아닌가..좀 애매한거같기도하고",
          "original_form": "아닌가..좀 애매한거같기도하고",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100041646.1.7",
          "form": "ㅋㅋㄱㅋ..아무튼그렇습니다ㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋ..아무튼그렇습니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100041646.1.8",
          "form": "직장마음에드시나요",
          "original_form": "직장마음에드시나요",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100041646.1.9",
          "form": "음..사실 저는 12년 정도 다니고있는데",
          "original_form": "음..사실 저는 12년 정도 다니고있는데",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100041646.1.10",
          "form": "그만두고싶어요",
          "original_form": "그만두고싶어요",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100041646.1.11",
          "form": "그렇게 힘든건아닌데 좀 갈수록 지치네요",
          "original_form": "그렇게 힘든건아닌데 좀 갈수록 지치네요",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100041646.1.12",
          "form": "와..진짜오래다니셨네요..대단하시네요ㅇㅁㅇ..",
          "original_form": "와..진짜오래다니셨네요..대단하시네요ㅇㅁㅇ..",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100041646.1.13",
          "form": "한 3년까진 힘들다가",
          "original_form": "한 3년까진 힘들다가",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100041646.1.14",
          "form": "56년되니 자신감이 생겼는데",
          "original_form": "56년되니 자신감이 생겼는데",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100041646.1.15",
          "form": "10년 넘으니 나이도 들고 이래서 체력이딸려요",
          "original_form": "10년 넘으니 나이도 들고 이래서 체력이딸려요",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100041646.1.16",
          "form": "한곳에12년이라니 상상이안돼요...  저 5년있을때도 ㅇㆍ 길다 이생각햇는데..",
          "original_form": "한곳에12년이라니 상상이안돼요...  저 5년있을때도 ㅇㆍ 길다 이생각햇는데..",
          "speaker_id": "2",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100041646.1.17",
          "form": "그렇군요 다른데 갈 자신이없어서 그냥 계속 다니게 되더라구요",
          "original_form": "그렇군요 다른데 갈 자신이없어서 그냥 계속 다니게 되더라구요",
          "speaker_id": "1",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100041646.1.18",
          "form": "그게좋은거같기도하고..ㅎㅎ",
          "original_form": "그게좋은거같기도하고..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100041646.1.19",
          "form": "보통 이직생각은 어떨때 드세요?",
          "original_form": "보통 이직생각은 어떨때 드세요?",
          "speaker_id": "1",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100041646.1.20",
          "form": "익숙해지는건 나쁘지않죠",
          "original_form": "익숙해지는건 나쁘지않죠",
          "speaker_id": "2",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100041646.1.21",
          "form": "음 맞아요 그리고 근무지가 계속 바뀌어서 매번 새출발하는 기분이기도하구요",
          "original_form": "음 맞아요 그리고 근무지가 계속 바뀌어서 매번 새출발하는 기분이기도하구요",
          "speaker_id": "1",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100041646.1.22",
          "form": "어느새 12년이 되었네요",
          "original_form": "어느새 12년이 되었네요",
          "speaker_id": "1",
          "time": "20210906 14:08"
        }
      ]
