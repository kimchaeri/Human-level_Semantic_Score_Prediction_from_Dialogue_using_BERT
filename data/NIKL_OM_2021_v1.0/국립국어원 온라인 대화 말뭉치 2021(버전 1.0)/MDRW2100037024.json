      "utterance": [
        {
          "id": "MDRW2100037024.1.1",
          "form": "뭘 아시는분이시군용",
          "original_form": "뭘 아시는분이시군용",
          "speaker_id": "2",
          "time": "20210831 17:22"
        },
        {
          "id": "MDRW2100037024.1.2",
          "form": "ㅋㅋㅋ먹는거에 진심입니당",
          "original_form": "ㅋㅋㅋ먹는거에 진심입니당",
          "speaker_id": "1",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100037024.1.3",
          "form": "최근에 뭐구매하셨어요",
          "original_form": "최근에 뭐구매하셨어요",
          "speaker_id": "1",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100037024.1.4",
          "form": "최근에 악세사리 재료 구매했어용ㅋㅋ",
          "original_form": "최근에 악세사리 재료 구매했어용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100037024.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100037024.1.6",
          "form": "전 갤탭이 최근에 구매한것중에 만족스러워요",
          "original_form": "전 갤탭이 최근에 구매한것중에 만족스러워요",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100037024.1.7",
          "form": "악세사리 직접 만드세요?",
          "original_form": "악세사리 직접 만드세요?",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100037024.1.8",
          "form": "오..갤탭!",
          "original_form": "오..갤탭!",
          "speaker_id": "2",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100037024.1.9",
          "form": "만들어보려구용!",
          "original_form": "만들어보려구용!",
          "speaker_id": "2",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100037024.1.10",
          "form": "유용하게쓰고있어요",
          "original_form": "유용하게쓰고있어요",
          "speaker_id": "1",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100037024.1.11",
          "form": "오오 어떤거요?",
          "original_form": "오오 어떤거요?",
          "speaker_id": "1",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100037024.1.12",
          "form": "친구들 줄 키링 만들었어용",
          "original_form": "친구들 줄 키링 만들었어용",
          "speaker_id": "2",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100037024.1.13",
          "form": "ㅎ힛",
          "original_form": "ㅎ힛",
          "speaker_id": "2",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100037024.1.14",
          "form": "손재주가 제법이신데요",
          "original_form": "손재주가 제법이신데요",
          "speaker_id": "1",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100037024.1.15",
          "form": "옛날에 동대문인가 악세사리파는곳 가봤는데 진짜 재료많고 뭐가뭔지모르겠더라구여",
          "original_form": "옛날에 동대문인가 악세사리파는곳 가봤는데 진짜 재료많고 뭐가뭔지모르겠더라구여",
          "speaker_id": "1",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100037024.1.16",
          "form": "저 이번에 처음으로 다녀와봤어요!",
          "original_form": "저 이번에 처음으로 다녀와봤어요!",
          "speaker_id": "2",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100037024.1.17",
          "form": "금욜인데도 사람도 많고.. 나중에 여길 본건지 안본건지 싶더라구요ㅋㅋㅋ",
          "original_form": "금욜인데도 사람도 많고.. 나중에 여길 본건지 안본건지 싶더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100037024.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ네네 저도 아니 넘 휘황찬란하고 종류도많은데 다 비슷비슷하고 어딘지모르겠더라구여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ네네 저도 아니 넘 휘황찬란하고 종류도많은데 다 비슷비슷하고 어딘지모르겠더라구여",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100037024.1.19",
          "form": "길잃기딱좋아여",
          "original_form": "길잃기딱좋아여",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100037024.1.20",
          "form": "진짜여ㅋㅋㅋ",
          "original_form": "진짜여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100037024.1.21",
          "form": "저 똑같은곳만 몇 번 본건지ㅋㅋ",
          "original_form": "저 똑같은곳만 몇 번 본건지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100037024.1.22",
          "form": "가격이 다 조금씩 다르대서 발품 팔면서 위치 찾아다녔는데 결국 못찾은거로 교훈 얻고 왔어요ㅋㅋㅋ",
          "original_form": "가격이 다 조금씩 다르대서 발품 팔면서 위치 찾아다녔는데 결국 못찾은거로 교훈 얻고 왔어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100037024.1.23",
          "form": "ㅋㅋㅋ첨에맘에들었을때 그때 딱사야되는거였져",
          "original_form": "ㅋㅋㅋ첨에맘에들었을때 그때 딱사야되는거였져",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100037024.1.24",
          "form": "키링몇개나만드셨어요",
          "original_form": "키링몇개나만드셨어요",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100037024.1.25",
          "form": "맞아여!! 다음에 그 가게 다시 못찾아욬ㅋㄲㅋ",
          "original_form": "맞아여!! 다음에 그 가게 다시 못찾아욬ㅋㄲㅋ",
