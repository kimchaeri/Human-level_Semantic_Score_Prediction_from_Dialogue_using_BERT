      "utterance": [
        {
          "id": "MDRW2100033673.1.1",
          "form": "name2님 맛점하셨나요",
          "original_form": "&name2&님 맛점하셨나요",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100033673.1.2",
          "form": "네ㅎㅎ점심 드셨어요?",
          "original_form": "네ㅎㅎ점심 드셨어요?",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100033673.1.3",
          "form": "네 저는 오뎅남은거 먹었죵",
          "original_form": "네 저는 오뎅남은거 먹었죵",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100033673.1.4",
          "form": "맛있겠네요ㅎㅎ 저는 비빔밥먹었어요ㅋㅋ",
          "original_form": "맛있겠네요ㅎㅎ 저는 비빔밥먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100033673.1.5",
          "form": "오 그것도 맛있겠다 계란들어간걸로?",
          "original_form": "오 그것도 맛있겠다 계란들어간걸로?",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100033673.1.6",
          "form": "전 그냥 오뎅이었어요",
          "original_form": "전 그냥 오뎅이었어요",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100033673.1.7",
          "form": "네ㅎㅎ 건강식이었네요ㅋㅋ두부도 들어가구",
          "original_form": "네ㅎㅎ 건강식이었네요ㅋㅋ두부도 들어가구",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100033673.1.8",
          "form": "배안찰거같은데ㅜㅜ괜찮나요?",
          "original_form": "배안찰거같은데ㅜㅜ괜찮나요?",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100033673.1.9",
          "form": "와 든든한 한끼네요",
          "original_form": "와 든든한 한끼네요",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100033673.1.10",
          "form": "ㅎㅎ 그냥 저냥 막 줏어먹어요 빈베에",
          "original_form": "ㅎㅎ 그냥 저냥 막 줏어먹어요 빈베에",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100033673.1.11",
          "form": "배에",
          "original_form": "배에",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100033673.1.12",
          "form": "이제 커피하시나요",
          "original_form": "이제 커피하시나요",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100033673.1.13",
          "form": "네 오늘은 누가 커피사오셔서 아메리카노네요ㅋㅋ",
          "original_form": "네 오늘은 누가 커피사오셔서 아메리카노네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100033673.1.14",
          "form": "커피드셨어여?",
          "original_form": "커피드셨어여?",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100033673.1.15",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100033673.1.16",
          "form": "와 멋진동료시네",
          "original_form": "와 멋진동료시네",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033673.1.17",
          "form": "휴가다녀오셔서ㅎㅎ 과자랑ㅎㅎ멋지세요",
          "original_form": "휴가다녀오셔서ㅎㅎ 과자랑ㅎㅎ멋지세요",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033673.1.18",
          "form": "오 좋은데 다녀오셨나부당",
          "original_form": "오 좋은데 다녀오셨나부당",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033673.1.19",
          "form": "경주다녀오셔서 경주빵을 주시네요ㅋㅋ",
          "original_form": "경주다녀오셔서 경주빵을 주시네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033673.1.20",
          "form": "오 좋은데 다녀오셨네요 정말",
          "original_form": "오 좋은데 다녀오셨네요 정말",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033673.1.21",
          "form": "경주에 한산했다나요? 경주빵맛있는데",
          "original_form": "경주에 한산했다나요? 경주빵맛있는데",
          "speaker_id": "1",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100033673.1.22",
          "form": "사람 엄청 많았대요ㅋㅋ겨우 사오셨다구ㅋㅋ",
          "original_form": "사람 엄청 많았대요ㅋㅋ겨우 사오셨다구ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:37"
        }
      ]
