      "utterance": [
        {
          "id": "MDRW2100041470.1.1",
          "form": "오늘은 넷플에서 뭐 보시나",
          "original_form": "오늘은 넷플에서 뭐 보시나",
          "speaker_id": "2",
          "time": "20210906 00:57"
        },
        {
          "id": "MDRW2100041470.1.2",
          "form": "어제 저녁부터 빅뱅이론 보고 있어",
          "original_form": "어제 저녁부터 빅뱅이론 보고 있어",
          "speaker_id": "1",
          "time": "20210906 00:57"
        },
        {
          "id": "MDRW2100041470.1.3",
          "form": "오랜만에 보니까 너무 웃겨",
          "original_form": "오랜만에 보니까 너무 웃겨",
          "speaker_id": "1",
          "time": "20210906 00:57"
        },
        {
          "id": "MDRW2100041470.1.4",
          "form": "빅뱅이론이 그 과학 너드 나오는 시트콤이지",
          "original_form": "빅뱅이론이 그 과학 너드 나오는 시트콤이지",
          "speaker_id": "2",
          "time": "20210906 01:00"
        },
        {
          "id": "MDRW2100041470.1.5",
          "form": "아 짐 파슨스 나오는 거!!!",
          "original_form": "아 짐 파슨스 나오는 거!!!",
          "speaker_id": "1",
          "time": "20210906 01:01"
        },
        {
          "id": "MDRW2100041470.1.6",
          "form": "어 뭔지 알겠다. 그거 쉘든 캐릭터가 너무 얄미웠어",
          "original_form": "어 뭔지 알겠다. 그거 쉘든 캐릭터가 너무 얄미웠어",
          "speaker_id": "2",
          "time": "20210906 01:01"
        },
        {
          "id": "MDRW2100041470.1.7",
          "form": "헐 난 쉘든이 최애캐릭터야ㅋㅋㅋ",
          "original_form": "헐 난 쉘든이 최애캐릭터야ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 01:02"
        },
        {
          "id": "MDRW2100041470.1.8",
          "form": "초반엔 얄미워도 나중엔 귀엽다구요ㅠ",
          "original_form": "초반엔 얄미워도 나중엔 귀엽다구요ㅠ",
          "speaker_id": "1",
          "time": "20210906 01:03"
        },
        {
          "id": "MDRW2100041470.1.9",
          "form": "그런가. 엄마는 페니만 좋았어",
          "original_form": "그런가. 엄마는 페니만 좋았어",
          "speaker_id": "2",
          "time": "20210906 01:06"
        },
        {
          "id": "MDRW2100041470.1.10",
          "form": "나두 페니 좋아",
          "original_form": "나두 페니 좋아",
          "speaker_id": "1",
          "time": "20210906 01:07"
        },
        {
          "id": "MDRW2100041470.1.11",
          "form": "넘 예쁘고 귀여워",
          "original_form": "넘 예쁘고 귀여워",
          "speaker_id": "1",
          "time": "20210906 01:07"
        },
        {
          "id": "MDRW2100041470.1.12",
          "form": "극중에서 레너드랑 사귀잖아",
          "original_form": "극중에서 레너드랑 사귀잖아",
          "speaker_id": "1",
          "time": "20210906 01:07"
        },
        {
          "id": "MDRW2100041470.1.13",
          "form": "진심 왜 사귀는지 모를...",
          "original_form": "진심 왜 사귀는지 모를...",
          "speaker_id": "1",
          "time": "20210906 01:07"
        },
        {
          "id": "MDRW2100041470.1.14",
          "form": "페니가 백만배 아까워",
          "original_form": "페니가 백만배 아까워",
          "speaker_id": "1",
          "time": "20210906 01:07"
        },
        {
          "id": "MDRW2100041470.1.15",
          "form": "레너드도 별로고 하워드도 별로야",
          "original_form": "레너드도 별로고 하워드도 별로야",
          "speaker_id": "2",
          "time": "20210906 01:08"
        },
        {
          "id": "MDRW2100041470.1.16",
          "form": "와 하워드 진짜.. 시즌 초반에 제일 싫어하는 캐릭터였어",
          "original_form": "와 하워드 진짜.. 시즌 초반에 제일 싫어하는 캐릭터였어",
          "speaker_id": "1",
          "time": "20210906 01:09"
        },
        {
          "id": "MDRW2100041470.1.17",
          "form": "근데 시즌 후반부 갈수록 나름 괜찮아져..",
          "original_form": "근데 시즌 후반부 갈수록 나름 괜찮아져..",
          "speaker_id": "1",
          "time": "20210906 01:09"
        },
        {
          "id": "MDRW2100041470.1.18",
          "form": "레너드는 계속 별로고ㅋㅋㅋ",
          "original_form": "레너드는 계속 별로고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 01:09"
        },
        {
          "id": "MDRW2100041470.1.19",
          "form": "라지는 좀 귀여웠던 듯. 그 인도인 캐릭터",
          "original_form": "라지는 좀 귀여웠던 듯. 그 인도인 캐릭터",
          "speaker_id": "2",
          "time": "20210906 01:09"
        },
        {
          "id": "MDRW2100041470.1.20",
          "form": "마저ㅋㅋㅋ 시즌 종영할 때 다 커플됐는데",
          "original_form": "마저ㅋㅋㅋ 시즌 종영할 때 다 커플됐는데",
          "speaker_id": "1",
          "time": "20210906 01:10"
        },
        {
          "id": "MDRW2100041470.1.21",
          "form": "라지만 러브라인 없어서 불쌍했음",
          "original_form": "라지만 러브라인 없어서 불쌍했음",
          "speaker_id": "1",
          "time": "20210906 01:10"
        },
        {
          "id": "MDRW2100041470.1.22",
          "form": "그래도 라지는 계속 여자친구 바뀌잖아",
          "original_form": "그래도 라지는 계속 여자친구 바뀌잖아",
          "speaker_id": "2",
          "time": "20210906 01:11"
        },
        {
          "id": "MDRW2100041470.1.23",
          "form": "결혼을 못해서 그렇지",
          "original_form": "결혼을 못해서 그렇지",
          "speaker_id": "2",
          "time": "20210906 01:11"
        },
        {
          "id": "MDRW2100041470.1.24",
          "form": "ㅋㅋㅋㅋ하워드도 버나데드랑 결혼을 했는데ㅠ 왜 라지만 결론을 못하고 끝났을까",
          "original_form": "ㅋㅋㅋㅋ하워드도 버나데드랑 결혼을 했는데ㅠ 왜 라지만 결론을 못하고 끝났을까",
          "speaker_id": "1",
          "time": "20210906 01:11"
        },
        {
          "id": "MDRW2100041470.1.25",
          "form": "쉘든도 러브라인 생길지 몰랐는데 에이미랑 이어져서 놀랐잖아",
          "original_form": "쉘든도 러브라인 생길지 몰랐는데 에이미랑 이어져서 놀랐잖아",
          "speaker_id": "1",
          "time": "20210906 01:12"
        }
      ]
    }
  ]
