      "utterance": [
        {
          "id": "MDRW2100017507.1.1",
          "form": "연애하고있어요?",
          "original_form": "연애하고있어요?",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017507.1.2",
          "form": "지금은 안하고 있습니다...!",
          "original_form": "지금은 안하고 있습니다...!",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017507.1.3",
          "form": "소개팅 해봤어요?",
          "original_form": "소개팅 해봤어요?",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017507.1.4",
          "form": "소개 받은것도 소개팅에 해당 되겠죠..?",
          "original_form": "소개 받은것도 소개팅에 해당 되겠죠..?",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017507.1.5",
          "form": "네! 잘 됐나요?",
          "original_form": "네! 잘 됐나요?",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017507.1.6",
          "form": "아뇨.. ㅎㅎ 제가 그만 연락하자고 해서...",
          "original_form": "아뇨.. ㅎㅎ 제가 그만 연락하자고 해서...",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017507.1.7",
          "form": "잘 안됐습니다..",
          "original_form": "잘 안됐습니다..",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017507.1.8",
          "form": "이상형이 어떻게 되세요?",
          "original_form": "이상형이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017507.1.9",
          "form": "전 목소리 좋은 귀여운 사람이요",
          "original_form": "전 목소리 좋은 귀여운 사람이요",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017507.1.10",
          "form": "어떤목소리 좋아하세요?",
          "original_form": "어떤목소리 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017507.1.11",
          "form": "음... 달달한 중저음 좋아하는것 같아요",
          "original_form": "음... 달달한 중저음 좋아하는것 같아요",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017507.1.12",
          "form": "자만추세요?",
          "original_form": "자만추세요?",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017507.1.13",
          "form": "네 자만추를 선호해요",
          "original_form": "네 자만추를 선호해요",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017507.1.14",
          "form": "가장 오래한 연애는 얼마나 했어요?",
          "original_form": "가장 오래한 연애는 얼마나 했어요?",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017507.1.15",
          "form": "몇달 안했던것 같아요",
          "original_form": "몇달 안했던것 같아요",
          "speaker_id": "2",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100017507.1.16",
          "form": "하고싶은 데이트 있어요?",
          "original_form": "하고싶은 데이트 있어요?",
          "speaker_id": "1",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017507.1.17",
          "form": "놀이공원 데이트 해보고싶어요",
          "original_form": "놀이공원 데이트 해보고싶어요",
