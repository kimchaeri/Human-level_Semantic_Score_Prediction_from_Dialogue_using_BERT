      "utterance": [
        {
          "id": "MDRW2100046197.1.1",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046197.1.2",
          "form": "요즘 잘나온다더라고요!!",
          "original_form": "요즘 잘나온다더라고요!!",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046197.1.3",
          "form": "근데 저도 사실은",
          "original_form": "근데 저도 사실은",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046197.1.4",
          "form": "소곱창은",
          "original_form": "소곱창은",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046197.1.5",
          "form": "배달 안먹어봤어요ㅋㅋㅋㅋㅋ",
          "original_form": "배달 안먹어봤어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046197.1.6",
          "form": "그 마늘가루?",
          "original_form": "그 마늘가루?",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046197.1.7",
          "form": "어떤..?",
          "original_form": "어떤..?",
          "speaker_id": "1",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046197.1.8",
          "form": "랑 부추구운게 나무 먹고싶어요",
          "original_form": "랑 부추구운게 나무 먹고싶어요",
          "speaker_id": "2",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046197.1.9",
          "form": "곱창위에 뿌려먹는거요!",
          "original_form": "곱창위에 뿌려먹는거요!",
          "speaker_id": "2",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046197.1.10",
          "form": "아 그거 체인점에서",
          "original_form": "아 그거 체인점에서",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046197.1.11",
          "form": "뿌려즈잖아요",
          "original_form": "뿌려즈잖아요",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046197.1.12",
          "form": "곱창이야기요!",
          "original_form": "곱창이야기요!",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046197.1.13",
          "form": "근데 그거 가루",
          "original_form": "근데 그거 가루",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046197.1.14",
          "form": "마늘아니에여!",
          "original_form": "마늘아니에여!",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046197.1.15",
          "form": "아니에요??!",
          "original_form": "아니에요??!",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046197.1.16",
          "form": "그럼 뭐에요??? 마늘가루인줄 알았는데",
          "original_form": "그럼 뭐에요??? 마늘가루인줄 알았는데",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046197.1.17",
          "form": "제가 알기로는",
          "original_form": "제가 알기로는",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046197.1.18",
          "form": "마늘이랑 마 가루 섞은거래요",
          "original_form": "마늘이랑 마 가루 섞은거래요",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046197.1.19",
          "form": "아하 마 가루",
          "original_form": "아하 마 가루",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046197.1.20",
          "form": "저 마 좋아해요 ㅎ",
          "original_form": "저 마 좋아해요 ㅎ",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046197.1.21",
          "form": "마 진짜 어릴때 먹은거같아요ㅋㅋㅋ",
          "original_form": "마 진짜 어릴때 먹은거같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046197.1.22",
          "form": "바나나랑 마 갈아서",
          "original_form": "바나나랑 마 갈아서",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046197.1.23",
          "form": "주스로",
          "original_form": "주스로",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046197.1.24",
          "form": "그거 생으로도 먹어도 맛있는데",
          "original_form": "그거 생으로도 먹어도 맛있는데",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046197.1.25",
          "form": "마를...생으로 먹어요....?",
          "original_form": "마를...생으로 먹어요....?",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046197.1.26",
          "form": "충격",
          "original_form": "충격",
          "speaker_id": "1",
          "time": "20210908 11:13"
        }
      ]
    }
