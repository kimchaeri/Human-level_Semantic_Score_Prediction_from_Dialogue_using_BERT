      "utterance": [
        {
          "id": "MDRW2100023800.1.1",
          "form": "한참볼땐 많이봣는데",
          "original_form": "한참볼땐 많이봣는데",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023800.1.2",
          "form": "그림형제도보고",
          "original_form": "그림형제도보고",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023800.1.3",
          "form": "정치물도보고 제목이 잊혀졌네요ㅠㅠ 오래전이라.ㅜㅠㅠ",
          "original_form": "정치물도보고 제목이 잊혀졌네요ㅠㅠ 오래전이라.ㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023800.1.4",
          "form": "하오카?",
          "original_form": "하오카?",
          "speaker_id": "1",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023800.1.5",
          "form": "요새 보는 드라마나 예능잇으세요?",
          "original_form": "요새 보는 드라마나 예능잇으세요?",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023800.1.6",
          "form": "옛날꼬긴 한데.. 싸인 봤어여..",
          "original_form": "옛날꼬긴 한데.. 싸인 봤어여..",
          "speaker_id": "1",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100023800.1.7",
          "form": "지정생존자 ! 잼있게봣어요!",
          "original_form": "지정생존자 ! 잼있게봣어요!",
          "speaker_id": "2",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100023800.1.8",
          "form": "아.. 그거도 초반엔 신작하죠..",
          "original_form": "아.. 그거도 초반엔 신작하죠..",
          "speaker_id": "1",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100023800.1.9",
          "form": "신.박.",
          "original_form": "신.박.",
          "speaker_id": "1",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100023800.1.10",
          "form": "마담 세크리터리랑 !",
          "original_form": "마담 세크리터리랑 !",
          "speaker_id": "2",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100023800.1.11",
          "form": "지정생존자는 시즌2는 별로엿던거같아요",
          "original_form": "지정생존자는 시즌2는 별로엿던거같아요",
          "speaker_id": "2",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100023800.1.12",
          "form": "앞부분만 ㅎㅎ",
          "original_form": "앞부분만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100023800.1.13",
          "form": "그툐..용두사미..ㅋㅋㅋ",
          "original_form": "그툐..용두사미..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100023800.1.14",
          "form": "소재만 상큼하고...",
          "original_form": "소재만 상큼하고...",
          "speaker_id": "1",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100023800.1.15",
          "form": "ㅎㅎ 한국버전은 보다 안본거같아요",
          "original_form": "ㅎㅎ 한국버전은 보다 안본거같아요",
          "speaker_id": "2",
          "time": "20210830 17:53"
        },
        {
          "id": "MDRW2100023800.1.16",
          "form": "김영민배우가 나온거같았는데",
          "original_form": "김영민배우가 나온거같았는데",
          "speaker_id": "2",
          "time": "20210830 17:53"
        },
        {
          "id": "MDRW2100023800.1.17",
          "form": "아.. 한국판 들어보긴햇는데.. 지진희 였던가여?",
          "original_form": "아.. 한국판 들어보긴햇는데.. 지진희 였던가여?",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100023800.1.18",
          "form": "지진희엿나요 ㅋㅋㅋㅋ",
          "original_form": "지진희엿나요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100023800.1.19",
          "form": "딴사람말햇네요 ㅋㅋ",
          "original_form": "딴사람말햇네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100023800.1.20",
          "form": "그 고전... 왕겜 같은건 보신적 잇어요?",
          "original_form": "그 고전... 왕겜 같은건 보신적 잇어요?",
          "speaker_id": "1",
          "time": "20210830 18:00"
        },
        {
          "id": "MDRW2100023800.1.21",
          "form": "보다 안보다했어요!",
          "original_form": "보다 안보다했어요!",
          "speaker_id": "2",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100023800.1.22",
          "form": "미드 중.. 전국민적인건 왕겜이 마지막이고..\n최근 화제작은 엄는거 같아요..",
          "original_form": "미드 중.. 전국민적인건 왕겜이 마지막이고..\n최근 화제작은 엄는거 같아요..",
          "speaker_id": "1",
          "time": "20210830 18:03"
        },
        {
          "id": "MDRW2100023800.1.23",
          "form": "저도 요새는 안본지 오래됐어요 ㅎㅎ 한참빠지면 몰아서 보는스타일이라",
          "original_form": "저도 요새는 안본지 오래됐어요 ㅎㅎ 한참빠지면 몰아서 보는스타일이라",
          "speaker_id": "2",
          "time": "20210830 18:03"
        },
        {
          "id": "MDRW2100023800.1.24",
          "form": "전 프렌즈많이봣어요 ㅋㅋ",
          "original_form": "전 프렌즈많이봣어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:04"
        },
        {
          "id": "MDRW2100023800.1.25",
          "form": "전 프렌즈 알았을때.. 그건 너무 고전이엿어서..",
          "original_form": "전 프렌즈 알았을때.. 그건 너무 고전이엿어서..",
          "speaker_id": "1",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100023800.1.26",
          "form": "프리즌브레이크가 시작이엇어요...",
          "original_form": "프리즌브레이크가 시작이엇어요...",
          "speaker_id": "1",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100023800.1.27",
          "form": "한 10년전쯤이네요 ㅋㅋㅋ",
          "original_form": "한 10년전쯤이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:12"
        },
        {
          "id": "MDRW2100023800.1.28",
          "form": "외장하드에 다 보유하고 자주봣는데 추억이네요",
          "original_form": "외장하드에 다 보유하고 자주봣는데 추억이네요",
          "speaker_id": "2",
          "time": "20210830 18:13"
        },
        {
          "id": "MDRW2100023800.1.29",
          "form": "네.. 맞아여.. 그때 입문해서.. \n미드 자주본거 같아요..",
          "original_form": "네.. 맞아여.. 그때 입문해서.. \n미드 자주본거 같아요..",
          "speaker_id": "1",
          "time": "20210830 18:15"
        }
      ]
    }
