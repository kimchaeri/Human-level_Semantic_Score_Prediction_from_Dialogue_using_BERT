      "utterance": [
        {
          "id": "MDRW2100005829.1.1",
          "form": "카페 자주가?",
          "original_form": "카페 자주가?",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100005829.1.2",
          "form": "카페 자주는 아니고 지인들 만날때나 혼자 가고싶을때 간혹 감",
          "original_form": "카페 자주는 아니고 지인들 만날때나 혼자 가고싶을때 간혹 감",
          "speaker_id": "2",
          "time": "20210907 13:40"
        },
        {
          "id": "MDRW2100005829.1.3",
          "form": "주로 카페 어디감?",
          "original_form": "주로 카페 어디감?",
          "speaker_id": "2",
          "time": "20210907 13:40"
        },
        {
          "id": "MDRW2100005829.1.4",
          "form": "사실 안간지 백만년됨",
          "original_form": "사실 안간지 백만년됨",
          "speaker_id": "1",
          "time": "20210907 13:41"
        },
        {
          "id": "MDRW2100005829.1.5",
          "form": "아나ㅋㅋ 너무 안간거 아녀",
          "original_form": "아나ㅋㅋ 너무 안간거 아녀",
          "speaker_id": "2",
          "time": "20210907 13:41"
        },
        {
          "id": "MDRW2100005829.1.6",
          "form": "코로나이후로 안갔어  ...",
          "original_form": "코로나이후로 안갔어  ...",
          "speaker_id": "1",
          "time": "20210907 13:41"
        },
        {
          "id": "MDRW2100005829.1.7",
          "form": "하긴 가서먹기 그렇지",
          "original_form": "하긴 가서먹기 그렇지",
          "speaker_id": "2",
          "time": "20210907 13:42"
        },
        {
          "id": "MDRW2100005829.1.8",
          "form": "나도 음료들고 집까지 온 적 많음ㅋㅋ",
          "original_form": "나도 음료들고 집까지 온 적 많음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:42"
        },
        {
          "id": "MDRW2100005829.1.9",
          "form": "친구만날때 빼고? 는 안가",
          "original_form": "친구만날때 빼고? 는 안가",
          "speaker_id": "1",
          "time": "20210907 13:42"
        },
        {
          "id": "MDRW2100005829.1.10",
          "form": "주로 어디?",
          "original_form": "주로 어디?",
          "speaker_id": "2",
          "time": "20210907 13:42"
        },
        {
          "id": "MDRW2100005829.1.11",
          "form": "그리고 막 스벅이나 투썸이런카페는 테이크아웃하기 너무아까워",
          "original_form": "그리고 막 스벅이나 투썸이런카페는 테이크아웃하기 너무아까워",
          "speaker_id": "1",
          "time": "20210907 13:42"
        },
        {
          "id": "MDRW2100005829.1.12",
          "form": "그래서안가",
          "original_form": "그래서안가",
          "speaker_id": "1",
          "time": "20210907 13:42"
        },
        {
          "id": "MDRW2100005829.1.13",
          "form": "친구만날때는  주로 서울이니까 서울 개인카페가지",
          "original_form": "친구만날때는  주로 서울이니까 서울 개인카페가지",
          "speaker_id": "1",
          "time": "20210907 13:43"
        },
        {
          "id": "MDRW2100005829.1.14",
          "form": "서울 멀으?",
          "original_form": "서울 멀으?",
          "speaker_id": "2",
          "time": "20210907 13:43"
        },
        {
          "id": "MDRW2100005829.1.15",
          "form": "이번에 쿼카카페가려고><",
          "original_form": "이번에 쿼카카페가려고><",
          "speaker_id": "1",
          "time": "20210907 13:43"
        },
        {
          "id": "MDRW2100005829.1.16",
          "form": "서울 멀으???",
          "original_form": "서울 멀으???",
          "speaker_id": "1",
          "time": "20210907 13:43"
        },
        {
          "id": "MDRW2100005829.1.17",
          "form": "머냐고 오는데",
          "original_form": "머냐고 오는데",
          "speaker_id": "2",
          "time": "20210907 13:43"
        },
        {
          "id": "MDRW2100005829.1.18",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:43"
        },
        {
          "id": "MDRW2100005829.1.19",
          "form": "쿼카 동물이 있는 카페인줄",
          "original_form": "쿼카 동물이 있는 카페인줄",
          "speaker_id": "2",
          "time": "20210907 13:44"
        },
        {
          "id": "MDRW2100005829.1.20",
          "form": "당연히멀지 버스타구 네시간반걸려 ㅋㅋㅋ",
          "original_form": "당연히멀지 버스타구 네시간반걸려 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100005829.1.21",
          "form": "편도로 ㅋㅋ",
          "original_form": "편도로 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100005829.1.22",
          "form": "서울엔 카페 많아서 좋다 ㅋㅋㅋㅋㅋㅋ 걍 아무데나 들어가도 다 이쁘고 맛있음",
          "original_form": "서울엔 카페 많아서 좋다 ㅋㅋㅋㅋㅋㅋ 걍 아무데나 들어가도 다 이쁘고 맛있음",
          "speaker_id": "1",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100005829.1.23",
          "form": "그건 맞아",
          "original_form": "그건 맞아",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100005829.1.24",
          "form": "수플레 팬케이크 먹어봄? 존맛임",
          "original_form": "수플레 팬케이크 먹어봄? 존맛임",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100005829.1.25",
          "form": "후후 예전에 먹어봤지",
          "original_form": "후후 예전에 먹어봤지",
          "speaker_id": "1",
          "time": "20210907 13:52"
        },
        {
          "id": "MDRW2100005829.1.26",
          "form": "여자들의... 식성을 얕보지말라",
          "original_form": "여자들의... 식성을 얕보지말라",
          "speaker_id": "1",
          "time": "20210907 13:53"
        },
        {
          "id": "MDRW2100005829.1.27",
          "form": "나는 아직 못 먹어본게 많아서 요즘은 뭐 사는거에 쓰는 것 보다 먹부림이나 구경 다니는거에 더 쓰고싶음 맛집탐방 같은거네",
          "original_form": "나는 아직 못 먹어본게 많아서 요즘은 뭐 사는거에 쓰는 것 보다 먹부림이나 구경 다니는거에 더 쓰고싶음 맛집탐방 같은거네",
          "speaker_id": "2",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100005829.1.28",
          "form": "전에 비건식당 가봤는데 맛있더라구",
          "original_form": "전에 비건식당 가봤는데 맛있더라구",
          "speaker_id": "2",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100005829.1.29",
          "form": "아 그때 슨스에쓴거봣옼ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 그때 슨스에쓴거봣옼ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100005829.1.30",
          "form": "비건 카페도 괜찮았어 거기 빵 맛있었음",
          "original_form": "비건 카페도 괜찮았어 거기 빵 맛있었음",
          "speaker_id": "2",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100005829.1.31",
          "form": "포항엔\r\n그런거\r\n없어",
          "original_form": "포항엔\r\n그런거\r\n없어",
