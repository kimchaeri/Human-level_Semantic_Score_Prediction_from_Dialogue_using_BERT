      "utterance": [
        {
          "id": "MDRW2100047629.1.1",
          "form": "저는 옥뷰티요ㅋㅋ",
          "original_form": "저는 옥뷰티요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:18"
        },
        {
          "id": "MDRW2100047629.1.2",
          "form": "옥뷰티 누구죠???",
          "original_form": "옥뷰티 누구죠???",
          "speaker_id": "1",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100047629.1.3",
          "form": "옥슈슈님???",
          "original_form": "옥슈슈님???",
          "speaker_id": "1",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100047629.1.4",
          "form": "서옥쌤...?",
          "original_form": "서옥쌤...?",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100047629.1.5",
          "form": "아ㅋㅋㅋㅋ 서로 다른 사람을",
          "original_form": "아ㅋㅋㅋㅋ 서로 다른 사람을",
          "speaker_id": "1",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100047629.1.6",
          "form": "말하고 있었네요",
          "original_form": "말하고 있었네요",
          "speaker_id": "1",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100047629.1.7",
          "form": "ㅋㅋㅋㅋㅋ옥슈슈님이라는 분도 뷰티유튜버신가봐요",
          "original_form": "ㅋㅋㅋㅋㅋ옥슈슈님이라는 분도 뷰티유튜버신가봐요",
          "speaker_id": "2",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100047629.1.8",
          "form": "ㅋㅋㅋㅋ네 맞아요 ㅎ",
          "original_form": "ㅋㅋㅋㅋ네 맞아요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100047629.1.9",
          "form": "근데 잘 보진 않아요 ㅎ",
          "original_form": "근데 잘 보진 않아요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100047629.1.10",
          "form": "저도 가끔 화장하다 새롭게 변신하고싶을 때 잠깐씩 봐요ㅋㅋㅋ",
          "original_form": "저도 가끔 화장하다 새롭게 변신하고싶을 때 잠깐씩 봐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100047629.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 전 대리만족으로요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 전 대리만족으로요",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100047629.1.12",
          "form": "제가 잘 안해서..",
          "original_form": "제가 잘 안해서..",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100047629.1.13",
          "form": "ㅋㅋㅋ저는 화장하는걸 좋아해서 화장품리뷰도 자주봐요",
          "original_form": "ㅋㅋㅋ저는 화장하는걸 좋아해서 화장품리뷰도 자주봐요",
          "speaker_id": "2",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100047629.1.14",
          "form": "ㅎ 오 정말요",
          "original_form": "ㅎ 오 정말요",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100047629.1.15",
          "form": "그거 말고는 또 뭐보세요 ㅎ",
          "original_form": "그거 말고는 또 뭐보세요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100047629.1.16",
          "form": "참피디님 먹방...?",
          "original_form": "참피디님 먹방...?",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100047629.1.17",
          "form": "아하ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아하ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:43"
        },
        {
          "id": "MDRW2100047629.1.18",
          "form": "두루두루 보시네요",
          "original_form": "두루두루 보시네요",
          "speaker_id": "1",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100047629.1.19",
          "form": "네 ㅋㅋㅋ다양하게 봅니다ㅋㅋ",
