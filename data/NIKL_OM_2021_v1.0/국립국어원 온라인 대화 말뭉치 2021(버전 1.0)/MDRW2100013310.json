      "utterance": [
        {
          "id": "MDRW2100013310.1.1",
          "form": "지금 거긴 비 오나요?",
          "original_form": "지금 거긴 비 오나요?",
          "speaker_id": "2",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013310.1.2",
          "form": "네 비가와요",
          "original_form": "네 비가와요",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013310.1.3",
          "form": "name2 지역은 어떠신가요",
          "original_form": "&name2& 지역은 어떠신가요",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013310.1.4",
          "form": "여기도 비가 계속 오네요",
          "original_form": "여기도 비가 계속 오네요",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013310.1.5",
          "form": "답이 늦었습니다.",
          "original_form": "답이 늦었습니다.",
          "speaker_id": "1",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013310.1.6",
          "form": "태풍이라 그러더니 장마가 길어지려나봐요",
          "original_form": "태풍이라 그러더니 장마가 길어지려나봐요",
          "speaker_id": "1",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013310.1.7",
          "form": "남부는 피해가 심해서 밭이 다 사라졌더라구요ㅠㅠ",
          "original_form": "남부는 피해가 심해서 밭이 다 사라졌더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013310.1.8",
          "form": "헉 그럴수가",
          "original_form": "헉 그럴수가",
          "speaker_id": "1",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013310.1.9",
          "form": "피해가 심각하네요",
          "original_form": "피해가 심각하네요",
          "speaker_id": "1",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013310.1.10",
          "form": "이번 여름때도 피해가심각하던데",
          "original_form": "이번 여름때도 피해가심각하던데",
          "speaker_id": "1",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013310.1.11",
          "form": "그러니까요 날씨가 요즘 참 이상하죠",
          "original_form": "그러니까요 날씨가 요즘 참 이상하죠",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013310.1.12",
          "form": "기상이후 때문이라던데 환경문제 진짜심각한가봐요ㅠ",
          "original_form": "기상이후 때문이라던데 환경문제 진짜심각한가봐요ㅠ",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013310.1.13",
          "form": "날씨도 점점동남아처럼 변하간다던데 걱정입니다",
          "original_form": "날씨도 점점동남아처럼 변하간다던데 걱정입니다",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013310.1.14",
          "form": "저는 가을을 굉장히 좋아하는 사람인데",
          "original_form": "저는 가을을 굉장히 좋아하는 사람인데",
          "speaker_id": "2",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013310.1.15",
          "form": "조금 슬퍼지네요",
          "original_form": "조금 슬퍼지네요",
          "speaker_id": "2",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013310.1.16",
          "form": "속상하시겠어요",
          "original_form": "속상하시겠어요",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013310.1.17",
          "form": "가을도점점짧아진다던데",
          "original_form": "가을도점점짧아진다던데",
          "speaker_id": "1",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013310.1.18",
          "form": "빨리 장마가 끝나면 좋겠네요",
          "original_form": "빨리 장마가 끝나면 좋겠네요",
          "speaker_id": "2",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100013310.1.19",
          "form": "그러게 말이에요",
          "original_form": "그러게 말이에요",
          "speaker_id": "1",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100013310.1.20",
          "form": "비오는거 정말 지겹습니다",
          "original_form": "비오는거 정말 지겹습니다",
          "speaker_id": "1",
          "time": "20210825 14:14"
        }
      ]
