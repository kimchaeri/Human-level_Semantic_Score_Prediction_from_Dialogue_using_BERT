      "utterance": [
        {
          "id": "MDRW2100031973.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 10:06"
        },
        {
          "id": "MDRW2100031973.1.2",
          "form": "네 영화 좋아해요",
          "original_form": "네 영화 좋아해요",
          "speaker_id": "2",
          "time": "20210905 10:09"
        },
        {
          "id": "MDRW2100031973.1.3",
          "form": "최근에 영화관은 자주 못갔지만",
          "original_form": "최근에 영화관은 자주 못갔지만",
          "speaker_id": "2",
          "time": "20210905 10:09"
        },
        {
          "id": "MDRW2100031973.1.4",
          "form": "최근에 뭐 보셨나요",
          "original_form": "최근에 뭐 보셨나요",
          "speaker_id": "1",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031973.1.5",
          "form": "최근에 영화관 가서 본 건...",
          "original_form": "최근에 영화관 가서 본 건...",
          "speaker_id": "2",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100031973.1.6",
          "form": "크루엘라요",
          "original_form": "크루엘라요",
          "speaker_id": "2",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100031973.1.7",
          "form": "아 봣어요",
          "original_form": "아 봣어요",
          "speaker_id": "1",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100031973.1.8",
          "form": "걸크러쉬",
          "original_form": "걸크러쉬",
          "speaker_id": "1",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100031973.1.9",
          "form": "네네 재미있었어요",
          "original_form": "네네 재미있었어요",
          "speaker_id": "2",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031973.1.10",
          "form": "네 재밌게봤어요 ㅎㅎ",
          "original_form": "네 재밌게봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031973.1.11",
          "form": "모가디슈 봤는데",
          "original_form": "모가디슈 봤는데",
          "speaker_id": "1",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031973.1.12",
          "form": "재밌었어요",
          "original_form": "재밌었어요",
          "speaker_id": "1",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031973.1.13",
          "form": "아 모가디슈 재밌다고 들었어요",
          "original_form": "아 모가디슈 재밌다고 들었어요",
          "speaker_id": "2",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031973.1.14",
          "form": "영화관 가서 봐야지 했는데",
          "original_form": "영화관 가서 봐야지 했는데",
          "speaker_id": "2",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031973.1.15",
          "form": "좀기다렸다 vod 서비스 하면 봐야겠어요",
          "original_form": "좀기다렸다 vod 서비스 하면 봐야겠어요",
          "speaker_id": "2",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031973.1.16",
          "form": "네 연기 잘합니다",
          "original_form": "네 연기 잘합니다",
          "speaker_id": "1",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031973.1.17",
          "form": "기대되네요",
          "original_form": "기대되네요",
          "speaker_id": "2",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100031973.1.18",
          "form": "넷플릭스도 보시나요?",
          "original_form": "넷플릭스도 보시나요?",
          "speaker_id": "2",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100031973.1.19",
          "form": "네 요새 디피 봐요 ㅎㅎ",
          "original_form": "네 요새 디피 봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031973.1.20",
          "form": "아 재미있나요?",
          "original_form": "아 재미있나요?",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031973.1.21",
          "form": "상위권에 랭크된 건 봤는데",
          "original_form": "상위권에 랭크된 건 봤는데",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031973.1.22",
          "form": "아 죽습니다",
          "original_form": "아 죽습니다",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100031973.1.23",
          "form": "구교환 너무",
          "original_form": "구교환 너무",
