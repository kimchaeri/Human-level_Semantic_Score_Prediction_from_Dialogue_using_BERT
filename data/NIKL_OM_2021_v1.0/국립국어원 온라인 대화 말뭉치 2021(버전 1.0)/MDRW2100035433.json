      "utterance": [
        {
          "id": "MDRW2100035433.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100035433.1.2",
          "form": "안녕하세료",
          "original_form": "안녕하세료",
          "speaker_id": "1",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100035433.1.3",
          "form": "오늘 점심 뭐 드셨어요?",
          "original_form": "오늘 점심 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100035433.1.4",
          "form": "두유 먹엇어요..",
          "original_form": "두유 먹엇어요..",
          "speaker_id": "1",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100035433.1.5",
          "form": "배 안 고프세요?",
          "original_form": "배 안 고프세요?",
          "speaker_id": "2",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100035433.1.6",
          "form": "저녁에 밥먹은걸로 만족하려고요",
          "original_form": "저녁에 밥먹은걸로 만족하려고요",
          "speaker_id": "1",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100035433.1.7",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100035433.1.8",
          "form": "동생이랑 술먹으려고요",
          "original_form": "동생이랑 술먹으려고요",
          "speaker_id": "1",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100035433.1.9",
          "form": "동생이랑 술 자주 먹나봐요",
          "original_form": "동생이랑 술 자주 먹나봐요",
          "speaker_id": "2",
          "time": "20210829 21:10"
        },
        {
          "id": "MDRW2100035433.1.10",
          "form": "종종먹어욬ㅋㅋ",
          "original_form": "종종먹어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100035433.1.11",
          "form": "안주도 함께 준비하셨나요",
          "original_form": "안주도 함께 준비하셨나요",
          "speaker_id": "2",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100035433.1.12",
          "form": "살찔까봐 술만 먹으려고요.",
          "original_form": "살찔까봐 술만 먹으려고요.",
          "speaker_id": "1",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100035433.1.13",
          "form": "그러시군요\n술만 먹으면 금방 취하실텐데",
          "original_form": "그러시군요\n술만 먹으면 금방 취하실텐데",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100035433.1.14",
          "form": "ㅋㅋ 그렇게 많이마쉬지않아서 괜찮아요..",
          "original_form": "ㅋㅋ 그렇게 많이마쉬지않아서 괜찮아요..",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100035433.1.15",
          "form": "그렇다면 다행이네요",
          "original_form": "그렇다면 다행이네요",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100035433.1.16",
          "form": "술 잘 마시는 편이신가요?",
          "original_form": "술 잘 마시는 편이신가요?",
          "speaker_id": "2",
          "time": "20210829 21:17"
        }
      ]
