      "utterance": [
        {
          "id": "MDRW2100007466.1.1",
          "form": "으 진짜 너무 싫어요ㅠㅠ",
          "original_form": "으 진짜 너무 싫어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007466.1.2",
          "form": "모기는 진짜 왜 존재할까요...",
          "original_form": "모기는 진짜 왜 존재할까요...",
          "speaker_id": "1",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007466.1.3",
          "form": "ㅠㅠㅠ그러게요..",
          "original_form": "ㅠㅠㅠ그러게요..",
          "speaker_id": "2",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007466.1.4",
          "form": "전 무는 것까진 그렇다 치는데",
          "original_form": "전 무는 것까진 그렇다 치는데",
          "speaker_id": "2",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007466.1.5",
          "form": "왱왱거리지만 않았으면 좋겠어요 ㅋㅋㅠㅠ",
          "original_form": "왱왱거리지만 않았으면 좋겠어요 ㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007466.1.6",
          "form": "그거 너무 짜증나요ㅋㅋㅋㅠ",
          "original_form": "그거 너무 짜증나요ㅋㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007466.1.7",
          "form": "들리면 잡기 전까지 잠을 못 자는...",
          "original_form": "들리면 잡기 전까지 잠을 못 자는...",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007466.1.8",
          "form": "그니까요ㅋㅋㅋㅋㅠ",
          "original_form": "그니까요ㅋㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007466.1.9",
          "form": "자려고 누웠는데 갑자기..",
          "original_form": "자려고 누웠는데 갑자기..",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007466.1.10",
          "form": "...왱~",
          "original_form": "...왱~",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007466.1.11",
          "form": "여름엔 전기파리채 필수예요...",
          "original_form": "여름엔 전기파리채 필수예요...",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007466.1.12",
          "form": "맞아요.. 꼭 머리맡에 두고 자야해요",
          "original_form": "맞아요.. 꼭 머리맡에 두고 자야해요",
          "speaker_id": "2",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007466.1.13",
          "form": "이번 여름엔 모기 좀 없나 싶었는데",
          "original_form": "이번 여름엔 모기 좀 없나 싶었는데",
          "speaker_id": "1",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007466.1.14",
          "form": "아직 안 나온 거였다니ㅠ",
          "original_form": "아직 안 나온 거였다니ㅠ",
          "speaker_id": "1",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007466.1.15",
          "form": "ㅋㅋㅋㅋㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007466.1.16",
          "form": "살짝 팁 드리자면",
          "original_form": "살짝 팁 드리자면",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007466.1.17",
          "form": "계피향 안 싫어하시면",
          "original_form": "계피향 안 싫어하시면",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007466.1.18",
          "form": "계피 스프레이로 만들어서 방에 뿌려두면",
          "original_form": "계피 스프레이로 만들어서 방에 뿌려두면",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007466.1.19",
          "form": "확실히 덜 오는것같아요",
          "original_form": "확실히 덜 오는것같아요",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007466.1.20",
          "form": "오 완전 꿀팁이에요!",
          "original_form": "오 완전 꿀팁이에요!",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007466.1.21",
          "form": "계피...구해봐야겠어요!",
          "original_form": "계피...구해봐야겠어요!",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007466.1.22",
          "form": "좋아요 ㅎㅎ",
          "original_form": "좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007466.1.23",
          "form": "저도 꿀팁 모기 물리고 즉시 그 부위 박박 씻으면 덜 가렵다는데 얼마전에 알았어요ㅋㅋㅋ",
          "original_form": "저도 꿀팁 모기 물리고 즉시 그 부위 박박 씻으면 덜 가렵다는데 얼마전에 알았어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007466.1.24",
          "form": "헐 오오 몰랐어요",
          "original_form": "헐 오오 몰랐어요",
          "speaker_id": "2",
          "time": "20210822 02:39"
        },
        {
          "id": "MDRW2100007466.1.25",
          "form": "와 진짜 꿀팁이예요ㅋㅋㅋ",
          "original_form": "와 진짜 꿀팁이예요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:39"
        },
        {
          "id": "MDRW2100007466.1.26",
          "form": "나중에 모기 물리면 바로 씻어야겠네요",
          "original_form": "나중에 모기 물리면 바로 씻어야겠네요",
          "speaker_id": "2",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007466.1.27",
          "form": "그쵸 모기 독을 빼야 한다더라구요ㅋㅋㅋ",
          "original_form": "그쵸 모기 독을 빼야 한다더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007466.1.28",
          "form": "오오오 그렇군요ㅋㅋㅋ",
          "original_form": "오오오 그렇군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:45"
        },
        {
          "id": "MDRW2100007466.1.29",
          "form": "저는 슬슬",
          "original_form": "저는 슬슬",
          "speaker_id": "2",
          "time": "20210822 02:45"
        },
        {
          "id": "MDRW2100007466.1.30",
          "form": "자러 가야 할 것 같아요",
          "original_form": "자러 가야 할 것 같아요",
          "speaker_id": "2",
          "time": "20210822 02:45"
        },
        {
          "id": "MDRW2100007466.1.31",
          "form": "오늘 대화 너무 재밌었어요!",
          "original_form": "오늘 대화 너무 재밌었어요!",
          "speaker_id": "2",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007466.1.32",
          "form": "저도 너무 재밌었어요!",
          "original_form": "저도 너무 재밌었어요!",
          "speaker_id": "1",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007466.1.33",
          "form": "다음에 또 대화해요! 안녕히 주무세요",
          "original_form": "다음에 또 대화해요! 안녕히 주무세요",
          "speaker_id": "1",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007466.1.34",
          "form": "좋은 밤 되세요!",
          "original_form": "좋은 밤 되세요!",
          "speaker_id": "2",
          "time": "20210822 02:49"
        }
      ]
    }
