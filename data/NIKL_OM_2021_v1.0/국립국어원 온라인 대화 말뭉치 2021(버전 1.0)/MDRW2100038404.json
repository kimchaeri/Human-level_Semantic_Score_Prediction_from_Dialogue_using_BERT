      "utterance": [
        {
          "id": "MDRW2100038404.1.1",
          "form": "너무덥네요 ㅠ",
          "original_form": "너무덥네요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100038404.1.2",
          "form": "시원한거 먹고 싶네요",
          "original_form": "시원한거 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210902 13:16"
        },
        {
          "id": "MDRW2100038404.1.3",
          "form": "저두요 ㅠㅠㅠ",
          "original_form": "저두요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100038404.1.4",
          "form": "빙수 먹고싶어요 ...",
          "original_form": "빙수 먹고싶어요 ...",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100038404.1.5",
          "form": "오늘 베스킨 파인트 사올까 싶네요",
          "original_form": "오늘 베스킨 파인트 사올까 싶네요",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100038404.1.6",
          "form": "헐너무 맛있겠네요",
          "original_form": "헐너무 맛있겠네요",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100038404.1.7",
          "form": "베스킨 맛중에",
          "original_form": "베스킨 맛중에",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100038404.1.8",
          "form": "어떤거 좋아하세요?",
          "original_form": "어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100038404.1.9",
          "form": "저 이상한나라에 솜사탕이랑 에플민트 최애입니당",
          "original_form": "저 이상한나라에 솜사탕이랑 에플민트 최애입니당",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100038404.1.10",
          "form": "맛잘알이시네 저도",
          "original_form": "맛잘알이시네 저도",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100038404.1.11",
          "form": "이상한나라에 솜사탕 좋아해요",
          "original_form": "이상한나라에 솜사탕 좋아해요",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100038404.1.12",
          "form": "그거 너무 맛있어요 ㅠㅠ",
          "original_form": "그거 너무 맛있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100038404.1.13",
          "form": "애플민트는. 사먹어본적은 없는데 가볍고맛있을거 같아요",
          "original_form": "애플민트는. 사먹어본적은 없는데 가볍고맛있을거 같아요",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100038404.1.14",
          "form": "그거 만든사람 똑똑햐요 진짜 ㅋㅋㅋ",
          "original_form": "그거 만든사람 똑똑햐요 진짜 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100038404.1.15",
          "form": "블랙소르베도 좋아했는데",
          "original_form": "블랙소르베도 좋아했는데",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100038404.1.16",
          "form": "단종 되더라구요..  ㅋㅋㅋㅋㅋㅋ",
          "original_form": "단종 되더라구요..  ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100038404.1.17",
          "form": "헉 그 까만 아이스크림말씀하시는거죠",
          "original_form": "헉 그 까만 아이스크림말씀하시는거죠",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038404.1.18",
          "form": "저는 보기만했어요",
          "original_form": "저는 보기만했어요",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038404.1.19",
          "form": "상큼한거 좋아하시나보다",
          "original_form": "상큼한거 좋아하시나보다",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038404.1.20",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038404.1.21",
          "form": "상큼하긴한데 민트도 살짝나서",
          "original_form": "상큼하긴한데 민트도 살짝나서",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038404.1.22",
          "form": "저민트파거든요",
          "original_form": "저민트파거든요",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038404.1.23",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038404.1.24",
          "form": "저두 민트파예요",
          "original_form": "저두 민트파예요",
          "speaker_id": "1",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100038404.1.25",
          "form": "민트는 차갑게 먹는게 제일 좋아요",
          "original_form": "민트는 차갑게 먹는게 제일 좋아요",
          "speaker_id": "1",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100038404.1.26",
          "form": "오 민트파라니 ㅠㅠㅠ",
          "original_form": "오 민트파라니 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100038404.1.27",
          "form": "민트 별미죠",
          "original_form": "민트 별미죠",
          "speaker_id": "1",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100038404.1.28",
          "form": "주기적으로 생각나요",
          "original_form": "주기적으로 생각나요",
