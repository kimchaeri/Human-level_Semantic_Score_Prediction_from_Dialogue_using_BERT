      "utterance": [
        {
          "id": "MDRW2100046089.1.1",
          "form": "넉넉한 옷종류 좋아하세요?",
          "original_form": "넉넉한 옷종류 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046089.1.2",
          "form": "동생이 이런 옷도 입어보라고 성화여서요ㅋㅋㅋㅋㅋ",
          "original_form": "동생이 이런 옷도 입어보라고 성화여서요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046089.1.3",
          "form": "평소에 잘 안 입으시는 옷을 구매하신 건가 보네요!",
          "original_form": "평소에 잘 안 입으시는 옷을 구매하신 건가 보네요!",
          "speaker_id": "1",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046089.1.4",
          "form": "최근에 옷을 안 사서요ㅋㅋㅋㅋ",
          "original_form": "최근에 옷을 안 사서요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046089.1.5",
          "form": "저도 최근에는 옷을 산게 없네요ㅎㅎ",
          "original_form": "저도 최근에는 옷을 산게 없네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046089.1.6",
          "form": "그래도 여름옷은 좀 많아서 괜찮았는대",
          "original_form": "그래도 여름옷은 좀 많아서 괜찮았는대",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046089.1.7",
          "form": "가을옷은 슬슬 사야하지 않나 싶어요ㅠ",
          "original_form": "가을옷은 슬슬 사야하지 않나 싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046089.1.8",
          "form": "아 근데 가을 옷 넘 애매하지 않나요ㅠㅠ",
          "original_form": "아 근데 가을 옷 넘 애매하지 않나요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046089.1.9",
          "form": "금방 지나가서ㅠㅠ",
          "original_form": "금방 지나가서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046089.1.10",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046089.1.11",
          "form": "코트나 가디건 입고 싶은데 몇 번 입으면 겨울이라 사기가 좀 꺼려지네요ㅠㅠ",
          "original_form": "코트나 가디건 입고 싶은데 몇 번 입으면 겨울이라 사기가 좀 꺼려지네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100046089.1.12",
          "form": "네 게다가 저는 추위도 잘 타서 코트는 진짜 아까워요",
          "original_form": "네 게다가 저는 추위도 잘 타서 코트는 진짜 아까워요",
          "speaker_id": "2",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046089.1.13",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046089.1.14",
          "form": "저도 추위를 많이 타서 겨울에도 코트입고 싶은데",
          "original_form": "저도 추위를 많이 타서 겨울에도 코트입고 싶은데",
          "speaker_id": "1",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046089.1.15",
          "form": "결국 롱패딩으로 돌아오게돼욬ㅋㅋ큐ㅠㅠ",
          "original_form": "결국 롱패딩으로 돌아오게돼욬ㅋㅋ큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046089.1.16",
          "form": "롱패딩은 진짜 생존템이에요",
          "original_form": "롱패딩은 진짜 생존템이에요",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046089.1.17",
          "form": "맞아요ㅠㅠ 김밥이라고 불릴지라도 롱패딩 없으면 어떻게 살아야할지ㅠㅠ",
          "original_form": "맞아요ㅠㅠ 김밥이라고 불릴지라도 롱패딩 없으면 어떻게 살아야할지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046089.1.18",
          "form": "다시 숏패딩 유행시키려는 것 같은뎈ㅋㅋㅋㅋㅋ 벗을 수가 없어요",
          "original_form": "다시 숏패딩 유행시키려는 것 같은뎈ㅋㅋㅋㅋㅋ 벗을 수가 없어요",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046089.1.19",
          "form": "숏패딩... 견딜 수 없어요",
          "original_form": "숏패딩... 견딜 수 없어요",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046089.1.20",
          "form": "롱패딩이 나오기 전으로는 절대...",
          "original_form": "롱패딩이 나오기 전으로는 절대...",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046089.1.21",
          "form": "솔직히 위에는 껴입어도 밑에는 방법리 없잖아요ㅠㅠ",
          "original_form": "솔직히 위에는 껴입어도 밑에는 방법리 없잖아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046089.1.22",
          "form": "그쵸그쵸 이 따뜻함을 알았는데 어떻게 알기 전으로 돌아가겠어요",
          "original_form": "그쵸그쵸 이 따뜻함을 알았는데 어떻게 알기 전으로 돌아가겠어요",
          "speaker_id": "2",
          "time": "20210908 10:34"
        }
      ]
    }
