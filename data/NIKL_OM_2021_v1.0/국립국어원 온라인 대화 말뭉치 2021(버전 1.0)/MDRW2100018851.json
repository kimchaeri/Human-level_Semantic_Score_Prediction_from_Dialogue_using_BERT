      "utterance": [
        {
          "id": "MDRW2100018851.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018851.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018851.1.3",
          "form": "점심드셨나요?",
          "original_form": "점심드셨나요?",
          "speaker_id": "2",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018851.1.4",
          "form": "저는 육개장 먹었어요~",
          "original_form": "저는 육개장 먹었어요~",
          "speaker_id": "1",
          "time": "20210827 12:54"
        },
        {
          "id": "MDRW2100018851.1.5",
          "form": "와 육개장 좋죠",
          "original_form": "와 육개장 좋죠",
          "speaker_id": "2",
          "time": "20210827 12:58"
        },
        {
          "id": "MDRW2100018851.1.6",
          "form": "저는 닭꼬치 먹으려구요",
          "original_form": "저는 닭꼬치 먹으려구요",
          "speaker_id": "2",
          "time": "20210827 12:58"
        },
        {
          "id": "MDRW2100018851.1.7",
          "form": "오!! 닭꼬치 ! 밀키트 그런건가요 아님 포장마차?",
          "original_form": "오!! 닭꼬치 ! 밀키트 그런건가요 아님 포장마차?",
          "speaker_id": "1",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100018851.1.8",
          "form": "가게에서 포장해왔어요!",
          "original_form": "가게에서 포장해왔어요!",
          "speaker_id": "2",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100018851.1.9",
          "form": "롱닭꼬치는 오랜만인데 팔더라구요!",
          "original_form": "롱닭꼬치는 오랜만인데 팔더라구요!",
          "speaker_id": "2",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100018851.1.10",
          "form": "우와~ 부러운데요? 닭꼬치하는가게 못본지 오래된거같아요",
          "original_form": "우와~ 부러운데요? 닭꼬치하는가게 못본지 오래된거같아요",
          "speaker_id": "1",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100018851.1.11",
          "form": "그쵸 어릴 땐 많았는데 요즘은 드물더라구요",
          "original_form": "그쵸 어릴 땐 많았는데 요즘은 드물더라구요",
          "speaker_id": "2",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018851.1.12",
          "form": "저녁은 정하셨어요?",
          "original_form": "저녁은 정하셨어요?",
          "speaker_id": "2",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018851.1.13",
          "form": "저녁은 삼계탕 먹을거같아요~",
          "original_form": "저녁은 삼계탕 먹을거같아요~",
          "speaker_id": "1",
          "time": "20210827 13:06"
        },
        {
          "id": "MDRW2100018851.1.14",
          "form": "여름에 보양식 좋네요!",
          "original_form": "여름에 보양식 좋네요!",
          "speaker_id": "2",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100018851.1.15",
          "form": "저도 국밥류로 먹어야겠어요",
          "original_form": "저도 국밥류로 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100018851.1.16",
          "form": "맞아요~ 국밥이 든든하니 좋죠",
          "original_form": "맞아요~ 국밥이 든든하니 좋죠",
          "speaker_id": "1",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100018851.1.17",
          "form": "이제 낮인데 커피는 드셨어요?",
          "original_form": "이제 낮인데 커피는 드셨어요?",
          "speaker_id": "2",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100018851.1.18",
          "form": "커피 안좋아해요.. 제가 그 주변에 몇없다는 커피안먹는지인 입니더 ㅋㅋ",
          "original_form": "커피 안좋아해요.. 제가 그 주변에 몇없다는 커피안먹는지인 입니더 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100018851.1.19",
          "form": "아하..! 제주위에 딱 한명있어요ㅋㅋㅋ 드문 분이셨군요",
          "original_form": "아하..! 제주위에 딱 한명있어요ㅋㅋㅋ 드문 분이셨군요",
          "speaker_id": "2",
          "time": "20210827 13:11"
        }
      ]
