      "utterance": [
        {
          "id": "MDRW2100032057.1.1",
          "form": "전어 너무 맛있죠",
          "original_form": "전어 너무 맛있죠",
          "speaker_id": "2",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032057.1.2",
          "form": "가을이라 전어가 많이 나겠어요 저도 전어 굉장히 좋아해요",
          "original_form": "가을이라 전어가 많이 나겠어요 저도 전어 굉장히 좋아해요",
          "speaker_id": "2",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032057.1.3",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032057.1.4",
          "form": "인도커리 주문해드시는건 어떤가요?",
          "original_form": "인도커리 주문해드시는건 어떤가요?",
          "speaker_id": "2",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032057.1.5",
          "form": "인터넷으로도 발달이 잘되어 있던데요!",
          "original_form": "인터넷으로도 발달이 잘되어 있던데요!",
          "speaker_id": "2",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032057.1.6",
          "form": "마켓컬리에 팔더라고요 한번 사먹어볼까 하고 있어요",
          "original_form": "마켓컬리에 팔더라고요 한번 사먹어볼까 하고 있어요",
          "speaker_id": "1",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032057.1.7",
          "form": "좋네요!",
          "original_form": "좋네요!",
          "speaker_id": "2",
          "time": "20210905 19:20"
        },
        {
          "id": "MDRW2100032057.1.8",
          "form": "마켓컬리전 이용해본적 없는데 자주 이용하시나요?",
          "original_form": "마켓컬리전 이용해본적 없는데 자주 이용하시나요?",
          "speaker_id": "2",
          "time": "20210905 19:21"
        },
        {
          "id": "MDRW2100032057.1.9",
          "form": "한번밖에 안써봤는데 캠핑하는 친구들이 밀키트 많이 산대서 저도 사볼까해요 ㅋㅋ",
          "original_form": "한번밖에 안써봤는데 캠핑하는 친구들이 밀키트 많이 산대서 저도 사볼까해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 11:36"
        },
        {
          "id": "MDRW2100032057.1.10",
          "form": "맞아요 캠핑에서도 굉장히 편하죠",
          "original_form": "맞아요 캠핑에서도 굉장히 편하죠",
          "speaker_id": "2",
          "time": "20210906 12:40"
        },
        {
          "id": "MDRW2100032057.1.11",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 12:40"
        },
        {
          "id": "MDRW2100032057.1.12",
          "form": "오늘 점심은 뭐 드셨어요?",
          "original_form": "오늘 점심은 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210906 12:49"
        },
        {
          "id": "MDRW2100032057.1.13",
          "form": "지금 먹을 게 없어서 밀키트 미리 사둘걸 싶네요",
          "original_form": "지금 먹을 게 없어서 밀키트 미리 사둘걸 싶네요",
          "speaker_id": "1",
          "time": "20210906 12:49"
        },
        {
          "id": "MDRW2100032057.1.14",
          "form": "저는 유부초밥먹었어요 ㅎㅎ",
          "original_form": "저는 유부초밥먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100032057.1.15",
          "form": "밀키트 어떤걸로 주문하실 계획잇ㄴ가요??",
          "original_form": "밀키트 어떤걸로 주문하실 계획잇ㄴ가요??",
          "speaker_id": "2",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100032057.1.16",
          "form": "저는 간단하게 나베도 잘되어있던데 그것도 좋더라구요",
          "original_form": "저는 간단하게 나베도 잘되어있던데 그것도 좋더라구요",
          "speaker_id": "2",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100032057.1.17",
          "form": "감자탕이 맛있대요!",
          "original_form": "감자탕이 맛있대요!",
          "speaker_id": "1",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100032057.1.18",
          "form": "밀푀유나베요!",
          "original_form": "밀푀유나베요!",
          "speaker_id": "2",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100032057.1.19",
          "form": "아 그거도 관심있어요",
          "original_form": "아 그거도 관심있어요",
          "speaker_id": "1",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100032057.1.20",
          "form": "여기선 안파는 음식이라 ㅠ",
          "original_form": "여기선 안파는 음식이라 ㅠ",
          "speaker_id": "1",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100032057.1.21",
          "form": "그죠 그럴때 밀키트로 먹으면 괜찮을 것 같아요 ㅎㅎ",
          "original_form": "그죠 그럴때 밀키트로 먹으면 괜찮을 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 13:24"
        },
        {
          "id": "MDRW2100032057.1.22",
          "form": "감자탕도 맛있나요??",
          "original_form": "감자탕도 맛있나요??",
          "speaker_id": "2",
          "time": "20210906 13:24"
        },
        {
          "id": "MDRW2100032057.1.23",
          "form": "사먹어보고싶네요 ㅎㅇ",
          "original_form": "사먹어보고싶네요 ㅎㅇ",
          "speaker_id": "2",
          "time": "20210906 13:24"
        },
        {
          "id": "MDRW2100032057.1.24",
          "form": "찾아보니 마라샹궈 밀키트도 있어요",
          "original_form": "찾아보니 마라샹궈 밀키트도 있어요",
          "speaker_id": "1",
          "time": "20210906 13:25"
        },
        {
          "id": "MDRW2100032057.1.25",
          "form": "맛있으려나",
          "original_form": "맛있으려나",
          "speaker_id": "1",
          "time": "20210906 13:25"
        },
        {
          "id": "MDRW2100032057.1.26",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100032057.1.27",
          "form": "궁금하네요 ㅎㅎ",
          "original_form": "궁금하네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100032057.1.28",
          "form": "마라샹궈 먹어보고싶은데 도전해보고 맛있으면 알려주세요",
          "original_form": "마라샹궈 먹어보고싶은데 도전해보고 맛있으면 알려주세요",
          "speaker_id": "2",
          "time": "20210906 17:52"
        }
      ]
    }
