      "utterance": [
        {
          "id": "MDRW2100041706.1.1",
          "form": "요리 자주 하세요?",
          "original_form": "요리 자주 하세요?",
          "speaker_id": "1",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041706.1.2",
          "form": "아니요 거의 안해요ㅋㅋ",
          "original_form": "아니요 거의 안해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100041706.1.3",
          "form": "생존에 위협을ㅈ느낄때만 합니당",
          "original_form": "생존에 위협을ㅈ느낄때만 합니당",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100041706.1.4",
          "form": "요리 좋아하시나요",
          "original_form": "요리 좋아하시나요",
          "speaker_id": "2",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100041706.1.5",
          "form": "아ㅋㅋㅋ 저도 요리 별로...",
          "original_form": "아ㅋㅋㅋ 저도 요리 별로...",
          "speaker_id": "1",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100041706.1.6",
          "form": "가끔하니까 더 안느는거 같아요",
          "original_form": "가끔하니까 더 안느는거 같아요",
          "speaker_id": "1",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100041706.1.7",
          "form": "요즘 조미료가 잘나와서",
          "original_form": "요즘 조미료가 잘나와서",
          "speaker_id": "2",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100041706.1.8",
          "form": "연두 같은거 넣으면 금방 맛있어지더라구요",
          "original_form": "연두 같은거 넣으면 금방 맛있어지더라구요",
          "speaker_id": "1",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041706.1.9",
          "form": "저는 조미료를 믿어요ㅋㅋ",
          "original_form": "저는 조미료를 믿어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041706.1.10",
          "form": "찌개할때도 라면스프 자주 넣어요ㅋㅋㅋ",
          "original_form": "찌개할때도 라면스프 자주 넣어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041706.1.11",
          "form": "김치찌개같은거요?",
          "original_form": "김치찌개같은거요?",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041706.1.12",
          "form": "떡볶이할때 라면스프있으면",
          "original_form": "떡볶이할때 라면스프있으면",
          "speaker_id": "2",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041706.1.13",
          "form": "초딩들도 잘하더라구요",
          "original_form": "초딩들도 잘하더라구요",
          "speaker_id": "2",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041706.1.14",
          "form": "네네 부대찌개할때도 넣구ㅋㅋ",
          "original_form": "네네 부대찌개할때도 넣구ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041706.1.15",
          "form": "부대찌개하실때",
          "original_form": "부대찌개하실때",
          "speaker_id": "2",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041706.1.16",
          "form": "면사리같은거 혹시 넣어드시나요",
          "original_form": "면사리같은거 혹시 넣어드시나요",
          "speaker_id": "2",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041706.1.17",
          "form": "네 넣어요. 라면사리 맛있잖아요",
          "original_form": "네 넣어요. 라면사리 맛있잖아요",
          "speaker_id": "1",
          "time": "20210906 14:55"
        },
        {
          "id": "MDRW2100041706.1.18",
          "form": "그쵸 근데 한번에 다 안먹으면 불어서 그게좀 고민이에요",
          "original_form": "그쵸 근데 한번에 다 안먹으면 불어서 그게좀 고민이에요",
          "speaker_id": "2",
          "time": "20210906 15:06"
        },
        {
          "id": "MDRW2100041706.1.19",
          "form": "그건 그래요. 그래서 라면만 열심히 찾아내서 먹어요ㅋㅋ",
          "original_form": "그건 그래요. 그래서 라면만 열심히 찾아내서 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100041706.1.20",
          "form": "역시 그 방법밖에 없는거군요",
          "original_form": "역시 그 방법밖에 없는거군요",
          "speaker_id": "2",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100041706.1.21",
          "form": "양 조절안하면 배터질때가 있어요",
          "original_form": "양 조절안하면 배터질때가 있어요",
          "speaker_id": "2",
          "time": "20210906 16:23"
        }
      ]
