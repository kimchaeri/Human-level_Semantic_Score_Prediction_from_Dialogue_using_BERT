        {
          "id": "MDRW2100020159.1.1",
          "form": "사실 입사첫날에",
          "original_form": "사실 입사첫날에",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020159.1.2",
          "form": "야근을 시키시는거에요",
          "original_form": "야근을 시키시는거에요",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020159.1.3",
          "form": "심지어 금요일이엇어요",
          "original_form": "심지어 금요일이엇어요",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020159.1.4",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020159.1.5",
          "form": "너무 빡치는거에요",
          "original_form": "너무 빡치는거에요",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020159.1.6",
          "form": "첫날에...!!!!",
          "original_form": "첫날에...!!!!",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020159.1.7",
          "form": "대학간거도 사실 꿀일줄알고",
          "original_form": "대학간거도 사실 꿀일줄알고",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020159.1.8",
          "form": "들어간건데",
          "original_form": "들어간건데",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020159.1.9",
          "form": "빡칠만해요",
          "original_form": "빡칠만해요",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020159.1.10",
          "form": "심지어 신입인 저만!",
          "original_form": "심지어 신입인 저만!",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020159.1.11",
          "form": "그래서 주말에 각성하고",
          "original_form": "그래서 주말에 각성하고",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020159.1.12",
          "form": "헐??? 아니 신고식도 아니고",
          "original_form": "헐??? 아니 신고식도 아니고",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020159.1.13",
          "form": "그다음주월요일부터 정말 칼같이 인사하고나갓어요",
          "original_form": "그다음주월요일부터 정말 칼같이 인사하고나갓어요",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020159.1.14",
          "form": "퇴근은 양보할수없다",
          "original_form": "퇴근은 양보할수없다",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020159.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ잘하셨네요 진짜",
          "original_form": "ㅋㅋㅋㅋㅋㅋ잘하셨네요 진짜",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020159.1.16",
          "form": "근데 제덕에 다들 눈치보면서 못갓엇는데",
          "original_form": "근데 제덕에 다들 눈치보면서 못갓엇는데",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020159.1.17",
          "form": "근데 제덕에 다들 눈치보면서 못갓엇는데",
          "original_form": "근데 제덕에 다들 눈치보면서 못갓엇는데",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020159.1.18",
          "form": "하나둘 따라나와서",
          "original_form": "하나둘 따라나와서",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020159.1.19",
          "form": "내 퇴근은 내가 지킨다!!!",
          "original_form": "내 퇴근은 내가 지킨다!!!",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020159.1.20",
          "form": "나중엔 거의다같이 칼퇴햇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "나중엔 거의다같이 칼퇴햇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020159.1.21",
          "form": "팀장님 끝까지 인사 안받아주시곸ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "팀장님 끝까지 인사 안받아주시곸ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020159.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ팀장님 혼자 쓸쓸하게...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ팀장님 혼자 쓸쓸하게...",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020159.1.23",
          "form": "저희 본부장님도 워커홀릭이셔서...라떼얘기하시면서 야근 권장 자주하세요ㅋ",
          "original_form": "저희 본부장님도 워커홀릭이셔서...라떼얘기하시면서 야근 권장 자주하세요ㅋ",
