      "utterance": [
        {
          "id": "MDRW2100044332.1.1",
          "form": "진짜 많다 중국 면 종류",
          "original_form": "진짜 많다 중국 면 종류",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044332.1.2",
          "form": "뭐뭐 먹어봤어 너는?",
          "original_form": "뭐뭐 먹어봤어 너는?",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044332.1.3",
          "form": "중국은 땅이 넓어서 그런가",
          "original_form": "중국은 땅이 넓어서 그런가",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044332.1.4",
          "form": "훠궈면, 옥수수면, 중국당면, 분모자",
          "original_form": "훠궈면, 옥수수면, 중국당면, 분모자",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044332.1.5",
          "form": "이렇게먹어본것같어",
          "original_form": "이렇게먹어본것같어",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044332.1.6",
          "form": "아 훠궈면이 따로 있어?",
          "original_form": "아 훠궈면이 따로 있어?",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044332.1.7",
          "form": "응 그건 사각형의 거의",
          "original_form": "응 그건 사각형의 거의",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044332.1.8",
          "form": "떡맛이야",
          "original_form": "떡맛이야",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044332.1.9",
          "form": "분모자는 떡이 아니라 면 류였어?!",
          "original_form": "분모자는 떡이 아니라 면 류였어?!",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044332.1.10",
          "form": "나 컬쳐쇼크 연속으로 받는 중",
          "original_form": "나 컬쳐쇼크 연속으로 받는 중",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044332.1.11",
          "form": "응 면류야",
          "original_form": "응 면류야",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044332.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044332.1.13",
          "form": "분모자도 인기많아",
          "original_form": "분모자도 인기많아",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044332.1.14",
          "form": "분모자는 그 니가 저번에 말한 부산 물떡이랑 비슷해?",
          "original_form": "분모자는 그 니가 저번에 말한 부산 물떡이랑 비슷해?",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044332.1.15",
          "form": "음 좀 달라",
          "original_form": "음 좀 달라",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044332.1.16",
          "form": "어떻게 달라?",
          "original_form": "어떻게 달라?",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044332.1.17",
          "form": "땐땐해서",
          "original_form": "땐땐해서",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044332.1.18",
          "form": "쫀득 땐땐해",
          "original_form": "쫀득 땐땐해",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044332.1.19",
          "form": "분모자가?",
          "original_form": "분모자가?",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044332.1.20",
          "form": "응 분모자가",
          "original_form": "응 분모자가",
          "speaker_id": "1",
          "time": "20210907 22:22"
        }
      ]
