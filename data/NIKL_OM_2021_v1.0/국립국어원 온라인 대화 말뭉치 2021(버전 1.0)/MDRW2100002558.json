      "utterance": [
        {
          "id": "MDRW2100002558.1.1",
          "form": "여행 어디 가고싶으세요?",
          "original_form": "여행 어디 가고싶으세요?",
          "speaker_id": "2",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002558.1.2",
          "form": "저 싱가폴이요ㅠㅠ",
          "original_form": "저 싱가폴이요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002558.1.3",
          "form": "오 싱가폴 좋죠",
          "original_form": "오 싱가폴 좋죠",
          "speaker_id": "2",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002558.1.4",
          "form": "가보셨어요?",
          "original_form": "가보셨어요?",
          "speaker_id": "2",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002558.1.5",
          "form": "못가봐서 가보고싶답니다ㅠ 배모양호텔",
          "original_form": "못가봐서 가보고싶답니다ㅠ 배모양호텔",
          "speaker_id": "1",
          "time": "20210820 14:20"
        },
        {
          "id": "MDRW2100002558.1.6",
          "form": "꼭갈거예요 코로나끝나면ㅠㅠ",
          "original_form": "꼭갈거예요 코로나끝나면ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 14:20"
        },
        {
          "id": "MDRW2100002558.1.7",
          "form": "오 배모양호텔이 유명한가요?",
          "original_form": "오 배모양호텔이 유명한가요?",
          "speaker_id": "2",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002558.1.8",
          "form": "마리나베이샌즈요ㅋㅋㅋ",
          "original_form": "마리나베이샌즈요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002558.1.9",
          "form": "야경이랑 등등 유명한곳이예요!",
          "original_form": "야경이랑 등등 유명한곳이예요!",
          "speaker_id": "1",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002558.1.10",
          "form": "우와 이름까지 알려주시고 감사해요",
          "original_form": "우와 이름까지 알려주시고 감사해요",
          "speaker_id": "2",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002558.1.11",
          "form": "한번 찾아봐야겠어요",
          "original_form": "한번 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002558.1.12",
          "form": "넵ㅋㅋㅋ진짜 제",
          "original_form": "넵ㅋㅋㅋ진짜 제",
          "speaker_id": "1",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002558.1.13",
          "form": "버킷리스트입니다",
          "original_form": "버킷리스트입니다",
          "speaker_id": "1",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002558.1.14",
          "form": "1박에 엄청비싸더라고요",
          "original_form": "1박에 엄청비싸더라고요",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002558.1.15",
          "form": "좋은 곳들은 다 비싸더라구요",
          "original_form": "좋은 곳들은 다 비싸더라구요",
          "speaker_id": "2",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002558.1.16",
          "form": "그래도 잘 수만 있다면 ㅎ",
          "original_form": "그래도 잘 수만 있다면 ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002558.1.17",
          "form": "맞아요ㅋㅋㅋ 저는 괌도",
          "original_form": "맞아요ㅋㅋㅋ 저는 괌도",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002558.1.18",
          "form": "가고싶어요ㅠ 코로나끝나면",
          "original_form": "가고싶어요ㅠ 코로나끝나면",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002558.1.19",
          "form": "우와 괌도 좋죠",
          "original_form": "우와 괌도 좋죠",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002558.1.20",
          "form": "저는 크리스마스를 미국에서 보내고싶네요",
          "original_form": "저는 크리스마스를 미국에서 보내고싶네요",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002558.1.21",
          "form": "멀었지믄요 ㅎㅎ",
          "original_form": "멀었지믄요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002558.1.22",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100002558.1.23",
          "form": "미국너무낭만적이죠",
          "original_form": "미국너무낭만적이죠",
          "speaker_id": "1",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100002558.1.24",
          "form": "ㅠㅠ 네 꼭 가고싶어요",
          "original_form": "ㅠㅠ 네 꼭 가고싶어요",
          "speaker_id": "2",
          "time": "20210822 19:56"
        }
      ]
