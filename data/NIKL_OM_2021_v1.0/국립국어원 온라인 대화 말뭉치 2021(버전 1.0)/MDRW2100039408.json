      "utterance": [
        {
          "id": "MDRW2100039408.1.1",
          "form": "제출 된건가오",
          "original_form": "제출 된건가오",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039408.1.2",
          "form": "그렇습니다",
          "original_form": "그렇습니다",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039408.1.3",
          "form": "천천히 오십셔",
          "original_form": "천천히 오십셔",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039408.1.4",
          "form": "감사핮니다",
          "original_form": "감사핮니다",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039408.1.5",
          "form": "새마을 시장에서 보통 뭐삼???",
          "original_form": "새마을 시장에서 보통 뭐삼???",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039408.1.6",
          "form": "야채요...",
          "original_form": "야채요...",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039408.1.7",
          "form": "엄마 장보러 따라가기에ㅋㅋ",
          "original_form": "엄마 장보러 따라가기에ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039408.1.8",
          "form": "거기 순대볶음 맛집인가 있던데 아나오?",
          "original_form": "거기 순대볶음 맛집인가 있던데 아나오?",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039408.1.9",
          "form": "어 혹시 부대찌개랑 같이 팔고 카드 안받는,,?",
          "original_form": "어 혹시 부대찌개랑 같이 팔고 카드 안받는,,?",
          "speaker_id": "1",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039408.1.10",
          "form": "앗 카드 안받아? 거기 맞는거 같아",
          "original_form": "앗 카드 안받아? 거기 맞는거 같아",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039408.1.11",
          "form": "보면 꾸준히 손님 있더라",
          "original_form": "보면 꾸준히 손님 있더라",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039408.1.12",
          "form": "치킨집 옆에 ㅇ0ㅇ",
          "original_form": "치킨집 옆에 ㅇ0ㅇ",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039408.1.13",
          "form": "맛있지않음 거기???",
          "original_form": "맛있지않음 거기???",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039408.1.14",
          "form": "ㅇㅇ부대찌개 묵어봄ㅋㅋ 그 순대볶음 맛있음??",
          "original_form": "ㅇㅇ부대찌개 묵어봄ㅋㅋ 그 순대볶음 맛있음??",
          "speaker_id": "2",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100039408.1.15",
          "form": "오 난 부대찌개는 못먹어봄",
          "original_form": "오 난 부대찌개는 못먹어봄",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039408.1.16",
          "form": "순대볶음 맛남 깔끔하고 맵고 양 많고",
          "original_form": "순대볶음 맛남 깔끔하고 맵고 양 많고",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039408.1.17",
          "form": "새마을시장 최고 맛집같음 갠적으로 ㅋㅋㅋ",
          "original_form": "새마을시장 최고 맛집같음 갠적으로 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039408.1.18",
          "form": "오오 담에 순대볶음 먹어야 겠따",
          "original_form": "오오 담에 순대볶음 먹어야 겠따",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039408.1.19",
          "form": "손님있을떄 지나가면 냄새가 쩔어..",
          "original_form": "손님있을떄 지나가면 냄새가 쩔어..",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039408.1.20",
          "form": "그치 그냥 못지나감ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그치 그냥 못지나감ㅋㅋㅋㅋㅋㅋ",
