      "utterance": [
        {
          "id": "MDRW2100044996.1.1",
          "form": "염색해야되는데;;;",
          "original_form": "염색해야되는데;;;",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044996.1.2",
          "form": "무슨색으로 하세요?",
          "original_form": "무슨색으로 하세요?",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044996.1.3",
          "form": "자연갈색",
          "original_form": "자연갈색",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044996.1.4",
          "form": "새치가많이생겨서 흐헝ㅜ",
          "original_form": "새치가많이생겨서 흐헝ㅜ",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044996.1.5",
          "form": "저도 예전엔 없었는데 요즘에 몇개씩 있더라구요ㅜㅜㅠ",
          "original_form": "저도 예전엔 없었는데 요즘에 몇개씩 있더라구요ㅜㅜㅠ",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044996.1.6",
          "form": "은근 거슬려요",
          "original_form": "은근 거슬려요",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044996.1.7",
          "form": "그쵸ㅠ염색하러갈시간이없네요ㅠ",
          "original_form": "그쵸ㅠ염색하러갈시간이없네요ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100044996.1.8",
          "form": "맞아요 미용실은 또 가면 금방 안끝나니까요ㅜㅜㅠ",
          "original_form": "맞아요 미용실은 또 가면 금방 안끝나니까요ㅜㅜㅠ",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100044996.1.9",
          "form": "요즘 파마도 하고싶네요",
          "original_form": "요즘 파마도 하고싶네요",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100044996.1.10",
          "form": "뿌염 너무 귀찮네요",
          "original_form": "뿌염 너무 귀찮네요",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100044996.1.11",
          "form": "셋팅펌으로여?",
          "original_form": "셋팅펌으로여?",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100044996.1.12",
          "form": "네네 가을되니까 하고싶네용ㅋㅋㅋㅋ",
          "original_form": "네네 가을되니까 하고싶네용ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100044996.1.13",
          "form": "염색은 얼마나 자주하세요?",
          "original_form": "염색은 얼마나 자주하세요?",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100044996.1.14",
          "form": "3갤에한번은해야되는듯해요",
          "original_form": "3갤에한번은해야되는듯해요",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100044996.1.15",
          "form": "더자주해야하는데못하네요",
          "original_form": "더자주해야하는데못하네요",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100044996.1.16",
          "form": "미용실가기 너무 귀찮고 시간없어서 더 그런것같아요",
          "original_form": "미용실가기 너무 귀찮고 시간없어서 더 그런것같아요",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100044996.1.17",
          "form": "셀프염색은 또 미용실만큼 잘 안되는것같코",
          "original_form": "셀프염색은 또 미용실만큼 잘 안되는것같코",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100044996.1.18",
          "form": "ㅋㄱㅋ",
          "original_form": "ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100044996.1.19",
          "form": "셀프하다가 망해서ㅋㅋㅋ",
          "original_form": "셀프하다가 망해서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100044996.1.20",
          "form": "미용실은 근데 넘비싸긴해요",
          "original_form": "미용실은 근데 넘비싸긴해요",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100044996.1.21",
          "form": "앜ㅋㅋㅋㅋㅋㅋ은근 어렵더라구요",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ은근 어렵더라구요",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100044996.1.22",
          "form": "9월안 염색이  목표ㅋㅋㅋ",
          "original_form": "9월안 염색이  목표ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
