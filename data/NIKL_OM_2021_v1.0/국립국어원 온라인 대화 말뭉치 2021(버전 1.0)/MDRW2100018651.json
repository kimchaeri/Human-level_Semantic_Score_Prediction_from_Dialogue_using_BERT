      "utterance": [
        {
          "id": "MDRW2100018651.1.1",
          "form": "여기도 비가 주륵주륵",
          "original_form": "여기도 비가 주륵주륵",
          "speaker_id": "2",
          "time": "20210827 10:04"
        },
        {
          "id": "MDRW2100018651.1.2",
          "form": "그렇군요..",
          "original_form": "그렇군요..",
          "speaker_id": "1",
          "time": "20210827 10:04"
        },
        {
          "id": "MDRW2100018651.1.3",
          "form": "추버이제 새벽에..",
          "original_form": "추버이제 새벽에..",
          "speaker_id": "1",
          "time": "20210827 10:04"
        },
        {
          "id": "MDRW2100018651.1.4",
          "form": "슬슬 극세사이불을 가져갈때가됐네요",
          "original_form": "슬슬 극세사이불을 가져갈때가됐네요",
          "speaker_id": "2",
          "time": "20210827 10:06"
        },
        {
          "id": "MDRW2100018651.1.5",
          "form": "니트들 꺼내야겠어요",
          "original_form": "니트들 꺼내야겠어요",
          "speaker_id": "2",
          "time": "20210827 10:06"
        },
        {
          "id": "MDRW2100018651.1.6",
          "form": "저 지금 이불 두개써요..극세사꺼내야지..",
          "original_form": "저 지금 이불 두개써요..극세사꺼내야지..",
          "speaker_id": "1",
          "time": "20210827 10:06"
        },
        {
          "id": "MDRW2100018651.1.7",
          "form": "비그치면 더울까바 망설이는중",
          "original_form": "비그치면 더울까바 망설이는중",
          "speaker_id": "1",
          "time": "20210827 10:07"
        },
        {
          "id": "MDRW2100018651.1.8",
          "form": "그정도는 아닌데 그렇게 추워?",
          "original_form": "그정도는 아닌데 그렇게 추워?",
          "speaker_id": "2",
          "time": "20210827 10:07"
        },
        {
          "id": "MDRW2100018651.1.9",
          "form": "밤에 추워 ㅎㅎ..",
          "original_form": "밤에 추워 ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210827 10:10"
        },
        {
          "id": "MDRW2100018651.1.10",
          "form": "선풍기때문인가 바람이 찹다..",
          "original_form": "선풍기때문인가 바람이 찹다..",
          "speaker_id": "1",
          "time": "20210827 10:10"
        },
        {
          "id": "MDRW2100018651.1.11",
          "form": "여긴 산인데 창문 열어놓고 조금 두툼한 거 덮고 자면 딱이야",
          "original_form": "여긴 산인데 창문 열어놓고 조금 두툼한 거 덮고 자면 딱이야",
          "speaker_id": "2",
          "time": "20210827 10:11"
        },
        {
          "id": "MDRW2100018651.1.12",
          "form": "진짜 비는 지겹다 이제",
          "original_form": "진짜 비는 지겹다 이제",
          "speaker_id": "2",
          "time": "20210827 10:11"
        },
        {
          "id": "MDRW2100018651.1.13",
          "form": "부럽다 공기 맑죠..",
          "original_form": "부럽다 공기 맑죠..",
          "speaker_id": "1",
          "time": "20210827 10:12"
        },
        {
          "id": "MDRW2100018651.1.14",
          "form": "여긴 안 맑아...",
          "original_form": "여긴 안 맑아...",
          "speaker_id": "1",
          "time": "20210827 10:13"
        },
        {
          "id": "MDRW2100018651.1.15",
          "form": "보은은 맑아요",
          "original_form": "보은은 맑아요",
          "speaker_id": "2",
          "time": "20210827 10:17"
        },
        {
          "id": "MDRW2100018651.1.16",
          "form": "좋다.ㅡ",
          "original_form": "좋다.ㅡ",
          "speaker_id": "1",
          "time": "20210827 10:17"
        },
        {
          "id": "MDRW2100018651.1.17",
          "form": "오창은 청주중에서 제일 공기 안좋기로 유명",
          "original_form": "오창은 청주중에서 제일 공기 안좋기로 유명",
          "speaker_id": "2",
          "time": "20210827 10:17"
        },
        {
          "id": "MDRW2100018651.1.18",
          "form": "거긴 프랜차이즈가많고 인구도 많고..차도많고..",
          "original_form": "거긴 프랜차이즈가많고 인구도 많고..차도많고..",
          "speaker_id": "1",
          "time": "20210827 10:18"
        },
        {
          "id": "MDRW2100018651.1.19",
          "form": "번화가자나",
          "original_form": "번화가자나",
          "speaker_id": "1",
          "time": "20210827 10:18"
        },
        {
          "id": "MDRW2100018651.1.20",
          "form": "산업단지라 네..",
          "original_form": "산업단지라 네..",
          "speaker_id": "2",
          "time": "20210827 10:18"
        },
        {
          "id": "MDRW2100018651.1.21",
          "form": "아시죠 산단..?",
          "original_form": "아시죠 산단..?",
          "speaker_id": "2",
          "time": "20210827 10:18"
        },
        {
          "id": "MDRW2100018651.1.22",
          "form": "고색동도 그렇지않니",
          "original_form": "고색동도 그렇지않니",
          "speaker_id": "2",
          "time": "20210827 10:18"
        },
        {
          "id": "MDRW2100018651.1.23",
          "form": "야 이제 춥다",
          "original_form": "야 이제 춥다",
          "speaker_id": "2",
          "time": "20210901 12:30"
        }
      ]
