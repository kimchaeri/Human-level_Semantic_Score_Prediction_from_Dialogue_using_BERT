        {
          "id": "MDRW2100027718.1.1",
          "form": "캄보디아는 어때",
          "original_form": "캄보디아는 어때",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027718.1.2",
          "form": "베트남과 비슷해",
          "original_form": "베트남과 비슷해",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027718.1.3",
          "form": "인접해서 같은 문화가 많아",
          "original_form": "인접해서 같은 문화가 많아",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027718.1.4",
          "form": "우리랑 중국 일본 처럼",
          "original_form": "우리랑 중국 일본 처럼",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027718.1.5",
          "form": "프놈펜은 세련되었지",
          "original_form": "프놈펜은 세련되었지",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027718.1.6",
          "form": "친구도 사귀고",
          "original_form": "친구도 사귀고",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027718.1.7",
          "form": "툭툭이가 흥정해야해서 힘들어",
          "original_form": "툭툭이가 흥정해야해서 힘들어",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027718.1.8",
          "form": "교통편이 힘든데",
          "original_form": "교통편이 힘든데",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027718.1.9",
          "form": "요즘은 바꼈을까",
          "original_form": "요즘은 바꼈을까",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027718.1.10",
          "form": "시간이 흘렀지",
          "original_form": "시간이 흘렀지",
          "speaker_id": "2",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027718.1.11",
          "form": "시엠립이 멋졌어",
          "original_form": "시엠립이 멋졌어",
          "speaker_id": "1",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100027718.1.12",
          "form": "수상마을이 놀라웠어",
          "original_form": "수상마을이 놀라웠어",
          "speaker_id": "2",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100027718.1.13",
          "form": "마을이 하늘에 떠있엇어",
          "original_form": "마을이 하늘에 떠있엇어",
          "speaker_id": "1",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100027718.1.14",
          "form": "잘지었더라",
          "original_form": "잘지었더라",
          "speaker_id": "1",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100027718.1.15",
          "form": "둥둥떠있지",
          "original_form": "둥둥떠있지",
          "speaker_id": "2",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100027718.1.16",
          "form": "공기는 나빴어",
