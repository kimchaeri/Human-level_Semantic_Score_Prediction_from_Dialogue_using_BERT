        {
          "id": "MDRW2100042432.1.1",
          "form": "서브주연이야",
          "original_form": "서브주연이야",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042432.1.2",
          "form": "문근영 나왔던건 기억나요",
          "original_form": "문근영 나왔던건 기억나요",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042432.1.3",
          "form": "너무 동안이라서 좀 와닿지가 않앗어 여기서",
          "original_form": "너무 동안이라서 좀 와닿지가 않앗어 여기서",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042432.1.4",
          "form": "문근영이 동안이긴 해요",
          "original_form": "문근영이 동안이긴 해요",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042432.1.5",
          "form": "소이현은 그다음해에 후아유 주연으로 또 찍었는데 시청률이",
          "original_form": "소이현은 그다음해에 후아유 주연으로 또 찍었는데 시청률이",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042432.1.6",
          "form": "그리고 박시후 비호감ㅋ",
          "original_form": "그리고 박시후 비호감ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042432.1.7",
          "form": "다들 비호감으로 생각할껄",
          "original_form": "다들 비호감으로 생각할껄",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042432.1.8",
          "form": "별로였겠네요 시청률이ㅋ",
          "original_form": "별로였겠네요 시청률이ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042432.1.9",
          "form": "1.4%인데",
          "original_form": "1.4%인데",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042432.1.10",
          "form": "시청률로 주연과 조연으로",
          "original_form": "시청률로 주연과 조연으로",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042432.1.11",
          "form": "소이현 옥택연 김재욱 출연",
          "original_form": "소이현 옥택연 김재욱 출연",
