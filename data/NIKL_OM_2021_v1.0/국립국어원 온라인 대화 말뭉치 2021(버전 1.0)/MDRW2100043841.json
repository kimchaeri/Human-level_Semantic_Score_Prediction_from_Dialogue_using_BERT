          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043841.1.3",
          "form": "name1님 좋아하세요??",
          "original_form": "&name1&님 좋아하세요??",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043841.1.4",
          "form": "ㅎㅎ액션좋아해요",
          "original_form": "ㅎㅎ액션좋아해요",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043841.1.5",
          "form": "저는 공포나 잔인한거 아니면 잘 봐요",
          "original_form": "저는 공포나 잔인한거 아니면 잘 봐요",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043841.1.6",
          "form": "뭐좋아하세요",
          "original_form": "뭐좋아하세요",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043841.1.7",
          "form": "베스트영화?",
          "original_form": "베스트영화?",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043841.1.8",
          "form": "베스트영화도 잘 안보고 그냥",
          "original_form": "베스트영화도 잘 안보고 그냥",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043841.1.9",
          "form": "포스터 예쁘면 보는거 같아요 ㅋㅋㅋㅋ",
          "original_form": "포스터 예쁘면 보는거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043841.1.10",
          "form": "ㅎㅎ마블보세요?",
          "original_form": "ㅎㅎ마블보세요?",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043841.1.11",
          "form": "마블도 안본답니닼ㅋㅋㅋㅋ",
          "original_form": "마블도 안본답니닼ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043841.1.12",
          "form": "꺅ㅠㅜ",
          "original_form": "꺅ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043841.1.13",
          "form": "ㅋㅋㅋ 진짜 베스트를 안보져?",
          "original_form": "ㅋㅋㅋ 진짜 베스트를 안보져?",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043841.1.14",
