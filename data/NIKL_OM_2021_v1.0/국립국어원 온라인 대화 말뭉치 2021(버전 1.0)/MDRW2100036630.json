      "utterance": [
        {
          "id": "MDRW2100036630.1.1",
          "form": "저는 올리브 오일 쓰다가 포도씨유로 최근에 바꿨는데",
          "original_form": "저는 올리브 오일 쓰다가 포도씨유로 최근에 바꿨는데",
          "speaker_id": "2",
          "time": "20210831 02:48"
        },
        {
          "id": "MDRW2100036630.1.2",
          "form": "전 선물들어오는걸로",
          "original_form": "전 선물들어오는걸로",
          "speaker_id": "1",
          "time": "20210831 02:48"
        },
        {
          "id": "MDRW2100036630.1.3",
          "form": "막씁니다",
          "original_form": "막씁니다",
          "speaker_id": "1",
          "time": "20210831 02:48"
        },
        {
          "id": "MDRW2100036630.1.4",
          "form": "몰랐는데 올리브 오일이 향이 되게 강하더라구요?",
          "original_form": "몰랐는데 올리브 오일이 향이 되게 강하더라구요?",
          "speaker_id": "2",
          "time": "20210831 02:48"
        },
        {
          "id": "MDRW2100036630.1.5",
          "form": "아하ㅋㅋㅋ 그게 좋죠!!",
          "original_form": "아하ㅋㅋㅋ 그게 좋죠!!",
          "speaker_id": "2",
          "time": "20210831 02:48"
        },
        {
          "id": "MDRW2100036630.1.6",
          "form": "저도 포도씨유 지금 쓰고있어요 ㅎㅎ",
          "original_form": "저도 포도씨유 지금 쓰고있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100036630.1.7",
          "form": "포도씨유가 가장 좋다고 들었어요",
          "original_form": "포도씨유가 가장 좋다고 들었어요",
          "speaker_id": "2",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100036630.1.8",
          "form": "그래서 제일 비싼가봐요",
          "original_form": "그래서 제일 비싼가봐요",
          "speaker_id": "2",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100036630.1.9",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100036630.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100036630.1.11",
          "form": "오호 좋은거였군여",
          "original_form": "오호 좋은거였군여",
          "speaker_id": "1",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100036630.1.12",
          "form": "오일이 종류가 많잖아요?",
          "original_form": "오일이 종류가 많잖아요?",
          "speaker_id": "2",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100036630.1.13",
          "form": "카놀라유가 안좋단건알았는데",
          "original_form": "카놀라유가 안좋단건알았는데",
          "speaker_id": "1",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100036630.1.14",
          "form": "근데 카놀라유는 요리할때 안좋다고 하더라구요ㅠㅠ",
          "original_form": "근데 카놀라유는 요리할때 안좋다고 하더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100036630.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100036630.1.16",
          "form": "네ㅜ",
          "original_form": "네ㅜ",
          "speaker_id": "1",
          "time": "20210831 02:50"
        },
        {
          "id": "MDRW2100036630.1.17",
          "form": "그래서 가장 저렴한가...",
          "original_form": "그래서 가장 저렴한가...",
          "speaker_id": "2",
          "time": "20210831 02:50"
        },
        {
          "id": "MDRW2100036630.1.18",
          "form": "파스타할때 그래서",
          "original_form": "파스타할때 그래서",
          "speaker_id": "2",
          "time": "20210831 02:50"
        },
        {
          "id": "MDRW2100036630.1.19",
          "form": "아깝더라도 포도씨유 써요",
          "original_form": "아깝더라도 포도씨유 써요",
          "speaker_id": "2",
          "time": "20210831 02:50"
        },
        {
          "id": "MDRW2100036630.1.20",
          "form": "전 있는기름으로",
          "original_form": "전 있는기름으로",
          "speaker_id": "1",
          "time": "20210831 02:50"
        }
      ]
