      "utterance": [
        {
          "id": "MDRW2100021148.1.1",
          "form": "오늘 날씨 너무덥지않나요 ㅠ",
          "original_form": "오늘 날씨 너무덥지않나요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:21"
        },
        {
          "id": "MDRW2100021148.1.2",
          "form": "저 방금 빵사러 나갔다 왔는데",
          "original_form": "저 방금 빵사러 나갔다 왔는데",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100021148.1.3",
          "form": "너무끔찍했어요",
          "original_form": "너무끔찍했어요",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100021148.1.4",
          "form": "바람은 하나 안불고 해는 쨍쨍하고",
          "original_form": "바람은 하나 안불고 해는 쨍쨍하고",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100021148.1.5",
          "form": "가을이 왜 ,,",
          "original_form": "가을이 왜 ,,",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100021148.1.6",
          "form": "오! 빵!! 저도 빵 참좋아해요 어떤빵사셨나요?",
          "original_form": "오! 빵!! 저도 빵 참좋아해요 어떤빵사셨나요?",
          "speaker_id": "2",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100021148.1.7",
          "form": "마쟈요오 너무더워서 에어컨틀엇어요ㅠ",
          "original_form": "마쟈요오 너무더워서 에어컨틀엇어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100021148.1.8",
          "form": "아니아니 지금 요기요에서 파리바게트 할인하거든요 ! 5천원 그래서 어쩔수없이 ~~~ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니아니 지금 요기요에서 파리바게트 할인하거든요 ! 5천원 그래서 어쩔수없이 ~~~ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100021148.1.9",
          "form": "피자빵이랑 연유바게트,, 에그타르트 ,,",
          "original_form": "피자빵이랑 연유바게트,, 에그타르트 ,,",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100021148.1.10",
          "form": "하늘은 되게 맑더라고요 ㅠ",
          "original_form": "하늘은 되게 맑더라고요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100021148.1.11",
          "form": "아 요기요! 아시는구나~! 전 그저께사먹었어요~~",
          "original_form": "아 요기요! 아시는구나~! 전 그저께사먹었어요~~",
          "speaker_id": "2",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100021148.1.12",
          "form": "마쟈요 하늘은 어찌나 청명한지!",
          "original_form": "마쟈요 하늘은 어찌나 청명한지!",
          "speaker_id": "2",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100021148.1.13",
          "form": "그래서 한강이나 다녀올까 고민도돼요 ,,",
          "original_form": "그래서 한강이나 다녀올까 고민도돼요 ,,",
          "speaker_id": "1",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100021148.1.14",
          "form": "오~ 서울사시는가봐요!!",
          "original_form": "오~ 서울사시는가봐요!!",
          "speaker_id": "2",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100021148.1.15",
          "form": "네! 서울이에요 !",
          "original_form": "네! 서울이에요 !",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100021148.1.16",
          "form": "서울은 매우 더워요 !",
          "original_form": "서울은 매우 더워요 !",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100021148.1.17",
          "form": "name2님은 어디사시나요?",
          "original_form": "&name2&님은 어디사시나요?",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100021148.1.18",
          "form": "아하~ 전 부산살아여 ㅎㅎㅎ",
          "original_form": "아하~ 전 부산살아여 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021148.1.19",
          "form": "부산!!역시좋은데사시네요!!!",
          "original_form": "부산!!역시좋은데사시네요!!!",
          "speaker_id": "1",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021148.1.20",
          "form": "부산도 역시 덥군요",
          "original_form": "부산도 역시 덥군요",
          "speaker_id": "1",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021148.1.21",
          "form": "앜ㅋㅋㅋㅋ 네ㅜㅜ오늘 여름날씨에여완전",
          "original_form": "앜ㅋㅋㅋㅋ 네ㅜㅜ오늘 여름날씨에여완전",
          "speaker_id": "2",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100021148.1.22",
          "form": "제가 지리를 잘모르는데",
          "original_form": "제가 지리를 잘모르는데",
          "speaker_id": "1",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100021148.1.23",
          "form": "부산이면 바닷가쪽인거죠 그럼 더 덥거나 더 춥거나 그런가요?!",
          "original_form": "부산이면 바닷가쪽인거죠 그럼 더 덥거나 더 춥거나 그런가요?!",
          "speaker_id": "1",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100021148.1.24",
          "form": "음 아무래도 바다근처라 온도변화가 엄청 크진않지만 남쪽에 있으니 아무래도 북쪽보단 더 더운거같긴해요 ㅋㅋ",
          "original_form": "음 아무래도 바다근처라 온도변화가 엄청 크진않지만 남쪽에 있으니 아무래도 북쪽보단 더 더운거같긴해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100021148.1.25",
          "form": "얼른 찐가을이 왔으면 좋겠는데",
          "original_form": "얼른 찐가을이 왔으면 좋겠는데",
          "speaker_id": "1",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100021148.1.26",
          "form": "일.이주정도왓다가 바로 겨울되겠죠ㅠ",
          "original_form": "일.이주정도왓다가 바로 겨울되겠죠ㅠ",
          "speaker_id": "1",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100021148.1.27",
