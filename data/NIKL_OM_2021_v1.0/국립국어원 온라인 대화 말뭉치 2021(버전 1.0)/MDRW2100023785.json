      "utterance": [
        {
          "id": "MDRW2100023785.1.1",
          "form": "혹시 치아 교정 해보셨나요",
          "original_form": "혹시 치아 교정 해보셨나요",
          "speaker_id": "1",
          "time": "20210830 17:40"
        },
        {
          "id": "MDRW2100023785.1.2",
          "form": "아니요.. 하실 계획이세요?",
          "original_form": "아니요.. 하실 계획이세요?",
          "speaker_id": "2",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100023785.1.3",
          "form": "저 덧니가 좀 있어서 치과에서 검진받았는데 생니 4개뽑아야 교정할수잇대요",
          "original_form": "저 덧니가 좀 있어서 치과에서 검진받았는데 생니 4개뽑아야 교정할수잇대요",
          "speaker_id": "1",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100023785.1.4",
          "form": "헉... 어릴때해야 좋지 않나여? \n성인은 넘 불편할듯..",
          "original_form": "헉... 어릴때해야 좋지 않나여? \n성인은 넘 불편할듯..",
          "speaker_id": "2",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100023785.1.5",
          "form": "그쳐",
          "original_form": "그쳐",
          "speaker_id": "1",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100023785.1.6",
          "form": "안하는 쪽으로 가닥잡았어요 ㅋㅋㅋ",
          "original_form": "안하는 쪽으로 가닥잡았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100023785.1.7",
          "form": "저도 덧니인데",
          "original_form": "저도 덧니인데",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023785.1.8",
          "form": "아래라서 말하거나해도 안보이더라구요..",
          "original_form": "아래라서 말하거나해도 안보이더라구요..",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023785.1.9",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "1",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100023785.1.10",
          "form": "저는 위 송곳니 두개가 덧니여서..",
          "original_form": "저는 위 송곳니 두개가 덧니여서..",
          "speaker_id": "1",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100023785.1.11",
          "form": "남은 인생이 더 기니까 할까 햇는데 이뽑긴 무섭네요",
          "original_form": "남은 인생이 더 기니까 할까 햇는데 이뽑긴 무섭네요",
          "speaker_id": "1",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100023785.1.12",
          "form": "위에 송곳니는 눈에 넘 잘띄긴하죠..",
          "original_form": "위에 송곳니는 눈에 넘 잘띄긴하죠..",
          "speaker_id": "2",
          "time": "20210830 17:53"
        },
        {
          "id": "MDRW2100023785.1.13",
          "form": "심한 덧니는 아니어서",
          "original_form": "심한 덧니는 아니어서",
          "speaker_id": "1",
          "time": "20210830 17:53"
        },
        {
          "id": "MDRW2100023785.1.14",
          "form": "근데 교정하면 평생해야한다해야해서..",
          "original_form": "근데 교정하면 평생해야한다해야해서..",
          "speaker_id": "2",
          "time": "20210830 17:53"
        },
        {
          "id": "MDRW2100023785.1.15",
          "form": "이빨 네개만큼의 가치가 있는지 모르겠어요 ㅋㅋㅋ",
          "original_form": "이빨 네개만큼의 가치가 있는지 모르겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:53"
        },
        {
          "id": "MDRW2100023785.1.16",
          "form": "교정 3년 하면 되는 줄 알앗는데 평생이라 하더라구요",
          "original_form": "교정 3년 하면 되는 줄 알앗는데 평생이라 하더라구요",
          "speaker_id": "1",
          "time": "20210830 17:53"
        },
        {
          "id": "MDRW2100023785.1.17",
          "form": "글트라구여.. 교정떼어도 뒤쪽에 뭐 붙여서..",
          "original_form": "글트라구여.. 교정떼어도 뒤쪽에 뭐 붙여서..",
          "speaker_id": "2",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100023785.1.18",
          "form": "헐 고생이네요",
          "original_form": "헐 고생이네요",
          "speaker_id": "1",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100023785.1.19",
          "form": "평생 관리해야하는..",
          "original_form": "평생 관리해야하는..",
          "speaker_id": "2",
