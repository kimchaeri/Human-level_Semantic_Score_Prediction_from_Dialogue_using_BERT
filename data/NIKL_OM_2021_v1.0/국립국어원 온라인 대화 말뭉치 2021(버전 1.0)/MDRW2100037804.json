      "utterance": [
        {
          "id": "MDRW2100037804.1.1",
          "form": "평양냉면은 별로 맛없었어요 비추해요 ㅋㅋ",
          "original_form": "평양냉면은 별로 맛없었어요 비추해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100037804.1.2",
          "form": "아 헐 저 한번도 안먹어봤어요 ㅋㅋ",
          "original_form": "아 헐 저 한번도 안먹어봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100037804.1.3",
          "form": "완전 아무맛도 안난다던데..",
          "original_form": "완전 아무맛도 안난다던데..",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100037804.1.4",
          "form": "가격에 비해 별로인거같아요",
          "original_form": "가격에 비해 별로인거같아요",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100037804.1.5",
          "form": "전 그정돈아니었는데 냉면1그릇에 넘비싸고 그걸 우상화하는 느낌이어서요",
          "original_form": "전 그정돈아니었는데 냉면1그릇에 넘비싸고 그걸 우상화하는 느낌이어서요",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100037804.1.6",
          "form": "아아 그렇군요!!",
          "original_form": "아아 그렇군요!!",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100037804.1.7",
          "form": "한그릇에 얼마에요?",
          "original_form": "한그릇에 얼마에요?",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100037804.1.8",
          "form": "요즘 가격 17000원까지 들었어요",
          "original_form": "요즘 가격 17000원까지 들었어요",
          "speaker_id": "1",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100037804.1.9",
          "form": "와 헐",
          "original_form": "와 헐",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100037804.1.10",
          "form": "가격 실환가요..?",
          "original_form": "가격 실환가요..?",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100037804.1.11",
          "form": "냉면이 매년마다 꼬박꼬박 올라요",
          "original_form": "냉면이 매년마다 꼬박꼬박 올라요",
          "speaker_id": "1",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100037804.1.12",
          "form": "가격이요",
          "original_form": "가격이요",
          "speaker_id": "1",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100037804.1.13",
          "form": "헐 그렇군요 ???",
          "original_form": "헐 그렇군요 ???",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100037804.1.14",
          "form": "저는 보통 밀면먹어서",
          "original_form": "저는 보통 밀면먹어서",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100037804.1.15",
          "form": "냉면 가격을 잘 몰랐는데ㅜㅜ",
          "original_form": "냉면 가격을 잘 몰랐는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100037804.1.16",
          "form": "밀면 좋아하시나봐요",
          "original_form": "밀면 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100037804.1.17",
          "form": "부산살때 가끔 먹었어요",
          "original_form": "부산살때 가끔 먹었어요",
          "speaker_id": "1",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100037804.1.18",
          "form": "엇 네! 저두 부산살아서",
          "original_form": "엇 네! 저두 부산살아서",
          "speaker_id": "2",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100037804.1.19",
          "form": "보통 밀면먹어요 ㅎㅎ",
          "original_form": "보통 밀면먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100037804.1.20",
          "form": "아 부산사시는군요 저는 창원이에요",
          "original_form": "아 부산사시는군요 저는 창원이에요",
          "speaker_id": "1",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100037804.1.21",
          "form": "오 완전 근처네요 ㅎㅎㅎ !",
          "original_form": "오 완전 근처네요 ㅎㅎㅎ !",
          "speaker_id": "2",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100037804.1.22",
          "form": "창원엔 뭐가 맛있어요 ?",
          "original_form": "창원엔 뭐가 맛있어요 ?",
          "speaker_id": "2",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100037804.1.23",
          "form": "창원에 맛집 별로 없어요 ㅋㅋ",
          "original_form": "창원에 맛집 별로 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100037804.1.24",
          "form": "일부러 와서 먹을 집이 있는지 잘...",
          "original_form": "일부러 와서 먹을 집이 있는지 잘...",
          "speaker_id": "1",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100037804.1.25",
          "form": "아 ㅎㅎ 그렇군요 !!!",
          "original_form": "아 ㅎㅎ 그렇군요 !!!",
          "speaker_id": "2",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100037804.1.26",
          "form": "부산엔 그래도 좀 있지않나요ㅎㅎ",
          "original_form": "부산엔 그래도 좀 있지않나요ㅎㅎ",
