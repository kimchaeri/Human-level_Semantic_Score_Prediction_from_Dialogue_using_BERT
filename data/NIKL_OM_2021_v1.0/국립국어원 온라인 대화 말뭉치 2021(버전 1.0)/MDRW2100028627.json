      "utterance": [
        {
          "id": "MDRW2100028627.1.1",
          "form": "게임해요?",
          "original_form": "게임해요?",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028627.1.2",
          "form": "저 닌텐도 스위치에서 젤다의 전설 좋아해요.",
          "original_form": "저 닌텐도 스위치에서 젤다의 전설 좋아해요.",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028627.1.3",
          "form": "콘솔겜.",
          "original_form": "콘솔겜.",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028627.1.4",
          "form": "전 폰겜이요",
          "original_form": "전 폰겜이요",
          "speaker_id": "1",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100028627.1.5",
          "form": "무슨 폰겜하세요?",
          "original_form": "무슨 폰겜하세요?",
          "speaker_id": "2",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100028627.1.6",
          "form": "저는 폰겜하면 쿠키런 오븐브레이크 밖에 안해요 제일 쉬워서",
          "original_form": "저는 폰겜하면 쿠키런 오븐브레이크 밖에 안해요 제일 쉬워서",
          "speaker_id": "2",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100028627.1.7",
          "form": "전 브롤해요",
          "original_form": "전 브롤해요",
          "speaker_id": "1",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100028627.1.8",
          "form": "방금 브롤 검색해봤어요.",
          "original_form": "방금 브롤 검색해봤어요.",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028627.1.9",
          "form": "이거 할만한가요?",
          "original_form": "이거 할만한가요?",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028627.1.10",
          "form": "초딩겜이여",
          "original_form": "초딩겜이여",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028627.1.11",
          "form": "그렇구나 난이도가 그럼 엄청 쉽겠네요",
          "original_form": "그렇구나 난이도가 그럼 엄청 쉽겠네요",
          "speaker_id": "2",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100028627.1.12",
          "form": "단순해여",
          "original_form": "단순해여",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028627.1.13",
          "form": "나중에 저도 기회가 되면 해볼게요",
          "original_form": "나중에 저도 기회가 되면 해볼게요",
          "speaker_id": "2",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028627.1.14",
          "form": "추천하는 다른 게임도 있나요?",
          "original_form": "추천하는 다른 게임도 있나요?",
          "speaker_id": "2",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028627.1.15",
          "form": "일단 이것만 해요 ㅋㅋ",
          "original_form": "일단 이것만 해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028627.1.16",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028627.1.17",
          "form": "그럼 쿠키런 하실 생각 없으세요? ㅋㅋ",
          "original_form": "그럼 쿠키런 하실 생각 없으세요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028627.1.18",
