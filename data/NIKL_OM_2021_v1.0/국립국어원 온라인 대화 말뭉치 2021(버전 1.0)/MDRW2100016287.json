        },
        {
          "id": "MDRW2100016287.1.2",
          "form": "안녕하세요 게임 좋아하세요?",
          "original_form": "안녕하세요 게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100016287.1.3",
          "form": "예전에 데바데했었어요?",
          "original_form": "예전에 데바데했었어요?",
          "speaker_id": "1",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100016287.1.4",
          "form": "전 게임 잘 안해서요",
          "original_form": "전 게임 잘 안해서요",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016287.1.5",
          "form": "집에 게임 돌아갈만한 컴퓨터도 없고요",
          "original_form": "집에 게임 돌아갈만한 컴퓨터도 없고요",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016287.1.6",
          "form": "한번도요?",
          "original_form": "한번도요?",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016287.1.7",
          "form": "데바데요?",
          "original_form": "데바데요?",
          "speaker_id": "2",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016287.1.8",
          "form": "네네 데바데는 한번도 안해봤어요",
          "original_form": "네네 데바데는 한번도 안해봤어요",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100016287.1.9",
          "form": "메이플은 해보셨나요?",
          "original_form": "메이플은 해보셨나요?",
          "speaker_id": "1",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100016287.1.10",
          "form": "게임을 안해서 해본 적은 없고",
          "original_form": "게임을 안해서 해본 적은 없고",
          "speaker_id": "2",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100016287.1.11",
          "form": "롤 보는건 좋아해요",
          "original_form": "롤 보는건 좋아해요",
          "speaker_id": "2",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100016287.1.12",
          "form": "응원하는 팀있으신가요",
          "original_form": "응원하는 팀있으신가요",
          "speaker_id": "1",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100016287.1.13",
          "form": "아 팀은 없지만 선수는 있어요",
          "original_form": "아 팀은 없지만 선수는 있어요",
          "speaker_id": "2",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100016287.1.14",
          "form": "타잔 팬이에요",
          "original_form": "타잔 팬이에요",
          "speaker_id": "2",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100016287.1.15",
          "form": "포지션이 뭔가요?",
          "original_form": "포지션이 뭔가요?",
          "speaker_id": "1",
          "time": "20210826 12:53"
        },
        {
          "id": "MDRW2100016287.1.16",
          "form": "타잔 선수요?",
          "original_form": "타잔 선수요?",
          "speaker_id": "2",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100016287.1.17",
          "form": "정글이에요",
