      "utterance": [
        {
          "id": "MDRW2100019048.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019048.1.2",
          "form": "안녕하세요ㅋ 배달음식 좋아하세요?",
          "original_form": "안녕하세요ㅋ 배달음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100019048.1.3",
          "form": "배달음식 좋아해요!",
          "original_form": "배달음식 좋아해요!",
          "speaker_id": "1",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100019048.1.4",
          "form": "배달음식 뭐 좋아하세요?",
          "original_form": "배달음식 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100019048.1.5",
          "form": "저는 피자 좋아해요 ㅎㅎ",
          "original_form": "저는 피자 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100019048.1.6",
          "form": "음 저도요ㅋ치킨피자는 진리ㅋ",
          "original_form": "음 저도요ㅋ치킨피자는 진리ㅋ",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019048.1.7",
          "form": "맞아요 ㅋㅋ 근데 요샌 브리또에 빠져서~",
          "original_form": "맞아요 ㅋㅋ 근데 요샌 브리또에 빠져서~",
          "speaker_id": "1",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019048.1.8",
          "form": "브리또는 피자친구라고생각해요",
          "original_form": "브리또는 피자친구라고생각해요",
          "speaker_id": "1",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019048.1.9",
          "form": "오호 저는 청년피자 시켜먹어요ㅋ",
          "original_form": "오호 저는 청년피자 시켜먹어요ㅋ",
          "speaker_id": "2",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019048.1.10",
          "form": "브리또는 많이 안 먹어봤어요ㅜㅜ",
          "original_form": "브리또는 많이 안 먹어봤어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019048.1.11",
          "form": "아하~ 청년피자 어떤게맛나나요?",
          "original_form": "아하~ 청년피자 어떤게맛나나요?",
          "speaker_id": "1",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019048.1.12",
          "form": "글쎄요...뭐 그냥 무난한맛인듯한데.. 최근 나온거 시켜요ㅋ",
          "original_form": "글쎄요...뭐 그냥 무난한맛인듯한데.. 최근 나온거 시켜요ㅋ",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019048.1.13",
          "form": "아 그러시구나! 저는 파파존스좋아해요",
          "original_form": "아 그러시구나! 저는 파파존스좋아해요",
          "speaker_id": "1",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019048.1.14",
          "form": "오호 저도 파파존스도 좋아해요ㅋ",
          "original_form": "오호 저도 파파존스도 좋아해요ㅋ",
          "speaker_id": "2",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019048.1.15",
          "form": "미국맛에 가장 가까운피자라고 생각해요 ㅋㅋ",
          "original_form": "미국맛에 가장 가까운피자라고 생각해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:32"
        }
      ]
