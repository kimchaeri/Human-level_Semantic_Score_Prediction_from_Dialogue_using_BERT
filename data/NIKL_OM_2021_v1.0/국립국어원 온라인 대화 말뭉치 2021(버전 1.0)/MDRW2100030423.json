      "utterance": [
        {
          "id": "MDRW2100030423.1.1",
          "form": "뭔소리여",
          "original_form": "뭔소리여",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030423.1.2",
          "form": "산에 왜가",
          "original_form": "산에 왜가",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030423.1.3",
          "form": "코로나로 난리인데!!",
          "original_form": "코로나로 난리인데!!",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030423.1.4",
          "form": "마스크끼고 다니잖아",
          "original_form": "마스크끼고 다니잖아",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030423.1.5",
          "form": "아빠는 2차까지 다 맞았다고 자랑하는거야?!",
          "original_form": "아빠는 2차까지 다 맞았다고 자랑하는거야?!",
          "speaker_id": "2",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030423.1.6",
          "form": "그런것도 있지 자기는 다 맞았으니까 괜찮다이거지",
          "original_form": "그런것도 있지 자기는 다 맞았으니까 괜찮다이거지",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030423.1.7",
          "form": "아냐 난 산에 가고싶지않아",
          "original_form": "아냐 난 산에 가고싶지않아",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030423.1.8",
          "form": "아빠는 새벽4시부터 준비한다고",
          "original_form": "아빠는 새벽4시부터 준비한다고",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030423.1.9",
          "form": "그럼 12시에 가자고할게",
          "original_form": "그럼 12시에 가자고할게",
          "speaker_id": "1",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100030423.1.10",
          "form": "아니야 싫어 너무 힘들어",
          "original_form": "아니야 싫어 너무 힘들어",
          "speaker_id": "2",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100030423.1.11",
          "form": "산에 갔다가 뭐 사올까",
          "original_form": "산에 갔다가 뭐 사올까",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100030423.1.12",
          "form": "맘스터치 햄버거가 먹고싶어",
          "original_form": "맘스터치 햄버거가 먹고싶어",
          "speaker_id": "2",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100030423.1.13",
          "form": "딥치즈버거로",
          "original_form": "딥치즈버거로",
          "speaker_id": "2",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100030423.1.14",
          "form": "세트로 사와??",
          "original_form": "세트로 사와??",
          "speaker_id": "1",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100030423.1.15",
          "form": "아니 감자튀김 안먹으니카 단품으로!",
          "original_form": "아니 감자튀김 안먹으니카 단품으로!",
          "speaker_id": "2",
          "time": "20210904 01:11"
        },
        {
          "id": "MDRW2100030423.1.16",
          "form": "알겠어",
          "original_form": "알겠어",
          "speaker_id": "1",
          "time": "20210904 01:11"
        },
        {
          "id": "MDRW2100030423.1.17",
          "form": "콜라는??",
          "original_form": "콜라는??",
          "speaker_id": "1",
          "time": "20210904 01:11"
        },
        {
          "id": "MDRW2100030423.1.18",
          "form": "콜라는 집에 제로콜라있으니까 괜찮아",
          "original_form": "콜라는 집에 제로콜라있으니까 괜찮아",
