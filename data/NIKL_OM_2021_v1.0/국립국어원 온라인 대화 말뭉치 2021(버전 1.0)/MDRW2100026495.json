      "utterance": [
        {
          "id": "MDRW2100026495.1.1",
          "form": "오늘 저녁 뭐드셨어요??",
          "original_form": "오늘 저녁 뭐드셨어요??",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026495.1.2",
          "form": "저는국수먹었습니다!",
          "original_form": "저는국수먹었습니다!",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026495.1.3",
          "form": "name2은요?!",
          "original_form": "&name2&은요?!",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026495.1.4",
          "form": "전 가자미조림 해먹었어요!",
          "original_form": "전 가자미조림 해먹었어요!",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026495.1.5",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026495.1.6",
          "form": "요리잘하시는군요!",
          "original_form": "요리잘하시는군요!",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026495.1.7",
          "form": "하다보니까 느네여 ㅋㅋㅋ",
          "original_form": "하다보니까 느네여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026495.1.8",
          "form": "국수 따뜻한거 아니면 차가운거 뭐로 드셨어요??",
          "original_form": "국수 따뜻한거 아니면 차가운거 뭐로 드셨어요??",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026495.1.9",
          "form": "뜨끈한 잔치국수먹었어요!",
          "original_form": "뜨끈한 잔치국수먹었어요!",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026495.1.10",
          "form": "맛있는거 드셨네요!!",
          "original_form": "맛있는거 드셨네요!!",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026495.1.11",
          "form": "잔치국수 땡기는 날씨였는데 메뉴 잘고르셨네요",
          "original_form": "잔치국수 땡기는 날씨였는데 메뉴 잘고르셨네요",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026495.1.12",
          "form": "비가좀와서",
          "original_form": "비가좀와서",
          "speaker_id": "1",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026495.1.13",
          "form": "잘어울리죠?!",
          "original_form": "잘어울리죠?!",
          "speaker_id": "1",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026495.1.14",
          "form": "전도먹고싶네요,,.",
          "original_form": "전도먹고싶네요,,.",
          "speaker_id": "1",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026495.1.15",
          "form": "저 오늘 호박전도 해먹었어요",
          "original_form": "저 오늘 호박전도 해먹었어요",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026495.1.16",
          "form": "ㅋㅋ 맛있어요",
          "original_form": "ㅋㅋ 맛있어요",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026495.1.17",
          "form": "호박전맛있죠ㅠ",
          "original_form": "호박전맛있죠ㅠ",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026495.1.18",
          "form": "김치전 먹고 싶었는데 호박이 위태로워서 해먹었어요 ㅋㅋ",
          "original_form": "김치전 먹고 싶었는데 호박이 위태로워서 해먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026495.1.19",
          "form": "냉장고털이하셨군요",
          "original_form": "냉장고털이하셨군요",
          "speaker_id": "1",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100026495.1.20",
          "form": "네 ㅋㅋㅋ 가자미도 냉동되어있던 가자미였어요ㅋㅋ",
          "original_form": "네 ㅋㅋㅋ 가자미도 냉동되어있던 가자미였어요ㅋㅋ",
