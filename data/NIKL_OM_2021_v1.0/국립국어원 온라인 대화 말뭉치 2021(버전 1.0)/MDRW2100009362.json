      "utterance": [
        {
          "id": "MDRW2100009362.1.1",
          "form": "네~~~~지금 샀어요^^",
          "original_form": "네~~~~지금 샀어요^^",
          "speaker_id": "2",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009362.1.2",
          "form": "뭐사셨어요?",
          "original_form": "뭐사셨어요?",
          "speaker_id": "1",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009362.1.3",
          "form": "토스적립이라 샀는데 괜히 산듯도하고~",
          "original_form": "토스적립이라 샀는데 괜히 산듯도하고~",
          "speaker_id": "2",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009362.1.4",
          "form": "쭈꾸미랑 닭발이요",
          "original_form": "쭈꾸미랑 닭발이요",
          "speaker_id": "2",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009362.1.5",
          "form": "얼마결제하셨는데요?",
          "original_form": "얼마결제하셨는데요?",
          "speaker_id": "1",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009362.1.6",
          "form": "18천이요.",
          "original_form": "18천이요.",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009362.1.7",
          "form": "전 안주야 7팩사고 19700원정도샀는데",
          "original_form": "전 안주야 7팩사고 19700원정도샀는데",
          "speaker_id": "1",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009362.1.8",
          "form": "토스3천 적립이라 15천~~ 가격은 적당히 잘산거같아요",
          "original_form": "토스3천 적립이라 15천~~ 가격은 적당히 잘산거같아요",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009362.1.9",
          "form": "토스할인 3천빼면 16000원대",
          "original_form": "토스할인 3천빼면 16000원대",
          "speaker_id": "1",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009362.1.10",
          "form": "쭈꾸미가 총 몇그람인가요?",
          "original_form": "쭈꾸미가 총 몇그람인가요?",
          "speaker_id": "1",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009362.1.11",
          "form": "ㅎ 전 안주야 첨사봤어요. 닭발좋아하는데 혼자먹기 괜찮을꺼같아요",
          "original_form": "ㅎ 전 안주야 첨사봤어요. 닭발좋아하는데 혼자먹기 괜찮을꺼같아요",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009362.1.12",
          "form": "쭈꾸미 300넘는거같아요",
          "original_form": "쭈꾸미 300넘는거같아요",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009362.1.13",
          "form": "350그람이요",
          "original_form": "350그람이요",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009362.1.14",
          "form": "안주야 맛 괜찮아요 양이 넘적어서 ㅋ",
          "original_form": "안주야 맛 괜찮아요 양이 넘적어서 ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009362.1.15",
          "form": "세팩 다드셔야될지도",
          "original_form": "세팩 다드셔야될지도",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009362.1.16",
          "form": "ㅎㅎ 신랑은안먹고 큰애랑 저만 좋아해요",
          "original_form": "ㅎㅎ 신랑은안먹고 큰애랑 저만 좋아해요",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009362.1.17",
          "form": "봐서 다 먹죠머^^",
          "original_form": "봐서 다 먹죠머^^",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009362.1.18",
          "form": "아ㅎ 전 닭발만 못먹어요   그래서 곱창이랑 닭똥집하나 넣고샀어요",
          "original_form": "아ㅎ 전 닭발만 못먹어요   그래서 곱창이랑 닭똥집하나 넣고샀어요",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009362.1.19",
          "form": "같아볶아도 맛있겠네요",
          "original_form": "같아볶아도 맛있겠네요",
          "speaker_id": "2",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009362.1.20",
          "form": "닭똥집은 마늘넣고 하얗게 볶은건가봐요",
          "original_form": "닭똥집은 마늘넣고 하얗게 볶은건가봐요",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009362.1.21",
          "form": "소금만넣고 튀기듯 볶는것도 맛있는데...한번 사다 해먹어야겠어요",
          "original_form": "소금만넣고 튀기듯 볶는것도 맛있는데...한번 사다 해먹어야겠어요",
          "speaker_id": "2",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009362.1.22",
          "form": "사다먹음 싸죠 닭똥집ㅎ",
          "original_form": "사다먹음 싸죠 닭똥집ㅎ",
          "speaker_id": "1",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009362.1.23",
          "form": "저녁간단하게 먹어서 배고파지네요^^",
          "original_form": "저녁간단하게 먹어서 배고파지네요^^",
          "speaker_id": "2",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009362.1.24",
          "form": "전 제육이 마지막 식사라",
          "original_form": "전 제육이 마지막 식사라",
          "speaker_id": "1",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009362.1.25",
          "form": "분리수거하고와서 닭똥집이나 먹어볼까봐요ㄹ",
          "original_form": "분리수거하고와서 닭똥집이나 먹어볼까봐요ㄹ",
          "speaker_id": "1",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009362.1.26",
          "form": "더 늦기전에 다녀오세요",
          "original_form": "더 늦기전에 다녀오세요",
