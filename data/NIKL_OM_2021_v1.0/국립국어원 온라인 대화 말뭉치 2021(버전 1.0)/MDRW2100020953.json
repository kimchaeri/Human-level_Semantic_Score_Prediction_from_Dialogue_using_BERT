      "utterance": [
        {
          "id": "MDRW2100020953.1.1",
          "form": "요기요는 올려놓는 빵이",
          "original_form": "요기요는 올려놓는 빵이",
          "speaker_id": "2",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020953.1.2",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020953.1.3",
          "form": "한정적인것 같아요",
          "original_form": "한정적인것 같아요",
          "speaker_id": "2",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020953.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020953.1.5",
          "form": "그리고 매장가보다 비싸더라구요",
          "original_form": "그리고 매장가보다 비싸더라구요",
          "speaker_id": "1",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020953.1.6",
          "form": "원하는 종류가",
          "original_form": "원하는 종류가",
          "speaker_id": "2",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020953.1.7",
          "form": "다없어요ㅜ",
          "original_form": "다없어요ㅜ",
          "speaker_id": "2",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020953.1.8",
          "form": "공감해요",
          "original_form": "공감해요",
          "speaker_id": "1",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020953.1.9",
          "form": "맘같으면 가서 바꾸고 싶어요 다른거로 ㅋㅋㅋ",
          "original_form": "맘같으면 가서 바꾸고 싶어요 다른거로 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020953.1.10",
          "form": "할인들어갈거 생각해서",
          "original_form": "할인들어갈거 생각해서",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020953.1.11",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020953.1.12",
          "form": "비싸게 올리나봐요",
          "original_form": "비싸게 올리나봐요",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020953.1.13",
          "form": "맞아요 그리고",
          "original_form": "맞아요 그리고",
          "speaker_id": "1",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020953.1.14",
          "form": "아무래도 수수료땜에 다른 가게들도 매장보다 비싸더라구요",
          "original_form": "아무래도 수수료땜에 다른 가게들도 매장보다 비싸더라구요",
          "speaker_id": "1",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020953.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020953.1.16",
          "form": "저진짜 너무 놀란게",
          "original_form": "저진짜 너무 놀란게",
          "speaker_id": "1",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020953.1.17",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020953.1.18",
          "form": "저희동네 가마로강정은",
          "original_form": "저희동네 가마로강정은",
          "speaker_id": "1",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020953.1.19",
          "form": "중자가 매장에서 8천인데",
          "original_form": "중자가 매장에서 8천인데",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020953.1.20",
          "form": "배민에서는 만이천이더라고요..!!!!",
          "original_form": "배민에서는 만이천이더라고요..!!!!",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020953.1.21",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020953.1.22",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020953.1.23",
          "form": "기절했어요...",
          "original_form": "기절했어요...",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020953.1.24",
          "form": "역시 포장이 낫네요",
          "original_form": "역시 포장이 낫네요",
          "speaker_id": "2",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020953.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020953.1.26",
          "form": "근데또 멀어서 귀찮아요 ㅋㅋㅋㅋ",
          "original_form": "근데또 멀어서 귀찮아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020953.1.27",
          "form": "배달비도 비싸고ㅜ",
          "original_form": "배달비도 비싸고ㅜ",
