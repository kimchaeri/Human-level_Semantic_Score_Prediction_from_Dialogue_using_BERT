      "utterance": [
        {
          "id": "MDRW2100013915.1.1",
          "form": "오 비슷하네요 배떡 진짜그냥그렇더라고요",
          "original_form": "오 비슷하네요 배떡 진짜그냥그렇더라고요",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013915.1.2",
          "form": "대신떡군이네",
          "original_form": "대신떡군이네",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013915.1.3",
          "form": "시킬때",
          "original_form": "시킬때",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013915.1.4",
          "form": "꼭 스파게티면을 추가하셔야돼요!",
          "original_form": "꼭 스파게티면을 추가하셔야돼요!",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013915.1.5",
          "form": "!? 스파게티면요? 오 맛있겠다",
          "original_form": "!? 스파게티면요? 오 맛있겠다",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013915.1.6",
          "form": "가격은 어때요?",
          "original_form": "가격은 어때요?",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013915.1.7",
          "form": "가격은 음 기억이안나네요ㅋㅋㅋㅋㅋ",
          "original_form": "가격은 음 기억이안나네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013915.1.8",
          "form": "튀김은 대게다리나 집게살+핫도그로요",
          "original_form": "튀김은 대게다리나 집게살+핫도그로요",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013915.1.9",
          "form": "꼭 이 요",
          "original_form": "꼭 이 요",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013915.1.10",
          "form": "오우야 다 맛있는거만 있네요",
          "original_form": "오우야 다 맛있는거만 있네요",
          "speaker_id": "2",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013915.1.11",
          "form": "아 오늘은 낙곱새 먹기로 했는데",
          "original_form": "아 오늘은 낙곱새 먹기로 했는데",
          "speaker_id": "2",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013915.1.12",
          "form": "언제 먹어보지.. 엊그제 먹어가지구ㅋㅋㅋ",
          "original_form": "언제 먹어보지.. 엊그제 먹어가지구ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013915.1.13",
          "form": "헐 낙곱새도",
          "original_form": "헐 낙곱새도",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013915.1.14",
          "form": "맛있잖아요",
          "original_form": "맛있잖아요",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013915.1.15",
          "form": "맛있는거드시네요",
          "original_form": "맛있는거드시네요",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013915.1.16",
          "form": "ㅋㅋ 감성낙곱새 맛있더라구요 저녁 뭐드실건가요?",
          "original_form": "ㅋㅋ 감성낙곱새 맛있더라구요 저녁 뭐드실건가요?",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013915.1.17",
          "form": "ㅋㅋㅋ 저며칠전에 국민낙곱새 먹었는데",
          "original_form": "ㅋㅋㅋ 저며칠전에 국민낙곱새 먹었는데",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100013915.1.18",
          "form": "초밥고민중입니다",
          "original_form": "초밥고민중입니다",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100013915.1.19",
          "form": "거기도 맛있어요? 국민낙곱새 첨들어봐요",
          "original_form": "거기도 맛있어요? 국민낙곱새 첨들어봐요",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100013915.1.20",
          "form": "ㅋㅋㅋ 감성이랑",
          "original_form": "ㅋㅋㅋ 감성이랑",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100013915.1.21",
          "form": "완전 복붙으로 생겼답니다",
          "original_form": "완전 복붙으로 생겼답니다",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100013915.1.22",
          "form": "엥ㅋㅋㅋ 하긴 다 거기서 거기겠죠",
          "original_form": "엥ㅋㅋㅋ 하긴 다 거기서 거기겠죠",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100013915.1.23",
          "form": "낙곱새 자체가ㅋㅋㅋㅋ",
          "original_form": "낙곱새 자체가ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100013915.1.24",
          "form": "리뷰사진보면",
          "original_form": "리뷰사진보면",
          "speaker_id": "1",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100013915.1.25",
          "form": "구분못할듯요ㅋㅋㅋ 어디가어디껀지",
          "original_form": "구분못할듯요ㅋㅋㅋ 어디가어디껀지",
          "speaker_id": "1",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100013915.1.26",
          "form": "그정도에요?ㅋㅋㅋㅋ 담에 비교 갑니다",
          "original_form": "그정도에요?ㅋㅋㅋㅋ 담에 비교 갑니다",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100013915.1.27",
          "form": "국민낙곱새 메모",
          "original_form": "국민낙곱새 메모",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100013915.1.28",
          "form": "ㅋㅋㅋ 전 저녁아직이랍니다",
          "original_form": "ㅋㅋㅋ 전 저녁아직이랍니다",
          "speaker_id": "1",
