      "utterance": [
        {
          "id": "MDRW2100005920.1.1",
          "form": "3주만에",
          "original_form": "3주만에",
          "speaker_id": "2",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100005920.1.2",
          "form": "연애하셨다구요?!",
          "original_form": "연애하셨다구요?!",
          "speaker_id": "2",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100005920.1.3",
          "form": "엄청빨리 하셨네용",
          "original_form": "엄청빨리 하셨네용",
          "speaker_id": "2",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100005920.1.4",
          "form": "오오오옹",
          "original_form": "오오오옹",
          "speaker_id": "2",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100005920.1.5",
          "form": "천샹연분이셨나봐요",
          "original_form": "천샹연분이셨나봐요",
          "speaker_id": "2",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100005920.1.6",
          "form": "음 그런데 개인적인 생각인데",
          "original_form": "음 그런데 개인적인 생각인데",
          "speaker_id": "1",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100005920.1.7",
          "form": "소개팅을 한다는건",
          "original_form": "소개팅을 한다는건",
          "speaker_id": "1",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100005920.1.8",
          "form": "좋은 사람 만나서 연애하고 싶어서 하는거 아닐까요",
          "original_form": "좋은 사람 만나서 연애하고 싶어서 하는거 아닐까요",
          "speaker_id": "1",
          "time": "20210907 16:32"
        },
        {
          "id": "MDRW2100005920.1.9",
          "form": "쌍방이다 연애를 목적으로 만나는 거니까",
          "original_form": "쌍방이다 연애를 목적으로 만나는 거니까",
          "speaker_id": "1",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100005920.1.10",
          "form": "서로 맘에 들면 시간 오래 안걸리고 연애 시작할 수 있다고 생각해요",
          "original_form": "서로 맘에 들면 시간 오래 안걸리고 연애 시작할 수 있다고 생각해요",
          "speaker_id": "1",
          "time": "20210907 16:34"
        },
        {
          "id": "MDRW2100005920.1.11",
          "form": "예에",
          "original_form": "예에",
          "speaker_id": "2",
          "time": "20210907 16:34"
        },
        {
          "id": "MDRW2100005920.1.12",
          "form": "근데 좋은사람만나기가",
          "original_form": "근데 좋은사람만나기가",
          "speaker_id": "2",
          "time": "20210907 16:34"
        },
        {
          "id": "MDRW2100005920.1.13",
          "form": "세상 어렵죠",
          "original_form": "세상 어렵죠",
          "speaker_id": "2",
          "time": "20210907 16:34"
        },
        {
          "id": "MDRW2100005920.1.14",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100005920.1.15",
          "form": "그리고 결혼이란거",
          "original_form": "그리고 결혼이란거",
          "speaker_id": "2",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100005920.1.16",
          "form": "음 그건 그렇죠",
          "original_form": "음 그건 그렇죠",
          "speaker_id": "1",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100005920.1.17",
          "form": "한 사람을 만나서 평생사랑한다는게",
          "original_form": "한 사람을 만나서 평생사랑한다는게",
          "speaker_id": "2",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100005920.1.18",
          "form": "물론 가능은하겠지만",
          "original_form": "물론 가능은하겠지만",
          "speaker_id": "2",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100005920.1.19",
          "form": "너무 신기하지않나요",
          "original_form": "너무 신기하지않나요",
          "speaker_id": "2",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100005920.1.20",
          "form": "결혼처럼 자유롭게",
          "original_form": "결혼처럼 자유롭게",
          "speaker_id": "2",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100005920.1.21",
          "form": "이혼도 할수있다면",
          "original_form": "이혼도 할수있다면",
          "speaker_id": "2",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100005920.1.22",
          "form": "기꺼이하겠지만!!",
          "original_form": "기꺼이하겠지만!!",
          "speaker_id": "2",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100005920.1.23",
          "form": "아 그건 동의합니다",
          "original_form": "아 그건 동의합니다",
          "speaker_id": "1",
          "time": "20210907 16:36"
        },
        {
          "id": "MDRW2100005920.1.24",
          "form": "한사람만 평생사랑하면서 사는거 쉽지 않을것 같아요",
          "original_form": "한사람만 평생사랑하면서 사는거 쉽지 않을것 같아요",
          "speaker_id": "1",
          "time": "20210907 16:36"
        },
        {
          "id": "MDRW2100005920.1.25",
          "form": "나중에는 의리로 함께하는것 같아요",
          "original_form": "나중에는 의리로 함께하는것 같아요",
          "speaker_id": "1",
          "time": "20210907 16:37"
        },
        {
          "id": "MDRW2100005920.1.26",
