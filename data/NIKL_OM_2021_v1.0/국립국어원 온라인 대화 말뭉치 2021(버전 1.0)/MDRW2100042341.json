      "utterance": [
        {
          "id": "MDRW2100042341.1.1",
          "form": "날씨에이어",
          "original_form": "날씨에이어",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042341.1.2",
          "form": "오늘도집안일이많으셧나요ㅜㅜ",
          "original_form": "오늘도집안일이많으셧나요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042341.1.3",
          "form": "살림은 해도 해도 끝이없죠",
          "original_form": "살림은 해도 해도 끝이없죠",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042341.1.4",
          "form": "근데 사실 전 요새 요리정도 하구요",
          "original_form": "근데 사실 전 요새 요리정도 하구요",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042341.1.5",
          "form": "남편이랑 딸이 집안일 더 하는 편이에요 ㅎㅎ",
          "original_form": "남편이랑 딸이 집안일 더 하는 편이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042341.1.6",
          "form": "오ㄱㅋㅋㄱㄲ  분담이잘되어있네요!!",
          "original_form": "오ㄱㅋㅋㄱㄲ  분담이잘되어있네요!!",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042341.1.7",
          "form": "저희 남편은 원래 집안일 잘 도와줬어요",
          "original_form": "저희 남편은 원래 집안일 잘 도와줬어요",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042341.1.8",
          "form": "오히려 딸에게 집안일을 잘 안시켰는데",
          "original_form": "오히려 딸에게 집안일을 잘 안시켰는데",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042341.1.9",
          "form": "요새는 좀 시키고 있답니다",
          "original_form": "요새는 좀 시키고 있답니다",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042341.1.10",
          "form": "ㅜㅜ아 너무보기좋은 가족이네요ㅜㅜ...아름답다ㅜㅜ...",
          "original_form": "ㅜㅜ아 너무보기좋은 가족이네요ㅜㅜ...아름답다ㅜㅜ...",
          "speaker_id": "1",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042341.1.11",
          "form": "이상적인가족",
          "original_form": "이상적인가족",
          "speaker_id": "1",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042341.1.12",
          "form": "남편이 집안일을 잘 해서 제가 딸 시키려고 하면 본인이 하겠다고 그랬었는데",
          "original_form": "남편이 집안일을 잘 해서 제가 딸 시키려고 하면 본인이 하겠다고 그랬었는데",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042341.1.13",
          "form": "요새는 딸좀 시키라고 하고 있어요",
          "original_form": "요새는 딸좀 시키라고 하고 있어요",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042341.1.14",
          "form": "딸도 배워야한다고 ㅎㅎ",
          "original_form": "딸도 배워야한다고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042341.1.15",
          "form": "맞아요 교육도 훌륭하시네요ㅜㅜ",
          "original_form": "맞아요 교육도 훌륭하시네요ㅜㅜ{emoji:♡}",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042341.1.16",
          "form": "조금씩배워야좋을거같아요ㅜㅜ",
          "original_form": "조금씩배워야좋을거같아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042341.1.17",
          "form": "그쵸 일단 이것도 기술이니까 배워놓으면 좋죠",
          "original_form": "그쵸 일단 이것도 기술이니까 배워놓으면 좋죠",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042341.1.18",
          "form": "그쵸ㅎㅎㅎ 너무좋은듯해요ㅎㅁㅎ)/",
          "original_form": "그쵸ㅎㅎㅎ 너무좋은듯해요ㅎㅁㅎ)/",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042341.1.19",
          "form": "딸아이가 그래도 알려주면 곧잘 해서 다행이에요",
          "original_form": "딸아이가 그래도 알려주면 곧잘 해서 다행이에요",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042341.1.20",
          "form": "저는 결혼생각은없지만 이런얘기들으면",
          "original_form": "저는 결혼생각은없지만 이런얘기들으면",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042341.1.21",
          "form": "뭔가부럽더라구요ㅎㅁㅎ",
          "original_form": "뭔가부럽더라구요ㅎㅁㅎ",
