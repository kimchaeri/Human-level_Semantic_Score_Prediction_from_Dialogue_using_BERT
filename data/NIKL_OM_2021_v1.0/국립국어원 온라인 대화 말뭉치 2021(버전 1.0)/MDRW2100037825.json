      "utterance": [
        {
          "id": "MDRW2100037825.1.1",
          "form": "학교다니시는 학생은아니시지여!",
          "original_form": "학교다니시는 학생은아니시지여!",
          "speaker_id": "2",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100037825.1.2",
          "form": "네 직장인이에요",
          "original_form": "네 직장인이에요",
          "speaker_id": "1",
          "time": "20210904 08:52"
        },
        {
          "id": "MDRW2100037825.1.3",
          "form": "학교다닐때 추억있으셔용?",
          "original_form": "학교다닐때 추억있으셔용?",
          "speaker_id": "2",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100037825.1.4",
          "form": "흠.. 졸업한지 너무 오래돼서 ㅜ 가물가물하네요",
          "original_form": "흠.. 졸업한지 너무 오래돼서 ㅜ 가물가물하네요",
          "speaker_id": "1",
          "time": "20210904 19:07"
        },
        {
          "id": "MDRW2100037825.1.5",
          "form": "ㅋㄱㅈㄱㅈㅋ",
          "original_form": "ㅋㄱㅈㄱㅈㅋ",
          "speaker_id": "2",
          "time": "20210904 20:03"
        },
        {
          "id": "MDRW2100037825.1.6",
          "form": "저는 조별활동할때 괴로웠던기억은있습니다",
          "original_form": "저는 조별활동할때 괴로웠던기억은있습니다",
          "speaker_id": "2",
          "time": "20210904 20:03"
        },
        {
          "id": "MDRW2100037825.1.7",
          "form": "ㅋㅋㅋㅋ사실 저는 조별활동도 별로 안 해봤...",
          "original_form": "ㅋㅋㅋㅋ사실 저는 조별활동도 별로 안 해봤...",
          "speaker_id": "1",
          "time": "20210905 12:50"
        },
        {
          "id": "MDRW2100037825.1.8",
          "form": "대학.4년 다니면 한 2번? 했던 것 같아요",
          "original_form": "대학.4년 다니면 한 2번? 했던 것 같아요",
          "speaker_id": "1",
          "time": "20210905 12:50"
        },
        {
          "id": "MDRW2100037825.1.9",
          "form": "아진짜요?  조별활동 많이없는곳 가셨나봐요!",
          "original_form": "아진짜요?  조별활동 많이없는곳 가셨나봐요!",
          "speaker_id": "2",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100037825.1.10",
          "form": "네 ㅋㅋㅋ 과가 그래서 그런가...",
          "original_form": "네 ㅋㅋㅋ 과가 그래서 그런가...",
          "speaker_id": "1",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100037825.1.11",
          "form": "그래서 조별활동의 괴로움?에 대해 잘 몰라요 ㅋㅋㅋ",
          "original_form": "그래서 조별활동의 괴로움?에 대해 잘 몰라요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100037825.1.12",
          "form": "아 부럽습니다ㅋㅋㅋㅋ\r\n진짜거의다조별이라 머리터질뻔했습니다ㅋㅋㅋㅋ",
          "original_form": "아 부럽습니다ㅋㅋㅋㅋ\r\n진짜거의다조별이라 머리터질뻔했습니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:53"
        },
        {
          "id": "MDRW2100037825.1.13",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "1",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100037825.1.14",
          "form": "과 여쭤봐도 되나용",
          "original_form": "과 여쭤봐도 되나용",
          "speaker_id": "1",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100037825.1.15",
          "form": "해양경영이었습니다ㅋㅋㅋ\r\n경영이랑거의비슷해서...어우",
          "original_form": "해양경영이었습니다ㅋㅋㅋ\r\n경영이랑거의비슷해서...어우",
          "speaker_id": "2",
          "time": "20210905 18:00"
        },
        {
          "id": "MDRW2100037825.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100037825.1.17",
          "form": "경영이 진짜 복전 많죠 ㅋㅋㅋ",
          "original_form": "경영이 진짜 복전 많죠 ㅋㅋㅋ",
