      "utterance": [
        {
          "id": "MDRW2100036235.1.1",
          "form": "저는 웬만하면 다맛있는 사람인데 이건추천각일 정도긴하지만 ㅎㅎ",
          "original_form": "저는 웬만하면 다맛있는 사람인데 이건추천각일 정도긴하지만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100036235.1.2",
          "form": "맛있었음 좋겠네요",
          "original_form": "맛있었음 좋겠네요",
          "speaker_id": "2",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100036235.1.3",
          "form": "치킨은 어디꺼 좋아하시나요?",
          "original_form": "치킨은 어디꺼 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100036235.1.4",
          "form": "저도 가리는거 잘 없어용ㅎ호",
          "original_form": "저도 가리는거 잘 없어용ㅎ호",
          "speaker_id": "1",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100036235.1.5",
          "form": "치킨은 배달어플에서 할인하는거",
          "original_form": "치킨은 배달어플에서 할인하는거",
          "speaker_id": "1",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100036235.1.6",
          "form": "그때그때 시켜먹어서",
          "original_form": "그때그때 시켜먹어서",
          "speaker_id": "1",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100036235.1.7",
          "form": "진짜 다 먹어요!",
          "original_form": "진짜 다 먹어요!",
          "speaker_id": "1",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100036235.1.8",
          "form": "그래도 하나 꼽자면 치킨매니아요!",
          "original_form": "그래도 하나 꼽자면 치킨매니아요!",
          "speaker_id": "1",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100036235.1.9",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100036235.1.10",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "2",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100036235.1.11",
          "form": "땡촌가 그거맛있는데",
          "original_form": "땡촌가 그거맛있는데",
          "speaker_id": "2",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100036235.1.12",
          "form": "엇 새로나온건가봐용!",
          "original_form": "엇 새로나온건가봐용!",
          "speaker_id": "1",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100036235.1.13",
          "form": "저는 여기서 새우치킨만 먹거든용ㅎㅎ",
          "original_form": "저는 여기서 새우치킨만 먹거든용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100036235.1.14",
          "form": "고 추 있는거요",
          "original_form": "고 추 있는거요",
          "speaker_id": "2",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100036235.1.15",
          "form": "원래있던거였어요 ㅋ",
          "original_form": "원래있던거였어요 ㅋ",
          "speaker_id": "2",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100036235.1.16",
          "form": "맵긴해요 근데 바삭도 해서좋아요",
          "original_form": "맵긴해요 근데 바삭도 해서좋아요",
          "speaker_id": "2",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100036235.1.17",
          "form": "그렇구나 제가 매운걸 안좋아해서",
          "original_form": "그렇구나 제가 매운걸 안좋아해서",
          "speaker_id": "1",
          "time": "20210830 19:43"
        },
        {
          "id": "MDRW2100036235.1.18",
          "form": "그런쪽으로는 관심을 안두다보니까ㅋㅋㅋ",
          "original_form": "그런쪽으로는 관심을 안두다보니까ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100036235.1.19",
          "form": "아 그럼 많이 매울수있어요",
          "original_form": "아 그럼 많이 매울수있어요",
          "speaker_id": "2",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100036235.1.20",
          "form": "매운거 잘드세요?",
          "original_form": "매운거 잘드세요?",
          "speaker_id": "1",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100036235.1.21",
          "form": "엽떡같은 매운거 빼고 좋아해요",
          "original_form": "엽떡같은 매운거 빼고 좋아해요",
          "speaker_id": "2",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100036235.1.22",
          "form": "캡사이신은 싫더라고요",
          "original_form": "캡사이신은 싫더라고요",
          "speaker_id": "2",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100036235.1.23",
          "form": "오호 닭발도 좋아하시나요?",
          "original_form": "오호 닭발도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100036235.1.24",
          "form": "아뇽 ㅎ향신료있는것도 안좋아해요",
          "original_form": "아뇽 ㅎ향신료있는것도 안좋아해요",
          "speaker_id": "2",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100036235.1.25",
          "form": "오홍 그러시구나 약간 인위적으로 맵다기보다는 얼큰한걸 좋아하시는것같아용!",
          "original_form": "오홍 그러시구나 약간 인위적으로 맵다기보다는 얼큰한걸 좋아하시는것같아용!",
          "speaker_id": "1",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100036235.1.26",
          "form": "맞아요 그렇네요 ㅎ",
          "original_form": "맞아요 그렇네요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 11:24"
        }
      ]
    }
  ]
