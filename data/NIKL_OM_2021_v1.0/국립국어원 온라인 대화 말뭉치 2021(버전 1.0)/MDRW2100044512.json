      "utterance": [
        {
          "id": "MDRW2100044512.1.1",
          "form": "케익 조아하세요,",
          "original_form": "케익 조아하세요,",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044512.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044512.1.3",
          "form": "네 좋아해요 님은 어때요?",
          "original_form": "네 좋아해요 님은 어때요?",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044512.1.4",
          "form": "조아해요 어떤거조아하ㅅㅔ용",
          "original_form": "조아해요 어떤거조아하ㅅㅔ용",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044512.1.5",
          "form": "전 과일 많은 케이크나 아이스크림 케이크요 님은요?",
          "original_form": "전 과일 많은 케이크나 아이스크림 케이크요 님은요?",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044512.1.6",
          "form": "저도 생크림케이크요",
          "original_form": "저도 생크림케이크요",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044512.1.7",
          "form": "주로 어디서드세요",
          "original_form": "주로 어디서드세요",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044512.1.8",
          "form": "뚜레쥬르요 아이스크림 케이크는 베라죠",
          "original_form": "뚜레쥬르요 아이스크림 케이크는 베라죠",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044512.1.9",
          "form": "님은 어디껄 드세요?",
          "original_form": "님은 어디껄 드세요?",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044512.1.10",
          "form": "ㅋㅋㅋㅋ 크 배라 그쵸",
          "original_form": "ㅋㅋㅋㅋ 크 배라 그쵸",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044512.1.11",
          "form": "전 조각",
          "original_form": "전 조각",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044512.1.12",
          "form": "투썸 ? 이요",
          "original_form": "투썸 ? 이요",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044512.1.13",
          "form": "투썸이 케이크 맛집이래요",
          "original_form": "투썸이 케이크 맛집이래요",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044512.1.14",
          "form": "전 이번에 신세계몰 네이버스토어에서 민트초코케이크 주문했어요",
          "original_form": "전 이번에 신세계몰 네이버스토어에서 민트초코케이크 주문했어요",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044512.1.15",
          "form": "우와 ㅋㅋㅋㅋㅋ 맛있겟다",
          "original_form": "우와 ㅋㅋㅋㅋㅋ 맛있겟다",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044512.1.16",
          "form": "민초조아하세여?",
          "original_form": "민초조아하세여?",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044512.1.17",
          "form": "네 민초단이에요!",
          "original_form": "네 민초단이에요!",
          "speaker_id": "2",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044512.1.18",
          "form": "저동",
          "original_form": "저동",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044512.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044512.1.20",
          "form": "민초 추천제춤 잇어요?",
          "original_form": "민초 추천제춤 잇어요?",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044512.1.21",
          "form": "최근에 먹은건 스벅 민초요!",
          "original_form": "최근에 먹은건 스벅 민초요!",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044512.1.22",
          "form": "와 그거맛잇더라구요",
          "original_form": "와 그거맛잇더라구요",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044512.1.23",
          "form": "전 시럽은 좀 빼요",
          "original_form": "전 시럽은 좀 빼요",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044512.1.24",
          "form": "저도 시럽 뺐어요 반정도!",
          "original_form": "저도 시럽 뺐어요 반정도!",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044512.1.25",
          "form": "제가 아는 커뮤에 레시피 공유된거 봤거든요",
          "original_form": "제가 아는 커뮤에 레시피 공유된거 봤거든요",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044512.1.26",
          "form": "오 ㅋㅋㅋㅋ 반정도 빼라하나요",
          "original_form": "오 ㅋㅋㅋㅋ 반정도 빼라하나요",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044512.1.27",
          "form": "네 시럽이 달아서 민트맛 묻힌대요",
          "original_form": "네 시럽이 달아서 민트맛 묻힌대요",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044512.1.28",
          "form": "ㅋㅋㅋㅋㅋ 인정 !",
          "original_form": "ㅋㅋㅋㅋㅋ 인정 !",
          "speaker_id": "1",
          "time": "20210907 23:11"
        }
      ]
    }
  ]
