        {
          "id": "MDRW2100046989.1.1",
          "form": "핫딜어디서보세요?",
          "original_form": "핫딜어디서보세요?",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100046989.1.2",
          "form": "저는 그냥 11번가에 뜬 거 쭉 봐요",
          "original_form": "저는 그냥 11번가에 뜬 거 쭉 봐요",
          "speaker_id": "1",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100046989.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100046989.1.4",
          "form": "그리고 이월 할 때 되면",
          "original_form": "그리고 이월 할 때 되면",
          "speaker_id": "1",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100046989.1.5",
          "form": "11번가 지마켓 옥션",
          "original_form": "11번가 지마켓 옥션",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100046989.1.6",
          "form": "다둘러보죠",
          "original_form": "다둘러보죠",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100046989.1.7",
          "form": "후리스랑 패딩 쳐보고",
          "original_form": "후리스랑 패딩 쳐보고",
          "speaker_id": "1",
          "time": "20210908 09:06"
        },
        {
