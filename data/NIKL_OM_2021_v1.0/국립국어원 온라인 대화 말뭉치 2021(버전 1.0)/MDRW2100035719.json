      "utterance": [
        {
          "id": "MDRW2100035719.1.1",
          "form": "그런것 같아요 ㅋㅋ",
          "original_form": "그런것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100035719.1.2",
          "form": "아침드셨나요?ㅋ",
          "original_form": "아침드셨나요?ㅋ",
          "speaker_id": "2",
          "time": "20210830 11:08"
        },
        {
          "id": "MDRW2100035719.1.3",
          "form": "아침은 원래 걸러요ㅎ",
          "original_form": "아침은 원래 걸러요ㅎ",
          "speaker_id": "1",
          "time": "20210830 11:08"
        },
        {
          "id": "MDRW2100035719.1.4",
          "form": "name2님은 식사하셨나요?",
          "original_form": "&name2&님은 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210830 11:08"
        },
        {
          "id": "MDRW2100035719.1.5",
          "form": "네 저는 점심도 잘 먹었어요 ㅎㅎ",
          "original_form": "네 저는 점심도 잘 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:39"
        },
        {
          "id": "MDRW2100035719.1.6",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100035719.1.7",
          "form": "냉면이요 ㅎㅎ",
          "original_form": "냉면이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100035719.1.8",
          "form": "헉! 물냉이요? 비냉이요?",
          "original_form": "헉! 물냉이요? 비냉이요?",
          "speaker_id": "1",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100035719.1.9",
          "form": "물냉으로요 ㅎㅎ 근데 집에서 대충 먹어서 고명이 ㅠㅠ 없었어여",
          "original_form": "물냉으로요 ㅎㅎ 근데 집에서 대충 먹어서 고명이 ㅠㅠ 없었어여",
          "speaker_id": "2",
          "time": "20210830 14:54"
        },
        {
          "id": "MDRW2100035719.1.10",
          "form": "냉면에는 무절임이 필순데",
          "original_form": "냉면에는 무절임이 필순데",
          "speaker_id": "2",
          "time": "20210830 14:54"
        },
        {
          "id": "MDRW2100035719.1.11",
          "form": "배달이 아니고 직접 만드셨어요?",
          "original_form": "배달이 아니고 직접 만드셨어요?",
          "speaker_id": "1",
          "time": "20210830 14:54"
        },
        {
          "id": "MDRW2100035719.1.12",
          "form": "그냥 끓이기만하는거요 ㅎㅎ 냉동된거 있어서 ㅎㅎ",
          "original_form": "그냥 끓이기만하는거요 ㅎㅎ 냉동된거 있어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100035719.1.13",
          "form": "아ㅎ 요새는 인스턴트 잘 나오죠",
          "original_form": "아ㅎ 요새는 인스턴트 잘 나오죠",
          "speaker_id": "1",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100035719.1.14",
          "form": "맞아요 ㅎㅎㅎ 지금 냉동실 파먹기 하는중이라 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎㅎ 지금 냉동실 파먹기 하는중이라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100035719.1.15",
          "form": "무절임도 첨부됐음 좋을텐데",
          "original_form": "무절임도 첨부됐음 좋을텐데",
          "speaker_id": "1",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100035719.1.16",
          "form": "기업이 센스가 없네요",
          "original_form": "기업이 센스가 없네요",
          "speaker_id": "1",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100035719.1.17",
          "form": "그게 없더라구요 근데 무절임을 한번 도전해봤는데 맛이없어서 버리고..그냥 ㅋㅋ먹어요",
