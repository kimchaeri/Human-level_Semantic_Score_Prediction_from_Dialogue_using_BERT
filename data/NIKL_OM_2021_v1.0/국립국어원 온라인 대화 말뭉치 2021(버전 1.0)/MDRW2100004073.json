      "utterance": [
        {
          "id": "MDRW2100004073.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004073.1.2",
          "form": "크로플 처음먹어봤는데 너무 만족스러워서 또 먹고싶어요",
          "original_form": "크로플 처음먹어봤는데 너무 만족스러워서 또 먹고싶어요",
          "speaker_id": "1",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004073.1.3",
          "form": "크로플 맛있더라구요",
          "original_form": "크로플 맛있더라구요",
          "speaker_id": "2",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004073.1.4",
          "form": "본가에 있을 때는 크로아상 생지 사서 와플기로 만들어 먹었는데",
          "original_form": "본가에 있을 때는 크로아상 생지 사서 와플기로 만들어 먹었는데",
          "speaker_id": "2",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004073.1.5",
          "form": "와플 가게에 크로플 메뉴 있는거같던데 다음에는 거기서 배달시켜볼까해요",
          "original_form": "와플 가게에 크로플 메뉴 있는거같던데 다음에는 거기서 배달시켜볼까해요",
          "speaker_id": "1",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004073.1.6",
          "form": "우와우와 와플메이커 있으시군요?",
          "original_form": "우와우와 와플메이커 있으시군요?",
          "speaker_id": "1",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004073.1.7",
          "form": "오 맛있겠네요",
          "original_form": "오 맛있겠네요",
          "speaker_id": "2",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004073.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004073.1.9",
          "form": "와플메이커로 와플빼고 다 해먹었어요",
          "original_form": "와플메이커로 와플빼고 다 해먹었어요",
          "speaker_id": "2",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004073.1.10",
          "form": "볶음밥 눌러먹으니까 맛있더라구요",
          "original_form": "볶음밥 눌러먹으니까 맛있더라구요",
          "speaker_id": "2",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004073.1.11",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004073.1.12",
          "form": "약간 설거지 귀찮을고같아서 구매리스트에만 있어요",
          "original_form": "약간 설거지 귀찮을고같아서 구매리스트에만 있어요",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004073.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ그건 그래요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그건 그래요",
          "speaker_id": "2",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004073.1.14",
          "form": "설거지 귀찮아요ㅋㅋ",
          "original_form": "설거지 귀찮아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004073.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쵸",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쵸",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004073.1.16",
          "form": "구래서 시켜먹거나 카페가서 즐기는게 코로나만 아니면...",
          "original_form": "구래서 시켜먹거나 카페가서 즐기는게 코로나만 아니면...",
          "speaker_id": "1",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004073.1.17",
          "form": "울퉁불퉁해서 더 귀찮아요",
          "original_form": "울퉁불퉁해서 더 귀찮아요",
          "speaker_id": "2",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004073.1.18",
          "form": "가서 먹는게 뒤처리가 덜 귀찮은데",
          "original_form": "가서 먹는게 뒤처리가 덜 귀찮은데",
          "speaker_id": "2",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004073.1.19",
          "form": "대신 가는게 귀찮죠",
          "original_form": "대신 가는게 귀찮죠",
          "speaker_id": "2",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004073.1.20",
          "form": "그건 그렇죠..",
          "original_form": "그건 그렇죠..",
          "speaker_id": "1",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004073.1.21",
          "form": "하지만 남이 만들어주고 치워줌이 좋기때뮨에..코로나 사라져랏",
          "original_form": "하지만 남이 만들어주고 치워줌이 좋기때뮨에..코로나 사라져랏",
          "speaker_id": "1",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004073.1.22",
          "form": "남이 만든게 제일 맛있죠",
          "original_form": "남이 만든게 제일 맛있죠",
          "speaker_id": "2",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004073.1.23",
          "form": "치워주면 더 좋고",
          "original_form": "치워주면 더 좋고",
          "speaker_id": "2",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004073.1.24",
          "form": "그렇죠 그게 바로 외식의 장점이죠",
          "original_form": "그렇죠 그게 바로 외식의 장점이죠",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004073.1.25",
          "form": "요즘은 외식 잘 못하죠",
          "original_form": "요즘은 외식 잘 못하죠",
          "speaker_id": "2",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004073.1.26",
          "form": "코로나 때문에",
