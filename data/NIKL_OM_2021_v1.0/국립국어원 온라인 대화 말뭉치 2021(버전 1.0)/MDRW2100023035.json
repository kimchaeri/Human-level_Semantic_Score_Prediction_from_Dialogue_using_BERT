      "utterance": [
        {
          "id": "MDRW2100023035.1.1",
          "form": "마라탕은 아닙니다!",
          "original_form": "마라탕은 아닙니다!",
          "speaker_id": "2",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100023035.1.2",
          "form": "매운 버전도 있고 안 배운 버전도 있고...",
          "original_form": "매운 버전도 있고 안 배운 버전도 있고...",
          "speaker_id": "1",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100023035.1.3",
          "form": "한식이라고하셨었죠",
          "original_form": "한식이라고하셨었죠",
          "speaker_id": "1",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100023035.1.4",
          "form": "밥이랑 먹으면 괴식...",
          "original_form": "밥이랑 먹으면 괴식...",
          "speaker_id": "1",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100023035.1.5",
          "form": "집에서 만들어 먹을 수 있나요",
          "original_form": "집에서 만들어 먹을 수 있나요",
          "speaker_id": "1",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100023035.1.6",
          "form": "네 한국 출신(?)이긴 해요",
          "original_form": "네 한국 출신(?)이긴 해요",
          "speaker_id": "2",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100023035.1.7",
          "form": "한국 출신은 또 뭐죠",
          "original_form": "한국 출신은 또 뭐죠",
          "speaker_id": "1",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100023035.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100023035.1.9",
          "form": "아마도 못 만들거예요",
          "original_form": "아마도 못 만들거예요",
          "speaker_id": "2",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100023035.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100023035.1.11",
          "form": "그릇 단위로 세지 않습니다",
          "original_form": "그릇 단위로 세지 않습니다",
          "speaker_id": "2",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100023035.1.12",
          "form": "인원 수로 세지 않습니다",
          "original_form": "인원 수로 세지 않습니다",
          "speaker_id": "2",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100023035.1.13",
          "form": "안 매운 것 매운 것 둘 다 있습니다",
          "original_form": "안 매운 것 매운 것 둘 다 있습니다",
          "speaker_id": "2",
          "time": "20210829 20:55"
        },
        {
          "id": "MDRW2100023035.1.14",
          "form": "밥반찬 아니지만 괴식으로 먹기도 합니다",
          "original_form": "밥반찬 아니지만 괴식으로 먹기도 합니다",
          "speaker_id": "2",
          "time": "20210829 20:55"
        },
        {
          "id": "MDRW2100023035.1.15",
          "form": "밥이랑",
          "original_form": "밥이랑",
          "speaker_id": "2",
          "time": "20210829 20:55"
        },
        {
          "id": "MDRW2100023035.1.16",
          "form": "아니 노하우 잊으셨지요! ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니 노하우 잊으셨지요! ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:56"
        },
        {
          "id": "MDRW2100023035.1.17",
          "form": "아 잠깐만요 흠",
          "original_form": "아 잠깐만요 흠",
          "speaker_id": "1",
          "time": "20210829 20:56"
        },
        {
          "id": "MDRW2100023035.1.18",
          "form": "고기 들어가나요?",
          "original_form": "고기 들어가나요?",
          "speaker_id": "1",
          "time": "20210829 20:56"
        },
        {
          "id": "MDRW2100023035.1.19",
          "form": "해산물이 들어가요",
          "original_form": "해산물이 들어가요",
          "speaker_id": "2",
          "time": "20210829 20:56"
        },
        {
          "id": "MDRW2100023035.1.20",
          "form": "아주 쬐이이이이이이이끔",
          "original_form": "아주 쬐이이이이이이이끔",
          "speaker_id": "2",
          "time": "20210829 20:56"
        },
        {
          "id": "MDRW2100023035.1.21",
          "form": "그리고 나머지는 다 야챼예요?",
          "original_form": "그리고 나머지는 다 야챼예요?",
          "speaker_id": "1",
          "time": "20210829 20:57"
        },
        {
          "id": "MDRW2100023035.1.22",
          "form": "밀가루예요!",
          "original_form": "밀가루예요!",
          "speaker_id": "2",
          "time": "20210829 20:57"
        },
        {
          "id": "MDRW2100023035.1.23",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "1",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100023035.1.24",
          "form": "부침개?!",
          "original_form": "부침개?!",
          "speaker_id": "1",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100023035.1.25",
          "form": "땡~~~~~~~아니랍니다!!",
          "original_form": "땡~~~~~~~아니랍니다!!",
          "speaker_id": "2",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100023035.1.26",
          "form": "힌트 하나 드릴게요!",
          "original_form": "힌트 하나 드릴게요!",
          "speaker_id": "2",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100023035.1.27",
          "form": "식사류가 아닙니다",
          "original_form": "식사류가 아닙니다",
          "speaker_id": "2",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100023035.1.28",
          "form": "먹을 건 맞아요",
          "original_form": "먹을 건 맞아요",
          "speaker_id": "2",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100023035.1.29",
          "form": "식사가 아니라면 간식?! 인가요",
          "original_form": "식사가 아니라면 간식?! 인가요",
          "speaker_id": "1",
          "time": "20210829 20:59"
        },
        {
          "id": "MDRW2100023035.1.30",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210829 20:59"
        }
      ]
    }
