      "utterance": [
        {
          "id": "MDRW2100042128.1.1",
          "form": "요즘 게임 하시는거 있으세요??",
          "original_form": "요즘 게임 하시는거 있으세요??",
          "speaker_id": "2",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100042128.1.2",
          "form": "아뇨 저는 없어요",
          "original_form": "아뇨 저는 없어요",
          "speaker_id": "1",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100042128.1.3",
          "form": "name2님은용?",
          "original_form": "&name2&님은용?",
          "speaker_id": "1",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100042128.1.4",
          "form": "저는 모바일게임 많이 하죠",
          "original_form": "저는 모바일게임 많이 하죠",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100042128.1.5",
          "form": "오오 어떤거요?",
          "original_form": "오오 어떤거요?",
          "speaker_id": "1",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100042128.1.6",
          "form": "리듬겜을 주로 많이 해요!",
          "original_form": "리듬겜을 주로 많이 해요!",
          "speaker_id": "2",
          "time": "20210906 20:01"
        },
        {
          "id": "MDRW2100042128.1.7",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210906 20:02"
        },
        {
          "id": "MDRW2100042128.1.8",
          "form": "저 진짜 못하는데ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 진짜 못하는데ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:02"
        },
        {
          "id": "MDRW2100042128.1.9",
          "form": "박자감각이 좋으시군요?",
          "original_form": "박자감각이 좋으시군요?",
          "speaker_id": "1",
          "time": "20210906 20:02"
        },
        {
          "id": "MDRW2100042128.1.10",
          "form": "막 엄청 잘하는건 아니에요 ㅋㅋ",
          "original_form": "막 엄청 잘하는건 아니에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:03"
        },
        {
          "id": "MDRW2100042128.1.11",
          "form": "그래두 하나도 안틀리고 다 하면 뿌듯하더라구용",
          "original_form": "그래두 하나도 안틀리고 다 하면 뿌듯하더라구용",
          "speaker_id": "2",
          "time": "20210906 20:03"
        },
        {
          "id": "MDRW2100042128.1.12",
          "form": "전 리듬게임하고나면 어깨랑 목이랑 다 아파요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 리듬게임하고나면 어깨랑 목이랑 다 아파요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:06"
        },
        {
          "id": "MDRW2100042128.1.13",
          "form": "너무 긴장을 많이해가지구",
          "original_form": "너무 긴장을 많이해가지구",
          "speaker_id": "1",
          "time": "20210906 20:06"
        },
        {
          "id": "MDRW2100042128.1.14",
          "form": "손도 아프고",
          "original_form": "손도 아프고",
          "speaker_id": "1",
          "time": "20210906 20:06"
        },
        {
          "id": "MDRW2100042128.1.15",
          "form": "유난입니다",
          "original_form": "유난입니다",
          "speaker_id": "1",
          "time": "20210906 20:06"
        },
        {
          "id": "MDRW2100042128.1.16",
          "form": "저두 그래요 ㅋㅋㅋㅋ",
          "original_form": "저두 그래요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:10"
        },
        {
          "id": "MDRW2100042128.1.17",
          "form": "너무 많이하다보면 손목이랑 손가락이 많이 아프더라구요",
          "original_form": "너무 많이하다보면 손목이랑 손가락이 많이 아프더라구요",
          "speaker_id": "2",
          "time": "20210906 20:10"
        },
        {
          "id": "MDRW2100042128.1.18",
          "form": "리듬게임하다가 문득 나 왜 잘하지? 하고 생각하면 끝나는거 아시죠",
          "original_form": "리듬게임하다가 문득 나 왜 잘하지? 하고 생각하면 끝나는거 아시죠",
          "speaker_id": "1",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100042128.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100042128.1.20",
          "form": "저만 그런가요?",
          "original_form": "저만 그런가요?",
          "speaker_id": "1",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100042128.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ저두 그래요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저두 그래요",
          "speaker_id": "2",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100042128.1.22",
          "form": "갑자기 나 너무 잘하는것같아! 생각하면 꼭 틀리더라구요",
          "original_form": "갑자기 나 너무 잘하는것같아! 생각하면 꼭 틀리더라구요",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100042128.1.23",
          "form": "그쵸그쵸ㅋㅋㅋㅋㅋㅋㅋ갑자기 손이 안움직여요 ㅋㅋㅋㅋ",
          "original_form": "그쵸그쵸ㅋㅋㅋㅋㅋㅋㅋ갑자기 손이 안움직여요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100042128.1.24",
          "form": "앗 그러면 name2님 인내의 숲 이런거도 잘하세요?",
          "original_form": "앗 그러면 &name2&님 인내의 숲 이런거도 잘하세요?",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042128.1.25",
          "form": "인내의숲 다 깨본적 있어요!",
          "original_form": "인내의숲 다 깨본적 있어요!",
          "speaker_id": "2",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042128.1.26",
          "form": "ㄷㄷ...",
          "original_form": "ㄷㄷ...",
          "speaker_id": "1",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042128.1.27",
          "form": "저는 메이플 안에 있는 그 인내의 숲 비슷한 두번점프 안되는 그런 게임 단 한개도 성공못했어요",
          "original_form": "저는 메이플 안에 있는 그 인내의 숲 비슷한 두번점프 안되는 그런 게임 단 한개도 성공못했어요",
