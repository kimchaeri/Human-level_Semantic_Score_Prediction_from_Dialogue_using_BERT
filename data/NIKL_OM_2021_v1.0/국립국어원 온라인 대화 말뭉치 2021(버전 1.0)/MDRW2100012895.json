      "utterance": [
        {
          "id": "MDRW2100012895.1.1",
          "form": "정말 수확이 짭짤해요 ㅎㅎ",
          "original_form": "정말 수확이 짭짤해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100012895.1.2",
          "form": "열매가 있는  식물이 키울때 보람 두배일것 같아요 ㅎㅎ",
          "original_form": "열매가 있는  식물이 키울때 보람 두배일것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100012895.1.3",
          "form": "네 맞아요 ㅎㅎ",
          "original_form": "네 맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100012895.1.4",
          "form": "키워서 먹으면 맛있어요",
          "original_form": "키워서 먹으면 맛있어요",
          "speaker_id": "2",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100012895.1.5",
          "form": "뭐든 키워먹음 맛있죠 ㅎㅎ",
          "original_form": "뭐든 키워먹음 맛있죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100012895.1.6",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100012895.1.7",
          "form": "네 친구들 만나서 먹고 귀가했습니다ㅎㅎ",
          "original_form": "네 친구들 만나서 먹고 귀가했습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100012895.1.8",
          "form": "아 잼나셨겠어요",
          "original_form": "아 잼나셨겠어요",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100012895.1.9",
          "form": "코로나로 다들 귀가가 빨라요",
          "original_form": "코로나로 다들 귀가가 빨라요",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100012895.1.10",
          "form": "아쉽죠 ㅡㅡ",
          "original_form": "아쉽죠 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100012895.1.11",
          "form": "그래도 아내분은 일찍와서 좋아 하시겠어요",
          "original_form": "그래도 아내분은 일찍와서 좋아 하시겠어요",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100012895.1.12",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100012895.1.13",
          "form": "전 불고기 해서 먹었습니다",
          "original_form": "전 불고기 해서 먹었습니다",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100012895.1.14",
          "form": "가지토마토볶음이랑 ㅎㅎ",
          "original_form": "가지토마토볶음이랑 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100012895.1.15",
          "form": "이자카야였는데",
          "original_form": "이자카야였는데",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100012895.1.16",
          "form": "다른건 기억이 ㅎㅎ",
          "original_form": "다른건 기억이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100012895.1.17",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100012895.1.18",
          "form": "술하셨나봐요",
          "original_form": "술하셨나봐요",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100012895.1.19",
          "form": "친구랑 술 빠질수 없죠 ㅎㅎ",
          "original_form": "친구랑 술 빠질수 없죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100012895.1.20",
          "form": "친구 해외가서",
          "original_form": "친구 해외가서",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100012895.1.21",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100012895.1.22",
          "form": "송별회겸!",
          "original_form": "송별회겸!",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100012895.1.23",
          "form": "요새 어찌가나요",
          "original_form": "요새 어찌가나요",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100012895.1.24",
          "form": "거하게 해야겠어요",
          "original_form": "거하게 해야겠어요",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100012895.1.25",
          "form": "송별회 해야죠 맛난거 많이 사드리셨죠 ?",
          "original_form": "송별회 해야죠 맛난거 많이 사드리셨죠 ?",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100012895.1.26",
          "form": "친구가 샀는데 ㅎㅎ",
          "original_form": "친구가 샀는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100012895.1.27",
          "form": "돈 잘벌어서....",
          "original_form": "돈 잘벌어서....",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100012895.1.28",
          "form": "아 돈잘버는 사람이 사야죠 ㅎㅎ",
          "original_form": "아 돈잘버는 사람이 사야죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100012895.1.29",
          "form": "메뉴도 그 친구가 시켜서 ㅎㅅㄴ",
          "original_form": "메뉴도 그 친구가 시켜서 ㅎㅅㄴ",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100012895.1.30",
          "form": "그래도 언제 돌아올지 모르니 잘만나고 오셨네요 쉬는날 시간을 잘보내셨네요",
          "original_form": "그래도 언제 돌아올지 모르니 잘만나고 오셨네요 쉬는날 시간을 잘보내셨네요",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100012895.1.31",
          "form": "아 그럼 생각 안나죠 ㅎㅎ",
          "original_form": "아 그럼 생각 안나죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100012895.1.32",
          "form": "메뉴 고르는게 힘든데",
          "original_form": "메뉴 고르는게 힘든데",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100012895.1.33",
          "form": "대신 잘해주셨네요 안그럼 아무거나 시전하죠 ㅋㅋ",
          "original_form": "대신 잘해주셨네요 안그럼 아무거나 시전하죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100012895.1.34",
          "form": "토마토가지볶음 맛있더라고요",
          "original_form": "토마토가지볶음 맛있더라고요",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100012895.1.35",
          "form": "",
          "original_form": "",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100012895.1.36",
          "form": "거지가 요리로 하면 맛나요",
          "original_form": "거지가 요리로 하면 맛나요",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100012895.1.37",
          "form": "가지",
          "original_form": "가지",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100012895.1.38",
          "form": "중국식 가지 볶음도 맛있어요 ㅎㅎ",
          "original_form": "중국식 가지 볶음도 맛있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100012895.1.39",
          "form": "ㅎㅎ 토마토베이스가 좋더라구요",
          "original_form": "ㅎㅎ 토마토베이스가 좋더라구요",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100012895.1.40",
          "form": "맛나죠 ㅎㅎ",
          "original_form": "맛나죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100012895.1.41",
          "form": "안느끼하고 맛나겠어요",
          "original_form": "안느끼하고 맛나겠어요",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100012895.1.42",
          "form": "요새 이자카야는 안가서 무슨 메뉴 있는지 모르겠네요",
          "original_form": "요새 이자카야는 안가서 무슨 메뉴 있는지 모르겠네요",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100012895.1.43",
          "form": "저희남편은 술먹고 꼭 집에서 라면 먹어요 ㅋㅋ",
          "original_form": "저희남편은 술먹고 꼭 집에서 라면 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100012895.1.44",
          "form": "술드셨으니 일찍주시고 낼 꼭 해장하세요 ㅎㅎ",
          "original_form": "술드셨으니 일찍주시고 낼 꼭 해장하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100012895.1.45",
          "form": "네 신기 ㅎㅎ",
          "original_form": "네 신기 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:47"
        }
      ]
