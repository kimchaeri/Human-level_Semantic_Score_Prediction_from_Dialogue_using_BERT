        {
          "id": "MDRW2100024809.1.1",
          "form": "name2씨 방학 동안 인턴 하느라 고생했어요!",
          "original_form": "&name2&씨 방학 동안 인턴 하느라 고생했어요!",
          "speaker_id": "1",
          "time": "20210831 18:57"
        },
        {
          "id": "MDRW2100024809.1.2",
          "form": "감사해요!!",
          "original_form": "감사해요!!",
          "speaker_id": "2",
          "time": "20210831 18:57"
        },
        {
          "id": "MDRW2100024809.1.3",
          "form": "언니 대학원생활은 어때요??",
          "original_form": "언니 대학원생활은 어때요??",
          "speaker_id": "2",
          "time": "20210831 18:57"
        },
        {
          "id": "MDRW2100024809.1.4",
          "form": "제 대학원 생활은",
          "original_form": "제 대학원 생활은",
          "speaker_id": "1",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024809.1.5",
          "form": "이랬다 저랬다 오르락 내리락 하는 게 있지만",
          "original_form": "이랬다 저랬다 오르락 내리락 하는 게 있지만",
          "speaker_id": "1",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024809.1.6",
          "form": "요즘은 아주 워라밸이 좋아요",
          "original_form": "요즘은 아주 워라밸이 좋아요",
          "speaker_id": "1",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024809.1.7",
          "form": "후후,,,",
          "original_form": "후후,,,",
          "speaker_id": "1",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024809.1.8",
          "form": "이게 다 말디가 고장나서겠지만....^^",
          "original_form": "이게 다 말디가 고장나서겠지만....^^",
          "speaker_id": "1",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024809.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ말디고장이 가져다준 의외의 순기능(?)",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ말디고장이 가져다준 의외의 순기능(?)",
          "speaker_id": "2",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024809.1.10",
          "form": "대학원 정말 고민이에요ㅋㅋ",
          "original_form": "대학원 정말 고민이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:12"
        },
        {
          "id": "MDRW2100024809.1.11",
          "form": "하 그쵸",
          "original_form": "하 그쵸",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024809.1.12",
          "form": "제가 name2씌였어도",
          "original_form": "제가 &name2&씌였어도",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024809.1.13",
          "form": "고민 많이 했을 것 같아요",
          "original_form": "고민 많이 했을 것 같아요",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024809.1.14",
          "form": "대학원은 교수님만 좋다고 해서 되는 게 아니라",
          "original_form": "대학원은 교수님만 좋다고 해서 되는 게 아니라",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024809.1.15",
          "form": "연구분야가 좋다고 해서 다 되는 것도 아니고....",
          "original_form": "연구분야가 좋다고 해서 다 되는 것도 아니고....",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024809.1.16",
          "form": "변수가 참 많은 곳 같아요!",
          "original_form": "변수가 참 많은 곳 같아요!",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024809.1.17",
          "form": "진짜요ㅋㅋㅋ",
          "original_form": "진짜요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:14"
        },
        {
          "id": "MDRW2100024809.1.18",
          "form": "그 변수를 다 고려해야하는게",
          "original_form": "그 변수를 다 고려해야하는게",
          "speaker_id": "2",
          "time": "20210831 19:14"
        },
        {
          "id": "MDRW2100024809.1.19",
          "form": "고려해서 정해야하는게 쉽지않네요ㅋㅋ쿠ㅜㅜㅠ",
          "original_form": "고려해서 정해야하는게 쉽지않네요ㅋㅋ쿠ㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210831 19:14"
        },
        {
          "id": "MDRW2100024809.1.20",
          "form": "맞아요....",
          "original_form": "맞아요....",
          "speaker_id": "1",
          "time": "20210831 20:02"
        },
        {
          "id": "MDRW2100024809.1.21",
          "form": "어느 것을 가장 중요하게 여길 것인가!",
          "original_form": "어느 것을 가장 중요하게 여길 것인가!",
