      "utterance": [
        {
          "id": "MDRW2100028789.1.1",
          "form": "선선하니 풀벌레 소리 들려요",
          "original_form": "선선하니 풀벌레 소리 들려요",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028789.1.2",
          "form": "아 시골사시나봐요",
          "original_form": "아 시골사시나봐요",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028789.1.3",
          "form": "가을밤 산책이 생각나네요",
          "original_form": "가을밤 산책이 생각나네요",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028789.1.4",
          "form": "산책을진짜조아하시네요",
          "original_form": "산책을진짜조아하시네요",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028789.1.5",
          "form": "이제 밤에는 제법 쌀쌀하겠어요.",
          "original_form": "이제 밤에는 제법 쌀쌀하겠어요.",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028789.1.6",
          "form": "그러네요 일교차가 심해질듯",
          "original_form": "그러네요 일교차가 심해질듯",
          "speaker_id": "1",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028789.1.7",
          "form": "가디건 준비해야겠어요",
          "original_form": "가디건 준비해야겠어요",
          "speaker_id": "2",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028789.1.8",
          "form": "그래도 마스크 쓰니 감기는 줄겠어요",
          "original_form": "그래도 마스크 쓰니 감기는 줄겠어요",
          "speaker_id": "1",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028789.1.9",
          "form": "단풍도 곧 일듯",
          "original_form": "단풍도 곧 일듯",
          "speaker_id": "2",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028789.1.10",
          "form": "가디건은 저는 살붙어서 못입겠네요",
          "original_form": "가디건은 저는 살붙어서 못입겠네요",
          "speaker_id": "1",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028789.1.11",
          "form": "겨울도 금방 올거같아요",
          "original_form": "겨울도 금방 올거같아요",
          "speaker_id": "2",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028789.1.12",
          "form": "단풍구경 좋죠",
          "original_form": "단풍구경 좋죠",
          "speaker_id": "1",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028789.1.13",
          "form": "목도리는 좀 짜야겠어요",
          "original_form": "목도리는 좀 짜야겠어요",
          "speaker_id": "2",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028789.1.14",
          "form": "겨울에 눈 보고싶네요",
