      "utterance": [
        {
          "id": "MDRW2100019221.1.1",
          "form": "오늘 저녁은 도루묵생선찜이야",
          "original_form": "오늘 저녁은 도루묵생선찜이야",
          "speaker_id": "1",
          "time": "20210827 16:24"
        },
        {
          "id": "MDRW2100019221.1.2",
          "form": "내스탈아님 흠",
          "original_form": "내스탈아님 흠",
          "speaker_id": "1",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019221.1.3",
          "form": "헉 나두...그건 안끌려..",
          "original_form": "헉 나두...그건 안끌려..",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019221.1.4",
          "form": "난 작은 생선은 그냥그런거같아.. 도루묵 추어 또뭐있지",
          "original_form": "난 작은 생선은 그냥그런거같아.. 도루묵 추어 또뭐있지",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019221.1.5",
          "form": "ㅋㅋ찜이아니라 조림이네 먄...",
          "original_form": "ㅋㅋ찜이아니라 조림이네 먄...",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019221.1.6",
          "form": "찜이나 조림이나 ㅋㅋㅋ",
          "original_form": "찜이나 조림이나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019221.1.7",
          "form": "어탕국수도 별로일꺼같음",
          "original_form": "어탕국수도 별로일꺼같음",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019221.1.8",
          "form": "어제 생막창집갓는데 같이간언니가 괜찮았나봐 ㅋㅋ오늘 남편한테또가자햇데",
          "original_form": "어제 생막창집갓는데 같이간언니가 괜찮았나봐 ㅋㅋ오늘 남편한테또가자햇데",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019221.1.9",
          "form": "ㅋㅋㅋ나도 어탕국수 싫어..",
          "original_form": "ㅋㅋㅋ나도 어탕국수 싫어..",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019221.1.10",
          "form": "추어탕도 싫어..민물고기싫어  !",
          "original_form": "추어탕도 싫어..민물고기싫어  !",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019221.1.11",
          "form": "난아에 먹어본적도없지만",
          "original_form": "난아에 먹어본적도없지만",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019221.1.12",
          "form": "나랑진짜비슷하네 ㅋㅋㅋ",
          "original_form": "나랑진짜비슷하네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019221.1.13",
          "form": "나도 안먹어봄ㅋㅋㅋㅋ",
          "original_form": "나도 안먹어봄ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019221.1.14",
          "form": "비주얼부터 탈락이여",
          "original_form": "비주얼부터 탈락이여",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019221.1.15",
          "form": "나도 막창 먹고싶다 크 ㅠㅠ 쫄깃쫄깃",
          "original_form": "나도 막창 먹고싶다 크 ㅠㅠ 쫄깃쫄깃",
          "speaker_id": "1",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019221.1.16",
          "form": "거긴초벌없이",
          "original_form": "거긴초벌없이",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019221.1.17",
          "form": "생으로와",
          "original_form": "생으로와",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019221.1.18",
          "form": "생으로줘 ㅋㅋㅋ",
          "original_form": "생으로줘 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019221.1.19",
          "form": "그럼 굽기 힘들지않아?",
          "original_form": "그럼 굽기 힘들지않아?",
          "speaker_id": "1",
          "time": "20210827 16:29"
        },
        {
          "id": "MDRW2100019221.1.20",
          "form": "좀 오래걸리긴해!!",
          "original_form": "좀 오래걸리긴해!!",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019221.1.21",
          "form": "4인분시켯는데 ㅋㅋ한번굽고 먹기시작할때 나머지다 올려서 구워서 머금 ㅋㅋ",
          "original_form": "4인분시켯는데 ㅋㅋ한번굽고 먹기시작할때 나머지다 올려서 구워서 머금 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019221.1.22",
          "form": "ㅋㅋ 나는 구워주는데가 좋다!!",
          "original_form": "ㅋㅋ 나는 구워주는데가 좋다!!",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019221.1.23",
          "form": "초벌은 또 ㅋㅋㅋ 향이좋지 ㅋㅋ",
          "original_form": "초벌은 또 ㅋㅋㅋ 향이좋지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019221.1.24",
          "form": "저긴 생이래도 진짜냄새도안남 ㅋㅋ",
          "original_form": "저긴 생이래도 진짜냄새도안남 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019221.1.25",
          "form": "뮤튼근데오빠가한번가고또안간다햇어",
          "original_form": "뮤튼근데오빠가한번가고또안간다햇어",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019221.1.26",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019221.1.27",
          "form": "굽기 힘들어서??",
          "original_form": "굽기 힘들어서??",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019221.1.28",
          "form": "오래걸리고 ㅋㅋㅋㅋ테이블폭파해 ㅋㅋㅋ기름범벅 ㅋㅋ",
          "original_form": "오래걸리고 ㅋㅋㅋㅋ테이블폭파해 ㅋㅋㅋ기름범벅 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019221.1.29",
          "form": "곱창전골 먹고싶당",
          "original_form": "곱창전골 먹고싶당",
          "speaker_id": "1",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100019221.1.30",
          "form": "우리아빠 내장 음식 짱싫어해... 콜레스테롤 높다고",
          "original_form": "우리아빠 내장 음식 짱싫어해... 콜레스테롤 높다고",
          "speaker_id": "1",
