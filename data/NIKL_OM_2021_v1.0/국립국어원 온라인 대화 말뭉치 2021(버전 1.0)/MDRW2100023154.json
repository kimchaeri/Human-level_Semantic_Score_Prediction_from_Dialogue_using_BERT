        },
        {
          "id": "MDRW2100023154.1.2",
          "form": "아 갑자기 발견햇는데",
          "original_form": "아 갑자기 발견햇는데",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023154.1.3",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023154.1.4",
          "form": "하몽 먹어봤어?",
          "original_form": "하몽 먹어봤어?",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023154.1.5",
          "form": "아니?",
          "original_form": "아니?",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023154.1.6",
          "form": "걍 생햄류란것만",
          "original_form": "걍 생햄류란것만",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023154.1.7",
          "form": "알아",
          "original_form": "알아",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023154.1.8",
          "form": "내가 지난번에 호캉스 갈 때 사갔다가",
          "original_form": "내가 지난번에 호캉스 갈 때 사갔다가",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023154.1.9",
          "form": "포장 안뜯고 남은걸 들고왔거든",
          "original_form": "포장 안뜯고 남은걸 들고왔거든",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023154.1.10",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023154.1.11",
          "form": "생햄은 다른거엿나",
          "original_form": "생햄은 다른거엿나",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023154.1.12",
          "form": "생햄 맞을걸?",
          "original_form": "생햄 맞을걸?",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023154.1.13",
          "form": "ㅇㅎ...",
          "original_form": "ㅇㅎ...",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023154.1.14",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023154.1.15",
          "form": "근데 생햄은 유통기한",
          "original_form": "근데 생햄은 유통기한",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023154.1.16",
          "form": "사실 하몽 두개랑 햄 하나 사갔는데",
          "original_form": "사실 하몽 두개랑 햄 하나 사갔는데",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023154.1.17",
          "form": "별로 안길텐데",
          "original_form": "별로 안길텐데",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023154.1.18",
          "form": "ㅇㅇ 9월 중순까지",
          "original_form": "ㅇㅇ 9월 중순까지",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023154.1.19",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023154.1.20",
          "form": "ㅇㅇ그런데?",
          "original_form": "ㅇㅇ그런데?",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023154.1.21",
          "form": "하몽을 뜯었는데 의외로 좀 비린맛이 있는거야",
          "original_form": "하몽을 뜯었는데 의외로 좀 비린맛이 있는거야",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100023154.1.22",
          "form": "비린맛+애매한 식감으로",
          "original_form": "비린맛+애매한 식감으로",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023154.1.23",
          "form": "하나 간신히 먹고",
          "original_form": "하나 간신히 먹고",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023154.1.24",
          "form": "햄 뜯어서 햄먹음...",
          "original_form": "햄 뜯어서 햄먹음...",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023154.1.25",
          "form": "ㅇㅎ.......",
          "original_form": "ㅇㅎ.......",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023154.1.26",
          "form": "암튼 그랬는데 냉장고에 넣어두고 잊고있다가",
          "original_form": "암튼 그랬는데 냉장고에 넣어두고 잊고있다가",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023154.1.27",
          "form": "지금 꺼내보니까",
          "original_form": "지금 꺼내보니까",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023154.1.28",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023154.1.29",
          "form": "하몽에 하얀 점같은게 생겻는데",
          "original_form": "하몽에 하얀 점같은게 생겻는데",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023154.1.30",
          "form": "이거 상한건가...???",
          "original_form": "이거 상한건가...???",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023154.1.31",
          "form": "으잉??",
          "original_form": "으잉??",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023154.1.32",
          "form": "곰팡이 아니야...?",
          "original_form": "곰팡이 아니야...?",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023154.1.33",
          "form": "냄새 맡아봐",
          "original_form": "냄새 맡아봐",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023154.1.34",
          "form": "진짜 곰팡이 핀건가ㅠ",
          "original_form": "진짜 곰팡이 핀건가ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023154.1.35",
          "form": "고기나 햄류는",
          "original_form": "고기나 햄류는",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023154.1.36",
          "form": "나 상한거 냄새로 구분 잘 못함....",
          "original_form": "나 상한거 냄새로 구분 잘 못함....",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023154.1.37",
          "form": "미심적다 싶으면 먹으면 안돼",
          "original_form": "미심적다 싶으면 먹으면 안돼",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023154.1.38",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100023154.1.39",
          "form": "아니 간만에 생각나서",
          "original_form": "아니 간만에 생각나서",
          "speaker_id": "1",
          "time": "20210829 22:17"
        }
      ]
    }
