      "utterance": [
        {
          "id": "MDRW2100015267.1.1",
          "form": "지금 대학생이세요..?",
          "original_form": "지금 대학생이세요..?",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015267.1.2",
          "form": "네 대학생이에요!!",
          "original_form": "네 대학생이에요!!",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015267.1.3",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015267.1.4",
          "form": "수업은 재밌게듣고계신가요?",
          "original_form": "수업은 재밌게듣고계신가요?",
          "speaker_id": "1",
          "time": "20210826 01:44"
        },
        {
          "id": "MDRW2100015267.1.5",
          "form": "아니요 ㅋㅋ ㅠㅠ 혹시 몇학년이세요?",
          "original_form": "아니요 ㅋㅋ ㅠㅠ 혹시 몇학년이세요?",
          "speaker_id": "2",
          "time": "20210826 02:22"
        },
        {
          "id": "MDRW2100015267.1.6",
          "form": "저는 이제 3학년이요~ㅎㅎ",
          "original_form": "저는 이제 3학년이요~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 02:23"
        },
        {
          "id": "MDRW2100015267.1.7",
          "form": "저는 1학년이에요!!",
          "original_form": "저는 1학년이에요!!",
          "speaker_id": "2",
          "time": "20210826 02:25"
        },
        {
          "id": "MDRW2100015267.1.8",
          "form": "대학수업 저는 그냥 고닥교같아요ㅠ 그냥 음 해야하니까 하고~ 재미는 너무 없어요..",
          "original_form": "대학수업 저는 그냥 고닥교같아요ㅠ 그냥 음 해야하니까 하고~ 재미는 너무 없어요..",
          "speaker_id": "2",
          "time": "20210826 02:25"
        },
        {
          "id": "MDRW2100015267.1.9",
          "form": "아하!! 그져 ㅠㅠ 너무좋을시기인데 코로나라 안타까워요 ㅠㅠ",
          "original_form": "아하!! 그져 ㅠㅠ 너무좋을시기인데 코로나라 안타까워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 02:25"
        },
        {
          "id": "MDRW2100015267.1.10",
          "form": "전 집순이라 괜찮아요 ",
          "original_form": "전 집순이라 괜찮아요 ",
          "speaker_id": "2",
          "time": "20210826 02:26"
        },
        {
          "id": "MDRW2100015267.1.11",
          "form": "아하~그럼 다행이네요!",
          "original_form": "아하~그럼 다행이네요!",
          "speaker_id": "1",
          "time": "20210826 02:27"
        },
        {
          "id": "MDRW2100015267.1.12",
          "form": "혹시 과 물어봐도 될까요?!",
          "original_form": "혹시 과 물어봐도 될까요?!",
          "speaker_id": "2",
          "time": "20210826 02:30"
        },
        {
          "id": "MDRW2100015267.1.13",
          "form": "저는 경영학과에요 ㅎㅎ",
          "original_form": "저는 경영학과에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 02:30"
        },
        {
          "id": "MDRW2100015267.1.14",
          "form": "오 문과의 탑",
          "original_form": "오 문과의 탑",
          "speaker_id": "2",
          "time": "20210826 02:34"
        },
        {
          "id": "MDRW2100015267.1.15",
          "form": "어머 아녀요~ ㅋㅋㅋㅋ",
          "original_form": "어머 아녀요~ ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:35"
        },
        {
          "id": "MDRW2100015267.1.16",
          "form": "혹시 문과이신가요??",
          "original_form": "혹시 문과이신가요??",
          "speaker_id": "1",
          "time": "20210826 02:35"
        },
        {
          "id": "MDRW2100015267.1.17",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210826 02:35"
        },
        {
          "id": "MDRW2100015267.1.18",
          "form": "그래서 저는 심화전공도 국어교육과예요ㅋㅋ",
          "original_form": "그래서 저는 심화전공도 국어교육과예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:36"
        },
        {
          "id": "MDRW2100015267.1.19",
          "form": "우와! 국어교육 멋있네요!! 혹시 교사 희망하고계신가요?",
          "original_form": "우와! 국어교육 멋있네요!! 혹시 교사 희망하고계신가요?",
          "speaker_id": "1",
          "time": "20210826 02:36"
        },
        {
          "id": "MDRW2100015267.1.20",
          "form": "아 네네 초등교사요!",
          "original_form": "아 네네 초등교사요!",
          "speaker_id": "2",
          "time": "20210826 02:37"
        },
        {
          "id": "MDRW2100015267.1.21",
          "form": "대학과제 어렵지 않으세요? 발표같은거..",
          "original_form": "대학과제 어렵지 않으세요? 발표같은거..",
          "speaker_id": "2",
          "time": "20210826 02:37"
        },
        {
          "id": "MDRW2100015267.1.22",
          "form": "오 교사 저두 한때 꿈이었는데 ㅠ",
          "original_form": "오 교사 저두 한때 꿈이었는데 ㅠ",
          "speaker_id": "1",
          "time": "20210826 02:38"
        },
        {
          "id": "MDRW2100015267.1.23",
          "form": "발표 너무어려워요! 특히 저는 유학생이라 외국어로발표해야해서 더 힘들어요 ㅠㅠ",
          "original_form": "발표 너무어려워요! 특히 저는 유학생이라 외국어로발표해야해서 더 힘들어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 02:38"
        }
      ]
    }
