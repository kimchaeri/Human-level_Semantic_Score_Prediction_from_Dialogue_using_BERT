      "utterance": [
        {
          "id": "MDRW2100041736.1.1",
          "form": "언니어늘 모먹었엉",
          "original_form": "언니어늘 모먹었엉",
          "speaker_id": "2",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100041736.1.2",
          "form": "나는 오늘 점심을 아주 건전하게 먹었엉",
          "original_form": "나는 오늘 점심을 아주 건전하게 먹었엉",
          "speaker_id": "1",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100041736.1.3",
          "form": "얼린 두부를 초고추장에 찍어먹고....",
          "original_form": "얼린 두부를 초고추장에 찍어먹고....",
          "speaker_id": "1",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100041736.1.4",
          "form": "그리고.....",
          "original_form": "그리고.....",
          "speaker_id": "1",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100041736.1.5",
          "form": "헐.....대 박 완 전 건 강 해",
          "original_form": "헐.....대 박 완 전 건 강 해",
          "speaker_id": "2",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041736.1.6",
          "form": "그리고...!?!?!?",
          "original_form": "그리고...!?!?!?",
          "speaker_id": "2",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041736.1.7",
          "form": "콩나물 무침....",
          "original_form": "콩나물 무침....",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041736.1.8",
          "form": "나 이렇게 건강하게 먹어도 되는걸까?",
          "original_form": "나 이렇게 건강하게 먹어도 되는걸까?",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041736.1.9",
          "form": "미쳤다... 엄청나... 마치 어른같아..",
          "original_form": "미쳤다... 엄청나... 마치 어른같아..",
          "speaker_id": "2",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100041736.1.10",
          "form": "나는 새벽 4시~5시쯤 주전부리 잔뜩먹고 지금 더부룩해서",
          "original_form": "나는 새벽 4시~5시쯤 주전부리 잔뜩먹고 지금 더부룩해서",
          "speaker_id": "2",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100041736.1.11",
          "form": "아무것고 먹지못했는데..... 어른답지못해",
          "original_form": "아무것고 먹지못했는데..... 어른답지못해",
          "speaker_id": "2",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100041736.1.12",
          "form": "어른....어른.......",
          "original_form": "어른....어른.......",
          "speaker_id": "1",
          "time": "20210906 16:59"
        },
        {
          "id": "MDRW2100041736.1.13",
          "form": "그 주전부리에 혹시",
          "original_form": "그 주전부리에 혹시",
          "speaker_id": "1",
          "time": "20210906 16:59"
        },
        {
          "id": "MDRW2100041736.1.14",
          "form": "어제 받은 그 빵들도 있는거니?",
          "original_form": "어제 받은 그 빵들도 있는거니?",
          "speaker_id": "1",
          "time": "20210906 16:59"
        },
        {
          "id": "MDRW2100041736.1.15",
          "form": "엉... 그빵들포함.........",
          "original_form": "엉... 그빵들포함.........",
          "speaker_id": "2",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041736.1.16",
          "form": "베스트는 초코마들렌이었어",
          "original_form": "베스트는 초코마들렌이었어",
          "speaker_id": "2",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041736.1.17",
          "form": "하",
          "original_form": "하",
          "speaker_id": "1",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041736.1.18",
          "form": "정말 맛있었겠다",
          "original_form": "정말 맛있었겠다",
          "speaker_id": "1",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041736.1.19",
          "form": "아니 나는 뭔가 케이크 뷔페",
          "original_form": "아니 나는 뭔가 케이크 뷔페",
          "speaker_id": "1",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041736.1.20",
          "form": "한면이 초콜렛으로 코팅이 된 것이... 아주고냥... ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ 달달~ 했어.......",
          "original_form": "한면이 초콜렛으로 코팅이 된 것이... 아주고냥... ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ 달달~ 했어.......",
          "speaker_id": "2",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041736.1.21",
          "form": "가고싶어",
