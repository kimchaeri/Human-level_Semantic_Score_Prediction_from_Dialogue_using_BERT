        },
        {
          "id": "MDRW2100046062.1.2",
          "form": "우리 집에 놀러오면 맛있는 거 해줄게",
          "original_form": "우리 집에 놀러오면 맛있는 거 해줄게",
          "speaker_id": "1",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046062.1.3",
          "form": "어떤 맛있는 거",
          "original_form": "어떤 맛있는 거",
          "speaker_id": "2",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046062.1.4",
          "form": "너 좋아하는 거",
          "original_form": "너 좋아하는 거",
          "speaker_id": "1",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046062.1.5",
          "form": "나 월남쌈 좋아해",
          "original_form": "나 월남쌈 좋아해",
          "speaker_id": "2",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046062.1.6",
          "form": "쌈에는 뭐가 들어가지",
          "original_form": "쌈에는 뭐가 들어가지",
          "speaker_id": "1",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046062.1.7",
          "form": "새우",
          "original_form": "새우",
          "speaker_id": "2",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046062.1.8",
          "form": "레미는 있는데",
          "original_form": "레미는 있는데",
          "speaker_id": "1",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046062.1.9",
          "form": "이것도 괜찮지",
          "original_form": "이것도 괜찮지",
          "speaker_id": "2",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046062.1.10",
          "form": "오이랑 당근도 있어",
          "original_form": "오이랑 당근도 있어",
          "speaker_id": "1",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046062.1.11",
          "form": "내가 그러면 월남쌈 사갈게",
          "original_form": "내가 그러면 월남쌈 사갈게",
          "speaker_id": "2",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046062.1.12",
          "form": "와서 같이 해서 먹자",
          "original_form": "와서 같이 해서 먹자",
