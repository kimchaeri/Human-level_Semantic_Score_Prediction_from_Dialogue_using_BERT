      "utterance": [
        {
          "id": "MDRW2100010489.1.1",
          "form": "으어 태풍온대서",
          "original_form": "으어 태풍온대서",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010489.1.2",
          "form": "얼른 집가야겠어요ㅡ",
          "original_form": "얼른 집가야겠어요ㅡ",
          "speaker_id": "2",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100010489.1.3",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100010489.1.4",
          "form": "비가 엄청오고있어요",
          "original_form": "비가 엄청오고있어요",
          "speaker_id": "1",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100010489.1.5",
          "form": "그쪽도 그렇죠?",
          "original_form": "그쪽도 그렇죠?",
          "speaker_id": "1",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100010489.1.6",
          "form": "여긴 아직은 최큼 약해요 빗줄기가",
          "original_form": "여긴 아직은 최큼 약해요 빗줄기가",
          "speaker_id": "2",
          "time": "20210823 19:15"
        },
        {
          "id": "MDRW2100010489.1.7",
          "form": "다행이애요",
          "original_form": "다행이애요",
          "speaker_id": "1",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100010489.1.8",
          "form": "여기는 아까 엄청 쏟아져서 ㅠㅠ",
          "original_form": "여기는 아까 엄청 쏟아져서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100010489.1.9",
          "form": "홀짝 젖었어요",
          "original_form": "홀짝 젖었어요",
          "speaker_id": "1",
          "time": "20210823 20:00"
        },
        {
          "id": "MDRW2100010489.1.10",
          "form": "이게 참 갑자기 왔다가 또 안오다해서",
          "original_form": "이게 참 갑자기 왔다가 또 안오다해서",
          "speaker_id": "2",
          "time": "20210823 20:12"
        },
        {
          "id": "MDRW2100010489.1.11",
          "form": "소나기인데",
          "original_form": "소나기인데",
          "speaker_id": "2",
          "time": "20210823 20:12"
        },
        {
          "id": "MDRW2100010489.1.12",
          "form": "아니 스콜이 더 맞을듯요 ㅋㅋ",
          "original_form": "아니 스콜이 더 맞을듯요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010489.1.13",
          "form": "그 말이 더 맞는것같내요 ㅋㅋㅋㅋ",
          "original_form": "그 말이 더 맞는것같내요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:05"
        },
        {
          "id": "MDRW2100010489.1.14",
          "form": "오늘이 더위 마지막날 처서러던데",
          "original_form": "오늘이 더위 마지막날 처서러던데",
          "speaker_id": "1",
          "time": "20210823 21:05"
        },
        {
          "id": "MDRW2100010489.1.15",
          "form": "가을 옷 슬슬 꺼내셨나요!",
          "original_form": "가을 옷 슬슬 꺼내셨나요!",
          "speaker_id": "1",
          "time": "20210823 21:05"
        },
        {
          "id": "MDRW2100010489.1.16",
          "form": "아직 여름옷이네요 어휴 옷은 왜 항상 입을만한게 없지",
          "original_form": "아직 여름옷이네요 어휴 옷은 왜 항상 입을만한게 없지",
          "speaker_id": "2",
          "time": "20210823 21:20"
        },
        {
          "id": "MDRW2100010489.1.17",
          "form": "항상 그렇죠 ㅋㅋㅋㅋ",
          "original_form": "항상 그렇죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:44"
        },
        {
          "id": "MDRW2100010489.1.18",
          "form": "특히 요즘은 패션이 너무 확확 변해서",
          "original_form": "특히 요즘은 패션이 너무 확확 변해서",
          "speaker_id": "1",
          "time": "20210823 21:44"
        },
        {
          "id": "MDRW2100010489.1.19",
          "form": "더 어려운것같아요",
          "original_form": "더 어려운것같아요",
          "speaker_id": "1",
          "time": "20210823 21:44"
        },
        {
          "id": "MDRW2100010489.1.20",
          "form": "오 여긴 이제 본격적으로 내리네요",
          "original_form": "오 여긴 이제 본격적으로 내리네요",
          "speaker_id": "2",
          "time": "20210823 21:46"
        },
        {
          "id": "MDRW2100010489.1.21",
          "form": "제가 과거를 미화하는건지",
          "original_form": "제가 과거를 미화하는건지",
          "speaker_id": "2",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100010489.1.22",
          "form": "제가 어릴땐 날씨가 이정도는 아니었던거 같은데 ㅋㅋ",
          "original_form": "제가 어릴땐 날씨가 이정도는 아니었던거 같은데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100010489.1.23",
          "form": "아니에요 예전에는 진짜 안이랬죠 ㅋㅋㅋㅋㅋ",
          "original_form": "아니에요 예전에는 진짜 안이랬죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 00:38"
        },
        {
          "id": "MDRW2100010489.1.24",
          "form": "집에 에어컨 없어도 살만했던 시절이 있는데..",
          "original_form": "집에 에어컨 없어도 살만했던 시절이 있는데..",
          "speaker_id": "1",
          "time": "20210824 00:38"
        },
        {
          "id": "MDRW2100010489.1.25",
          "form": "어제는 제법 세차게 비가왔네요 ㄷㄷㄷ",
          "original_form": "어제는 제법 세차게 비가왔네요 ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210824 07:38"
        },
        {
          "id": "MDRW2100010489.1.26",
          "form": "지금도 열심히 내리고있네요 ㅋㅋㅋ",
          "original_form": "지금도 열심히 내리고있네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100010489.1.27",
          "form": "돌아다니기가 힘들어요 ㅠ",
          "original_form": "돌아다니기가 힘들어요 ㅠ",
          "speaker_id": "1",
          "time": "20210824 14:10"
        }
      ]
    }
