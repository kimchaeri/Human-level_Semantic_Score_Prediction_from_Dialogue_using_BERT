      "utterance": [
        {
          "id": "MDRW2100019862.1.1",
          "form": "아.. 여기는 오늘 선선하던데...",
          "original_form": "아.. 여기는 오늘 선선하던데...",
          "speaker_id": "2",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019862.1.2",
          "form": "아정말요 부럽다",
          "original_form": "아정말요 부럽다",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019862.1.3",
          "form": "남쪽지역이세요?",
          "original_form": "남쪽지역이세요?",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019862.1.4",
          "form": "네네 부산이에용",
          "original_form": "네네 부산이에용",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019862.1.5",
          "form": "부산이라면 수긍.. ㅎㅎㅎ",
          "original_form": "부산이라면 수긍.. ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019862.1.6",
          "form": "부산은 패딩도 필요엄쓸거 같아요ㅡㅡ",
          "original_form": "부산은 패딩도 필요엄쓸거 같아요ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019862.1.7",
          "form": "아 .. 그래도롱패딩 필요해요ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "아 .. 그래도롱패딩 필요해요ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019862.1.8",
          "form": "겨울에 눈도 잘 안오잖아요? \r\n패딩은 사치에요.. ㅋㅋㅋ",
          "original_form": "겨울에 눈도 잘 안오잖아요? \r\n패딩은 사치에요.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019862.1.9",
          "form": "그쵸 윗지방분들에겐 명함도 못내밀죠 ㅋㅋ",
          "original_form": "그쵸 윗지방분들에겐 명함도 못내밀죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019862.1.10",
          "form": "철원 사람들은 더 대단하더라구요..",
          "original_form": "철원 사람들은 더 대단하더라구요..",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019862.1.11",
          "form": "한겨울에 서울와서 덥다고 반팔입어요.. ㅋㅋ",
          "original_form": "한겨울에 서울와서 덥다고 반팔입어요.. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019862.1.12",
          "form": "와오 정말인가유",
          "original_form": "와오 정말인가유",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019862.1.13",
          "form": "네.. 철원으로 군대갓다온 친구들이.그러더라구요..",
          "original_form": "네.. 철원으로 군대갓다온 친구들이.그러더라구요..",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019862.1.14",
          "form": "맞아요 그럴것같아요",
          "original_form": "맞아요 그럴것같아요",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019862.1.15",
          "form": "바람은 안불어요? 태풍 완전 끝?",
          "original_form": "바람은 안불어요? 태풍 완전 끝?",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100019862.1.16",
          "form": "네 끝 아까 비가 왔다가 안왔다가 하긴했어요",
          "original_form": "네 끝 아까 비가 왔다가 안왔다가 하긴했어요",
