      "utterance": [
        {
          "id": "MDRW2100002195.1.1",
          "form": "난 만화책이 좋아",
          "original_form": "난 만화책이 좋아",
          "speaker_id": "1",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002195.1.2",
          "form": "오 어떤거",
          "original_form": "오 어떤거",
          "speaker_id": "2",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002195.1.3",
          "form": "나도 한때 해적왕을 꿈꿨었는데",
          "original_form": "나도 한때 해적왕을 꿈꿨었는데",
          "speaker_id": "2",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002195.1.4",
          "form": "사실 웹툰을 좀 더 많이보긴 해",
          "original_form": "사실 웹툰을 좀 더 많이보긴 해",
          "speaker_id": "1",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002195.1.5",
          "form": "해적왕 모두의 꿈 아니겠니",
          "original_form": "해적왕 모두의 꿈 아니겠니",
          "speaker_id": "1",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002195.1.6",
          "form": "난 웹툰은 잘 안보긴 하는데",
          "original_form": "난 웹툰은 잘 안보긴 하는데",
          "speaker_id": "2",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002195.1.7",
          "form": "머니게임은 봤었어",
          "original_form": "머니게임은 봤었어",
          "speaker_id": "2",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002195.1.8",
          "form": "근데 그림이 뭔가 어지러워서 끝까지는 못봤어",
          "original_form": "근데 그림이 뭔가 어지러워서 끝까지는 못봤어",
          "speaker_id": "2",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002195.1.9",
          "form": "그림체 좀 무섭긴 해",
          "original_form": "그림체 좀 무섭긴 해",
          "speaker_id": "1",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002195.1.10",
          "form": "소설같은건 많이 읽는편이야?",
          "original_form": "소설같은건 많이 읽는편이야?",
          "speaker_id": "1",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002195.1.11",
          "form": "더글라스 케네디나 귀욤 미소꺼 자주 봤지",
          "original_form": "더글라스 케네디나 귀욤 미소꺼 자주 봤지",
          "speaker_id": "2",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002195.1.12",
          "form": "오호 난 일본 추리소설 좋아해",
          "original_form": "오호 난 일본 추리소설 좋아해",
          "speaker_id": "1",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002195.1.13",
          "form": "고백이었나? 그거도 추리소설이었지? 나도 그런거 좋아함ㅋㅋㅋ",
          "original_form": "고백이었나? 그거도 추리소설이었지? 나도 그런거 좋아함ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002195.1.14",
          "form": "하나만 추천해줘봐",
          "original_form": "하나만 추천해줘봐",
          "speaker_id": "2",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002195.1.15",
          "form": "용의자 x의 헌신 재밌었어",
          "original_form": "용의자 x의 헌신 재밌었어",
          "speaker_id": "1",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002195.1.16",
          "form": "영화로도 나왔었잖아!",
          "original_form": "영화로도 나왔었잖아!",
          "speaker_id": "1",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002195.1.17",
          "form": "아 그거 봤던것같은데",
          "original_form": "아 그거 봤던것같은데",
          "speaker_id": "2",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002195.1.18",
          "form": "그치 어디선가 들어봤을꺼야",
          "original_form": "그치 어디선가 들어봤을꺼야",
          "speaker_id": "1",
          "time": "20210820 00:38"
        },
        {
          "id": "MDRW2100002195.1.19",
          "form": "그런건 읽으면 뭔가 반전 딱 보고나서 와... 하고는",
          "original_form": "그런건 읽으면 뭔가 반전 딱 보고나서 와... 하고는",
          "speaker_id": "2",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002195.1.20",
          "form": "다음날 리셋되잖아ㅋㅋㅋ",
          "original_form": "다음날 리셋되잖아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002195.1.21",
          "form": "볼때마다 새로워",
          "original_form": "볼때마다 새로워",
          "speaker_id": "2",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002195.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ마자 새로운 떡밥을 보게된다구",
          "original_form": "ㅋㅋㅋㅋㅋㅋ마자 새로운 떡밥을 보게된다구",
          "speaker_id": "1",
          "time": "20210820 08:23"
        }
