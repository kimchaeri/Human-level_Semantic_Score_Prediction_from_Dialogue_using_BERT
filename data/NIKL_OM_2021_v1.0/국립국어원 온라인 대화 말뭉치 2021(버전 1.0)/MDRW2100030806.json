      "utterance": [
        {
          "id": "MDRW2100030806.1.1",
          "form": "아쉽다 서울가서 보면 좋겠네",
          "original_form": "아쉽다 서울가서 보면 좋겠네",
          "speaker_id": "2",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030806.1.2",
          "form": "가까우니까 보러갈수 있어요 ㅎㅎ",
          "original_form": "가까우니까 보러갈수 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030806.1.3",
          "form": "오페라의 유령도 좋더라",
          "original_form": "오페라의 유령도 좋더라",
          "speaker_id": "2",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030806.1.4",
          "form": "그건 영화로 보셨죠?",
          "original_form": "그건 영화로 보셨죠?",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030806.1.5",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030806.1.6",
          "form": "뮤지컬 원작인가",
          "original_form": "뮤지컬 원작인가",
          "speaker_id": "2",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030806.1.7",
          "form": "아마 소설원작 뮤지컬일거같은데",
          "original_form": "아마 소설원작 뮤지컬일거같은데",
          "speaker_id": "1",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030806.1.8",
          "form": "영화 원작은 뮤지컬이지",
          "original_form": "영화 원작은 뮤지컬이지",
          "speaker_id": "2",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030806.1.9",
          "form": "대충 그런거같기도 하네요 ㅋㅋㅋ",
          "original_form": "대충 그런거같기도 하네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030806.1.10",
          "form": "거기 남자배우들 잘생겼잖아",
          "original_form": "거기 남자배우들 잘생겼잖아",
          "speaker_id": "2",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030806.1.11",
          "form": "뮤지컬이라서 많이 바뀌긴 했는데",
          "original_form": "뮤지컬이라서 많이 바뀌긴 했는데",
          "speaker_id": "1",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030806.1.12",
          "form": "영호ㅓ도 잘생기긴 했죠 ㅎ",
          "original_form": "영호ㅓ도 잘생기긴 했죠 ㅎ",
          "speaker_id": "1",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030806.1.13",
          "form": "노래도 잘부르고",
          "original_form": "노래도 잘부르고",
          "speaker_id": "2",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030806.1.14",
          "form": "가면쓰고 못생겼다고 하는데",
          "original_form": "가면쓰고 못생겼다고 하는데",
          "speaker_id": "2",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030806.1.15",
          "form": "하나도 안못생겼죠 벗고다녀야해",
          "original_form": "하나도 안못생겼죠 벗고다녀야해",
          "speaker_id": "1",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030806.1.16",
          "form": "아무튼 영화라 이것저것 연출 좋더라",
          "original_form": "아무튼 영화라 이것저것 연출 좋더라",
          "speaker_id": "2",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030806.1.17",
          "form": "저는 뮤지컬판도 봤었는데",
          "original_form": "저는 뮤지컬판도 봤었는데",
          "speaker_id": "1",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030806.1.18",
          "form": "연출 괜찮았어요 돈 많이 들어서 그런가",
          "original_form": "연출 괜찮았어요 돈 많이 들어서 그런가",
          "speaker_id": "1",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030806.1.19",
          "form": "막 돈 많이 쓴 티 나서 좋았어요",
          "original_form": "막 돈 많이 쓴 티 나서 좋았어요",
          "speaker_id": "1",
          "time": "20210904 12:47"
        }
