      "utterance": [
        {
          "id": "MDRW2100005306.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005306.1.2",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005306.1.3",
          "form": "요즘 책이나 잡지 읽은거 있니",
          "original_form": "요즘 책이나 잡지 읽은거 있니",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005306.1.4",
          "form": "난 주로 웹소설을 읽는 편인데 종이책을 읽은지는 오래된 것 같아",
          "original_form": "난 주로 웹소설을 읽는 편인데 종이책을 읽은지는 오래된 것 같아",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005306.1.5",
          "form": "나도 종아책은 안 읽은 지 오조억년 전이고",
          "original_form": "나도 종아책은 안 읽은 지 오조억년 전이고",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005306.1.6",
          "form": "웹툰이나 퉵소걸만 보는 것 같아",
          "original_form": "웹툰이나 퉵소걸만 보는 것 같아",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005306.1.7",
          "form": "웹소설",
          "original_form": "웹소설",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005306.1.8",
          "form": "지난달에 요즘책방 정주행했는데 책들이 재밌어보이긴 하더라",
          "original_form": "지난달에 요즘책방 정주행했는데 책들이 재밌어보이긴 하더라",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005306.1.9",
          "form": "호밀밭의 파수꾼 우리집에 있었는데 한 번도 안읽어서 그냥 버렸어",
          "original_form": "호밀밭의 파수꾼 우리집에 있었는데 한 번도 안읽어서 그냥 버렸어",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005306.1.10",
          "form": "오 책방이 아직도 남아있어?",
          "original_form": "오 책방이 아직도 남아있어?",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005306.1.11",
          "form": "추억이넹",
          "original_form": "추억이넹",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005306.1.12",
          "form": "아니 요즘책방이라는 프로그램이 있다",
          "original_form": "아니 요즘책방이라는 프로그램이 있다",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005306.1.13",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005306.1.14",
          "form": "책 내용 강독해주고 그 책에 대해서 얘기하는 프로그램이야",
          "original_form": "책 내용 강독해주고 그 책에 대해서 얘기하는 프로그램이야",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005306.1.15",
          "form": "책소개 프로그램이나",
          "original_form": "책소개 프로그램이나",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005306.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005306.1.17",
          "form": "거기서 젤 기억나는 책이 뭐야",
          "original_form": "거기서 젤 기억나는 책이 뭐야",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005306.1.18",
          "form": "동물농장인가 그런 제목이었는데",
          "original_form": "동물농장인가 그런 제목이었는데",
          "speaker_id": "1",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005306.1.19",
          "form": "탭이라 호밀밭 파수꾼 지금봤네 갸웃곀ㅋㅋㅋㅋㅋ",
          "original_form": "탭이라 호밀밭 파수꾼 지금봤네 갸웃곀ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005306.1.20",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005306.1.21",
          "form": "나 동물농정이렁 호밀벝 둘다",
          "original_form": "나 동물농정이렁 호밀벝 둘다",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005306.1.22",
          "form": "동물들이 인간에 대항해서 농장주를 몰아내고",
          "original_form": "동물들이 인간에 대항해서 농장주를 몰아내고",
          "speaker_id": "1",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005306.1.23",
          "form": "읽어보긴했어",
          "original_form": "읽어보긴했어",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005306.1.24",
          "form": "돼지가 나중에 군림하는 ㅇㅇ",
          "original_form": "돼지가 나중에 군림하는 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005306.1.25",
          "form": "그거 진짜",
          "original_form": "그거 진짜",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005306.1.26",
          "form": "명불허전이지",
          "original_form": "명불허전이지",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005306.1.27",
          "form": "그책 독후감을 중딩때부터 고딩때까지",
          "original_form": "그책 독후감을 중딩때부터 고딩때까지",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005306.1.28",
          "form": "써먹었다",
          "original_form": "써먹었다",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005306.1.29",
          "form": "올 써먹긴 좋은 책이긴 하다",
          "original_form": "올 써먹긴 좋은 책이긴 하다",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005306.1.30",
          "form": "호밀밭 파수꾼 내용은",
          "original_form": "호밀밭 파수꾼 내용은",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005306.1.31",
          "form": "혹시아니",
          "original_form": "혹시아니",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005306.1.32",
          "form": "그건 내용이 좀 복잡하긴 한데",
          "original_form": "그건 내용이 좀 복잡하긴 한데",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005306.1.33",
          "form": "주인공이 고등학교인가 자퇴하고 가출해서 이러쿵저러쿵 겪는 내용인데 솔직히 수토리 다 기억안나고",
          "original_form": "주인공이 고등학교인가 자퇴하고 가출해서 이러쿵저러쿵 겪는 내용인데 솔직히 수토리 다 기억안나고",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005306.1.34",
          "form": "하나 인상적인건 그 책이 혼잣말하듯이 책을 써서 처음 나왔을 때 되게 충격적이었데",
          "original_form": "하나 인상적인건 그 책이 혼잣말하듯이 책을 써서 처음 나왔을 때 되게 충격적이었데",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005306.1.35",
          "form": "그게 최초라고 했던 것 같아",
          "original_form": "그게 최초라고 했던 것 같아",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005306.1.36",
          "form": "맞아 혼잣말이 신박하긴 했어",
          "original_form": "맞아 혼잣말이 신박하긴 했어",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005306.1.37",
          "form": "내용은 마약에 성매매에",
          "original_form": "내용은 마약에 성매매에",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005306.1.38",
          "form": "최악까지 가서 충격적이거",
          "original_form": "최악까지 가서 충격적이거",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005306.1.39",
          "form": "뭐여",
          "original_form": "뭐여",
          "speaker_id": "2",
          "time": "20210821 22:01"
        }
      ]
    }
