      "utterance": [
        {
          "id": "MDRW2100016402.1.1",
          "form": "곰표맥주 먹어봤어?",
          "original_form": "곰표맥주 먹어봤어?",
          "speaker_id": "1",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016402.1.2",
          "form": "응! 먹어봤지",
          "original_form": "응! 먹어봤지",
          "speaker_id": "2",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100016402.1.3",
          "form": "그거 엄청 잘팔려서 못먹을줄 알았는데 동네 편의점에 있더라 ㅋㅋ",
          "original_form": "그거 엄청 잘팔려서 못먹을줄 알았는데 동네 편의점에 있더라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100016402.1.4",
          "form": "ㅋㅋㅋ맞아 우리 편의점에도 있더라",
          "original_form": "ㅋㅋㅋ맞아 우리 편의점에도 있더라",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100016402.1.5",
          "form": "난 그저그렇던데 맛있었어??",
          "original_form": "난 그저그렇던데 맛있었어??",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100016402.1.6",
          "form": "나도 그저 그랬어",
          "original_form": "나도 그저 그랬어",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100016402.1.7",
          "form": "그치 생각보단 별로였어",
          "original_form": "그치 생각보단 별로였어",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100016402.1.8",
          "form": "이게 없어서 못먹을 정도는 아닌거 같은디.. 이런 생각 들더라 ㅋㅋㅋ",
          "original_form": "이게 없어서 못먹을 정도는 아닌거 같은디.. 이런 생각 들더라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100016402.1.9",
          "form": "ㅋㅋㅋ 그래서 마케팅이란 말이 있더라",
          "original_form": "ㅋㅋㅋ 그래서 마케팅이란 말이 있더라",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100016402.1.10",
          "form": "마케팅의 중요성 ㅋㅋㅋ",
          "original_form": "마케팅의 중요성 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100016402.1.11",
          "form": "ㅋㅋㅋ맞아",
          "original_form": "ㅋㅋㅋ맞아",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100016402.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100016402.1.13",
          "form": "맛은 그저그런데 패키지는 귀엽더라",
          "original_form": "맛은 그저그런데 패키지는 귀엽더라",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100016402.1.14",
          "form": "그건 인정",
          "original_form": "그건 인정",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100016402.1.15",
          "form": "아 맞아 ㅋㅋㅋㅋ",
          "original_form": "아 맞아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100016402.1.16",
          "form": "오 혹시 흑맥주중에 추천해줄만한거 있어?!",
          "original_form": "오 혹시 흑맥주중에 추천해줄만한거 있어?!",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100016402.1.17",
          "form": "뭐 먹어야될지 모르겠어ㅜㅜ",
          "original_form": "뭐 먹어야될지 모르겠어ㅜㅜ",
