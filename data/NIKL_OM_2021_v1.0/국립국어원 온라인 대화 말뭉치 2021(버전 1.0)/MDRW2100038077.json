      "utterance": [
        {
          "id": "MDRW2100038077.1.1",
          "form": "가족있으신가요?ㅋ",
          "original_form": "가족있으신가요?ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038077.1.2",
          "form": "ㅋㅋ네 있죵",
          "original_form": "ㅋㅋ네 있죵",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038077.1.3",
          "form": "지금은 혼자요",
          "original_form": "지금은 혼자요",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038077.1.4",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038077.1.5",
          "form": "그분들도",
          "original_form": "그분들도",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038077.1.6",
          "form": "가족이라 생각하나요?",
          "original_form": "가족이라 생각하나요?",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038077.1.7",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038077.1.8",
          "form": "ㅋㅋㅋ그랬음좋겠네요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ그랬음좋겠네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038077.1.9",
          "form": "있을때 잘하세요ㅋㅋㅋㅋㅋ",
          "original_form": "있을때 잘하세요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038077.1.10",
          "form": "ㅋㅋㅋㅋㅋ그 그래야쥬ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ그 그래야쥬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038077.1.11",
          "form": "어머니환갑때 뭐해드려야할까여",
          "original_form": "어머니환갑때 뭐해드려야할까여",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038077.1.12",
          "form": "저는 여행보내드렸는데",
          "original_form": "저는 여행보내드렸는데",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038077.1.13",
          "form": "지금은 그러기는 힘든시국이고",
          "original_form": "지금은 그러기는 힘든시국이고",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100038077.1.14",
          "form": "띠용~ㅜㅜ",
          "original_form": "띠용~ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100038077.1.15",
          "form": "용돈?",
          "original_form": "용돈?",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100038077.1.16",
          "form": "무난하군용",
          "original_form": "무난하군용",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100038077.1.17",
          "form": "돈이 최고일 나이잖아요..",
          "original_form": "돈이 최고일 나이잖아요..",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100038077.1.18",
          "form": "그쵸 쓰기도편해요",
          "original_form": "그쵸 쓰기도편해요",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100038077.1.19",
          "form": "집을 사야하는데",
          "original_form": "집을 사야하는데",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100038077.1.20",
          "form": "돈이 없어요",
          "original_form": "돈이 없어요",
          "speaker_id": "1",
          "time": "20210901 23:37"
        }
      ]
