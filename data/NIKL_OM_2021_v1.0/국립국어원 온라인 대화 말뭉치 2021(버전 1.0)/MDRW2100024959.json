        },
        {
          "id": "MDRW2100024959.1.5",
          "form": "네ㅎㅎㅎ",
          "original_form": "네ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100024959.1.6",
          "form": "어떤 분야에서 일하세요~?",
          "original_form": "어떤 분야에서 일하세요~?",
          "speaker_id": "1",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100024959.1.7",
          "form": "환경쪽이요 ㅎㅎ name1님은요?",
          "original_form": "환경쪽이요 ㅎㅎ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024959.1.8",
          "form": "전 교육쪽이요ㅎㅎㅎ",
          "original_form": "전 교육쪽이요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100024959.1.9",
          "form": "환경쪽이면 보람있으시겠어요",
          "original_form": "환경쪽이면 보람있으시겠어요",
          "speaker_id": "1",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100024959.1.10",
          "form": "삶과 직접적인 직업이라",
          "original_form": "삶과 직접적인 직업이라",
          "speaker_id": "1",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100024959.1.11",
          "form": "처음에는 그랬는데 무뎌지는거같아요..ㅎㅎ..",
          "original_form": "처음에는 그랬는데 무뎌지는거같아요..ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024959.1.12",
          "form": "교육이야말로 보람차실거같아요",
          "original_form": "교육이야말로 보람차실거같아요",
          "speaker_id": "2",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024959.1.13",
          "form": "그래도 내 자식은 보람차지 않더라구요ㅋㅋㅋㅋ",
          "original_form": "그래도 내 자식은 보람차지 않더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100024959.1.14",
          "form": "앜ㅋㅋㅋㅋㅠㅠ자녀가 있으신가요?",
          "original_form": "앜ㅋㅋㅋㅋㅠㅠ자녀가 있으신가요?",
