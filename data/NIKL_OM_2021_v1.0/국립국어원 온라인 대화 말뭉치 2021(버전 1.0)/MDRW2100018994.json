      "utterance": [
        {
          "id": "MDRW2100018994.1.1",
          "form": "안녕하세요 책 읽는거 좋아하세요?",
          "original_form": "안녕하세요 책 읽는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 14:30"
        },
        {
          "id": "MDRW2100018994.1.2",
          "form": "많이 안 읽었는데 최근에 그래도 읽어보려고 노력하고 있어요",
          "original_form": "많이 안 읽었는데 최근에 그래도 읽어보려고 노력하고 있어요",
          "speaker_id": "2",
          "time": "20210827 14:37"
        },
        {
          "id": "MDRW2100018994.1.3",
          "form": "선생님은 책 읽는 거 좋아하시나요?",
          "original_form": "선생님은 책 읽는 거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 14:37"
        },
        {
          "id": "MDRW2100018994.1.4",
          "form": "저는 예전에 많이 읽었네요 ㅎㅎ 지금은 거의 안읽어요 ㅎㅎ",
          "original_form": "저는 예전에 많이 읽었네요 ㅎㅎ 지금은 거의 안읽어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100018994.1.5",
          "form": "그렇군요 보통 어떤 장르 보셨나요?",
          "original_form": "그렇군요 보통 어떤 장르 보셨나요?",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100018994.1.6",
          "form": "저는 추리소설 좋아했어요!",
          "original_form": "저는 추리소설 좋아했어요!",
          "speaker_id": "1",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100018994.1.7",
          "form": "아하 저는 추리 너무 어렵더라구요ㅋㅋㅋ",
          "original_form": "아하 저는 추리 너무 어렵더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100018994.1.8",
          "form": "저는 에세이류 좋아해요",
          "original_form": "저는 에세이류 좋아해요",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100018994.1.9",
          "form": "오 저도 생각 많을때 에세이류를 읽고싶은데",
          "original_form": "오 저도 생각 많을때 에세이류를 읽고싶은데",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100018994.1.10",
          "form": "추천해주실 작품 있나요?",
          "original_form": "추천해주실 작품 있나요?",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100018994.1.11",
          "form": "갑자기 추천해드리려니 생각은 안 나는데",
          "original_form": "갑자기 추천해드리려니 생각은 안 나는데",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100018994.1.12",
          "form": "시집 괜찮으시다면 꽃을 보듯 너를 본다 추천드려요",
          "original_form": "시집 괜찮으시다면 꽃을 보듯 너를 본다 추천드려요",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100018994.1.13",
          "form": "생각 많을 때 시집 읽으니까 좋더라구요",
          "original_form": "생각 많을 때 시집 읽으니까 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100018994.1.14",
          "form": "오 시집도 좋아해요! 예전에 자주 읽었어요",
          "original_form": "오 시집도 좋아해요! 예전에 자주 읽었어요",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100018994.1.15",
          "form": "검색해보니 유명한 구절이 나오네요 ㅋㅋ",
          "original_form": "검색해보니 유명한 구절이 나오네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100018994.1.16",
          "form": "유명한 시인 시집이라서 그런진 몰라도",
          "original_form": "유명한 시인 시집이라서 그런진 몰라도",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100018994.1.17",
          "form": "저는 유명한 시 많이 보여서 생각날 때 몇 번 펼쳐보기도 해요",
          "original_form": "저는 유명한 시 많이 보여서 생각날 때 몇 번 펼쳐보기도 해요",
          "speaker_id": "2",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100018994.1.18",
          "form": "요즘 도서관이 하는지 모르겠지만 ㅠㅠ",
          "original_form": "요즘 도서관이 하는지 모르겠지만 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100018994.1.19",
          "form": "기억해두고 가서 봐야겠어요!",
          "original_form": "기억해두고 가서 봐야겠어요!",
          "speaker_id": "1",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100018994.1.20",
          "form": "가을이랑 잘 어울리는 시집이라서 추천드릴게요~",
          "original_form": "가을이랑 잘 어울리는 시집이라서 추천드릴게요~",
          "speaker_id": "2",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100018994.1.21",
          "form": "추천 감사해요 ㅎㅎ 가을에는 시집인거같아요 !",
          "original_form": "추천 감사해요 ㅎㅎ 가을에는 시집인거같아요 !",
          "speaker_id": "1",
          "time": "20210827 15:04"
        }
      ]
    }
