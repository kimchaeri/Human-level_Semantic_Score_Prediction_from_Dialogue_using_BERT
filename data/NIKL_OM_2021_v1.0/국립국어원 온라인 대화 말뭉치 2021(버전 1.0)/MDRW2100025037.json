      "utterance": [
        {
          "id": "MDRW2100025037.1.1",
          "form": "밖에 비와요",
          "original_form": "밖에 비와요",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025037.1.2",
          "form": "맞아요ㅜㅜ 밖에 비가 와요",
          "original_form": "맞아요ㅜㅜ 밖에 비가 와요",
          "speaker_id": "1",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025037.1.3",
          "form": "오늘도 운전하기 힘드셨겠어요",
          "original_form": "오늘도 운전하기 힘드셨겠어요",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025037.1.4",
          "form": "네 엄청막혀서ㅜㅜ",
          "original_form": "네 엄청막혀서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025037.1.5",
          "form": "여름엔 안오고",
          "original_form": "여름엔 안오고",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025037.1.6",
          "form": "날좋을 가을에 맨날 비라니",
          "original_form": "날좋을 가을에 맨날 비라니",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025037.1.7",
          "form": "장롱면허 좀 탈출하나 했더니",
          "original_form": "장롱면허 좀 탈출하나 했더니",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025037.1.8",
          "form": "비가 이리 오네요 허허 ㅜㅜ",
          "original_form": "비가 이리 오네요 허허 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025037.1.9",
          "form": "ㅋㅋㅋ 장롱면허에요?",
          "original_form": "ㅋㅋㅋ 장롱면허에요?",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025037.1.10",
          "form": "면허 언제 따셨나요?",
          "original_form": "면허 언제 따셨나요?",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025037.1.11",
          "form": "23살인가 24살인가 ㅋㅋ\n기억도 가물가물하네요 ㅋㅋ",
          "original_form": "23살인가 24살인가 ㅋㅋ\n기억도 가물가물하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025037.1.12",
          "form": "비오는날 갬성 터지게\n음악 들으며 드라이브 하고싶은데",
          "original_form": "비오는날 갬성 터지게\n음악 들으며 드라이브 하고싶은데",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025037.1.13",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100025037.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025037.1.15",
          "form": "늦지 않았어요",
          "original_form": "늦지 않았어요",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025037.1.16",
          "form": "지금이라도 남편분에게",
          "original_form": "지금이라도 남편분에게",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025037.1.17",
          "form": "배우세요",
          "original_form": "배우세요",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025037.1.18",
          "form": "ㅋㅋㅋㅋㅋ저희 이미\n몇 번 도장 찍을뻔 했어여ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ저희 이미\n몇 번 도장 찍을뻔 했어여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:23"
        },
        {
          "id": "MDRW2100025037.1.19",
          "form": "비만 안오면\n혼자 끌고 나가서\n연습하는게 더 좋아요 ㅋ",
          "original_form": "비만 안오면\n혼자 끌고 나가서\n연습하는게 더 좋아요 ㅋ",
          "speaker_id": "1",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100025037.1.20",
          "form": "앗....",
          "original_form": "앗....",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025037.1.21",
          "form": "셀프연수?",
          "original_form": "셀프연수?",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025037.1.22",
          "form": "부부간에 운전연수는 금기사항이군여",
          "original_form": "부부간에 운전연수는 금기사항이군여",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025037.1.23",
          "form": "네ㅋㅋ 금기입니다!\n겨울되기전에 가을에\n이 숙제를 끝내고야 말겠어요!",
          "original_form": "네ㅋㅋ 금기입니다!\n겨울되기전에 가을에\n이 숙제를 끝내고야 말겠어요!",
          "speaker_id": "1",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100025037.1.24",
          "form": "전 군대서 욕먹으며 갈굼당하며 배워서 잘 써먹고 있죠 ㅋㅋㅋ",
          "original_form": "전 군대서 욕먹으며 갈굼당하며 배워서 잘 써먹고 있죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100025037.1.25",
          "form": "ㅋㅋ저도 갈굼이 필요한걸까요ㅜㅜ",
          "original_form": "ㅋㅋ저도 갈굼이 필요한걸까요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100025037.1.26",
          "form": "드림카타고 가을엔\n혼자 드라이브 갈거에여!",
          "original_form": "드림카타고 가을엔\n혼자 드라이브 갈거에여!",
          "speaker_id": "1",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100025037.1.27",
          "form": "혼자 어디가시게요?",
          "original_form": "혼자 어디가시게요?",
          "speaker_id": "2",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025037.1.28",
          "form": "차에서라도\n가을을 만끽해야디ㅋㅋ",
          "original_form": "차에서라도\n가을을 만끽해야디ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025037.1.29",
          "form": "그럴라면 어서 연습하세여",
          "original_form": "그럴라면 어서 연습하세여",
          "speaker_id": "2",
          "time": "20210831 23:36"
        }
      ]
    }
  ]
