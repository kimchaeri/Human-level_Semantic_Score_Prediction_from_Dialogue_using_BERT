      "utterance": [
        {
          "id": "MDRW2100035627.1.1",
          "form": "혹시 여행 다녀와보신곳",
          "original_form": "혹시 여행 다녀와보신곳",
          "speaker_id": "1",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100035627.1.2",
          "form": "있으세여?",
          "original_form": "있으세여?",
          "speaker_id": "1",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100035627.1.3",
          "form": "저는 경주나 전주?",
          "original_form": "저는 경주나 전주?",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100035627.1.4",
          "form": "안간지 넘 오래죠ㅠ name1님은요?",
          "original_form": "안간지 넘 오래죠ㅠ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100035627.1.5",
          "form": "오오 경주는 저도 수학여행때",
          "original_form": "오오 경주는 저도 수학여행때",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100035627.1.6",
          "form": "가본적있어용ㅎㅎ",
          "original_form": "가본적있어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100035627.1.7",
          "form": "ㅋㅋ좋아요",
          "original_form": "ㅋㅋ좋아요",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100035627.1.8",
          "form": "저 3년전에 일본다녀왔어용!",
          "original_form": "저 3년전에 일본다녀왔어용!",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100035627.1.9",
          "form": "스타벅스가 기왓집이에요",
          "original_form": "스타벅스가 기왓집이에요",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100035627.1.10",
          "form": "엇 마자요",
          "original_form": "엇 마자요",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100035627.1.11",
          "form": "오 저도 한 4년전쯤",
          "original_form": "오 저도 한 4년전쯤",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100035627.1.12",
          "form": "그런곳도 있구ㅋㅋㅋㅋ",
          "original_form": "그런곳도 있구ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100035627.1.13",
          "form": "일본다녀왔어요",
          "original_form": "일본다녀왔어요",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100035627.1.14",
          "form": "어디 가셨어용?",
          "original_form": "어디 가셨어용?",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100035627.1.15",
          "form": "교토..?",
          "original_form": "교토..?",
          "speaker_id": "2",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100035627.1.16",
          "form": "도쿄??",
          "original_form": "도쿄??",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100035627.1.17",
          "form": "아아!",
          "original_form": "아아!",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100035627.1.18",
          "form": "도쿄는 완전 도시이죠?",
          "original_form": "도쿄는 완전 도시이죠?",
          "speaker_id": "2",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100035627.1.19",
          "form": "저도 거기 갔어요",
          "original_form": "저도 거기 갔어요",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100035627.1.20",
          "form": "넹넹 물가 완전 비싸구",
          "original_form": "넹넹 물가 완전 비싸구",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100035627.1.21",
          "form": "그쳐ㅋㅋㅋ",
          "original_form": "그쳐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100035627.1.22",
          "form": "가장 번화한 서울 중심지에 있는 그런 느낌ㅋㅋㅋㅋ",
          "original_form": "가장 번화한 서울 중심지에 있는 그런 느낌ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:31"
        }
      ]
