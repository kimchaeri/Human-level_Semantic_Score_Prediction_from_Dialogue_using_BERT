      "utterance": [
        {
          "id": "MDRW2100036674.1.1",
          "form": "그래서 와플팬은 정말 신중히 구매하세요",
          "original_form": "그래서 와플팬은 정말 신중히 구매하세요",
          "speaker_id": "1",
          "time": "20210831 07:38"
        },
        {
          "id": "MDRW2100036674.1.2",
          "form": "에어프라이어는 있으세요?",
          "original_form": "에어프라이어는 있으세요?",
          "speaker_id": "1",
          "time": "20210831 07:38"
        },
        {
          "id": "MDRW2100036674.1.3",
          "form": "ㅎㅎ 와플팬은 있어요",
          "original_form": "ㅎㅎ 와플팬은 있어요",
          "speaker_id": "2",
          "time": "20210831 07:38"
        },
        {
          "id": "MDRW2100036674.1.4",
          "form": "네네 둘다있는데 워낙 요알못이라",
          "original_form": "네네 둘다있는데 워낙 요알못이라",
          "speaker_id": "2",
          "time": "20210831 07:38"
        },
        {
          "id": "MDRW2100036674.1.5",
          "form": "그냥 굴리네요ㅜ",
          "original_form": "그냥 굴리네요ㅜ",
          "speaker_id": "2",
          "time": "20210831 07:38"
        },
        {
          "id": "MDRW2100036674.1.6",
          "form": "유튜브서 인절미가 와플팬에 다 늘어붙고 실패한걸 봐서요",
          "original_form": "유튜브서 인절미가 와플팬에 다 늘어붙고 실패한걸 봐서요",
          "speaker_id": "2",
          "time": "20210831 07:39"
        },
        {
          "id": "MDRW2100036674.1.7",
          "form": "성공하는 요령이 있나요",
          "original_form": "성공하는 요령이 있나요",
          "speaker_id": "2",
          "time": "20210831 07:39"
        },
        {
          "id": "MDRW2100036674.1.8",
          "form": "아~ 둘다 있구나",
          "original_form": "아~ 둘다 있구나",
          "speaker_id": "1",
          "time": "20210831 08:24"
        },
        {
          "id": "MDRW2100036674.1.9",
          "form": "그 두개가 살찌는 지름길인데 ㅋㅋ",
          "original_form": "그 두개가 살찌는 지름길인데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 08:25"
        },
        {
          "id": "MDRW2100036674.1.10",
          "form": "일단 살짝 기름은 발라봐요 눌러붙지 않게",
          "original_form": "일단 살짝 기름은 발라봐요 눌러붙지 않게",
          "speaker_id": "1",
          "time": "20210831 08:25"
        },
        {
          "id": "MDRW2100036674.1.11",
          "form": "기름이 요령이군요!",
          "original_form": "기름이 요령이군요!",
          "speaker_id": "2",
          "time": "20210831 08:31"
        },
        {
          "id": "MDRW2100036674.1.12",
          "form": "ㅎㅎ 그럼 이제 인절미만 사옴 되겠어요",
          "original_form": "ㅎㅎ 그럼 이제 인절미만 사옴 되겠어요",
          "speaker_id": "2",
          "time": "20210831 08:32"
        },
        {
          "id": "MDRW2100036674.1.13",
          "form": "ㅎㅎㅎ 도전성공하길 빌어요",
          "original_form": "ㅎㅎㅎ 도전성공하길 빌어요",
          "speaker_id": "1",
          "time": "20210831 08:42"
        },
        {
          "id": "MDRW2100036674.1.14",
          "form": "에프는 주로 냉동식품 돌려먹잖아요",
          "original_form": "에프는 주로 냉동식품 돌려먹잖아요",
          "speaker_id": "1",
          "time": "20210831 08:42"
        },
        {
          "id": "MDRW2100036674.1.15",
          "form": "성공한 음식 있나요?",
          "original_form": "성공한 음식 있나요?",
          "speaker_id": "1",
          "time": "20210831 08:42"
        },
        {
          "id": "MDRW2100036674.1.16",
          "form": "거의 사용을 안해서..",
          "original_form": "거의 사용을 안해서..",
          "speaker_id": "2",
          "time": "20210831 08:43"
        },
        {
          "id": "MDRW2100036674.1.17",
          "form": "김말이 에프에 돌려먹었는데 괜찮더라고요",
          "original_form": "김말이 에프에 돌려먹었는데 괜찮더라고요",
          "speaker_id": "2",
          "time": "20210831 08:43"
        },
        {
          "id": "MDRW2100036674.1.18",
          "form": "아~ 김말이 진짜 좋아하는데 ㅎ",
          "original_form": "아~ 김말이 진짜 좋아하는데 ㅎ",
          "speaker_id": "1",
          "time": "20210831 08:44"
        },
        {
          "id": "MDRW2100036674.1.19",
          "form": "전 찹쌀탕수육 돌려먹으니까 맛있더라구요",
          "original_form": "전 찹쌀탕수육 돌려먹으니까 맛있더라구요",
          "speaker_id": "1",
          "time": "20210831 08:44"
        },
        {
          "id": "MDRW2100036674.1.20",
          "form": "찹쌀탕수육 어디거 사셨어요?",
          "original_form": "찹쌀탕수육 어디거 사셨어요?",
          "speaker_id": "2",
          "time": "20210831 08:45"
        },
        {
          "id": "MDRW2100036674.1.21",
          "form": "롯데몰이요",
          "original_form": "롯데몰이요",
          "speaker_id": "1",
          "time": "20210831 08:51"
        },
        {
          "id": "MDRW2100036674.1.22",
          "form": "검색하니 여러브랜드가 나오네요",
          "original_form": "검색하니 여러브랜드가 나오네요",
          "speaker_id": "2",
          "time": "20210831 08:52"
        },
        {
          "id": "MDRW2100036674.1.23",
          "form": "다맛있어 보여 고민중ㅎㅎ",
          "original_form": "다맛있어 보여 고민중ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 08:53"
        },
        {
          "id": "MDRW2100036674.1.24",
          "form": "ㅎㅎ 냉동식품은 다 맛있죠 고칼로리 잖아요 ㅋㅋ",
          "original_form": "ㅎㅎ 냉동식품은 다 맛있죠 고칼로리 잖아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 08:57"
        },
        {
          "id": "MDRW2100036674.1.25",
          "form": "정말 맛은 살과 등가교환이네요ㅎ",
          "original_form": "정말 맛은 살과 등가교환이네요ㅎ",
          "speaker_id": "2",
          "time": "20210831 08:58"
        },
        {
          "id": "MDRW2100036674.1.26",
          "form": "너무 슬퍼요",
          "original_form": "너무 슬퍼요",
          "speaker_id": "1",
          "time": "20210831 09:01"
        },
        {
          "id": "MDRW2100036674.1.27",
          "form": "먹을수록 근육이 생겼으면 좋겠어요",
          "original_form": "먹을수록 근육이 생겼으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210831 09:01"
        }
      ]
    }
