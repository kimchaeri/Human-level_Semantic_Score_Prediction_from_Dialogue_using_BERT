      "utterance": [
        {
          "id": "MDRW2100006560.1.1",
          "form": "구매하고픈 가전제품 있나요",
          "original_form": "구매하고픈 가전제품 있나요",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100006560.1.2",
          "form": "구매하고픈.가전 있나요?",
          "original_form": "구매하고픈.가전 있나요?",
          "speaker_id": "1",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100006560.1.3",
          "form": "스타일러요!",
          "original_form": "스타일러요!",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100006560.1.4",
          "form": "오 저두요...ㅎㅎ",
          "original_form": "오 저두요...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100006560.1.5",
          "form": "처음엔 다들 떨떠름 햿는데",
          "original_form": "처음엔 다들 떨떠름 햿는데",
          "speaker_id": "2",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100006560.1.6",
          "form": "써본 사람들 평이 생각보다 괜찮더라구요",
          "original_form": "써본 사람들 평이 생각보다 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100006560.1.7",
          "form": "요즘은 신혼 필수 가전됐더라구요 ㅎ",
          "original_form": "요즘은 신혼 필수 가전됐더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100006560.1.8",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100006560.1.9",
          "form": "하기야 가격이 세서 혼수로 들일만 한거같아요",
          "original_form": "하기야 가격이 세서 혼수로 들일만 한거같아요",
          "speaker_id": "2",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100006560.1.10",
          "form": "네 살면서 구입하긴 쉽지가 않네요",
          "original_form": "네 살면서 구입하긴 쉽지가 않네요",
          "speaker_id": "1",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100006560.1.11",
          "form": "그쵸 그래서 저도 위시리스트에만 넣어두고 잇어요",
          "original_form": "그쵸 그래서 저도 위시리스트에만 넣어두고 잇어요",
          "speaker_id": "2",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100006560.1.12",
          "form": "다른건 더 없나요?",
          "original_form": "다른건 더 없나요?",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100006560.1.13",
          "form": "네 전 딱히 생각나는게 없네요",
          "original_form": "네 전 딱히 생각나는게 없네요",
          "speaker_id": "2",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100006560.1.14",
          "form": "사고싶은 가전 있으신가오?",
          "original_form": "사고싶은 가전 있으신가오?",
          "speaker_id": "2",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100006560.1.15",
          "form": "와이프가 다이슨에어랩을 원합니다",
          "original_form": "와이프가 다이슨에어랩을 원합니다",
          "speaker_id": "1",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100006560.1.16",
          "form": "아 그거 얘기 들어봤어요",
          "original_form": "아 그거 얘기 들어봤어요",
          "speaker_id": "2",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100006560.1.17",
          "form": "많이등 사용하나봐요 주변에사",
          "original_form": "많이등 사용하나봐요 주변에사",
          "speaker_id": "1",
          "time": "20210908 02:31"
        }
