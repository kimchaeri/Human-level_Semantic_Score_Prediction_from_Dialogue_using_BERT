      "utterance": [
        {
          "id": "MDRW2100047464.1.1",
          "form": "슬슬 가을옷도 사야되는데 사셨나요 ㅎㅎ",
          "original_form": "슬슬 가을옷도 사야되는데 사셨나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100047464.1.2",
          "form": "요새 어디 잘안나가서",
          "original_form": "요새 어디 잘안나가서",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100047464.1.3",
          "form": "안샀어요",
          "original_form": "안샀어요",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100047464.1.4",
          "form": "근데 티셔츠 좀 사려고요",
          "original_form": "근데 티셔츠 좀 사려고요",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100047464.1.5",
          "form": "사셨어요?",
          "original_form": "사셨어요?",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100047464.1.6",
          "form": "전 아직 셔핑 못했어요",
          "original_form": "전 아직 셔핑 못했어요",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100047464.1.7",
          "form": "쇼핑",
          "original_form": "쇼핑",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100047464.1.8",
          "form": "저는 맨투맨이랑 후드티 샀어요 ㅎㅎ",
          "original_form": "저는 맨투맨이랑 후드티 샀어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100047464.1.9",
          "form": "가을엔 필수죠 ㅎㅎ",
          "original_form": "가을엔 필수죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100047464.1.10",
          "form": "잘사셨네요 ㅎㅎ 맨투맨 후드 편하고 좋아요",
          "original_form": "잘사셨네요 ㅎㅎ 맨투맨 후드 편하고 좋아요",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100047464.1.11",
          "form": "저도 맨투맨 잘입어요ㅎㅎ",
          "original_form": "저도 맨투맨 잘입어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100047464.1.12",
          "form": "네 맨투맨이 편해서 잘 입어요",
          "original_form": "네 맨투맨이 편해서 잘 입어요",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100047464.1.13",
          "form": "흰티 안에 입고 입으면 또 이쁘더라고요 ㅋㅋ",
          "original_form": "흰티 안에 입고 입으면 또 이쁘더라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100047464.1.14",
          "form": "맞아요 전 귀찮아서 알지만 하나만 입네요",
          "original_form": "맞아요 전 귀찮아서 알지만 하나만 입네요",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100047464.1.15",
          "form": "ㅎㅎ 즐겨입는 브랜드 있으신가요 좋아하는 브랜드나",
          "original_form": "ㅎㅎ 즐겨입는 브랜드 있으신가요 좋아하는 브랜드나",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100047464.1.16",
          "form": "전 딱히 없어요",
          "original_form": "전 딱히 없어요",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100047464.1.17",
          "form": "그때 그때 사고",
          "original_form": "그때 그때 사고",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100047464.1.18",
          "form": "특히 특사를 좋아해요",
          "original_form": "특히 특사를 좋아해요",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100047464.1.19",
          "form": "선호 브랜드 있으시죠?",
          "original_form": "선호 브랜드 있으시죠?",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100047464.1.20",
          "form": "특가를 좋아해요 ㅋㅋ",
          "original_form": "특가를 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100047464.1.21",
          "form": "저는 그냥 스파브랜드 돌려입어요 ㅋㅋㅋ 저도 따로 없는거 같은 ㅋㅋㅋ",
          "original_form": "저는 그냥 스파브랜드 돌려입어요 ㅋㅋㅋ 저도 따로 없는거 같은 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100047464.1.22",
          "form": "세일가가 정가 느낌 ㅋㅋㅋ",
          "original_form": "세일가가 정가 느낌 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100047464.1.23",
          "form": "아 그렇죠 ㅎㅎ저도 그래요",
          "original_form": "아 그렇죠 ㅎㅎ저도 그래요",
          "speaker_id": "2",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100047464.1.24",
          "form": "항시보면 세일가 가 맨날 그가격이고 정가 같아요 ㅎㅎ",
          "original_form": "항시보면 세일가 가 맨날 그가격이고 정가 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100047464.1.25",
          "form": "그니까요 ㅋㅋㅋ 그래서 세일가를 잘 봐둬야겠더라고요.. 거의 막 반값에 팔고 그러니까.. ㅎㅎ",
          "original_form": "그니까요 ㅋㅋㅋ 그래서 세일가를 잘 봐둬야겠더라고요.. 거의 막 반값에 팔고 그러니까.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100047464.1.26",
          "form": "저도 맨날 보다가 딱하면  사요 ㅎㅎ",
          "original_form": "저도 맨날 보다가 딱하면  사요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100047464.1.27",
          "form": "자주 들여다봐요 ㅋㅋ",
          "original_form": "자주 들여다봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100047464.1.28",
          "form": "쇼핑하실때 주저없이 사시나요? 전 결정장애라 엄청 오래걸려요 ㅋㅋㅋ",
          "original_form": "쇼핑하실때 주저없이 사시나요? 전 결정장애라 엄청 오래걸려요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100047464.1.29",
          "form": "막싼건 그냥 확사고",
          "original_form": "막싼건 그냥 확사고",
          "speaker_id": "2",
          "time": "20210828 12:55"
        },
        {
          "id": "MDRW2100047464.1.30",
          "form": "세일인하면 며칠 ㅡㅡ더 지켜보다 품절돼요",
          "original_form": "세일인하면 며칠 ㅡㅡ더 지켜보다 품절돼요",
          "speaker_id": "2",
          "time": "20210828 12:55"
        },
        {
          "id": "MDRW2100047464.1.31",
          "form": "ㅋㅋㅋㅋ 맞아요 그런거 득템 잘하는 사람들 보면 신기해요",
          "original_form": "ㅋㅋㅋㅋ 맞아요 그런거 득템 잘하는 사람들 보면 신기해요",
          "speaker_id": "1",
          "time": "20210828 12:56"
        },
        {
          "id": "MDRW2100047464.1.32",
