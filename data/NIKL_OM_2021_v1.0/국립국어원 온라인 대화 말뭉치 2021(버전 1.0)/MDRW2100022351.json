      "utterance": [
        {
          "id": "MDRW2100022351.1.1",
          "form": "고양이는 오래키우셧어요?",
          "original_form": "고양이는 오래키우셧어요?",
          "speaker_id": "1",
          "time": "20210829 12:56"
        },
        {
          "id": "MDRW2100022351.1.2",
          "form": "이제 3년됐어요!",
          "original_form": "이제 3년됐어요!",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022351.1.3",
          "form": "3살이에용",
          "original_form": "3살이에용",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022351.1.4",
          "form": "무슨 색이에요? 단모종?",
          "original_form": "무슨 색이에요? 단모종?",
          "speaker_id": "1",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100022351.1.5",
          "form": "백호에요!",
          "original_form": "백호에요!",
          "speaker_id": "2",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022351.1.6",
          "form": "넹 단모요!",
          "original_form": "넹 단모요!",
          "speaker_id": "2",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022351.1.7",
          "form": "호랑이 무늬요? ㅋㅋㅋ",
          "original_form": "호랑이 무늬요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022351.1.8",
          "form": "네!ㅋㅋㅋ",
          "original_form": "네!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022351.1.9",
          "form": "진짜 엄청 예뻐요",
          "original_form": "진짜 엄청 예뻐요",
          "speaker_id": "2",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022351.1.10",
          "form": "무슨 종이에여? 벵가리? 브리숏?",
          "original_form": "무슨 종이에여? 벵가리? 브리숏?",
          "speaker_id": "1",
          "time": "20210829 15:21"
        },
        {
          "id": "MDRW2100022351.1.11",
          "form": "아메숏이에요!",
          "original_form": "아메숏이에요!",
          "speaker_id": "2",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022351.1.12",
          "form": "사고는 많이 안쳐요?",
          "original_form": "사고는 많이 안쳐요?",
          "speaker_id": "1",
          "time": "20210829 15:26"
        },
        {
          "id": "MDRW2100022351.1.13",
          "form": "네~ 엄청 얌전해요",
          "original_form": "네~ 엄청 얌전해요",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022351.1.14",
          "form": "겁도 많고ㅋㅋ",
          "original_form": "겁도 많고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022351.1.15",
          "form": "사람 무서워하고요? ㅋㅋㅋ",
          "original_form": "사람 무서워하고요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022351.1.16",
          "form": "넹ㅋㅋ 근데 호기심은 또 많아서 지켜보거나 조심히 다가가요ㅋㅋㅋ",
          "original_form": "넹ㅋㅋ 근데 호기심은 또 많아서 지켜보거나 조심히 다가가요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022351.1.17",
          "form": "진짜 겁쟁이들은 완전 숨어서 나오지도 않던데..",
          "original_form": "진짜 겁쟁이들은 완전 숨어서 나오지도 않던데..",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022351.1.18",
          "form": "앗.. 그럼 소리에 겁이 많은걸로..",
          "original_form": "앗.. 그럼 소리에 겁이 많은걸로..",
          "speaker_id": "2",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022351.1.19",
          "form": "ㅋㅋ 친구네 고양이 1주일동안 밥주러 간적있는데..",
          "original_form": "ㅋㅋ 친구네 고양이 1주일동안 밥주러 간적있는데..",
          "speaker_id": "1",
          "time": "20210829 16:01"
        },
        {
          "id": "MDRW2100022351.1.20",
          "form": "마지막까지 데면데면.. ㅋㅋㅋ",
          "original_form": "마지막까지 데면데면.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:01"
        },
        {
          "id": "MDRW2100022351.1.21",
          "form": "헣ㅋㅋㅋㅋㅋ",
          "original_form": "헣ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100022351.1.22",
          "form": "밥주는데 데면데면이군요..",
          "original_form": "밥주는데 데면데면이군요..",
          "speaker_id": "2",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100022351.1.23",
          "form": "저희애들 겁 없었네요",
          "original_form": "저희애들 겁 없었네요",
          "speaker_id": "2",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100022351.1.24",
          "form": "애들? 여러마리에여?",
          "original_form": "애들? 여러마리에여?",
          "speaker_id": "1",
          "time": "20210829 16:04"
        },
        {
          "id": "MDRW2100022351.1.25",
          "form": "2마리에요!",
          "original_form": "2마리에요!",
          "speaker_id": "2",
          "time": "20210829 16:06"
        }
      ]
    }
