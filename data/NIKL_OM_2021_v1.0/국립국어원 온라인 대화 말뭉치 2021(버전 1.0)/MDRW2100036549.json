      "utterance": [
        {
          "id": "MDRW2100036549.1.1",
          "form": "오리좋아하세요?",
          "original_form": "오리좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 00:23"
        },
        {
          "id": "MDRW2100036549.1.2",
          "form": "안녕하세용 저 오리 좋아해요...!",
          "original_form": "안녕하세용 저 오리 좋아해요...!",
          "speaker_id": "2",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100036549.1.3",
          "form": "그런데 살아있는 오리를 좋아해요....ㅎㅎㅎ",
          "original_form": "그런데 살아있는 오리를 좋아해요....ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100036549.1.4",
          "form": "ㅠ아..그러시구나ㅠㅠㅠㅋㅋㅋㅋ 먹는건 뭐좋아하세요",
          "original_form": "ㅠ아..그러시구나ㅠㅠㅠㅋㅋㅋㅋ 먹는건 뭐좋아하세요",
          "speaker_id": "1",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100036549.1.5",
          "form": "저 꽃게탕 좋아해요! 요즘 철이 돌아와서 넘 기뻐요ㅋㅋㅋ",
          "original_form": "저 꽃게탕 좋아해요! 요즘 철이 돌아와서 넘 기뻐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100036549.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100036549.1.7",
          "form": "오 요즘이 철인가요??",
          "original_form": "오 요즘이 철인가요??",
          "speaker_id": "1",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100036549.1.8",
          "form": "네네 꽃게는 가을이 철이랍니당ㅎㅎ",
          "original_form": "네네 꽃게는 가을이 철이랍니당ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100036549.1.9",
          "form": "저는 다좋아합니다 꽃게도 좋아하구요 살꽉찬 꽃게 ㅠㅠㅠ",
          "original_form": "저는 다좋아합니다 꽃게도 좋아하구요 살꽉찬 꽃게 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100036549.1.10",
          "form": "쪄먹곳싶네요ㅠㅍㅍ",
          "original_form": "쪄먹곳싶네요ㅠㅍㅍ",
          "speaker_id": "1",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100036549.1.11",
          "form": "아 쪄서 먹어도 맛있고 탕도 맛있고ㅠㅠ",
          "original_form": "아 쪄서 먹어도 맛있고 탕도 맛있고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100036549.1.12",
          "form": "name1님은",
          "original_form": "&name1&님은",
          "speaker_id": "2",
          "time": "20210831 00:29"
        },
        {
          "id": "MDRW2100036549.1.13",
          "form": "오늘 이 아니구나 어제",
          "original_form": "오늘 이 아니구나 어제",
          "speaker_id": "2",
          "time": "20210831 00:29"
        },
        {
          "id": "MDRW2100036549.1.14",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210831 00:29"
        },
        {
          "id": "MDRW2100036549.1.15",
          "form": "저 청정원 아라비아따소스로 파스타 해먹었어용",
          "original_form": "저 청정원 아라비아따소스로 파스타 해먹었어용",
          "speaker_id": "1",
          "time": "20210831 00:29"
        },
        {
          "id": "MDRW2100036549.1.16",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210831 00:29"
        },
        {
          "id": "MDRW2100036549.1.17",
          "form": "오 시판 소스중에",
          "original_form": "오 시판 소스중에",
          "speaker_id": "2",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100036549.1.18",
          "form": "청정원 소스가 제일 맛있어요ㅋㅋㅋㅋ",
          "original_form": "청정원 소스가 제일 맛있어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100036549.1.19",
          "form": "저는 과일 거봉먹고",
          "original_form": "저는 과일 거봉먹고",
          "speaker_id": "2",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100036549.1.20",
          "form": "카레도 먹구요ㅋㅋㅋ",
          "original_form": "카레도 먹구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100036549.1.21",
          "form": "마자요 갠찮더라구용",
          "original_form": "마자요 갠찮더라구용",
          "speaker_id": "1",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100036549.1.22",
          "form": "카레 ㅋㅋㅋㅋ 젼 자취해서 그런건 안해먹는데 가끔 엄마카레 그립네용",
          "original_form": "카레 ㅋㅋㅋㅋ 젼 자취해서 그런건 안해먹는데 가끔 엄마카레 그립네용",
          "speaker_id": "1",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100036549.1.23",
          "form": "아하 자취하시면 무슨 요리를 가장 자주해드세요?",
          "original_form": "아하 자취하시면 무슨 요리를 가장 자주해드세요?",
          "speaker_id": "2",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100036549.1.24",
          "form": "시판소스 파스타요...걍 면삶으면 돼서 젤 간단 ㅠㅠㅠㅠㅋㅋㅋ",
          "original_form": "시판소스 파스타요...걍 면삶으면 돼서 젤 간단 ㅠㅠㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100036549.1.25",
          "form": "아 그쵸그쵸ㅋㅋㅋ",
          "original_form": "아 그쵸그쵸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100036549.1.26",
          "form": "그리고 삼겹살도 굽기만 하면되니까",
          "original_form": "그리고 삼겹살도 굽기만 하면되니까",
          "speaker_id": "2",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100036549.1.27",
          "form": "간단하구요",
          "original_form": "간단하구요",
          "speaker_id": "2",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100036549.1.28",
          "form": "ㅋㅋㅋㅋ 마자용 연기빼기귀찮아서 고기를 자주는 안구워욬ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 마자용 연기빼기귀찮아서 고기를 자주는 안구워욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100036549.1.29",
          "form": "환기가 잘 안돼요ㅠㅠㅠㅠ",
          "original_form": "환기가 잘 안돼요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100036549.1.30",
          "form": "앗 그건 그래요ㅠㅠ 특히 겨울에 먹으면",
          "original_form": "앗 그건 그래요ㅠㅠ 특히 겨울에 먹으면",
          "speaker_id": "2",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100036549.1.31",
          "form": "추워도 기다려야해요ㅋㅋㅋㅋ",
          "original_form": "추워도 기다려야해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:32"
        }
      ]
