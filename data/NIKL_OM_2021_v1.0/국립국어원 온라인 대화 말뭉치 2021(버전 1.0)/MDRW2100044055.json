      "utterance": [
        {
          "id": "MDRW2100044055.1.1",
          "form": "메밀전같은 느낌인가?",
          "original_form": "메밀전같은 느낌인가?",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044055.1.2",
          "form": "약간 비슷한데 좀 더 쫄깃 바삭",
          "original_form": "약간 비슷한데 좀 더 쫄깃 바삭",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044055.1.3",
          "form": "기름짐이 매우 느껴졌지만 한끼 정도 괜찮을맛",
          "original_form": "기름짐이 매우 느껴졌지만 한끼 정도 괜찮을맛",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044055.1.4",
          "form": "기회되면 먹어봐야겠다",
          "original_form": "기회되면 먹어봐야겠다",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044055.1.5",
          "form": "궁금해죽것음",
          "original_form": "궁금해죽것음",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044055.1.6",
          "form": "ㅋㅋㅋㅋ난 혹시 다시 가게되면",
          "original_form": "ㅋㅋㅋㅋ난 혹시 다시 가게되면",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044055.1.7",
          "form": "해물찜으로 시도해본다",
          "original_form": "해물찜으로 시도해본다",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044055.1.8",
          "form": "옆테이블이 넘 맛있게 먹었어ㅡㅡ",
          "original_form": "옆테이블이 넘 맛있게 먹었어ㅡㅡ",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044055.1.9",
          "form": "ㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044055.1.10",
          "form": "뺏어먹지",
          "original_form": "뺏어먹지",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044055.1.11",
          "form": "ㅋㅋㅋㅋㅋ눈으로는 뺏어먹었어 이미",
          "original_form": "ㅋㅋㅋㅋㅋ눈으로는 뺏어먹었어 이미",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044055.1.12",
          "form": "하도 쳐다봐서ㅋㅋㅋㅋㅋ",
          "original_form": "하도 쳐다봐서ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044055.1.13",
          "form": "거긴 밥 볶아주더라고",
          "original_form": "거긴 밥 볶아주더라고",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044055.1.14",
          "form": "ㅋㅋㅋㅋ아진짜ㅠ",
          "original_form": "ㅋㅋㅋㅋ아진짜ㅠ",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044055.1.15",
          "form": "나 밥먹었는데 배고파질라해",
          "original_form": "나 밥먹었는데 배고파질라해",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044055.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ먹을거 얘기하면 그렇지뭐ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ먹을거 얘기하면 그렇지뭐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044055.1.17",
          "form": "어우 소화제 하나 먹어야지",
          "original_form": "어우 소화제 하나 먹어야지",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044055.1.18",
          "form": "야무지게 먹었구나!",
          "original_form": "야무지게 먹었구나!",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044055.1.19",
          "form": "너무 먹었어 마리아퍼",
          "original_form": "너무 먹었어 마리아퍼",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044055.1.20",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044055.1.21",
          "form": "소화 잘 시키고 나서 누워야해",
          "original_form": "소화 잘 시키고 나서 누워야해",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044055.1.22",
          "form": "내 말이 그말이다 ㅉㅉㅋㅋㅋㅋ",
          "original_form": "내 말이 그말이다 ㅉㅉㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044055.1.23",
          "form": "거기 식당 신기한게 안에 카페도 다 만들어놨어 무료 카페처롬",
          "original_form": "거기 식당 신기한게 안에 카페도 다 만들어놨어 무료 카페처롬",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044055.1.24",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044055.1.25",
          "form": "요즘 식당들도 시설 참 잘해놔",
          "original_form": "요즘 식당들도 시설 참 잘해놔",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044055.1.26",
          "form": "응응 거긴 평점이 천오백개? 막 이래서ㅋㅋ",
          "original_form": "응응 거긴 평점이 천오백개? 막 이래서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044055.1.27",
          "form": "맏음직 스러웠음",
          "original_form": "맏음직 스러웠음",
          "speaker_id": "1",
          "time": "20210907 21:43"
        }
      ]
    }
