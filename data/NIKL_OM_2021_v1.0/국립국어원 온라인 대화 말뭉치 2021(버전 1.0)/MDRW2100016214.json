      "utterance": [
        {
          "id": "MDRW2100016214.1.1",
          "form": "드디어 여름의 끝입니다",
          "original_form": "드디어 여름의 끝입니다",
          "speaker_id": "2",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016214.1.2",
          "form": "그러게요 요즘 비가 많이와서 선선해졌네요",
          "original_form": "그러게요 요즘 비가 많이와서 선선해졌네요",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016214.1.3",
          "form": "비피해 없으셨어요?",
          "original_form": "비피해 없으셨어요?",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016214.1.4",
          "form": "저는 상관없엇어요 ㅎㅎ 그렇게 많이는 안왓던거같아용",
          "original_form": "저는 상관없엇어요 ㅎㅎ 그렇게 많이는 안왓던거같아용",
          "speaker_id": "2",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100016214.1.5",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016214.1.6",
          "form": "저희도 피해는 없는데",
          "original_form": "저희도 피해는 없는데",
          "speaker_id": "1",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016214.1.7",
          "form": "요즘 빨래말리기가  어려워요",
          "original_form": "요즘 빨래말리기가  어려워요",
          "speaker_id": "1",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100016214.1.8",
          "form": "저는 제습기사고 삶의질이 올라갔어요",
          "original_form": "저는 제습기사고 삶의질이 올라갔어요",
          "speaker_id": "2",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100016214.1.9",
          "form": "건조기는 이제 필수템이라 그래서",
          "original_form": "건조기는 이제 필수템이라 그래서",
          "speaker_id": "2",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100016214.1.10",
          "form": "살려구요",
          "original_form": "살려구요",
          "speaker_id": "2",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100016214.1.11",
          "form": "건조기 쓰는데 정말 편하긴한데 수건이나 싼옷은 괜찮은데",
          "original_form": "건조기 쓰는데 정말 편하긴한데 수건이나 싼옷은 괜찮은데",
          "speaker_id": "1",
          "time": "20210826 11:54"
        },
        {
          "id": "MDRW2100016214.1.12",
          "form": "좀 비싼거 돌리면 줄것같아서 걱정이에요",
          "original_form": "좀 비싼거 돌리면 줄것같아서 걱정이에요",
          "speaker_id": "1",
          "time": "20210826 11:54"
        },
        {
          "id": "MDRW2100016214.1.13",
          "form": "맞아요 옷은 줄어서 안된다 하더라구요",
          "original_form": "맞아요 옷은 줄어서 안된다 하더라구요",
          "speaker_id": "2",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016214.1.14",
          "form": "좋아하는 계절 잇으신가요?",
          "original_form": "좋아하는 계절 잇으신가요?",
          "speaker_id": "2",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016214.1.15",
          "form": "저는 가을 겨울이요 여릉은 더워서",
          "original_form": "저는 가을 겨울이요 여릉은 더워서",
          "speaker_id": "1",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100016214.1.16",
          "form": "저도 더위에 쥐약이라 겨울 좋아합니다",
          "original_form": "저도 더위에 쥐약이라 겨울 좋아합니다",
          "speaker_id": "2",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100016214.1.17",
          "form": "그리고 저는 가디건 좋아하는데",
          "original_form": "그리고 저는 가디건 좋아하는데",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016214.1.18",
          "form": "역시 가디건은 가을이랑 잘 어울려서",
          "original_form": "역시 가디건은 가을이랑 잘 어울려서",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016214.1.19",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016214.1.20",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016214.1.21",
          "form": "뭘아시네요",
          "original_form": "뭘아시네요",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100016214.1.22",
          "form": "요 며칠 날씨 좋았죠",
          "original_form": "요 며칠 날씨 좋았죠",
          "speaker_id": "1",
          "time": "20210907 08:46"
        },
        {
          "id": "MDRW2100016214.1.23",
          "form": "네 오늘은 흐리네요ㅜ",
          "original_form": "네 오늘은 흐리네요ㅜ",
