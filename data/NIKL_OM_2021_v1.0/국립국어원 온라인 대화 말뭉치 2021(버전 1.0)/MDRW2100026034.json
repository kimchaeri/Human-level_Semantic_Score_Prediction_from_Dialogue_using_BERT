        {
          "id": "MDRW2100026034.1.1",
          "form": "대리님은 일 잘 맞는 거 같으세여??",
          "original_form": "대리님은 일 잘 맞는 거 같으세여??",
          "speaker_id": "1",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100026034.1.2",
          "form": "저요? 사무직이 맞긴 한것같아요",
          "original_form": "저요? 사무직이 맞긴 한것같아요",
          "speaker_id": "2",
          "time": "20210901 19:05"
        },
        {
          "id": "MDRW2100026034.1.3",
          "form": "에너지있게 일하는 것보다요ㅋㅋ",
          "original_form": "에너지있게 일하는 것보다요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:06"
        },
        {
          "id": "MDRW2100026034.1.4",
          "form": "주임님은여",
          "original_form": "주임님은여",
          "speaker_id": "2",
          "time": "20210901 19:06"
        },
        {
          "id": "MDRW2100026034.1.5",
          "form": "그래도 그럭저럭 맞는 거 같긴 한데",
          "original_form": "그래도 그럭저럭 맞는 거 같긴 한데",
          "speaker_id": "1",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100026034.1.6",
          "form": "9-6아닌 직업 해보고싶어요 ㅋㅋ",
          "original_form": "9-6아닌 직업 해보고싶어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100026034.1.7",
          "form": "요즘 일 넘 많은데 다 반복작업? 이라",
          "original_form": "요즘 일 넘 많은데 다 반복작업? 이라",
          "speaker_id": "1",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100026034.1.8",
          "form": "지갸움ㅋㅋㅋ",
          "original_form": "지갸움ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100026034.1.9",
          "form": "ㅋㅋㅋㅋㅋ판매직할때랑 비교하면 어디가 더 낫나요?",
          "original_form": "ㅋㅋㅋㅋㅋ판매직할때랑 비교하면 어디가 더 낫나요?",
          "speaker_id": "2",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100026034.1.10",
          "form": "백번이해해요",
          "original_form": "백번이해해요",
          "speaker_id": "2",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100026034.1.11",
          "form": "사무직",
          "original_form": "사무직",
          "speaker_id": "1",
          "time": "20210901 19:14"
        },
        {
          "id": "MDRW2100026034.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:14"
        },
        {
          "id": "MDRW2100026034.1.13",
          "form": "인간스트레스 넘 힘들구",
          "original_form": "인간스트레스 넘 힘들구",
          "speaker_id": "1",
          "time": "20210901 19:14"
        },
        {
          "id": "MDRW2100026034.1.14",
          "form": "사무직이 더 좋은 거 같아요 그래도 ㅋㅋㅋㅋ",
          "original_form": "사무직이 더 좋은 거 같아요 그래도 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:14"
        },
        {
          "id": "MDRW2100026034.1.15",
          "form": "ㅋㅋㅋㅋㅋ미투",
          "original_form": "ㅋㅋㅋㅋㅋ미투",
          "speaker_id": "2",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026034.1.16",
          "form": "이러나저러나 스트레스받는거",
          "original_form": "이러나저러나 스트레스받는거",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026034.1.17",
          "form": "몸이라도 편히",
          "original_form": "몸이라도 편히",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026034.1.18",
          "form": "마자여 글구 남등 놀 때 놀고 쉴때 쉬는게 더 낫더라구요",
          "original_form": "마자여 글구 남등 놀 때 놀고 쉴때 쉬는게 더 낫더라구요",
          "speaker_id": "1",
          "time": "20210901 19:47"
        },
        {
          "id": "MDRW2100026034.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:47"
        },
        {
          "id": "MDRW2100026034.1.20",
          "form": "아 것도 있네여ㅋㅋㅋ",
          "original_form": "아 것도 있네여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:54"
        },
        {
          "id": "MDRW2100026034.1.21",
          "form": "주말에쉬는것도복임",
          "original_form": "주말에쉬는것도복임",
          "speaker_id": "2",
          "time": "20210901 19:54"
        },
        {
          "id": "MDRW2100026034.1.22",
          "form": "그쵸 ㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100026034.1.23",
          "form": "남들 쉴 때 일하면 좀 짱나더라고요ㅠㅠ",
          "original_form": "남들 쉴 때 일하면 좀 짱나더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100026034.1.24",
          "form": "맞아여 약속잡기도 힘들고요",
          "original_form": "맞아여 약속잡기도 힘들고요",
          "speaker_id": "2",
          "time": "20210901 20:08"
        },
        {
          "id": "MDRW2100026034.1.25",
          "form": "아 진짜 저 오늘 ㅋㅋ 근로장려금 신청하라구 날라옴",
          "original_form": "아 진짜 저 오늘 ㅋㅋ 근로장려금 신청하라구 날라옴",
          "speaker_id": "1",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100026034.1.26",
          "form": "대리님은 받으셨어요???",
          "original_form": "대리님은 받으셨어요???",
          "speaker_id": "1",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100026034.1.27",
          "form": "월급 쪼금 탔다구.. 나라에서 돈도 주고ㅠ",
          "original_form": "월급 쪼금 탔다구.. 나라에서 돈도 주고ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100026034.1.28",
          "form": "아녀 저는 뭐 온게 없어요ㅋㅋ작년에 언제취직했지",
          "original_form": "아녀 저는 뭐 온게 없어요ㅋㅋ작년에 언제취직했지",
          "speaker_id": "2",
          "time": "20210901 20:16"
        },
        {
          "id": "MDRW2100026034.1.29",
          "form": "9월부터일해서 안날라온건가",
          "original_form": "9월부터일해서 안날라온건가",
          "speaker_id": "2",
          "time": "20210901 20:17"
        },
        {
          "id": "MDRW2100026034.1.30",
          "form": "찾아봐야겠어여",
          "original_form": "찾아봐야겠어여",
          "speaker_id": "2",
          "time": "20210901 20:17"
        },
        {
          "id": "MDRW2100026034.1.31",
          "form": "전이거 신청해서 받아본적이 없어요.",
          "original_form": "전이거 신청해서 받아본적이 없어요.",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026034.1.32",
          "form": "그럼 담애 날아오겠다",
          "original_form": "그럼 담애 날아오겠다",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026034.1.33",
          "form": "올해 받은 급여 2000 넘으세여?",
          "original_form": "올해 받은 급여 2000 넘으세여?",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026034.1.34",
          "form": "올해 연봉ㅋㅋㅋ 이천 안돼야 자격이더라고요ㅠ",
          "original_form": "올해 연봉ㅋㅋㅋ 이천 안돼야 자격이더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026034.1.35",
          "form": "대리님 재산 없으실테니까",
          "original_form": "대리님 재산 없으실테니까",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026034.1.36",
          "form": "이상한데요",
          "original_form": "이상한데요",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026034.1.37",
          "form": "동생들도 백수인데",
          "original_form": "동생들도 백수인데",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026034.1.38",
          "form": "대상자가 아니래요",
          "original_form": "대상자가 아니래요",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026034.1.39",
          "form": "무슨기준인거지",
          "original_form": "무슨기준인거지",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026034.1.40",
          "form": "그 뭐지",
          "original_form": "그 뭐지",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026034.1.41",
          "form": "근로소득이.아예 0이어두 안 나옴여 ㅋㅋㅋ",
          "original_form": "근로소득이.아예 0이어두 안 나옴여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026034.1.42",
          "form": "가구원소득이 얼마이상이면 안나오는건가?",
          "original_form": "가구원소득이 얼마이상이면 안나오는건가?",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026034.1.43",
          "form": "셋째 완전백수임",
          "original_form": "셋째 완전백수임",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026034.1.44",
          "form": "알바도안함 심지어",
          "original_form": "알바도안함 심지어",
          "speaker_id": "2",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026034.1.45",
          "form": "이번년도 3월부터요",
          "original_form": "이번년도 3월부터요",
          "speaker_id": "2",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026034.1.46",
          "form": "그니까 근로소득이 조금은 잇어야돼요",
          "original_form": "그니까 근로소득이 조금은 잇어야돼요",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026034.1.47",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026034.1.48",
          "form": "글구 가구당 1명일걸요 신청가능한게",
          "original_form": "글구 가구당 1명일걸요 신청가능한게",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026034.1.49",
          "form": "아빠가 해야하나",
          "original_form": "아빠가 해야하나",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026034.1.50",
          "form": "아그래요?ㅋㅋㅋ",
          "original_form": "아그래요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026034.1.51",
          "form": "대상자조회에 해당안된다고하니깐 뭐 받을돈 못받는건 아니겠죠",
          "original_form": "대상자조회에 해당안된다고하니깐 뭐 받을돈 못받는건 아니겠죠",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026034.1.52",
          "form": "넹ㅋㅋㅋㅋ 거기 써 잇어요",
          "original_form": "넹ㅋㅋㅋㅋ 거기 써 잇어요",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026034.1.53",
          "form": "마자요.. 대상자면 요즘은 먼저 카톡으로 날아오더라구요",
          "original_form": "마자요.. 대상자면 요즘은 먼저 카톡으로 날아오더라구요",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026034.1.54",
          "form": "그쵸? 뭔가 그러네 ㅋㅋㅋㅋ괜히 억울",
          "original_form": "그쵸? 뭔가 그러네 ㅋㅋㅋㅋ괜히 억울",
          "speaker_id": "2",
