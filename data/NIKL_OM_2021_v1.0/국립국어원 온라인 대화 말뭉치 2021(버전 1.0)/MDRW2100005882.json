        },
        {
          "id": "MDRW2100005882.1.3",
          "form": "동영상보기나",
          "original_form": "동영상보기나",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005882.1.4",
          "form": "맞아요 ㅋㅋㅋ 폰같지가 않으요",
          "original_form": "맞아요 ㅋㅋㅋ 폰같지가 않으요",
          "speaker_id": "1",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100005882.1.5",
          "form": "펼치면 약간 아이패드 미니같은",
          "original_form": "펼치면 약간 아이패드 미니같은",
          "speaker_id": "1",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100005882.1.6",
          "form": "근데 배터리가 엄청 빨리 닳긴하더라구요",
          "original_form": "근데 배터리가 엄청 빨리 닳긴하더라구요",
          "speaker_id": "1",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100005882.1.7",
          "form": "아 그건 좀 별로네여",
          "original_form": "아 그건 좀 별로네여",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100005882.1.8",
          "form": "근데 디자인을 사는거죠",
          "original_form": "근데 디자인을 사는거죠",
          "speaker_id": "2",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100005882.1.9",
          "form": "남들의 부러움과 ㅋㅋㅋ",
          "original_form": "남들의 부러움과 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100005882.1.10",
          "form": "ㅋㅋㅋㅋ맞아요 그거 꺼내면",
          "original_form": "ㅋㅋㅋㅋ맞아요 그거 꺼내면",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100005882.1.11",
          "form": "다들 신기해해요",
          "original_form": "다들 신기해해요",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100005882.1.12",
          "form": "친구가 관종이라 또 행복해함 ㅋㅋㅋㅋ",
          "original_form": "친구가 관종이라 또 행복해함 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005882.1.13",
          "form": "이번에도 업글하는 사람부터 엄청 할듯요",
          "original_form": "이번에도 업글하는 사람부터 엄청 할듯요",
          "speaker_id": "2",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005882.1.14",
          "form": "저 어제 집가다가 맞은편 분 플립3 크림 쓰는거봣어요",
          "original_form": "저 어제 집가다가 맞은편 분 플립3 크림 쓰는거봣어요",
          "speaker_id": "1",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100005882.1.15",
          "form": "사전예약...하셨군요...",
          "original_form": "사전예약...하셨군요...",
          "speaker_id": "1",
          "time": "20210907 15:53"
        },
