      "utterance": [
        {
          "id": "MDRW2100008600.1.1",
          "form": "귀찮아서 온라인으로 주문도 했는데 무료배송비 채우려고 돈을 더 쓰는 것 같아서",
          "original_form": "귀찮아서 온라인으로 주문도 했는데 무료배송비 채우려고 돈을 더 쓰는 것 같아서",
          "speaker_id": "2",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008600.1.2",
          "form": "요즘엔 마트 가려고 해요",
          "original_form": "요즘엔 마트 가려고 해요",
          "speaker_id": "2",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008600.1.3",
          "form": "마트가면 이건 꼭 산다",
          "original_form": "마트가면 이건 꼭 산다",
          "speaker_id": "1",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008600.1.4",
          "form": "하는 거 있나요",
          "original_form": "하는 거 있나요",
          "speaker_id": "1",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008600.1.5",
          "form": "내 장바구니의 소울메이트다 하는거",
          "original_form": "내 장바구니의 소울메이트다 하는거",
          "speaker_id": "1",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008600.1.6",
          "form": "없는 것 같아요",
          "original_form": "없는 것 같아요",
          "speaker_id": "2",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008600.1.7",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "1",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008600.1.8",
          "form": "과일 할인하면 좀 사는 정도?",
          "original_form": "과일 할인하면 좀 사는 정도?",
          "speaker_id": "2",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008600.1.9",
          "form": "뭐 있으세요?",
          "original_form": "뭐 있으세요?",
          "speaker_id": "2",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008600.1.10",
          "form": "전 아무래도 먹거리자주사는거같아요",
          "original_form": "전 아무래도 먹거리자주사는거같아요",
          "speaker_id": "1",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008600.1.11",
          "form": "자주 드시는 것 있나요????",
          "original_form": "자주 드시는 것 있나요????",
          "speaker_id": "2",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008600.1.12",
          "form": "냉장고에 있으면 든든한 오리훈제 꼭 사요",
          "original_form": "냉장고에 있으면 든든한 오리훈제 꼭 사요",
          "speaker_id": "1",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008600.1.13",
          "form": "오리훈제 좋아하시나요",
          "original_form": "오리훈제 좋아하시나요",
          "speaker_id": "1",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008600.1.14",
          "form": "오 네네",
          "original_form": "오 네네",
          "speaker_id": "2",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008600.1.15",
          "form": "오리훈제 사면 머스타드도 꼭 같이사는데",
          "original_form": "오리훈제 사면 머스타드도 꼭 같이사는데",
          "speaker_id": "1",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008600.1.16",
          "form": "오리엔 머스타드가 진짜 잘 어울리죠",
          "original_form": "오리엔 머스타드가 진짜 잘 어울리죠",
          "speaker_id": "2",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008600.1.17",
          "form": "약간 외국꺼는 사지마세요",
          "original_form": "약간 외국꺼는 사지마세요",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008600.1.18",
          "form": "겁나 맛없어요",
          "original_form": "겁나 맛없어요",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008600.1.19",
          "form": "헐 머스타드 맛이 그렇게 달라요?????",
          "original_form": "헐 머스타드 맛이 그렇게 달라요?????",
          "speaker_id": "2",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008600.1.20",
          "form": "선호하는 제품 브랜드 있나요?",
          "original_form": "선호하는 제품 브랜드 있나요?",
          "speaker_id": "2",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008600.1.21",
          "form": "저는 오뚜기  선호해요",
          "original_form": "저는 오뚜기  선호해요",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008600.1.22",
          "form": "국내꺼는 달달한데 외국꺼중에 좀 하인즈말고",
          "original_form": "국내꺼는 달달한데 외국꺼중에 좀 하인즈말고",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008600.1.23",
          "form": "있거든요 약간 뚜껑뾰족한브랜드",
          "original_form": "있거든요 약간 뚜껑뾰족한브랜드",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008600.1.24",
          "form": "그거 겁나 맛없어요",
          "original_form": "그거 겁나 맛없어요",
          "speaker_id": "1",
          "time": "20210822 16:59"
        }
      ]
    }
