      "utterance": [
        {
          "id": "MDRW2100025883.1.1",
          "form": "치즈랑 피망이면 이제 다 오셨는데요?",
          "original_form": "치즈랑 피망이면 이제 다 오셨는데요?",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025883.1.2",
          "form": "빵이 들어간댔나요? 기억이 안 나브러",
          "original_form": "빵이 들어간댔나요? 기억이 안 나브러",
          "speaker_id": "2",
          "time": "20210901 17:42"
        },
        {
          "id": "MDRW2100025883.1.3",
          "form": "넵넵ㅋㅋㅋㅋㅋㅋㅋㅋㅋ접근성도 좋은 음식!",
          "original_form": "넵넵ㅋㅋㅋㅋㅋㅋㅋㅋㅋ접근성도 좋은 음식!",
          "speaker_id": "1",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025883.1.4",
          "form": "정답! 피자?",
          "original_form": "정답! 피자?",
          "speaker_id": "2",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025883.1.5",
          "form": "맞아요ㅋㅋㅋㅋㅋㅋ 쫌쉬우ㅏㅆ죠?",
          "original_form": "맞아요ㅋㅋㅋㅋㅋㅋ 쫌쉬우ㅏㅆ죠?",
          "speaker_id": "1",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100025883.1.6",
          "form": "오예~ ㅋㅋㅋㅋㅋ네! 이번엔 거뜬했습니다",
          "original_form": "오예~ ㅋㅋㅋㅋㅋ네! 이번엔 거뜬했습니다",
          "speaker_id": "2",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100025883.1.7",
          "form": "음식 얘기 혹시 아직 안 질리셨으면 제가 지금 먹고 있는 저녁 메뉴 맞혀보세요",
          "original_form": "음식 얘기 혹시 아직 안 질리셨으면 제가 지금 먹고 있는 저녁 메뉴 맞혀보세요",
          "speaker_id": "1",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100025883.1.8",
          "form": "ㅋㅋㅋ음식 얘기가 제일 바로바로 나누기 편해서 좋아요",
          "original_form": "ㅋㅋㅋ음식 얘기가 제일 바로바로 나누기 편해서 좋아요",
          "speaker_id": "2",
          "time": "20210901 17:45"
        },
        {
          "id": "MDRW2100025883.1.9",
          "form": "쌀 들어가나요?",
          "original_form": "쌀 들어가나요?",
          "speaker_id": "2",
          "time": "20210901 17:45"
        },
        {
          "id": "MDRW2100025883.1.10",
          "form": "흠 밥이랑 같이 먹는 음식이에요",
          "original_form": "흠 밥이랑 같이 먹는 음식이에요",
          "speaker_id": "1",
          "time": "20210901 17:46"
        },
        {
          "id": "MDRW2100025883.1.11",
          "form": "으으음 빨간색인가요?",
          "original_form": "으으음 빨간색인가요?",
          "speaker_id": "2",
          "time": "20210901 17:46"
        },
        {
          "id": "MDRW2100025883.1.12",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "1",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100025883.1.13",
          "form": "매운 음식 아닙니다",
          "original_form": "매운 음식 아닙니다",
          "speaker_id": "1",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100025883.1.14",
          "form": "일식인가요?",
          "original_form": "일식인가요?",
          "speaker_id": "2",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100025883.1.15",
          "form": "일식 아닙니다! ㅋㅋㅋㅋ",
          "original_form": "일식 아닙니다! ㅋㅋㅋㅋ",
          "speaker_id": "1",
