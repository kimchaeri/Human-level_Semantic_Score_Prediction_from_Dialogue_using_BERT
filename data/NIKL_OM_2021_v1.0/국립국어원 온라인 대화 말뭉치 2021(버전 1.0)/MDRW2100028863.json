      "utterance": [
        {
          "id": "MDRW2100028863.1.1",
          "form": "아 이 스 티",
          "original_form": "아 이 스 티",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028863.1.2",
          "form": "코 카 콜 라",
          "original_form": "코 카 콜 라",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028863.1.3",
          "form": "당신은?",
          "original_form": "당신은?",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028863.1.4",
          "form": "달콤한 거 좋아하는구나",
          "original_form": "달콤한 거 좋아하는구나",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028863.1.5",
          "form": "나는 아아",
          "original_form": "나는 아아",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028863.1.6",
          "form": "왜 아아 마시는지 물어봐줘",
          "original_form": "왜 아아 마시는지 물어봐줘",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028863.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ엨ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ엨ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028863.1.8",
          "form": "왜드심?",
          "original_form": "왜드심?",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028863.1.9",
          "form": "정확히는 마시게 된 이유가",
          "original_form": "정확히는 마시게 된 이유가",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028863.1.10",
          "form": "있어",
          "original_form": "있어",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028863.1.11",
          "form": "무엇인가요",
          "original_form": "무엇인가요",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028863.1.12",
          "form": "대학생 때 나는 돈이 없었지만 아침에 커피를 마시고 싶었지",
          "original_form": "대학생 때 나는 돈이 없었지만 아침에 커피를 마시고 싶었지",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028863.1.13",
          "form": "학교 건물에는 잉글리쉬 카페가 있었는데",
          "original_form": "학교 건물에는 잉글리쉬 카페가 있었는데",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028863.1.14",
          "form": "멋으로요?",
          "original_form": "멋으로요?",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028863.1.15",
          "form": "거기서 주문하면 아아가 1000원이엇어",
          "original_form": "거기서 주문하면 아아가 1000원이엇어",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028863.1.16",
          "form": "나는 그 쓴맛을",
          "original_form": "나는 그 쓴맛을",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028863.1.17",
          "form": "거기서 마시면서 적응해나갔지",
          "original_form": "거기서 마시면서 적응해나갔지",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028863.1.18",
          "form": "하루는 어쩌구저쩌구 플리즈했는데",
          "original_form": "하루는 어쩌구저쩌구 플리즈했는데",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028863.1.19",
          "form": "플리즈",
          "original_form": "플리즈",
          "speaker_id": "2",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028863.1.20",
          "form": "원래 핫 아이스 물어보는데 안물어봐서",
          "original_form": "원래 핫 아이스 물어보는데 안물어봐서",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028863.1.21",
          "form": "오잉했는데",
          "original_form": "오잉했는데",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028863.1.22",
          "form": "종이컵에 담아주는 거야 핫커피로",
          "original_form": "종이컵에 담아주는 거야 핫커피로",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028863.1.23",
          "form": "그래서 으잉 라고",
          "original_form": "그래서 으잉 라고",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028863.1.24",
          "form": "했다가 결제도 하는데 반값해주더라고",
          "original_form": "했다가 결제도 하는데 반값해주더라고",
          "speaker_id": "1",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028863.1.25",
          "form": "아 오늘 뭔 이벤트다 하고 받았는데",
          "original_form": "아 오늘 뭔 이벤트다 하고 받았는데",
          "speaker_id": "1",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028863.1.26",
          "form": "엄청 가벼워!",
          "original_form": "엄청 가벼워!",
          "speaker_id": "1",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028863.1.27",
          "form": "이유는... 내가 아메리카노 플리즈라고 안하고",
          "original_form": "이유는... 내가 아메리카노 플리즈라고 안하고",
          "speaker_id": "1",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028863.1.28",
          "form": "에스프레스 플리즈라고 햇던거지..",
          "original_form": "에스프레스 플리즈라고 햇던거지..",
          "speaker_id": "1",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028863.1.29",
          "form": "그때 에스프레소를 첨 알앗다",
          "original_form": "그때 에스프레소를 첨 알앗다",
          "speaker_id": "1",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028863.1.30",
          "form": "지옥의 맛인데요",
          "original_form": "지옥의 맛인데요",
          "speaker_id": "2",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028863.1.31",
          "form": "전 그때서야 비로소 어른이 되었죠",
          "original_form": "전 그때서야 비로소 어른이 되었죠",
          "speaker_id": "1",
          "time": "20210902 22:31"
        },
        {
          "id": "MDRW2100028863.1.32",
          "form": "그전까지는 커피를 모르는 애송이였군",
          "original_form": "그전까지는 커피를 모르는 애송이였군",
          "speaker_id": "2",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100028863.1.33",
          "form": "인생의 쓴맛을... 알게되어버린..",
          "original_form": "인생의 쓴맛을... 알게되어버린..",
          "speaker_id": "1",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028863.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋ너무 직접적인 맛이네",
          "original_form": "ㅋㅋㅋㅋㅋㅋ너무 직접적인 맛이네",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028863.1.35",
          "form": "점심 모먹어",
          "original_form": "점심 모먹어",
          "speaker_id": "1",
