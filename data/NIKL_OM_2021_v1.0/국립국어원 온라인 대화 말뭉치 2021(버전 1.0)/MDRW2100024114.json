      "utterance": [
        {
          "id": "MDRW2100024114.1.1",
          "form": "컴퓨터에 관심이 좀 많았어요",
          "original_form": "컴퓨터에 관심이 좀 많았어요",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024114.1.2",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024114.1.3",
          "form": "근데 화학쌤이 너무",
          "original_form": "근데 화학쌤이 너무",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024114.1.4",
          "form": "오~ 근데 그것도 프로그래밍에 관심을?",
          "original_form": "오~ 근데 그것도 프로그래밍에 관심을?",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024114.1.5",
          "form": "불꽃 색깔 설명하면서",
          "original_form": "불꽃 색깔 설명하면서",
          "speaker_id": "2",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100024114.1.6",
          "form": "ㅋㅋㅋ아 불꽃",
          "original_form": "ㅋㅋㅋ아 불꽃",
          "speaker_id": "1",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100024114.1.7",
          "form": "이 친구랑 이친구가 만나면 노란색 병아리가 되네~",
          "original_form": "이 친구랑 이친구가 만나면 노란색 병아리가 되네~",
          "speaker_id": "2",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100024114.1.8",
          "form": "이런 설명으러 하셔서",
          "original_form": "이런 설명으러 하셔서",
          "speaker_id": "2",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100024114.1.9",
          "form": "불꽃 진짴ㅋㅋㅋ 웃긴다 노란색 병아리",
          "original_form": "불꽃 진짴ㅋㅋㅋ 웃긴다 노란색 병아리",
          "speaker_id": "1",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100024114.1.10",
          "form": "귀여우신데요?",
          "original_form": "귀여우신데요?",
          "speaker_id": "1",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100024114.1.11",
          "form": "너무 짜증났어욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무 짜증났어욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100024114.1.12",
          "form": "노란색 병아리가 되네",
          "original_form": "노란색 병아리가 되네",
          "speaker_id": "2",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100024114.1.13",
          "form": "흰 옷을 입어요~",
          "original_form": "흰 옷을 입어요~",
          "speaker_id": "2",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100024114.1.14",
          "form": "지금 너무 웃겨서 웃는 중이에요",
          "original_form": "지금 너무 웃겨서 웃는 중이에요",
          "speaker_id": "1",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100024114.1.15",
          "form": "그 선생님 나름 기억에 남게 하려고 스토리 짜신거 같은뎈ㅋㅋ",
          "original_form": "그 선생님 나름 기억에 남게 하려고 스토리 짜신거 같은뎈ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100024114.1.16",
          "form": "붉게 타오르는 불꽃을 함께 구경하는 사이가 되았네~",
          "original_form": "붉게 타오르는 불꽃을 함께 구경하는 사이가 되았네~",
          "speaker_id": "2",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100024114.1.17",
          "form": "아니 무슨 스토리텔링하는데 아예 모르겠어요",
          "original_form": "아니 무슨 스토리텔링하는데 아예 모르겠어요",
          "speaker_id": "2",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100024114.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100024114.1.19",
          "form": "ㅋㅋㅋㅋㅋ지금 와서 생각해봐도 이해 안되는 교수법이셨나요..",
          "original_form": "ㅋㅋㅋㅋㅋ지금 와서 생각해봐도 이해 안되는 교수법이셨나요..",
          "speaker_id": "1",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100024114.1.20",
          "form": "넼ㅋㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100024114.1.21",
          "form": "괴짜선생님이셨ㄱ군ㅋㅋㅋ",
          "original_form": "괴짜선생님이셨ㄱ군ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100024114.1.22",
          "form": "발가락양말도 그땐 좀 맘에 안들고",
          "original_form": "발가락양말도 그땐 좀 맘에 안들고",
          "speaker_id": "2",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100024114.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100024114.1.24",
          "form": "아 ㅋㅋ 무좀양말",
          "original_form": "아 ㅋㅋ 무좀양말",
          "speaker_id": "1",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100024114.1.25",
          "form": "그냥 그런게 민감한 나이",
          "original_form": "그냥 그런게 민감한 나이",
          "speaker_id": "2",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100024114.1.26",
          "form": "그런 선생님들 종종 계시긴 했죠",
          "original_form": "그런 선생님들 종종 계시긴 했죠",
          "speaker_id": "1",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100024114.1.27",
          "form": "패션이 되게 난해하신 선생님 많았었는데",
          "original_form": "패션이 되게 난해하신 선생님 많았었는데",
          "speaker_id": "1",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100024114.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100024114.1.29",
          "form": "특히 한자선생님 꼭 개량한복입으심",
          "original_form": "특히 한자선생님 꼭 개량한복입으심",
          "speaker_id": "1",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100024114.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100024114.1.31",
          "form": "역사쌤 한자쌤",
          "original_form": "역사쌤 한자쌤",
          "speaker_id": "2",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100024114.1.32",
          "form": "그들만의 옷차림..",
          "original_form": "그들만의 옷차림..",
          "speaker_id": "1",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100024114.1.33",
          "form": "저 중학교도 자사중이여가지고",
          "original_form": "저 중학교도 자사중이여가지고",
