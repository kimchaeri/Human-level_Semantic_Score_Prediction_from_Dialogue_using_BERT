      "utterance": [
        {
          "id": "MDRW2100032804.1.1",
          "form": "아니 진짜로 너무하자나요",
          "original_form": "아니 진짜로 너무하자나요",
          "speaker_id": "2",
          "time": "20210905 23:54"
        },
        {
          "id": "MDRW2100032804.1.2",
          "form": "본진으로 정해버렸을 뿌닌데",
          "original_form": "본진으로 정해버렸을 뿌닌데",
          "speaker_id": "2",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032804.1.3",
          "form": "정신적 외도를 하면 안되고... 휴",
          "original_form": "정신적 외도를 하면 안되고... 휴",
          "speaker_id": "2",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032804.1.4",
          "form": "진짜 너무해요,,,ㄱ",
          "original_form": "진짜 너무해요,,,ㄱ",
          "speaker_id": "1",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032804.1.5",
          "form": "근데 또.... 하나의 몸안에 두개의 심장은 있을수업서..하면서",
          "original_form": "근데 또.... 하나의 몸안에 두개의 심장은 있을수업서..하면서",
          "speaker_id": "1",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032804.1.6",
          "form": "한놈만 파요..ㅋㅋ",
          "original_form": "한놈만 파요..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032804.1.7",
          "form": "여자라면 응당 남자 백명은 거느려야 하는데...",
          "original_form": "여자라면 응당 남자 백명은 거느려야 하는데...",
          "speaker_id": "2",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032804.1.8",
          "form": "그니까여ㅠ 최애가 머라고",
          "original_form": "그니까여ㅠ 최애가 머라고",
          "speaker_id": "2",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032804.1.9",
          "form": "에휴",
          "original_form": "에휴",
          "speaker_id": "2",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032804.1.10",
          "form": "진짜요 응당 이쁘장한 남자 백명은 거느려야 하는게 국룰이죠,,",
          "original_form": "진짜요 응당 이쁘장한 남자 백명은 거느려야 하는게 국룰이죠,,",
          "speaker_id": "1",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100032804.1.11",
          "form": "조신하구 참하고 어리고 잘생기고 잘하는...",
          "original_form": "조신하구 참하고 어리고 잘생기고 잘하는...",
          "speaker_id": "2",
          "time": "20210905 23:56"
        },
        {
          "id": "MDRW2100032804.1.12",
          "form": "아이돌...",
          "original_form": "아이돌...",
          "speaker_id": "2",
          "time": "20210905 23:56"
        },
        {
          "id": "MDRW2100032804.1.13",
          "form": "일것...",
          "original_form": "일것...",
          "speaker_id": "2",
          "time": "20210905 23:56"
        },
        {
          "id": "MDRW2100032804.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 완벽해요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 완벽해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:56"
        },
        {
          "id": "MDRW2100032804.1.15",
          "form": "진짜..... 근데 신기한게 본진있다고 다른남돌들 관심은 없어지더라구요,,,",
          "original_form": "진짜..... 근데 신기한게 본진있다고 다른남돌들 관심은 없어지더라구요,,,",
          "speaker_id": "1",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100032804.1.16",
          "form": "아 그쵸그쵸",
          "original_form": "아 그쵸그쵸",
          "speaker_id": "2",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100032804.1.17",
          "form": "본진 있으면 좀 사그라들어여",
          "original_form": "본진 있으면 좀 사그라들어여",
          "speaker_id": "2",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100032804.1.18",
          "form": "뭔 유사를 먹는 것도 아니고,, 걍 전 멀티가 안 되더라구여ㅋㅋㅋㅋㅋㅋ",
          "original_form": "뭔 유사를 먹는 것도 아니고,, 걍 전 멀티가 안 되더라구여ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100032804.1.19",
          "form": "진짜 사람맘이 너무 억울하다..",
          "original_form": "진짜 사람맘이 너무 억울하다..",
          "speaker_id": "1",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100032804.1.20",
          "form": "다 달달해 보이는데(?)",
          "original_form": "다 달달해 보이는데(?)",
          "speaker_id": "1",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100032804.1.21",
          "form": "그니까여",
          "original_form": "그니까여",
          "speaker_id": "2",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100032804.1.22",
          "form": "최애는 최애고",
          "original_form": "최애는 최애고",
          "speaker_id": "2",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100032804.1.23",
          "form": "차은우는 차은우라서",
          "original_form": "차은우는 차은우라서",
          "speaker_id": "2",
          "time": "20210905 23:58"
        },
        {
          "id": "MDRW2100032804.1.24",
          "form": "전 차은우도 좋아해요",
          "original_form": "전 차은우도 좋아해요",
          "speaker_id": "2",
          "time": "20210905 23:58"
        },
        {
          "id": "MDRW2100032804.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 차은웈ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 차은웈ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:58"
        },
        {
          "id": "MDRW2100032804.1.26",
          "form": "역시 아시는분이세요",
          "original_form": "역시 아시는분이세요",
          "speaker_id": "1",
          "time": "20210905 23:58"
        },
        {
          "id": "MDRW2100032804.1.27",
          "form": "최애있고 차애있고 은우있다",
          "original_form": "최애있고 차애있고 은우있다",
          "speaker_id": "1",
          "time": "20210905 23:58"
        },
        {
          "id": "MDRW2100032804.1.28",
          "form": "막 음중 첨으로 1위했다고 우는데",
          "original_form": "막 음중 첨으로 1위했다고 우는데",
          "speaker_id": "2",
          "time": "20210905 23:58"
        },
        {
          "id": "MDRW2100032804.1.29",
          "form": "귀엽더라고요",
          "original_form": "귀엽더라고요",
          "speaker_id": "2",
          "time": "20210905 23:58"
        },
        {
          "id": "MDRW2100032804.1.30",
          "form": "진짜 너무 ㅇㅖ쁘게 생겼어요,,",
          "original_form": "진짜 너무 ㅇㅖ쁘게 생겼어요,,",
          "speaker_id": "1",
          "time": "20210905 23:58"
        },
        {
          "id": "MDRW2100032804.1.31",
          "form": "제걸로 만들어버리고 싶었어요",
          "original_form": "제걸로 만들어버리고 싶었어요",
          "speaker_id": "2",
          "time": "20210905 23:58"
        }
      ]
