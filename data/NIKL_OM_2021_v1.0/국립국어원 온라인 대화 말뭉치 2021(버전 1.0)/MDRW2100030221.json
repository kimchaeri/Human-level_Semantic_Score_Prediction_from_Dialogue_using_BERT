      "utterance": [
        {
          "id": "MDRW2100030221.1.1",
          "form": "진짜 끝내주는 짜파게티를 끓여먹었어",
          "original_form": "진짜 끝내주는 짜파게티를 끓여먹었어",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030221.1.2",
          "form": "짜파게티라니",
          "original_form": "짜파게티라니",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030221.1.3",
          "form": "끝내줘도 짜파게티잖아",
          "original_form": "끝내줘도 짜파게티잖아",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030221.1.4",
          "form": "거의 세미짜장면에 가깝게",
          "original_form": "거의 세미짜장면에 가깝게",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030221.1.5",
          "form": "야채 고기 듬뿍 넣고",
          "original_form": "야채 고기 듬뿍 넣고",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030221.1.6",
          "form": "진짜 웃긴다",
          "original_form": "진짜 웃긴다",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030221.1.7",
          "form": "후추 뿌리고 국물 자작하게 졸였더니 간이 푹 배서",
          "original_form": "후추 뿌리고 국물 자작하게 졸였더니 간이 푹 배서",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030221.1.8",
          "form": "심혈을 기우렸네",
          "original_form": "심혈을 기우렸네",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030221.1.9",
          "form": "너무 맛잌ㅅ었어",
          "original_form": "너무 맛잌ㅅ었어",
          "speaker_id": "2",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030221.1.10",
          "form": "기울였네",
          "original_form": "기울였네",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030221.1.11",
          "form": "뭔가 이상하더라니",
          "original_form": "뭔가 이상하더라니",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030221.1.12",
          "form": "응 내 평생 끓인 짜파게티중",
          "original_form": "응 내 평생 끓인 짜파게티중",
          "speaker_id": "2",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030221.1.13",
          "form": "가장 심혈 기울임",
          "original_form": "가장 심혈 기울임",
          "speaker_id": "2",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030221.1.14",
          "form": "원탑",
          "original_form": "원탑",
          "speaker_id": "2",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030221.1.15",
          "form": "진짜 웃기셔",
          "original_form": "진짜 웃기셔",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030221.1.16",
          "form": "훠훠훠",
          "original_form": "훠훠훠",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030221.1.17",
          "form": "난 진라면 잘끓이는데",
          "original_form": "난 진라면 잘끓이는데",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030221.1.18",
          "form": "우리 만나면 라면대결하자",
          "original_form": "우리 만나면 라면대결하자",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030221.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋아",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030221.1.20",
          "form": "너 짜파구리 먹어봤니?",
          "original_form": "너 짜파구리 먹어봤니?",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030221.1.21",
          "form": "아니 너구리라면 싫어해",
          "original_form": "아니 너구리라면 싫어해",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030221.1.22",
          "form": "진라면과 짜파게티 섞으면 무슨 맛이 날 지 안 궁금하니",
          "original_form": "진라면과 짜파게티 섞으면 무슨 맛이 날 지 안 궁금하니",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030221.1.23",
          "form": "면이 굵고 힘이 없어서",
          "original_form": "면이 굵고 힘이 없어서",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030221.1.24",
          "form": "짜파게티도 사실 싫어해",
          "original_form": "짜파게티도 사실 싫어해",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030221.1.25",
          "form": "느끼해",
          "original_form": "느끼해",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030221.1.26",
          "form": "이럴수가 진라면 순정파야?",
          "original_form": "이럴수가 진라면 순정파야?",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030221.1.27",
          "form": "근데 짜장면은 먹어",
          "original_form": "근데 짜장면은 먹어",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030221.1.28",
          "form": "온리 진?",
          "original_form": "온리 진?",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030221.1.29",
          "form": "어 진매만 먹어",
          "original_form": "어 진매만 먹어",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030221.1.30",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030221.1.31",
          "form": "ㅋㅋㅌㅌㅋㅋㅋㅋ같은 짜 씨인데 왜 짜파구리는 배척해",
          "original_form": "ㅋㅋㅌㅌㅋㅋㅋㅋ같은 짜 씨인데 왜 짜파구리는 배척해",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030221.1.32",
          "form": "짜파게티도 싫어하는데 왜",
          "original_form": "짜파게티도 싫어하는데 왜",
          "speaker_id": "1",
          "time": "20210903 23:07"
        }
      ]
    }
