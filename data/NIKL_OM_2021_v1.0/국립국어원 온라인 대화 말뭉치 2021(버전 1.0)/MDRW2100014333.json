      "utterance": [
        {
          "id": "MDRW2100014333.1.1",
          "form": "의웬데?",
          "original_form": "의웬데?",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014333.1.2",
          "form": "그치 의외지?",
          "original_form": "그치 의외지?",
          "speaker_id": "1",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014333.1.3",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014333.1.4",
          "form": "흑돼지도 맛있었어",
          "original_form": "흑돼지도 맛있었어",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014333.1.5",
          "form": "흑돼지도 맛있지 한번도 안 먹어봤지만",
          "original_form": "흑돼지도 맛있지 한번도 안 먹어봤지만",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014333.1.6",
          "form": "갈치도 하나도 안 비려",
          "original_form": "갈치도 하나도 안 비려",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014333.1.7",
          "form": "아 배고프다",
          "original_form": "아 배고프다",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014333.1.8",
          "form": "배고파?",
          "original_form": "배고파?",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014333.1.9",
          "form": "ㅋㅋ 약간 음료 땡겨",
          "original_form": "ㅋㅋ 약간 음료 땡겨",
          "speaker_id": "2",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014333.1.10",
          "form": "우리 여행 얘기하고 있었잖아",
          "original_form": "우리 여행 얘기하고 있었잖아",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014333.1.11",
          "form": "그러게?ㅋㅋ",
          "original_form": "그러게?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014333.1.12",
          "form": "아 제주도에 유자 음료도 엄청 맛있어",
          "original_form": "아 제주도에 유자 음료도 엄청 맛있어",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014333.1.13",
          "form": "유자 별로 안좋아해서",
          "original_form": "유자 별로 안좋아해서",
          "speaker_id": "2",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014333.1.14",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "1",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014333.1.15",
          "form": "해외여행 가고 싶다",
