      "utterance": [
        {
          "id": "MDRW2100033778.1.1",
          "form": "응 비싼거",
          "original_form": "응 비싼거",
          "speaker_id": "1",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033778.1.2",
          "form": "응응 그러니깐ㅋㅋ",
          "original_form": "응응 그러니깐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033778.1.3",
          "form": "나도 내일 사봐야겠다ㅋㅋ",
          "original_form": "나도 내일 사봐야겠다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033778.1.4",
          "form": "우딜 놓칠까봐 긴장되더라",
          "original_form": "우딜 놓칠까봐 긴장되더라",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033778.1.5",
          "form": "웃겼음ㅋㅋㅋ",
          "original_form": "웃겼음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033778.1.6",
          "form": "나도 두근두근 이거 못잡으면 돈주고 사는거니까",
          "original_form": "나도 두근두근 이거 못잡으면 돈주고 사는거니까",
          "speaker_id": "1",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033778.1.7",
          "form": "그러니깐ㅋㅋㅋ",
          "original_form": "그러니깐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033778.1.8",
          "form": "배달비라도 받고 하면좋으니ㅋ",
          "original_form": "배달비라도 받고 하면좋으니ㅋ",
          "speaker_id": "2",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033778.1.9",
          "form": "정말 노르디스크 그거 할때도 그랬는데 그거 뺨침",
          "original_form": "정말 노르디스크 그거 할때도 그랬는데 그거 뺨침",
          "speaker_id": "1",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033778.1.10",
          "form": "그러니깐ㅋㅋ좋당ㅋㅋ",
          "original_form": "그러니깐ㅋㅋ좋당ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100033778.1.11",
          "form": "노르디스크는 20프로쿠폰 우짜노ㅋㅋ 그때 또 사장님한테 물어봐야되나 ㅋㅋ",
          "original_form": "노르디스크는 20프로쿠폰 우짜노ㅋㅋ 그때 또 사장님한테 물어봐야되나 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100033778.1.12",
          "form": "계속 전화하기는 그렇고",
          "original_form": "계속 전화하기는 그렇고",
          "speaker_id": "1",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100033778.1.13",
          "form": "응 그렇지ㅋㅋ",
          "original_form": "응 그렇지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100033778.1.14",
          "form": "된다캐야 미리 깁콘을 사놓는데 ㅋㅋㅋ",
          "original_form": "된다캐야 미리 깁콘을 사놓는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100033778.1.15",
          "form": "급할때 찾으면 없잖아",
          "original_form": "급할때 찾으면 없잖아",
          "speaker_id": "1",
          "time": "20210825 01:35"
        },
        {
          "id": "MDRW2100033778.1.16",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "2",
          "time": "20210825 01:35"
        },
        {
          "id": "MDRW2100033778.1.17",
          "form": "뭐 하긴 근데 사고 안된다카면 취소하면 기한내는 100프로 아냐??",
          "original_form": "뭐 하긴 근데 사고 안된다카면 취소하면 기한내는 100프로 아냐??",
          "speaker_id": "1",
          "time": "20210825 01:35"
        },
        {
          "id": "MDRW2100033778.1.18",
          "form": "흠ㅋㅋㅋㅋㄱ",
          "original_form": "흠ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210825 01:35"
        },
        {
          "id": "MDRW2100033778.1.19",
          "form": "100프로지",
          "original_form": "100프로지",
          "speaker_id": "2",
          "time": "20210825 01:35"
        },
        {
          "id": "MDRW2100033778.1.20",
          "form": "아 기한내",
          "original_form": "아 기한내",
