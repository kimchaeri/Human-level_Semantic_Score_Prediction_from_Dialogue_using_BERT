      "utterance": [
        {
          "id": "MDRW2100016295.1.1",
          "form": "마트가면 주로 뭐 구매하시나요",
          "original_form": "마트가면 주로 뭐 구매하시나요",
          "speaker_id": "1",
          "time": "20210826 11:43"
        },
        {
          "id": "MDRW2100016295.1.2",
          "form": "주로 식료품이죠 ㅎ",
          "original_form": "주로 식료품이죠 ㅎ",
          "speaker_id": "2",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016295.1.3",
          "form": "식료품 어떤거 주로 사시나요",
          "original_form": "식료품 어떤거 주로 사시나요",
          "speaker_id": "1",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100016295.1.4",
          "form": "냉동식품..?",
          "original_form": "냉동식품..?",
          "speaker_id": "1",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100016295.1.5",
          "form": "냉동은 되도록 안사려고해요",
          "original_form": "냉동은 되도록 안사려고해요",
          "speaker_id": "2",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016295.1.6",
          "form": "신선제품 위주로?",
          "original_form": "신선제품 위주로?",
          "speaker_id": "2",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016295.1.7",
          "form": "아진짜요?",
          "original_form": "아진짜요?",
          "speaker_id": "1",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016295.1.8",
          "form": "아.. 신선식품이면 채소 과일 생선 이런거 사시나보다",
          "original_form": "아.. 신선식품이면 채소 과일 생선 이런거 사시나보다",
          "speaker_id": "1",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016295.1.9",
          "form": "네 채소나 육류 주로 사욯",
          "original_form": "네 채소나 육류 주로 사욯",
          "speaker_id": "2",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016295.1.10",
          "form": "최근에 구매한 신선식품이 있나요",
          "original_form": "최근에 구매한 신선식품이 있나요",
          "speaker_id": "1",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016295.1.11",
          "form": "어제 돼지목살 샀어요 ㅎ",
          "original_form": "어제 돼지목살 샀어요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016295.1.12",
          "form": "돼지목살 너무 좋아해요",
          "original_form": "돼지목살 너무 좋아해요",
          "speaker_id": "1",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016295.1.13",
          "form": "구워먹는 용도로 구매하셨나요",
          "original_form": "구워먹는 용도로 구매하셨나요",
          "speaker_id": "1",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016295.1.14",
          "form": "네  시즈닝가루 뿌리고 에어프라이어에 넣너서 구워먹어요",
          "original_form": "네  시즈닝가루 뿌리고 에어프라이어에 넣너서 구워먹어요",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100016295.1.15",
          "form": "시즈닝은 뭐뿌리시나요?",
          "original_form": "시즈닝은 뭐뿌리시나요?",
          "speaker_id": "1",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100016295.1.16",
          "form": "코스트코에서 산 몬트리얼스즈닝뿌려요",
          "original_form": "코스트코에서 산 몬트리얼스즈닝뿌려요",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100016295.1.17",
          "form": "많이 짜사 조금만 뿌려서 먹어요",
          "original_form": "많이 짜사 조금만 뿌려서 먹어요",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100016295.1.18",
          "form": "저도 그렇게 한 번 먹어봐야겠어요",
          "original_form": "저도 그렇게 한 번 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 12:58"
        }
      ]
