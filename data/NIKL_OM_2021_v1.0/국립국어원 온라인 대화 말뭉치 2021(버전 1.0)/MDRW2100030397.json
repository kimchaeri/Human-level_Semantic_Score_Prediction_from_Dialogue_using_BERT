      "utterance": [
        {
          "id": "MDRW2100030397.1.1",
          "form": "에어프라이어",
          "original_form": "에어프라이어",
          "speaker_id": "1",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030397.1.2",
          "form": "사야되는데!!!",
          "original_form": "사야되는데!!!",
          "speaker_id": "1",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030397.1.3",
          "form": "ㅋㅋㅋ에어프라이어 진짜 편해요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ에어프라이어 진짜 편해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030397.1.4",
          "form": "빵 많이 드시거나 튀김류? 냉동음식류? 많이 드시면 후회 안하는 소비입니다",
          "original_form": "빵 많이 드시거나 튀김류? 냉동음식류? 많이 드시면 후회 안하는 소비입니다",
          "speaker_id": "2",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030397.1.5",
          "form": "아 고장나서",
          "original_form": "아 고장나서",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030397.1.6",
          "form": "새거 사야되서 ㅋㅋ",
          "original_form": "새거 사야되서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030397.1.7",
          "form": "아아아아 ㅋㅋㅋ",
          "original_form": "아아아아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030397.1.8",
          "form": "이미 갖고계셨군요 ㅋㅋ 근데어쩌다가 고장났어요??",
          "original_form": "이미 갖고계셨군요 ㅋㅋ 근데어쩌다가 고장났어요??",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030397.1.9",
          "form": "늙어서 수명이....ㅋㅋ",
          "original_form": "늙어서 수명이....ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030397.1.10",
          "form": "오 완전 초반부터 사용하셨나봐요",
          "original_form": "오 완전 초반부터 사용하셨나봐요",
          "speaker_id": "2",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030397.1.11",
          "form": "에어프라이어도 그렇게 갈아줘야되는줄 몰랐어요",
          "original_form": "에어프라이어도 그렇게 갈아줘야되는줄 몰랐어요",
          "speaker_id": "2",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030397.1.12",
          "form": "얘도 나이가 들죠",
          "original_form": "얘도 나이가 들죠",
          "speaker_id": "1",
          "time": "20210904 01:06"
        },
        {
          "id": "MDRW2100030397.1.13",
          "form": "와이프가 자취때 쓰던거 들고 온거라",
          "original_form": "와이프가 자취때 쓰던거 들고 온거라",
          "speaker_id": "1",
          "time": "20210904 01:06"
        },
        {
          "id": "MDRW2100030397.1.14",
          "form": "전자렌지처럼 천년만년 같이 갈 줄 알았는데 ㅠㅠ",
          "original_form": "전자렌지처럼 천년만년 같이 갈 줄 알았는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030397.1.15",
          "form": "약한 친구군요 ㅠㅠㅠ",
          "original_form": "약한 친구군요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030397.1.16",
          "form": "ㅋㅋ전자렌지도 죽어요!",
          "original_form": "ㅋㅋ전자렌지도 죽어요!",
          "speaker_id": "1",
          "time": "20210904 01:12"
        },
        {
          "id": "MDRW2100030397.1.17",
          "form": "ㅋㅋㅋ그럼 저희집 전자렌지가 워낙 튼튼한가봐요",
          "original_form": "ㅋㅋㅋ그럼 저희집 전자렌지가 워낙 튼튼한가봐요",
          "speaker_id": "2",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100030397.1.18",
          "form": "20살도 넘었는데 너무 멀쩡해요 ㅋㅋㅋㅋ",
          "original_form": "20살도 넘었는데 너무 멀쩡해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:18"
        },
        {
          "id": "MDRW2100030397.1.19",
          "form": "새로 살 기회를 안주네요 ㅋㅋ",
          "original_form": "새로 살 기회를 안주네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:18"
        },
        {
          "id": "MDRW2100030397.1.20",
          "form": "20살이요?",
          "original_form": "20살이요?",
          "speaker_id": "1",
          "time": "20210904 01:19"
        },
        {
          "id": "MDRW2100030397.1.21",
          "form": "2002 월드컵보다 전...",
          "original_form": "2002 월드컵보다 전...",
          "speaker_id": "1",
          "time": "20210904 01:20"
        },
        {
          "id": "MDRW2100030397.1.22",
          "form": "네 본가에 있는거 분명 제가 어릴때부터 쓰던앤데 아직도 너무 멀쩡해요 ㅋㅋㅋ",
          "original_form": "네 본가에 있는거 분명 제가 어릴때부터 쓰던앤데 아직도 너무 멀쩡해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100030397.1.23",
          "form": "ㅋㅋ장수하시네요",
          "original_form": "ㅋㅋ장수하시네요",
