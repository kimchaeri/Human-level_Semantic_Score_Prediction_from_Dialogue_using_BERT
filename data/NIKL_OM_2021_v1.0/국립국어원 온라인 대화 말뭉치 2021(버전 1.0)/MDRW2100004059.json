      "utterance": [
        {
          "id": "MDRW2100004059.1.1",
          "form": "종교있으신가요? 전무교인데 활동은 많이해봤어요 ㅋㅋㅋ",
          "original_form": "종교있으신가요? 전무교인데 활동은 많이해봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004059.1.2",
          "form": "저도 무교에요ㅋㅋㅋ ㅋㅋㅋ",
          "original_form": "저도 무교에요ㅋㅋㅋ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004059.1.3",
          "form": "무승 활동 하셨어요?",
          "original_form": "무승 활동 하셨어요?",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004059.1.4",
          "form": "저희집자체가 뼛속까지 무교집안인데 ㅋㅋㅋㅋ저 초등학생때까지 교회다녔어요",
          "original_form": "저희집자체가 뼛속까지 무교집안인데 ㅋㅋㅋㅋ저 초등학생때까지 교회다녔어요",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004059.1.5",
          "form": "간식줘서 ㅋㅋㅋㅋㅋ",
          "original_form": "간식줘서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004059.1.6",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004059.1.7",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ ㅎ필수죠",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ ㅎ필수죠",
          "speaker_id": "2",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004059.1.8",
          "form": "저도 어릴 때 사촌 때문에 교회 조금 다녔어요 놀로...",
          "original_form": "저도 어릴 때 사촌 때문에 교회 조금 다녔어요 놀로...",
          "speaker_id": "2",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004059.1.9",
          "form": "네 그리고 친한친구가 다니다보니 걍 놀러다냤었어요",
          "original_form": "네 그리고 친한친구가 다니다보니 걍 놀러다냤었어요",
          "speaker_id": "1",
          "time": "20210821 14:47"
        },
        {
          "id": "MDRW2100004059.1.10",
          "form": "그러다가 정신차려보니 성가대하고있더라구여",
          "original_form": "그러다가 정신차려보니 성가대하고있더라구여",
          "speaker_id": "1",
          "time": "20210821 14:47"
        },
        {
          "id": "MDRW2100004059.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:47"
        },
        {
          "id": "MDRW2100004059.1.12",
          "form": "ㅋㅋㅋㅋ ㅋㅋㅋㅋ 그정도면 나름 교회에",
          "original_form": "ㅋㅋㅋㅋ ㅋㅋㅋㅋ 그정도면 나름 교회에",
          "speaker_id": "2",
          "time": "20210821 14:48"
        },
        {
          "id": "MDRW2100004059.1.13",
          "form": "진심 아닌가요?",
          "original_form": "진심 아닌가요?",
          "speaker_id": "2",
          "time": "20210821 14:48"
        },
        {
          "id": "MDRW2100004059.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ하지만 제마음엔 예수말고 과자가있었던거죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ하지만 제마음엔 예수말고 과자가있었던거죠",
          "speaker_id": "1",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004059.1.15",
          "form": "초딩때끝으로 그이후엔 한번도 안갔어요 ㅋㅋㅋㅋ이제 용돈받기시작하니까",
          "original_form": "초딩때끝으로 그이후엔 한번도 안갔어요 ㅋㅋㅋㅋ이제 용돈받기시작하니까",
          "speaker_id": "1",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004059.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 자본주의가 아주 ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 자본주의가 아주 ㅎ",
          "speaker_id": "2",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004059.1.17",
          "form": "저도 그랬어요 !!!!",
          "original_form": "저도 그랬어요 !!!!",
          "speaker_id": "2",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004059.1.18",
          "form": "어릴때만 ㅎㄹ",
          "original_form": "어릴때만 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004059.1.19",
          "form": "네 ㅋㅋ석가탄신일엔절에가고 성탄일엔교회가고 그런거죠",
          "original_form": "네 ㅋㅋ석가탄신일엔절에가고 성탄일엔교회가고 그런거죠",
          "speaker_id": "1",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004059.1.20",
          "form": "원래 그런게 맞는거죠 ㅎ ㅎㅎ",
          "original_form": "원래 그런게 맞는거죠 ㅎ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004059.1.21",
          "form": "전 절은 산책 할겸도 가기도 했어요",
          "original_form": "전 절은 산책 할겸도 가기도 했어요",
          "speaker_id": "2",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004059.1.22",
          "form": "관광지느낌이 있어서 놀러가고그랬던것 같아요",
          "original_form": "관광지느낌이 있어서 놀러가고그랬던것 같아요",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004059.1.23",
          "form": "맞아요 약간 잔잔하니 마음의 평화를 얻고싶을 때도 좋아요",
          "original_form": "맞아요 약간 잔잔하니 마음의 평화를 얻고싶을 때도 좋아요",
          "speaker_id": "2",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004059.1.24",
          "form": "장소도 자연에둘러쌓여있는곳이 많고",
          "original_form": "장소도 자연에둘러쌓여있는곳이 많고",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004059.1.25",
          "form": "마음이편해지는 기분이에요",
          "original_form": "마음이편해지는 기분이에요",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004059.1.26",
          "form": "생각하니 다시 가고싶네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "생각하니 다시 가고싶네요ㅋㅋㅋㅋㅋㅋ",
