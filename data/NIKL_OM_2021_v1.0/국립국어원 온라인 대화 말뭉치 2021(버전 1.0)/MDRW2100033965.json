        },
        {
          "id": "MDRW2100033965.1.3",
          "form": "카페 가고 싶어",
          "original_form": "카페 가고 싶어",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100033965.1.4",
          "form": "아 나도",
          "original_form": "아 나도",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100033965.1.5",
          "form": "안 간지 너무 오래됐어",
          "original_form": "안 간지 너무 오래됐어",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100033965.1.6",
          "form": "마시고 싶은 거 있어?",
          "original_form": "마시고 싶은 거 있어?",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100033965.1.7",
          "form": "나야 뭐 아아지ㅋㅋㅋ",
          "original_form": "나야 뭐 아아지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100033965.1.8",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100033965.1.9",
          "form": "나는 음료보다는 마카롱이랑 케이크 먹고 싶다ㅠ",
          "original_form": "나는 음료보다는 마카롱이랑 케이크 먹고 싶다ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100033965.1.10",
          "form": "어떤 케이크?",
          "original_form": "어떤 케이크?",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100033965.1.11",
          "form": "투썸 아박 먹고 싶다",
          "original_form": "투썸 아박 먹고 싶다",
          "speaker_id": "1",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100033965.1.12",
          "form": "그 쿠앤크?",
          "original_form": "그 쿠앤크?",
          "speaker_id": "2",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100033965.1.13",
          "form": "엉 그거",
          "original_form": "엉 그거",
          "speaker_id": "1",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100033965.1.14",
          "form": "나한테는 너무 달았어ㅋㅋㅋ",
          "original_form": "나한테는 너무 달았어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100033965.1.15",
          "form": "그래도 카페 가고 싶다",
          "original_form": "그래도 카페 가고 싶다",
          "speaker_id": "2",
          "time": "20210825 17:09"
