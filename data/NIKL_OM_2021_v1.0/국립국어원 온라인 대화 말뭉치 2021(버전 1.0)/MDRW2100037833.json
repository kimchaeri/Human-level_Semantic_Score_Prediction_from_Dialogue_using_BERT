      "utterance": [
        {
          "id": "MDRW2100037833.1.1",
          "form": "와 ㅋㅋ 그 분 되게 길게 연애하는 스타일인가봐요",
          "original_form": "와 ㅋㅋ 그 분 되게 길게 연애하는 스타일인가봐요",
          "speaker_id": "2",
          "time": "20210901 19:18"
        },
        {
          "id": "MDRW2100037833.1.2",
          "form": "네 그러니깐요 중딩때부터 자기가 고백해서 사귄거래요",
          "original_form": "네 그러니깐요 중딩때부터 자기가 고백해서 사귄거래요",
          "speaker_id": "1",
          "time": "20210901 19:18"
        },
        {
          "id": "MDRW2100037833.1.3",
          "form": "그러다가 최근에 여친이 새남친생겨서",
          "original_form": "그러다가 최근에 여친이 새남친생겨서",
          "speaker_id": "1",
          "time": "20210901 19:18"
        },
        {
          "id": "MDRW2100037833.1.4",
          "form": "헤어지자고하니",
          "original_form": "헤어지자고하니",
          "speaker_id": "1",
          "time": "20210901 19:18"
        },
        {
          "id": "MDRW2100037833.1.5",
          "form": "제가 붙작으라고하니깐 그러기싫데요",
          "original_form": "제가 붙작으라고하니깐 그러기싫데요",
          "speaker_id": "1",
          "time": "20210901 19:18"
        },
        {
          "id": "MDRW2100037833.1.6",
          "form": "걔가 차인거니깐",
          "original_form": "걔가 차인거니깐",
          "speaker_id": "1",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100037833.1.7",
          "form": "흠...정말 자존심 센 편인가봐요",
          "original_form": "흠...정말 자존심 센 편인가봐요",
          "speaker_id": "2",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100037833.1.8",
          "form": "앜ㅋㅋㅋ 우리 오래갈수있을까요",
          "original_form": "앜ㅋㅋㅋ 우리 오래갈수있을까요",
          "speaker_id": "1",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100037833.1.9",
          "form": "자주싸웠는데",
          "original_form": "자주싸웠는데",
          "speaker_id": "1",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100037833.1.10",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100037833.1.11",
          "form": "우리는정식으로 사귀고있는게아닌데",
          "original_form": "우리는정식으로 사귀고있는게아닌데",
          "speaker_id": "1",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100037833.1.12",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100037833.1.13",
          "form": "뭐 꼭 사귀자 하고 선언해야 사귀는건 아니잖아요! ㅎㅎ",
          "original_form": "뭐 꼭 사귀자 하고 선언해야 사귀는건 아니잖아요! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100037833.1.14",
          "form": "그건 그렇죠....",
          "original_form": "그건 그렇죠....",
          "speaker_id": "1",
          "time": "20210901 19:20"
        },
