      "utterance": [
        {
          "id": "MDRW2100031444.1.1",
          "form": "피곤하겠지만.. 좀만 버티면 쉴 수 있을거야",
          "original_form": "피곤하겠지만.. 좀만 버티면 쉴 수 있을거야",
          "speaker_id": "1",
          "time": "20210904 21:37"
        },
        {
          "id": "MDRW2100031444.1.2",
          "form": "군대가 쉬러 가는 곳이라니 표현이 좀 어색하긴 하지만...",
          "original_form": "군대가 쉬러 가는 곳이라니 표현이 좀 어색하긴 하지만...",
          "speaker_id": "1",
          "time": "20210904 21:38"
        },
        {
          "id": "MDRW2100031444.1.3",
          "form": "ㅇㅇㅇㅇ 아냐근데",
          "original_form": "ㅇㅇㅇㅇ 아냐근데",
          "speaker_id": "2",
          "time": "20210904 21:38"
        },
        {
          "id": "MDRW2100031444.1.4",
          "form": "아이돌한텐 진짜 쉬러가는거같을듯",
          "original_form": "아이돌한텐 진짜 쉬러가는거같을듯",
          "speaker_id": "2",
          "time": "20210904 21:38"
        },
        {
          "id": "MDRW2100031444.1.5",
          "form": "잠 규칙적으로 꼬박꼬박 재우고",
          "original_form": "잠 규칙적으로 꼬박꼬박 재우고",
          "speaker_id": "1",
          "time": "20210904 21:59"
        },
        {
          "id": "MDRW2100031444.1.6",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100031444.1.7",
          "form": "삼시세-끼 챙겨먹이고ㅡㅡ",
          "original_form": "삼시세-끼 챙겨먹이고ㅡㅡ",
          "speaker_id": "1",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100031444.1.8",
          "form": "필터링 아오",
          "original_form": "필터링 아오",
          "speaker_id": "1",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100031444.1.9",
          "form": "아무튼 불규칙한 아이돌생활 바로잡아주기엔",
          "original_form": "아무튼 불규칙한 아이돌생활 바로잡아주기엔",
          "speaker_id": "1",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100031444.1.10",
          "form": "탁월할듯",
          "original_form": "탁월할듯",
