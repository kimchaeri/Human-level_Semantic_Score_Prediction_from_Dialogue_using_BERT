      "utterance": [
        {
          "id": "MDRW2100003067.1.1",
          "form": "마침 귤을 사먹고싶네요ㅠㅠ",
          "original_form": "마침 귤을 사먹고싶네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 18:14"
        },
        {
          "id": "MDRW2100003067.1.2",
          "form": "ㅎㅎ 요즘에는 만감류(귤)같은 게 진짜 사시사철 나오더라고요.",
          "original_form": "ㅎㅎ 요즘에는 만감류(귤)같은 게 진짜 사시사철 나오더라고요.",
          "speaker_id": "2",
          "time": "20210820 18:15"
        },
        {
          "id": "MDRW2100003067.1.3",
          "form": "지금은 하우스 밀감 나오는듯요??",
          "original_form": "지금은 하우스 밀감 나오는듯요??",
          "speaker_id": "2",
          "time": "20210820 18:16"
        },
        {
          "id": "MDRW2100003067.1.4",
          "form": "와 정말 귤을 잘 아시네요ㅎㅎ",
          "original_form": "와 정말 귤을 잘 아시네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 18:16"
        },
        {
          "id": "MDRW2100003067.1.5",
          "form": "근데 지금 귤은 비싸지 않나요?",
          "original_form": "근데 지금 귤은 비싸지 않나요?",
          "speaker_id": "1",
          "time": "20210820 18:16"
        },
        {
          "id": "MDRW2100003067.1.6",
          "form": "비싸죠..",
          "original_form": "비싸죠..",
          "speaker_id": "2",
          "time": "20210820 18:16"
        },
        {
          "id": "MDRW2100003067.1.7",
          "form": "사는곳이 제주도다 보니까.. 어찌어찌 얻어서 먹는 정도만 먹어요.",
          "original_form": "사는곳이 제주도다 보니까.. 어찌어찌 얻어서 먹는 정도만 먹어요.",
          "speaker_id": "2",
          "time": "20210820 18:17"
        },
        {
          "id": "MDRW2100003067.1.8",
          "form": "요즘 장보는거는 다 인터넷으로만 사는거 같아요.",
          "original_form": "요즘 장보는거는 다 인터넷으로만 사는거 같아요.",
          "speaker_id": "2",
          "time": "20210820 18:18"
        },
        {
          "id": "MDRW2100003067.1.9",
          "form": "직접가서 사는거 좋아하세요?",
          "original_form": "직접가서 사는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 18:18"
        },
        {
          "id": "MDRW2100003067.1.10",
          "form": "와 제주도!",
          "original_form": "와 제주도!",
          "speaker_id": "1",
          "time": "20210820 18:18"
        },
        {
          "id": "MDRW2100003067.1.11",
          "form": "과일은 아직",
          "original_form": "과일은 아직",
          "speaker_id": "1",
          "time": "20210820 18:18"
        },
        {
          "id": "MDRW2100003067.1.12",
          "form": "골라야되겠더라고요ㅠ",
          "original_form": "골라야되겠더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210820 18:18"
        },
        {
          "id": "MDRW2100003067.1.13",
          "form": "그쵸그쵸..",
          "original_form": "그쵸그쵸..",
          "speaker_id": "2",
          "time": "20210820 18:19"
        },
        {
          "id": "MDRW2100003067.1.14",
          "form": "저도 과일이랑 채소 고기는.. 눈으로 보고 사야해서 그것만 마트나 시장가서 몰아서 사와요.ㅠㅠ",
          "original_form": "저도 과일이랑 채소 고기는.. 눈으로 보고 사야해서 그것만 마트나 시장가서 몰아서 사와요.ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 18:19"
        },
        {
          "id": "MDRW2100003067.1.15",
          "form": "제주도는 마트에서 귤이 싼가요??",
          "original_form": "제주도는 마트에서 귤이 싼가요??",
          "speaker_id": "1",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100003067.1.16",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100003067.1.17",
          "form": "아뇨 비싸요..ㅠ",
          "original_form": "아뇨 비싸요..ㅠ",
          "speaker_id": "2",
          "time": "20210820 18:57"
        },
        {
          "id": "MDRW2100003067.1.18",
          "form": "그 농담 있잖아요",
          "original_form": "그 농담 있잖아요",
          "speaker_id": "1",
          "time": "20210820 19:01"
        },
        {
          "id": "MDRW2100003067.1.19",
          "form": "제주도 학생은 등교하면서 귤을 따먹는다는..",
          "original_form": "제주도 학생은 등교하면서 귤을 따먹는다는..",
          "speaker_id": "1",
          "time": "20210820 19:01"
        },
        {
          "id": "MDRW2100003067.1.20",
          "form": "놀리는건 아녜요ㅠ",
          "original_form": "놀리는건 아녜요ㅠ",
          "speaker_id": "1",
          "time": "20210820 19:02"
        },
        {
          "id": "MDRW2100003067.1.21",
          "form": "예전에는 진짜 그랬어요..",
          "original_form": "예전에는 진짜 그랬어요..",
          "speaker_id": "2",
          "time": "20210820 19:12"
        },
        {
          "id": "MDRW2100003067.1.22",
          "form": "고등학교 3면이 귤밭....",
          "original_form": "고등학교 3면이 귤밭....",
          "speaker_id": "2",
          "time": "20210820 19:12"
        },
        {
          "id": "MDRW2100003067.1.23",
          "form": "와 친환경 고등학교네요!!",
          "original_form": "와 친환경 고등학교네요!!",
          "speaker_id": "1",
          "time": "20210820 19:46"
        },
        {
          "id": "MDRW2100003067.1.24",
          "form": "서리때문에 밭주인님의 민원이 끈이지 않았던...",
          "original_form": "서리때문에 밭주인님의 민원이 끈이지 않았던...",
          "speaker_id": "2",
          "time": "20210820 19:46"
        },
        {
          "id": "MDRW2100003067.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ귀엽다 귤서리",
          "original_form": "ㅋㅋㅋㅋㅋㅋ귀엽다 귤서리",
          "speaker_id": "1",
          "time": "20210820 19:47"
        },
        {
          "id": "MDRW2100003067.1.26",
          "form": "학생들한테 싸게좀 파시지",
          "original_form": "학생들한테 싸게좀 파시지",
          "speaker_id": "1",
          "time": "20210820 19:48"
        },
        {
          "id": "MDRW2100003067.1.27",
          "form": "그래서 나중에는 따서 비상품 콘테이너를 학교 담벼락에 주시더라고요ㅡㅋㅋㅋㅋ",
          "original_form": "그래서 나중에는 따서 비상품 콘테이너를 학교 담벼락에 주시더라고요ㅡㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 19:50"
        }
      ]
