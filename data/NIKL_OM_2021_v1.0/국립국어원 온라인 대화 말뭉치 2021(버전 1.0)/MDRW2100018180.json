      "utterance": [
        {
          "id": "MDRW2100018180.1.1",
          "form": "어떤 연예인 좋아하세요?",
          "original_form": "어떤 연예인 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018180.1.2",
          "form": "나혼산에 성훈 좋아요",
          "original_form": "나혼산에 성훈 좋아요",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018180.1.3",
          "form": "좋아하시는 연예인 있으신가요?",
          "original_form": "좋아하시는 연예인 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018180.1.4",
          "form": "전 요즘 아이유가 참 좋더라구요",
          "original_form": "전 요즘 아이유가 참 좋더라구요",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018180.1.5",
          "form": "오 저도 좋아해요",
          "original_form": "오 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018180.1.6",
          "form": "유튜브로 계속 찾아보고 했네요ㅋ",
          "original_form": "유튜브로 계속 찾아보고 했네요ㅋ",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018180.1.7",
          "form": "유튜브 알고리즘이 무서워요",
          "original_form": "유튜브 알고리즘이 무서워요",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018180.1.8",
          "form": "헤어나올수가 없어요",
          "original_form": "헤어나올수가 없어요",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018180.1.9",
          "form": "그니깐요 계속 보게 되는ㅋㅋ",
          "original_form": "그니깐요 계속 보게 되는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018180.1.10",
          "form": "나보다 나를 더 잘아는 느낌이에요",
          "original_form": "나보다 나를 더 잘아는 느낌이에요",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018180.1.11",
          "form": "유튜브가 이제 생활이 됐어요",
          "original_form": "유튜브가 이제 생활이 됐어요",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018180.1.12",
          "form": "티비나 드라마도 보시나요?",
          "original_form": "티비나 드라마도 보시나요?",
          "speaker_id": "2",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018180.1.13",
          "form": "티비를 보긴 하는데 빈센조 이후로 챙겨보는 드라마는 없어요",
          "original_form": "티비를 보긴 하는데 빈센조 이후로 챙겨보는 드라마는 없어요",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018180.1.14",
          "form": "빈센조 재밌나요? 아직 못봐서요",
          "original_form": "빈센조 재밌나요? 아직 못봐서요",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018180.1.15",
          "form": "네 재밌어요. 끝에 내용들이 좀 그렇긴 한데 괜찮은 편이예요",
          "original_form": "네 재밌어요. 끝에 내용들이 좀 그렇긴 한데 괜찮은 편이예요",
          "speaker_id": "1",
          "time": "20210826 23:20"
        }
