      "utterance": [
        {
          "id": "MDRW2100009519.1.1",
          "form": "안녕하세요 대학생이신가요? 곧 개강이네요!",
          "original_form": "안녕하세요 대학생이신가요? 곧 개강이네요!",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009519.1.2",
          "form": "으악!! 끔찍하네요ㅠㅜ",
          "original_form": "으악!! 끔찍하네요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009519.1.3",
          "form": "수강신청은 하셨나요?",
          "original_form": "수강신청은 하셨나요?",
          "speaker_id": "1",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100009519.1.4",
          "form": "수강신청 했어요!",
          "original_form": "수강신청 했어요!",
          "speaker_id": "2",
          "time": "20210823 00:18"
        },
        {
          "id": "MDRW2100009519.1.5",
          "form": "실패했지만요...",
          "original_form": "실패했지만요...",
          "speaker_id": "2",
          "time": "20210823 00:18"
        },
        {
          "id": "MDRW2100009519.1.6",
          "form": "수강신청 하셨어요?",
          "original_form": "수강신청 하셨어요?",
          "speaker_id": "2",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100009519.1.7",
          "form": "저는 휴학생이에요",
          "original_form": "저는 휴학생이에요",
          "speaker_id": "1",
          "time": "20210823 00:21"
        },
        {
          "id": "MDRW2100009519.1.8",
          "form": "수강신청 얼마나 실패하셨어요?",
          "original_form": "수강신청 얼마나 실패하셨어요?",
          "speaker_id": "1",
          "time": "20210823 00:21"
        },
        {
          "id": "MDRW2100009519.1.9",
          "form": "원래 들어야했던",
          "original_form": "원래 들어야했던",
          "speaker_id": "2",
          "time": "20210823 00:21"
        },
        {
          "id": "MDRW2100009519.1.10",
          "form": "전공을 신청 못 햇ㅇㆍ요",
          "original_form": "전공을 신청 못 햇ㅇㆍ요",
          "speaker_id": "2",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009519.1.11",
          "form": "교수님께 말씀드렸는데 안된다고 하셔서..",
          "original_form": "교수님께 말씀드렸는데 안된다고 하셔서..",
          "speaker_id": "2",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009519.1.12",
          "form": "신청 못 했죠",
          "original_form": "신청 못 했죠",
          "speaker_id": "2",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009519.1.13",
          "form": "헐 전공은 너무 큰데... 이제 다시 기회가 없나요?",
          "original_form": "헐 전공은 너무 큰데... 이제 다시 기회가 없나요?",
          "speaker_id": "1",
          "time": "20210823 00:24"
        },
        {
          "id": "MDRW2100009519.1.14",
          "form": "내년에 들으라고 하시더라구요ㅠㅜ",
          "original_form": "내년에 들으라고 하시더라구요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210823 00:25"
        },
        {
          "id": "MDRW2100009519.1.15",
          "form": "어쩔 수 없죠ㅠㅜ",
          "original_form": "어쩔 수 없죠ㅠㅜ",
          "speaker_id": "2",
          "time": "20210823 00:25"
        },
        {
          "id": "MDRW2100009519.1.16",
          "form": "아이고... 너무 아깝네요 꼭 자리가 생기길...ㅠㅠ",
          "original_form": "아이고... 너무 아깝네요 꼭 자리가 생기길...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 00:26"
        },
        {
          "id": "MDRW2100009519.1.17",
          "form": "그때는 꼭 들어야죠!!",
          "original_form": "그때는 꼭 들어야죠!!",
          "speaker_id": "2",
          "time": "20210823 00:27"
        },
        {
          "id": "MDRW2100009519.1.18",
          "form": "화이팅!!",
          "original_form": "화이팅!!",
          "speaker_id": "2",
          "time": "20210823 00:27"
        },
        {
          "id": "MDRW2100009519.1.19",
          "form": "휴학 하신거 부러워요!!",
          "original_form": "휴학 하신거 부러워요!!",
          "speaker_id": "2",
          "time": "20210823 00:27"
        },
        {
          "id": "MDRW2100009519.1.20",
          "form": "전 다른 거 준비하느라 휴학해서...ㅠ 원래 1년 생각했는데 2년으로 늘어났네요ㅎ",
          "original_form": "전 다른 거 준비하느라 휴학해서...ㅠ 원래 1년 생각했는데 2년으로 늘어났네요ㅎ",
          "speaker_id": "1",
          "time": "20210823 00:28"
        },
        {
          "id": "MDRW2100009519.1.21",
          "form": "동아리는 하시나요?",
          "original_form": "동아리는 하시나요?",
          "speaker_id": "1",
          "time": "20210823 00:28"
        },
        {
          "id": "MDRW2100009519.1.22",
          "form": "영상 제작 동아리해요!",
          "original_form": "영상 제작 동아리해요!",
          "speaker_id": "2",
          "time": "20210823 00:29"
        },
        {
          "id": "MDRW2100009519.1.23",
          "form": "큰 기술 없이 간단한 것만 하는 동아리에요",
          "original_form": "큰 기술 없이 간단한 것만 하는 동아리에요",
          "speaker_id": "2",
          "time": "20210823 00:29"
        },
        {
          "id": "MDRW2100009519.1.24",
          "form": "우와 그래도 뭐를 만드는 데서 보람이 있겠어요!",
          "original_form": "우와 그래도 뭐를 만드는 데서 보람이 있겠어요!",
          "speaker_id": "1",
          "time": "20210823 00:30"
        },
        {
          "id": "MDRW2100009519.1.25",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210823 00:33"
        },
        {
          "id": "MDRW2100009519.1.26",
          "form": "과정은 힘들지만",
          "original_form": "과정은 힘들지만",
          "speaker_id": "2",
          "time": "20210823 00:34"
        },
        {
          "id": "MDRW2100009519.1.27",
          "form": "결과가 나오면 뿌듯하고 좋아요",
          "original_form": "결과가 나오면 뿌듯하고 좋아요",
          "speaker_id": "2",
          "time": "20210823 00:34"
        }
      ]
    }
