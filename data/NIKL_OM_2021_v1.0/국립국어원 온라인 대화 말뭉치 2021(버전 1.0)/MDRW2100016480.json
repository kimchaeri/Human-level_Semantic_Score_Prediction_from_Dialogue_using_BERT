      "utterance": [
        {
          "id": "MDRW2100016480.1.1",
          "form": "최근에 구매하신 거 있으세요??",
          "original_form": "최근에 구매하신 거 있으세요??",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016480.1.2",
          "form": "저 폼롤러 샀어요",
          "original_form": "저 폼롤러 샀어요",
          "speaker_id": "2",
          "time": "20210826 13:07"
        },
        {
          "id": "MDRW2100016480.1.3",
          "form": "그건 스트레칭용인가요?",
          "original_form": "그건 스트레칭용인가요?",
          "speaker_id": "1",
          "time": "20210826 13:07"
        },
        {
          "id": "MDRW2100016480.1.4",
          "form": "네 스트레칭용이에요",
          "original_form": "네 스트레칭용이에요",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016480.1.5",
          "form": "근력운동도 가능해요",
          "original_form": "근력운동도 가능해요",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016480.1.6",
          "form": "효과좋나요??",
          "original_form": "효과좋나요??",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016480.1.7",
          "form": "저는 너무 잘 쓰고 있어요",
          "original_form": "저는 너무 잘 쓰고 있어요",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016480.1.8",
          "form": "오 신기하네요",
          "original_form": "오 신기하네요",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016480.1.9",
          "form": "어디서 구매하셨어요?",
          "original_form": "어디서 구매하셨어요?",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016480.1.10",
          "form": "마트에서 샀어요 ㅋㅋㅋㅋ",
          "original_form": "마트에서 샀어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016480.1.11",
          "form": "마트에서 별거별거 다 파네요 ㅋㅋㅋ",
          "original_form": "마트에서 별거별거 다 파네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016480.1.12",
          "form": "요가매트랑 같이",
          "original_form": "요가매트랑 같이",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016480.1.13",
          "form": "아 저도 요가매트 필요한데 ㅠㅠ",
          "original_form": "아 저도 요가매트 필요한데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016480.1.14",
          "form": "소소하게 홈트하기 좋아요",
          "original_form": "소소하게 홈트하기 좋아요",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016480.1.15",
          "form": "저도 참고해야겠어요 ㅋㅋㅋ",
          "original_form": "저도 참고해야겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016480.1.16",
          "form": "네 요가매트는 두꺼운게 좋아요",
          "original_form": "네 요가매트는 두꺼운게 좋아요",
          "speaker_id": "2",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016480.1.17",
          "form": "저렴한건 얇더라고요 ㅠㅠ 다 이유가있었네요",
          "original_form": "저렴한건 얇더라고요 ㅠㅠ 다 이유가있었네요",
          "speaker_id": "1",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016480.1.18",
          "form": "네 두꺼운게 안정감이 있어요",
          "original_form": "네 두꺼운게 안정감이 있어요",
          "speaker_id": "2",
