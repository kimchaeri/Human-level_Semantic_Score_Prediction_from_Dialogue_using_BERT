        },
        {
          "id": "MDRW2100038826.1.2",
          "form": "비 오셨나요?",
          "original_form": "비 오셨나요?",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038826.1.3",
          "form": "비오진 않았고 흐리기만 했어요",
          "original_form": "비오진 않았고 흐리기만 했어요",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100038826.1.4",
          "form": "저희도 비는 안 왔어요",
          "original_form": "저희도 비는 안 왔어요",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100038826.1.5",
          "form": "어제는 비 오셨나요?",
          "original_form": "어제는 비 오셨나요?",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100038826.1.6",
          "form": "어제는 엄청 왔었어요",
          "original_form": "어제는 엄청 왔었어요",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100038826.1.7",
          "form": "헉 그렇군요",
          "original_form": "헉 그렇군요",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100038826.1.8",
          "form": "피해는 없으셨나요?",
          "original_form": "피해는 없으셨나요?",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100038826.1.9",
          "form": "여기는 도심지라서 따로 피해는 없었어요",
          "original_form": "여기는 도심지라서 따로 피해는 없었어요",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100038826.1.10",
          "form": "다행이네요!",
          "original_form": "다행이네요!",
          "speaker_id": "1",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100038826.1.11",
          "form": "가을 좋아하시나요?",
          "original_form": "가을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100038826.1.12",
          "form": "가을 좋죠",
          "original_form": "가을 좋죠",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038826.1.13",
          "form": "단풍 구경하는거 좋아해요",
          "original_form": "단풍 구경하는거 좋아해요",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038826.1.14",
          "form": "저도 가을 너무 좋아해요",
          "original_form": "저도 가을 너무 좋아해요",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038826.1.15",
          "form": "비오다가 가을 다 끝날까봐 무서워요ㅋㅋㅋㅋ",
          "original_form": "비오다가 가을 다 끝날까봐 무서워요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038826.1.16",
          "form": "이제 비 그쳤으니까 가을일거예요",
          "original_form": "이제 비 그쳤으니까 가을일거예요",
