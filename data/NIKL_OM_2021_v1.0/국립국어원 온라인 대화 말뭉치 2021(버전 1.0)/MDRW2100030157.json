      "utterance": [
        {
          "id": "MDRW2100030157.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100030157.1.2",
          "form": "저 초밥 좋아해요! 연어초밥이나 참치초밥이요!",
          "original_form": "저 초밥 좋아해요! 연어초밥이나 참치초밥이요!",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030157.1.3",
          "form": "우와 ㅜㅠ 초밥",
          "original_form": "우와 ㅜㅠ 초밥",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030157.1.4",
          "form": "생각하니 먹고 싶어요!!",
          "original_form": "생각하니 먹고 싶어요!!",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030157.1.5",
          "form": "저도 초밥좋아해요 ㅠㅠ",
          "original_form": "저도 초밥좋아해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030157.1.6",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100030157.1.7",
          "form": "그쵸ㅠㅠㅠ 홍대에 여우골... 아시나요!? 여기가 정말 참맛이랍니다..",
          "original_form": "그쵸ㅠㅠㅠ 홍대에 여우골... 아시나요!? 여기가 정말 참맛이랍니다..",
          "speaker_id": "2",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100030157.1.8",
          "form": "여우골이요?ㅠㅠ",
          "original_form": "여우골이요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030157.1.9",
          "form": "참맛이요?ㅠㅠ",
          "original_form": "참맛이요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030157.1.10",
          "form": "가봐야겠어요 ㅠㅠㅠ",
          "original_form": "가봐야겠어요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030157.1.11",
          "form": "알려주셔서 감사해요ㅠㅠ",
          "original_form": "알려주셔서 감사해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030157.1.12",
          "form": "저는 강남에 있는 은행골에서 맛있게 먹었어요",
          "original_form": "저는 강남에 있는 은행골에서 맛있게 먹었어요",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030157.1.13",
          "form": "흡!! 여기가 원! 조! 랍니다! ㅠㅠㅠㅠ 진짜 너무 맛있어요ㅠㅠㅠ",
          "original_form": "흡!! 여기가 원! 조! 랍니다! ㅠㅠㅠㅠ 진짜 너무 맛있어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030157.1.14",
          "form": "참치회 좋아하시면 참치초밥으로 드셔보세요 12피스에 25000원이었던거로 기억하는데",
          "original_form": "참치회 좋아하시면 참치초밥으로 드셔보세요 12피스에 25000원이었던거로 기억하는데",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030157.1.15",
          "form": "입에서 참치가 녹아 사라져요..",
          "original_form": "입에서 참치가 녹아 사라져요..",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030157.1.16",
          "form": "으아 진짜 먹으러 갈꺼예요",
          "original_form": "으아 진짜 먹으러 갈꺼예요",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030157.1.17",
          "form": "여우골..",
          "original_form": "여우골..",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030157.1.18",
          "form": "참치...",
          "original_form": "참치...",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030157.1.19",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030157.1.20",
          "form": "배고파요 ㅠㅠ",
          "original_form": "배고파요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030157.1.21",
          "form": "지금 kbs에서 편식당 하는데 조인성 배우님이 먹는거 보니까...",
          "original_form": "지금 kbs에서 편식당 하는데 조인성 배우님이 먹는거 보니까...",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030157.1.22",
          "form": "너무 한식땡겨요",
          "original_form": "너무 한식땡겨요",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
