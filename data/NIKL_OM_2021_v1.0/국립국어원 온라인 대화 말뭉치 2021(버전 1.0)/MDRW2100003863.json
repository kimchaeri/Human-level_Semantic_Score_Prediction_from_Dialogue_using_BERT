      "utterance": [
        {
          "id": "MDRW2100003863.1.1",
          "form": "짐붐바 스테이크랑 스파이시 갈릭 씨푸드라이스",
          "original_form": "짐붐바 스테이크랑 스파이시 갈릭 씨푸드라이스",
          "speaker_id": "2",
          "time": "20210821 13:03"
        },
        {
          "id": "MDRW2100003863.1.2",
          "form": "오 맛있겠네 ㅎㅎ",
          "original_form": "오 맛있겠네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 13:03"
        },
        {
          "id": "MDRW2100003863.1.3",
          "form": "그리고 투움바 파스타",
          "original_form": "그리고 투움바 파스타",
          "speaker_id": "2",
          "time": "20210821 13:03"
        },
        {
          "id": "MDRW2100003863.1.4",
          "form": "배터지겠는데 ㅋㅋㅋ",
          "original_form": "배터지겠는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 13:03"
        },
        {
          "id": "MDRW2100003863.1.5",
          "form": "다 런치 메뉴라 양 많을 듯",
          "original_form": "다 런치 메뉴라 양 많을 듯",
          "speaker_id": "2",
          "time": "20210821 13:03"
        },
        {
          "id": "MDRW2100003863.1.6",
          "form": "간만에 포식하겠군",
          "original_form": "간만에 포식하겠군",
          "speaker_id": "1",
          "time": "20210821 13:03"
        },
        {
          "id": "MDRW2100003863.1.7",
          "form": "빵도 넉넉히 달라고 말해야지",
          "original_form": "빵도 넉넉히 달라고 말해야지",
          "speaker_id": "2",
          "time": "20210821 13:04"
        },
        {
          "id": "MDRW2100003863.1.8",
          "form": "ㅋㅋㅋ좋네",
          "original_form": "ㅋㅋㅋ좋네",
          "speaker_id": "1",
          "time": "20210821 13:04"
        },
        {
          "id": "MDRW2100003863.1.9",
          "form": "딜리버리는 왜 할인을 안해주나 몰러",
          "original_form": "딜리버리는 왜 할인을 안해주나 몰러",
          "speaker_id": "2",
          "time": "20210821 13:04"
        },
        {
          "id": "MDRW2100003863.1.10",
          "form": "글게 가서 포장하기 귀찮은데",
          "original_form": "글게 가서 포장하기 귀찮은데",
          "speaker_id": "1",
          "time": "20210821 13:04"
        },
        {
          "id": "MDRW2100003863.1.11",
          "form": "아웃백 할인은 직접가야만 해준다고 하니 ㅠ 귀찮아도 가야지 뭐 ㅎㅎ",
          "original_form": "아웃백 할인은 직접가야만 해준다고 하니 ㅠ 귀찮아도 가야지 뭐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 13:04"
        },
        {
          "id": "MDRW2100003863.1.12",
          "form": "가지고 오다가 면 다 부는거 아냐? ㅋㅋ",
          "original_form": "가지고 오다가 면 다 부는거 아냐? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 13:04"
        },
        {
          "id": "MDRW2100003863.1.13",
          "form": "불어도 맛있옹 ㅎㅎ",
          "original_form": "불어도 맛있옹 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 13:05"
        },
        {
          "id": "MDRW2100003863.1.14",
          "form": "아웃백 주문할 때 초코소스도 달라고 해야겠다",
          "original_form": "아웃백 주문할 때 초코소스도 달라고 해야겠다",
