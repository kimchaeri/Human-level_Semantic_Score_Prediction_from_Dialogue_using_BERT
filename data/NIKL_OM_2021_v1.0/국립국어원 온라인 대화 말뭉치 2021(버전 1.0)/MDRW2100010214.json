      "utterance": [
        {
          "id": "MDRW2100010214.1.1",
          "form": "너 카트라이더 러쉬 해봤니?",
          "original_form": "너 카트라이더 러쉬 해봤니?",
          "speaker_id": "1",
          "time": "20210823 15:44"
        },
        {
          "id": "MDRW2100010214.1.2",
          "form": "오오 해봤지~",
          "original_form": "오오 해봤지~",
          "speaker_id": "2",
          "time": "20210823 15:44"
        },
        {
          "id": "MDRW2100010214.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210823 15:44"
        },
        {
          "id": "MDRW2100010214.1.4",
          "form": "나두 예전에 꽤 많이 플레이했어",
          "original_form": "나두 예전에 꽤 많이 플레이했어",
          "speaker_id": "1",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010214.1.5",
          "form": "잘하니?",
          "original_form": "잘하니?",
          "speaker_id": "2",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010214.1.6",
          "form": "그냥 그저그래",
          "original_form": "그냥 그저그래",
          "speaker_id": "1",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010214.1.7",
          "form": "카트라이더에서 좋아하는 캐릭터는 뭐였어?",
          "original_form": "카트라이더에서 좋아하는 캐릭터는 뭐였어?",
          "speaker_id": "2",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010214.1.8",
          "form": "나는 우니를 좋아해.",
          "original_form": "나는 우니를 좋아해.",
          "speaker_id": "2",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010214.1.9",
          "form": "나는 그 빨간색",
          "original_form": "나는 그 빨간색",
          "speaker_id": "1",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010214.1.10",
          "form": "우니 귀엽지않니?",
          "original_form": "우니 귀엽지않니?",
          "speaker_id": "2",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010214.1.11",
          "form": "이름이 뭐더라",
          "original_form": "이름이 뭐더라",
          "speaker_id": "1",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010214.1.12",
          "form": "빨간색이라면 배찌 말이구나",
          "original_form": "빨간색이라면 배찌 말이구나",
          "speaker_id": "2",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010214.1.13",
          "form": "맞아 나는 배찌좋아해",
          "original_form": "맞아 나는 배찌좋아해",
          "speaker_id": "1",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010214.1.14",
          "form": "하지만 배찌는 플레이하기 어려웠어. 그렇지?",
          "original_form": "하지만 배찌는 플레이하기 어려웠어. 그렇지?",
          "speaker_id": "2",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010214.1.15",
          "form": "그럼 너는 우니 스킨도 샀니??",
          "original_form": "그럼 너는 우니 스킨도 샀니??",
          "speaker_id": "1",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010214.1.16",
          "form": "기간제였잖아.",
          "original_form": "기간제였잖아.",
          "speaker_id": "2",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010214.1.17",
          "form": "맞아 배찌는 갖기가 힘들었어",
          "original_form": "맞아 배찌는 갖기가 힘들었어",
          "speaker_id": "1",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010214.1.18",
          "form": "그럼. 머리를 민 우니를 샀지",
          "original_form": "그럼. 머리를 민 우니를 샀지",
          "speaker_id": "2",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010214.1.19",
          "form": "그거 꽤나 웃겼어.",
          "original_form": "그거 꽤나 웃겼어.",
          "speaker_id": "2",
          "time": "20210823 15:46"
        }
      ]
    }
