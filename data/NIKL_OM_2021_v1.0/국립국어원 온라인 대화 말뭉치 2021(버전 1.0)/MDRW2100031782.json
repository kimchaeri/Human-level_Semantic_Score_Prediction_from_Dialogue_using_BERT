      "utterance": [
        {
          "id": "MDRW2100031782.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210905 00:31"
        },
        {
          "id": "MDRW2100031782.1.2",
          "form": "안녕하세요. 찜닭에 잡채 나왓나요?",
          "original_form": "안녕하세요. 찜닭에 잡채 나왓나요?",
          "speaker_id": "1",
          "time": "20210905 01:51"
        },
        {
          "id": "MDRW2100031782.1.3",
          "form": "잡채는 안 나왔어요\r\n당면은 들어가 있었지만요",
          "original_form": "잡채는 안 나왔어요\r\n당면은 들어가 있었지만요",
          "speaker_id": "2",
          "time": "20210905 01:53"
        },
        {
          "id": "MDRW2100031782.1.4",
          "form": "오.. 그렇군요 ㅋㅋㅋ두꺼운당면인가요?",
          "original_form": "오.. 그렇군요 ㅋㅋㅋ두꺼운당면인가요?",
          "speaker_id": "1",
          "time": "20210905 02:17"
        },
        {
          "id": "MDRW2100031782.1.5",
          "form": "아니요 일반적인 당면이에요",
          "original_form": "아니요 일반적인 당면이에요",
          "speaker_id": "2",
          "time": "20210905 02:26"
        },
        {
          "id": "MDRW2100031782.1.6",
          "form": "오 그렇군요. 두꺼운당면 좋아하세요?",
          "original_form": "오 그렇군요. 두꺼운당면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 02:31"
        },
        {
          "id": "MDRW2100031782.1.7",
          "form": "아니요 저는 개인적으로 일반 당면이 더 좋더라구요",
          "original_form": "아니요 저는 개인적으로 일반 당면이 더 좋더라구요",
          "speaker_id": "2",
          "time": "20210905 02:35"
        },
        {
          "id": "MDRW2100031782.1.8",
          "form": "오.... 그러시군요. !!",
          "original_form": "오.... 그러시군요. !!",
          "speaker_id": "1",
          "time": "20210905 04:11"
        },
        {
          "id": "MDRW2100031782.1.9",
          "form": "점심맛있게  드셧어요?",
          "original_form": "점심맛있게  드셧어요?",
          "speaker_id": "1",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100031782.1.10",
          "form": "아직이요  \r\n이제 먹으려구요",
          "original_form": "아직이요  \r\n이제 먹으려구요",
          "speaker_id": "2",
          "time": "20210905 14:00"
        },
        {
          "id": "MDRW2100031782.1.11",
          "form": "오 늦게드시네요.",
          "original_form": "오 늦게드시네요.",
          "speaker_id": "1",
          "time": "20210905 15:41"
        },
        {
          "id": "MDRW2100031782.1.12",
          "form": "네 좀 늦었죠\r\n요즘 좀 늦어졌어요",
          "original_form": "네 좀 늦었죠\r\n요즘 좀 늦어졌어요",
          "speaker_id": "2",
          "time": "20210905 15:43"
        },
        {
          "id": "MDRW2100031782.1.13",
          "form": "뭐드셧어요?",
          "original_form": "뭐드셧어요?",
          "speaker_id": "1",
          "time": "20210905 17:47"
        },
        {
          "id": "MDRW2100031782.1.14",
          "form": "어제 저녁먹고 찜닭이 많이 남아서 그거 먹었어요",
          "original_form": "어제 저녁먹고 찜닭이 많이 남아서 그거 먹었어요",
