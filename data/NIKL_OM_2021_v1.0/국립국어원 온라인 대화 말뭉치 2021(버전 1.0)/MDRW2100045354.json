        {
          "id": "MDRW2100045354.1.1",
          "form": "형제관계가 어떻게 되세요 ㅎㅎ",
          "original_form": "형제관계가 어떻게 되세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045354.1.2",
          "form": "남동생 잇어요",
          "original_form": "남동생 잇어요",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045354.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045354.1.4",
          "form": "저는 여동생이요!!",
          "original_form": "저는 여동생이요!!",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045354.1.5",
          "form": "사이 좋으세요? ㅋㅋ",
          "original_form": "사이 좋으세요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045354.1.6",
          "form": "와 대박부럽",
          "original_form": "와 대박부럽",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045354.1.7",
          "form": "자매 진짜 부러워욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "자매 진짜 부러워욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045354.1.8",
          "form": "다들 부러워하더라고요 여동생의 존재를..",
          "original_form": "다들 부러워하더라고요 여동생의 존재를..",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045354.1.9",
          "form": "근데 사실 성별만 같은 동생일뿐.. ㅋㅋㅋㅋㅋ",
          "original_form": "근데 사실 성별만 같은 동생일뿐.. ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045354.1.10",
          "form": "사이가 소원한가요?",
          "original_form": "사이가 소원한가요?",
          "speaker_id": "2",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045354.1.11",
          "form": "소원까진 아닌데 약간 오면 어 왔냐 이런 느낌이에요 ㅋㅋㅋ 서로 막 그리워하고 이런건 없어요 ㅋㅋㅋ",
          "original_form": "소원까진 아닌데 약간 오면 어 왔냐 이런 느낌이에요 ㅋㅋㅋ 서로 막 그리워하고 이런건 없어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045354.1.12",
          "form": "에이 그정도면 사이좋은거죠",
          "original_form": "에이 그정도면 사이좋은거죠",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045354.1.13",
          "form": "그런가요 하긴 ㅋㅋㅋㅋ name2님은 남동생이랑 어릴때 싸웠나요",
          "original_form": "그런가요 하긴 ㅋㅋㅋㅋ &name2&님은 남동생이랑 어릴때 싸웠나요",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045354.1.14",
          "form": "음 다른집보다는 덜싸운 편이에요",
          "original_form": "음 다른집보다는 덜싸운 편이에요",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045354.1.15",
