      "utterance": [
        {
          "id": "MDRW2100021984.1.1",
          "form": "일이군요ㅎㅎㅎ",
          "original_form": "일이군요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021984.1.2",
          "form": "일하러가서 일안하시구",
          "original_form": "일하러가서 일안하시구",
          "speaker_id": "1",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021984.1.3",
          "form": "수다만 떨고 오신거에요 ㅎㅎ?",
          "original_form": "수다만 떨고 오신거에요 ㅎㅎ?",
          "speaker_id": "1",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021984.1.4",
          "form": "일은 했어요ㅎㅎ",
          "original_form": "일은 했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021984.1.5",
          "form": "근데 수다 떨다가 12시에 나올꺼 2시에나옴요ㅠㅠ",
          "original_form": "근데 수다 떨다가 12시에 나올꺼 2시에나옴요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021984.1.6",
          "form": "선장님도 말많으시고",
          "original_form": "선장님도 말많으시고",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021984.1.7",
          "form": "나도 말많고ㅎㅎ",
          "original_form": "나도 말많고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021984.1.8",
          "form": "우앙 근데 배타면 재밋어요?",
          "original_form": "우앙 근데 배타면 재밋어요?",
          "speaker_id": "1",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021984.1.9",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210829 00:29"
        },
        {
          "id": "MDRW2100021984.1.10",
          "form": "저같은사람은 못타요ㅎ",
          "original_form": "저같은사람은 못타요ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:29"
        },
        {
          "id": "MDRW2100021984.1.11",
          "form": "수다를 못떨잖아요ㅎ",
          "original_form": "수다를 못떨잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:29"
        },
        {
          "id": "MDRW2100021984.1.12",
          "form": "그리고 가족들 친구들 보고싶은데 못봐서ㅎ",
          "original_form": "그리고 가족들 친구들 보고싶은데 못봐서ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:29"
        },
        {
          "id": "MDRW2100021984.1.13",
          "form": "앗 저 배를 안타봐서 ㅋ",
          "original_form": "앗 저 배를 안타봐서 ㅋ",
          "speaker_id": "1",
          "time": "20210829 00:29"
        },
        {
          "id": "MDRW2100021984.1.14",
          "form": "근데 재밌긴 재밌어요ㅎㅎ",
          "original_form": "근데 재밌긴 재밌어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:29"
        },
        {
          "id": "MDRW2100021984.1.15",
          "form": "한번도 안타봄  ㅡㅁ ㅡ_ 요",
          "original_form": "한번도 안타봄  ㅡㅁ ㅡ_ 요",
          "speaker_id": "1",
          "time": "20210829 00:29"
        },
        {
          "id": "MDRW2100021984.1.16",
          "form": "지금은 코로나라ㅎ 밖에못나가는데",
          "original_form": "지금은 코로나라ㅎ 밖에못나가는데",
          "speaker_id": "2",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021984.1.17",
          "form": "예전에는 많이 나갔어요ㅎㅎ",
          "original_form": "예전에는 많이 나갔어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021984.1.18",
          "form": "그래서 여행아닌여행도 했구요ㅎ",
          "original_form": "그래서 여행아닌여행도 했구요ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021984.1.19",
          "form": "전 안타봐서 그런가",
          "original_form": "전 안타봐서 그런가",
          "speaker_id": "1",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021984.1.20",
          "form": "재밋을가 같아요 ㅎㅎ",
          "original_form": "재밋을가 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021984.1.21",
          "form": "ㅎㅎ 재밌어요ㅎ",
          "original_form": "ㅎㅎ 재밌어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021984.1.22",
          "form": "나중에 늙어서 보트나 하나구입할예정인데ㅎ 그때까지 인연계속되면 한번태워드릴께요ㅎㅎ",
          "original_form": "나중에 늙어서 보트나 하나구입할예정인데ㅎ 그때까지 인연계속되면 한번태워드릴께요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021984.1.23",
          "form": "우앙 ㅋ 좋아요 ㅎㅎ",
          "original_form": "우앙 ㅋ 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021984.1.24",
          "form": "좋은 사람이니까ㅎㅎ",
          "original_form": "좋은 사람이니까ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:33"
        },
        {
          "id": "MDRW2100021984.1.25",
          "form": "오 첫배~",
          "original_form": "오 첫배~",
          "speaker_id": "1",
          "time": "20210829 00:33"
        },
        {
          "id": "MDRW2100021984.1.26",
          "form": "이래뵈도ㅎ 항해사출신이라 운전은 잘해요ㅎㅎ",
          "original_form": "이래뵈도ㅎ 항해사출신이라 운전은 잘해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:34"
        },
        {
          "id": "MDRW2100021984.1.27",
          "form": "왜 그런지 모르겟는데ㅎㅎ",
          "original_form": "왜 그런지 모르겟는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:34"
        },
        {
          "id": "MDRW2100021984.1.28",
          "form": "기대 해볼게요",
          "original_form": "기대 해볼게요",
          "speaker_id": "1",
          "time": "20210829 00:34"
        },
        {
          "id": "MDRW2100021984.1.29",
          "form": "저는 드라이브가 좋아요ㅎㅎ 바다랑 육지랑 아주 다른데ㅎ",
          "original_form": "저는 드라이브가 좋아요ㅎㅎ 바다랑 육지랑 아주 다른데ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:34"
        }
      ]
    }
  ]
