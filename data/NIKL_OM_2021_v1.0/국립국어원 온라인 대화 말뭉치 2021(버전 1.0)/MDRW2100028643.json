      "utterance": [
        {
          "id": "MDRW2100028643.1.1",
          "form": "새학긴데 잘 적응햇니?",
          "original_form": "새학긴데 잘 적응햇니?",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028643.1.2",
          "form": "당근이지 ㅎㅎ",
          "original_form": "당근이지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028643.1.3",
          "form": "친구들이랑 안어색해?",
          "original_form": "친구들이랑 안어색해?",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028643.1.4",
          "form": "나도 어색할줄 알았는데",
          "original_form": "나도 어색할줄 알았는데",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028643.1.5",
          "form": "어제본것처럼",
          "original_form": "어제본것처럼",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028643.1.6",
          "form": "자연스럽더라구.",
          "original_form": "자연스럽더라구.",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028643.1.7",
          "form": "오히려 더 친해진 느낌?",
          "original_form": "오히려 더 친해진 느낌?",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028643.1.8",
          "form": "사실 어제본걸지도몰라...",
          "original_form": "사실 어제본걸지도몰라...",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028643.1.9",
          "form": "방학은 없었던거지",
          "original_form": "방학은 없었던거지",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028643.1.10",
          "form": "일리가있네",
          "original_form": "일리가있네",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028643.1.11",
          "form": "자연스럽게 2학기로 넘어온거지...",
          "original_form": "자연스럽게 2학기로 넘어온거지...",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028643.1.12",
          "form": "생각해보니까 진짜 그랬어",
          "original_form": "생각해보니까 진짜 그랬어",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028643.1.13",
          "form": "방학은 허상이구",
          "original_form": "방학은 허상이구",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028643.1.14",
          "form": "그런가봐..",
          "original_form": "그런가봐..",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028643.1.15",
          "form": "나는 방학이 너무 길었어 ㅎㅎ",
          "original_form": "나는 방학이 너무 길었어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028643.1.16",
          "form": "너무 좋았엉 ㅋㅋ",
          "original_form": "너무 좋았엉 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028643.1.17",
          "form": "참 좋겠다 ㅎㅎ",
          "original_form": "참 좋겠다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028643.1.18",
          "form": "대학생이니깐 ㅎㅎ",
          "original_form": "대학생이니깐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028643.1.19",
          "form": "그래도 이제 과제때문에",
          "original_form": "그래도 이제 과제때문에",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028643.1.20",
          "form": "개꿀이지.",
          "original_form": "개꿀이지.",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028643.1.21",
          "form": "밤새게생겼네 ㅎㅎ",
          "original_form": "밤새게생겼네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028643.1.22",
          "form": "대학생이니깐 ㅎㅎ",
          "original_form": "대학생이니깐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028643.1.23",
          "form": "그래도 과제때문에 힘들려면 좀 남았어 ㅎㅎ",
          "original_form": "그래도 과제때문에 힘들려면 좀 남았어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028643.1.24",
          "form": "그렇구나 ㅎㅎ",
          "original_form": "그렇구나 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028643.1.25",
          "form": "원래 학기초에는 널널하잖아",
          "original_form": "원래 학기초에는 널널하잖아",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028643.1.26",
          "form": "대학교 시험은",
          "original_form": "대학교 시험은",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028643.1.27",
          "form": "언제 봐?",
          "original_form": "언제 봐?",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028643.1.28",
          "form": "8주마다 한번씩 봐",
          "original_form": "8주마다 한번씩 봐",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028643.1.29",
          "form": "8주차 16주차에 보는거지",
          "original_form": "8주차 16주차에 보는거지",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028643.1.30",
          "form": "그렇구나!!",
          "original_form": "그렇구나!!",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028643.1.31",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028643.1.32",
          "form": "나도 빨리 대학교 가구싶어",
          "original_form": "나도 빨리 대학교 가구싶어",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028643.1.33",
          "form": "고등학교때랑은 다르게",
          "original_form": "고등학교때랑은 다르게",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028643.1.34",
          "form": "대부분 비슷해",
          "original_form": "대부분 비슷해",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028643.1.35",
          "form": "화이팅!!",
          "original_form": "화이팅!!",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028643.1.36",
          "form": "동아리 하기 싫다",
          "original_form": "동아리 하기 싫다",
          "speaker_id": "1",
