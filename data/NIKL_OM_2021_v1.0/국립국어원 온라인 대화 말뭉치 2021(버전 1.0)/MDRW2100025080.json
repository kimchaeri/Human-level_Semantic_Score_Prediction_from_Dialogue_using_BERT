      "utterance": [
        {
          "id": "MDRW2100025080.1.1",
          "form": "혼자 여행 가보신적 있으세요?",
          "original_form": "혼자 여행 가보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025080.1.2",
          "form": "길치라서 혼자 가본적은 없어요ㅠ",
          "original_form": "길치라서 혼자 가본적은 없어요ㅠ",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100025080.1.3",
          "form": "혼자 가보셨어요??",
          "original_form": "혼자 가보셨어요??",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100025080.1.4",
          "form": "네네 혼자 자주 가요 ㅎ",
          "original_form": "네네 혼자 자주 가요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025080.1.5",
          "form": "혼자 생각 정리도하고 ~ 하고싶은대로 하고 편해요",
          "original_form": "혼자 생각 정리도하고 ~ 하고싶은대로 하고 편해요",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025080.1.6",
          "form": "저도 혼자여행 가보고싶은데 길치라 용기가 안 나네요 ㅎ",
          "original_form": "저도 혼자여행 가보고싶은데 길치라 용기가 안 나네요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100025080.1.7",
          "form": "기억에 남는 여행지 있나요?",
          "original_form": "기억에 남는 여행지 있나요?",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100025080.1.8",
          "form": "저도 길치인데 잘 다녔어요!!!!!!!!",
          "original_form": "저도 길치인데 잘 다녔어요!!!!!!!!",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025080.1.9",
          "form": "음 여러곳이 있는데 싱가포르랑 대만이요!",
          "original_form": "음 여러곳이 있는데 싱가포르랑 대만이요!",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025080.1.10",
          "form": "싱가포르! 저도 다녀왔어요 ㅎㅎ\r\n어디가 제일 좋으셨나요?",
          "original_form": "싱가포르! 저도 다녀왔어요 ㅎㅎ\r\n어디가 제일 좋으셨나요?",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025080.1.11",
          "form": "저는 센토사? 맞나?  거기요!!!!!",
          "original_form": "저는 센토사? 맞나?  거기요!!!!!",
          "speaker_id": "1",
          "time": "20210901 09:22"
        },
        {
          "id": "MDRW2100025080.1.12",
          "form": "집라인 타고",
          "original_form": "집라인 타고",
          "speaker_id": "1",
          "time": "20210901 09:23"
        },
        {
          "id": "MDRW2100025080.1.13",
          "form": "완전 재밌었어요",
          "original_form": "완전 재밌었어요",
          "speaker_id": "1",
          "time": "20210901 09:23"
        },
        {
          "id": "MDRW2100025080.1.14",
          "form": "센토사는 진짜 필수코스 같아요 ㅋㅋㅋㅋ",
          "original_form": "센토사는 진짜 필수코스 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:32"
        },
        {
          "id": "MDRW2100025080.1.15",
          "form": "센토사에서 일주일 놀라고 해도 가능할 것 같아요 저는 ㅋㅋㅋㅋ",
          "original_form": "센토사에서 일주일 놀라고 해도 가능할 것 같아요 저는 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:32"
        },
        {
          "id": "MDRW2100025080.1.16",
          "form": "오 맞아요 ㅎ 저 하루는 유니버셜가고 하루는 섬돌아다니고 그랬거든요",
          "original_form": "오 맞아요 ㅎ 저 하루는 유니버셜가고 하루는 섬돌아다니고 그랬거든요",
          "speaker_id": "1",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025080.1.17",
          "form": "너무 재밌었어요 진짜",
          "original_form": "너무 재밌었어요 진짜",
          "speaker_id": "1",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025080.1.18",
          "form": "해변 돌아다니실 때 공작새 보셨나요?",
          "original_form": "해변 돌아다니실 때 공작새 보셨나요?",
          "speaker_id": "2",
          "time": "20210901 16:16"
        },
        {
          "id": "MDRW2100025080.1.19",
          "form": "예?",
          "original_form": "예?",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025080.1.20",
          "form": "공작새요?",
          "original_form": "공작새요?",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025080.1.21",
          "form": "아뇨 ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025080.1.22",
          "form": "어디에 있었어요?",
          "original_form": "어디에 있었어요?",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025080.1.23",
          "form": "저 막 루지타고 그래서ㅋㅋ",
          "original_form": "저 막 루지타고 그래서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025080.1.24",
          "form": "실로소 비치였나 거기였던 것 같아요!",
          "original_form": "실로소 비치였나 거기였던 것 같아요!",
          "speaker_id": "2",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025080.1.25",
          "form": "루지 재밌죠 ㅋㅋㅋㅋ 저는 밤에 타서 더 스릴있게 탄 것 같아요 ㅋㅋㅋ",
          "original_form": "루지 재밌죠 ㅋㅋㅋㅋ 저는 밤에 타서 더 스릴있게 탄 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025080.1.26",
          "form": "아하 ㅎ 아쉽게도 저는 못봤네요 흡",
          "original_form": "아하 ㅎ 아쉽게도 저는 못봤네요 흡",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100025080.1.27",
          "form": "와 밤에 진짜 재밌었겠네요",
          "original_form": "와 밤에 진짜 재밌었겠네요",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100025080.1.28",
          "form": "곤돌라 ? 그거 올라가는거 너무 무서웟어요",
          "original_form": "곤돌라 ? 그거 올라가는거 너무 무서웟어요",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100025080.1.29",
          "form": "같이 간 친구도 곤돌라 무서워하던데",
          "original_form": "같이 간 친구도 곤돌라 무서워하던데",
          "speaker_id": "2",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025080.1.30",
          "form": "곤돌라 말고는 올라갈 방법이 없어서 잠깐동안 힘드셨겠어요ㅠ",
          "original_form": "곤돌라 말고는 올라갈 방법이 없어서 잠깐동안 힘드셨겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210901 17:57"
        }
      ]
