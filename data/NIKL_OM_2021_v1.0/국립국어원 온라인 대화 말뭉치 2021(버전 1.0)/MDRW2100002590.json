        {
          "id": "MDRW2100002590.1.1",
          "form": "취업했나요?",
          "original_form": "취업했나요?",
          "speaker_id": "1",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002590.1.2",
          "form": "안녕하세요  네 했어요",
          "original_form": "안녕하세요  네 했어요",
          "speaker_id": "2",
          "time": "20210820 14:23"
        },
        {
          "id": "MDRW2100002590.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002590.1.4",
          "form": "무슨 일 하시나요?",
          "original_form": "무슨 일 하시나요?",
          "speaker_id": "2",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002590.1.5",
          "form": "사무직이요",
          "original_form": "사무직이요",
          "speaker_id": "1",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002590.1.6",
          "form": "저도 사무직이에요",
          "original_form": "저도 사무직이에요",
          "speaker_id": "2",
          "time": "20210820 15:17"
        },
        {
          "id": "MDRW2100002590.1.7",
          "form": "앉아만있어서 자꾸 허리가 아파요",
          "original_form": "앉아만있어서 자꾸 허리가 아파요",
          "speaker_id": "2",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002590.1.8",
          "form": "맨손체조 하세요",
          "original_form": "맨손체조 하세요",
          "speaker_id": "1",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002590.1.9",
          "form": "사무실에서 맨손체조하시나요?",
          "original_form": "사무실에서 맨손체조하시나요?",
          "speaker_id": "2",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002590.1.10",
          "form": "해야 하는데 안하죸ㅋ",
          "original_form": "해야 하는데 안하죸ㅋ",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002590.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 해야하는데 하고 마음만먹어요 매일",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 해야하는데 하고 마음만먹어요 매일",
          "speaker_id": "2",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002590.1.12",
          "form": "쉽지 않아여",
          "original_form": "쉽지 않아여",
          "speaker_id": "1",
          "time": "20210820 15:33"
