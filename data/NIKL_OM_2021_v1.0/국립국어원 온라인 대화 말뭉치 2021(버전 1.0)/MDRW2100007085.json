      "utterance": [
        {
          "id": "MDRW2100007085.1.1",
          "form": "여행 가고싶다ㅠ",
          "original_form": "여행 가고싶다ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007085.1.2",
          "form": "나두 코로나만 끝나면 계절마다 갈거야",
          "original_form": "나두 코로나만 끝나면 계절마다 갈거야",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007085.1.3",
          "form": "올해 첨으로 여행못갔네..지독한 코로나..",
          "original_form": "올해 첨으로 여행못갔네..지독한 코로나..",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007085.1.4",
          "form": "맞아ㅠ 진짜 우리 처음으로 여름 그냥 보냈다",
          "original_form": "맞아ㅠ 진짜 우리 처음으로 여름 그냥 보냈다",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007085.1.5",
          "form": "오늘 뜬금없이 대만 일월담 차계란 생각났어",
          "original_form": "오늘 뜬금없이 대만 일월담 차계란 생각났어",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007085.1.6",
          "form": "넌참 대만 좋아해ㅋㅋ 뭐 나도 좋아하긴 하지만",
          "original_form": "넌참 대만 좋아해ㅋㅋ 뭐 나도 좋아하긴 하지만",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007085.1.7",
          "form": "대만 그 특유의 정서가 좋아",
          "original_form": "대만 그 특유의 정서가 좋아",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007085.1.8",
          "form": "뭔가 여유롭고 느긋한 느낌??",
          "original_form": "뭔가 여유롭고 느긋한 느낌??",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007085.1.9",
          "form": "그리고 사실 입맛에 잘맞아",
          "original_form": "그리고 사실 입맛에 잘맞아",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007085.1.10",
          "form": "음식 안맞으면 힘들텐데 잘 맞음",
          "original_form": "음식 안맞으면 힘들텐데 잘 맞음",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007085.1.11",
          "form": "근데 내가볼때 너랑 난 음식 가리는거 없이 잘맞아서 어딜가도 음식고생은 안할거야",
          "original_form": "근데 내가볼때 너랑 난 음식 가리는거 없이 잘맞아서 어딜가도 음식고생은 안할거야",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007085.1.12",
          "form": "윽 근데 고수는 좀 아냐",
          "original_form": "윽 근데 고수는 좀 아냐",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007085.1.13",
          "form": "나 사실 방금 베트남 여행 생각했는데 베트남 쌀국수 말고 분짜? 그거 먹을때 향채 다발로 주는 건 좀 어려울듯..",
          "original_form": "나 사실 방금 베트남 여행 생각했는데 베트남 쌀국수 말고 분짜? 그거 먹을때 향채 다발로 주는 건 좀 어려울듯..",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007085.1.14",
          "form": "아 고수가 있구나..그래 나도 고수는 좀 그래",
          "original_form": "아 고수가 있구나..그래 나도 고수는 좀 그래",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007085.1.15",
          "form": "그 기억나? 용컹제 썬메리빵집에서 샀던 고로켘ㅋㅋㅋㅋㅋ",
          "original_form": "그 기억나? 용컹제 썬메리빵집에서 샀던 고로켘ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007085.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ어 그건 진짜 내 인생 최악의 고로케였다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ어 그건 진짜 내 인생 최악의 고로케였다",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007085.1.17",
          "form": "고수맛고로케ㅋㅋㅋ",
          "original_form": "고수맛고로케ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007085.1.18",
