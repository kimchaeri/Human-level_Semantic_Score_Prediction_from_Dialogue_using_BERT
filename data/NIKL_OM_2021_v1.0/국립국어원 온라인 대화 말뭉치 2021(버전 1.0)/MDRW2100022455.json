      "utterance": [
        {
          "id": "MDRW2100022455.1.1",
          "form": "단점은 빨리 그만듄다는 거에요..",
          "original_form": "단점은 빨리 그만듄다는 거에요..",
          "speaker_id": "1",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022455.1.2",
          "form": "아앗...ㅠㅠㅠ",
          "original_form": "아앗...ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022455.1.3",
          "form": "일이 힘든가봐요ㅠㅠㅠ",
          "original_form": "일이 힘든가봐요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022455.1.4",
          "form": "네ㅠ 저 대학때 프로듀사 인기였는데",
          "original_form": "네ㅠ 저 대학때 프로듀사 인기였는데",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022455.1.5",
          "form": "공효진 차태현같은 사수 없고..",
          "original_form": "공효진 차태현같은 사수 없고..",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022455.1.6",
          "form": "김수현같은 동기는 더더욱 없고..",
          "original_form": "김수현같은 동기는 더더욱 없고..",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022455.1.7",
          "form": "그렇싐다",
          "original_form": "그렇싐다",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022455.1.8",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100022455.1.9",
          "form": "그런 곳이 있다면",
          "original_form": "그런 곳이 있다면",
          "speaker_id": "2",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022455.1.10",
          "form": "거긴 유토피아네요",
          "original_form": "거긴 유토피아네요",
          "speaker_id": "2",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022455.1.11",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022455.1.12",
          "form": "맞아요ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:50"
        },
        {
          "id": "MDRW2100022455.1.13",
          "form": "퇴사는 제 사전엔 없는거죵ㅋㅋㅋㅋㅋ",
          "original_form": "퇴사는 제 사전엔 없는거죵ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:51"
        },
        {
          "id": "MDRW2100022455.1.14",
          "form": "ㅋㅋㅋㅋㅋ그럼요그럼요",
          "original_form": "ㅋㅋㅋㅋㅋ그럼요그럼요",
          "speaker_id": "2",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100022455.1.15",
          "form": "평생직장 해야죠 그런 곳은",
          "original_form": "평생직장 해야죠 그런 곳은",
          "speaker_id": "2",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100022455.1.16",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100022455.1.17",
          "form": "맞아용ㅋㅋㅋ",
          "original_form": "맞아용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:23"
        },
        {
          "id": "MDRW2100022455.1.18",
          "form": "name2은 전공이 어떤거신가요?",
          "original_form": "&name2&은 전공이 어떤거신가요?",
          "speaker_id": "1",
          "time": "20210829 15:23"
        },
        {
          "id": "MDRW2100022455.1.19",
          "form": "아 저는 식품입니다!!",
          "original_form": "아 저는 식품입니다!!",
          "speaker_id": "2",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022455.1.20",
          "form": "전공으로 취업하시는군요!",
          "original_form": "전공으로 취업하시는군요!",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022455.1.21",
          "form": "네넹!!",
          "original_form": "네넹!!",
          "speaker_id": "2",
          "time": "20210829 16:10"
        },
        {
          "id": "MDRW2100022455.1.22",
          "form": "전공 쪽으로 한 번은 시도해봐야 후회 안 할 거 같아서요ㅎㅎ",
          "original_form": "전공 쪽으로 한 번은 시도해봐야 후회 안 할 거 같아서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:10"
        },
        {
          "id": "MDRW2100022455.1.23",
          "form": "적성이 잘 맞는 건 아니지만",
          "original_form": "적성이 잘 맞는 건 아니지만",
          "speaker_id": "2",
          "time": "20210829 16:11"
        },
        {
          "id": "MDRW2100022455.1.24",
          "form": "뭐 딱히 원하는 직종도 없어서",
          "original_form": "뭐 딱히 원하는 직종도 없어서",
          "speaker_id": "2",
          "time": "20210829 16:11"
        },
        {
          "id": "MDRW2100022455.1.25",
          "form": "그냥 전공 살리려고요ㅎㅎ",
          "original_form": "그냥 전공 살리려고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:11"
        },
        {
          "id": "MDRW2100022455.1.26",
          "form": "좋죠~",
          "original_form": "좋죠~",
          "speaker_id": "1",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100022455.1.27",
          "form": "해보지 않음 모르긴하니까요!",
          "original_form": "해보지 않음 모르긴하니까요!",
          "speaker_id": "1",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100022455.1.28",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "2",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022455.1.29",
          "form": "해보고 안 맞으면",
          "original_form": "해보고 안 맞으면",
          "speaker_id": "2",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022455.1.30",
          "form": "그때 다른 거 하죠 뭐~ㅎㄹ",
          "original_form": "그때 다른 거 하죠 뭐~ㅎㄹ",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022455.1.31",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022455.1.32",
          "form": "더 나이들기전에 해야죠",
          "original_form": "더 나이들기전에 해야죠",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022455.1.33",
          "form": "지금와서 생각하면 저때는 직업관련 체험해보는거나 진로를 생각해보는게 없는게 지금도 아쉬워요",
          "original_form": "지금와서 생각하면 저때는 직업관련 체험해보는거나 진로를 생각해보는게 없는게 지금도 아쉬워요",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022455.1.34",
          "form": "직업 관련 체험해 보는 게 있어요??",
          "original_form": "직업 관련 체험해 보는 게 있어요??",
          "speaker_id": "2",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022455.1.35",
          "form": "애들 체험해 볼 수 있는게 있더라구요!",
          "original_form": "애들 체험해 볼 수 있는게 있더라구요!",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022455.1.36",
          "form": "아 자유학기제 말씀하시는 건가요??",
          "original_form": "아 자유학기제 말씀하시는 건가요??",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022455.1.37",
          "form": "어린이 직업체험이요!",
          "original_form": "어린이 직업체험이요!",
          "speaker_id": "1",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022455.1.38",
          "form": "아아 잡월드 같은 거요??",
          "original_form": "아아 잡월드 같은 거요??",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022455.1.39",
          "form": "네네!!",
          "original_form": "네네!!",
