      "utterance": [
        {
          "id": "MDRW2100012179.1.1",
          "form": "디저트는 뭘 드셨나요",
          "original_form": "디저트는 뭘 드셨나요",
          "speaker_id": "1",
          "time": "20210824 20:06"
        },
        {
          "id": "MDRW2100012179.1.2",
          "form": "디저트는 복숭아를 먹었습니다 ㅋㅋ",
          "original_form": "디저트는 복숭아를 먹었습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012179.1.3",
          "form": "오늘도 맛있는 디저트 드셨나요~?",
          "original_form": "오늘도 맛있는 디저트 드셨나요~?",
          "speaker_id": "2",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012179.1.4",
          "form": "저는 밥은 아직이라 이제 밥 먹으려구요",
          "original_form": "저는 밥은 아직이라 이제 밥 먹으려구요",
          "speaker_id": "1",
          "time": "20210824 20:34"
        },
        {
          "id": "MDRW2100012179.1.5",
          "form": "딱복 물복 어느 취향이신가요",
          "original_form": "딱복 물복 어느 취향이신가요",
          "speaker_id": "1",
          "time": "20210824 20:35"
        },
        {
          "id": "MDRW2100012179.1.6",
          "form": "둘다 좋아해요 ㅋㅋ 오늘먹은건 물복숭아였어요!",
          "original_form": "둘다 좋아해요 ㅋㅋ 오늘먹은건 물복숭아였어요!",
          "speaker_id": "2",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012179.1.7",
          "form": "달달달달 자체 ㅎㅎ",
          "original_form": "달달달달 자체 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012179.1.8",
          "form": "와 한동안 복숭아 안먹었는데ㅜ",
          "original_form": "와 한동안 복숭아 안먹었는데ㅜ",
          "speaker_id": "1",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100012179.1.9",
          "form": "맛있겠어요",
          "original_form": "맛있겠어요",
          "speaker_id": "1",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100012179.1.10",
          "form": "저는 물복파이지만",
          "original_form": "저는 물복파이지만",
          "speaker_id": "1",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100012179.1.11",
          "form": "복숭아 이즈 뭔들이지요ㅎ",
          "original_form": "복숭아 이즈 뭔들이지요ㅎ",
          "speaker_id": "1",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100012179.1.12",
          "form": "저도 요즘에는 과일 별로 안챙겨먹었거든요~",
          "original_form": "저도 요즘에는 과일 별로 안챙겨먹었거든요~",
          "speaker_id": "2",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100012179.1.13",
          "form": "근데 너무 맛있더라구요!!!!!",
          "original_form": "근데 너무 맛있더라구요!!!!!",
          "speaker_id": "2",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100012179.1.14",
          "form": "올여름에 수박을 맛있게 먹었어요 ㅋㅋ",
          "original_form": "올여름에 수박을 맛있게 먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100012179.1.15",
          "form": "샤인머스켓도 많이 먹던데 제 입맛엔 안맞더라구요ㅜㅜ",
          "original_form": "샤인머스켓도 많이 먹던데 제 입맛엔 안맞더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100012179.1.16",
          "form": "여름에 워낙 더워서 수박이 꿀맛이었죠ㅎ",
          "original_form": "여름에 워낙 더워서 수박이 꿀맛이었죠ㅎ",
          "speaker_id": "1",
          "time": "20210824 20:43"
        },
        {
          "id": "MDRW2100012179.1.17",
          "form": "진짜 수박은 정말 맛있는거 같아요 달콤하고 수분가득하고!",
          "original_form": "진짜 수박은 정말 맛있는거 같아요 달콤하고 수분가득하고!",
          "speaker_id": "2",
          "time": "20210824 20:44"
        },
        {
          "id": "MDRW2100012179.1.18",
          "form": "예전엔 몰랐는데 나이 들수록 좋아지는 과일이죠ㅎ",
          "original_form": "예전엔 몰랐는데 나이 들수록 좋아지는 과일이죠ㅎ",
          "speaker_id": "1",
          "time": "20210824 20:44"
        },
        {
          "id": "MDRW2100012179.1.19",
          "form": "쿠팡서 파인애플 손질된거 대량으로 사 냉장고에 넣어두고 먹으니 그것도 좋더라고요",
          "original_form": "쿠팡서 파인애플 손질된거 대량으로 사 냉장고에 넣어두고 먹으니 그것도 좋더라고요",
          "speaker_id": "1",
          "time": "20210824 20:45"
        },
        {
          "id": "MDRW2100012179.1.20",
          "form": "저는 어릴때 없어서 못먹었는데 요즘은 없으면 없는대로 .... 파에 가까워졌어요 ㅎㅎ",
          "original_form": "저는 어릴때 없어서 못먹었는데 요즘은 없으면 없는대로 .... 파에 가까워졌어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:45"
        },
        {
          "id": "MDRW2100012179.1.21",
          "form": "오~",
          "original_form": "오~",
          "speaker_id": "2",
          "time": "20210824 20:45"
        },
        {
          "id": "MDRW2100012179.1.22",
          "form": "진짜 요즘 너무 편리해요 ㅋㅋㅋ",
          "original_form": "진짜 요즘 너무 편리해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:45"
        },
        {
          "id": "MDRW2100012179.1.23",
          "form": "앗ㅎㅎ 저는 반대로 예전에는 굳이 과일을 이랬는데 요새는 챙겨먹어요",
          "original_form": "앗ㅎㅎ 저는 반대로 예전에는 굳이 과일을 이랬는데 요새는 챙겨먹어요",
          "speaker_id": "1",
          "time": "20210824 20:45"
        },
        {
          "id": "MDRW2100012179.1.24",
          "form": "그쵸\r\n파인애플 그렇게 파니 좋아요",
          "original_form": "그쵸\r\n파인애플 그렇게 파니 좋아요",
          "speaker_id": "1",
          "time": "20210824 20:46"
        },
        {
          "id": "MDRW2100012179.1.25",
          "form": "저는 예전에 어디더라... 티몬인가? 거기서 연어 키로그램으로 사다가 집에서 썰어먹었어요 ㅋㅋㅋㅋ",
          "original_form": "저는 예전에 어디더라... 티몬인가? 거기서 연어 키로그램으로 사다가 집에서 썰어먹었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:46"
        },
        {
          "id": "MDRW2100012179.1.26",
          "form": "와! 소스는 어떻게 하셨어요?",
          "original_form": "와! 소스는 어떻게 하셨어요?",
          "speaker_id": "1",
          "time": "20210824 20:46"
        },
        {
          "id": "MDRW2100012179.1.27",
          "form": "케이퍼같은 것",
          "original_form": "케이퍼같은 것",
          "speaker_id": "1",
          "time": "20210824 20:46"
        },
        {
          "id": "MDRW2100012179.1.28",
          "form": "같이 팔더라구요!",
          "original_form": "같이 팔더라구요!",
          "speaker_id": "2",
          "time": "20210824 20:48"
        }
      ]
