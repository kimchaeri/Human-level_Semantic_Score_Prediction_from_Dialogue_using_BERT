      "utterance": [
        {
          "id": "MDRW2100003462.1.1",
          "form": "어떤 썰이요?",
          "original_form": "어떤 썰이요?",
          "speaker_id": "2",
          "time": "20210820 23:20"
        },
        {
          "id": "MDRW2100003462.1.2",
          "form": "어떻게 만나셨는지? ㅋㅋ",
          "original_form": "어떻게 만나셨는지? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:22"
        },
        {
          "id": "MDRW2100003462.1.3",
          "form": "소개팅으로 만났어요",
          "original_form": "소개팅으로 만났어요",
          "speaker_id": "2",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003462.1.4",
          "form": "소개팅으로 만나서",
          "original_form": "소개팅으로 만나서",
          "speaker_id": "1",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003462.1.5",
          "form": "이리 오래 연애할줄이야 ㅋㅋ",
          "original_form": "이리 오래 연애할줄이야 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003462.1.6",
          "form": "신기하세요?ㅋㅋㅋㅋ",
          "original_form": "신기하세요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003462.1.7",
          "form": "5년했으면 할만큼 했죠?",
          "original_form": "5년했으면 할만큼 했죠?",
          "speaker_id": "2",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003462.1.8",
          "form": "네ㅋㅋ 신기방기해요ㅋㅋ",
          "original_form": "네ㅋㅋ 신기방기해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:27"
        },
        {
          "id": "MDRW2100003462.1.9",
          "form": "저는 자만추이기도하고",
          "original_form": "저는 자만추이기도하고",
          "speaker_id": "1",
          "time": "20210820 23:27"
        },
        {
          "id": "MDRW2100003462.1.10",
          "form": "긴 연애를 해본적이 없어서요 ㅋㅋ",
          "original_form": "긴 연애를 해본적이 없어서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:27"
        },
        {
          "id": "MDRW2100003462.1.11",
          "form": "제일 긴 연애가 몇개월?",
          "original_form": "제일 긴 연애가 몇개월?",
          "speaker_id": "2",
          "time": "20210820 23:28"
        },
        {
          "id": "MDRW2100003462.1.12",
          "form": "저는 1년반이었던거 같아요ㅋㅋ",
          "original_form": "저는 1년반이었던거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:29"
        },
        {
          "id": "MDRW2100003462.1.13",
          "form": "그정도 연애하고 결혼하는게 딱이죠",
          "original_form": "그정도 연애하고 결혼하는게 딱이죠",
          "speaker_id": "2",
          "time": "20210820 23:29"
        },
        {
          "id": "MDRW2100003462.1.14",
          "form": "저흰 어쩌다보니 길어졌어요 ㅋㅋㅋ",
          "original_form": "저흰 어쩌다보니 길어졌어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 23:29"
        },
        {
          "id": "MDRW2100003462.1.15",
          "form": "2년짜때 겁나 싸우고",
          "original_form": "2년짜때 겁나 싸우고",
          "speaker_id": "2",
          "time": "20210820 23:30"
        },
        {
          "id": "MDRW2100003462.1.16",
          "form": "5년  연애에도 위기는 있었네요? ㅋㅋ",
          "original_form": "5년  연애에도 위기는 있었네요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:30"
        },
        {
          "id": "MDRW2100003462.1.17",
          "form": "네 거의 헤어지기 직전까지",
          "original_form": "네 거의 헤어지기 직전까지",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100003462.1.18",
          "form": "미친듯이 싸웠어요 ㅋㅋ",
          "original_form": "미친듯이 싸웠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100003462.1.19",
          "form": "1년차때 진짜 하나도 안싸웠는데",
          "original_form": "1년차때 진짜 하나도 안싸웠는데",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100003462.1.20",
          "form": "주로 싸운 이유는요?",
          "original_form": "주로 싸운 이유는요?",
          "speaker_id": "1",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100003462.1.21",
          "form": "그때 너무 참은거 같아요",
          "original_form": "그때 너무 참은거 같아요",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100003462.1.22",
          "form": "참았던게 터진거죠",
          "original_form": "참았던게 터진거죠",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100003462.1.23",
          "form": "서로 참았던게 뻥 터진...ㅋㅋ",
          "original_form": "서로 참았던게 뻥 터진...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003462.1.24",
          "form": "그래도 그 시기를 잘 극복하셨네요!",
          "original_form": "그래도 그 시기를 잘 극복하셨네요!",
          "speaker_id": "1",
          "time": "20210820 23:36"
        }
      ]
    }
