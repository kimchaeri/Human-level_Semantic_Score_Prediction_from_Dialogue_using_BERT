      "utterance": [
        {
          "id": "MDRW2100027166.1.1",
          "form": "아침식사는 하셨나용",
          "original_form": "아침식사는 하셨나용",
          "speaker_id": "1",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100027166.1.2",
          "form": "아니요 ㅎㅎ",
          "original_form": "아니요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027166.1.3",
          "form": "콘푸로스트 한 주먹",
          "original_form": "콘푸로스트 한 주먹",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027166.1.4",
          "form": "먹긴했어요",
          "original_form": "먹긴했어요",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027166.1.5",
          "form": "어...왜 한주먹만 드신거죠!",
          "original_form": "어...왜 한주먹만 드신거죠!",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027166.1.6",
          "form": "감질나게",
          "original_form": "감질나게",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027166.1.7",
          "form": "우유를",
          "original_form": "우유를",
          "speaker_id": "2",
          "time": "20210902 11:07"
        },
        {
          "id": "MDRW2100027166.1.8",
          "form": "누가 다 먹어버려서 ㅠㅠ",
          "original_form": "누가 다 먹어버려서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 11:07"
        },
        {
          "id": "MDRW2100027166.1.9",
          "form": "ㅜㅇㅜ 아 우유 없음 못 먹죠!",
          "original_form": "ㅜㅇㅜ 아 우유 없음 못 먹죠!",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100027166.1.10",
          "form": "사실 그 우유 제가 먹은듯....",
          "original_form": "사실 그 우유 제가 먹은듯....",
          "speaker_id": "2",
          "time": "20210902 11:32"
        },
        {
          "id": "MDRW2100027166.1.11",
          "form": "아니ㅋ 어서 우유를 채워놓으셔야죠!",
          "original_form": "아니ㅋ 어서 우유를 채워놓으셔야죠!",
          "speaker_id": "1",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100027166.1.12",
          "form": "나가기가 귀찮아서 ㅋㅋㅋ",
          "original_form": "나가기가 귀찮아서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100027166.1.13",
          "form": "이따 나갈까봐요",
          "original_form": "이따 나갈까봐요",
          "speaker_id": "2",
          "time": "20210902 12:57"
        },
        {
          "id": "MDRW2100027166.1.14",
          "form": "ㅎㅎ 전 라이언역장님 영접하러 다시 나가려다 어머니\r\n께 등짝 스매싱맞고 붙잡혔습니다",
          "original_form": "ㅎㅎ 전 라이언역장님 영접하러 다시 나가려다 어머니\r\n께 등짝 스매싱맞고 붙잡혔습니다",
          "speaker_id": "1",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100027166.1.15",
          "form": "백신 맞은 게 어딜 싸돌아다니냐구ㅜ",
          "original_form": "백신 맞은 게 어딜 싸돌아다니냐구ㅜ",
          "speaker_id": "1",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100027166.1.16",
          "form": "집에 잠깐 들르는게 아닌데!",
          "original_form": "집에 잠깐 들르는게 아닌데!",
          "speaker_id": "1",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100027166.1.17",
          "form": "엄청 멀쩡한데!.",
          "original_form": "엄청 멀쩡한데!.",
          "speaker_id": "1",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027166.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ먹을거 사온다고 하시고",
          "original_form": "ㅋㅋㅋㅋㅋㅋ먹을거 사온다고 하시고",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027166.1.19",
          "form": "잠시 탈출해보시죠",
          "original_form": "잠시 탈출해보시죠",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027166.1.20",
          "form": "어머니가 나를 너무 잘 알아요ㅎ",
          "original_form": "어머니가 나를 너무 잘 알아요ㅎ",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027166.1.21",
          "form": "집앞 홈플러스조차 배달로 시켜먹는 집순이를ㅜ",
          "original_form": "집앞 홈플러스조차 배달로 시켜먹는 집순이를ㅜ",
          "speaker_id": "1",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100027166.1.22",
          "form": "그거 일요일에도 남아있을까요",
          "original_form": "그거 일요일에도 남아있을까요",
          "speaker_id": "1",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100027166.1.23",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100027166.1.24",
          "form": "라이언역장님이요?",
          "original_form": "라이언역장님이요?",
          "speaker_id": "2",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100027166.1.25",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100027166.1.26",
          "form": "재고가 더 들어오느냐가 관건인데",
          "original_form": "재고가 더 들어오느냐가 관건인데",
          "speaker_id": "2",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100027166.1.27",
          "form": "아님 온라인입고 안되나 뒤적뒤적",
          "original_form": "아님 온라인입고 안되나 뒤적뒤적",
          "speaker_id": "1",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100027166.1.28",
          "form": "제가 프리미엄 받고 팔려고요",
          "original_form": "제가 프리미엄 받고 팔려고요",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027166.1.29",
          "form": "^ㅅ^",
          "original_form": "^ㅅ^",
          "speaker_id": "1",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027166.1.30",
          "form": "경찰 아저씨 여기예욧!!!",
          "original_form": "경찰 아저씨 여기예욧!!!",
          "speaker_id": "1",
          "time": "20210902 13:32"
        }
      ]
