      "utterance": [
        {
          "id": "MDRW2100045062.1.1",
          "form": "저는 치킨보다 피자파",
          "original_form": "저는 치킨보다 피자파",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045062.1.2",
          "form": "영양밸런스도 더 좋죠",
          "original_form": "영양밸런스도 더 좋죠",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045062.1.3",
          "form": "피자도 밸런스는 안 좋은 것 같아요",
          "original_form": "피자도 밸런스는 안 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045062.1.4",
          "form": "그럴거면 샐러드 먹죠",
          "original_form": "그럴거면 샐러드 먹죠",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045062.1.5",
          "form": "치킨보단 나아요",
          "original_form": "치킨보단 나아요",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045062.1.6",
          "form": "그럼 치킨말고 샐러드 드세요",
          "original_form": "그럼 치킨말고 샐러드 드세요",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045062.1.7",
          "form": "그렇게 만들어진게 !",
          "original_form": "그렇게 만들어진게 !",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045062.1.8",
          "form": "그래봤자 치킨샐러드 먹을거면서 웃긴인간아 ㅋㅋ",
          "original_form": "그래봤자 치킨샐러드 먹을거면서 웃긴인간아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045062.1.9",
          "form": "치킨샐러드",
          "original_form": "치킨샐러드",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045062.1.10",
          "form": "그것도 좋아합니다",
          "original_form": "그것도 좋아합니다",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045062.1.11",
          "form": "봐봐",
          "original_form": "봐봐",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045062.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045062.1.13",
          "form": "아니 좀 웃긴인간으로 살면 어때요",
          "original_form": "아니 좀 웃긴인간으로 살면 어때요",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045062.1.14",
          "form": "맛난것만 먹으면 장땡 아닌가요",
          "original_form": "맛난것만 먹으면 장땡 아닌가요",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045062.1.15",
          "form": "밸런스 어디감?",
          "original_form": "밸런스 어디감?",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045062.1.16",
          "form": "걍 피자먹어요",
          "original_form": "걍 피자먹어요",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045062.1.17",
          "form": "피자는 건강해",
          "original_form": "피자는 건강해",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045062.1.18",
          "form": "거짓말 치지마요",
          "original_form": "거짓말 치지마요",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045062.1.19",
          "form": "고르곤졸라나",
          "original_form": "고르곤졸라나",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045062.1.20",
          "form": "꿀도요",
          "original_form": "꿀도요",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045062.1.21",
          "form": "그건 탄수화물 범벅이잖아요",
          "original_form": "그건 탄수화물 범벅이잖아요",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045062.1.22",
          "form": "뭐가 영양이죠",
          "original_form": "뭐가 영양이죠",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045062.1.23",
          "form": "건강식품임",
          "original_form": "건강식품임",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045062.1.24",
          "form": "츠지도 몸에 좋지만 지방이 섞여있어서 고칼로리 라구오",
          "original_form": "츠지도 몸에 좋지만 지방이 섞여있어서 고칼로리 라구오",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045062.1.25",
          "form": "뭐든 과하면 문제입니다",
          "original_form": "뭐든 과하면 문제입니다",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045062.1.26",
          "form": "건강식품을 밀도우위에 올려먹음 피자에요",
          "original_form": "건강식품을 밀도우위에 올려먹음 피자에요",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045062.1.27",
          "form": "피자는 치즈가 투머치예요",
          "original_form": "피자는 치즈가 투머치예요",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045062.1.28",
          "form": "요즘 영양제도 챙겨먹어요",
          "original_form": "요즘 영양제도 챙겨먹어요",
          "speaker_id": "1",
          "time": "20210908 00:46"
        }
      ]
    }
  ]
