      "utterance": [
        {
          "id": "MDRW2100021230.1.1",
          "form": "안짱다리를 고치고싶어요ㅠ",
          "original_form": "안짱다리를 고치고싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021230.1.2",
          "form": "아 저 어릴때",
          "original_form": "아 저 어릴때",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021230.1.3",
          "form": "안짱으로걸어서",
          "original_form": "안짱으로걸어서",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021230.1.4",
          "form": "신발안쪽만닳고그랬어요",
          "original_form": "신발안쪽만닳고그랬어요",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021230.1.5",
          "form": "저 지금 그래요ㅠㅠ",
          "original_form": "저 지금 그래요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021230.1.6",
          "form": "고치셨나요?",
          "original_form": "고치셨나요?",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021230.1.7",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021230.1.8",
          "form": "일부러 팔자로 걸었어요",
          "original_form": "일부러 팔자로 걸었어요",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021230.1.9",
          "form": "우와 그럼 고쳐지나요?",
          "original_form": "우와 그럼 고쳐지나요?",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021230.1.10",
          "form": "네!! 신기하게 이제 신발안쪽이",
          "original_form": "네!! 신기하게 이제 신발안쪽이",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021230.1.11",
          "form": "닳지않더라고요",
          "original_form": "닳지않더라고요",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021230.1.12",
          "form": "오오오! 완전 신기하네요",
          "original_form": "오오오! 완전 신기하네요",
          "speaker_id": "2",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100021230.1.13",
          "form": "내일부터 그렇게 걸어봐야겠어요",
          "original_form": "내일부터 그렇게 걸어봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100021230.1.14",
          "form": "근데 부작용 어느순간 팔자가 돼요",
          "original_form": "근데 부작용 어느순간 팔자가 돼요",
          "speaker_id": "1",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100021230.1.15",
          "form": "다시 안으로 걷기해서 일자가됐습니다이젠",
          "original_form": "다시 안으로 걷기해서 일자가됐습니다이젠",
          "speaker_id": "1",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100021230.1.16",
          "form": "오오... 여러 단계를 거치셨군요",
          "original_form": "오오... 여러 단계를 거치셨군요",
          "speaker_id": "2",
          "time": "20210829 14:49"
        },
        {
          "id": "MDRW2100021230.1.17",
          "form": "오늘 신경쓰면서 걸으니 좀 나은거같다시더라구요",
          "original_form": "오늘 신경쓰면서 걸으니 좀 나은거같다시더라구요",
          "speaker_id": "2",
          "time": "20210829 14:50"
        },
        {
          "id": "MDRW2100021230.1.18",
          "form": "신경써서 걸으면",
          "original_form": "신경써서 걸으면",
          "speaker_id": "1",
          "time": "20210829 14:50"
        },
        {
          "id": "MDRW2100021230.1.19",
          "form": "확실히 더 운동도 되는것같아요",
          "original_form": "확실히 더 운동도 되는것같아요",
          "speaker_id": "1",
          "time": "20210829 14:50"
        },
        {
          "id": "MDRW2100021230.1.20",
          "form": "허리도 더 펴지고",
          "original_form": "허리도 더 펴지고",
          "speaker_id": "1",
          "time": "20210829 14:50"
        },
        {
          "id": "MDRW2100021230.1.21",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210829 14:57"
        },
        {
          "id": "MDRW2100021230.1.22",
          "form": "걸을때 뒤쿰치가 먼저 안닿는것도 알게됐어요!",
          "original_form": "걸을때 뒤쿰치가 먼저 안닿는것도 알게됐어요!",
          "speaker_id": "2",
          "time": "20210829 14:58"
        },
        {
          "id": "MDRW2100021230.1.23",
          "form": "어 저도! 엄청대충걸었는데",
          "original_form": "어 저도! 엄청대충걸었는데",
          "speaker_id": "1",
          "time": "20210829 15:00"
        },
        {
          "id": "MDRW2100021230.1.24",
          "form": "친구가말해준 후로 곧게걸으니까",
          "original_form": "친구가말해준 후로 곧게걸으니까",
          "speaker_id": "1",
          "time": "20210829 15:00"
        },
        {
          "id": "MDRW2100021230.1.25",
          "form": "살도빠지더라고요",
          "original_form": "살도빠지더라고요",
          "speaker_id": "1",
          "time": "20210829 15:00"
        }
      ]
    }
  ]
