      "utterance": [
        {
          "id": "MDRW2100045187.1.1",
          "form": "ㅋㅈㅋㅈ진짜 이시국 연애도 신기하고ㅋㅋ 근데 결혼까지 가는거 보면 인연은 역시",
          "original_form": "ㅋㅈㅋㅈ진짜 이시국 연애도 신기하고ㅋㅋ 근데 결혼까지 가는거 보면 인연은 역시",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045187.1.2",
          "form": "저도이시국에누가애낳나했는데",
          "original_form": "저도이시국에누가애낳나했는데",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045187.1.3",
          "form": "둘째출산ㅋㅋㅋ",
          "original_form": "둘째출산ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045187.1.4",
          "form": "name1님이요?",
          "original_form": "&name1&님이요?",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045187.1.5",
          "form": "첫찌 유치원엄마들도 마스크끼고만나서 마스크벗으면아직도적응안되요ㅋㅋ",
          "original_form": "첫찌 유치원엄마들도 마스크끼고만나서 마스크벗으면아직도적응안되요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045187.1.6",
          "form": "축하드립니다!!",
          "original_form": "축하드립니다!!",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045187.1.7",
          "form": "넼ㅋㅋ 난",
          "original_form": "넼ㅋㅋ 난",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045187.1.8",
          "form": "회사동료들도ㅋㅋ",
          "original_form": "회사동료들도ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045187.1.9",
          "form": "낯설어요",
          "original_form": "낯설어요",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045187.1.10",
          "form": "회사도글쿤요",
          "original_form": "회사도글쿤요",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045187.1.11",
          "form": "네ㅋㅋ 애들이 제일 힘들죠ㅜ",
          "original_form": "네ㅋㅋ 애들이 제일 힘들죠ㅜ",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045187.1.12",
          "form": "94쓰기싫대요",
          "original_form": "94쓰기싫대요",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045187.1.13",
          "form": "비말만썼더니ㅠ",
          "original_form": "비말만썼더니ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045187.1.14",
          "form": "밖에마스크안쓰고나가면 손으로코막아요ㅠ",
          "original_form": "밖에마스크안쓰고나가면 손으로코막아요ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045187.1.15",
          "form": "불쌍",
          "original_form": "불쌍",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045187.1.16",
          "form": "빨리 좋아져야하는데요ㅜ",
          "original_form": "빨리 좋아져야하는데요ㅜ",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045187.1.17",
          "form": "말도 늦게 배운대요ㅜ",
          "original_form": "말도 늦게 배운대요ㅜ",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045187.1.18",
          "form": "어린아기는 최대한집에데꼬있는게맞는거같아요",
          "original_form": "어린아기는 최대한집에데꼬있는게맞는거같아요",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045187.1.19",
          "form": "엄마입보면서 말배우게해야죠ㅠ",
          "original_form": "엄마입보면서 말배우게해야죠ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045187.1.20",
          "form": "보통 몇시에 주무세요?",
          "original_form": "보통 몇시에 주무세요?",
