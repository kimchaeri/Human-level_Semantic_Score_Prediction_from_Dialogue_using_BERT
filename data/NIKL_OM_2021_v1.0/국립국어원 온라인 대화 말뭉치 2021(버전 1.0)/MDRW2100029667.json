      "utterance": [
        {
          "id": "MDRW2100029667.1.1",
          "form": "요새 무슨책보세요?",
          "original_form": "요새 무슨책보세요?",
          "speaker_id": "1",
          "time": "20210903 12:02"
        },
        {
          "id": "MDRW2100029667.1.2",
          "form": "저는 마아라의 편지라는 종교책 최근에 봤네용",
          "original_form": "저는 마아라의 편지라는 종교책 최근에 봤네용",
          "speaker_id": "2",
          "time": "20210903 14:32"
        },
        {
          "id": "MDRW2100029667.1.3",
          "form": "무슨내용인가요?",
          "original_form": "무슨내용인가요?",
          "speaker_id": "1",
          "time": "20210903 14:37"
        },
        {
          "id": "MDRW2100029667.1.4",
          "form": "마아라는 불교에서 말하는 마군이, 마왕인데요",
          "original_form": "마아라는 불교에서 말하는 마군이, 마왕인데요",
          "speaker_id": "2",
          "time": "20210903 14:49"
        },
        {
          "id": "MDRW2100029667.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100029667.1.6",
          "form": "마왕요",
          "original_form": "마왕요",
          "speaker_id": "1",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100029667.1.7",
          "form": "타화자재천이라는 건데 뭐든지 자기마음대로 이루어진다 뜻이에요",
          "original_form": "타화자재천이라는 건데 뭐든지 자기마음대로 이루어진다 뜻이에요",
          "speaker_id": "2",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100029667.1.8",
          "form": "자기맘대로?",
          "original_form": "자기맘대로?",
          "speaker_id": "1",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100029667.1.9",
          "form": "이 마왕이 고타마싯다르타를",
          "original_form": "이 마왕이 고타마싯다르타를",
          "speaker_id": "2",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100029667.1.10",
          "form": "그게 되나요?",
          "original_form": "그게 되나요?",
          "speaker_id": "1",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100029667.1.11",
          "form": "설화인가요?",
          "original_form": "설화인가요?",
          "speaker_id": "1",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100029667.1.12",
          "form": "그니까 번뇌의 마왕인거죵",
          "original_form": "그니까 번뇌의 마왕인거죵",
          "speaker_id": "2",
          "time": "20210903 14:51"
        },
        {
          "id": "MDRW2100029667.1.13",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210903 14:51"
        },
        {
          "id": "MDRW2100029667.1.14",
          "form": "불교에서 말하는",
          "original_form": "불교에서 말하는",
          "speaker_id": "2",
          "time": "20210903 14:51"
        },
        {
          "id": "MDRW2100029667.1.15",
          "form": "심오하네요",
          "original_form": "심오하네요",
