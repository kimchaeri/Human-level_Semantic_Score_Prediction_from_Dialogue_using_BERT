          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028982.1.3",
          "form": "뭐야 갑자기 왜바뀌어요 ㅠ",
          "original_form": "뭐야 갑자기 왜바뀌어요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028982.1.4",
          "form": "갑자기바꼇다",
          "original_form": "갑자기바꼇다",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028982.1.5",
          "form": "저 방금 대패삼겹먹음",
          "original_form": "저 방금 대패삼겹먹음",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028982.1.6",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028982.1.7",
          "form": "전 마지막에 먹은 게 찹쌀떡이예요",
          "original_form": "전 마지막에 먹은 게 찹쌀떡이예요",
          "speaker_id": "2",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028982.1.8",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028982.1.9",
          "form": "저녁안드셨어요?",
          "original_form": "저녁안드셨어요?",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028982.1.10",
          "form": "모찌가게에서 엄마가 사왔어",
          "original_form": "모찌가게에서 엄마가 사왔어",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028982.1.11",
          "form": "과일모찌",
          "original_form": "과일모찌",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028982.1.12",
          "form": "모찌모찌",
          "original_form": "모찌모찌",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028982.1.13",
          "form": "먹었죠 그런데 2시에.",
          "original_form": "먹었죠 그런데 2시에.",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028982.1.14",
          "form": "저녁",
          "original_form": "저녁",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028982.1.15",
          "form": "먹으라고",
          "original_form": "먹으라고",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028982.1.16",
          "form": "~~~",
          "original_form": "~~~",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028982.1.17",
          "form": "아니 근데...",
          "original_form": "아니 근데...",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028982.1.18",
          "form": "떡은 배불러요",
          "original_form": "떡은 배불러요",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028982.1.19",
          "form": "그 찹쌀떡 큰거 3개나 먹었어요",
          "original_form": "그 찹쌀떡 큰거 3개나 먹었어요",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028982.1.20",
          "form": "지금도 배부르세요?",
          "original_form": "지금도 배부르세요?",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028982.1.21",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028982.1.22",
          "form": "그럴리가",
          "original_form": "그럴리가",
          "speaker_id": "2",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028982.1.23",
          "form": "그치만",
          "original_form": "그치만",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028982.1.24",
          "form": "시간이늦어서",
          "original_form": "시간이늦어서",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028982.1.25",
          "form": "못먹겠다",
          "original_form": "못먹겠다",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028982.1.26",
          "form": "어짜피 저 야식 안먹어요",
          "original_form": "어짜피 저 야식 안먹어요",
          "speaker_id": "2",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028982.1.27",
          "form": "로제떡볶이 먹고싶다",
          "original_form": "로제떡볶이 먹고싶다",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028982.1.28",
          "form": "야식먹으면 건강에 나빠요",
          "original_form": "야식먹으면 건강에 나빠요",
          "speaker_id": "2",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028982.1.29",
          "form": "그리고 무엇보다 저 내일 건강검진 받으러 갈거라",
          "original_form": "그리고 무엇보다 저 내일 건강검진 받으러 갈거라",
          "speaker_id": "2",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028982.1.30",
          "form": "3시간 이후에 자면 저녁이잖아요",
          "original_form": "3시간 이후에 자면 저녁이잖아요",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028982.1.31",
          "form": "금식이예요",
          "original_form": "금식이예요",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028982.1.32",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028982.1.33",
          "form": "못먹네",
          "original_form": "못먹네",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028982.1.34",
          "form": "물도 마시면 안된대요",
          "original_form": "물도 마시면 안된대요",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028982.1.35",
          "form": "그건 오전인가 그랬어요",
          "original_form": "그건 오전인가 그랬어요",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028982.1.36",
          "form": "저녁엔 가능한듯?",
          "original_form": "저녁엔 가능한듯?",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028982.1.37",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028982.1.38",
          "form": "저 고민이에요",
          "original_form": "저 고민이에요",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028982.1.39",
          "form": "저 물 못마시면 죽음",
          "original_form": "저 물 못마시면 죽음",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028982.1.40",
          "form": "목말라서",
          "original_form": "목말라서",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028982.1.41",
          "form": "배민킬때마다",
          "original_form": "배민킬때마다",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028982.1.42",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028982.1.43",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028982.1.44",
          "form": "그럼 배민을 삭제하면 되겠다",
          "original_form": "그럼 배민을 삭제하면 되겠다",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028982.1.45",
          "form": "뭐가나쁜말이야",
          "original_form": "뭐가나쁜말이야",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028982.1.46",
          "form": "로제떡볶이 시키구싶은데",
          "original_form": "로제떡볶이 시키구싶은데",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028982.1.47",
          "form": "안먹어본거랑",
          "original_form": "안먹어본거랑",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028982.1.48",
          "form": "이미먹어봣는데",
          "original_form": "이미먹어봣는데",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028982.1.49",
          "form": "사진 안찍어서 블로그 못올리는거",
          "original_form": "사진 안찍어서 블로그 못올리는거",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028982.1.50",
          "form": "중에 고민함",
          "original_form": "중에 고민함",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028982.1.51",
          "form": "우와...",
          "original_form": "우와...",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028982.1.52",
          "form": "더 먹고싶은 순서대로 먹어요",
          "original_form": "더 먹고싶은 순서대로 먹어요",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028982.1.53",
          "form": "어짜피 다먹을거잔아",
          "original_form": "어짜피 다먹을거잔아",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028982.1.54",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028982.1.55",
          "form": "이미 블로그에올린거먹어야됨ㅠ",
          "original_form": "이미 블로그에올린거먹어야됨ㅠ",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028982.1.56",
          "form": "미치겠다",
          "original_form": "미치겠다",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028982.1.57",
          "form": "그냥... 먹어요",
          "original_form": "그냥... 먹어요",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028982.1.58",
          "form": "먹고싶은걸",
          "original_form": "먹고싶은걸",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028982.1.59",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028982.1.60",
          "form": "그럼 다음에 시킬게",
          "original_form": "그럼 다음에 시킬게",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028982.1.61",
          "form": "로제닭볶이랑... 또 머지",
          "original_form": "로제닭볶이랑... 또 머지",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028982.1.62",
          "form": "전 이번주에만 마라탕 세 번 먹게되거든요",
          "original_form": "전 이번주에만 마라탕 세 번 먹게되거든요",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028982.1.63",
          "form": "머있었는데",
          "original_form": "머있었는데",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028982.1.64",
          "form": "마싱ㅆ겠다",
          "original_form": "마싱ㅆ겠다",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028982.1.65",
          "form": "그럼 마라로제떡볶이랑",
          "original_form": "그럼 마라로제떡볶이랑",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028982.1.66",
          "form": "로제닭볶이중에",
          "original_form": "로제닭볶이중에",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028982.1.67",
          "form": "골라주세요",
          "original_form": "골라주세요",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028982.1.68",
          "form": "한번은 만들어먹었고 한번은 친구랑 마오차이가게갔고 나머지 한번은 토요일에 언니가 만들어준댔음",
          "original_form": "한번은 만들어먹었고 한번은 친구랑 마오차이가게갔고 나머지 한번은 토요일에 언니가 만들어준댔음",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028982.1.69",
          "form": "마라 궁금하니까 먹어주세요",
          "original_form": "마라 궁금하니까 먹어주세요",
          "speaker_id": "2",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028982.1.70",
          "form": "난토요일에 중국음식 먹는데",
          "original_form": "난토요일에 중국음식 먹는데",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028982.1.71",
          "form": "ㅋㅋㅋㅋ좋아요",
          "original_form": "ㅋㅋㅋㅋ좋아요",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028982.1.72",
          "form": "전 중국음식... 느끼해서 잘 못먹어요",
