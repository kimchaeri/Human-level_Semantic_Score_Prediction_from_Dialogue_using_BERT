      "utterance": [
        {
          "id": "MDRW2100015729.1.1",
          "form": "안녕하세여!",
          "original_form": "안녕하세여!",
          "speaker_id": "1",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015729.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015729.1.3",
          "form": "요새 티비 프로그램 보세요?",
          "original_form": "요새 티비 프로그램 보세요?",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015729.1.4",
          "form": "음... 런닝맨이랑 식스센스 봐요!",
          "original_form": "음... 런닝맨이랑 식스센스 봐요!",
          "speaker_id": "2",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015729.1.5",
          "form": "저도  그 두개 좋아해요!",
          "original_form": "저도  그 두개 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015729.1.6",
          "form": "티빙으러 보고있어요ㅎㅎ",
          "original_form": "티빙으러 보고있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015729.1.7",
          "form": "name2님은 넷플릭스 결제 하셨어요?",
          "original_form": "&name2&님은 넷플릭스 결제 하셨어요?",
          "speaker_id": "1",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015729.1.8",
          "form": "넹넹 ㅋㅋㅋㅋㅋㅋ근데 ㅠㅠ 결제만해놓고 요근래 본게없어욬ㅋㅋㅋ",
          "original_form": "넹넹 ㅋㅋㅋㅋㅋㅋ근데 ㅠㅠ 결제만해놓고 요근래 본게없어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015729.1.9",
          "form": "저두요ㅋㅋㅋㅋ자기 바빠서ㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋㅋ자기 바빠서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015729.1.10",
          "form": "은든히 넷플릭스 볼게 없더라고요",
          "original_form": "은든히 넷플릭스 볼게 없더라고요",
          "speaker_id": "1",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015729.1.11",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100015729.1.12",
          "form": "나중에 이거봐야지~~ 하고 찜하는 용도로만",
          "original_form": "나중에 이거봐야지~~ 하고 찜하는 용도로만",
          "speaker_id": "2",
          "time": "20210826 00:45"
        },
        {
          "id": "MDRW2100015729.1.13",
          "form": "들락날락해욬ㅋㅋㅋㅋ",
          "original_form": "들락날락해욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:45"
        },
        {
          "id": "MDRW2100015729.1.14",
          "form": "저도욬ㅋㅋㅋㅋ뭐볼지만 제일 시간 많이 걸리고",
          "original_form": "저도욬ㅋㅋㅋㅋ뭐볼지만 제일 시간 많이 걸리고",
          "speaker_id": "1",
          "time": "20210826 00:45"
        },
        {
          "id": "MDRW2100015729.1.15",
          "form": "정작 보지는 않고",
          "original_form": "정작 보지는 않고",
          "speaker_id": "1",
          "time": "20210826 00:45"
        },
        {
          "id": "MDRW2100015729.1.16",
          "form": "맞아요... 사실 안본지 한참됐는데...",
          "original_form": "맞아요... 사실 안본지 한참됐는데...",
          "speaker_id": "2",
          "time": "20210826 00:45"
        },
        {
          "id": "MDRW2100015729.1.17",
          "form": "음원스트리밍 하는것도 따로 결제하세요??",
          "original_form": "음원스트리밍 하는것도 따로 결제하세요??",
          "speaker_id": "1",
          "time": "20210826 00:45"
        },
        {
          "id": "MDRW2100015729.1.18",
          "form": "아 저 음원스트리밍은 딱히 안해욬ㅋㅋㅋㅋ 유튜브로 노래 들어요",
          "original_form": "아 저 음원스트리밍은 딱히 안해욬ㅋㅋㅋㅋ 유튜브로 노래 들어요",
          "speaker_id": "2",
          "time": "20210826 00:45"
        },
        {
          "id": "MDRW2100015729.1.19",
          "form": "저는 그것도 결제해놓고 잘 안들어요",
          "original_form": "저는 그것도 결제해놓고 잘 안들어요",
          "speaker_id": "1",
          "time": "20210826 00:46"
        },
        {
          "id": "MDRW2100015729.1.20",
          "form": "요즘 유튜브로 다 할수 있죠",
          "original_form": "요즘 유튜브로 다 할수 있죠",
