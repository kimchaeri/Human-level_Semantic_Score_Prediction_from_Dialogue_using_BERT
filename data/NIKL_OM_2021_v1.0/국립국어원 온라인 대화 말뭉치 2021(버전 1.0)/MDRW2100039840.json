      "utterance": [
        {
          "id": "MDRW2100039840.1.1",
          "form": "최근에 사고 싶은 물건 있으신가요?",
          "original_form": "최근에 사고 싶은 물건 있으신가요?",
          "speaker_id": "1",
          "time": "20210903 21:58"
        },
        {
          "id": "MDRW2100039840.1.2",
          "form": "전 갤워치가 너무 사고 싶네요...",
          "original_form": "전 갤워치가 너무 사고 싶네요...",
          "speaker_id": "1",
          "time": "20210903 21:59"
        },
        {
          "id": "MDRW2100039840.1.3",
          "form": "저는 태블릿이나 노트북사고 싶어요!",
          "original_form": "저는 태블릿이나 노트북사고 싶어요!",
          "speaker_id": "2",
          "time": "20210903 21:59"
        },
        {
          "id": "MDRW2100039840.1.4",
          "form": "노트북은 지금쓰던게 낡았고 태블릿은 아예 새로 사야해요",
          "original_form": "노트북은 지금쓰던게 낡았고 태블릿은 아예 새로 사야해요",
          "speaker_id": "2",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100039840.1.5",
          "form": "앗 둘다 좋죠~ 뭘로 사고 싶으세요 브랜드??",
          "original_form": "앗 둘다 좋죠~ 뭘로 사고 싶으세요 브랜드??",
          "speaker_id": "1",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100039840.1.6",
          "form": "저도 노트북 한 5년 써서 슬슬 보내줘야겠네요...ㅋㅋㅋ쿠ㅜ",
          "original_form": "저도 노트북 한 5년 써서 슬슬 보내줘야겠네요...ㅋㅋㅋ쿠ㅜ",
          "speaker_id": "1",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100039840.1.7",
          "form": "태블릿은 삼성 갤럭시탭 생각하고 있고",
          "original_form": "태블릿은 삼성 갤럭시탭 생각하고 있고",
          "speaker_id": "2",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100039840.1.8",
          "form": "노트북은 모르겠어요ㅋㅋㅋㅋ 브랜드가 워낙 다양해서",
          "original_form": "노트북은 모르겠어요ㅋㅋㅋㅋ 브랜드가 워낙 다양해서",
          "speaker_id": "2",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100039840.1.9",
          "form": "주위에선 그램 많이 쓰는데 무겁더라도 성능이 좋은걸 사고싶거든요!",
          "original_form": "주위에선 그램 많이 쓰는데 무겁더라도 성능이 좋은걸 사고싶거든요!",
          "speaker_id": "2",
          "time": "20210903 22:03"
        },
        {
          "id": "MDRW2100039840.1.10",
          "form": "오 게이밍 노트북 알아보시는 거예요?",
          "original_form": "오 게이밍 노트북 알아보시는 거예요?",
          "speaker_id": "1",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039840.1.11",
          "form": "마자요 요즘은 정말 아수스? 뭐 그런 것두 있고 다양하더라구요",
          "original_form": "마자요 요즘은 정말 아수스? 뭐 그런 것두 있고 다양하더라구요",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039840.1.12",
          "form": "아직 알아보진 못했어요ㅋㅋㅋㅋ",
          "original_form": "아직 알아보진 못했어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039840.1.13",
          "form": "너무비싸서 엄두가 안나서",
          "original_form": "너무비싸서 엄두가 안나서",
          "speaker_id": "2",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039840.1.14",
          "form": "아 그쵸ㅠㅜㅜ눈물만....",
          "original_form": "아 그쵸ㅠㅜㅜ눈물만....",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039840.1.15",
          "form": "그래도 진지하게 알아보면 적당한 가격에 괜찮은 모델이",
          "original_form": "그래도 진지하게 알아보면 적당한 가격에 괜찮은 모델이",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039840.1.16",
          "form": "비트코인 유행한 이후로 노트북 가격이 너무 올라서",
          "original_form": "비트코인 유행한 이후로 노트북 가격이 너무 올라서",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039840.1.17",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039840.1.18",
          "form": "있지 않을까 싶어요!",
          "original_form": "있지 않을까 싶어요!",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039840.1.19",
          "form": "비트코인ㅠㅠㅠㅠ",
          "original_form": "비트코인ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039840.1.20",
          "form": "그거때문에 그래픽카드 가격이 폭등했다더라구요",
          "original_form": "그거때문에 그래픽카드 가격이 폭등했다더라구요",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039840.1.21",
          "form": "그래서 그런지 기술은 발전하는데 노트북 가격은 항상 비싼 것 같고",
          "original_form": "그래서 그런지 기술은 발전하는데 노트북 가격은 항상 비싼 것 같고",
          "speaker_id": "2",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039840.1.22",
          "form": "맞아요....눈물나요",
          "original_form": "맞아요....눈물나요",
          "speaker_id": "1",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039840.1.23",
          "form": "정말 큰 맘먹고 사야하는...",
          "original_form": "정말 큰 맘먹고 사야하는...",
          "speaker_id": "1",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039840.1.24",
          "form": "하 그치만 마음만은 이미 맥북입니다",
          "original_form": "하 그치만 마음만은 이미 맥북입니다",
          "speaker_id": "1",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039840.1.25",
          "form": "그리고 하나사면 오래써야만 할 것 같죠",
          "original_form": "그리고 하나사면 오래써야만 할 것 같죠",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039840.1.26",
          "form": "맥북 너무 예쁘더라구요",
          "original_form": "맥북 너무 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039840.1.27",
          "form": "전 매킨토시에 적응을 못할 것 같지만ㅋㄱㅋㅠ",
          "original_form": "전 매킨토시에 적응을 못할 것 같지만ㅋㄱㅋㅠ",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039840.1.28",
          "form": "사과로고가 진짜 예쁘긴 예뻐요!",
          "original_form": "사과로고가 진짜 예쁘긴 예뻐요!",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039840.1.29",
          "form": "저두요ㅋㅋㅋㅋㅋ그냥 디자인이랑",
          "original_form": "저두요ㅋㅋㅋㅋㅋ그냥 디자인이랑",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039840.1.30",
          "form": "다들 뭔가 좋다던데 이유가 있지 않을까?",
          "original_form": "다들 뭔가 좋다던데 이유가 있지 않을까?",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039840.1.31",
          "form": "하는 마음에 궁금한ㅋㅋㅋ거 같아용",
          "original_form": "하는 마음에 궁금한ㅋㅋㅋ거 같아용",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039840.1.32",
          "form": "마자요 사과 짱...",
          "original_form": "마자요 사과 짱...",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039840.1.33",
          "form": "디자인이 예쁘면 이미 가치를 반쯤 다 한거죠",
          "original_form": "디자인이 예쁘면 이미 가치를 반쯤 다 한거죠",
          "speaker_id": "2",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039840.1.34",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039840.1.35",
          "form": "어차피 애플인 이상 성능에 신경은 썼을테고",
          "original_form": "어차피 애플인 이상 성능에 신경은 썼을테고",
          "speaker_id": "2",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039840.1.36",
          "form": "맞아요 애플이 주는 믿음이 또 있어서",
          "original_form": "맞아요 애플이 주는 믿음이 또 있어서",
          "speaker_id": "1",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100039840.1.37",
          "form": "저는 비록 아이패드도 적응하지 못했지만ㅋㅋㅋㅠ",
          "original_form": "저는 비록 아이패드도 적응하지 못했지만ㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100039840.1.38",
          "form": "애플 기계는 오래 안정적으로 쓸 수 있을 것 같단 믿음은 굳건해요",
          "original_form": "애플 기계는 오래 안정적으로 쓸 수 있을 것 같단 믿음은 굳건해요",
