      "utterance": [
        {
          "id": "MDRW2100006280.1.1",
          "form": "오늘 비 많이 왔어요",
          "original_form": "오늘 비 많이 왔어요",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006280.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006280.1.3",
          "form": "비가 와서 그런가 쌀쌀해",
          "original_form": "비가 와서 그런가 쌀쌀해",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006280.1.4",
          "form": "진짜요 엄청 쌀쌀하죠",
          "original_form": "진짜요 엄청 쌀쌀하죠",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006280.1.5",
          "form": "밤에는 정말 이불 덮고 자야 돼",
          "original_form": "밤에는 정말 이불 덮고 자야 돼",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006280.1.6",
          "form": "국물이 땡겨요",
          "original_form": "국물이 땡겨요",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006280.1.7",
          "form": "낮에도 긴팔을 입어도 되겠어",
          "original_form": "낮에도 긴팔을 입어도 되겠어",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006280.1.8",
          "form": "전 얼마전부터 이불 싸매고 자요",
          "original_form": "전 얼마전부터 이불 싸매고 자요",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006280.1.9",
          "form": "근데 모기가 많아서 선풍기를 틀고 싶어",
          "original_form": "근데 모기가 많아서 선풍기를 틀고 싶어",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006280.1.10",
          "form": "긴팔 입어도 돼요!!",
          "original_form": "긴팔 입어도 돼요!!",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006280.1.11",
          "form": "선풍기는 좀 오바",
          "original_form": "선풍기는 좀 오바",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006280.1.12",
          "form": "비가 안 오면 낮에는 낮에는 또 가끔 더워",
          "original_form": "비가 안 오면 낮에는 낮에는 또 가끔 더워",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006280.1.13",
          "form": "맞아요 이상해 날씨가",
          "original_form": "맞아요 이상해 날씨가",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006280.1.14",
          "form": "가을 장마라는 말이 맞아",
          "original_form": "가을 장마라는 말이 맞아",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006280.1.15",
          "form": "장마가 끝나야 될텐데요",
          "original_form": "장마가 끝나야 될텐데요",
          "speaker_id": "2",
