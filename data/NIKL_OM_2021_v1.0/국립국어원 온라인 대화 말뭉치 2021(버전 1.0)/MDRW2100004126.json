      "utterance": [
        {
          "id": "MDRW2100004126.1.1",
          "form": "저는 고양이 알러지가 있어요...ㅠ",
          "original_form": "저는 고양이 알러지가 있어요...ㅠ",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004126.1.2",
          "form": "근데 고양이 엄청 좋아함",
          "original_form": "근데 고양이 엄청 좋아함",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004126.1.3",
          "form": "저도 안키우는데",
          "original_form": "저도 안키우는데",
          "speaker_id": "2",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004126.1.4",
          "form": "고양이 엄청 좋아해요",
          "original_form": "고양이 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004126.1.5",
          "form": "아 고양이 최고죠 진짜ㅠㅠ",
          "original_form": "아 고양이 최고죠 진짜ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004126.1.6",
          "form": "강아지 vs 고양이",
          "original_form": "강아지 vs 고양이",
          "speaker_id": "1",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004126.1.7",
          "form": "전 고양이파요",
          "original_form": "전 고양이파요",
          "speaker_id": "2",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004126.1.8",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004126.1.9",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004126.1.10",
          "form": "약간 시크하면서 귀엽고 앙큼한 매력이 있죠",
          "original_form": "약간 시크하면서 귀엽고 앙큼한 매력이 있죠",
          "speaker_id": "1",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004126.1.11",
          "form": "예전에는 강아지였는데",
          "original_form": "예전에는 강아지였는데",
          "speaker_id": "2",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004126.1.12",
          "form": "고양이로 바꼈습니다",
          "original_form": "고양이로 바꼈습니다",
          "speaker_id": "2",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004126.1.13",
          "form": "오오 계기는요??",
          "original_form": "오오 계기는요??",
          "speaker_id": "1",
          "time": "20210821 15:05"
        },
        {
          "id": "MDRW2100004126.1.14",
          "form": "고양이의 그 매력적인 얼굴에 빠졌어요",
          "original_form": "고양이의 그 매력적인 얼굴에 빠졌어요",
          "speaker_id": "2",
          "time": "20210821 15:05"
        },
        {
          "id": "MDRW2100004126.1.15",
          "form": "그리고 젤리요",
          "original_form": "그리고 젤리요",
          "speaker_id": "2",
          "time": "20210821 15:05"
        },
        {
          "id": "MDRW2100004126.1.16",
          "form": "아 젤리 못잃어 진짜ㅠㅠㅠ",
          "original_form": "아 젤리 못잃어 진짜ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004126.1.17",
          "form": "그 솜방망이도 너무 귀엽지 않나요",
          "original_form": "그 솜방망이도 너무 귀엽지 않나요",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004126.1.18",
          "form": "정말요",
          "original_form": "정말요",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004126.1.19",
          "form": "솜방망이로 맞아보고 싶어요",
          "original_form": "솜방망이로 맞아보고 싶어요",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004126.1.20",
          "form": "앜ㅋㅋㅋㅋㅋㅋ개웃곀ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ개웃곀ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004126.1.21",
          "form": "키워본 동물 있으세요?",
          "original_form": "키워본 동물 있으세요?",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004126.1.22",
          "form": "어릴 때 강아지요",
          "original_form": "어릴 때 강아지요",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004126.1.23",
          "form": "제가 너무 어려서 입양 갔어요 흑",
          "original_form": "제가 너무 어려서 입양 갔어요 흑",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004126.1.24",
          "form": "에고ㅠㅠ 저는 거북이랑 햄스터 키워봤어요!",
          "original_form": "에고ㅠㅠ 저는 거북이랑 햄스터 키워봤어요!",
          "speaker_id": "1",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004126.1.25",
          "form": "오 거북이는 어떤가요????",
          "original_form": "오 거북이는 어떤가요????",
          "speaker_id": "2",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100004126.1.26",
          "form": "정말 매력적인 동물인데",
          "original_form": "정말 매력적인 동물인데",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004126.1.27",
          "form": "생각보다 엄청 까다로워요",
          "original_form": "생각보다 엄청 까다로워요",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004126.1.28",
          "form": "그때 너무 어려서 뭘 모르고 키우다가",
          "original_form": "그때 너무 어려서 뭘 모르고 키우다가",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004126.1.29",
          "form": "햇빛을 덜받아서 거북이 등딱지가 물러서 죽었어요..ㅜㅜ",
          "original_form": "햇빛을 덜받아서 거북이 등딱지가 물러서 죽었어요..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004126.1.30",
          "form": "나중에 알고보니까 거북이들은 일광욕을 주기적으로 시켜줘야 한다고 하더라구요..",
          "original_form": "나중에 알고보니까 거북이들은 일광욕을 주기적으로 시켜줘야 한다고 하더라구요..",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004126.1.31",
          "form": "헉.... 진짜요? 안타깝네요 흑",
          "original_form": "헉.... 진짜요? 안타깝네요 흑",
          "speaker_id": "2",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004126.1.32",
          "form": "일광욕을 꾸준히 해줘야한다니 뭔가 신선하네요!",
          "original_form": "일광욕을 꾸준히 해줘야한다니 뭔가 신선하네요!",
          "speaker_id": "2",
          "time": "20210821 15:21"
        }
      ]
    }
