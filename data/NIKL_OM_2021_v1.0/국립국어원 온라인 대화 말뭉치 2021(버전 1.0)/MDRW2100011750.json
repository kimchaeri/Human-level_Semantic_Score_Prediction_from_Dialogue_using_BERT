      "utterance": [
        {
          "id": "MDRW2100011750.1.1",
          "form": "봉사해보신적 있으신가요",
          "original_form": "봉사해보신적 있으신가요",
          "speaker_id": "1",
          "time": "20210824 15:41"
        },
        {
          "id": "MDRW2100011750.1.2",
          "form": "대학때 한두번해본게 전부요^^",
          "original_form": "대학때 한두번해본게 전부요^^",
          "speaker_id": "2",
          "time": "20210824 15:43"
        },
        {
          "id": "MDRW2100011750.1.3",
          "form": "어떤 봉사였나요?",
          "original_form": "어떤 봉사였나요?",
          "speaker_id": "1",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011750.1.4",
          "form": "발달장애아들 돌보는거요",
          "original_form": "발달장애아들 돌보는거요",
          "speaker_id": "2",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011750.1.5",
          "form": "여름학교 이런거였던듯",
          "original_form": "여름학교 이런거였던듯",
          "speaker_id": "2",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011750.1.6",
          "form": "저는 청소년센터에서 봉사해봤어요",
          "original_form": "저는 청소년센터에서 봉사해봤어요",
          "speaker_id": "1",
          "time": "20210824 15:52"
        },
        {
          "id": "MDRW2100011750.1.7",
          "form": "검정고시 가르치는거였는데",
          "original_form": "검정고시 가르치는거였는데",
          "speaker_id": "1",
          "time": "20210824 15:52"
        },
        {
          "id": "MDRW2100011750.1.8",
          "form": "좋은일 하셨네요",
          "original_form": "좋은일 하셨네요",
          "speaker_id": "2",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011750.1.9",
          "form": "애들이 합격해서 뿌듯했네요",
          "original_form": "애들이 합격해서 뿌듯했네요",
          "speaker_id": "1",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011750.1.10",
          "form": "봉사  좋은거같아요",
          "original_form": "봉사  좋은거같아요",
          "speaker_id": "2",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011750.1.11",
          "form": "저도 하고나서 장애인에대한 생각이많이 바뀌더라구요",
          "original_form": "저도 하고나서 장애인에대한 생각이많이 바뀌더라구요",
          "speaker_id": "2",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011750.1.12",
          "form": "동호회 활동 하시나요",
          "original_form": "동호회 활동 하시나요",
          "speaker_id": "1",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011750.1.13",
          "form": "맞아요 봉사가 인식 개선 목적도 있죠",
          "original_form": "맞아요 봉사가 인식 개선 목적도 있죠",
          "speaker_id": "1",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011750.1.14",
          "form": "집순이라 딱히 활동하는게 없네요",
          "original_form": "집순이라 딱히 활동하는게 없네요",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011750.1.15",
          "form": "취미로 해보고싶긴한데",
          "original_form": "취미로 해보고싶긴한데",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011750.1.16",
          "form": "뭘해야될지도 모르겠고",
          "original_form": "뭘해야될지도 모르겠고",
          "speaker_id": "2",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011750.1.17",
          "form": "저도 집순이에요",
          "original_form": "저도 집순이에요",
          "speaker_id": "1",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011750.1.18",
          "form": "코로나때문에 만나기도 힘들죠",
          "original_form": "코로나때문에 만나기도 힘들죠",
          "speaker_id": "1",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011750.1.19",
          "form": "코로나땜에 모임도 못하니",
          "original_form": "코로나땜에 모임도 못하니",
          "speaker_id": "2",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011750.1.20",
