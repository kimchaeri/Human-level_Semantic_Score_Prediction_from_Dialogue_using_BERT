      "utterance": [
        {
          "id": "MDRW2100016392.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016392.1.2",
          "form": "좋아해요 ㅎㅎ",
          "original_form": "좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016392.1.3",
          "form": "이번여름휴가",
          "original_form": "이번여름휴가",
          "speaker_id": "2",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100016392.1.4",
          "form": "울릉도 독도 다녀왓어요",
          "original_form": "울릉도 독도 다녀왓어요",
          "speaker_id": "2",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100016392.1.5",
          "form": "와우!",
          "original_form": "와우!",
          "speaker_id": "1",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100016392.1.6",
          "form": "독도가기 올해 저의버킷리스트인데!!",
          "original_form": "독도가기 올해 저의버킷리스트인데!!",
          "speaker_id": "1",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100016392.1.7",
          "form": "어땠어요?",
          "original_form": "어땠어요?",
          "speaker_id": "1",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100016392.1.8",
          "form": "여름휴가때가려다가못갔거든요",
          "original_form": "여름휴가때가려다가못갔거든요",
          "speaker_id": "1",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100016392.1.9",
          "form": "남친 버킷리스트여서 다녀왓는데",
          "original_form": "남친 버킷리스트여서 다녀왓는데",
          "speaker_id": "2",
          "time": "20210826 12:29"
        },
        {
          "id": "MDRW2100016392.1.10",
          "form": "와우!",
          "original_form": "와우!",
          "speaker_id": "1",
          "time": "20210826 12:29"
        },
        {
          "id": "MDRW2100016392.1.11",
          "form": "독도 입도 럭키!!!!",
          "original_form": "독도 입도 럭키!!!!",
          "speaker_id": "2",
          "time": "20210826 12:29"
        },
        {
          "id": "MDRW2100016392.1.12",
          "form": "와우ㅋㅋ",
          "original_form": "와우ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:29"
        },
        {
          "id": "MDRW2100016392.1.13",
          "form": "날씨가허락했나봐요",
          "original_form": "날씨가허락했나봐요",
          "speaker_id": "1",
          "time": "20210826 12:29"
        },
        {
          "id": "MDRW2100016392.1.14",
          "form": "독도에 몇시간 머물렀어요?",
          "original_form": "독도에 몇시간 머물렀어요?",
          "speaker_id": "1",
          "time": "20210826 12:29"
        },
        {
          "id": "MDRW2100016392.1.15",
          "form": "제가 갓던 전날에는 똑같이 맑앗는데도 입도 못햇다 하더라구요",
          "original_form": "제가 갓던 전날에는 똑같이 맑앗는데도 입도 못햇다 하더라구요",
          "speaker_id": "2",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100016392.1.16",
          "form": "파도가 장판처럼 잔잔해야한데요",
          "original_form": "파도가 장판처럼 잔잔해야한데요",
          "speaker_id": "2",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100016392.1.17",
          "form": "25분?",
          "original_form": "25분?",
          "speaker_id": "2",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100016392.1.18",
          "form": "생각보다는 짧네요",
          "original_form": "생각보다는 짧네요",
          "speaker_id": "1",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016392.1.19",
          "form": "진짜 정신없이 눈에담고",
          "original_form": "진짜 정신없이 눈에담고",
          "speaker_id": "2",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016392.1.20",
          "form": "사진찍고ㅜㅜ",
          "original_form": "사진찍고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016392.1.21",
          "form": "독도전체 걸어서 둘러볼수있어요?",
          "original_form": "독도전체 걸어서 둘러볼수있어요?",
          "speaker_id": "1",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016392.1.22",
          "form": "그게 안되더라구요ㅜㅜ",
          "original_form": "그게 안되더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100016392.1.23",
          "form": "앞쪽에만 걸을수있나요?",
          "original_form": "앞쪽에만 걸을수있나요?",
          "speaker_id": "1",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100016392.1.24",
          "form": "작년태풍영향+코시국 때문에",
          "original_form": "작년태풍영향+코시국 때문에",
          "speaker_id": "2",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100016392.1.25",
          "form": "걷는다기보다",
          "original_form": "걷는다기보다",
          "speaker_id": "2",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100016392.1.26",
          "form": "사진찍을정도?",
          "original_form": "사진찍을정도?",
          "speaker_id": "2",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100016392.1.27",
          "form": "아.. .  그렇군요.  추석즈음에라도갈까생각중인뎀. .",
          "original_form": "아.. .  그렇군요.  추석즈음에라도갈까생각중인뎀. .",
          "speaker_id": "1",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016392.1.28",
          "form": "울릉도에는 몇일 있었어요?",
          "original_form": "울릉도에는 몇일 있었어요?",
          "speaker_id": "1",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016392.1.29",
          "form": "진짜 점찍고 오는 느낌,, ?",
          "original_form": "진짜 점찍고 오는 느낌,, ?",
          "speaker_id": "2",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016392.1.30",
          "form": "그마저도 입도하기 힘들데요",
          "original_form": "그마저도 입도하기 힘들데요",
          "speaker_id": "2",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016392.1.31",
          "form": "그래도 가보고싶어요!",
          "original_form": "그래도 가보고싶어요!",
          "speaker_id": "1",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016392.1.32",
          "form": "5박6일 정도 있었어요",
          "original_form": "5박6일 정도 있었어요",
          "speaker_id": "2",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016392.1.33",
          "form": "강추!!!!!",
          "original_form": "강추!!!!!",
          "speaker_id": "2",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016392.1.34",
          "form": "밟아봤다는 느낌!",
          "original_form": "밟아봤다는 느낌!",
          "speaker_id": "1",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016392.1.35",
          "form": "대자연 입니다",
          "original_form": "대자연 입니다",
          "speaker_id": "2",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016392.1.36",
          "form": "독도를!!",
          "original_form": "독도를!!",
          "speaker_id": "1",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016392.1.37",
          "form": "감동ㅠㅠ",
          "original_form": "감동ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016392.1.38",
          "form": "울릉도는 물가 엄청 비싸고 제주에 비에 열악하지만",
          "original_form": "울릉도는 물가 엄청 비싸고 제주에 비에 열악하지만",
          "speaker_id": "2",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016392.1.39",
          "form": "그럼 울릉도에서 계속 날씨 확인하고",
          "original_form": "그럼 울릉도에서 계속 날씨 확인하고",
          "speaker_id": "1",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016392.1.40",
          "form": "대기하는거예요?",
          "original_form": "대기하는거예요?",
          "speaker_id": "1",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016392.1.41",
          "form": "확실히 대자연,낯선 느낌 신기햇어요",
          "original_form": "확실히 대자연,낯선 느낌 신기햇어요",
          "speaker_id": "2",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016392.1.42",
          "form": "입도는 배로 독도접안 가능한지 확인해야해서",
          "original_form": "입도는 배로 독도접안 가능한지 확인해야해서",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100016392.1.43",
          "form": "배타고 가는중에 확실히 알수있는거같았어요",
          "original_form": "배타고 가는중에 확실히 알수있는거같았어요",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100016392.1.44",
          "form": "아. .  배를타도 독도에서 내릴수가없을수도 있나봐요?",
          "original_form": "아. .  배를타도 독도에서 내릴수가없을수도 있나봐요?",
          "speaker_id": "1",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100016392.1.45",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100016392.1.46",
          "form": "제가갓던 전날 사람들은",
          "original_form": "제가갓던 전날 사람들은",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100016392.1.47",
          "form": "입도못하고",
          "original_form": "입도못하고",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100016392.1.48",
          "form": "배에서 독도 한바퀴돌앗데요",
          "original_form": "배에서 독도 한바퀴돌앗데요",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100016392.1.49",
          "form": "독도 여객선 터미널에 매일 전화해보셔야할듯!!",
          "original_form": "독도 여객선 터미널에 매일 전화해보셔야할듯!!",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100016392.1.50",
          "form": "헉 얼마나아쉴울까요!",
          "original_form": "헉 얼마나아쉴울까요!",
          "speaker_id": "1",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100016392.1.51",
          "form": "운이좋으셨네요",
          "original_form": "운이좋으셨네요",
          "speaker_id": "1",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100016392.1.52",
          "form": "저도 꼭 가봐야겠어요!",
          "original_form": "저도 꼭 가봐야겠어요!",
          "speaker_id": "1",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100016392.1.53",
          "form": "확실히 한여름 제일 더울때가 바람도덜불고 확률 높다고 그랬어요",
          "original_form": "확실히 한여름 제일 더울때가 바람도덜불고 확률 높다고 그랬어요",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016392.1.54",
          "form": "추석때 가시는구나!!!!",
          "original_form": "추석때 가시는구나!!!!",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016392.1.55",
          "form": "사람 진짜 진짜 많아유ㅠㅠ",
          "original_form": "사람 진짜 진짜 많아유ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016392.1.56",
          "form": "그렇군요!",
          "original_form": "그렇군요!",
          "speaker_id": "1",
          "time": "20210826 12:41"
        }
