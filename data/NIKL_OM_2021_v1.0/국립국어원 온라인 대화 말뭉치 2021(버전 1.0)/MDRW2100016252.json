      "utterance": [
        {
          "id": "MDRW2100016252.1.1",
          "form": "종교... 뭐 믿니",
          "original_form": "종교... 뭐 믿니",
          "speaker_id": "1",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100016252.1.2",
          "form": "저는 무교입니다만...",
          "original_form": "저는 무교입니다만...",
          "speaker_id": "2",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100016252.1.3",
          "form": "에 저도...",
          "original_form": "에 저도...",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100016252.1.4",
          "form": "하지만 교회가 주는 선물들에 관심이 많습니다",
          "original_form": "하지만 교회가 주는 선물들에 관심이 많습니다",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100016252.1.5",
          "form": "언제냐.. 포도주 주는날ㅋㅋㅋㅋ",
          "original_form": "언제냐.. 포도주 주는날ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100016252.1.6",
          "form": "그런 날이 있어? 나도 알려줘",
          "original_form": "그런 날이 있어? 나도 알려줘",
          "speaker_id": "1",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100016252.1.7",
          "form": "아냐 계란 주는 날있자너",
          "original_form": "아냐 계란 주는 날있자너",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100016252.1.8",
          "form": "그날 들어가면 포도주도 주던데",
          "original_form": "그날 들어가면 포도주도 주던데",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100016252.1.9",
          "form": "아 부활절",
          "original_form": "아 부활절",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100016252.1.10",
          "form": "줄만하네",
          "original_form": "줄만하네",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100016252.1.11",
          "form": "그치ㅋㅋㅋㅋㅋ",
          "original_form": "그치ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100016252.1.12",
          "form": "나는 그냥 평소에 주는 물티슈나 생각했는데",
          "original_form": "나는 그냥 평소에 주는 물티슈나 생각했는데",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100016252.1.13",
          "form": "물티슈 전도 좋아요",
          "original_form": "물티슈 전도 좋아요",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100016252.1.14",
          "form": "맞아 쓸데가 많음",
          "original_form": "맞아 쓸데가 많음",
          "speaker_id": "2",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100016252.1.15",
          "form": "제일 유용해",
          "original_form": "제일 유용해",
          "speaker_id": "1",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100016252.1.16",
          "form": "요새는 못봤네. 코로나라 안하나?",
          "original_form": "요새는 못봤네. 코로나라 안하나?",
          "speaker_id": "2",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100016252.1.17",
          "form": "앗 글게",
          "original_form": "앗 글게",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100016252.1.18",
          "form": "요즘은 그런거 하면",
          "original_form": "요즘은 그런거 하면",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100016252.1.19",
          "form": "욕먹을듯",
          "original_form": "욕먹을듯",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
