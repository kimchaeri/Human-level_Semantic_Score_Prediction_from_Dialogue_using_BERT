      "utterance": [
        {
          "id": "MDRW2100032312.1.1",
          "form": "최근에 산거 있으세요?",
          "original_form": "최근에 산거 있으세요?",
          "speaker_id": "1",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100032312.1.2",
          "form": "저는 오렌지 샀어요 ㅋㅋㅋ",
          "original_form": "저는 오렌지 샀어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:15"
        },
        {
          "id": "MDRW2100032312.1.3",
          "form": "친구가 맛있대서요 ㅎㅎ",
          "original_form": "친구가 맛있대서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 15:15"
        },
        {
          "id": "MDRW2100032312.1.4",
          "form": "아 오렌지 철인가요?",
          "original_form": "아 오렌지 철인가요?",
          "speaker_id": "1",
          "time": "20210905 15:16"
        },
        {
          "id": "MDRW2100032312.1.5",
          "form": "수입이겠지만ㄴㅋㅋ",
          "original_form": "수입이겠지만ㄴㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:16"
        },
        {
          "id": "MDRW2100032312.1.6",
          "form": "ㅋㅋㅋㅋㅋ네네 수입이에여 호주산ㅋㅋㅋ큐ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ네네 수입이에여 호주산ㅋㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 15:16"
        },
        {
          "id": "MDRW2100032312.1.7",
          "form": "수입도 많이나올때 먹어야 맛있드라구요",
          "original_form": "수입도 많이나올때 먹어야 맛있드라구요",
          "speaker_id": "1",
          "time": "20210905 15:17"
        },
        {
          "id": "MDRW2100032312.1.8",
          "form": "마트에서 보이면 사봐야겠어요ㅋㅋ",
          "original_form": "마트에서 보이면 사봐야겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:17"
        },
        {
          "id": "MDRW2100032312.1.9",
          "form": "ㅋㅋㅋㅋ네네 맛있더라구요",
          "original_form": "ㅋㅋㅋㅋ네네 맛있더라구요",
          "speaker_id": "2",
          "time": "20210905 15:17"
        },
        {
          "id": "MDRW2100032312.1.10",
          "form": "name1님은 최근에 머 사셨나여",
          "original_form": "&name1&님은 최근에 머 사셨나여",
          "speaker_id": "2",
          "time": "20210905 15:17"
        },
        {
          "id": "MDRW2100032312.1.11",
          "form": "저는 포도요ㅋㅋ",
          "original_form": "저는 포도요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100032312.1.12",
          "form": "제일좋아해요 과일중에",
          "original_form": "제일좋아해요 과일중에",
          "speaker_id": "1",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100032312.1.13",
          "form": "오오 포도 ㅠㅠ 이번에 한번을 못먹어봤네여 ㅠㅠ",
          "original_form": "오오 포도 ㅠㅠ 이번에 한번을 못먹어봤네여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100032312.1.14",
          "form": "이제 막 많이 나오고있어요ㅎㅎ",
          "original_form": "이제 막 많이 나오고있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100032312.1.15",
          "form": "슬슬 먹어봐여겠네요 ㅎㅎ 씨없는거드세요??",
          "original_form": "슬슬 먹어봐여겠네요 ㅎㅎ 씨없는거드세요??",
          "speaker_id": "2",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100032312.1.16",
          "form": "씨있는것도 먹어요'",
          "original_form": "씨있는것도 먹어요'",
          "speaker_id": "1",
          "time": "20210905 15:51"
        },
        {
          "id": "MDRW2100032312.1.17",
          "form": "그냥 삼켜서ㅎㅎ",
          "original_form": "그냥 삼켜서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:51"
        },
        {
          "id": "MDRW2100032312.1.18",
          "form": "오오 저두여 ㅋㅋㅋ 포도라면다좋아해여 거봉도좋고 샤인머스캣도 좋구여",
          "original_form": "오오 저두여 ㅋㅋㅋ 포도라면다좋아해여 거봉도좋고 샤인머스캣도 좋구여",
          "speaker_id": "2",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032312.1.19",
          "form": "그쵸 다 맛있어요!!",
          "original_form": "그쵸 다 맛있어요!!",
          "speaker_id": "1",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032312.1.20",
          "form": "가을철에 나오는 한국포도가 젤 맛있긴해요",
          "original_form": "가을철에 나오는 한국포도가 젤 맛있긴해요",
          "speaker_id": "1",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032312.1.21",
          "form": "수입건 좀 별로더라구요 제입맛엔",
          "original_form": "수입건 좀 별로더라구요 제입맛엔",
          "speaker_id": "1",
          "time": "20210905 15:53"
        },
        {
          "id": "MDRW2100032312.1.22",
          "form": "맞아여 한국게 달달하고 맛있죸ㅋㅋㅋ",
          "original_form": "맞아여 한국게 달달하고 맛있죸ㅋㅋㅋ",
