      "utterance": [
        {
          "id": "MDRW2100008372.1.1",
          "form": "밥차려먹어야되는데 넘귀찮네요",
          "original_form": "밥차려먹어야되는데 넘귀찮네요",
          "speaker_id": "1",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008372.1.2",
          "form": "아직도 식사안하신거에요?",
          "original_form": "아직도 식사안하신거에요?",
          "speaker_id": "2",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008372.1.3",
          "form": "네네 후라이팬설거지해야돼서ㅋ",
          "original_form": "네네 후라이팬설거지해야돼서ㅋ",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008372.1.4",
          "form": "울딸램들도 이제일어나서 어슬렁거린긴해요",
          "original_form": "울딸램들도 이제일어나서 어슬렁거린긴해요",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008372.1.5",
          "form": "집안일은 해도해도 끝이없어요",
          "original_form": "집안일은 해도해도 끝이없어요",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008372.1.6",
          "form": "오늘은 할일 쌓였어요ㅠㅠ",
          "original_form": "오늘은 할일 쌓였어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008372.1.7",
          "form": "꼬맹이도없는데 치우고돌아서면 그대로인 느낌이에요",
          "original_form": "꼬맹이도없는데 치우고돌아서면 그대로인 느낌이에요",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008372.1.8",
          "form": "머리카락은 치워도치워도 계속나오구요",
          "original_form": "머리카락은 치워도치워도 계속나오구요",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008372.1.9",
          "form": "맞아요 ㅎ 그놈의 머리카락",
          "original_form": "맞아요 ㅎ 그놈의 머리카락",
          "speaker_id": "1",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008372.1.10",
          "form": "저희집은 바닥이 타일이라고해야되나 하얀바닥이라",
          "original_form": "저희집은 바닥이 타일이라고해야되나 하얀바닥이라",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008372.1.11",
          "form": "머리카락이 넘 잘보여요",
          "original_form": "머리카락이 넘 잘보여요",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008372.1.12",
          "form": "울집은 구석구석숨어있다가 잘나오네요",
          "original_form": "울집은 구석구석숨어있다가 잘나오네요",
          "speaker_id": "2",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008372.1.13",
          "form": "청소기를 먼저 돌리고 밥을 차리는게 낫겠죠?ㅎ",
          "original_form": "청소기를 먼저 돌리고 밥을 차리는게 낫겠죠?ㅎ",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008372.1.14",
          "form": "네. 청소하실꺼면 청소기먼저~~",
          "original_form": "네. 청소하실꺼면 청소기먼저~~",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008372.1.15",
          "form": "집에서 많이 먹지도 않는거같은데 플라스틱이 왜케 나오는지",
          "original_form": "집에서 많이 먹지도 않는거같은데 플라스틱이 왜케 나오는지",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008372.1.16",
          "form": "분리수거할게 산더미네요ㅎㅎ",
          "original_form": "분리수거할게 산더미네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008372.1.17",
          "form": "시켜먹는게많아서 저도 플라스틱 엄청나요",
          "original_form": "시켜먹는게많아서 저도 플라스틱 엄청나요",
          "speaker_id": "2",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008372.1.18",
          "form": "음식물도 버려야되고 ㅠㅠ",
          "original_form": "음식물도 버려야되고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008372.1.19",
          "form": "요즘 장을 많이 보니 그렇더라구요",
          "original_form": "요즘 장을 많이 보니 그렇더라구요",
          "speaker_id": "1",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008372.1.20",
          "form": "포장지랑 음료마신 병이랑 생수병",
          "original_form": "포장지랑 음료마신 병이랑 생수병",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008372.1.21",
          "form": "그니깐요. 몰 잘해먹지도않는데 음쓰도 한가득나오더라구요",
          "original_form": "그니깐요. 몰 잘해먹지도않는데 음쓰도 한가득나오더라구요",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008372.1.22",
          "form": "저도요 혼자사는데도 그래요",
          "original_form": "저도요 혼자사는데도 그래요",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008372.1.23",
          "form": "빨리안버리면 여름이라 날파리날려서",
          "original_form": "빨리안버리면 여름이라 날파리날려서",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008372.1.24",
          "form": "ㅎ 초파리는 과일같은거있음 바로 나오더라구요",
          "original_form": "ㅎ 초파리는 과일같은거있음 바로 나오더라구요",
          "speaker_id": "2",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008372.1.25",
          "form": "없다가 대체 어디로 들어오는지  신기",
          "original_form": "없다가 대체 어디로 들어오는지  신기",
          "speaker_id": "1",
          "time": "20210822 15:36"
        }
      ]
