        {
          "id": "MDRW2100020261.1.1",
          "form": "올해",
          "original_form": "올해",
          "speaker_id": "2",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020261.1.2",
          "form": "월드컵 경기 꿀잼이였전거같아요",
          "original_form": "월드컵 경기 꿀잼이였전거같아요",
          "speaker_id": "2",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020261.1.3",
          "form": "월드컵이래... 올림픽!!",
          "original_form": "월드컵이래... 올림픽!!",
          "speaker_id": "2",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020261.1.4",
          "form": "맞아요ㅋㅋ 어떤 거 제일 재밌게 보셨어요?",
          "original_form": "맞아요ㅋㅋ 어떤 거 제일 재밌게 보셨어요?",
          "speaker_id": "1",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020261.1.5",
          "form": "당연 배구@@ㅋㅋ",
          "original_form": "당연 배구@@ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:56"
        },
        {
          "id": "MDRW2100020261.1.6",
          "form": "축구도 재밌었구 양궁도",
          "original_form": "축구도 재밌었구 양궁도",
          "speaker_id": "2",
          "time": "20210828 00:56"
        },
        {
          "id": "MDRW2100020261.1.7",
          "form": "배드민턴도",
          "original_form": "배드민턴도",
          "speaker_id": "2",
          "time": "20210828 00:56"
        },
        {
          "id": "MDRW2100020261.1.8",
          "form": "펜싱도 재밌었어요",
          "original_form": "펜싱도 재밌었어요",
          "speaker_id": "1",
          "time": "20210828 00:56"
        },
        {
          "id": "MDRW2100020261.1.9",
          "form": "펜싱을 못봤네요ㅠ",
          "original_form": "펜싱을 못봤네요ㅠ",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020261.1.10",
          "form": "혹시 평소에도 보는 종목 있으신가요?",
          "original_form": "혹시 평소에도 보는 종목 있으신가요?",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020261.1.11",
          "form": "평소에 챙겨보는건 딱히 없고...",
          "original_form": "평소에 챙겨보는건 딱히 없고...",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020261.1.12",
          "form": "농구 축구 배구 야구 ..직관해봤어요 ㅎㅎ",
          "original_form": "농구 축구 배구 야구 ..직관해봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020261.1.13",
          "form": "국내 프로리그는 다 가보셨네요ㅋㅋ",
          "original_form": "국내 프로리그는 다 가보셨네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020261.1.14",
          "form": "ㅋㅋㅋㅋ다 공짜표",
          "original_form": "ㅋㅋㅋㅋ다 공짜표",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020261.1.15",
          "form": "어디서 구하셨는지 부럽네요 ㅋㅋ",
          "original_form": "어디서 구하셨는지 부럽네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020261.1.16",
          "form": "근데 직관보다는",
          "original_form": "근데 직관보다는",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020261.1.17",
          "form": "집에서 티비로보는게 더 좋은거같아요 해설들으면서",
          "original_form": "집에서 티비로보는게 더 좋은거같아요 해설들으면서",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020261.1.18",
          "form": "잘 모르는데 가서보니 더 모르겠더라구요",
          "original_form": "잘 모르는데 가서보니 더 모르겠더라구요",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020261.1.19",
          "form": "중계가 훨씬 잘 보이긴 하죠ㅋㅋ",
          "original_form": "중계가 훨씬 잘 보이긴 하죠ㅋㅋ",
