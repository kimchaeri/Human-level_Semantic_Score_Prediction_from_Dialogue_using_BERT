      "utterance": [
        {
          "id": "MDRW2100008270.1.1",
          "form": "흡 넵",
          "original_form": "흡 넵",
          "speaker_id": "2",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008270.1.2",
          "form": "근데 군대때문에 헤어졌어요",
          "original_form": "근데 군대때문에 헤어졌어요",
          "speaker_id": "2",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008270.1.3",
          "form": "아아아 ㅠㅠㅠ 이런",
          "original_form": "아아아 ㅠㅠㅠ 이런",
          "speaker_id": "1",
          "time": "20210822 14:40"
        },
        {
          "id": "MDRW2100008270.1.4",
          "form": "name1님은 어떠신가유",
          "original_form": "&name1&님은 어떠신가유",
          "speaker_id": "2",
          "time": "20210822 14:40"
        },
        {
          "id": "MDRW2100008270.1.5",
          "form": "전 한번도 안해봤어요 ㅋㅋ",
          "original_form": "전 한번도 안해봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:40"
        },
        {
          "id": "MDRW2100008270.1.6",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008270.1.7",
          "form": "그러면 이상형이 어케되세요?!",
          "original_form": "그러면 이상형이 어케되세요?!",
          "speaker_id": "2",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008270.1.8",
          "form": "이상형도 잘 생각해본 적 없지만...",
          "original_form": "이상형도 잘 생각해본 적 없지만...",
          "speaker_id": "1",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008270.1.9",
          "form": "일단 제일 중요한 것은 대화가 잘 되고 같이 취미를 즐길 수 있는 사람이요!",
          "original_form": "일단 제일 중요한 것은 대화가 잘 되고 같이 취미를 즐길 수 있는 사람이요!",
          "speaker_id": "1",
          "time": "20210822 14:43"
        },
        {
          "id": "MDRW2100008270.1.10",
          "form": "헉 취미 너무 중요하죠",
          "original_form": "헉 취미 너무 중요하죠",
          "speaker_id": "2",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008270.1.11",
          "form": "대화가 진짜 중요한것같아요",
          "original_form": "대화가 진짜 중요한것같아요",
          "speaker_id": "2",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008270.1.12",
          "form": "암만 잘생겨도 대화가 안통하면...",
          "original_form": "암만 잘생겨도 대화가 안통하면...",
          "speaker_id": "2",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008270.1.13",
          "form": "모니터와 다를바가 없어요",
          "original_form": "모니터와 다를바가 없어요",
          "speaker_id": "2",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008270.1.14",
          "form": "정말요 말이 통해야 더 친밀해지고 그러죠",
          "original_form": "정말요 말이 통해야 더 친밀해지고 그러죠",
          "speaker_id": "1",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008270.1.15",
          "form": "모니터 ㅋㅋㅋ 맞아요!!",
          "original_form": "모니터 ㅋㅋㅋ 맞아요!!",
          "speaker_id": "1",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008270.1.16",
          "form": "그러면 혹시 사람 소개받아본적은 있으신가요",
          "original_form": "그러면 혹시 사람 소개받아본적은 있으신가요",
          "speaker_id": "2",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008270.1.17",
          "form": "아뇨 없어요",
          "original_form": "아뇨 없어요",
          "speaker_id": "1",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008270.1.18",
          "form": "최근까지 연애에 아예 관심이 없던 사람이라 ㅠㅠㅋㅋ",
          "original_form": "최근까지 연애에 아예 관심이 없던 사람이라 ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008270.1.19",
          "form": "소개받으면 처음 보는 사람이랑 만나는건데 쉽지않을 것 같아요",
          "original_form": "소개받으면 처음 보는 사람이랑 만나는건데 쉽지않을 것 같아요",
          "speaker_id": "1",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008270.1.20",
          "form": "낯가림이 심해서... ㄷㄷ",
          "original_form": "낯가림이 심해서... ㄷㄷ",
          "speaker_id": "1",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008270.1.21",
          "form": "헉 글쵸글쵸",
          "original_form": "헉 글쵸글쵸",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008270.1.22",
          "form": "저도 아직 한 번도 소개로 잘된적이 없어요",
          "original_form": "저도 아직 한 번도 소개로 잘된적이 없어요",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008270.1.23",
          "form": "저도 요즘 약간",
          "original_form": "저도 요즘 약간",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008270.1.24",
          "form": "연애 꼭 해야하나 싶어욬ㅋㅋㅋㅋ",
          "original_form": "연애 꼭 해야하나 싶어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008270.1.25",
          "form": "사귄것도 주변 애들이 다 사귀어서 그냥 분위기따라 사귄 느낌이어서",
          "original_form": "사귄것도 주변 애들이 다 사귀어서 그냥 분위기따라 사귄 느낌이어서",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008270.1.26",
          "form": "진짜 좋아하는 사람 살면서 꼭 한 번 만나보고는 싶어요",
          "original_form": "진짜 좋아하는 사람 살면서 꼭 한 번 만나보고는 싶어요",
          "speaker_id": "2",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008270.1.27",
          "form": "진짜 그랬으면 좋겠네요",
          "original_form": "진짜 그랬으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008270.1.28",
          "form": "저는 평생을 같이 살아도 서로 잘 맞는 사람을 한명 꼭 만났으면 좋겠어요",
          "original_form": "저는 평생을 같이 살아도 서로 잘 맞는 사람을 한명 꼭 만났으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008270.1.29",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100008270.1.30",
          "form": "친구든 연인이든 상관없이말이에요",
          "original_form": "친구든 연인이든 상관없이말이에요",
          "speaker_id": "1",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100008270.1.31",
          "form": "정말 잘 맞는 친구있으면 그 친구랑 평생 살아도 좋아요 ㅋㅋ",
          "original_form": "정말 잘 맞는 친구있으면 그 친구랑 평생 살아도 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008270.1.32",
          "form": "맞아욬ㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008270.1.33",
          "form": "사실 결혼하는게 외로워서가 크다고 하는데",
          "original_form": "사실 결혼하는게 외로워서가 크다고 하는데",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008270.1.34",
          "form": "진짜 잘맞는 친구 있고 그 친구가 보호자가 되는  법만 만들어져있으면",
          "original_form": "진짜 잘맞는 친구 있고 그 친구가 보호자가 되는  법만 만들어져있으면",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008270.1.35",
          "form": "그냥 안해도 상관없을것같아욬ㅋㅋㅋㅋ",
          "original_form": "그냥 안해도 상관없을것같아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008270.1.36",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008270.1.37",
          "form": "그런 법이 만들어지면 좋겠어요 정말!",
          "original_form": "그런 법이 만들어지면 좋겠어요 정말!",
          "speaker_id": "1",
          "time": "20210822 15:05"
        }
      ]
