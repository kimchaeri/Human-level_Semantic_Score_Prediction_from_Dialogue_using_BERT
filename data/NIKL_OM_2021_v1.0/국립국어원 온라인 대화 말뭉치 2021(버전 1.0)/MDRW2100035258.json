      "utterance": [
        {
          "id": "MDRW2100035258.1.1",
          "form": "커피 즐기시나요",
          "original_form": "커피 즐기시나요",
          "speaker_id": "2",
          "time": "20210829 12:56"
        },
        {
          "id": "MDRW2100035258.1.2",
          "form": "네~^^",
          "original_form": "네~^^",
          "speaker_id": "1",
          "time": "20210829 12:56"
        },
        {
          "id": "MDRW2100035258.1.3",
          "form": "좋아해요~",
          "original_form": "좋아해요~",
          "speaker_id": "1",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100035258.1.4",
          "form": "어느 체인점 다니시나요?",
          "original_form": "어느 체인점 다니시나요?",
          "speaker_id": "2",
          "time": "20210829 12:58"
        },
        {
          "id": "MDRW2100035258.1.5",
          "form": "간다면 스벅요~ 주말에만 가여ㅠ애땜에 차로 가야하니ㅠㅠ",
          "original_form": "간다면 스벅요~ 주말에만 가여ㅠ애땜에 차로 가야하니ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100035258.1.6",
          "form": "나머진 집커피로ㅠ",
          "original_form": "나머진 집커피로ㅠ",
          "speaker_id": "1",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100035258.1.7",
          "form": "드라이브 스루 이용하시는거죠? 그런 카페가 편하죠?",
          "original_form": "드라이브 스루 이용하시는거죠? 그런 카페가 편하죠?",
          "speaker_id": "2",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100035258.1.8",
          "form": "맞아요ㅠ근데 디티가 멀어서",
          "original_form": "맞아요ㅠ근데 디티가 멀어서",
          "speaker_id": "1",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100035258.1.9",
          "form": "그냥 사이렌오더로시키고 얼른 다녀와요 애있으면 디티로가고요",
          "original_form": "그냥 사이렌오더로시키고 얼른 다녀와요 애있으면 디티로가고요",
          "speaker_id": "1",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100035258.1.10",
          "form": "그렇군요 어떤 메뉴 좋아하세요?",
          "original_form": "그렇군요 어떤 메뉴 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100035258.1.11",
          "form": "더블칩 에스프레소 프라프치노요",
          "original_form": "더블칩 에스프레소 프라프치노요",
          "speaker_id": "1",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100035258.1.12",
          "form": "앗...저도 프라프치노 좋아해요 ㅠㅠ",
          "original_form": "앗...저도 프라프치노 좋아해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100035258.1.13",
          "form": "맛있죠~ ㅎ 비싸서 그렇지 ㅠㅠㅋ",
          "original_form": "맛있죠~ ㅎ 비싸서 그렇지 ㅠㅠㅋ",
          "speaker_id": "1",
          "time": "20210829 14:59"
        },
        {
          "id": "MDRW2100035258.1.14",
          "form": "맞아요 왜이리 비싼거죠..한끼 가격..ㅋ",
          "original_form": "맞아요 왜이리 비싼거죠..한끼 가격..ㅋ",
          "speaker_id": "2",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100035258.1.15",
          "form": "맞아요 그래도 지난번에 미니 음료 나왔을때 루프탑그레이라떼는 너무 맛있었는데 ㅠㅠ 두번밖에 못먹고 끝..",
          "original_form": "맞아요 그래도 지난번에 미니 음료 나왔을때 루프탑그레이라떼는 너무 맛있었는데 ㅠㅠ 두번밖에 못먹고 끝..",
          "speaker_id": "1",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100035258.1.16",
          "form": "저희 매장은 아예 재료 소진 으로 초반부터 못먹어봤어요 ㅠㅠ",
          "original_form": "저희 매장은 아예 재료 소진 으로 초반부터 못먹어봤어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 15:51"
        }
      ]
