      "utterance": [
        {
          "id": "MDRW2100046913.1.1",
          "form": "10월 대체공휴일 확대 적용하면 언제야? 알아?",
          "original_form": "10월 대체공휴일 확대 적용하면 언제야? 알아?",
          "speaker_id": "1",
          "time": "20211006 00:44"
        },
        {
          "id": "MDRW2100046913.1.2",
          "form": "대체 공휴일을 확대해?",
          "original_form": "대체 공휴일을 확대해?",
          "speaker_id": "2",
          "time": "20211006 00:45"
        },
        {
          "id": "MDRW2100046913.1.3",
          "form": "확대해??",
          "original_form": "확대해??",
          "speaker_id": "1",
          "time": "20211006 00:45"
        },
        {
          "id": "MDRW2100046913.1.4",
          "form": "대체공휴일 미적용하는 분야/직군이 많아서 구런가",
          "original_form": "대체공휴일 미적용하는 분야/직군이 많아서 구런가",
          "speaker_id": "2",
          "time": "20211006 00:45"
        },
        {
          "id": "MDRW2100046913.1.5",
          "form": "4,11일이 이미 대체공휴일 아닌가",
          "original_form": "4,11일이 이미 대체공휴일 아닌가",
          "speaker_id": "1",
          "time": "20211006 00:46"
        },
        {
          "id": "MDRW2100046913.1.6",
          "form": "응 맞아",
          "original_form": "응 맞아",
          "speaker_id": "2",
          "time": "20211006 00:46"
        },
        {
          "id": "MDRW2100046913.1.7",
          "form": "근데 뭘 확대하는거지?",
          "original_form": "근데 뭘 확대하는거지?",
          "speaker_id": "2",
          "time": "20211006 00:46"
        },
        {
          "id": "MDRW2100046913.1.8",
          "form": "찾아보께",
          "original_form": "찾아보께",
          "speaker_id": "1",
          "time": "20211006 00:46"
        },
        {
          "id": "MDRW2100046913.1.9",
          "form": "원래 어린이날, 추석, 설날만 적용이었대",
          "original_form": "원래 어린이날, 추석, 설날만 적용이었대",
          "speaker_id": "1",
          "time": "20211006 00:47"
        },
        {
          "id": "MDRW2100046913.1.10",
          "form": "그리고 올해 1월부터는 30인 이상 기업으로 확대되었다네",
          "original_form": "그리고 올해 1월부터는 30인 이상 기업으로 확대되었다네",
          "speaker_id": "2",
          "time": "20211006 00:49"
        },
        {
          "id": "MDRW2100046913.1.11",
          "form": "개천절, 한글날 된 게 확대적용인가봐",
          "original_form": "개천절, 한글날 된 게 확대적용인가봐",
          "speaker_id": "1",
          "time": "20211006 00:49"
        },
        {
          "id": "MDRW2100046913.1.12",
          "form": "유급휴일로",
          "original_form": "유급휴일로",
          "speaker_id": "2",
          "time": "20211006 00:49"
        },
        {
          "id": "MDRW2100046913.1.13",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20211006 00:49"
        },
        {
          "id": "MDRW2100046913.1.14",
          "form": "모든 공휴일로 확대적용",
          "original_form": "모든 공휴일로 확대적용",
          "speaker_id": "1",
          "time": "20211006 00:49"
        },
        {
          "id": "MDRW2100046913.1.15",
          "form": "잘됐네",
          "original_form": "잘됐네",
          "speaker_id": "1",
          "time": "20211006 00:49"
        },
        {
          "id": "MDRW2100046913.1.16",
          "form": "응 좋은데!",
          "original_form": "응 좋은데!",
          "speaker_id": "2",
          "time": "20211006 00:50"
        },
        {
          "id": "MDRW2100046913.1.17",
          "form": "한국인들 일 넘 열심히해",
          "original_form": "한국인들 일 넘 열심히해",
          "speaker_id": "1",
          "time": "20211006 00:51"
        },
        {
          "id": "MDRW2100046913.1.18",
          "form": "그지",
          "original_form": "그지",
          "speaker_id": "1",
          "time": "20211006 00:51"
        },
        {
          "id": "MDRW2100046913.1.19",
          "form": "헤엄쳐서라도 출근하는 민족이자나",
          "original_form": "헤엄쳐서라도 출근하는 민족이자나",
          "speaker_id": "2",
          "time": "20211006 00:51"
        },
        {
          "id": "MDRW2100046913.1.20",
          "form": "대단들해",
          "original_form": "대단들해",
          "speaker_id": "2",
          "time": "20211006 00:51"
        },
        {
          "id": "MDRW2100046913.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아시",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아시",
          "speaker_id": "1",
          "time": "20211006 00:51"
        },
        {
          "id": "MDRW2100046913.1.22",
          "form": "놀토부터 꾸준한 발전이다",
          "original_form": "놀토부터 꾸준한 발전이다",
          "speaker_id": "1",
          "time": "20211006 00:52"
        },
        {
          "id": "MDRW2100046913.1.23",
          "form": "또 누구보다 휴일 찾아 근데",
          "original_form": "또 누구보다 휴일 찾아 근데",
          "speaker_id": "2",
          "time": "20211006 00:52"
        },
        {
          "id": "MDRW2100046913.1.24",
          "form": "재밌어",
          "original_form": "재밌어",
          "speaker_id": "2",
          "time": "20211006 00:52"
        },
        {
          "id": "MDRW2100046913.1.25",
          "form": "찾아야지 열심히하는데",
          "original_form": "찾아야지 열심히하는데",
          "speaker_id": "1",
          "time": "20211006 00:52"
        },
        {
          "id": "MDRW2100046913.1.26",
          "form": "OECD근로시간 1등일거야",
          "original_form": "OECD근로시간 1등일거야",
          "speaker_id": "1",
          "time": "20211006 00:52"
        },
        {
          "id": "MDRW2100046913.1.27",
          "form": "그러네 1등이야",
          "original_form": "그러네 1등이야",
          "speaker_id": "2",
          "time": "20211006 00:54"
        },
        {
          "id": "MDRW2100046913.1.28",
          "form": "후아...",
          "original_form": "후아...",
          "speaker_id": "2",
          "time": "20211006 00:54"
        },
        {
          "id": "MDRW2100046913.1.29",
