      "utterance": [
        {
          "id": "MDRW2100042842.1.1",
          "form": "112동도 기미걱정을 하네",
          "original_form": "112동도 기미걱정을 하네",
          "speaker_id": "2",
          "time": "20210907 09:54"
        },
        {
          "id": "MDRW2100042842.1.2",
          "form": "같이 레이저 하러 가라",
          "original_form": "같이 레이저 하러 가라",
          "speaker_id": "2",
          "time": "20210907 09:54"
        },
        {
          "id": "MDRW2100042842.1.3",
          "form": "같이 살도 빼야 되고 같이 레이저도 찢어야 등",
          "original_form": "같이 살도 빼야 되고 같이 레이저도 찢어야 등",
          "speaker_id": "1",
          "time": "20210907 09:54"
        },
        {
          "id": "MDRW2100042842.1.4",
          "form": "둘 다 의지가 약한ㅋㅋㅋㅋㅋ",
          "original_form": "둘 다 의지가 약한ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:55"
        },
        {
          "id": "MDRW2100042842.1.5",
          "form": "항상 함께 하지만",
          "original_form": "항상 함께 하지만",
          "speaker_id": "2",
          "time": "20210907 09:56"
        },
        {
          "id": "MDRW2100042842.1.6",
          "form": "항상 별 효과가 없네",
          "original_form": "항상 별 효과가 없네",
          "speaker_id": "2",
          "time": "20210907 09:56"
        },
        {
          "id": "MDRW2100042842.1.7",
          "form": "그러니까요ㅋㅋㅋㅋ",
          "original_form": "그러니까요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:58"
        },
        {
          "id": "MDRW2100042842.1.8",
          "form": "그리고 늘 같이 먹죠ㅋㅋㅋㅋ",
          "original_form": "그리고 늘 같이 먹죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:58"
        },
        {
          "id": "MDRW2100042842.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042842.1.10",
          "form": "안 된다 정말",
          "original_form": "안 된다 정말",
          "speaker_id": "2",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042842.1.11",
          "form": "먹는 건  또 얼마나 잘 맞는지ㅋㅋㅋㅋ",
          "original_form": "먹는 건  또 얼마나 잘 맞는지ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:00"
        },
        {
          "id": "MDRW2100042842.1.12",
          "form": "환상에 먹짝꿍ㅋㅋㅋㅋ",
          "original_form": "환상에 먹짝꿍ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:00"
        },
        {
          "id": "MDRW2100042842.1.13",
          "form": "그래 진짜 잘먹는다 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래 진짜 잘먹는다 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042842.1.14",
          "form": "접종하러왔다",
          "original_form": "접종하러왔다",
          "speaker_id": "2",
          "time": "20210907 12:06"
        },
        {
          "id": "MDRW2100042842.1.15",
          "form": "빨리 맞구먹으러가야지 ㅋㅋㅋㄱ",
          "original_form": "빨리 맞구먹으러가야지 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 12:06"
        },
        {
          "id": "MDRW2100042842.1.16",
          "form": "오늘 왔었으면 뭐 될 뻔ㅋㅋㅋㅋㅋ",
          "original_form": "오늘 왔었으면 뭐 될 뻔ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100042842.1.17",
          "form": "아이고 진이 다 빠지네예ㅠ",
          "original_form": "아이고 진이 다 빠지네예ㅠ",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100042842.1.18",
          "form": "손님 많았나??",
          "original_form": "손님 많았나??",
          "speaker_id": "2",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100042842.1.19",
          "form": "난 이제 name3이 집 나와서 어 name4이 데리러 가고 있다",
          "original_form": "난 이제 &name3&이 집 나와서 어 &name4&이 데리러 가고 있다",
          "speaker_id": "2",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100042842.1.20",
          "form": "아 여름 옥 교환 겁나 많이 오고ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 여름 옥 교환 겁나 많이 오고ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100042842.1.21",
          "form": "교환을 40만원 치했어요 언니",
          "original_form": "교환을 40만원 치했어요 언니",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100042842.1.22",
