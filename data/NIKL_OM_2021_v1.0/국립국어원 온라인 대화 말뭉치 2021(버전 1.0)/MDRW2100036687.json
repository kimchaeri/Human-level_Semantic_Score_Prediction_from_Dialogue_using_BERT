      "utterance": [
        {
          "id": "MDRW2100036687.1.1",
          "form": "아니 오빠한테",
          "original_form": "아니 오빠한테",
          "speaker_id": "2",
          "time": "20210831 08:50"
        },
        {
          "id": "MDRW2100036687.1.2",
          "form": "상테크막혔다고",
          "original_form": "상테크막혔다고",
          "speaker_id": "2",
          "time": "20210831 08:50"
        },
        {
          "id": "MDRW2100036687.1.3",
          "form": "응 ㅋㅋ그러니까",
          "original_form": "응 ㅋㅋ그러니까",
          "speaker_id": "1",
          "time": "20210831 08:50"
        },
        {
          "id": "MDRW2100036687.1.4",
          "form": "실적쌓아야되는카드 있는데 난감하네",
          "original_form": "실적쌓아야되는카드 있는데 난감하네",
          "speaker_id": "2",
          "time": "20210831 08:50"
        },
        {
          "id": "MDRW2100036687.1.5",
          "form": "했더니",
          "original_form": "했더니",
          "speaker_id": "2",
          "time": "20210831 08:50"
        },
        {
          "id": "MDRW2100036687.1.6",
          "form": "안쓰면되지?그래??",
          "original_form": "안쓰면되지?그래??",
          "speaker_id": "1",
          "time": "20210831 08:50"
        },
        {
          "id": "MDRW2100036687.1.7",
          "form": "아직 실적쌓을까드가 남았나? 이럼ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아직 실적쌓을까드가 남았나? 이럼ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 08:50"
        },
        {
          "id": "MDRW2100036687.1.8",
          "form": "해지하라고ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "해지하라고ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 08:51"
        },
        {
          "id": "MDRW2100036687.1.9",
          "form": "아 웃겨 ㅋㅋ 무슨카든데ㅋㅋ",
          "original_form": "아 웃겨 ㅋㅋ 무슨카든데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 08:51"
        },
        {
          "id": "MDRW2100036687.1.10",
          "form": "정수기,식세기 ㅋㅋ 카드있지ㅋ",
          "original_form": "정수기,식세기 ㅋㅋ 카드있지ㅋ",
          "speaker_id": "2",
          "time": "20210831 08:51"
        },
        {
          "id": "MDRW2100036687.1.11",
          "form": "아 맞네 그거는 해지모하지ㅠㅠ",
          "original_form": "아 맞네 그거는 해지모하지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 08:51"
        },
        {
          "id": "MDRW2100036687.1.12",
          "form": "근데 또 막상쓰면 쓰는데 아무래도 복잡하지.. 그럼 다른것도 혜택이 줄고 ㅠㅠ",
          "original_form": "근데 또 막상쓰면 쓰는데 아무래도 복잡하지.. 그럼 다른것도 혜택이 줄고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 08:52"
        },
        {
          "id": "MDRW2100036687.1.13",
          "form": "신한ㅡ더모아,오빠현대ㅡ폰,내현대ㅡ식세기,우리카드ㅡ정수기",
          "original_form": "신한ㅡ더모아,오빠현대ㅡ폰,내현대ㅡ식세기,우리카드ㅡ정수기",
          "speaker_id": "2",
          "time": "20210831 08:52"
        },
        {
          "id": "MDRW2100036687.1.14",
          "form": "이래있징",
          "original_form": "이래있징",
          "speaker_id": "2",
          "time": "20210831 08:52"
        },
        {
          "id": "MDRW2100036687.1.15",
          "form": "근데 상테크안되면",
          "original_form": "근데 상테크안되면",
          "speaker_id": "2",
          "time": "20210831 08:52"
        },
        {
          "id": "MDRW2100036687.1.16",
          "form": "오빠현대를 하나없애야지뭐ㅜ 내현대는 일단 놔두고",
          "original_form": "오빠현대를 하나없애야지뭐ㅜ 내현대는 일단 놔두고",
          "speaker_id": "2",
          "time": "20210831 08:53"
        },
        {
          "id": "MDRW2100036687.1.17",
          "form": "될거같은데 일단 한번 해보고ㅠㅠ",
          "original_form": "될거같은데 일단 한번 해보고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 08:53"
        },
        {
          "id": "MDRW2100036687.1.18",
          "form": "나는 이래되면 학원비를 다 돌려야지ㅠㅠ",
          "original_form": "나는 이래되면 학원비를 다 돌려야지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 08:53"
        },
        {
          "id": "MDRW2100036687.1.19",
          "form": "응응 좀 해보고 안되면 하나해지하게",
          "original_form": "응응 좀 해보고 안되면 하나해지하게",
          "speaker_id": "2",
          "time": "20210831 08:53"
        },
        {
          "id": "MDRW2100036687.1.20",
          "form": "혹시 경북쿠폰 그거 썼나??",
          "original_form": "혹시 경북쿠폰 그거 썼나??",
          "speaker_id": "1",
          "time": "20210831 08:54"
        },
        {
          "id": "MDRW2100036687.1.21",
          "form": "경북쿠폰 뭐지?",
          "original_form": "경북쿠폰 뭐지?",
          "speaker_id": "2",
          "time": "20210831 08:54"
        },
        {
          "id": "MDRW2100036687.1.22",
          "form": "경북쿠폰이 어디꺼지?",
          "original_form": "경북쿠폰이 어디꺼지?",
          "speaker_id": "2",
          "time": "20210831 08:55"
        },
        {
          "id": "MDRW2100036687.1.23",
          "form": "지마켓?",
          "original_form": "지마켓?",
          "speaker_id": "2",
