      "utterance": [
        {
          "id": "MDRW2100018896.1.1",
          "form": "봉사 경험있으세요?",
          "original_form": "봉사 경험있으세요?",
          "speaker_id": "1",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100018896.1.2",
          "form": "전 학점 때문에",
          "original_form": "전 학점 때문에",
          "speaker_id": "2",
          "time": "20210830 15:21"
        },
        {
          "id": "MDRW2100018896.1.3",
          "form": "한 봉사밖에 없어요 ㅠㅠ",
          "original_form": "한 봉사밖에 없어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 15:21"
        },
        {
          "id": "MDRW2100018896.1.4",
          "form": "보통은 그렇져 .. 저두용ㅎ 지금은 기부금단체에 정기납부하는거밖에 ..",
          "original_form": "보통은 그렇져 .. 저두용ㅎ 지금은 기부금단체에 정기납부하는거밖에 ..",
          "speaker_id": "1",
          "time": "20210830 15:40"
        },
        {
          "id": "MDRW2100018896.1.5",
          "form": "ㅠ 맞아요",
          "original_form": "ㅠ 맞아요",
          "speaker_id": "2",
          "time": "20210830 16:18"
        },
        {
          "id": "MDRW2100018896.1.6",
          "form": "근데 기부는....",
          "original_form": "근데 기부는....",
          "speaker_id": "2",
          "time": "20210830 16:18"
        },
        {
          "id": "MDRW2100018896.1.7",
          "form": "왜용?",
          "original_form": "왜용?",
          "speaker_id": "1",
          "time": "20210830 16:19"
        },
        {
          "id": "MDRW2100018896.1.8",
          "form": "기부는 뭔가 쉽지 않더라구요",
          "original_form": "기부는 뭔가 쉽지 않더라구요",
          "speaker_id": "2",
          "time": "20210830 16:20"
        },
        {
          "id": "MDRW2100018896.1.9",
          "form": "저도 그냥 턱턱 내고싶은데 망설여지는..",
          "original_form": "저도 그냥 턱턱 내고싶은데 망설여지는..",
          "speaker_id": "2",
          "time": "20210830 16:20"
        },
        {
          "id": "MDRW2100018896.1.10",
          "form": "ㅋㅋㅋㅋ맞아여 글고 그돈도 워낙 단체에서 지네들이 쓰는경우도많고해서",
          "original_form": "ㅋㅋㅋㅋ맞아여 글고 그돈도 워낙 단체에서 지네들이 쓰는경우도많고해서",
          "speaker_id": "1",
          "time": "20210830 16:20"
        },
        {
          "id": "MDRW2100018896.1.11",
          "form": "그렇긴한데 그냥ㅋㅋ 도움을 주고있다..라는 마음의위안이랄까요 ㅋㅋ",
          "original_form": "그렇긴한데 그냥ㅋㅋ 도움을 주고있다..라는 마음의위안이랄까요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:21"
        },
        {
          "id": "MDRW2100018896.1.12",
          "form": "ㅋㅋㅋㅌ 맞아요 ㅎ",
          "original_form": "ㅋㅋㅋㅌ 맞아요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100018896.1.13",
          "form": "어디 기부하고 계시나요?",
          "original_form": "어디 기부하고 계시나요?",
          "speaker_id": "2",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100018896.1.14",
          "form": "그냥 아이들위주로요 ㅎㅎ 기회조차 없는 애들도 참많더라구요 ㅠ",
          "original_form": "그냥 아이들위주로요 ㅎㅎ 기회조차 없는 애들도 참많더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100018896.1.15",
          "form": "대단하시네여",
          "original_form": "대단하시네여",
          "speaker_id": "2",
          "time": "20210831 08:35"
        },
        {
          "id": "MDRW2100018896.1.16",
          "form": "쉬운일이 아닐텐데!!!",
          "original_form": "쉬운일이 아닐텐데!!!",
          "speaker_id": "2",
          "time": "20210831 08:35"
        },
        {
          "id": "MDRW2100018896.1.17",
          "form": "들어가고싶은 동호회있나요?",
          "original_form": "들어가고싶은 동호회있나요?",
          "speaker_id": "1",
