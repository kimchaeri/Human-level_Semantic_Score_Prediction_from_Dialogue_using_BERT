      "utterance": [
        {
          "id": "MDRW2100034282.1.1",
          "form": "오 비혼하시면",
          "original_form": "오 비혼하시면",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100034282.1.2",
          "form": "뭘 준비하셔야하나요?",
          "original_form": "뭘 준비하셔야하나요?",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100034282.1.3",
          "form": "저도 비혼이지만 암것도 하는게없어서요 ㅋㅋㅋㅋ",
          "original_form": "저도 비혼이지만 암것도 하는게없어서요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100034282.1.4",
          "form": "저도 잘 아는 건 아니지만 그냥 지금 든 생각에",
          "original_form": "저도 잘 아는 건 아니지만 그냥 지금 든 생각에",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100034282.1.5",
          "form": "제가 갑자기 죽으면 재산처리나 뭐 사망신고나 그런 처리들을 할 자식이나 배우자가 없으니까",
          "original_form": "제가 갑자기 죽으면 재산처리나 뭐 사망신고나 그런 처리들을 할 자식이나 배우자가 없으니까",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100034282.1.6",
          "form": "변호사한테라도 연락이 가게해서 정리가 되게끔해야겠다 싶어서요 ㅋㅋㅋㅋ",
          "original_form": "변호사한테라도 연락이 가게해서 정리가 되게끔해야겠다 싶어서요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100034282.1.7",
          "form": "아핰ㅋㅋㅋ 유서에 공증받아둔다? 뭐 그렇게들 말하던데",
          "original_form": "아핰ㅋㅋㅋ 유서에 공증받아둔다? 뭐 그렇게들 말하던데",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100034282.1.8",
          "form": "저는 보호자 없으니.. 보험..??생각햇어요",
          "original_form": "저는 보호자 없으니.. 보험..??생각햇어요",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100034282.1.9",
          "form": "보험도 생각해봐야겠네요 ㅋㅋㅋㅋ",
          "original_form": "보험도 생각해봐야겠네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100034282.1.10",
          "form": "실비 하나 있거든요 ㅋㅋㅋㅋㅋ",
          "original_form": "실비 하나 있거든요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100034282.1.11",
          "form": "ㅋㅋㅋㅋㅋ저도 실비하나있는데 암보험 하나 들까봐요",
          "original_form": "ㅋㅋㅋㅋㅋ저도 실비하나있는데 암보험 하나 들까봐요",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100034282.1.12",
          "form": "암이랑 요양원보험도 알아봐야 겠어요 ㅋㅋㅋㅋ",
          "original_form": "암이랑 요양원보험도 알아봐야 겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100034282.1.13",
          "form": "요양원보험도 있나욬ㅋㅋㅋㅋㅋ",
          "original_form": "요양원보험도 있나욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034282.1.14",
          "form": "실버타운은 많이 비쌀까요 ㅋㅋㅋㅋ",
          "original_form": "실버타운은 많이 비쌀까요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034282.1.15",
          "form": "비혼하려면 준비할게 꽤있네요",
          "original_form": "비혼하려면 준비할게 꽤있네요",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034282.1.16",
          "form": "아 그냥 제 생각에 떠오르는 걸로 적은 거에 요ㅋㅋㅋㅋㅋ",
          "original_form": "아 그냥 제 생각에 떠오르는 걸로 적은 거에 요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034282.1.17",
          "form": "저는 반려동물을 안키우려고요 비용도 그렇고 책임을 못질수도있으니",
          "original_form": "저는 반려동물을 안키우려고요 비용도 그렇고 책임을 못질수도있으니",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100034282.1.18",
          "form": "아 그렇네요!",
          "original_form": "아 그렇네요!",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100034282.1.19",
          "form": "저 지금 한 마리 키우고 있는데 이 아이까지만 함께 해야겠어요",
          "original_form": "저 지금 한 마리 키우고 있는데 이 아이까지만 함께 해야겠어요",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100034282.1.20",
          "form": "아 헐 마지막 사랑이라니 좀 그건 슬프네요",
