      "utterance": [
        {
          "id": "MDRW2100017771.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100017771.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017771.1.3",
          "form": "요즘 날씨가 심상치않네요",
          "original_form": "요즘 날씨가 심상치않네요",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017771.1.4",
          "form": "그러게요 너무 오락가락해서 어떻게 맞춰야 할지 모르겠어요...",
          "original_form": "그러게요 너무 오락가락해서 어떻게 맞춰야 할지 모르겠어요...",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017771.1.5",
          "form": "그래도 선선해진건 다행인거 같아요",
          "original_form": "그래도 선선해진건 다행인거 같아요",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017771.1.6",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017771.1.7",
          "form": "아침에 일기예보 보시나요?",
          "original_form": "아침에 일기예보 보시나요?",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017771.1.8",
          "form": "아니요... 퇴근할때 봅니다 ㅋㅋㅋ 그래서 늘 못맞춰요",
          "original_form": "아니요... 퇴근할때 봅니다 ㅋㅋㅋ 그래서 늘 못맞춰요",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017771.1.9",
          "form": "ㅋㅋㅋ최근엔 잘 챙기셨죠?",
          "original_form": "ㅋㅋㅋ최근엔 잘 챙기셨죠?",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017771.1.10",
          "form": "그냥 맞고 다닙니다 ㅎㅎ",
          "original_form": "그냥 맞고 다닙니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017771.1.11",
          "form": "세상에나",
          "original_form": "세상에나",
          "speaker_id": "1",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017771.1.12",
          "form": "내일은 비올 예정이니 우산챙기셔요",
          "original_form": "내일은 비올 예정이니 우산챙기셔요",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017771.1.13",
          "form": "내일 비가 오고 있으면 챙길텐데...",
          "original_form": "내일 비가 오고 있으면 챙길텐데...",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017771.1.14",
          "form": "출근할때는 안올 확률이 높은데..",
          "original_form": "출근할때는 안올 확률이 높은데..",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017771.1.15",
          "form": "젠장이네요...",
          "original_form": "젠장이네요...",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017771.1.16",
          "form": "지금 챙겨요..",
          "original_form": "지금 챙겨요..",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017771.1.17",
          "form": "넘나 일어나기 귀찮습미다",
          "original_form": "넘나 일어나기 귀찮습미다",
          "speaker_id": "2",
          "time": "20210826 21:16"
        }
      ]
