      "utterance": [
        {
          "id": "MDRW2100035626.1.1",
          "form": "혹시 소속사가 jyp인가요",
          "original_form": "혹시 소속사가 jyp인가요",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035626.1.2",
          "form": "아니에요 ㅎㅎㅎ !",
          "original_form": "아니에요 ㅎㅎㅎ !",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035626.1.3",
          "form": "앗...! 이제 어떡하지",
          "original_form": "앗...! 이제 어떡하지",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035626.1.4",
          "form": "히트곡이 있나요",
          "original_form": "히트곡이 있나요",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035626.1.5",
          "form": "네 있습니다 !",
          "original_form": "네 있습니다 !",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035626.1.6",
          "form": "막 탑 히트곡은 아니에요 ㅎㅎ",
          "original_form": "막 탑 히트곡은 아니에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100035626.1.7",
          "form": "앗 어렵다...!",
          "original_form": "앗 어렵다...!",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100035626.1.8",
          "form": "멤버수 변동이 있었나요?",
          "original_form": "멤버수 변동이 있었나요?",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100035626.1.9",
          "form": "엇 네 있었어요 !",
          "original_form": "엇 네 있었어요 !",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100035626.1.10",
          "form": "데뷔한 지는 몇 년 정도 됐어요,?",
          "original_form": "데뷔한 지는 몇 년 정도 됐어요,?",
          "speaker_id": "2",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100035626.1.11",
          "form": "아마 3-4년차일꺼에요 !",
          "original_form": "아마 3-4년차일꺼에요 !",
          "speaker_id": "1",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100035626.1.12",
          "form": "오디션 프로그램 출신 멤버가 있나요",
          "original_form": "오디션 프로그램 출신 멤버가 있나요",
          "speaker_id": "2",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100035626.1.13",
          "form": "네 있습니다 ㅎㅎ",
          "original_form": "네 있습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100035626.1.14",
          "form": "어...려워요...",
          "original_form": "어...려워요...",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100035626.1.15",
          "form": "힌트.... 부탁....",
          "original_form": "힌트.... 부탁....",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100035626.1.16",
          "form": "음.. 논란이 있는 멤버가 있었답니다 ㅎㅎ",
          "original_form": "음.. 논란이 있는 멤버가 있었답니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100035626.1.17",
          "form": "큰 힌트에요 ㅋㅋㅋ",
          "original_form": "큰 힌트에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100035626.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ여돌 논란은 눈감아주자 주의라서!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ여돌 논란은 눈감아주자 주의라서!",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
