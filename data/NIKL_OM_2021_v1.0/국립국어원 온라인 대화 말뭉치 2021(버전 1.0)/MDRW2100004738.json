      "utterance": [
        {
          "id": "MDRW2100004738.1.1",
          "form": "내일 출근하시나요?",
          "original_form": "내일 출근하시나요?",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004738.1.2",
          "form": "이건 두명을 죽이는 행위에요",
          "original_form": "이건 두명을 죽이는 행위에요",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004738.1.3",
          "form": "저는 대학원생이라 내일 출근한답니다",
          "original_form": "저는 대학원생이라 내일 출근한답니다",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004738.1.4",
          "form": "하지만 저는 당신이 저와같은 생활을 한다는건",
          "original_form": "하지만 저는 당신이 저와같은 생활을 한다는건",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004738.1.5",
          "form": "알고있어요",
          "original_form": "알고있어요",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004738.1.6",
          "form": "까짓거 갈때까지 가보죠^^",
          "original_form": "까짓거 갈때까지 가보죠^^",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004738.1.7",
          "form": "교수님이랑 연락은 되시나요?",
          "original_form": "교수님이랑 연락은 되시나요?",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004738.1.8",
          "form": "아 전혀 안되죠 미치죠",
          "original_form": "아 전혀 안되죠 미치죠",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004738.1.9",
          "form": "보통 연락이 안되고 미칠 땐 어떡하시나요?",
          "original_form": "보통 연락이 안되고 미칠 땐 어떡하시나요?",
          "speaker_id": "2",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004738.1.10",
          "form": "저는 오히려 좋아서 술과 게임으로 심신을 달래요",
          "original_form": "저는 오히려 좋아서 술과 게임으로 심신을 달래요",
          "speaker_id": "2",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004738.1.11",
          "form": "네 저도 교수욕 쌔리 갈기다가 살인하는 게임 키곤 해요^^",
          "original_form": "네 저도 교수욕 쌔리 갈기다가 살인하는 게임 키곤 해요^^",
          "speaker_id": "1",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004738.1.12",
          "form": "살인하는 것이 몬스터가 맞나요?",
          "original_form": "살인하는 것이 몬스터가 맞나요?",
          "speaker_id": "2",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004738.1.13",
          "form": "무언가를 투영하는것은 아닌가요?",
          "original_form": "무언가를 투영하는것은 아닌가요?",
          "speaker_id": "2",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004738.1.14",
          "form": "순수한 스트레스 해소용입니다^^",
          "original_form": "순수한 스트레스 해소용입니다^^",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004738.1.15",
          "form": "건전한 게임문화를 즐기는 것이지요",
          "original_form": "건전한 게임문화를 즐기는 것이지요",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004738.1.16",
          "form": "게임은 정말 멋져요",
          "original_form": "게임은 정말 멋져요",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004738.1.17",
          "form": "학업스트레스를 풀기위해",
          "original_form": "학업스트레스를 풀기위해",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004738.1.18",
          "form": "현실에선 할 수 없는 범죄행위를",
          "original_form": "현실에선 할 수 없는 범죄행위를",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004738.1.19",
          "form": "할수잇죠",
          "original_form": "할수잇죠",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004738.1.20",
          "form": "하교 후에 친구랑 게임하는 게 낙입니다..",
          "original_form": "하교 후에 친구랑 게임하는 게 낙입니다..",
          "speaker_id": "1",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004738.1.21",
          "form": "오버쿡드 하고싶어요",
          "original_form": "오버쿡드 하고싶어요",
          "speaker_id": "1",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004738.1.22",
          "form": "저희는",
          "original_form": "저희는",
          "speaker_id": "2",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004738.1.23",
          "form": "학교생활에 대해서만",
          "original_form": "학교생활에 대해서만",
          "speaker_id": "2",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004738.1.24",
          "form": "얘기해야해요",
          "original_form": "얘기해야해요",
          "speaker_id": "2",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004738.1.25",
          "form": "과제가 밀릴 때 비법이 있나요?",
          "original_form": "과제가 밀릴 때 비법이 있나요?",
          "speaker_id": "2",
