      "utterance": [
        {
          "id": "MDRW2100031723.1.1",
          "form": "민초단이신가요?",
          "original_form": "민초단이신가요?",
          "speaker_id": "2",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031723.1.2",
          "form": "민초단이죠ㅎㅎ",
          "original_form": "민초단이죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031723.1.3",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "1",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031723.1.4",
          "form": "저도 없어서 못먹어요ㅋㅋ",
          "original_form": "저도 없어서 못먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031723.1.5",
          "form": "어떤거 좋아해요?",
          "original_form": "어떤거 좋아해요?",
          "speaker_id": "1",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031723.1.6",
          "form": "아무래두 아이스크림이요!",
          "original_form": "아무래두 아이스크림이요!",
          "speaker_id": "2",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031723.1.7",
          "form": "베라 민초 좋죠",
          "original_form": "베라 민초 좋죠",
          "speaker_id": "1",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031723.1.8",
          "form": "네 꼭 있어야해요ㅋㅋ",
          "original_form": "네 꼭 있어야해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031723.1.9",
          "form": "요즘 보니깐 민초가 점령했더라고요",
          "original_form": "요즘 보니깐 민초가 점령했더라고요",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031723.1.10",
          "form": "저도 좀 놀랐어요",
          "original_form": "저도 좀 놀랐어요",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031723.1.11",
          "form": "민초초코파이나 민초아몬드처럼ㅋㅋ",
          "original_form": "민초초코파이나 민초아몬드처럼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031723.1.12",
          "form": "전 민초소주보고 진짜..ㅋㅋㅋ첨에 거짓말인줄",
          "original_form": "전 민초소주보고 진짜..ㅋㅋㅋ첨에 거짓말인줄",
          "speaker_id": "2",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100031723.1.13",
          "form": "민초소주드셔보셨어요?",
          "original_form": "민초소주드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100031723.1.14",
          "form": "전 아직ㅠㅋ",
          "original_form": "전 아직ㅠㅋ",
          "speaker_id": "1",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100031723.1.15",
          "form": "네ㅋㅋㅋ제입에는 그냥 그렇더라고요ㅠ",
          "original_form": "네ㅋㅋㅋ제입에는 그냥 그렇더라고요ㅠ",
