      "utterance": [
        {
          "id": "MDRW2100036543.1.1",
          "form": "그럼 운동을 삶에 곁들여야죠",
          "original_form": "그럼 운동을 삶에 곁들여야죠",
          "speaker_id": "2",
          "time": "20210831 00:18"
        },
        {
          "id": "MDRW2100036543.1.2",
          "form": "ㅠㅠ 운동 하시나여??",
          "original_form": "ㅠㅠ 운동 하시나여??",
          "speaker_id": "1",
          "time": "20210831 00:20"
        },
        {
          "id": "MDRW2100036543.1.3",
          "form": "아뇽 맬 작심삼일되요ㅜㅜ",
          "original_form": "아뇽 맬 작심삼일되요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036543.1.4",
          "form": "저도요ㅠ",
          "original_form": "저도요ㅠ",
          "speaker_id": "1",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036543.1.5",
          "form": "아침에 지하철환승 달리기는 맬하는군요 ㅋㅋ",
          "original_form": "아침에 지하철환승 달리기는 맬하는군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100036543.1.6",
          "form": "아 그것도운동ㅋㅋ",
          "original_form": "아 그것도운동ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:36"
        },
        {
          "id": "MDRW2100036543.1.7",
          "form": "아 안하는것보다 낫죠ㅋㅋ",
          "original_form": "아 안하는것보다 낫죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:04"
        },
        {
          "id": "MDRW2100036543.1.8",
          "form": "저는집순이라",
          "original_form": "저는집순이라",
          "speaker_id": "1",
          "time": "20210831 11:06"
        },
        {
          "id": "MDRW2100036543.1.9",
          "form": "그것도안해요ㅋㅋ가끔 애랑유모차밀고동네한바쿠",
          "original_form": "그것도안해요ㅋㅋ가끔 애랑유모차밀고동네한바쿠",
          "speaker_id": "1",
          "time": "20210831 11:06"
        },
        {
          "id": "MDRW2100036543.1.10",
          "form": "산책이 몸에 좋대요",
          "original_form": "산책이 몸에 좋대요",
          "speaker_id": "2",
          "time": "20210831 11:18"
        },
        {
          "id": "MDRW2100036543.1.11",
          "form": "너무 단시간이라ㅋㅋㅋ",
          "original_form": "너무 단시간이라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:19"
        },
        {
          "id": "MDRW2100036543.1.12",
          "form": "안하는거보다 좋아요",
          "original_form": "안하는거보다 좋아요",
          "speaker_id": "2",
          "time": "20210831 14:03"
        },
        {
          "id": "MDRW2100036543.1.13",
          "form": "그건 그렇죠 ㅎㅎㅎ",
          "original_form": "그건 그렇죠 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036543.1.14",
          "form": "오늘은 누워서 폰보며  복근운동했어요",
          "original_form": "오늘은 누워서 폰보며  복근운동했어요",
