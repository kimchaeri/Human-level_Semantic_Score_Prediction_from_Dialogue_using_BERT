        },
        {
          "id": "MDRW2100003442.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003442.1.3",
          "form": "혹시 어느음식 좋아하세요?",
          "original_form": "혹시 어느음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 23:12"
        },
        {
          "id": "MDRW2100003442.1.4",
          "form": "저는 면류 좋아해요",
          "original_form": "저는 면류 좋아해요",
          "speaker_id": "2",
          "time": "20210820 23:16"
        },
        {
          "id": "MDRW2100003442.1.5",
          "form": "고기도 좋아하고요",
          "original_form": "고기도 좋아하고요",
          "speaker_id": "2",
          "time": "20210820 23:19"
        },
        {
          "id": "MDRW2100003442.1.6",
          "form": "저도 라면좋아하는데",
          "original_form": "저도 라면좋아하는데",
          "speaker_id": "1",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003442.1.7",
          "form": "라면좋아하세요?",
          "original_form": "라면좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003442.1.8",
          "form": "네 아주 좋아해요",
          "original_form": "네 아주 좋아해요",
          "speaker_id": "2",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003442.1.9",
          "form": "일주일에 두세번씩 먹거든요",
          "original_form": "일주일에 두세번씩 먹거든요",
          "speaker_id": "2",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003442.1.10",
          "form": "어떤 라면이 제일 좋으세요?",
          "original_form": "어떤 라면이 제일 좋으세요?",
          "speaker_id": "1",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100003442.1.11",
          "form": "진라면 매운맛을 주로먹어요",
          "original_form": "진라면 매운맛을 주로먹어요",
          "speaker_id": "2",
          "time": "20210820 23:27"
        },
        {
          "id": "MDRW2100003442.1.12",
          "form": "저랑 같네요",
          "original_form": "저랑 같네요",
          "speaker_id": "1",
          "time": "20210820 23:29"
        },
        {
          "id": "MDRW2100003442.1.13",
          "form": "배달음식 자주 드세요?",
          "original_form": "배달음식 자주 드세요?",
          "speaker_id": "1",
          "time": "20210820 23:29"
        },
        {
          "id": "MDRW2100003442.1.14",
          "form": "주말에 한번씩 먹어요 치킨같은거",
          "original_form": "주말에 한번씩 먹어요 치킨같은거",
          "speaker_id": "2",
          "time": "20210820 23:30"
        },
        {
          "id": "MDRW2100003442.1.15",
          "form": "한달에 얼마정도 쓰세요?",
          "original_form": "한달에 얼마정도 쓰세요?",
          "speaker_id": "1",
          "time": "20210820 23:31"
        },
        {
          "id": "MDRW2100003442.1.16",
          "form": "어떤거 배달해 드세요?",
          "original_form": "어떤거 배달해 드세요?",
          "speaker_id": "2",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100003442.1.17",
          "form": "저는 라멘이나 피자같은거 먹네요",
          "original_form": "저는 라멘이나 피자같은거 먹네요",
          "speaker_id": "1",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003442.1.18",
          "form": "라멘을 배달해본적은 없는거 같아요",
          "original_form": "라멘을 배달해본적은 없는거 같아요",
          "speaker_id": "2",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100003442.1.19",
          "form": "전 떡볶이도 좋아해서 많이 배달시켜요",
          "original_form": "전 떡볶이도 좋아해서 많이 배달시켜요",
          "speaker_id": "2",
          "time": "20210820 23:38"
        }
