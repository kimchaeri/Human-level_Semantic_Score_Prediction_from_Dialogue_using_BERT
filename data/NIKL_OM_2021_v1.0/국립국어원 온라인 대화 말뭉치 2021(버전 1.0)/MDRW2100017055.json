      "utterance": [
        {
          "id": "MDRW2100017055.1.1",
          "form": "음 관심은 잇어요",
          "original_form": "음 관심은 잇어요",
          "speaker_id": "2",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100017055.1.2",
          "form": "혼자 소소하게 네일 바르는것 정도?",
          "original_form": "혼자 소소하게 네일 바르는것 정도?",
          "speaker_id": "2",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100017055.1.3",
          "form": "네일 저도 예전에 좋아했어요",
          "original_form": "네일 저도 예전에 좋아했어요",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017055.1.4",
          "form": "저도 요즘은 자주 바르진 않아요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 요즘은 자주 바르진 않아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100017055.1.5",
          "form": "원래 스트레스 쌓일때 매니큐어 하나씩 사고 그랫거든요",
          "original_form": "원래 스트레스 쌓일때 매니큐어 하나씩 사고 그랫거든요",
          "speaker_id": "2",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100017055.1.6",
          "form": "그런분들 많더라구여",
          "original_form": "그런분들 많더라구여",
          "speaker_id": "1",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100017055.1.7",
          "form": "전 젤굳히는 기계까지 샀었어요 ㅋㅋㅋ",
          "original_form": "전 젤굳히는 기계까지 샀었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100017055.1.8",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017055.1.9",
          "form": "그리고 굳이 손톱에 바르지 않더라도",
          "original_form": "그리고 굳이 손톱에 바르지 않더라도",
          "speaker_id": "2",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017055.1.10",
          "form": "스트레스풀기용으로 ㅋㅋㅋ",
          "original_form": "스트레스풀기용으로 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017055.1.11",
          "form": "매니큐어 색깔별로 모아두면 보기만해도 그냥 기분좋음ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "매니큐어 색깔별로 모아두면 보기만해도 그냥 기분좋음ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017055.1.12",
          "form": "맞아요 그런게 있어요",
          "original_form": "맞아요 그런게 있어요",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017055.1.13",
          "form": "네일받아보셨어요?",
          "original_form": "네일받아보셨어요?",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017055.1.14",
          "form": "네 자주는 아니고 가끔",
          "original_form": "네 자주는 아니고 가끔",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017055.1.15",
          "form": "코로나 이후로는 받아본적이 없어요",
          "original_form": "코로나 이후로는 받아본적이 없어요",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017055.1.16",
          "form": "전 한번도 안 해봤어요",
          "original_form": "전 한번도 안 해봤어요",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017055.1.17",
          "form": "기분전환용으로 좋아요",
          "original_form": "기분전환용으로 좋아요",
          "speaker_id": "2",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100017055.1.18",
          "form": "맞아요 가끔 그런게 필요하더라구요",
          "original_form": "맞아요 가끔 그런게 필요하더라구요",
          "speaker_id": "1",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100017055.1.19",
          "form": "네 혼자 손톱 보면서 기분좋아짐ㅋㅋㅋㅋㅋ",
          "original_form": "네 혼자 손톱 보면서 기분좋아짐ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:18"
        }
