      "utterance": [
        {
          "id": "MDRW2100008085.1.1",
          "form": "애들 맨날 그림 그리더니 벌써 다 썼구나",
          "original_form": "애들 맨날 그림 그리더니 벌써 다 썼구나",
          "speaker_id": "2",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008085.1.2",
          "form": "ㅇㅇ 하루에도 수십장씩 그려대니 남아나질 않아",
          "original_form": "ㅇㅇ 하루에도 수십장씩 그려대니 남아나질 않아",
          "speaker_id": "1",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008085.1.3",
          "form": "앞뒤로 다 쓰라고 해 ㅠ",
          "original_form": "앞뒤로 다 쓰라고 해 ㅠ",
          "speaker_id": "2",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008085.1.4",
          "form": "앞에 연필 잔뜩 묻어서 뒤집으면 책상 난리 나 ㅋㅋ",
          "original_form": "앞에 연필 잔뜩 묻어서 뒤집으면 책상 난리 나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008085.1.5",
          "form": "훌 그렇겠네!",
          "original_form": "훌 그렇겠네!",
          "speaker_id": "2",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008085.1.6",
          "form": "그냥 내가 묶어서 이면지로 쓸께",
          "original_form": "그냥 내가 묶어서 이면지로 쓸께",
          "speaker_id": "1",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008085.1.7",
          "form": "ㅇㅇ 계산하거나 이럴 때 간단하게 써",
          "original_form": "ㅇㅇ 계산하거나 이럴 때 간단하게 써",
          "speaker_id": "2",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008085.1.8",
          "form": "종이가 너무 아까워 ㅠㅠ",
          "original_form": "종이가 너무 아까워 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008085.1.9",
          "form": "아껴쓰는 법 배워야지",
          "original_form": "아껴쓰는 법 배워야지",
          "speaker_id": "2",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100008085.1.10",
          "form": "a4 용지 적당한 걸로 알아볼께",
          "original_form": "a4 용지 적당한 걸로 알아볼께",
          "speaker_id": "1",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100008085.1.11",
          "form": "ㅇㅇ 너무 얇은 건 사지마~ 인쇄품질 안좋아",
          "original_form": "ㅇㅇ 너무 얇은 건 사지마~ 인쇄품질 안좋아",
          "speaker_id": "2",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100008085.1.12",
          "form": "ㅇㅇ 알았어. 매번 쓰던 거 사면 되지 뭐 ㅎㅎ",
          "original_form": "ㅇㅇ 알았어. 매번 쓰던 거 사면 되지 뭐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100008085.1.13",
          "form": "응~지금 쓰고 있는 거 정도로",
          "original_form": "응~지금 쓰고 있는 거 정도로",
          "speaker_id": "2",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100008085.1.14",
          "form": "오키~ 도톰하니 좋더라",
          "original_form": "오키~ 도톰하니 좋더라",
          "speaker_id": "1",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100008085.1.15",
          "form": "큰 아들 단어는 워드마스터로 해야겠어",
          "original_form": "큰 아들 단어는 워드마스터로 해야겠어",
