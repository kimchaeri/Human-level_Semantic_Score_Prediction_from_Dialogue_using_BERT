        },
        {
          "id": "MDRW2100046313.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211019 18:28"
        },
        {
          "id": "MDRW2100046313.1.3",
          "form": "먹방 좋아하시나용?",
          "original_form": "먹방 좋아하시나용?",
          "speaker_id": "2",
          "time": "20211019 18:29"
        },
        {
          "id": "MDRW2100046313.1.4",
          "form": "아뇨.. 먹는 걸 별로 안 좋아해서요",
          "original_form": "아뇨.. 먹는 걸 별로 안 좋아해서요",
          "speaker_id": "1",
          "time": "20211019 18:29"
        },
        {
          "id": "MDRW2100046313.1.5",
          "form": "먹는걸 별로 안좋아하신다니...",
          "original_form": "먹는걸 별로 안좋아하신다니...",
          "speaker_id": "2",
          "time": "20211019 18:29"
        },
        {
          "id": "MDRW2100046313.1.6",
          "form": "다른 세계 사람같군요..",
          "original_form": "다른 세계 사람같군요..",
          "speaker_id": "2",
          "time": "20211019 18:29"
        },
        {
          "id": "MDRW2100046313.1.7",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 18:29"
        },
        {
          "id": "MDRW2100046313.1.8",
          "form": "님은 좋아하세요?",
          "original_form": "님은 좋아하세요?",
          "speaker_id": "1",
          "time": "20211019 18:29"
        },
        {
          "id": "MDRW2100046313.1.9",
          "form": "어우 먹는거.. 최고죠",
          "original_form": "어우 먹는거.. 최고죠",
          "speaker_id": "2",
          "time": "20211019 18:30"
        },
        {
          "id": "MDRW2100046313.1.10",
          "form": "삶의 이유죠",
          "original_form": "삶의 이유죠",
          "speaker_id": "2",
          "time": "20211019 18:30"
        },
        {
          "id": "MDRW2100046313.1.11",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 18:30"
        },
        {
          "id": "MDRW2100046313.1.12",
          "form": "뭐 제일 좋아하세요?",
          "original_form": "뭐 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20211019 18:30"
        },
        {
          "id": "MDRW2100046313.1.13",
          "form": "하 너무 많은데",
          "original_form": "하 너무 많은데",
          "speaker_id": "2",
          "time": "20211019 18:30"
        },
        {
          "id": "MDRW2100046313.1.14",
          "form": "올드보이처럼",
          "original_form": "올드보이처럼",
          "speaker_id": "2",
          "time": "20211019 18:30"
        },
        {
          "id": "MDRW2100046313.1.15",
          "form": "한 메뉴만 먹어야한다면",
          "original_form": "한 메뉴만 먹어야한다면",
          "speaker_id": "2",
          "time": "20211019 18:30"
        },
        {
          "id": "MDRW2100046313.1.16",
          "form": "닭볶음탕을 선택할듯합니다...",
          "original_form": "닭볶음탕을 선택할듯합니다...",
          "speaker_id": "2",
          "time": "20211019 18:30"
        },
        {
          "id": "MDRW2100046313.1.17",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20211019 18:30"
        },
        {
          "id": "MDRW2100046313.1.18",
          "form": "저도 닭볶음탕은 좋아해요",
          "original_form": "저도 닭볶음탕은 좋아해요",
          "speaker_id": "1",
          "time": "20211019 18:30"
        },
        {
          "id": "MDRW2100046313.1.19",
          "form": "갑자기 먹고싶네요",
          "original_form": "갑자기 먹고싶네요",
          "speaker_id": "1",
          "time": "20211019 18:30"
        },
        {
          "id": "MDRW2100046313.1.20",
          "form": "캬",
          "original_form": "캬",
          "speaker_id": "2",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046313.1.21",
          "form": "그래도 대화가 좀 통하네요",
          "original_form": "그래도 대화가 좀 통하네요",
          "speaker_id": "2",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046313.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046313.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046313.1.24",
          "form": "엽떡 아시죠",
          "original_form": "엽떡 아시죠",
          "speaker_id": "2",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046313.1.25",
          "form": "당연히 알죠",
          "original_form": "당연히 알죠",
          "speaker_id": "1",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046313.1.26",
          "form": "엽떡에서 파는 닭볶음탕이 정말 기가막힌답니다",
          "original_form": "엽떡에서 파는 닭볶음탕이 정말 기가막힌답니다",
          "speaker_id": "2",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046313.1.27",
          "form": "정말..",
          "original_form": "정말..",
          "speaker_id": "2",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046313.1.28",
          "form": "강추드립니다..",
          "original_form": "강추드립니다..",
          "speaker_id": "2",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046313.1.29",
          "form": "떡볶이 집에서 닭볶음탕이라뇨..",
          "original_form": "떡볶이 집에서 닭볶음탕이라뇨..",
          "speaker_id": "1",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046313.1.30",
          "form": "제 진심이 느껴지시나요",
          "original_form": "제 진심이 느껴지시나요",
          "speaker_id": "2",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046313.1.31",
          "form": "하",
          "original_form": "하",
          "speaker_id": "2",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046313.1.32",
          "form": "그렇게 생각하시겠지만",
          "original_form": "그렇게 생각하시겠지만",
          "speaker_id": "2",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046313.1.33",
          "form": "먹으면 생각이 바뀌실 겁니다....",
          "original_form": "먹으면 생각이 바뀌실 겁니다....",
          "speaker_id": "2",
          "time": "20211019 18:32"
        },
        {
          "id": "MDRW2100046313.1.34",
          "form": "다음에 꼭 먹어보겠습니다 ㅎㅎ",
          "original_form": "다음에 꼭 먹어보겠습니다 ㅎㅎ",
