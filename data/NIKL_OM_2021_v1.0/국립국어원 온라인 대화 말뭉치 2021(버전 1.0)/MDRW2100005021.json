      "utterance": [
        {
          "id": "MDRW2100005021.1.1",
          "form": "name2 공연이나 전시 좋아해?",
          "original_form": "&name2& 공연이나 전시 좋아해?",
          "speaker_id": "1",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100005021.1.2",
          "form": "웅 좋아해! name1는?",
          "original_form": "웅 좋아해! &name1&는?",
          "speaker_id": "2",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100005021.1.3",
          "form": "name1도 좋아해.최근에는 뮤지컬 위키드 봤어",
          "original_form": "&name1&도 좋아해.최근에는 뮤지컬 위키드 봤어",
          "speaker_id": "1",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100005021.1.4",
          "form": "name2은 최근에 머 봤어?",
          "original_form": "&name2&은 최근에 머 봤어?",
          "speaker_id": "1",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100005021.1.5",
          "form": "나는 최근에 dvd로 몬테크리스토 봤어!",
          "original_form": "나는 최근에 dvd로 몬테크리스토 봤어!",
          "speaker_id": "2",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100005021.1.6",
          "form": "위키드는 대극장 공연이야?",
          "original_form": "위키드는 대극장 공연이야?",
          "speaker_id": "2",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100005021.1.7",
          "form": "웅 드림씨어터 가서 봤어",
          "original_form": "웅 드림씨어터 가서 봤어",
          "speaker_id": "1",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100005021.1.8",
          "form": "콘서트 좋아해?",
          "original_form": "콘서트 좋아해?",
          "speaker_id": "1",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100005021.1.9",
          "form": "웅 좋아해 !",
          "original_form": "웅 좋아해 !",
          "speaker_id": "2",
          "time": "20210821 20:02"
        },
        {
          "id": "MDRW2100005021.1.10",
          "form": "최근에 가본 콘서트가 뭐야?",
          "original_form": "최근에 가본 콘서트가 뭐야?",
          "speaker_id": "2",
          "time": "20210821 20:02"
        },
        {
          "id": "MDRW2100005021.1.11",
          "form": "난 마지막으로 에일리 콘서트 갔어",
          "original_form": "난 마지막으로 에일리 콘서트 갔어",
          "speaker_id": "1",
          "time": "20210821 20:02"
        },
        {
          "id": "MDRW2100005021.1.12",
          "form": "에일리 콘서트! 어디서 예매했어?",
          "original_form": "에일리 콘서트! 어디서 예매했어?",
          "speaker_id": "2",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100005021.1.13",
          "form": "아마 인터파크 였던거같아 더보이즈 콘서트 가고싶은데 코로나 때문에 콘서트를 안하네",
          "original_form": "아마 인터파크 였던거같아 더보이즈 콘서트 가고싶은데 코로나 때문에 콘서트를 안하네",
          "speaker_id": "1",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100005021.1.14",
          "form": "더보이즈 좋아하는구나 !",
          "original_form": "더보이즈 좋아하는구나 !",
          "speaker_id": "2",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100005021.1.15",
          "form": "더보이즈 콘서트 가봤어?",
          "original_form": "더보이즈 콘서트 가봤어?",
          "speaker_id": "2",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100005021.1.16",
          "form": "아니 못 가봤어 저번에 열렸었는데 코로나 때문에 취소됐어",
          "original_form": "아니 못 가봤어 저번에 열렸었는데 코로나 때문에 취소됐어",
          "speaker_id": "1",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100005021.1.17",
          "form": "name2은 마지막 콘서트가 뭐야?",
          "original_form": "&name2&은 마지막 콘서트가 뭐야?",
          "speaker_id": "1",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100005021.1.18",
          "form": "나는 god콘서트! 와 너무 오래전이다!",
          "original_form": "나는 god콘서트! 와 너무 오래전이다!",
          "speaker_id": "2",
          "time": "20210821 20:06"
        },
        {
          "id": "MDRW2100005021.1.19",
          "form": "최근에 영화관 가본적있어?",
          "original_form": "최근에 영화관 가본적있어?",
          "speaker_id": "2",
          "time": "20210821 20:06"
        },
        {
          "id": "MDRW2100005021.1.20",
          "form": "코로나 때문에 안간지 오래됐어 무슨 전쟁영화를 마지막으로 본 것 같은데",
          "original_form": "코로나 때문에 안간지 오래됐어 무슨 전쟁영화를 마지막으로 본 것 같은데",
          "speaker_id": "1",
          "time": "20210821 20:07"
        },
        {
          "id": "MDRW2100005021.1.21",
          "form": "추천해줄 영화 있어?",
          "original_form": "추천해줄 영화 있어?",
          "speaker_id": "1",
          "time": "20210821 20:07"
        },
        {
          "id": "MDRW2100005021.1.22",
          "form": "음, 최근에 재밌게 본건 엑시트야",
          "original_form": "음, 최근에 재밌게 본건 엑시트야",
          "speaker_id": "2",
          "time": "20210821 20:07"
        },
        {
          "id": "MDRW2100005021.1.23",
          "form": "조정석이랑 윤아가 나와!",
          "original_form": "조정석이랑 윤아가 나와!",
          "speaker_id": "2",
          "time": "20210821 20:08"
        }
      ]
    }
