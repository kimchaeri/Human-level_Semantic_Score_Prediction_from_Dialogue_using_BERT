      "utterance": [
        {
          "id": "MDRW2100030779.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 12:26"
        },
        {
          "id": "MDRW2100030779.1.2",
          "form": "반가워요",
          "original_form": "반가워요",
          "speaker_id": "2",
          "time": "20210904 12:27"
        },
        {
          "id": "MDRW2100030779.1.3",
          "form": "넵 반가워요 오늘 날씨가좋아요",
          "original_form": "넵 반가워요 오늘 날씨가좋아요",
          "speaker_id": "1",
          "time": "20210904 12:27"
        },
        {
          "id": "MDRW2100030779.1.4",
          "form": "그러게요! 해가쨍쨍하네요",
          "original_form": "그러게요! 해가쨍쨍하네요",
          "speaker_id": "2",
          "time": "20210904 12:28"
        },
        {
          "id": "MDRW2100030779.1.5",
          "form": "주말인데 어디 가시나여",
          "original_form": "주말인데 어디 가시나여",
          "speaker_id": "2",
          "time": "20210904 12:29"
        },
        {
          "id": "MDRW2100030779.1.6",
          "form": "맞아요~ 놀러가기 좋은 날씨에요",
          "original_form": "맞아요~ 놀러가기 좋은 날씨에요",
          "speaker_id": "1",
          "time": "20210904 12:29"
        },
        {
          "id": "MDRW2100030779.1.7",
          "form": "오늘 대학가 가려고요! 오늘 약속있으세요?",
          "original_form": "오늘 대학가 가려고요! 오늘 약속있으세요?",
          "speaker_id": "1",
          "time": "20210904 12:29"
        },
        {
          "id": "MDRW2100030779.1.8",
          "form": "아뇨 오늘은 없고 내일이네요ㅠㅜ내일도 날씨가 좋으려나요",
          "original_form": "아뇨 오늘은 없고 내일이네요ㅠㅜ내일도 날씨가 좋으려나요",
          "speaker_id": "2",
          "time": "20210904 12:30"
        },
        {
          "id": "MDRW2100030779.1.9",
          "form": "내일도 좋을거에요",
          "original_form": "내일도 좋을거에요",
          "speaker_id": "1",
          "time": "20210904 12:31"
        },
        {
          "id": "MDRW2100030779.1.10",
          "form": "오늘 푹 쉬고 내일 신나게 노시면 되겠어요!",
          "original_form": "오늘 푹 쉬고 내일 신나게 노시면 되겠어요!",
          "speaker_id": "1",
          "time": "20210904 12:32"
        },
        {
          "id": "MDRW2100030779.1.11",
          "form": "네 그럼 좋겠네요ㅎㅎㅎ",
          "original_form": "네 그럼 좋겠네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030779.1.12",
          "form": "낮에는 해가 너무 쨍쨍해보이는데 양산챙겨가세요ㅜ",
          "original_form": "낮에는 해가 너무 쨍쨍해보이는데 양산챙겨가세요ㅜ",
          "speaker_id": "2",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030779.1.13",
          "form": "ㅎㅎ감사합니다 요새 우양산 필수에요",
          "original_form": "ㅎㅎ감사합니다 요새 우양산 필수에요",
          "speaker_id": "1",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030779.1.14",
          "form": "넷플릿스 보시나요?",
          "original_form": "넷플릿스 보시나요?",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100030779.1.15",
          "form": "결제는 하고있는데...",
          "original_form": "결제는 하고있는데...",
          "speaker_id": "2",
          "time": "20210904 22:08"
        },
        {
          "id": "MDRW2100030779.1.16",
          "form": "뭔가 집중해서 보기가 힘드네요 요즘에ㅋㅋ",
          "original_form": "뭔가 집중해서 보기가 힘드네요 요즘에ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:08"
        },
        {
          "id": "MDRW2100030779.1.17",
          "form": "뭐 보세요?",
          "original_form": "뭐 보세요?",
          "speaker_id": "2",
          "time": "20210904 22:08"
        },
        {
          "id": "MDRW2100030779.1.18",
          "form": "ㅎㅎ에밀리 파리에가다? 이거최근에봤는데",
          "original_form": "ㅎㅎ에밀리 파리에가다? 이거최근에봤는데",
          "speaker_id": "1",
          "time": "20210904 22:21"
        },
        {
          "id": "MDRW2100030779.1.19",
          "form": "재밌었어요",
          "original_form": "재밌었어요",
          "speaker_id": "1",
          "time": "20210904 22:21"
        },
        {
          "id": "MDRW2100030779.1.20",
          "form": "오 참고할게요ㅋㅋ저는 그 영국드라마 보다가 말았는데",
          "original_form": "오 참고할게요ㅋㅋ저는 그 영국드라마 보다가 말았는데",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100030779.1.21",
          "form": "갑자기 제목이 기억 안나버리기 .....",
          "original_form": "갑자기 제목이 기억 안나버리기 .....",
