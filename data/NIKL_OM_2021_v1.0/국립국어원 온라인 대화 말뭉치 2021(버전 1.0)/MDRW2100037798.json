      "utterance": [
        {
          "id": "MDRW2100037798.1.1",
          "form": "보내거야",
          "original_form": "보내거야",
          "speaker_id": "2",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100037798.1.2",
          "form": "영상",
          "original_form": "영상",
          "speaker_id": "2",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100037798.1.3",
          "form": "버전 2개 만들어서",
          "original_form": "버전 2개 만들어서",
          "speaker_id": "2",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100037798.1.4",
          "form": "엽기, 정상",
          "original_form": "엽기, 정상",
          "speaker_id": "2",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100037798.1.5",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100037798.1.6",
          "form": "결혼...",
          "original_form": "결혼...",
          "speaker_id": "1",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100037798.1.7",
          "form": "그치 결혼 하겠구나...",
          "original_form": "그치 결혼 하겠구나...",
          "speaker_id": "1",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100037798.1.8",
          "form": "우리중 한명이상은 하지 않을까...?",
          "original_form": "우리중 한명이상은 하지 않을까...?",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100037798.1.9",
          "form": "일단 난 아님",
          "original_form": "일단 난 아님",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100037798.1.10",
          "form": "나도 안 할 거야",
          "original_form": "나도 안 할 거야",
          "speaker_id": "1",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100037798.1.11",
          "form": "결혼, 임신, 출산, 육아 다 안 할 거야...",
          "original_form": "결혼, 임신, 출산, 육아 다 안 할 거야...",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100037798.1.12",
          "form": "하면 결혼과 육아 둘중에 하나 할듯",
          "original_form": "하면 결혼과 육아 둘중에 하나 할듯",
          "speaker_id": "2",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100037798.1.13",
          "form": "돈 많이 벌어서...",
          "original_form": "돈 많이 벌어서...",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100037798.1.14",
          "form": "혼자 아기 키울래",
          "original_form": "혼자 아기 키울래",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100037798.1.15",
          "form": "해외 나가서 ㅎ",
          "original_form": "해외 나가서 ㅎ",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100037798.1.16",
          "form": "그래 여긴 환경이 좋지 않아...",
          "original_form": "그래 여긴 환경이 좋지 않아...",
          "speaker_id": "1",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100037798.1.17",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100037798.1.18",
          "form": "애기한테 안좋아",
          "original_form": "애기한테 안좋아",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100037798.1.19",
          "form": "가부장적이고 약자에 대한 혐오가 너무 심해",
          "original_form": "가부장적이고 약자에 대한 혐오가 너무 심해",
          "speaker_id": "1",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100037798.1.20",
          "form": "언어치료 열심히",
          "original_form": "언어치료 열심히",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100037798.1.21",
          "form": "배워서",
          "original_form": "배워서",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100037798.1.22",
          "form": "나가야지",
          "original_form": "나가야지",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037798.1.23",
          "form": "여기보다",
          "original_form": "여기보다",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037798.1.24",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037798.1.25",
          "form": "해외가 더 취급이 좋아",
          "original_form": "해외가 더 취급이 좋아",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037798.1.26",
          "form": "해외에서는",
          "original_form": "해외에서는",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037798.1.27",
          "form": "언어치료사가",
          "original_form": "언어치료사가",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037798.1.28",
          "form": "의사랑 거의 동급이야",
          "original_form": "의사랑 거의 동급이야",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037798.1.29",
          "form": "한국이야",
          "original_form": "한국이야",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037798.1.30",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037798.1.31",
          "form": "이 위치지만",
          "original_form": "이 위치지만",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037798.1.32",
          "form": "왕",
          "original_form": "왕",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037798.1.33",
          "form": "그럼 해외로 가야지",
          "original_form": "그럼 해외로 가야지",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037798.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
