        },
        {
          "id": "MDRW2100031466.1.2",
          "form": "저알바했었어가지고",
          "original_form": "저알바했었어가지고",
          "speaker_id": "2",
          "time": "20210904 21:47"
        },
        {
          "id": "MDRW2100031466.1.3",
          "form": "잘까진라니지만",
          "original_form": "잘까진라니지만",
          "speaker_id": "2",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100031466.1.4",
          "form": "우아 !! 진짜요?? 저는 익은지 안익은지 몰라서",
          "original_form": "우아 !! 진짜요?? 저는 익은지 안익은지 몰라서",
          "speaker_id": "1",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100031466.1.5",
          "form": "막집어 먹어요 ㅋㅋ",
          "original_form": "막집어 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100031466.1.6",
          "form": "ㅋㅋㅋㅋㅋ헐 그럼탈나요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ헐 그럼탈나요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100031466.1.7",
          "form": "주기적으로 구충제 먹고있어요 ㅋㅋ",
          "original_form": "주기적으로 구충제 먹고있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100031466.1.8",
          "form": "알바하시면 고기 많이 드셧겠어요!!",
          "original_form": "알바하시면 고기 많이 드셧겠어요!!",
          "speaker_id": "1",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100031466.1.9",
          "form": "지금은 일관둬서..ㅎ",
          "original_form": "지금은 일관둬서..ㅎ",
          "speaker_id": "2",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100031466.1.10",
          "form": "고기는 소고기 돼지고기 뭘더 좋아하세요",
          "original_form": "고기는 소고기 돼지고기 뭘더 좋아하세요",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031466.1.11",
          "form": "전 둘다좋아햐요 ㅋㅋ",
          "original_form": "전 둘다좋아햐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100031466.1.12",
          "form": "저는 얇은게 좋아요 턱이 약해서 ㅋㅋ",
          "original_form": "저는 얇은게 좋아요 턱이 약해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100031466.1.13",
          "form": "대패 삼겹살 !",
          "original_form": "대패 삼겹살 !",
          "speaker_id": "1",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100031466.1.14",
          "form": "아!!먼느낌인지알죠",
          "original_form": "아!!먼느낌인지알죠",
          "speaker_id": "2",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100031466.1.15",
          "form": "저는 고기앞에선 가리는게없어욬ㅋ",
          "original_form": "저는 고기앞에선 가리는게없어욬ㅋ",
          "speaker_id": "2",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100031466.1.16",
          "form": "고기에다가 머 찍어먹는거 좋아하세요",
          "original_form": "고기에다가 머 찍어먹는거 좋아하세요",
          "speaker_id": "1",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100031466.1.17",
          "form": "쌈장이요!!",
          "original_form": "쌈장이요!!",
          "speaker_id": "2",
          "time": "20210905 08:05"
        },
        {
