      "utterance": [
        {
          "id": "MDRW2100040428.1.1",
          "form": "주제를 바꿔보았어요! ㅋㅋㅋㅋ",
          "original_form": "주제를 바꿔보았어요! ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:37"
        },
        {
          "id": "MDRW2100040428.1.2",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "1",
          "time": "20210904 19:37"
        },
        {
          "id": "MDRW2100040428.1.3",
          "form": "아녀 아직이요! name1님은용?",
          "original_form": "아녀 아직이요! &name1&님은용?",
          "speaker_id": "2",
          "time": "20210904 19:38"
        },
        {
          "id": "MDRW2100040428.1.4",
          "form": "저도 이제 슬슬 먹으려구요 ㅎㅎㅎ",
          "original_form": "저도 이제 슬슬 먹으려구요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 19:39"
        },
        {
          "id": "MDRW2100040428.1.5",
          "form": "아까 점심에는 수제비를 먹었답니다",
          "original_form": "아까 점심에는 수제비를 먹었답니다",
          "speaker_id": "1",
          "time": "20210904 19:39"
        },
        {
          "id": "MDRW2100040428.1.6",
          "form": "name2님은 점심에 뭘 드셨나요?",
          "original_form": "&name2&님은 점심에 뭘 드셨나요?",
          "speaker_id": "1",
          "time": "20210904 19:39"
        },
        {
          "id": "MDRW2100040428.1.7",
          "form": "우와 저 수제비 좋아해요 못먹은지 오래됐다 ㅠㅠㅠㅠ",
          "original_form": "우와 저 수제비 좋아해요 못먹은지 오래됐다 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 19:41"
        },
        {
          "id": "MDRW2100040428.1.8",
          "form": "저 점심에 간단하게 동그랑땡에다가 밥에 김싸서 먹었어요ㅋㅋㅋㅋ",
          "original_form": "저 점심에 간단하게 동그랑땡에다가 밥에 김싸서 먹었어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:41"
        },
        {
          "id": "MDRW2100040428.1.9",
          "form": "어떤 수제비였나요?",
          "original_form": "어떤 수제비였나요?",
          "speaker_id": "2",
          "time": "20210904 19:42"
        },
        {
          "id": "MDRW2100040428.1.10",
          "form": "집에서 엄마랑 아빠가 만들어준 수제비였어요 ",
          "original_form": "집에서 엄마랑 아빠가 만들어준 수제비였어요 ",
          "speaker_id": "1",
          "time": "20210904 19:43"
        },
        {
          "id": "MDRW2100040428.1.11",
          "form": "근데 사실... 맛은... 그닥...이었답니다ㅠ",
          "original_form": "근데 사실... 맛은... 그닥...이었답니다ㅠ",
          "speaker_id": "1",
          "time": "20210904 19:43"
        },
        {
          "id": "MDRW2100040428.1.12",
          "form": "역시 사먹는게 맛있는걸까요?",
          "original_form": "역시 사먹는게 맛있는걸까요?",
          "speaker_id": "1",
          "time": "20210904 19:43"
        },
        {
          "id": "MDRW2100040428.1.13",
          "form": "동그랑땡 맛있었겠어요!!!!",
          "original_form": "동그랑땡 맛있었겠어요!!!!",
          "speaker_id": "1",
          "time": "20210904 19:43"
        },
        {
          "id": "MDRW2100040428.1.14",
          "form": "ㅋㅋㅋㅋㅋ 사먹는건 다 조미료땜에 맛있는거랬어요",
          "original_form": "ㅋㅋㅋㅋㅋ 사먹는건 다 조미료땜에 맛있는거랬어요",
          "speaker_id": "2",
          "time": "20210904 19:44"
        },
        {
          "id": "MDRW2100040428.1.15",
          "form": "동그랑땡 비비고 최고",
          "original_form": "동그랑땡 비비고 최고",
          "speaker_id": "2",
          "time": "20210904 19:44"
        },
        {
          "id": "MDRW2100040428.1.16",
          "form": "비비고 짱입니다",
          "original_form": "비비고 짱입니다",
          "speaker_id": "2",
          "time": "20210904 19:44"
        },
        {
          "id": "MDRW2100040428.1.17",
          "form": "우와!",
          "original_form": "우와!",
          "speaker_id": "1",
          "time": "20210904 19:44"
        },
        {
          "id": "MDRW2100040428.1.18",
          "form": "맞아요 비비고는",
          "original_form": "맞아요 비비고는",
          "speaker_id": "1",
          "time": "20210904 19:44"
        },
        {
          "id": "MDRW2100040428.1.19",
          "form": "만두도 맛있잖아요!!!",
          "original_form": "만두도 맛있잖아요!!!",
          "speaker_id": "1",
          "time": "20210904 19:45"
        },
        {
          "id": "MDRW2100040428.1.20",
          "form": "아니다 만두가 처음이었죠? 비비고가?!",
          "original_form": "아니다 만두가 처음이었죠? 비비고가?!",
          "speaker_id": "1",
          "time": "20210904 19:45"
        },
        {
          "id": "MDRW2100040428.1.21",
          "form": "네네 맞아요!",
          "original_form": "네네 맞아요!",
          "speaker_id": "2",
          "time": "20210904 19:45"
        },
        {
          "id": "MDRW2100040428.1.22",
          "form": "암튼 비비고 알게된 이후로 명절 동그랑땡은 무조건 비비고 사서 굽습니다 ㅋㅋㅋㅋ",
          "original_form": "암튼 비비고 알게된 이후로 명절 동그랑땡은 무조건 비비고 사서 굽습니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100040428.1.23",
          "form": "와 좋은거 알아가네요 ㅎㅎㅎ",
          "original_form": "와 좋은거 알아가네요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100040428.1.24",
          "form": "직접 치대서 만든것보다 훨씬 맛있어요",
          "original_form": "직접 치대서 만든것보다 훨씬 맛있어요",
          "speaker_id": "2",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100040428.1.25",
          "form": "비비고.... 동그랑땡...",
          "original_form": "비비고.... 동그랑땡...",
          "speaker_id": "1",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100040428.1.26",
          "form": "최고!!",
          "original_form": "최고!!",
          "speaker_id": "2",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100040428.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
