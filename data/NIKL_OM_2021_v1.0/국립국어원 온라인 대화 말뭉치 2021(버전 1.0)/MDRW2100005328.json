      "utterance": [
        {
          "id": "MDRW2100005328.1.1",
          "form": "미역국은 아빠가 끓여주셨어요",
          "original_form": "미역국은 아빠가 끓여주셨어요",
          "speaker_id": "1",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005328.1.2",
          "form": "무슨 날은 아닌데 미역국이 나와서 띠용했어요",
          "original_form": "무슨 날은 아닌데 미역국이 나와서 띠용했어요",
          "speaker_id": "1",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005328.1.3",
          "form": "좋아하는 국이 있나요?",
          "original_form": "좋아하는 국이 있나요?",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005328.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005328.1.5",
          "form": "저는 음...감자 들어간 국 칼칼한 국 종류를 좋아해요",
          "original_form": "저는 음...감자 들어간 국 칼칼한 국 종류를 좋아해요",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005328.1.6",
          "form": "예를 들면?",
          "original_form": "예를 들면?",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005328.1.7",
          "form": "감자 들어간 칼칼한 국이 뭐가있죠?",
          "original_form": "감자 들어간 칼칼한 국이 뭐가있죠?",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005328.1.8",
          "form": "그냥 무국이든 된장국이든",
          "original_form": "그냥 무국이든 된장국이든",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005328.1.9",
          "form": "아무 국에 감자가 들어가요 저희집은",
          "original_form": "아무 국에 감자가 들어가요 저희집은",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005328.1.10",
          "form": "무국에요?",
          "original_form": "무국에요?",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005328.1.11",
          "form": "생소한데 어떤가요?",
          "original_form": "생소한데 어떤가요?",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005328.1.12",
          "form": "청양 고오오우추를 좀 썰어 넣으면",
          "original_form": "청양 고오오우추를 좀 썰어 넣으면",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005328.1.13",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005328.1.14",
          "form": "아 무국에 감자를 넣으면 감자국이 되긴 하지요....",
          "original_form": "아 무국에 감자를 넣으면 감자국이 되긴 하지요....",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005328.1.15",
          "form": "맞아요 국이 좀 부족하다 싶으면 고추넣으면 되더라구요",
          "original_form": "맞아요 국이 좀 부족하다 싶으면 고추넣으면 되더라구요",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005328.1.16",
          "form": "된장국에도 감자가 들어가구요",
          "original_form": "된장국에도 감자가 들어가구요",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005328.1.17",
          "form": "저희집은 좀 칼칼한걸 좋아해서 뭐든 조금 칼칼하게 만드세요",
          "original_form": "저희집은 좀 칼칼한걸 좋아해서 뭐든 조금 칼칼하게 만드세요",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005328.1.18",
          "form": "된장국에 감자넣으면 뭔가 좀 더 맛있는 거 같아요",
          "original_form": "된장국에 감자넣으면 뭔가 좀 더 맛있는 거 같아요",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005328.1.19",
          "form": "제가 감자를 겁나 좋아해서 맛있죠",
          "original_form": "제가 감자를 겁나 좋아해서 맛있죠",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005328.1.20",
          "form": "정작 국은 잘 안먹어요",
          "original_form": "정작 국은 잘 안먹어요",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005328.1.21",
          "form": "말아먹어야 먹게되요",
          "original_form": "말아먹어야 먹게되요",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005328.1.22",
          "form": "저도요ㅋㅋ 국은 건더기만 먹어요",
          "original_form": "저도요ㅋㅋ 국은 건더기만 먹어요",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005328.1.23",
          "form": "약간 국물까지 먹기에는 좀 부담스러운?",
          "original_form": "약간 국물까지 먹기에는 좀 부담스러운?",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005328.1.24",
          "form": "그게 좀 있는 거 같아요",
          "original_form": "그게 좀 있는 거 같아요",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005328.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005328.1.26",
          "form": "원래 밥 먹을때 액체 많이 먹으면 안된다잖아요",
          "original_form": "원래 밥 먹을때 액체 많이 먹으면 안된다잖아요",
          "speaker_id": "2",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005328.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005328.1.28",
          "form": "식도에 안좋대요",
          "original_form": "식도에 안좋대요",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005328.1.29",
          "form": "근데 저는... 그냥 원래 반찬도 잘안먹는 성향이라",
          "original_form": "근데 저는... 그냥 원래 반찬도 잘안먹는 성향이라",
