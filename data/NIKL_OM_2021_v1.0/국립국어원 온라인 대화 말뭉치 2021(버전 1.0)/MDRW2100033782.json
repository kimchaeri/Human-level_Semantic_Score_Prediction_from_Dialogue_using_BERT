        {
          "id": "MDRW2100033782.1.31",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:53"
        },
        {
          "id": "MDRW2100033782.1.32",
          "form": "함 만나서 음식투어해야되나 ㅎ",
          "original_form": "함 만나서 음식투어해야되나 ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:53"
        },
        {
          "id": "MDRW2100033782.1.33",
          "form": "저도아메 갈증나서 물없을때나 먹을정도ㅋ",
          "original_form": "저도아메 갈증나서 물없을때나 먹을정도ㅋ",
          "speaker_id": "2",
          "time": "20210825 01:54"
        },
        {
          "id": "MDRW2100033782.1.34",
          "form": "ㅋㅋㅋㅋ 입맛이 너무잘맞을것같네요ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 입맛이 너무잘맞을것같네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:54"
        },
        {
          "id": "MDRW2100033782.1.35",
          "form": "입맛이 너무",
          "original_form": "입맛이 너무",
