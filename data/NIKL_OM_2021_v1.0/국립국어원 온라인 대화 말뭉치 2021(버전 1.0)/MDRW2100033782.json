      "utterance": [
        {
          "id": "MDRW2100033782.1.1",
          "form": "안녕하세요ㅎ",
          "original_form": "안녕하세요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:38"
        },
        {
          "id": "MDRW2100033782.1.2",
          "form": "저 이번꺼하고 자러갑니다ㅋㅋ",
          "original_form": "저 이번꺼하고 자러갑니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:39"
        },
        {
          "id": "MDRW2100033782.1.3",
          "form": "이번에 뭐할까요?",
          "original_form": "이번에 뭐할까요?",
          "speaker_id": "2",
          "time": "20210825 01:39"
        },
        {
          "id": "MDRW2100033782.1.4",
          "form": "비냉vs물냉 뭐좋아하세요?",
          "original_form": "비냉vs물냉 뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 01:43"
        },
        {
          "id": "MDRW2100033782.1.5",
          "form": "비냉이요ㅎ",
          "original_form": "비냉이요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:43"
        },
        {
          "id": "MDRW2100033782.1.6",
          "form": "전 회냉면좋아해요",
          "original_form": "전 회냉면좋아해요",
          "speaker_id": "2",
          "time": "20210825 01:43"
        },
        {
          "id": "MDRW2100033782.1.7",
          "form": "와우 회냉면",
          "original_form": "와우 회냉면",
          "speaker_id": "1",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100033782.1.8",
          "form": "ㅋㅋㅋㅋ 님저랑",
          "original_form": "ㅋㅋㅋㅋ 님저랑",
          "speaker_id": "1",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100033782.1.9",
          "form": "취향이 비슷하시네요",
          "original_form": "취향이 비슷하시네요",
          "speaker_id": "1",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100033782.1.10",
          "form": "광어vs연어??",
          "original_form": "광어vs연어??",
          "speaker_id": "1",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100033782.1.11",
          "form": "회는 광어죠ㅎ",
          "original_form": "회는 광어죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100033782.1.12",
          "form": "쫄깃한 식감 좋아해요",
          "original_form": "쫄깃한 식감 좋아해요",
          "speaker_id": "2",
          "time": "20210825 01:47"
        },
        {
          "id": "MDRW2100033782.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:47"
        },
        {
          "id": "MDRW2100033782.1.14",
          "form": "님저랑 입맛이 천생연분이네요ㅋㅋㅋㅋ",
          "original_form": "님저랑 입맛이 천생연분이네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:47"
        },
        {
          "id": "MDRW2100033782.1.15",
          "form": "김치찌개vs된장찌개?",
          "original_form": "김치찌개vs된장찌개?",
          "speaker_id": "1",
          "time": "20210825 01:47"
        },
        {
          "id": "MDRW2100033782.1.16",
          "form": "요건 좀 고민이ㅋ",
          "original_form": "요건 좀 고민이ㅋ",
          "speaker_id": "2",
          "time": "20210825 01:47"
        },
        {
          "id": "MDRW2100033782.1.17",
          "form": "김치찌개에 돼지고기 들어가나요?ㅎ",
          "original_form": "김치찌개에 돼지고기 들어가나요?ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:48"
        },
        {
          "id": "MDRW2100033782.1.18",
          "form": "ㅋㅋㅋ 들어가죠",
          "original_form": "ㅋㅋㅋ 들어가죠",
          "speaker_id": "1",
          "time": "20210825 01:49"
        },
        {
          "id": "MDRW2100033782.1.19",
          "form": "목살로요!",
          "original_form": "목살로요!",
          "speaker_id": "1",
          "time": "20210825 01:49"
        },
        {
          "id": "MDRW2100033782.1.20",
          "form": "그럼  김치찌개요ㅎ",
          "original_form": "그럼  김치찌개요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:49"
        },
        {
          "id": "MDRW2100033782.1.21",
          "form": "ㅋㅋㅋ 계속똑같네요",
          "original_form": "ㅋㅋㅋ 계속똑같네요",
          "speaker_id": "1",
          "time": "20210825 01:50"
        },
        {
          "id": "MDRW2100033782.1.22",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210825 01:50"
        },
        {
          "id": "MDRW2100033782.1.23",
          "form": "아메리카노vs프라푸치노?",
          "original_form": "아메리카노vs프라푸치노?",
          "speaker_id": "1",
          "time": "20210825 01:51"
        },
        {
          "id": "MDRW2100033782.1.24",
          "form": "전  아메를  안마셔요^^",
          "original_form": "전  아메를  안마셔요^^",
          "speaker_id": "2",
          "time": "20210825 01:51"
        },
        {
          "id": "MDRW2100033782.1.25",
          "form": "무조건 프라푸치노",
          "original_form": "무조건 프라푸치노",
          "speaker_id": "2",
          "time": "20210825 01:51"
        },
        {
          "id": "MDRW2100033782.1.26",
          "form": "와우...미쳤다리",
          "original_form": "와우...미쳤다리",
          "speaker_id": "1",
          "time": "20210825 01:52"
        },
        {
          "id": "MDRW2100033782.1.27",
          "form": "님 저인가요?",
          "original_form": "님 저인가요?",
