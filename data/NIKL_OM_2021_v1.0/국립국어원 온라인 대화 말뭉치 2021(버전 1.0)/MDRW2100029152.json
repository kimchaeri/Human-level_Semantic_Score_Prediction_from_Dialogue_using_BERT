      "utterance": [
        {
          "id": "MDRW2100029152.1.1",
          "form": "다른 웹툰은",
          "original_form": "다른 웹툰은",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029152.1.2",
          "form": "뭐 보시나여?",
          "original_form": "뭐 보시나여?",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029152.1.3",
          "form": "귀곡의 문이요",
          "original_form": "귀곡의 문이요",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029152.1.4",
          "form": "이영싫 작가님이라 믿고 보고있어요",
          "original_form": "이영싫 작가님이라 믿고 보고있어요",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029152.1.5",
          "form": "되게 많이 보시는군요!!!!",
          "original_form": "되게 많이 보시는군요!!!!",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100029152.1.6",
          "form": "많이 보는건 아니에요ㅋㅋㅋ",
          "original_form": "많이 보는건 아니에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029152.1.7",
          "form": "예전에는 요일마다 몇개씩 봤는데",
          "original_form": "예전에는 요일마다 몇개씩 봤는데",
          "speaker_id": "1",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029152.1.8",
          "form": "이젠 귀찮더라구요",
          "original_form": "이젠 귀찮더라구요",
          "speaker_id": "1",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029152.1.9",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029152.1.10",
          "form": "전 며칠전에",
          "original_form": "전 며칠전에",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029152.1.11",
          "form": "라일락 이백퍼인가",
          "original_form": "라일락 이백퍼인가",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029152.1.12",
          "form": "그거 봤어요 ㄹ",
          "original_form": "그거 봤어요 ㄹ",
          "speaker_id": "2",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029152.1.13",
          "form": "오 무슨 장르에요?",
          "original_form": "오 무슨 장르에요?",
          "speaker_id": "1",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029152.1.14",
          "form": "약간.. 판타지..?",
          "original_form": "약간.. 판타지..?",
          "speaker_id": "2",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029152.1.15",
          "form": "영혼이 바뀌는ㅋㅋㅋㅋ",
          "original_form": "영혼이 바뀌는ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029152.1.16",
          "form": "요새 그런거 유행인가봐요",
          "original_form": "요새 그런거 유행인가봐요",
          "speaker_id": "1",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029152.1.17",
          "form": "다른세계를 가던가 하는거...",
          "original_form": "다른세계를 가던가 하는거...",
          "speaker_id": "1",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029152.1.18",
          "form": "ㅋㅋㅋㅋ아 그래요? 이겅 완결해서",
          "original_form": "ㅋㅋㅋㅋ아 그래요? 이겅 완결해서",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100029152.1.19",
          "form": "하루만에 정주행 끝냈어여 ㅎ",
          "original_form": "하루만에 정주행 끝냈어여 ㅎ",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100029152.1.20",
          "form": "재밌나봐요",
          "original_form": "재밌나봐요",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029152.1.21",
          "form": "ㅋㅋㅋ네 제가 좋아하는.웹툰 재질이였어요",
          "original_form": "ㅋㅋㅋ네 제가 좋아하는.웹툰 재질이였어요",
          "speaker_id": "2",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029152.1.22",
          "form": "찾아봐야 겠네요ㅋㅋㅋ",
          "original_form": "찾아봐야 겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:20"
        },
        {
          "id": "MDRW2100029152.1.23",
          "form": "근데 좋아하실지믄 모르겠네요 ㅎ",
          "original_form": "근데 좋아하실지믄 모르겠네요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 08:27"
        }
      ]
