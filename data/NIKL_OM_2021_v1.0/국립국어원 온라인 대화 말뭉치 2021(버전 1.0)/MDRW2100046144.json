      "utterance": [
        {
          "id": "MDRW2100046144.1.1",
          "form": "사고픈 물건 있으세요?",
          "original_form": "사고픈 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046144.1.2",
          "form": "음 신발?",
          "original_form": "음 신발?",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046144.1.3",
          "form": "삭스부츠?",
          "original_form": "삭스부츠?",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046144.1.4",
          "form": "저도 신발이요 벙수운동화 사고 싶어요",
          "original_form": "저도 신발이요 벙수운동화 사고 싶어요",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046144.1.5",
          "form": "방수운동화요",
          "original_form": "방수운동화요",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046144.1.6",
          "form": "어디꺼요? ㅎㅎ",
          "original_form": "어디꺼요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046144.1.7",
          "form": "스케쳐스랑 아식스 고민 중이에요",
          "original_form": "스케쳐스랑 아식스 고민 중이에요",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046144.1.8",
          "form": "저 스케쳐스 조아해요",
          "original_form": "저 스케쳐스 조아해요",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046144.1.9",
          "form": "진짜편함ㅜㅜ",
          "original_form": "진짜편함ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046144.1.10",
          "form": "저두요 스케쳐스 넘 편해요 ㅎㅎ",
          "original_form": "저두요 스케쳐스 넘 편해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046144.1.11",
          "form": "고워크!!",
          "original_form": "고워크!!",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046144.1.12",
          "form": "엄마사드렷어여",
          "original_form": "엄마사드렷어여",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046144.1.13",
          "form": "고워크 유명하죠 ㅎㅎ 아식스도 편하고 방수기능이 좋아서 고민되요",
          "original_form": "고워크 유명하죠 ㅎㅎ 아식스도 편하고 방수기능이 좋아서 고민되요",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046144.1.14",
          "form": "네네 직접신어보시고",
          "original_form": "네네 직접신어보시고",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046144.1.15",
          "form": "픽!하세요",
          "original_form": "픽!하세요",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046144.1.16",
          "form": "저는 스케쳐스한푯!!",
          "original_form": "저는 스케쳐스한푯!!",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046144.1.17",
          "form": "네 둘 다 매장 가보려구요",
          "original_form": "네 둘 다 매장 가보려구요",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046144.1.18",
          "form": "좋아하는 브랜드잇으신가여",
          "original_form": "좋아하는 브랜드잇으신가여",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046144.1.19",
          "form": "무슨 브랜드요? 신발이요?",
          "original_form": "무슨 브랜드요? 신발이요?",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046144.1.20",
          "form": "그냥 좋아하시는?",
          "original_form": "그냥 좋아하시는?",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046144.1.21",
          "form": "저는 나이키 파타고니아욧",
          "original_form": "저는 나이키 파타고니아욧",
          "speaker_id": "2",
          "time": "20210908 10:49"
        }
      ]
