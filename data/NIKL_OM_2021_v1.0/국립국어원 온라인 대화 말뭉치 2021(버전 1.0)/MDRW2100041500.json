      "utterance": [
        {
          "id": "MDRW2100041500.1.1",
          "form": "네 빙수후기 기대할게요 ㅋㅋㅋ",
          "original_form": "네 빙수후기 기대할게요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 07:43"
        },
        {
          "id": "MDRW2100041500.1.2",
          "form": "와 이제아침엔 정말 선선하네요",
          "original_form": "와 이제아침엔 정말 선선하네요",
          "speaker_id": "1",
          "time": "20210906 07:43"
        },
        {
          "id": "MDRW2100041500.1.3",
          "form": "그쵸? 완연한 가을입니당",
          "original_form": "그쵸? 완연한 가을입니당",
          "speaker_id": "2",
          "time": "20210906 07:44"
        },
        {
          "id": "MDRW2100041500.1.4",
          "form": "그리고 오늘부터 비온다 했던 듯요",
          "original_form": "그리고 오늘부터 비온다 했던 듯요",
          "speaker_id": "2",
          "time": "20210906 07:45"
        },
        {
          "id": "MDRW2100041500.1.5",
          "form": "헐 정말요? 비라니...ㅠㅠ",
          "original_form": "헐 정말요? 비라니...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 07:46"
        },
        {
          "id": "MDRW2100041500.1.6",
          "form": "퇴근이후에오면 좋겠네요",
          "original_form": "퇴근이후에오면 좋겠네요",
          "speaker_id": "1",
          "time": "20210906 07:46"
        },
        {
          "id": "MDRW2100041500.1.7",
          "form": "제가 집에 가기전까지만이라도 맑았으면 ㅜ.ㅜ",
          "original_form": "제가 집에 가기전까지만이라도 맑았으면 ㅜ.ㅜ",
          "speaker_id": "2",
          "time": "20210906 07:47"
        },
        {
          "id": "MDRW2100041500.1.8",
          "form": "맞아요!! 여행중이신데ㅠㅠ!!!",
          "original_form": "맞아요!! 여행중이신데ㅠㅠ!!!",
          "speaker_id": "1",
          "time": "20210906 08:19"
        },
        {
          "id": "MDRW2100041500.1.9",
          "form": "그래도 예보보니까 5시6시이후로 내리나봐요!!",
          "original_form": "그래도 예보보니까 5시6시이후로 내리나봐요!!",
          "speaker_id": "1",
          "time": "20210906 08:19"
        },
        {
          "id": "MDRW2100041500.1.10",
          "form": "오, 그나마 다행이네요",
          "original_form": "오, 그나마 다행이네요",
          "speaker_id": "2",
          "time": "20210906 08:20"
        },
        {
          "id": "MDRW2100041500.1.11",
          "form": "비오고 나면 쌀쌀해지겠어요",
          "original_form": "비오고 나면 쌀쌀해지겠어요",
          "speaker_id": "2",
          "time": "20210906 08:21"
        },
        {
          "id": "MDRW2100041500.1.12",
          "form": "그러게요 ㅠㅠ 아직 가디건산거 안왓는데 ㅋㅋㅋㅋ",
          "original_form": "그러게요 ㅠㅠ 아직 가디건산거 안왓는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 08:32"
        },
        {
          "id": "MDRW2100041500.1.13",
          "form": "트렌치 꺼내야하려나봐요",
          "original_form": "트렌치 꺼내야하려나봐요",
          "speaker_id": "1",
          "time": "20210906 08:32"
        },
        {
          "id": "MDRW2100041500.1.14",
          "form": "오오, 저도 옷장 정리 한번 해야겠네요",
          "original_form": "오오, 저도 옷장 정리 한번 해야겠네요",
          "speaker_id": "2",
          "time": "20210906 08:34"
        },
        {
          "id": "MDRW2100041500.1.15",
          "form": "name1님은 어느 계절 좋아하세요?",
          "original_form": "&name1&님은 어느 계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 08:34"
        },
        {
          "id": "MDRW2100041500.1.16",
          "form": "저는 봄이랑 초여름 그리구 늦여름이요!!ㅋㅋ",
          "original_form": "저는 봄이랑 초여름 그리구 늦여름이요!!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 08:36"
        },
        {
          "id": "MDRW2100041500.1.17",
          "form": "오, 상반기를 좋아하시네요",
          "original_form": "오, 상반기를 좋아하시네요",
          "speaker_id": "2",
          "time": "20210906 08:37"
        },
        {
          "id": "MDRW2100041500.1.18",
          "form": "네ㅠㅠ 추운것도 사실 매력이있지만",
          "original_form": "네ㅠㅠ 추운것도 사실 매력이있지만",
