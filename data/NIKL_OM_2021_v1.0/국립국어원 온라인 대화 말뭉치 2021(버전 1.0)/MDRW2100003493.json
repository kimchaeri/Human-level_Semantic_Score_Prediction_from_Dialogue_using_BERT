      "utterance": [
        {
          "id": "MDRW2100003493.1.1",
          "form": "바람의 나라가 그렇게 재밌니",
          "original_form": "바람의 나라가 그렇게 재밌니",
          "speaker_id": "1",
          "time": "20210820 23:45"
        },
        {
          "id": "MDRW2100003493.1.2",
          "form": "별로",
          "original_form": "별로",
          "speaker_id": "2",
          "time": "20210820 23:46"
        },
        {
          "id": "MDRW2100003493.1.3",
          "form": "요번에 용궁 업데이트 해서 좀 할만함 해보실?",
          "original_form": "요번에 용궁 업데이트 해서 좀 할만함 해보실?",
          "speaker_id": "2",
          "time": "20210820 23:46"
        },
        {
          "id": "MDRW2100003493.1.4",
          "form": "저번에 일본 업데이트 하고 요번엔 용궁업데이트했는데",
          "original_form": "저번에 일본 업데이트 하고 요번엔 용궁업데이트했는데",
          "speaker_id": "2",
          "time": "20210820 23:46"
        },
        {
          "id": "MDRW2100003493.1.5",
          "form": "근데 왜그렇게 맨날 중독자처럼 하는데",
          "original_form": "근데 왜그렇게 맨날 중독자처럼 하는데",
          "speaker_id": "1",
          "time": "20210820 23:46"
        },
        {
          "id": "MDRW2100003493.1.6",
          "form": "용궁업데이트는 근데 약간 망한 느낌임 일본은 괜찮았는데",
          "original_form": "용궁업데이트는 근데 약간 망한 느낌임 일본은 괜찮았는데",
          "speaker_id": "2",
          "time": "20210820 23:46"
        },
        {
          "id": "MDRW2100003493.1.7",
          "form": "바람의나라 프로게이머 될라고?",
          "original_form": "바람의나라 프로게이머 될라고?",
          "speaker_id": "1",
          "time": "20210820 23:46"
        },
        {
          "id": "MDRW2100003493.1.8",
          "form": "누가 시켜준다나",
          "original_form": "누가 시켜준다나",
          "speaker_id": "1",
          "time": "20210820 23:46"
        },
        {
          "id": "MDRW2100003493.1.9",
          "form": "내가 어릴때부터 바람했는데 커서도 바람은 해야지 인정?",
          "original_form": "내가 어릴때부터 바람했는데 커서도 바람은 해야지 인정?",
          "speaker_id": "2",
          "time": "20210820 23:47"
        },
        {
          "id": "MDRW2100003493.1.10",
          "form": "님도 쿠키런 그만하고 바람하셈 요번에 렙 500까지 1주면 쌉가능함",
          "original_form": "님도 쿠키런 그만하고 바람하셈 요번에 렙 500까지 1주면 쌉가능함",
          "speaker_id": "2",
          "time": "20210820 23:47"
        },
        {
          "id": "MDRW2100003493.1.11",
          "form": "1주년이라서 많이 퍼준다, 현질안해도됨 괜찮제?",
          "original_form": "1주년이라서 많이 퍼준다, 현질안해도됨 괜찮제?",
          "speaker_id": "2",
          "time": "20210820 23:47"
        },
        {
          "id": "MDRW2100003493.1.12",
          "form": "무슨소리",
          "original_form": "무슨소리",
          "speaker_id": "1",
          "time": "20210820 23:47"
        },
        {
          "id": "MDRW2100003493.1.13",
          "form": "얼른 다운받으시오 온치온치로 새로키워라 내가 키아줄게",
          "original_form": "얼른 다운받으시오 온치온치로 새로키워라 내가 키아줄게",
          "speaker_id": "2",
          "time": "20210820 23:47"
        },
        {
          "id": "MDRW2100003493.1.14",
          "form": "쿠키런 별로 안한다 요새",
          "original_form": "쿠키런 별로 안한다 요새",
          "speaker_id": "1",
          "time": "20210820 23:47"
        },
        {
          "id": "MDRW2100003493.1.15",
          "form": "게임은 다 부질없는것",
          "original_form": "게임은 다 부질없는것",
          "speaker_id": "1",
          "time": "20210820 23:48"
        },
        {
          "id": "MDRW2100003493.1.16",
          "form": "쿠키런 왜 별로 안하는데 요새 게임 부질없기는 할만하다",
          "original_form": "쿠키런 왜 별로 안하는데 요새 게임 부질없기는 할만하다",
          "speaker_id": "2",
          "time": "20210820 23:48"
        },
        {
          "id": "MDRW2100003493.1.17",
          "form": "바람의 한 1000만원만 현질하고 싶군 휴가 1년내고",
          "original_form": "바람의 한 1000만원만 현질하고 싶군 휴가 1년내고",
          "speaker_id": "2",
          "time": "20210820 23:49"
        },
        {
          "id": "MDRW2100003493.1.18",
          "form": "접은 사람이 많아서 하기 싫어짐",
          "original_form": "접은 사람이 많아서 하기 싫어짐",
          "speaker_id": "1",
          "time": "20210820 23:49"
        },
        {
          "id": "MDRW2100003493.1.19",
          "form": "음 인생 나락가겠군 그럼",
          "original_form": "음 인생 나락가겠군 그럼",
          "speaker_id": "1",
          "time": "20210820 23:49"
        },
        {
          "id": "MDRW2100003493.1.20",
          "form": "인생 나락이라니, 나는 안그래도 주식으로 돈많이벌기다",
          "original_form": "인생 나락이라니, 나는 안그래도 주식으로 돈많이벌기다",
          "speaker_id": "2",
          "time": "20210820 23:50"
        },
        {
          "id": "MDRW2100003493.1.21",
          "form": "님도 주식으로 많이벌자나 그거 하면서 바람이나하셈",
          "original_form": "님도 주식으로 많이벌자나 그거 하면서 바람이나하셈",
          "speaker_id": "2",
          "time": "20210820 23:50"
        },
        {
          "id": "MDRW2100003493.1.22",
          "form": "무슨소리하느냐 주식으로 번 돈은 노후를 위해 써야지",
          "original_form": "무슨소리하느냐 주식으로 번 돈은 노후를 위해 써야지",
          "speaker_id": "1",
          "time": "20210820 23:51"
        }
      ]
