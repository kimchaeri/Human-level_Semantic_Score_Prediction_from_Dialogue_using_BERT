      "utterance": [
        {
          "id": "MDRW2100025202.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210901 00:44"
        },
        {
          "id": "MDRW2100025202.1.2",
          "form": "최근 잘 사신 물건 있으신가요?",
          "original_form": "최근 잘 사신 물건 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 00:44"
        },
        {
          "id": "MDRW2100025202.1.3",
          "form": "최근에 강아지 사료와 간식을 샀어요",
          "original_form": "최근에 강아지 사료와 간식을 샀어요",
          "speaker_id": "2",
          "time": "20210901 00:46"
        },
        {
          "id": "MDRW2100025202.1.4",
          "form": "님은 뭐 사셨나요",
          "original_form": "님은 뭐 사셨나요",
          "speaker_id": "2",
          "time": "20210901 00:46"
        },
        {
          "id": "MDRW2100025202.1.5",
          "form": "음 저는 화장품 샀어요",
          "original_form": "음 저는 화장품 샀어요",
          "speaker_id": "1",
          "time": "20210901 00:46"
        },
        {
          "id": "MDRW2100025202.1.6",
          "form": "강아지 키우시나봐요~",
          "original_form": "강아지 키우시나봐요~",
          "speaker_id": "1",
          "time": "20210901 00:46"
        },
        {
          "id": "MDRW2100025202.1.7",
          "form": "네 두마리 귀여워요ㅎ",
          "original_form": "네 두마리 귀여워요ㅎ",
          "speaker_id": "2",
          "time": "20210901 00:47"
        },
        {
          "id": "MDRW2100025202.1.8",
          "form": "기초화장품 사셨나요?",
          "original_form": "기초화장품 사셨나요?",
          "speaker_id": "2",
          "time": "20210901 00:47"
        },
        {
          "id": "MDRW2100025202.1.9",
          "form": "네~ 세럼이요!",
          "original_form": "네~ 세럼이요!",
          "speaker_id": "1",
          "time": "20210901 00:48"
        },
        {
          "id": "MDRW2100025202.1.10",
          "form": "우와 두 마리나..너무 귀엽겠네요 ㅎㅎ",
          "original_form": "우와 두 마리나..너무 귀엽겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 00:49"
        },
        {
          "id": "MDRW2100025202.1.11",
          "form": "아ㅎ 요새 마스크 끼느라 색조보다 기초위주로 사게 되는 것 같아요ㅎ",
          "original_form": "아ㅎ 요새 마스크 끼느라 색조보다 기초위주로 사게 되는 것 같아요ㅎ",
          "speaker_id": "2",
          "time": "20210901 00:49"
        },
        {
          "id": "MDRW2100025202.1.12",
          "form": "중고거래도 하시나요?ㅎㅎ",
          "original_form": "중고거래도 하시나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 00:49"
        },
        {
          "id": "MDRW2100025202.1.13",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210901 00:50"
        },
        {
          "id": "MDRW2100025202.1.14",
          "form": "사실 바지를 샀는데",
          "original_form": "사실 바지를 샀는데",
          "speaker_id": "1",
          "time": "20210901 00:50"
        },
        {
          "id": "MDRW2100025202.1.15",
          "form": "실패해서",
          "original_form": "실패해서",
          "speaker_id": "1",
          "time": "20210901 00:50"
        },
        {
          "id": "MDRW2100025202.1.16",
          "form": "ㅜㅇㅜ 아이고",
          "original_form": "ㅜㅇㅜ 아이고",
          "speaker_id": "2",
          "time": "20210901 00:51"
        },
        {
          "id": "MDRW2100025202.1.17",
          "form": "중고거래로 옷은 비추입니다...ㅋㅋ",
          "original_form": "중고거래로 옷은 비추입니다...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 00:51"
        },
        {
          "id": "MDRW2100025202.1.18",
          "form": "핏이 별로인가요 아님 사이즈가?",
          "original_form": "핏이 별로인가요 아님 사이즈가?",
          "speaker_id": "2",
          "time": "20210901 00:51"
        },
        {
          "id": "MDRW2100025202.1.19",
          "form": "옷 자체의 퀄리티가 좀 그랬어요",
          "original_form": "옷 자체의 퀄리티가 좀 그랬어요",
          "speaker_id": "1",
          "time": "20210901 00:52"
        },
        {
          "id": "MDRW2100025202.1.20",
          "form": "물론 다 그런건 아닐 것 같기도 하지만요",
          "original_form": "물론 다 그런건 아닐 것 같기도 하지만요",
          "speaker_id": "1",
          "time": "20210901 00:52"
        },
        {
          "id": "MDRW2100025202.1.21",
          "form": "ㅜㅜ 이쁘고 핏좋고 퀄리티좋으면 직접 입죠, 아무래도 어정쩡한 것 위주로 팔게 되나봐요",
          "original_form": "ㅜㅜ 이쁘고 핏좋고 퀄리티좋으면 직접 입죠, 아무래도 어정쩡한 것 위주로 팔게 되나봐요",
          "speaker_id": "2",
          "time": "20210901 00:52"
        },
        {
          "id": "MDRW2100025202.1.22",
          "form": "사실 저도 중고거래 옷은 다 실패...",
          "original_form": "사실 저도 중고거래 옷은 다 실패...",
          "speaker_id": "2",
          "time": "20210901 00:52"
        },
        {
          "id": "MDRW2100025202.1.23",
          "form": "하긴 그렇죠 ㅠㅋㅋㅋ",
          "original_form": "하긴 그렇죠 ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 00:52"
        },
        {
          "id": "MDRW2100025202.1.24",
          "form": "그쵸 옷은 저도 이제 안사려구요 중고거래로는",
          "original_form": "그쵸 옷은 저도 이제 안사려구요 중고거래로는",
          "speaker_id": "1",
          "time": "20210901 00:53"
        },
        {
          "id": "MDRW2100025202.1.25",
          "form": "퀄이 좋아도 뭔가 미묘하더라고요ㅜㅇ",
          "original_form": "퀄이 좋아도 뭔가 미묘하더라고요ㅜㅇ",
          "speaker_id": "2",
          "time": "20210901 00:53"
        },
        {
          "id": "MDRW2100025202.1.26",
          "form": "신발은 또 괜찮아요ㅎ",
          "original_form": "신발은 또 괜찮아요ㅎ",
          "speaker_id": "2",
          "time": "20210901 00:53"
        }
      ]
    }
  ]
