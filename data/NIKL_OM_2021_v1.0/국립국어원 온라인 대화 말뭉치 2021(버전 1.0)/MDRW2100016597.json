      "utterance": [
        {
          "id": "MDRW2100016597.1.1",
          "form": "애들은 뭐해?",
          "original_form": "애들은 뭐해?",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016597.1.2",
          "form": "연산 하고 있어",
          "original_form": "연산 하고 있어",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016597.1.3",
          "form": "오늘은 집중해서 잘하고 있나?",
          "original_form": "오늘은 집중해서 잘하고 있나?",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016597.1.4",
          "form": "ㅇㅇ 큰 아들이야 항상 잘하고",
          "original_form": "ㅇㅇ 큰 아들이야 항상 잘하고",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016597.1.5",
          "form": "작은 아들은?",
          "original_form": "작은 아들은?",
          "speaker_id": "2",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016597.1.6",
          "form": "꼬맹이는 오늘 나눗셈하고 분수 연산인데 생각보단 잘하네 ㅎㅎㅎ",
          "original_form": "꼬맹이는 오늘 나눗셈하고 분수 연산인데 생각보단 잘하네 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016597.1.7",
          "form": "생각보다.. 라는 말이 애매한데? ㅋㅋㅋ",
          "original_form": "생각보다.. 라는 말이 애매한데? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016597.1.8",
          "form": "ㅎㅎㅎ 오늘은 속도가 꽤 빨라",
          "original_form": "ㅎㅎㅎ 오늘은 속도가 꽤 빨라",
          "speaker_id": "1",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016597.1.9",
          "form": "굿 천재모드구만",
          "original_form": "굿 천재모드구만",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016597.1.10",
          "form": "쓸데없는 소리 내지도 않고 집중해서 잘하네",
          "original_form": "쓸데없는 소리 내지도 않고 집중해서 잘하네",
          "speaker_id": "1",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016597.1.11",
          "form": "오오 폭풍칭찬 해줘야겠구만",
          "original_form": "오오 폭풍칭찬 해줘야겠구만",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016597.1.12",
          "form": "날씨가 시원해서 집증 더 잘하나 ㅎㄹ",
          "original_form": "날씨가 시원해서 집증 더 잘하나 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016597.1.13",
          "form": "그럴지도.. 여름엔 좀 힘들어하더만",
          "original_form": "그럴지도.. 여름엔 좀 힘들어하더만",
          "speaker_id": "2",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016597.1.14",
          "form": "오늘처럼만 공부하면 인서울 하겠어 ㅋㅋㅋ",
          "original_form": "오늘처럼만 공부하면 인서울 하겠어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016597.1.15",
          "form": "ㅋㅋㅋ 매일 그렇게 하는 게 기적같은 일이지",
          "original_form": "ㅋㅋㅋ 매일 그렇게 하는 게 기적같은 일이지",
          "speaker_id": "2",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016597.1.16",
          "form": "큰 아들도 연산해?",
          "original_form": "큰 아들도 연산해?",
