      "utterance": [
        {
          "id": "MDRW2100039491.1.1",
          "form": "공연보러가시는거 좋아하세요?",
          "original_form": "공연보러가시는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 18:13"
        },
        {
          "id": "MDRW2100039491.1.2",
          "form": "아 사랑하죠",
          "original_form": "아 사랑하죠",
          "speaker_id": "2",
          "time": "20210903 18:14"
        },
        {
          "id": "MDRW2100039491.1.3",
          "form": "코로나 죽일라고요",
          "original_form": "코로나 죽일라고요",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039491.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039491.1.5",
          "form": "저는 아이돌 콘서트 관람을",
          "original_form": "저는 아이돌 콘서트 관람을",
          "speaker_id": "1",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039491.1.6",
          "form": "주로 다녔는데",
          "original_form": "주로 다녔는데",
          "speaker_id": "1",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039491.1.7",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039491.1.8",
          "form": "..더보기 네요",
          "original_form": "..더보기 네요",
          "speaker_id": "1",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039491.1.9",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100039491.1.10",
          "form": "..더보기에요.",
          "original_form": "..더보기에요.",
          "speaker_id": "2",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039491.1.11",
          "form": "뮤지컬도 보고싶은데",
          "original_form": "뮤지컬도 보고싶은데",
          "speaker_id": "2",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039491.1.12",
          "form": "사실 좋아하는 아이돌이 뮤지컬하고잇긴헌데 돈이없어서 못갓습니다 ㅎ",
          "original_form": "사실 좋아하는 아이돌이 뮤지컬하고잇긴헌데 돈이없어서 못갓습니다 ㅎ",
          "speaker_id": "2",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039491.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039491.1.14",
          "form": "....",
          "original_form": "....",
          "speaker_id": "1",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039491.1.15",
          "form": "전시회도 가고싶은데",
          "original_form": "전시회도 가고싶은데",
          "speaker_id": "1",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039491.1.16",
          "form": "돈도 없고",
          "original_form": "돈도 없고",
          "speaker_id": "1",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039491.1.17",
          "form": "시간도 없어서못가네요",
          "original_form": "시간도 없어서못가네요",
          "speaker_id": "1",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039491.1.18",
          "form": "..전시회",
          "original_form": "..전시회",
          "speaker_id": "2",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100039491.1.19",
          "form": "우리 종강하면 꼬옥",
          "original_form": "우리 종강하면 꼬옥",
          "speaker_id": "2",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100039491.1.20",
          "form": "전시회가요",
          "original_form": "전시회가요",
          "speaker_id": "2",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100039491.1.21",
          "form": "그럴까요?",
          "original_form": "그럴까요?",
          "speaker_id": "1",
