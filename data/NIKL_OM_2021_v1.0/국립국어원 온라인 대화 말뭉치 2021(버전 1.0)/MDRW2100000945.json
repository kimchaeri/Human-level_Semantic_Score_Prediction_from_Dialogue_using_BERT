      "utterance": [
        {
          "id": "MDRW2100000945.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000945.1.2",
          "form": "좋아해요ㅎㅎ근데 코로나 때문에 여행 못 가서 답답하네요ㅜ",
          "original_form": "좋아해요ㅎㅎ근데 코로나 때문에 여행 못 가서 답답하네요ㅜ",
          "speaker_id": "2",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000945.1.3",
          "form": "코로나끝나면 어떤곳에 제일 가고싶으세요?",
          "original_form": "코로나끝나면 어떤곳에 제일 가고싶으세요?",
          "speaker_id": "1",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000945.1.4",
          "form": "강원도요ㅎㅎ 가서 바다 보고 싶어요!",
          "original_form": "강원도요ㅎㅎ 가서 바다 보고 싶어요!",
          "speaker_id": "2",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000945.1.5",
          "form": "혹시 가고싶은 곳 있으세요?ㅋㅋ",
          "original_form": "혹시 가고싶은 곳 있으세요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000945.1.6",
          "form": "제주도요~~",
          "original_form": "제주도요~~",
          "speaker_id": "1",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000945.1.7",
          "form": "제주도 좋죠ㅋㅋ저는 수학여행 때 이후로 가본적이 없네요ㅋㅋㅋㅋ",
          "original_form": "제주도 좋죠ㅋㅋ저는 수학여행 때 이후로 가본적이 없네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000945.1.8",
          "form": "저도 10년정도 된거 같아요ㅎㅅ",
          "original_form": "저도 10년정도 된거 같아요ㅎㅅ",
          "speaker_id": "1",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000945.1.9",
          "form": "저도 그정도 되네요ㅋㅋㅋㅋㅋ10월에 담양에 가 볼 생각인데 가보신적 있으세요?",
          "original_form": "저도 그정도 되네요ㅋㅋㅋㅋㅋ10월에 담양에 가 볼 생각인데 가보신적 있으세요?",
          "speaker_id": "2",
          "time": "20210819 20:56"
        },
        {
          "id": "MDRW2100000945.1.10",
          "form": "담양은 한번도 안가봤어요~",
          "original_form": "담양은 한번도 안가봤어요~",
          "speaker_id": "1",
          "time": "20210819 21:12"
        },
        {
          "id": "MDRW2100000945.1.11",
          "form": "저도 가본적 없어서 기대되네요ㅋㅋㅋㅋ",
          "original_form": "저도 가본적 없어서 기대되네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:13"
        },
        {
          "id": "MDRW2100000945.1.12",
          "form": "떡갈비? 대나무? 이런것만 떠오르네요ㅎㅎ",
          "original_form": "떡갈비? 대나무? 이런것만 떠오르네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:14"
        },
        {
          "id": "MDRW2100000945.1.13",
          "form": "맞아요! 대나무숲 가보려고요ㅋㅋ",
          "original_form": "맞아요! 대나무숲 가보려고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:15"
        },
        {
          "id": "MDRW2100000945.1.14",
          "form": "맛있는 맛집도 많을 꺼 같아요",
          "original_form": "맛있는 맛집도 많을 꺼 같아요",
