        {
          "id": "MDRW2100003849.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003849.1.2",
          "form": "네 집안일하세요?",
          "original_form": "네 집안일하세요?",
          "speaker_id": "2",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003849.1.3",
          "form": "네 저도 해요",
          "original_form": "네 저도 해요",
          "speaker_id": "1",
          "time": "20210821 12:55"
        },
        {
          "id": "MDRW2100003849.1.4",
          "form": "집안일은 해도해도  티가 안나서 귀찮아요",
          "original_form": "집안일은 해도해도  티가 안나서 귀찮아요",
          "speaker_id": "2",
          "time": "20210821 12:56"
        },
        {
          "id": "MDRW2100003849.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 12:57"
        },
        {
          "id": "MDRW2100003849.1.6",
          "form": "왜 사람쓰는지 알겠더라구요",
          "original_form": "왜 사람쓰는지 알겠더라구요",
          "speaker_id": "2",
          "time": "20210821 12:57"
        },
        {
          "id": "MDRW2100003849.1.7",
          "form": "업체가 잘하죠",
          "original_form": "업체가 잘하죠",
          "speaker_id": "1",
          "time": "20210821 12:58"
        },
        {
          "id": "MDRW2100003849.1.8",
          "form": "집안일하다보면 피곤하고 짜증나서ㅎ",
          "original_form": "집안일하다보면 피곤하고 짜증나서ㅎ",
          "speaker_id": "2",
          "time": "20210821 12:59"
        },
        {
          "id": "MDRW2100003849.1.9",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210821 13:00"
        },
        {
          "id": "MDRW2100003849.1.10",
          "form": "몸 상해요",
          "original_form": "몸 상해요",
          "speaker_id": "1",
          "time": "20210821 13:00"
        },
        {
          "id": "MDRW2100003849.1.11",
          "form": "왜 건조기나 로봇청소기가 잘팔리는지 알거같아요",
          "original_form": "왜 건조기나 로봇청소기가 잘팔리는지 알거같아요",
          "speaker_id": "2",
          "time": "20210821 13:00"
        },
        {
          "id": "MDRW2100003849.1.12",
          "form": "편리하죠",
          "original_form": "편리하죠",
          "speaker_id": "1",
