      "utterance": [
        {
          "id": "MDRW2100015365.1.1",
          "form": "여행 좋아하십니까?",
          "original_form": "여행 좋아하십니까?",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015365.1.2",
          "form": "네 좋아하는데",
          "original_form": "네 좋아하는데",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015365.1.3",
          "form": "이시국이라 못가서 아쉽네요",
          "original_form": "이시국이라 못가서 아쉽네요",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015365.1.4",
          "form": "아.. 맞아요 ㅜㅜ",
          "original_form": "아.. 맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015365.1.5",
          "form": "저는 작년에도 여행 못가서.. 더더욱 슬퍼요",
          "original_form": "저는 작년에도 여행 못가서.. 더더욱 슬퍼요",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015365.1.6",
          "form": "좋아하세요??",
          "original_form": "좋아하세요??",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015365.1.7",
          "form": "그니깐요",
          "original_form": "그니깐요",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015365.1.8",
          "form": "제일 기억에 남은 여행은 뭐에요?",
          "original_form": "제일 기억에 남은 여행은 뭐에요?",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015365.1.9",
          "form": "저는 대학생때 유럽여행한거 ㅠㅠ",
          "original_form": "저는 대학생때 유럽여행한거 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015365.1.10",
          "form": "와 유럽여행!",
          "original_form": "와 유럽여행!",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015365.1.11",
          "form": "몇 개국 돌아다녔어요?",
          "original_form": "몇 개국 돌아다녔어요?",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015365.1.12",
          "form": "알바 열심히 해서 갔었는데 신기루같네요 ㅋㅋㅋ",
          "original_form": "알바 열심히 해서 갔었는데 신기루같네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015365.1.13",
          "form": "동유럽? 서유럽?",
          "original_form": "동유럽? 서유럽?",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015365.1.14",
          "form": "저는 아예 한달정도가서 둘다 갔어요",
          "original_form": "저는 아예 한달정도가서 둘다 갔어요",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015365.1.15",
          "form": "방학한달 할애했네요 ㅋㅋㅋ",
          "original_form": "방학한달 할애했네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015365.1.16",
          "form": "name1님은 기억에 남는 곳 있나요??",
          "original_form": "&name1&님은 기억에 남는 곳 있나요??",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015365.1.17",
          "form": "부럽네요..",
          "original_form": "부럽네요..",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015365.1.18",
          "form": "저는 캄보디아에 갔다 온게 기억에 남아요!",
          "original_form": "저는 캄보디아에 갔다 온게 기억에 남아요!",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015365.1.19",
          "form": "고등학교 때 첫 해외 간거여서요 ㅎㅎ",
          "original_form": "고등학교 때 첫 해외 간거여서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015365.1.20",
          "form": "오오 가고싶언던곳인데 어떠셨나요.:.",
          "original_form": "오오 가고싶언던곳인데 어떠셨나요.:.",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015365.1.21",
          "form": "유적지 있죠 막 큰거",
          "original_form": "유적지 있죠 막 큰거",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015365.1.22",
          "form": "벽돌있고",
          "original_form": "벽돌있고",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015365.1.23",
          "form": "앙코르와트요?",
          "original_form": "앙코르와트요?",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015365.1.24",
          "form": "아 맞아요 그거",
          "original_form": "아 맞아요 그거",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015365.1.25",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015365.1.26",
          "form": "네 좋았어요 뭔가 진짜 다른 세계 온 기분이라서 ㅋㅋ",
          "original_form": "네 좋았어요 뭔가 진짜 다른 세계 온 기분이라서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015365.1.27",
          "form": "저 앙코르와트 관광 때 재미있던 썰 있어요",
          "original_form": "저 앙코르와트 관광 때 재미있던 썰 있어요",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015365.1.28",
          "form": "아 뭔데요?",
          "original_form": "아 뭔데요?",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015365.1.29",
          "form": "궁금하네요 ㅋㅋㅋ",
          "original_form": "궁금하네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015365.1.30",
          "form": "캄보디아는 크메르어를 사용해서, 영어 할 줄 아는 현지 가이드하고, 영어 할 줄 아는 한국인 가이드 이렇게 2명이 안내해줬어요.",
          "original_form": "캄보디아는 크메르어를 사용해서, 영어 할 줄 아는 현지 가이드하고, 영어 할 줄 아는 한국인 가이드 이렇게 2명이 안내해줬어요.",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015365.1.31",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015365.1.32",
          "form": "근데 현지 가이드가 한국 가이드에게 영어로 막 얘기해줬는데 한국인 가이드가 저한테 그걸 또 영어로 전달했어요 ㅋㅋㅋㅋ",
          "original_form": "근데 현지 가이드가 한국 가이드에게 영어로 막 얘기해줬는데 한국인 가이드가 저한테 그걸 또 영어로 전달했어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015365.1.33",
          "form": "아 이중이네요 ㅋㅋ",
          "original_form": "아 이중이네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015365.1.34",
          "form": "그땐 다 알아들어서 이상한 줄 몰랐는데 친구들이 말해줘서 알았어요 ㅋㅋㅋㅋ",
          "original_form": "그땐 다 알아들어서 이상한 줄 몰랐는데 친구들이 말해줘서 알았어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015365.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ웃기네요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ웃기네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015365.1.36",
          "form": "외국인인줄 안건가요? ㅋㅋ",
          "original_form": "외국인인줄 안건가요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015365.1.37",
          "form": "그 분이 자기도 모르게 나오신 것 같아요 현지 가이드랑 영어로 얘기하다보니 습관적으로 ㅋㅋ",
          "original_form": "그 분이 자기도 모르게 나오신 것 같아요 현지 가이드랑 영어로 얘기하다보니 습관적으로 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015365.1.38",
          "form": "아하 ㅋㅋㅋ전 name1님이 이국적으로 생기신줄 ㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋ전 &name1&님이 이국적으로 생기신줄 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015365.1.39",
          "form": "근데 진짜 웃긴일화네요 ㅋㅋ",
          "original_form": "근데 진짜 웃긴일화네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015365.1.40",
          "form": "서울에 가볼만한 관광 명소 아시나요!",
          "original_form": "서울에 가볼만한 관광 명소 아시나요!",
          "speaker_id": "1",
          "time": "20210825 22:59"
        }
      ]
