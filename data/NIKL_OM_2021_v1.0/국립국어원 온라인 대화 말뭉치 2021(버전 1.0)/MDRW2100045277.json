      "utterance": [
        {
          "id": "MDRW2100045277.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045277.1.2",
          "form": "전 민트 좋아해요 님은요?",
          "original_form": "전 민트 좋아해요 님은요?",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045277.1.3",
          "form": "아 맞다 두부도 좋아해요",
          "original_form": "아 맞다 두부도 좋아해요",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045277.1.4",
          "form": "ㅋㅋ 전 요즘 오리요!",
          "original_form": "ㅋㅋ 전 요즘 오리요!",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045277.1.5",
          "form": "오리고기 맛있죠 양념된거!",
          "original_form": "오리고기 맛있죠 양념된거!",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045277.1.6",
          "form": "요즘 손질다해서 주더라고요",
          "original_form": "요즘 손질다해서 주더라고요",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045277.1.7",
          "form": "맞아요 딱 먹기 좋아요",
          "original_form": "맞아요 딱 먹기 좋아요",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045277.1.8",
          "form": "건강식이니까요!!!",
          "original_form": "건강식이니까요!!!",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045277.1.9",
          "form": "쌈 싸먹으면 맛있어요",
          "original_form": "쌈 싸먹으면 맛있어요",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045277.1.10",
          "form": "생오리로 구워먹어도 맛나요",
          "original_form": "생오리로 구워먹어도 맛나요",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045277.1.11",
          "form": "부추랑 먹으면 좋더라구요",
          "original_form": "부추랑 먹으면 좋더라구요",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045277.1.12",
          "form": "요리해보셨어요?",
          "original_form": "요리해보셨어요?",
          "speaker_id": "1",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045277.1.13",
          "form": "가족이랑 먹을때 그 자리에서 해서 먹어요\r\n네 요리는 해요",
          "original_form": "가족이랑 먹을때 그 자리에서 해서 먹어요\r\n네 요리는 해요",
          "speaker_id": "2",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045277.1.14",
          "form": "빨리 익으니까 좋아요",
          "original_form": "빨리 익으니까 좋아요",
          "speaker_id": "1",
          "time": "20210908 01:51"
        }
