      "utterance": [
        {
          "id": "MDRW2100014099.1.1",
          "form": "어... 생각이 성숙한 사람?",
          "original_form": "어... 생각이 성숙한 사람?",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014099.1.2",
          "form": "오 ㅎㅎ좋네요",
          "original_form": "오 ㅎㅎ좋네요",
          "speaker_id": "1",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014099.1.3",
          "form": "외적으론요?",
          "original_form": "외적으론요?",
          "speaker_id": "1",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014099.1.4",
          "form": "외적으로는...",
          "original_form": "외적으로는...",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014099.1.5",
          "form": "사실 제가 키가 좀 커서 ㅠ",
          "original_form": "사실 제가 키가 좀 커서 ㅠ",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014099.1.6",
          "form": "저보다 작은 사람은 좀...",
          "original_form": "저보다 작은 사람은 좀...",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014099.1.7",
          "form": "키가 몇이신데요?!",
          "original_form": "키가 몇이신데요?!",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014099.1.8",
          "form": "170이요....ㅎ ㅠ",
          "original_form": "170이요....ㅎ ㅠ",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014099.1.9",
          "form": "굽있는거 신으면 174ㅋㅋ",
          "original_form": "굽있는거 신으면 174ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014099.1.10",
          "form": "ㅋㅋㅋㅋ요새 키큰남자들 많잖아용 ㅎㅎ희망을 가져요!!",
          "original_form": "ㅋㅋㅋㅋ요새 키큰남자들 많잖아용 ㅎㅎ희망을 가져요!!",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014099.1.11",
          "form": "ㅎㅎㅎ ㅠㅠㅠㅠㅠ 파이팅!!!!",
          "original_form": "ㅎㅎㅎ ㅠㅠㅠㅠㅠ 파이팅!!!!",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014099.1.12",
          "form": "키말고는 안보세요?",
          "original_form": "키말고는 안보세요?",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014099.1.13",
          "form": "사실 얼굴은 잘 모르겠고 제가 뚱뚱한 사람 정말 ... 조금.. 싫어하구요",
          "original_form": "사실 얼굴은 잘 모르겠고 제가 뚱뚱한 사람 정말 ... 조금.. 싫어하구요",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014099.1.14",
          "form": "사실 다른 거 다 모르겠고 진짜 깨끗한 사람 ㅠㅠ 저 좋은 향 나는사람 좋아해요",
          "original_form": "사실 다른 거 다 모르겠고 진짜 깨끗한 사람 ㅠㅠ 저 좋은 향 나는사람 좋아해요",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014099.1.15",
          "form": "후각에 민감하시구나 !",
          "original_form": "후각에 민감하시구나 !",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014099.1.16",
          "form": "그것도 그렇구 깨끗한 걸 좋아해서요ㅋㅋ",
          "original_form": "그것도 그렇구 깨끗한 걸 좋아해서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014099.1.17",
          "form": "잘 안씻는 남자 너무 많더라구요 ㅡㅡ",
          "original_form": "잘 안씻는 남자 너무 많더라구요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014099.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저도 드러운사람은",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저도 드러운사람은",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014099.1.19",
          "form": "싫네용 ㅠ",
          "original_form": "싫네용 ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014099.1.20",
          "form": "그쵸 ㅠ 자기관리는 좀 잘 했으묜... 하는 마음이에요",
