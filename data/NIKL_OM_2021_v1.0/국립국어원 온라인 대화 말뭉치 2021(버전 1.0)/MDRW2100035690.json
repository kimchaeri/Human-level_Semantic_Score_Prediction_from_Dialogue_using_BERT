      "utterance": [
        {
          "id": "MDRW2100035690.1.1",
          "form": "다른 증세는 없으셨나요?",
          "original_form": "다른 증세는 없으셨나요?",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035690.1.2",
          "form": "어 엄청 졸렸어요!",
          "original_form": "어 엄청 졸렸어요!",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035690.1.3",
          "form": "그래도 응급실까진 안 가셔서 다행이네요ㅠㅠ",
          "original_form": "그래도 응급실까진 안 가셔서 다행이네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035690.1.4",
          "form": "제친구는 배탈났었어요",
          "original_form": "제친구는 배탈났었어요",
          "speaker_id": "2",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035690.1.5",
          "form": "응급실 가셨나요? 복통이랑 심장아픈거랑 사람은 구별하기 힘들다고 응급실 가야한다던데ㅠ",
          "original_form": "응급실 가셨나요? 복통이랑 심장아픈거랑 사람은 구별하기 힘들다고 응급실 가야한다던데ㅠ",
          "speaker_id": "1",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100035690.1.6",
          "form": "내과는 다녀왔다더라고요",
          "original_form": "내과는 다녀왔다더라고요",
          "speaker_id": "2",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100035690.1.7",
          "form": "토하고 설사하고,,",
          "original_form": "토하고 설사하고,,",
          "speaker_id": "2",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100035690.1.8",
          "form": "아이고ㅠㅠㅠㅠ 2차 맞기가 겁나시겠어요ㅠㅠ",
          "original_form": "아이고ㅠㅠㅠㅠ 2차 맞기가 겁나시겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100035690.1.9",
          "form": "친구가 워낙 비실펄슨이라",
          "original_form": "친구가 워낙 비실펄슨이라",
          "speaker_id": "2",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100035690.1.10",
          "form": "근데 친구는 근육통에는 약먹으니까 바로 괜찮아졌어요",
          "original_form": "근데 친구는 근육통에는 약먹으니까 바로 괜찮아졌어요",
          "speaker_id": "2",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100035690.1.11",
          "form": "저는 약먹어도 별로 안풀리고ㅠ",
          "original_form": "저는 약먹어도 별로 안풀리고ㅠ",
          "speaker_id": "2",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100035690.1.12",
          "form": "저는 한 팔로 며칠을 살면 어카나 싶어요....스마트폰 없이 못 사는데ㅜㅜㅜ",
          "original_form": "저는 한 팔로 며칠을 살면 어카나 싶어요....스마트폰 없이 못 사는데ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 23:25"
        },
        {
          "id": "MDRW2100035690.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ약이 잘들길 바랍니다,,,!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ약이 잘들길 바랍니다,,,!",
          "speaker_id": "2",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100035690.1.14",
          "form": "한손으로도 완전가능,,",
          "original_form": "한손으로도 완전가능,,",
          "speaker_id": "2",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100035690.1.15",
          "form": "경험담인가요ㅋㅋㅋㅋ",
          "original_form": "경험담인가요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:27"
        },
        {
          "id": "MDRW2100035690.1.16",
          "form": "ㅋㅋㄱㅋㄱㄱㄱ정답입니다,,,",
          "original_form": "ㅋㅋㄱㅋㄱㄱㄱ정답입니다,,,",
          "speaker_id": "2",
          "time": "20210829 23:27"
        },
        {
          "id": "MDRW2100035690.1.17",
          "form": "잘때만 불편했어요 근육통온건,,,",
          "original_form": "잘때만 불편했어요 근육통온건,,,",
          "speaker_id": "2",
          "time": "20210829 23:27"
        },
        {
          "id": "MDRW2100035690.1.18",
          "form": "그쪽으로 누울수가없어요",
          "original_form": "그쪽으로 누울수가없어요",
          "speaker_id": "2",
          "time": "20210829 23:27"
        },
        {
          "id": "MDRW2100035690.1.19",
          "form": "뒤척이다 맞은 팔 쪽으로 돌리면 천국의 계단인가요....ㅠㅠ",
          "original_form": "뒤척이다 맞은 팔 쪽으로 돌리면 천국의 계단인가요....ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100035690.1.20",
          "form": "그것도 정답,,,",
          "original_form": "그것도 정답,,,",
          "speaker_id": "2",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100035690.1.21",
          "form": "바로깨더라고요",
          "original_form": "바로깨더라고요",
          "speaker_id": "2",
          "time": "20210829 23:28"
        }
      ]
    }
