      "utterance": [
        {
          "id": "MDRW2100022558.1.1",
          "form": "안녕하세요 name2님",
          "original_form": "안녕하세요 &name2&님",
          "speaker_id": "1",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022558.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022558.1.3",
          "form": "오늘 하늘이 예쁜것 같아요",
          "original_form": "오늘 하늘이 예쁜것 같아요",
          "speaker_id": "1",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100022558.1.4",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "2",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022558.1.5",
          "form": "가을이라 하늘이 예쁘더라구요",
          "original_form": "가을이라 하늘이 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022558.1.6",
          "form": "좋아하는 날씨 있으세요?",
          "original_form": "좋아하는 날씨 있으세요?",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022558.1.7",
          "form": "계절이나~",
          "original_form": "계절이나~",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022558.1.8",
          "form": "전 겨울 좋아해요",
          "original_form": "전 겨울 좋아해요",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022558.1.9",
          "form": "눈 오는 걸 좋아해서요",
          "original_form": "눈 오는 걸 좋아해서요",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022558.1.10",
          "form": "차갑고 고요한 그런 날씨가 좋더라구요",
          "original_form": "차갑고 고요한 그런 날씨가 좋더라구요",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022558.1.11",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022558.1.12",
          "form": "저는 여름이요!",
          "original_form": "저는 여름이요!",
          "speaker_id": "1",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022558.1.13",
          "form": "여름!",
          "original_form": "여름!",
          "speaker_id": "2",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022558.1.14",
          "form": "추위를 많이 타서 안추우면 다 좋아요 :)",
          "original_form": "추위를 많이 타서 안추우면 다 좋아요 :)",
          "speaker_id": "1",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022558.1.15",
          "form": "얼마전 여름 끝나가서",
          "original_form": "얼마전 여름 끝나가서",
          "speaker_id": "2",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022558.1.16",
          "form": "아쉬우셨겠어요",
          "original_form": "아쉬우셨겠어요",
          "speaker_id": "2",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022558.1.17",
          "form": "ㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022558.1.18",
          "form": "겨울엔 주로 뭐하세요~?",
          "original_form": "겨울엔 주로 뭐하세요~?",
          "speaker_id": "1",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022558.1.19",
          "form": "집에서 귤까먹어요",
          "original_form": "집에서 귤까먹어요",
          "speaker_id": "2",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022558.1.20",
          "form": "눈 오면 나가서 맞기도해요!",
          "original_form": "눈 오면 나가서 맞기도해요!",
          "speaker_id": "2",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022558.1.21",
          "form": "괜히 만저보기도 하고",
          "original_form": "괜히 만저보기도 하고",
          "speaker_id": "2",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022558.1.22",
          "form": "물론 녹으면 질척거려서",
          "original_form": "물론 녹으면 질척거려서",
          "speaker_id": "2",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022558.1.23",
          "form": "그건 싫지만요",
          "original_form": "그건 싫지만요",
          "speaker_id": "2",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022558.1.24",
          "form": "맞아요 ㅋㅋ눈은 딱 하얗게 내렸을때",
          "original_form": "맞아요 ㅋㅋ눈은 딱 하얗게 내렸을때",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022558.1.25",
          "form": "이쁜것같아요",
          "original_form": "이쁜것같아요",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022558.1.26",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022558.1.27",
          "form": "뽀드득 뽀드득",
          "original_form": "뽀드득 뽀드득",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022558.1.28",
          "form": "새하얀 공간 발자국 남기는것도 좋아해요",
          "original_form": "새하얀 공간 발자국 남기는것도 좋아해요",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022558.1.29",
          "form": "뭔가 동심으로 돌아간느낌",
          "original_form": "뭔가 동심으로 돌아간느낌",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022558.1.30",
          "form": "그러네요!눈의 순수함",
          "original_form": "그러네요!눈의 순수함",
          "speaker_id": "1",
