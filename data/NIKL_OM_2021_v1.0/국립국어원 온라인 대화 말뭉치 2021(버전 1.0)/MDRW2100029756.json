      "utterance": [
        {
          "id": "MDRW2100029756.1.1",
          "form": "안녕하세요 제주는 어떠신가요?",
          "original_form": "안녕하세요 제주는 어떠신가요?",
          "speaker_id": "1",
          "time": "20210903 13:00"
        },
        {
          "id": "MDRW2100029756.1.2",
          "form": "제주는 계속 비가 오네요 최근에 영화보신거 있으세요?",
          "original_form": "제주는 계속 비가 오네요 최근에 영화보신거 있으세요?",
          "speaker_id": "2",
          "time": "20210903 15:12"
        },
        {
          "id": "MDRW2100029756.1.3",
          "form": "최근에 구 스파이더맨 시리즈 복습했어요! 곧 신작 개봉한대서요 ㅎㅎ",
          "original_form": "최근에 구 스파이더맨 시리즈 복습했어요! 곧 신작 개봉한대서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 15:13"
        },
        {
          "id": "MDRW2100029756.1.4",
          "form": "아하 전 모가디슈 봤어요",
          "original_form": "아하 전 모가디슈 봤어요",
          "speaker_id": "2",
          "time": "20210903 15:14"
        },
        {
          "id": "MDRW2100029756.1.5",
          "form": "오 기대했다가 시간이 안돼서 못봤는데 어떤가요?",
          "original_form": "오 기대했다가 시간이 안돼서 못봤는데 어떤가요?",
          "speaker_id": "1",
          "time": "20210903 15:14"
        },
        {
          "id": "MDRW2100029756.1.6",
          "form": "볼만한데 류승완감독님 작품은 다 보구나면 좀 2프로 부족한 느낌이 ㅎㅎ",
          "original_form": "볼만한데 류승완감독님 작품은 다 보구나면 좀 2프로 부족한 느낌이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 15:17"
        },
        {
          "id": "MDRW2100029756.1.7",
          "form": "아하 글쿤요.. 제목은 무슨뜻이래요?",
          "original_form": "아하 글쿤요.. 제목은 무슨뜻이래요?",
          "speaker_id": "1",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100029756.1.8",
          "form": "그게 어느나라 수도이름인데",
          "original_form": "그게 어느나라 수도이름인데",
          "speaker_id": "2",
          "time": "20210903 15:20"
        },
        {
          "id": "MDRW2100029756.1.9",
          "form": "소말리아인가",
          "original_form": "소말리아인가",
          "speaker_id": "2",
          "time": "20210903 15:20"
        },
        {
          "id": "MDRW2100029756.1.10",
          "form": "맞네요",
          "original_form": "맞네요",
          "speaker_id": "2",
          "time": "20210903 15:21"
        },
        {
          "id": "MDRW2100029756.1.11",
          "form": "아하 소말리아라.. 해적이야기인가요? 포스터가 뭔가 느낌있던데",
          "original_form": "아하 소말리아라.. 해적이야기인가요? 포스터가 뭔가 느낌있던데",
          "speaker_id": "1",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100029756.1.12",
          "form": "아니요 거기 파견된 외교관들 이야기요",
          "original_form": "아니요 거기 파견된 외교관들 이야기요",
          "speaker_id": "2",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100029756.1.13",
          "form": "아! 완전 제가 편견을 갖고있었네요",
          "original_form": "아! 완전 제가 편견을 갖고있었네요",
          "speaker_id": "1",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100029756.1.14",
          "form": "더욱 보고 싶어졌어요",
          "original_form": "더욱 보고 싶어졌어요",
          "speaker_id": "1",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100029756.1.15",
          "form": "김윤식님 연기가 넘 좋아요 그리고 허진호씨",
          "original_form": "김윤식님 연기가 넘 좋아요 그리고 허진호씨",
          "speaker_id": "2",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029756.1.16",
          "form": "와 넷플릭스같은데에 떴을까요? 궁금하네요",
          "original_form": "와 넷플릭스같은데에 떴을까요? 궁금하네요",
          "speaker_id": "1",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100029756.1.17",
          "form": "신작이라 아직 안나왔을 듯 해요",
          "original_form": "신작이라 아직 안나왔을 듯 해요",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100029756.1.18",
          "form": "아하 기회되면 한번 봐야겠어요",
          "original_form": "아하 기회되면 한번 봐야겠어요",
          "speaker_id": "1",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100029756.1.19",
          "form": "넷플릭스 쓰시면 종이의집 보셨어여?",
          "original_form": "넷플릭스 쓰시면 종이의집 보셨어여?",
          "speaker_id": "2",
          "time": "20210903 16:55"
        },
        {
          "id": "MDRW2100029756.1.20",
          "form": "아 초반에 조금 보다가 하차했어요ㅠㅠ",
          "original_form": "아 초반에 조금 보다가 하차했어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 16:56"
        },
        {
          "id": "MDRW2100029756.1.21",
          "form": "저한텐 조금 매운맛이더라구요",
          "original_form": "저한텐 조금 매운맛이더라구요",
          "speaker_id": "1",
          "time": "20210903 16:56"
        },
        {
          "id": "MDRW2100029756.1.22",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "2",
          "time": "20210903 16:57"
        }
      ]
    }
