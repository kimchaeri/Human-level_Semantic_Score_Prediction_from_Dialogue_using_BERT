          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035640.1.3",
          "form": "걔껴서 했잖아요",
          "original_form": "걔껴서 했잖아요",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035640.1.4",
          "form": "이번에...",
          "original_form": "이번에...",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035640.1.5",
          "form": "한번쯤은 쉴법도한데 그죠",
          "original_form": "한번쯤은 쉴법도한데 그죠",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035640.1.6",
          "form": "맞아요 아까 인가보다가",
          "original_form": "맞아요 아까 인가보다가",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035640.1.7",
          "form": "거의 안쉬지않았나요?",
          "original_form": "거의 안쉬지않았나요?",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035640.1.8",
          "form": "그냥 그 킹덤만 안나오고",
          "original_form": "그냥 그 킹덤만 안나오고",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035640.1.9",
          "form": "활동은 안쉬었을걸요",
          "original_form": "활동은 안쉬었을걸요",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100035640.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100035640.1.11",
          "form": "뭐 걔 직캠 조회수가 젤",
          "original_form": "뭐 걔 직캠 조회수가 젤",
