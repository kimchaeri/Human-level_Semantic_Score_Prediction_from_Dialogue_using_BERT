      "utterance": [
        {
          "id": "MDRW2100027337.1.1",
          "form": "아침먹었어요?",
          "original_form": "아침먹었어요?",
          "speaker_id": "1",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027337.1.2",
          "form": "네 아침먹었어요",
          "original_form": "네 아침먹었어요",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100027337.1.3",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100027337.1.4",
          "form": "네 아침도 먹고",
          "original_form": "네 아침도 먹고",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100027337.1.5",
          "form": "점심도 먹고",
          "original_form": "점심도 먹고",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100027337.1.6",
          "form": "ㅋㅋ저도 방금 점심먹었어요 \n뭐드셨나요?",
          "original_form": "ㅋㅋ저도 방금 점심먹었어요 \n뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100027337.1.7",
          "form": "평범한 반찬 메뉴었지만 바나나나온게 기억이남네요",
          "original_form": "평범한 반찬 메뉴었지만 바나나나온게 기억이남네요",
          "speaker_id": "1",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100027337.1.8",
          "form": "ㅋㅋ후식으로 바나나가 나왔어요?",
          "original_form": "ㅋㅋ후식으로 바나나가 나왔어요?",
          "speaker_id": "2",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100027337.1.9",
          "form": "네ㅋㅋㄱㅋ",
          "original_form": "네ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100027337.1.10",
          "form": "아침엔 뭐드셨나요?",
          "original_form": "아침엔 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210902 12:35"
        },
        {
          "id": "MDRW2100027337.1.11",
          "form": "못먹었어요....출근때문에",
          "original_form": "못먹었어요....출근때문에",
          "speaker_id": "1",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100027337.1.12",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100027337.1.13",
          "form": "헐 ㅠ 밥안먹고 일하는거 너무 힘들지않나요",
          "original_form": "헐 ㅠ 밥안먹고 일하는거 너무 힘들지않나요",
          "speaker_id": "2",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100027337.1.14",
          "form": "그러니깐요...",
          "original_form": "그러니깐요...",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027337.1.15",
          "form": "아침은 못먹고 점심배부르게 먹으니깐",
          "original_form": "아침은 못먹고 점심배부르게 먹으니깐",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027337.1.16",
          "form": "식곤증이.....",
          "original_form": "식곤증이.....",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027337.1.17",
          "form": "저도 지금 너무 졸려서",
          "original_form": "저도 지금 너무 졸려서",
          "speaker_id": "2",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100027337.1.18",
          "form": "과자먹고있어요",
          "original_form": "과자먹고있어요",
          "speaker_id": "2",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100027337.1.19",
          "form": "잠올땐먹는게최고죠",
          "original_form": "잠올땐먹는게최고죠",
