      "utterance": [
        {
          "id": "MDRW2100039948.1.1",
          "form": "꼭 사서와야할 쇼핑품목이 있을까요?",
          "original_form": "꼭 사서와야할 쇼핑품목이 있을까요?",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100039948.1.2",
          "form": "저는 오늘 잠깐 육지에 부모님 뵈러 왔는데",
          "original_form": "저는 오늘 잠깐 육지에 부모님 뵈러 왔는데",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100039948.1.3",
          "form": "유기농 청귤이랑 마음샌드 제주산 갈치 사왔어요",
          "original_form": "유기농 청귤이랑 마음샌드 제주산 갈치 사왔어요",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100039948.1.4",
          "form": "친구들은 마음샌드 제일 좋아하던데 ㅎㅎ",
          "original_form": "친구들은 마음샌드 제일 좋아하던데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100039948.1.5",
          "form": "마음샌드ㅋㅋㅋㅋㄱㄲ",
          "original_form": "마음샌드ㅋㅋㅋㅋㄱㄲ",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100039948.1.6",
          "form": "청귤도 상큼한게 좋네요!",
          "original_form": "청귤도 상큼한게 좋네요!",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100039948.1.7",
          "form": "다음에 가면 다 사서와야겠네요ㅎㅎ",
          "original_form": "다음에 가면 다 사서와야겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100039948.1.8",
          "form": "사실 스타벅스도 제주 리미티드 제품들이 너무 잘 나와서",
          "original_form": "사실 스타벅스도 제주 리미티드 제품들이 너무 잘 나와서",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039948.1.9",
          "form": "그런것도 좋을 것 같네요",
          "original_form": "그런것도 좋을 것 같네요",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039948.1.10",
          "form": "텀블러!",
          "original_form": "텀블러!",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039948.1.11",
          "form": "텀블러는 꼭 사오려구요",
          "original_form": "텀블러는 꼭 사오려구요",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039948.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039948.1.13",
          "form": "어쩜 그렇게 소비자  마음을 잘아는지ㅋㅋ",
          "original_form": "어쩜 그렇게 소비자  마음을 잘아는지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039948.1.14",
          "form": "하지만 어른들은 갈치를 가장 좋아하시네요",
          "original_form": "하지만 어른들은 갈치를 가장 좋아하시네요",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039948.1.15",
          "form": "저도 스벅카드 충전해놓은거 쓰려고 매장가면",
          "original_form": "저도 스벅카드 충전해놓은거 쓰려고 매장가면",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039948.1.16",
          "form": "원래 아메리카노만 마시는데 제주한정 음료같은거 한번씩 먹게 되더라구요",
          "original_form": "원래 아메리카노만 마시는데 제주한정 음료같은거 한번씩 먹게 되더라구요",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039948.1.17",
          "form": "갈치가 좋긴하져ㅋㅋ",
          "original_form": "갈치가 좋긴하져ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039948.1.18",
          "form": "한정음료도 맛나죠?",
          "original_form": "한정음료도 맛나죠?",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100039948.1.19",
          "form": "한정이라는게 소비자마음을 또 ㅋㅋㅋ사로잡죠",
          "original_form": "한정이라는게 소비자마음을 또 ㅋㅋㅋ사로잡죠",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100039948.1.20",
          "form": "네 우도땅콩라떼 이런건 좀 ㅋㅋㅋ 별로였는데",
          "original_form": "네 우도땅콩라떼 이런건 좀 ㅋㅋㅋ 별로였는데",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100039948.1.21",
          "form": "엇 별로던가요?ㅋㅋ",
          "original_form": "엇 별로던가요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100039948.1.22",
          "form": "네에 넘 달아서",
          "original_form": "네에 넘 달아서",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100039948.1.23",
          "form": "땅콩라떼라...왠지 고소함에 끝판일것같은데ㅋ",
          "original_form": "땅콩라떼라...왠지 고소함에 끝판일것같은데ㅋ",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100039948.1.24",
          "form": "천혜향블랙티 블렌디드가",
          "original_form": "천혜향블랙티 블렌디드가",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100039948.1.25",
          "form": "최고예요",
          "original_form": "최고예요",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100039948.1.26",
          "form": "스벅 최근에 스탠리랑 콜라보한 텀블러 샀었는데",
          "original_form": "스벅 최근에 스탠리랑 콜라보한 텀블러 샀었는데",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100039948.1.27",
          "form": "냉온 다 엄청 오래가서 감탄했어요",
          "original_form": "냉온 다 엄청 오래가서 감탄했어요",
          "speaker_id": "1",
          "time": "20210903 23:35"
        }
      ]
