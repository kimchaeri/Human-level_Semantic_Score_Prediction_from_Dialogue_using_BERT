      "utterance": [
        {
          "id": "MDRW2100008745.1.1",
          "form": "키우는 동물 있어?",
          "original_form": "키우는 동물 있어?",
          "speaker_id": "1",
          "time": "20210822 17:59"
        },
        {
          "id": "MDRW2100008745.1.2",
          "form": "아니 나 키우는 동물은 없어",
          "original_form": "아니 나 키우는 동물은 없어",
          "speaker_id": "2",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008745.1.3",
          "form": "키웠던 동물도?",
          "original_form": "키웠던 동물도?",
          "speaker_id": "1",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008745.1.4",
          "form": "응 동물은 한 번도 키워본 적 없어",
          "original_form": "응 동물은 한 번도 키워본 적 없어",
          "speaker_id": "2",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008745.1.5",
          "form": "혹시 키우고 있어?",
          "original_form": "혹시 키우고 있어?",
          "speaker_id": "2",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008745.1.6",
          "form": "아니 사실 나도 한 번도ㅋㅋㅋ",
          "original_form": "아니 사실 나도 한 번도ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008745.1.7",
          "form": "근데 주변에 키우는 사람들은 있어",
          "original_form": "근데 주변에 키우는 사람들은 있어",
          "speaker_id": "1",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008745.1.8",
          "form": "고양이, 개, 고슴도치..",
          "original_form": "고양이, 개, 고슴도치..",
          "speaker_id": "1",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008745.1.9",
          "form": "아아 맞아 나도 주변에 고양이나 개 키우는 건 많이 봤어",
          "original_form": "아아 맞아 나도 주변에 고양이나 개 키우는 건 많이 봤어",
          "speaker_id": "2",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008745.1.10",
          "form": "고슴도치는 좀 신기하네",
          "original_form": "고슴도치는 좀 신기하네",
          "speaker_id": "2",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008745.1.11",
          "form": "그치 고슴도치는 쓰다듬기도 어렵더라",
          "original_form": "그치 고슴도치는 쓰다듬기도 어렵더라",
          "speaker_id": "1",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100008745.1.12",
          "form": "가시를 안 세우고 있는 걸 만져도 따가워..",
          "original_form": "가시를 안 세우고 있는 걸 만져도 따가워..",
          "speaker_id": "1",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100008745.1.13",
          "form": "알레르기 있거나.. 뭐 그런 건 아니지?",
          "original_form": "알레르기 있거나.. 뭐 그런 건 아니지?",
          "speaker_id": "1",
          "time": "20210822 18:12"
        },
        {
          "id": "MDRW2100008745.1.14",
          "form": "아 나 고양이털 알레르기는 있어",
          "original_form": "아 나 고양이털 알레르기는 있어",
          "speaker_id": "2",
          "time": "20210822 18:13"
        },
        {
          "id": "MDRW2100008745.1.15",
          "form": "헐 그렇구나",
          "original_form": "헐 그렇구나",
          "speaker_id": "1",
          "time": "20210822 18:14"
        },
        {
          "id": "MDRW2100008745.1.16",
          "form": "키워보고 싶지는 않았어?",
          "original_form": "키워보고 싶지는 않았어?",
          "speaker_id": "1",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008745.1.17",
          "form": "가끔 그런 생각할 때도 있는데",
          "original_form": "가끔 그런 생각할 때도 있는데",
          "speaker_id": "2",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008745.1.18",
          "form": "반려동물이 혹여나 아프거나 무지개다리 건너면 내가 마음이 많이 아플 것 같아서..?",
          "original_form": "반려동물이 혹여나 아프거나 무지개다리 건너면 내가 마음이 많이 아플 것 같아서..?",
          "speaker_id": "2",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008745.1.19",
          "form": "그게 마음에 걸려서 못 키우겠더라고",
          "original_form": "그게 마음에 걸려서 못 키우겠더라고",
          "speaker_id": "2",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008745.1.20",
          "form": "오 되게 마음이 깊구나",
          "original_form": "오 되게 마음이 깊구나",
          "speaker_id": "1",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008745.1.21",
          "form": "보통 애완동물의 귀여운 모습을 생각하지 마지막을 떠올리진 않으니까",
          "original_form": "보통 애완동물의 귀여운 모습을 생각하지 마지막을 떠올리진 않으니까",
          "speaker_id": "1",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100008745.1.22",
          "form": "맞는 말이야 나도 그거 생각하면 못 키울 거 같다",
          "original_form": "맞는 말이야 나도 그거 생각하면 못 키울 거 같다",
          "speaker_id": "1",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100008745.1.23",
          "form": "그치.. 그리고 금전적인 문제도 좀 있어서?",
          "original_form": "그치.. 그리고 금전적인 문제도 좀 있어서?",
          "speaker_id": "2",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008745.1.24",
          "form": "밥도 사야하고 미용도 시켜줘야하고 아프면 병원도 가야하니...",
          "original_form": "밥도 사야하고 미용도 시켜줘야하고 아프면 병원도 가야하니...",
          "speaker_id": "2",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008745.1.25",
          "form": "맞아 동물 키우는 게 쉬운 일이 아니지..",
          "original_form": "맞아 동물 키우는 게 쉬운 일이 아니지..",
          "speaker_id": "1",
          "time": "20210822 18:27"
        }
      ]
    }
