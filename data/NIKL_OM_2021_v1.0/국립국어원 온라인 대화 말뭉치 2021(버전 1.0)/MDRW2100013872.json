      "utterance": [
        {
          "id": "MDRW2100013872.1.1",
          "form": "남치니 나이도 있고 오래 만나왔으니 내후년쯤 보고있어요",
          "original_form": "남치니 나이도 있고 오래 만나왔으니 내후년쯤 보고있어요",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013872.1.2",
          "form": "근데 저는 이런 생각도 좀 들긴 해요",
          "original_form": "근데 저는 이런 생각도 좀 들긴 해요",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013872.1.3",
          "form": "이제 친구들도 다들 잘 못만나고",
          "original_form": "이제 친구들도 다들 잘 못만나고",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013872.1.4",
          "form": "좀 일찍 둘이 합쳐서 돈모으는게 좋지않을까",
          "original_form": "좀 일찍 둘이 합쳐서 돈모으는게 좋지않을까",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013872.1.5",
          "form": "놀건 얼추 다 놀아봤으니깐",
          "original_form": "놀건 얼추 다 놀아봤으니깐",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013872.1.6",
          "form": "전 못놀아봐서....",
          "original_form": "전 못놀아봐서....",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013872.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013872.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013872.1.9",
          "form": "방탕함 그 자체시네요",
          "original_form": "방탕함 그 자체시네요",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013872.1.10",
          "form": "얼마나 더 노시려고!",
          "original_form": "얼마나 더 노시려고!",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013872.1.11",
          "form": "ㅋㅋㅋㅋㅋ니가 뭘 알아!!!ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋㅋ니가 뭘 알아!!!ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013872.1.12",
          "form": "저 클럽 한 번도 안가봤고여...",
          "original_form": "저 클럽 한 번도 안가봤고여...",
          "speaker_id": "2",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013872.1.13",
          "form": "친구끼리 여행도 못가봤고...",
          "original_form": "친구끼리 여행도 못가봤고...",
          "speaker_id": "2",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013872.1.14",
          "form": "친구집에서 자본 적도 없고",
          "original_form": "친구집에서 자본 적도 없고",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013872.1.15",
          "form": "통금도 있어서 늦게까지 놀지도 못했고ㅜ",
          "original_form": "통금도 있어서 늦게까지 놀지도 못했고ㅜ",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013872.1.16",
          "form": "얘기할 수록 화나네...!!!",
          "original_form": "얘기할 수록 화나네...!!!",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013872.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013872.1.18",
          "form": "아이구야ㅜㅜ",
          "original_form": "아이구야ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013872.1.19",
          "form": "그 저",
          "original_form": "그 저",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013872.1.20",
          "form": "화이팅",
          "original_form": "화이팅",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013872.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
