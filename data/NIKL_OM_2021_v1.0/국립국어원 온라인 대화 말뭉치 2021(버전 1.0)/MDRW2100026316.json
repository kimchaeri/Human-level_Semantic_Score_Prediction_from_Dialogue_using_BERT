      "utterance": [
        {
          "id": "MDRW2100026316.1.1",
          "form": "좋아하는 음식 메뉴가 뭐에요?",
          "original_form": "좋아하는 음식 메뉴가 뭐에요?",
          "speaker_id": "2",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026316.1.2",
          "form": "저는 떡볶이 좋아해요",
          "original_form": "저는 떡볶이 좋아해요",
          "speaker_id": "1",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026316.1.3",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026316.1.4",
          "form": "엽떡파에요 신전파에요?",
          "original_form": "엽떡파에요 신전파에요?",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026316.1.5",
          "form": "저는 신전이요 ㅋㅋ",
          "original_form": "저는 신전이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026316.1.6",
          "form": "신전 좋아요",
          "original_form": "신전 좋아요",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100026316.1.7",
          "form": "신전 김밥이 너무 맛있어요",
          "original_form": "신전 김밥이 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100026316.1.8",
          "form": "뭐드세요? ㅋㅋ",
          "original_form": "뭐드세요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100026316.1.9",
          "form": "저는 떡볶이는 엽떡파인데",
          "original_form": "저는 떡볶이는 엽떡파인데",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026316.1.10",
          "form": "김밥은 신전 심밥이 맛있더라고요",
          "original_form": "김밥은 신전 심밥이 맛있더라고요",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026316.1.11",
          "form": "아하 전 로제떡볶이 좋아해서",
          "original_form": "아하 전 로제떡볶이 좋아해서",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026316.1.12",
          "form": "신전로제가 진짜 갑이에요",
          "original_form": "신전로제가 진짜 갑이에요",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026316.1.13",
          "form": "오 전 아직 로제 안먹어 봤는데",
          "original_form": "오 전 아직 로제 안먹어 봤는데",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026316.1.14",
          "form": "맛있나요?!!",
          "original_form": "맛있나요?!!",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026316.1.15",
          "form": "진짜 엄청 맛있어요",
          "original_form": "진짜 엄청 맛있어요",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026316.1.16",
          "form": "튀김드세요?",
          "original_form": "튀김드세요?",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026316.1.17",
          "form": "네무슨튀김 좋아하세요?",
          "original_form": "네무슨튀김 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026316.1.18",
          "form": "신전 오징어 튀김",
          "original_form": "신전 오징어 튀김",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026316.1.19",
          "form": "대박이에요",
          "original_form": "대박이에요",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026316.1.20",
          "form": "와 맛있겠어요!!",
          "original_form": "와 맛있겠어요!!",
          "speaker_id": "2",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026316.1.21",
          "form": "저는 튀김이라면 이것저것 다 잘먹어요",
          "original_form": "저는 튀김이라면 이것저것 다 잘먹어요",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026316.1.22",
          "form": "근데 만두는 배달만두를 따라갈수 없는거같아요",
          "original_form": "근데 만두는 배달만두를 따라갈수 없는거같아요",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026316.1.23",
          "form": "배달만두가 브랜드이름이에요?",
          "original_form": "배달만두가 브랜드이름이에요?",
          "speaker_id": "1",
          "time": "20210901 21:55"
        }
