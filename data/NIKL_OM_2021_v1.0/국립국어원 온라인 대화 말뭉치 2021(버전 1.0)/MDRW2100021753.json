      "utterance": [
        {
          "id": "MDRW2100021753.1.1",
          "form": "유튭보시나요",
          "original_form": "유튭보시나요",
          "speaker_id": "1",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021753.1.2",
          "form": "관심있는것만 봐요!",
          "original_form": "관심있는것만 봐요!",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021753.1.3",
          "form": "어떤거요??",
          "original_form": "어떤거요??",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021753.1.4",
          "form": "요즘은 새덕후 채널 좋아해요!ㅋㅋㅋ",
          "original_form": "요즘은 새덕후 채널 좋아해요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021753.1.5",
          "form": "진짜 새관련 채널인가요?",
          "original_form": "진짜 새관련 채널인가요?",
          "speaker_id": "1",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021753.1.6",
          "form": "네 ㅋㅋㅋ 채널 이름이 새덕후에요 ㅋㅋ",
          "original_form": "네 ㅋㅋㅋ 채널 이름이 새덕후에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021753.1.7",
          "form": "신기하네요ㅎㅎ",
          "original_form": "신기하네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021753.1.8",
          "form": "우리나라의 새관련 영상들 올리시는데 퀄리티가 장난아니에요",
          "original_form": "우리나라의 새관련 영상들 올리시는데 퀄리티가 장난아니에요",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021753.1.9",
          "form": "게스트는 윤무부 박사님인가요ㅎㅎ",
          "original_form": "게스트는 윤무부 박사님인가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021753.1.10",
          "form": "오랜만에 듣는 이름이네욬ㅋㅋㅋ 그분은 나오신적없으세요 ㅋㅋㅋ",
          "original_form": "오랜만에 듣는 이름이네욬ㅋㅋㅋ 그분은 나오신적없으세요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021753.1.11",
          "form": "새하면 떠오르는 분이네요ㅎㅎ",
          "original_form": "새하면 떠오르는 분이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100021753.1.12",
          "form": "ㅋㅋ 보다가 새를 좋아해서 본게 아니라 우연히 보게되서 그 후에 새가 좋아졌어요 ㅋㅋ",
          "original_form": "ㅋㅋ 보다가 새를 좋아해서 본게 아니라 우연히 보게되서 그 후에 새가 좋아졌어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:32"
        },
        {
          "id": "MDRW2100021753.1.13",
          "form": "한번 찾아바야겠어요ㅎㅎ",
          "original_form": "한번 찾아바야겠어요ㅎㅎ",
          "speaker_id": "1",
