          "speaker_id": "2",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014374.1.9",
          "form": "일반 테러가 아닌듯하다",
          "original_form": "일반 테러가 아닌듯하다",
          "speaker_id": "1",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014374.1.10",
          "form": "그럼",
          "original_form": "그럼",
