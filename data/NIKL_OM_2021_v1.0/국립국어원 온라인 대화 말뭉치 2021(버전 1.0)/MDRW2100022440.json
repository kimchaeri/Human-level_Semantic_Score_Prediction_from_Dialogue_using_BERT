        {
          "id": "MDRW2100022440.1.1",
          "form": "저 일단 비번부터 전남친 이름이라서",
          "original_form": "저 일단 비번부터 전남친 이름이라서",
          "speaker_id": "2",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100022440.1.2",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100022440.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100022440.1.4",
          "form": "비번부터 바꿔야되고",
          "original_form": "비번부터 바꿔야되고",
          "speaker_id": "2",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100022440.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100022440.1.6",
          "form": "6년이면 그럴만해요",
          "original_form": "6년이면 그럴만해요",
          "speaker_id": "1",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100022440.1.7",
          "form": "네 언제 싸이월드 탐방했었거든요",
          "original_form": "네 언제 싸이월드 탐방했었거든요",
          "speaker_id": "2",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100022440.1.8",
          "form": "서로의 이름으로 비번바꿔두고",
          "original_form": "서로의 이름으로 비번바꿔두고",
          "speaker_id": "2",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100022440.1.9",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100022440.1.10",
          "form": "숨김없이 흑역사까지 모두 보여준 사이",
          "original_form": "숨김없이 흑역사까지 모두 보여준 사이",
          "speaker_id": "2",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100022440.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100022440.1.12",
          "form": "거기 사진은 괜찮아요?",
          "original_form": "거기 사진은 괜찮아요?",
          "speaker_id": "1",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100022440.1.13",
          "form": "네 물론 이미 전남친 보여준다고 그 전전 사람들도 말끔히 정리햇엇고",
          "original_form": "네 물론 이미 전남친 보여준다고 그 전전 사람들도 말끔히 정리햇엇고",
          "speaker_id": "2",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100022440.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100022440.1.15",
          "form": "전남친은 싸이에 사진올릴때는 또 아니기땜에...",
          "original_form": "전남친은 싸이에 사진올릴때는 또 아니기땜에...",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022440.1.16",
          "form": "거기 다  있었군여",
          "original_form": "거기 다  있었군여",
          "speaker_id": "1",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022440.1.17",
          "form": "휴 님은 다 정리가 되어잇나요",
          "original_form": "휴 님은 다 정리가 되어잇나요",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022440.1.18",
          "form": "저요?",
          "original_form": "저요?",
          "speaker_id": "1",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022440.1.19",
          "form": "비번도 몰라요",
          "original_form": "비번도 몰라요",
          "speaker_id": "1",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100022440.1.20",
          "form": "무슨사진 있는지 기억도 안나요",
          "original_form": "무슨사진 있는지 기억도 안나요",
          "speaker_id": "1",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100022440.1.21",
          "form": "ㅋㅋㅋㅋㅋ봉쇄해도 되겠네요",
          "original_form": "ㅋㅋㅋㅋㅋ봉쇄해도 되겠네요",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100022440.1.22",
          "form": "근데 거기 흑역사 사진 겁나 많은데",
          "original_form": "근데 거기 흑역사 사진 겁나 많은데",
          "speaker_id": "1",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100022440.1.23",
          "form": "저도 한참 사진 많이 찍을 때니깐 또....",
          "original_form": "저도 한참 사진 많이 찍을 때니깐 또....",
          "speaker_id": "2",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100022440.1.24",
          "form": "그땐 그 귀찮은짓을 왜케 했는지",
          "original_form": "그땐 그 귀찮은짓을 왜케 했는지",
          "speaker_id": "1",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100022440.1.25",
          "form": "그러니까요 지금은 너무 귀찮아서 못하는데ㅋㅋㅋㅋ",
          "original_form": "그러니까요 지금은 너무 귀찮아서 못하는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:10"
        }
      ]
