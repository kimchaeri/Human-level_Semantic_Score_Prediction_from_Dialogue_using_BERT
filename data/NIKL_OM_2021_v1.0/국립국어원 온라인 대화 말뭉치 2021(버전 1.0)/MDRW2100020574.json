      "utterance": [
        {
          "id": "MDRW2100020574.1.1",
          "form": "혹시 아이스크림 좋아하세요",
          "original_form": "혹시 아이스크림 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 10:01"
        },
        {
          "id": "MDRW2100020574.1.2",
          "form": "맞아요 그날의 기분에 따라 달라질뿐",
          "original_form": "맞아요 그날의 기분에 따라 달라질뿐",
          "speaker_id": "2",
          "time": "20210828 10:01"
        },
        {
          "id": "MDRW2100020574.1.3",
          "form": "아이스크림 좋아합니다",
          "original_form": "아이스크림 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 10:01"
        },
        {
          "id": "MDRW2100020574.1.4",
          "form": "혹시 아이스크림 좋아하시나요",
          "original_form": "혹시 아이스크림 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 10:01"
        },
        {
          "id": "MDRW2100020574.1.5",
          "form": "하 여기서",
          "original_form": "하 여기서",
          "speaker_id": "2",
          "time": "20210828 10:01"
        },
        {
          "id": "MDRW2100020574.1.6",
          "form": "어려운 질문",
          "original_form": "어려운 질문",
          "speaker_id": "2",
          "time": "20210828 10:01"
        },
        {
          "id": "MDRW2100020574.1.7",
          "form": "설마",
          "original_form": "설마",
          "speaker_id": "1",
          "time": "20210828 10:01"
        },
        {
          "id": "MDRW2100020574.1.8",
          "form": "하나 가야겠네요",
          "original_form": "하나 가야겠네요",
          "speaker_id": "2",
          "time": "20210828 10:01"
        },
        {
          "id": "MDRW2100020574.1.9",
          "form": "민초 vs 반민초",
          "original_form": "민초 vs 반민초",
          "speaker_id": "2",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020574.1.10",
          "form": "민초 반민초...?",
          "original_form": "민초 반민초...?",
          "speaker_id": "1",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020574.1.11",
          "form": "소오름 나 그거치고있었는데ㅋㅋㅋ",
          "original_form": "소오름 나 그거치고있었는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020574.1.12",
          "form": "어서 대답해보세요!",
          "original_form": "어서 대답해보세요!",
          "speaker_id": "2",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020574.1.13",
          "form": "전 못먹습니다 (단호)",
          "original_form": "전 못먹습니다 (단호)",
          "speaker_id": "1",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020574.1.14",
          "form": "아 저런",
          "original_form": "아 저런",
          "speaker_id": "2",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020574.1.15",
          "form": "우리의 우정이",
          "original_form": "우리의 우정이",
          "speaker_id": "2",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020574.1.16",
          "form": "이렇게 금이가나요",
          "original_form": "이렇게 금이가나요",
          "speaker_id": "2",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020574.1.17",
          "form": "아 우리 한 100m멀어졌나요",
          "original_form": "아 우리 한 100m멀어졌나요",
          "speaker_id": "1",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020574.1.18",
          "form": "안타깝게도 전 민초를..",
          "original_form": "안타깝게도 전 민초를..",
          "speaker_id": "1",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020574.1.19",
          "form": "50m로 타협보죠",
          "original_form": "50m로 타협보죠",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020574.1.20",
          "form": "저그렇게 꽉막힌 사람 아닙니다",
          "original_form": "저그렇게 꽉막힌 사람 아닙니다",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020574.1.21",
          "form": "좋아요 50m면 해볼만하네요",
          "original_form": "좋아요 50m면 해볼만하네요",
          "speaker_id": "1",
          "time": "20210828 10:03"
        }
      ]
