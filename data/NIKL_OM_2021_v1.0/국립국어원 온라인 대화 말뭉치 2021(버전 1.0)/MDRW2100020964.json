      "utterance": [
        {
          "id": "MDRW2100020964.1.1",
          "form": "가을 장마 이제 끝인거죠?",
          "original_form": "가을 장마 이제 끝인거죠?",
          "speaker_id": "2",
          "time": "20210828 14:34"
        },
        {
          "id": "MDRW2100020964.1.2",
          "form": "여기는 다음주도 비예보에요..",
          "original_form": "여기는 다음주도 비예보에요..",
          "speaker_id": "1",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100020964.1.3",
          "form": "걍 계속 올듯.. 넘 싫으네요",
          "original_form": "걍 계속 올듯.. 넘 싫으네요",
          "speaker_id": "1",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100020964.1.4",
          "form": "으악~~비 이젠 그만 왔으면 좋겠어요",
          "original_form": "으악~~비 이젠 그만 왔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020964.1.5",
          "form": "빨래도 안마르고.. \r\n여름장마 잘 넘겼는데.. 가을 장마라니..",
          "original_form": "빨래도 안마르고.. \r\n여름장마 잘 넘겼는데.. 가을 장마라니..",
          "speaker_id": "1",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020964.1.6",
          "form": "빨래는 건조기로 해결이 어느정도 되는데 집안에 습기가 장난아니에요",
          "original_form": "빨래는 건조기로 해결이 어느정도 되는데 집안에 습기가 장난아니에요",
          "speaker_id": "2",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100020964.1.7",
          "form": "항상 온도보단 습기죠...",
          "original_form": "항상 온도보단 습기죠...",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020964.1.8",
          "form": "전 긴옷입고 에어컨 가동중.. ㅋㅋㅋ",
          "original_form": "전 긴옷입고 에어컨 가동중.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020964.1.9",
          "form": "이게 뭐하는딧인지..",
          "original_form": "이게 뭐하는딧인지..",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020964.1.10",
          "form": "습도 높으면 불쾌지수도 같이올라가요",
          "original_form": "습도 높으면 불쾌지수도 같이올라가요",
          "speaker_id": "2",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020964.1.11",
          "form": "글쵸... 긴팔 꺼내셧나요?",
          "original_form": "글쵸... 긴팔 꺼내셧나요?",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100020964.1.12",
          "form": "아직요. 아직은 반팔로 견딜만한것 같아요",
          "original_form": "아직요. 아직은 반팔로 견딜만한것 같아요",
          "speaker_id": "2",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100020964.1.13",
          "form": "원래 9월말까진 반팔 입었던 기억이..",
          "original_form": "원래 9월말까진 반팔 입었던 기억이..",
          "speaker_id": "1",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100020964.1.14",
          "form": "그리고 10월에 패딩.. ㅋㅋㅋㅋ",
          "original_form": "그리고 10월에 패딩.. ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100020964.1.15",
          "form": "10월 패딩 빠른것 같아요",
          "original_form": "10월 패딩 빠른것 같아요",
          "speaker_id": "2",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100020964.1.16",
          "form": "추위에 약하시군요",
          "original_form": "추위에 약하시군요",
          "speaker_id": "2",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100020964.1.17",
          "form": "ㅋㅋㅋ 겨울에 롱패딩입으시나요?",
          "original_form": "ㅋㅋㅋ 겨울에 롱패딩입으시나요?",
          "speaker_id": "1",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100020964.1.18",
          "form": "네. 겨울엔 롱패딩이죠",
          "original_form": "네. 겨울엔 롱패딩이죠",
          "speaker_id": "2",
          "time": "20210828 15:58"
        },
        {
          "id": "MDRW2100020964.1.19",
          "form": "롱패딩은 유행이 아니라 그냥 일상템 된듯..",
          "original_form": "롱패딩은 유행이 아니라 그냥 일상템 된듯..",
          "speaker_id": "1",
          "time": "20210828 15:59"
