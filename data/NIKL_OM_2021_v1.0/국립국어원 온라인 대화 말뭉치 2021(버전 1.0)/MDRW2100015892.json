      "utterance": [
        {
          "id": "MDRW2100015892.1.1",
          "form": "혹시 올해 야구 보시나요?",
          "original_form": "혹시 올해 야구 보시나요?",
          "speaker_id": "1",
          "time": "20210826 02:44"
        },
        {
          "id": "MDRW2100015892.1.2",
          "form": "가끔씩 봐요.",
          "original_form": "가끔씩 봐요.",
          "speaker_id": "2",
          "time": "20210826 02:45"
        },
        {
          "id": "MDRW2100015892.1.3",
          "form": "저도 진짜 가끔씩 봐요",
          "original_form": "저도 진짜 가끔씩 봐요",
          "speaker_id": "1",
          "time": "20210826 02:45"
        },
        {
          "id": "MDRW2100015892.1.4",
          "form": "요즘은 딱히 재미있지도 않고...",
          "original_form": "요즘은 딱히 재미있지도 않고...",
          "speaker_id": "1",
          "time": "20210826 02:45"
        },
        {
          "id": "MDRW2100015892.1.5",
          "form": "여러 사건사고가 많았죠",
          "original_form": "여러 사건사고가 많았죠",
          "speaker_id": "2",
          "time": "20210826 02:45"
        },
        {
          "id": "MDRW2100015892.1.6",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210826 02:45"
        },
        {
          "id": "MDRW2100015892.1.7",
          "form": "최근 두산 선수 약물 의혹도 있었던 것 같은데",
          "original_form": "최근 두산 선수 약물 의혹도 있었던 것 같은데",
          "speaker_id": "2",
          "time": "20210826 02:46"
        },
        {
          "id": "MDRW2100015892.1.8",
          "form": "저도 그 소식 듣고 놀랐는데",
          "original_form": "저도 그 소식 듣고 놀랐는데",
          "speaker_id": "1",
          "time": "20210826 02:46"
        },
        {
          "id": "MDRW2100015892.1.9",
          "form": "무혐의인가? 판정 났다고 하더라구요",
          "original_form": "무혐의인가? 판정 났다고 하더라구요",
          "speaker_id": "1",
          "time": "20210826 02:46"
        },
        {
          "id": "MDRW2100015892.1.10",
          "form": "다행이네요!",
          "original_form": "다행이네요!",
          "speaker_id": "2",
          "time": "20210826 02:46"
        },
        {
          "id": "MDRW2100015892.1.11",
          "form": "그러니까요.",
          "original_form": "그러니까요.",
          "speaker_id": "1",
          "time": "20210826 02:46"
        },
        {
          "id": "MDRW2100015892.1.12",
          "form": "천만다행이에요",
          "original_form": "천만다행이에요",
          "speaker_id": "1",
          "time": "20210826 02:46"
        },
        {
          "id": "MDRW2100015892.1.13",
          "form": "코로나로 리그 중단도 됐었잖아요",
          "original_form": "코로나로 리그 중단도 됐었잖아요",
          "speaker_id": "2",
          "time": "20210826 02:47"
        },
        {
          "id": "MDRW2100015892.1.14",
          "form": "확실히 그것 때문에 좌석 간 거리두기도 하고",
          "original_form": "확실히 그것 때문에 좌석 간 거리두기도 하고",
          "speaker_id": "1",
          "time": "20210826 02:47"
        },
        {
          "id": "MDRW2100015892.1.15",
          "form": "방역에 신경쓰는 것 같아요",
          "original_form": "방역에 신경쓰는 것 같아요",
          "speaker_id": "1",
          "time": "20210826 02:47"
        },
        {
          "id": "MDRW2100015892.1.16",
          "form": "선수들 확진되면 큰일이니까요",
          "original_form": "선수들 확진되면 큰일이니까요",
          "speaker_id": "2",
          "time": "20210826 02:48"
        },
        {
          "id": "MDRW2100015892.1.17",
          "form": "좋아하는 선수 있으세요?",
          "original_form": "좋아하는 선수 있으세요?",
          "speaker_id": "1",
          "time": "20210826 02:48"
        },
        {
          "id": "MDRW2100015892.1.18",
          "form": "저는 양현종 선수요",
          "original_form": "저는 양현종 선수요",
          "speaker_id": "2",
          "time": "20210826 02:48"
        }
      ]
