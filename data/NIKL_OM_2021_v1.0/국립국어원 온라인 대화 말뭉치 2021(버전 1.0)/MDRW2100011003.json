        {
          "id": "MDRW2100011003.1.1",
          "form": "동물 좋아하세요?",
          "original_form": "동물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100011003.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100011003.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100011003.1.4",
          "form": "전 강아지 키워요",
          "original_form": "전 강아지 키워요",
          "speaker_id": "1",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100011003.1.5",
          "form": "강아지 이름이 뭔가요?",
          "original_form": "강아지 이름이 뭔가요?",
          "speaker_id": "2",
          "time": "20210824 06:48"
        },
        {
          "id": "MDRW2100011003.1.6",
          "form": "뽕이ㅎㅎ",
          "original_form": "뽕이ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 10:19"
        },
        {
          "id": "MDRW2100011003.1.7",
          "form": "뽕이 귀여워요",
          "original_form": "뽕이 귀여워요",
          "speaker_id": "2",
          "time": "20210824 10:20"
        },
        {
          "id": "MDRW2100011003.1.8",
          "form": "수컷인가요?",
          "original_form": "수컷인가요?",
          "speaker_id": "2",
          "time": "20210824 10:20"
        },
        {
          "id": "MDRW2100011003.1.9",
          "form": "여자아이여ㅎ",
          "original_form": "여자아이여ㅎ",
          "speaker_id": "1",
          "time": "20210824 10:20"
        },
        {
          "id": "MDRW2100011003.1.10",
          "form": "여자라 더 사랑스럽겠어요~밥은 잘 먹나요?",
          "original_form": "여자라 더 사랑스럽겠어요~밥은 잘 먹나요?",
          "speaker_id": "2",
          "time": "20210824 10:25"
        },
        {
          "id": "MDRW2100011003.1.11",
          "form": "엄청 잘먹고요ㅋ",
          "original_form": "엄청 잘먹고요ㅋ",
          "speaker_id": "1",
          "time": "20210824 11:19"
        },
        {
          "id": "MDRW2100011003.1.12",
          "form": "간식은 뭘 좋아해요?",
          "original_form": "간식은 뭘 좋아해요?",
          "speaker_id": "2",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100011003.1.13",
          "form": "오리고기 좋아해요ㅎ",
          "original_form": "오리고기 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210824 11:34"
        },
        {
          "id": "MDRW2100011003.1.14",
          "form": "오리고기를 좋아하는군요~식비가 많이 드시겠어요~",
          "original_form": "오리고기를 좋아하는군요~식비가 많이 드시겠어요~",
          "speaker_id": "2",
          "time": "20210824 11:34"
        },
        {
          "id": "MDRW2100011003.1.15",
          "form": "사료비가 넘사벽",
          "original_form": "사료비가 넘사벽",
          "speaker_id": "1",
