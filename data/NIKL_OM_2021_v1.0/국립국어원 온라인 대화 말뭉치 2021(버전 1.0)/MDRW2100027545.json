      "utterance": [
        {
          "id": "MDRW2100027545.1.1",
          "form": "최근에 책 읽은 적 있어?",
          "original_form": "최근에 책 읽은 적 있어?",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027545.1.2",
          "form": "최근에 읽은 책은 없는데 관심있는 책은 있어.",
          "original_form": "최근에 읽은 책은 없는데 관심있는 책은 있어.",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027545.1.3",
          "form": "요즘 유럽복식으로 말이 많잖아.",
          "original_form": "요즘 유럽복식으로 말이 많잖아.",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027545.1.4",
          "form": "맞아 그건 나도 봤어",
          "original_form": "맞아 그건 나도 봤어",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027545.1.5",
          "form": "그래서 이지은 작가가 참여한 전시회 도록을 열심히 찾아봤는데",
          "original_form": "그래서 이지은 작가가 참여한 전시회 도록을 열심히 찾아봤는데",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027545.1.6",
          "form": "안나오더라...",
          "original_form": "안나오더라...",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027545.1.7",
          "form": "아 그건 나도 탐나더라",
          "original_form": "아 그건 나도 탐나더라",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027545.1.8",
          "form": "전시회 도록 종류가 많아서 중고로도 많이 팔더라 엄청 탐났음",
          "original_form": "전시회 도록 종류가 많아서 중고로도 많이 팔더라 엄청 탐났음",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027545.1.9",
          "form": "전시회 도록 좋지...전에 입시 할때는 확실히 작가 관련 책도 많이 샀던 듯",
          "original_form": "전시회 도록 좋지...전에 입시 할때는 확실히 작가 관련 책도 많이 샀던 듯",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027545.1.10",
          "form": "중고로 찾다가 한정판? 파리 의류 관련 도록 도서 파는 거 봤는데..",
          "original_form": "중고로 찾다가 한정판? 파리 의류 관련 도록 도서 파는 거 봤는데..",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027545.1.11",
          "form": "인상주의 관련 두꺼운 원서도 아직 있어",
          "original_form": "인상주의 관련 두꺼운 원서도 아직 있어",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027545.1.12",
          "form": "내용이 더 궁금해서 구글링했는데 내용이 더 안나오는거야",
          "original_form": "내용이 더 궁금해서 구글링했는데 내용이 더 안나오는거야",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027545.1.13",
          "form": "아마존 가격을 보니 75달러래",
          "original_form": "아마존 가격을 보니 75달러래",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027545.1.14",
          "form": "8만원인거야",
          "original_form": "8만원인거야",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027545.1.15",
          "form": "중고로 25000원에 팔던데",
          "original_form": "중고로 25000원에 팔던데",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027545.1.16",
          "form": "어라 조금만 힘내면 살 수 있겠는데",
          "original_form": "어라 조금만 힘내면 살 수 있겠는데",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027545.1.17",
          "form": "엄청 책사고 싶었어",
          "original_form": "엄청 책사고 싶었어",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027545.1.18",
          "form": "사자!",
          "original_form": "사자!",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027545.1.19",
          "form": "아냐, 영어 원서보다는",
          "original_form": "아냐, 영어 원서보다는",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027545.1.20",
          "form": "나도 최근에 책 왕창 사서",
          "original_form": "나도 최근에 책 왕창 사서",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027545.1.21",
          "form": "말릴 생각 없음",
          "original_form": "말릴 생각 없음",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027545.1.22",
          "form": "이지은 작가가 만든 다른 책도 자료가 좋아보였어",
          "original_form": "이지은 작가가 만든 다른 책도 자료가 좋아보였어",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027545.1.23",
          "form": "기왕이면 국내도서가.. 좋지",
          "original_form": "기왕이면 국내도서가.. 좋지",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027545.1.24",
          "form": "아...탐나는군.",
          "original_form": "아...탐나는군.",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027545.1.25",
          "form": "맞아 원서 해독도 까다롭고",
          "original_form": "맞아 원서 해독도 까다롭고",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027545.1.26",
          "form": "심지어 그게 프랑스어라면..?",
          "original_form": "심지어 그게 프랑스어라면..?",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027545.1.27",
          "form": "난 그래서 최근에는 계속 국내 여성작가 신작 모아서 삼",
          "original_form": "난 그래서 최근에는 계속 국내 여성작가 신작 모아서 삼",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027545.1.28",
          "form": "나는 펀딩으로 출판사에서 내는 한정판 제작도서 모으는 재미",
          "original_form": "나는 펀딩으로 출판사에서 내는 한정판 제작도서 모으는 재미",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027545.1.29",
          "form": "특히 sf 중에 신선한 내용이 많더라고",
          "original_form": "특히 sf 중에 신선한 내용이 많더라고",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027545.1.30",
          "form": "이번에 행복한 왕자 초판 복원본 샀는데 아직도 못읽어봤어",
          "original_form": "이번에 행복한 왕자 초판 복원본 샀는데 아직도 못읽어봤어",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
