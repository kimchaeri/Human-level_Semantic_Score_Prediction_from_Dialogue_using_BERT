      "utterance": [
        {
          "id": "MDRW2100037473.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "2",
          "time": "20210901 00:37"
        },
        {
          "id": "MDRW2100037473.1.2",
          "form": "그러면 명절음식은 안드세요?",
          "original_form": "그러면 명절음식은 안드세요?",
          "speaker_id": "1",
          "time": "20210901 00:38"
        },
        {
          "id": "MDRW2100037473.1.3",
          "form": "먹기야 먹죠ㅋㅋㅋ",
          "original_form": "먹기야 먹죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:39"
        },
        {
          "id": "MDRW2100037473.1.4",
          "form": "근데 몇 년 전 부터는",
          "original_form": "근데 몇 년 전 부터는",
          "speaker_id": "2",
          "time": "20210901 00:39"
        },
        {
          "id": "MDRW2100037473.1.5",
          "form": "명절음식 거의 안 만들어서",
          "original_form": "명절음식 거의 안 만들어서",
          "speaker_id": "2",
          "time": "20210901 00:39"
        },
        {
          "id": "MDRW2100037473.1.6",
          "form": "명절음식의 소용돌이 기억 속에서 빠져나가고 있어요",
          "original_form": "명절음식의 소용돌이 기억 속에서 빠져나가고 있어요",
          "speaker_id": "2",
          "time": "20210901 00:39"
        },
        {
          "id": "MDRW2100037473.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 오.. 거기서 안좋아하는 메뉴가 뭐에요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 오.. 거기서 안좋아하는 메뉴가 뭐에요?",
          "speaker_id": "1",
          "time": "20210901 00:48"
        },
        {
          "id": "MDRW2100037473.1.8",
          "form": "음 아무래도 생선메뉴요",
          "original_form": "음 아무래도 생선메뉴요",
          "speaker_id": "2",
          "time": "20210901 00:50"
        },
        {
          "id": "MDRW2100037473.1.9",
          "form": "생선찐거라던가 명태전이라던가 문어라던가",
          "original_form": "생선찐거라던가 명태전이라던가 문어라던가",
          "speaker_id": "2",
          "time": "20210901 00:50"
        },
        {
          "id": "MDRW2100037473.1.10",
          "form": "그럼 안드세여?",
          "original_form": "그럼 안드세여?",
          "speaker_id": "1",
          "time": "20210901 00:54"
        },
        {
          "id": "MDRW2100037473.1.11",
          "form": "네 생선메뉴는 안 먹어요",
          "original_form": "네 생선메뉴는 안 먹어요",
          "speaker_id": "2",
          "time": "20210901 00:54"
        },
        {
          "id": "MDRW2100037473.1.12",
          "form": "오... 새우튀김은요",
          "original_form": "오... 새우튀김은요",
          "speaker_id": "1",
          "time": "20210901 00:55"
        },
        {
          "id": "MDRW2100037473.1.13",
          "form": "새우튀김은 먹을줄 아는데 저희 집은 명절에 새우튀김은 안 해서 먹을일이 없었이요",
          "original_form": "새우튀김은 먹을줄 아는데 저희 집은 명절에 새우튀김은 안 해서 먹을일이 없었이요",
          "speaker_id": "2",
          "time": "20210901 00:56"
        },
        {
          "id": "MDRW2100037473.1.14",
          "form": "오..그렇군요..",
          "original_form": "오..그렇군요..",
          "speaker_id": "1",
          "time": "20210901 00:56"
        },
        {
          "id": "MDRW2100037473.1.15",
          "form": "좋아하기는 동그랑땡 하면 좋아했었어요ㅋㅋ \n두부도 잘먹고",
          "original_form": "좋아하기는 동그랑땡 하면 좋아했었어요ㅋㅋ \n두부도 잘먹고",
          "speaker_id": "2",
          "time": "20210901 00:57"
        },
        {
          "id": "MDRW2100037473.1.16",
          "form": "ㅋㅋㅋㅋㅋ 오.. 제일 기본적인게 제일맛잇죠",
          "original_form": "ㅋㅋㅋㅋㅋ 오.. 제일 기본적인게 제일맛잇죠",
          "speaker_id": "1",
          "time": "20210901 00:59"
        },
        {
          "id": "MDRW2100037473.1.17",
          "form": "네ㅋㅋ 그리고 튀김류들은 만들어서 따뜻할 때 먹는게 제일 맛있어요",
          "original_form": "네ㅋㅋ 그리고 튀김류들은 만들어서 따뜻할 때 먹는게 제일 맛있어요",
          "speaker_id": "2",
          "time": "20210901 01:00"
        },
        {
          "id": "MDRW2100037473.1.18",
          "form": "요리하다가 맛보고 요런 재미요ㅋㅋㅋ",
          "original_form": "요리하다가 맛보고 요런 재미요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 01:01"
        }
      ]
