      "utterance": [
        {
          "id": "MDRW2100041241.1.1",
          "form": "일단 마늘치킨부터 도전하고!",
          "original_form": "일단 마늘치킨부터 도전하고!",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100041241.1.2",
          "form": "다음에 깐풍치킨 도전해보께요!ㅋㅋㅋㅋ",
          "original_form": "다음에 깐풍치킨 도전해보께요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100041241.1.3",
          "form": "내일은 뭐 드실건가용",
          "original_form": "내일은 뭐 드실건가용",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100041241.1.4",
          "form": "평범하게 집밥 먹을거 같네요ㅋㅋ",
          "original_form": "평범하게 집밥 먹을거 같네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100041241.1.5",
          "form": "음 주로 해드시는건 뭐 있어요?",
          "original_form": "음 주로 해드시는건 뭐 있어요?",
          "speaker_id": "1",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100041241.1.6",
          "form": "거의 사먹어가지구...",
          "original_form": "거의 사먹어가지구...",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100041241.1.7",
          "form": "김치찌개정도요?",
          "original_form": "김치찌개정도요?",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100041241.1.8",
          "form": "김치찌개 소울푸드죠 너무 좋아요",
          "original_form": "김치찌개 소울푸드죠 너무 좋아요",
          "speaker_id": "1",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100041241.1.9",
          "form": "name2님은 고기 넣는게 좋으세요 참치 넣는게 좋으세요?",
          "original_form": "&name2&님은 고기 넣는게 좋으세요 참치 넣는게 좋으세요?",
          "speaker_id": "1",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100041241.1.10",
          "form": "고기요. 돼지고기ㅋㅋㅋ 없을땐 참치도 자주 넣지만요",
          "original_form": "고기요. 돼지고기ㅋㅋㅋ 없을땐 참치도 자주 넣지만요",
          "speaker_id": "2",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100041241.1.11",
          "form": "저도요!! 고기 최고!!ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도요!! 고기 최고!!ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100041241.1.12",
          "form": "참치는 국물에 완전 흩어져서 먹기 어려워요 ㅠ",
          "original_form": "참치는 국물에 완전 흩어져서 먹기 어려워요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100041241.1.13",
          "form": "참치가 없어!ㅋㅋㅋ",
          "original_form": "참치가 없어!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100041241.1.14",
          "form": "전 되기직전에 넣어서 뭉텅이로 집어먹어요ㅋㅋ",
          "original_form": "전 되기직전에 넣어서 뭉텅이로 집어먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100041241.1.15",
          "form": "앗? 그런 방법이 있었네요? ㅋㅋㅋㅋ 그럼 떠 먹고 나도 고대로 있어요?",
          "original_form": "앗? 그런 방법이 있었네요? ㅋㅋㅋㅋ 그럼 떠 먹고 나도 고대로 있어요?",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100041241.1.16",
          "form": "네ㅋㅋ 나중에 다시 끓이느라 뒤적거리면 흩어지긴 햐요ㅋㅋㅋ",
          "original_form": "네ㅋㅋ 나중에 다시 끓이느라 뒤적거리면 흩어지긴 햐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100041241.1.17",
          "form": "ㅋㅋㅋ 나중에 저도 따라해봐야겠어요 나중에 넣는건 왜 생각 못했나 싶어요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 나중에 저도 따라해봐야겠어요 나중에 넣는건 왜 생각 못했나 싶어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100041241.1.18",
          "form": "두부 좋아하시나요?",
          "original_form": "두부 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100041241.1.19",
          "form": "네 좋아해요. 간장찍어먹는거 맛있어요",
          "original_form": "네 좋아해요. 간장찍어먹는거 맛있어요",
          "speaker_id": "2",
          "time": "20210905 21:41"
        },
        {
          "id": "MDRW2100041241.1.20",
          "form": "저도 좋아해요! 두부로 한 모든걸 다 좋아하능것 같아요 ㅋㅋㅋ",
          "original_form": "저도 좋아해요! 두부로 한 모든걸 다 좋아하능것 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:41"
        },
        {
          "id": "MDRW2100041241.1.21",
          "form": "김치찌개에 두부도 한모 다넣어요",
          "original_form": "김치찌개에 두부도 한모 다넣어요",
          "speaker_id": "1",
          "time": "20210905 21:41"
        },
        {
          "id": "MDRW2100041241.1.22",
          "form": "두부 넣어도 맛있져ㅋㅋ",
          "original_form": "두부 넣어도 맛있져ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100041241.1.23",
          "form": "전 두부 조림을 더 좋아하긴 해요",
          "original_form": "전 두부 조림을 더 좋아하긴 해요",
          "speaker_id": "2",
          "time": "20210905 21:43"
        }
      ]
