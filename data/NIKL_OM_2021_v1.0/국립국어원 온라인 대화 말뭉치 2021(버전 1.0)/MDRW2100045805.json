      "utterance": [
        {
          "id": "MDRW2100045805.1.1",
          "form": "정수기가 말라가네요",
          "original_form": "정수기가 말라가네요",
          "speaker_id": "2",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045805.1.2",
          "form": "물도 빨리 모으셔야",
          "original_form": "물도 빨리 모으셔야",
          "speaker_id": "1",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045805.1.3",
          "form": "현실처럼 비가 오는 클라스 ㄷㄷ",
          "original_form": "현실처럼 비가 오는 클라스 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045805.1.4",
          "form": "비소리 대박무숩네요",
          "original_form": "비소리 대박무숩네요",
          "speaker_id": "1",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045805.1.5",
          "form": "냉장고가 2대있으신대",
          "original_form": "냉장고가 2대있으신대",
          "speaker_id": "1",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100045805.1.6",
          "form": "음식이 별로없나바요",
          "original_form": "음식이 별로없나바요",
          "speaker_id": "2",
          "time": "20210908 07:27"
        },
        {
          "id": "MDRW2100045805.1.7",
          "form": "물고랴 캐릭터..",
          "original_form": "물고랴 캐릭터..",
          "speaker_id": "1",
          "time": "20210908 07:27"
        },
        {
          "id": "MDRW2100045805.1.8",
          "form": "물을 많이먹내요",
          "original_form": "물을 많이먹내요",
          "speaker_id": "1",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100045805.1.9",
          "form": "아.. 밖에서 물을 받고있네요",
          "original_form": "아.. 밖에서 물을 받고있네요",
          "speaker_id": "2",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100045805.1.10",
          "form": "은근 힘들다 게임",
          "original_form": "은근 힘들다 게임",
          "speaker_id": "1",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100045805.1.11",
          "form": "뭐만 해도 지치는 ㅎㅎ",
          "original_form": "뭐만 해도 지치는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100045805.1.12",
          "form": "수시로 배고프구요",
          "original_form": "수시로 배고프구요",
          "speaker_id": "1",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100045805.1.13",
          "form": "지난시간 보면 배고플만 해요",
          "original_form": "지난시간 보면 배고플만 해요",
          "speaker_id": "2",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045805.1.14",
          "form": "장마라 비 자주와서 다행",
          "original_form": "장마라 비 자주와서 다행",
          "speaker_id": "1",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045805.1.15",
          "form": "물이라도 많이 모아야",
          "original_form": "물이라도 많이 모아야",
          "speaker_id": "2",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045805.1.16",
          "form": "장마라 비가 많이 온데요",
          "original_form": "장마라 비가 많이 온데요",
          "speaker_id": "1",
