      "utterance": [
        {
          "id": "MDRW2100045462.1.1",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045462.1.2",
          "form": "좀 순등순등하고",
          "original_form": "좀 순등순등하고",
          "speaker_id": "2",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045462.1.3",
          "form": "대화 잘통하고",
          "original_form": "대화 잘통하고",
          "speaker_id": "2",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045462.1.4",
          "form": "눈치 있고",
          "original_form": "눈치 있고",
          "speaker_id": "2",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045462.1.5",
          "form": "배려심 넘치는 사람이요",
          "original_form": "배려심 넘치는 사람이요",
          "speaker_id": "2",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045462.1.6",
          "form": "와 완전 저내요 ㅋㅋ",
          "original_form": "와 완전 저내요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045462.1.7",
          "form": "이거 고백? ㅋㅋ",
          "original_form": "이거 고백? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045462.1.8",
          "form": "저희.. 사귈래요?",
          "original_form": "저희.. 사귈래요?",
          "speaker_id": "2",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045462.1.9",
          "form": "맞아요.. 고백...",
          "original_form": "맞아요.. 고백...",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045462.1.10",
          "form": "야... 잎이 5개인데",
          "original_form": "야... 잎이 5개인데",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045462.1.11",
          "form": "아뇨 저 눈 높아요",
          "original_form": "아뇨 저 눈 높아요",
          "speaker_id": "1",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045462.1.12",
          "form": "하나가 떨어졌어",
          "original_form": "하나가 떨어졌어",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045462.1.13",
          "form": "이런 사가지",
          "original_form": "이런 사가지",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045462.1.14",
          "form": "님 연애 포기했잖아요 저도 초기하세요",
          "original_form": "님 연애 포기했잖아요 저도 초기하세요",
          "speaker_id": "1",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045462.1.15",
          "form": "포기",
          "original_form": "포기",
          "speaker_id": "1",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045462.1.16",
          "form": "맞아요 연애 초기예요",
          "original_form": "맞아요 연애 초기예요",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045462.1.17",
          "form": "우리 오늘부터 1일?",
          "original_form": "우리 오늘부터 1일?",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045462.1.18",
          "form": "초기화 ㅋㅋ",
          "original_form": "초기화 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045462.1.19",
          "form": "찡긋 찡긋 0_<",
          "original_form": "찡긋 찡긋 0_<",
          "speaker_id": "2",
          "time": "20210908 02:43"
        },
        {
          "id": "MDRW2100045462.1.20",
          "form": "언제나 처음처럼 사랑해...",
          "original_form": "언제나 처음처럼 사랑해...",
          "speaker_id": "2",
          "time": "20210908 02:43"
        },
        {
          "id": "MDRW2100045462.1.21",
          "form": "전 눈높다니까요 ㅋㅋ",
          "original_form": "전 눈높다니까요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:43"
        },
        {
          "id": "MDRW2100045462.1.22",
          "form": "저도 눈 높아서",
          "original_form": "저도 눈 높아서",
          "speaker_id": "2",
          "time": "20210908 02:43"
        },
        {
          "id": "MDRW2100045462.1.23",
          "form": "눈이 맞았네요",
          "original_form": "눈이 맞았네요",
          "speaker_id": "2",
          "time": "20210908 02:43"
        },
        {
          "id": "MDRW2100045462.1.24",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:43"
        },
        {
          "id": "MDRW2100045462.1.25",
          "form": "완전 천생연분인데",
          "original_form": "완전 천생연분인데",
          "speaker_id": "2",
          "time": "20210908 02:43"
        },
        {
          "id": "MDRW2100045462.1.26",
          "form": "전 돈만봐요..",
          "original_form": "전 돈만봐요..",
          "speaker_id": "1",
          "time": "20210908 02:43"
        },
        {
          "id": "MDRW2100045462.1.27",
          "form": "제 이상형은",
          "original_form": "제 이상형은",
          "speaker_id": "1",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045462.1.28",
          "form": "고양이상 키크고 마른 성격 좀 하자있는 사람이요",
          "original_form": "고양이상 키크고 마른 성격 좀 하자있는 사람이요",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045462.1.29",
          "form": "그리고 돈",
          "original_form": "그리고 돈",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045462.1.30",
          "form": "명예",
          "original_form": "명예",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045462.1.31",
