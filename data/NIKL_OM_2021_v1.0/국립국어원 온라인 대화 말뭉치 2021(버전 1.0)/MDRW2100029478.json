      "utterance": [
        {
          "id": "MDRW2100029478.1.1",
          "form": "그러더라구요",
          "original_form": "그러더라구요",
          "speaker_id": "2",
          "time": "20210903 09:39"
        },
        {
          "id": "MDRW2100029478.1.2",
          "form": "다른 사고싶은거 있나요?",
          "original_form": "다른 사고싶은거 있나요?",
          "speaker_id": "1",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100029478.1.3",
          "form": "간절기되니까 또 옷사고싶어져요ㅋㅌ",
          "original_form": "간절기되니까 또 옷사고싶어져요ㅋㅌ",
          "speaker_id": "2",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100029478.1.4",
          "form": "맞아용 아근데 요즘 옷입기 넘힘들어요 ㅠ 낮엔 덥고 아침밤은 싸하고",
          "original_form": "맞아용 아근데 요즘 옷입기 넘힘들어요 ㅠ 낮엔 덥고 아침밤은 싸하고",
          "speaker_id": "1",
          "time": "20210903 10:01"
        },
        {
          "id": "MDRW2100029478.1.5",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100029478.1.6",
          "form": "여름가디건같은거 가지고다녀요ㅜㅜ",
          "original_form": "여름가디건같은거 가지고다녀요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100029478.1.7",
          "form": "그래야겠어요 !",
          "original_form": "그래야겠어요 !",
          "speaker_id": "1",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029478.1.8",
          "form": "옷은 맨날 사는거같은데",
          "original_form": "옷은 맨날 사는거같은데",
          "speaker_id": "2",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029478.1.9",
          "form": "계절돌아오면 입을게 없을까요ㅋㅋ",
          "original_form": "계절돌아오면 입을게 없을까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029478.1.10",
          "form": "그러니깐요 ㅠㅋㅋㅋ 인터넷으로 옷말고 구매해보신 제품있나요",
          "original_form": "그러니깐요 ㅠㅋㅋㅋ 인터넷으로 옷말고 구매해보신 제품있나요",
          "speaker_id": "1",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100029478.1.11",
          "form": "엄청 이것저것 많이사요",
          "original_form": "엄청 이것저것 많이사요",
          "speaker_id": "2",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029478.1.12",
          "form": "직구도많이하고ㅋㅋ",
          "original_form": "직구도많이하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029478.1.13",
          "form": "저는 먹는거위주로 ..",
          "original_form": "저는 먹는거위주로 ..",
          "speaker_id": "1",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029478.1.14",
          "form": "어디 쇼핑몰 많이써요? 전 11번가용",
          "original_form": "어디 쇼핑몰 많이써요? 전 11번가용",
          "speaker_id": "1",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029478.1.15",
          "form": "아 저도요!",
          "original_form": "아 저도요!",
          "speaker_id": "2",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029478.1.16",
          "form": "아이디어스라고 수제품파는데있는데",
          "original_form": "아이디어스라고 수제품파는데있는데",
          "speaker_id": "2",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029478.1.17",
          "form": "거기서도많이사요",
          "original_form": "거기서도많이사요",
          "speaker_id": "2",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029478.1.18",
          "form": "아 알아용 거기 아이디어상품들 많더라구용",
          "original_form": "아 알아용 거기 아이디어상품들 많더라구용",
          "speaker_id": "1",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100029478.1.19",
          "form": "마자요마자요",
          "original_form": "마자요마자요",
          "speaker_id": "2",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100029478.1.20",
          "form": "다양하고!",
          "original_form": "다양하고!",
          "speaker_id": "2",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100029478.1.21",
          "form": "쇼핑 즐겨하세요?",
          "original_form": "쇼핑 즐겨하세요?",
          "speaker_id": "1",
