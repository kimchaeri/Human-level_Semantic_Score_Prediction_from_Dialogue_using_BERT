        },
        {
          "id": "MDRW2100002328.1.3",
          "form": "자녀계획 없으면",
          "original_form": "자녀계획 없으면",
          "speaker_id": "2",
          "time": "20210820 09:35"
        },
        {
          "id": "MDRW2100002328.1.4",
          "form": "여기도 전세를 안줘",
          "original_form": "여기도 전세를 안줘",
          "speaker_id": "1",
          "time": "20210820 09:35"
        },
        {
          "id": "MDRW2100002328.1.5",
          "form": "맞벌이 없어?",
          "original_form": "맞벌이 없어?",
          "speaker_id": "2",
          "time": "20210820 09:36"
        },
        {
          "id": "MDRW2100002328.1.6",
          "form": "와이프 지금 취업준비중",
