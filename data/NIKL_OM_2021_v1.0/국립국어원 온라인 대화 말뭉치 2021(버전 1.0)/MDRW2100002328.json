          "speaker_id": "1",
          "time": "20210820 09:36"
        },
        {
          "id": "MDRW2100002328.1.7",
          "form": "근데 월세 싸긴하다",
          "original_form": "근데 월세 싸긴하다",
          "speaker_id": "2",
          "time": "20210820 09:36"
        },
        {
          "id": "MDRW2100002328.1.8",
          "form": "너와이프일해?",
          "original_form": "너와이프일해?",
