      "utterance": [
        {
          "id": "MDRW2100014037.1.1",
          "form": "근무중이신가요",
          "original_form": "근무중이신가요",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014037.1.2",
          "form": "네 회사원이에요ㅎㅎ",
          "original_form": "네 회사원이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014037.1.3",
          "form": "업무 스트레스는 없으신가요",
          "original_form": "업무 스트레스는 없으신가요",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014037.1.4",
          "form": "저는 업무에 비해서 사람을 안뽑아줘서 스트레스에요ㅜㅜ",
          "original_form": "저는 업무에 비해서 사람을 안뽑아줘서 스트레스에요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014037.1.5",
          "form": "아 업무량이 많으신가보네요",
          "original_form": "아 업무량이 많으신가보네요",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014037.1.6",
          "form": "네ㅜ 한명  정도 더 있으면 딱인데",
          "original_form": "네ㅜ 한명  정도 더 있으면 딱인데",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014037.1.7",
          "form": "힘드네요ㅋㅋㅋ",
          "original_form": "힘드네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014037.1.8",
          "form": "그럼 업무스트레스는 어떻게 해소하시나요",
          "original_form": "그럼 업무스트레스는 어떻게 해소하시나요",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014037.1.9",
          "form": "전 그냥 남편한테 뒷담화 하면서 풀어요ㅋㅋㅋㅋ",
          "original_form": "전 그냥 남편한테 뒷담화 하면서 풀어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014037.1.10",
          "form": "오 좋은 방법이네요 남편이 잘 들어주시나봐요",
          "original_form": "오 좋은 방법이네요 남편이 잘 들어주시나봐요",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014037.1.11",
          "form": "남편도 같이 회사 사람들 뒷담화해요ㅋㅋㅋ",
          "original_form": "남편도 같이 회사 사람들 뒷담화해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014037.1.12",
          "form": "업무스트레스 해소에는 수다가 최고인것같아요.ㅡ일종의 뒷담화이기도 하고..ㅋ",
          "original_form": "업무스트레스 해소에는 수다가 최고인것같아요.ㅡ일종의 뒷담화이기도 하고..ㅋ",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014037.1.13",
          "form": "ㅋㅋㅋ맞아요 남편이랑 수다떨고 나면 또 담날 일할때 문제없더라고요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ맞아요 남편이랑 수다떨고 나면 또 담날 일할때 문제없더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014037.1.14",
          "form": "좋네요 동료들과는 잘 맞으시나요",
          "original_form": "좋네요 동료들과는 잘 맞으시나요",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014037.1.15",
          "form": "네 동료들이랑은 잘 지내죠ㅎㅎ",
          "original_form": "네 동료들이랑은 잘 지내죠ㅎㅎ",
