        },
        {
          "id": "MDRW2100020129.1.3",
          "form": "효연 신곡 들어봤니",
          "original_form": "효연 신곡 들어봤니",
          "speaker_id": "2",
          "time": "20210829 01:04"
        },
        {
          "id": "MDRW2100020129.1.4",
          "form": "소녀시대 효연?",
          "original_form": "소녀시대 효연?",
          "speaker_id": "1",
          "time": "20210829 01:04"
        },
        {
          "id": "MDRW2100020129.1.5",
          "form": "솔로 앨범 냈는데 몰랐구나",
          "original_form": "솔로 앨범 냈는데 몰랐구나",
          "speaker_id": "2",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100020129.1.6",
          "form": "맞아 몰랐어",
          "original_form": "맞아 몰랐어",
          "speaker_id": "1",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100020129.1.7",
          "form": "효연이 노래를 잘하더라",
          "original_form": "효연이 노래를 잘하더라",
          "speaker_id": "2",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100020129.1.8",
          "form": "효연 은 춤을 잘 추지",
          "original_form": "효연 은 춤을 잘 추지",
          "speaker_id": "1",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100020129.1.9",
          "form": "그건 알았는데 노래도 잘한다니까",
          "original_form": "그건 알았는데 노래도 잘한다니까",
          "speaker_id": "2",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100020129.1.10",
          "form": "너가 그렇게 말하니 궁금하네",
          "original_form": "너가 그렇게 말하니 궁금하네",
          "speaker_id": "1",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100020129.1.11",
          "form": "여름에 어울리는 노래",
          "original_form": "여름에 어울리는 노래",
          "speaker_id": "2",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100020129.1.12",
          "form": "상큼 발랄하겠다",
          "original_form": "상큼 발랄하겠다",
          "speaker_id": "1",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100020129.1.13",
          "form": "맞아 그리고 무비 도 이뻐",
          "original_form": "맞아 그리고 무비 도 이뻐",
          "speaker_id": "2",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100020129.1.14",
          "form": "효연 은 아이돌 능력 지가 좋구나",
          "original_form": "효연 은 아이돌 능력 지가 좋구나",
          "speaker_id": "1",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100020129.1.15",
          "form": "그러니까 말이야 멋있어",
          "original_form": "그러니까 말이야 멋있어",
          "speaker_id": "2",
