      "utterance": [
        {
          "id": "MDRW2100013704.1.1",
          "form": "다이어트중이셔요?",
          "original_form": "다이어트중이셔요?",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013704.1.2",
          "form": "아녀 지금은 안 하고 있어요 name1님은 다욧중이신가요??",
          "original_form": "아녀 지금은 안 하고 있어요 &name1&님은 다욧중이신가요??",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013704.1.3",
          "form": "해야하는데 게을러서 문제네요",
          "original_form": "해야하는데 게을러서 문제네요",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013704.1.4",
          "form": "식단관리하시는분들 대단하셔요",
          "original_form": "식단관리하시는분들 대단하셔요",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013704.1.5",
          "form": "확실히 다이어트는 식단이 8할인 거 같아요 ㅠㅠ",
          "original_form": "확실히 다이어트는 식단이 8할인 거 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013704.1.6",
          "form": "전에 운동만 했더니 진짜 더디게 빠지더라구요",
          "original_form": "전에 운동만 했더니 진짜 더디게 빠지더라구요",
          "speaker_id": "2",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013704.1.7",
          "form": "저는 먹는걸 너무 좋아해서",
          "original_form": "저는 먹는걸 너무 좋아해서",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013704.1.8",
          "form": "다이어트는 못할듯요.",
          "original_form": "다이어트는 못할듯요.",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013704.1.9",
          "form": "ㅋㅋㅋㅋㅋ 어떤 음싣 좋아하세요?",
          "original_form": "ㅋㅋㅋㅋㅋ 어떤 음싣 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013704.1.10",
          "form": "음식도 이너뷰티죠 ^^~",
          "original_form": "음식도 이너뷰티죠 ^^~",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013704.1.11",
          "form": "다좋아해요,특히 고기류",
          "original_form": "다좋아해요,특히 고기류",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013704.1.12",
          "form": "고기가 면역력에 참 좋져 ㅎㅎㅎ",
          "original_form": "고기가 면역력에 참 좋져 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013704.1.13",
          "form": "황제 다욧 한번 해보세요!",
          "original_form": "황제 다욧 한번 해보세요!",
          "speaker_id": "2",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013704.1.14",
          "form": "돈많이들지않나요 ㅎ",
          "original_form": "돈많이들지않나요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100013704.1.15",
          "form": "앗.. 맞아요 ㅎㅎ...",
          "original_form": "앗.. 맞아요 ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013704.1.16",
          "form": "다이어트도 진짜 돈 많이 드는 거 같어요",
          "original_form": "다이어트도 진짜 돈 많이 드는 거 같어요",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013704.1.17",
          "form": "돈안드는 운동은 걷기운동뿐인가봐요",
          "original_form": "돈안드는 운동은 걷기운동뿐인가봐요",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100013704.1.18",
          "form": "운동과 식이요법 다 잘해야하는데",
          "original_form": "운동과 식이요법 다 잘해야하는데",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100013704.1.19",
          "form": "맞아요 운동도 다 돈이에요 ㅠㅠ",
          "original_form": "맞아요 운동도 다 돈이에요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100013704.1.20",
          "form": "운동복도 다 돈이구 ㅠㅠ",
          "original_form": "운동복도 다 돈이구 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100013704.1.21",
          "form": "갖춰입지않고 걍 걷기운동하는건 돈안들지요 ㅎ",
          "original_form": "갖춰입지않고 걍 걷기운동하는건 돈안들지요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100013704.1.22",
          "form": "근데 걷기 운동이 효율이 넘 떨어져요 ㅠㅍ",
          "original_form": "근데 걷기 운동이 효율이 넘 떨어져요 ㅠㅍ",
          "speaker_id": "2",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100013704.1.23",
          "form": "계단오르기해요",
          "original_form": "계단오르기해요",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100013704.1.24",
          "form": "재미는 없지만요",
          "original_form": "재미는 없지만요",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100013704.1.25",
          "form": "무릎이 안 좋아서 흑흑 .. 근데 오르기 정돈 괜찮은 거 같아요",
          "original_form": "무릎이 안 좋아서 흑흑 .. 근데 오르기 정돈 괜찮은 거 같아요",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100013704.1.26",
          "form": "맞아요 무릎조심",
          "original_form": "맞아요 무릎조심",
          "speaker_id": "1",
