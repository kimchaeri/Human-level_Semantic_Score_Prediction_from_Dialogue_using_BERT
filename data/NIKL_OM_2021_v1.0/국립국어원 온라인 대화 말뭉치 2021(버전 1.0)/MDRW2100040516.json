      "utterance": [
        {
          "id": "MDRW2100040516.1.1",
          "form": "음 저는 캐나다여",
          "original_form": "음 저는 캐나다여",
          "speaker_id": "2",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100040516.1.2",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100040516.1.3",
          "form": "저는  미국쪽 가보고싶네요ㅜㅜ",
          "original_form": "저는  미국쪽 가보고싶네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100040516.1.4",
          "form": "아 저도 거기 가고싶어요",
          "original_form": "아 저도 거기 가고싶어요",
          "speaker_id": "2",
          "time": "20210905 12:58"
        },
        {
          "id": "MDRW2100040516.1.5",
          "form": "특히 크리스마스에여",
          "original_form": "특히 크리스마스에여",
          "speaker_id": "2",
          "time": "20210905 12:58"
        },
        {
          "id": "MDRW2100040516.1.6",
          "form": "마자요 분위기 너무 좋을것같아요",
          "original_form": "마자요 분위기 너무 좋을것같아요",
          "speaker_id": "1",
          "time": "20210905 17:47"
        },
        {
          "id": "MDRW2100040516.1.7",
          "form": "진짜요 ㅠㅠ",
          "original_form": "진짜요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 19:27"
        },
        {
          "id": "MDRW2100040516.1.8",
          "form": "가고싶어요 ㅠㅠ",
          "original_form": "가고싶어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 19:27"
        },
        {
          "id": "MDRW2100040516.1.9",
          "form": "현실은 집에서 캐롤을 듣겠죠ㅠㅠ",
          "original_form": "현실은 집에서 캐롤을 듣겠죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100040516.1.10",
          "form": "ㅋㅋㅋㅋㅋ 하",
          "original_form": "ㅋㅋㅋㅋㅋ 하",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100040516.1.11",
          "form": "슬프네요..",
          "original_form": "슬프네요..",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100040516.1.12",
          "form": "트리라도 사서 꾸밀까봐요ㅋㅋ",
          "original_form": "트리라도 사서 꾸밀까봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:30"
        },
        {
          "id": "MDRW2100040516.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ트리 좋죠!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ트리 좋죠!",
          "speaker_id": "2",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100040516.1.14",
          "form": "크리스마스느낌 낭낭..",
          "original_form": "크리스마스느낌 낭낭..",
          "speaker_id": "2",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100040516.1.15",
          "form": "어릴때 이후로 해본적이 없는것같아용",
          "original_form": "어릴때 이후로 해본적이 없는것같아용",
          "speaker_id": "1",
          "time": "20210905 19:40"
        },
        {
          "id": "MDRW2100040516.1.16",
          "form": "ㅋㅋㅋ 저도요",
          "original_form": "ㅋㅋㅋ 저도요",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100040516.1.17",
          "form": "커서는 한번듀..",
          "original_form": "커서는 한번듀..",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100040516.1.18",
          "form": "귀찮기듀하구요",
          "original_form": "귀찮기듀하구요",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100040516.1.19",
          "form": "마자용..귀차니즘..ㅜ 아주 미니사이즈로 사야겠어욬ㅋ",
          "original_form": "마자용..귀차니즘..ㅜ 아주 미니사이즈로 사야겠어욬ㅋ",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100040516.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ시작은 좋으나",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ시작은 좋으나",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100040516.1.21",
          "form": "그 후가..",
          "original_form": "그 후가..",
          "speaker_id": "2",
          "time": "20210905 20:45"
        }
      ]
    }
