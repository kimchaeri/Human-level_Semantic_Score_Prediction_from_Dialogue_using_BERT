      "utterance": [
        {
          "id": "MDRW2100011377.1.1",
          "form": "최근에 구매하신것있나요?",
          "original_form": "최근에 구매하신것있나요?",
          "speaker_id": "1",
          "time": "20210824 11:48"
        },
        {
          "id": "MDRW2100011377.1.2",
          "form": "걷기 기계 샀어요.",
          "original_form": "걷기 기계 샀어요.",
          "speaker_id": "2",
          "time": "20210824 11:51"
        },
        {
          "id": "MDRW2100011377.1.3",
          "form": "누누님은요?",
          "original_form": "누누님은요?",
          "speaker_id": "2",
          "time": "20210824 11:51"
        },
        {
          "id": "MDRW2100011377.1.4",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210824 12:10"
        },
        {
          "id": "MDRW2100011377.1.5",
          "form": "스텝퍼?같은건가요",
          "original_form": "스텝퍼?같은건가요",
          "speaker_id": "1",
          "time": "20210824 12:10"
        },
        {
          "id": "MDRW2100011377.1.6",
          "form": "핸드폰 운동시키는 기계요 ㅎ",
          "original_form": "핸드폰 운동시키는 기계요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 12:34"
        },
        {
          "id": "MDRW2100011377.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100011377.1.8",
          "form": "앱테크이군요?",
          "original_form": "앱테크이군요?",
          "speaker_id": "1",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100011377.1.9",
          "form": "글쵸 아시네요^^",
          "original_form": "글쵸 아시네요^^",
          "speaker_id": "2",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100011377.1.10",
          "form": "ㅋㅋㅋㅋ 귀엽네요",
          "original_form": "ㅋㅋㅋㅋ 귀엽네요",
          "speaker_id": "1",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100011377.1.11",
          "form": "다른 사시고싶으신건있나요?",
          "original_form": "다른 사시고싶으신건있나요?",
          "speaker_id": "1",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100011377.1.12",
          "form": "가을 옷이요. 긴팔",
          "original_form": "가을 옷이요. 긴팔",
          "speaker_id": "2",
          "time": "20210824 12:52"
        },
        {
          "id": "MDRW2100011377.1.13",
          "form": "아무래도 가을이 왔죠ㅋㅋㅋ",
          "original_form": "아무래도 가을이 왔죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:57"
        },
        {
          "id": "MDRW2100011377.1.14",
          "form": "저는 로퍼사고싶네요",
          "original_form": "저는 로퍼사고싶네요",
          "speaker_id": "1",
          "time": "20210824 12:57"
        },
        {
          "id": "MDRW2100011377.1.15",
          "form": "계절 바뀌면 신발도 바꿔 신어야죠",
          "original_form": "계절 바뀌면 신발도 바꿔 신어야죠",
          "speaker_id": "2",
          "time": "20210824 13:19"
        },
        {
          "id": "MDRW2100011377.1.16",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100011377.1.17",
          "form": "저는 운동화를 샀어요",
          "original_form": "저는 운동화를 샀어요",
          "speaker_id": "1",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100011377.1.18",
          "form": "저도 가을 신발 필요한데 ㅋ \r\n이것저것 살게 늘어나네요",
          "original_form": "저도 가을 신발 필요한데 ㅋ \r\n이것저것 살게 늘어나네요",
          "speaker_id": "2",
          "time": "20210824 14:29"
        },
        {
          "id": "MDRW2100011377.1.19",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:19"
