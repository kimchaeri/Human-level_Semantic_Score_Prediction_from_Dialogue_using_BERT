        },
        {
          "id": "MDRW2100010411.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210823 17:44"
        },
        {
          "id": "MDRW2100010411.1.3",
          "form": "반려동물키우세요?",
          "original_form": "반려동물키우세요?",
          "speaker_id": "2",
          "time": "20210823 17:46"
        },
        {
          "id": "MDRW2100010411.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210823 17:48"
        },
        {
          "id": "MDRW2100010411.1.5",
          "form": "키우시나요?",
          "original_form": "키우시나요?",
          "speaker_id": "1",
          "time": "20210823 17:48"
        },
        {
          "id": "MDRW2100010411.1.6",
          "form": "아뇨ㅠ 키우고싶은데 부담돼서 못키우죠",
          "original_form": "아뇨ㅠ 키우고싶은데 부담돼서 못키우죠",
          "speaker_id": "2",
          "time": "20210823 17:49"
        },
        {
          "id": "MDRW2100010411.1.7",
          "form": "보는건 좋아해요",
          "original_form": "보는건 좋아해요",
          "speaker_id": "2",
          "time": "20210823 17:49"
        },
        {
          "id": "MDRW2100010411.1.8",
          "form": "아 맞아요ㅠ",
          "original_form": "아 맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210823 17:50"
        },
        {
          "id": "MDRW2100010411.1.9",
          "form": "저는 키우고싶어요",
          "original_form": "저는 키우고싶어요",
          "speaker_id": "1",
          "time": "20210823 17:50"
        },
        {
          "id": "MDRW2100010411.1.10",
          "form": "어떤동물좋아하세요?",
          "original_form": "어떤동물좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 17:51"
        },
        {
          "id": "MDRW2100010411.1.11",
          "form": "저 강아지요ㅋㅋㅋ",
          "original_form": "저 강아지요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:36"
        },
        {
          "id": "MDRW2100010411.1.12",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210824 10:36"
        },
        {
          "id": "MDRW2100010411.1.13",
          "form": "저도 강아지요!",
          "original_form": "저도 강아지요!",
          "speaker_id": "2",
          "time": "20210824 10:37"
        },
        {
          "id": "MDRW2100010411.1.14",
          "form": "고양이도 사랑해요!!ㅋㅋ",
          "original_form": "고양이도 사랑해요!!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:37"
        },
        {
          "id": "MDRW2100010411.1.15",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:56"
        },
        {
          "id": "MDRW2100010411.1.16",
          "form": "고양이 강아지 다 너무좋아요",
          "original_form": "고양이 강아지 다 너무좋아요",
          "speaker_id": "1",
          "time": "20210824 10:56"
        },
        {
          "id": "MDRW2100010411.1.17",
          "form": "키울수 있으면 노란 치즈고양이를 키우고싶어요",
          "original_form": "키울수 있으면 노란 치즈고양이를 키우고싶어요",
          "speaker_id": "2",
          "time": "20210824 10:57"
        },
        {
          "id": "MDRW2100010411.1.18",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 10:57"
        },
        {
          "id": "MDRW2100010411.1.19",
          "form": "와우 치즈고양이좋죠",
          "original_form": "와우 치즈고양이좋죠",
          "speaker_id": "1",
          "time": "20210824 11:03"
        },
        {
          "id": "MDRW2100010411.1.20",
          "form": "저는 포메 키우고싶어요!",
          "original_form": "저는 포메 키우고싶어요!",
          "speaker_id": "1",
          "time": "20210824 11:03"
        },
        {
          "id": "MDRW2100010411.1.21",
          "form": "포메~넘 귀엽죠 동그랗게 털깎으면진짜귀엽잖아요ㅜ",
          "original_form": "포메~넘 귀엽죠 동그랗게 털깎으면진짜귀엽잖아요ㅜ",
          "speaker_id": "2",
          "time": "20210824 11:04"
        },
        {
          "id": "MDRW2100010411.1.22",
          "form": "맞아요ㅠㅜ진짜",
          "original_form": "맞아요ㅠㅜ진짜",
          "speaker_id": "1",
          "time": "20210824 11:09"
        },
        {
          "id": "MDRW2100010411.1.23",
          "form": "키우면 사랑일것같아요",
