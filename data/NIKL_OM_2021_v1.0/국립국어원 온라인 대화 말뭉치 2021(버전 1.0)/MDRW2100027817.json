      "utterance": [
        {
          "id": "MDRW2100027817.1.1",
          "form": "안녕하세요 좋아하는 과일있으신가용?",
          "original_form": "안녕하세요 좋아하는 과일있으신가용?",
          "speaker_id": "1",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027817.1.2",
          "form": "자두요",
          "original_form": "자두요",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027817.1.3",
          "form": "간단하게 먹을 수 있어서",
          "original_form": "간단하게 먹을 수 있어서",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027817.1.4",
          "form": "특히 더 좋아합니다.",
          "original_form": "특히 더 좋아합니다.",
          "speaker_id": "2",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027817.1.5",
          "form": "자두 새콤달콤맛있죠",
          "original_form": "자두 새콤달콤맛있죠",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027817.1.6",
          "form": "그 포도나 수박은 먹기 귀찮잖아요;;",
          "original_form": "그 포도나 수박은 먹기 귀찮잖아요;;",
          "speaker_id": "2",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027817.1.7",
          "form": "맛은 있긴 하지만...",
          "original_form": "맛은 있긴 하지만...",
          "speaker_id": "2",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027817.1.8",
          "form": "포도도 껍질째 먹는거는 좋아용",
          "original_form": "포도도 껍질째 먹는거는 좋아용",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027817.1.9",
          "form": "확실히 영양이 많다고는 하는데",
          "original_form": "확실히 영양이 많다고는 하는데",
          "speaker_id": "2",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027817.1.10",
          "form": "그 미묘하게 씁쓸한게 있는지라",
          "original_form": "그 미묘하게 씁쓸한게 있는지라",
          "speaker_id": "2",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027817.1.11",
          "form": "오 그쵸",
          "original_form": "오 그쵸",
          "speaker_id": "1",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027817.1.12",
          "form": "그걸 좋아아는 사람도 있다지만",
          "original_form": "그걸 좋아아는 사람도 있다지만",
          "speaker_id": "2",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027817.1.13",
          "form": "아 그런 의미에서",
          "original_form": "아 그런 의미에서",
          "speaker_id": "2",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027817.1.14",
          "form": "샤인머스캣인가",
          "original_form": "샤인머스캣인가",
          "speaker_id": "2",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027817.1.15",
          "form": "그런거나 청포도를 더 좋아하긴 합니다.",
          "original_form": "그런거나 청포도를 더 좋아하긴 합니다.",
          "speaker_id": "2",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027817.1.16",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027817.1.17",
          "form": "청포도 맛있죵",
          "original_form": "청포도 맛있죵",
          "speaker_id": "1",
          "time": "20210902 15:31"
        },
        {
          "id": "MDRW2100027817.1.18",
          "form": "옛날에 캔디로만 먹었는데",
          "original_form": "옛날에 캔디로만 먹었는데",
          "speaker_id": "2",
          "time": "20210902 15:31"
        },
        {
          "id": "MDRW2100027817.1.19",
          "form": "실제 과일도 맛있긴 하더라고요",
          "original_form": "실제 과일도 맛있긴 하더라고요",
          "speaker_id": "2",
          "time": "20210902 15:31"
        },
        {
          "id": "MDRW2100027817.1.20",
          "form": "뭐 좋아하시는 과일이...?",
          "original_form": "뭐 좋아하시는 과일이...?",
          "speaker_id": "2",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100027817.1.21",
          "form": "포도랑 복숭아 좋아해용",
          "original_form": "포도랑 복숭아 좋아해용",
          "speaker_id": "1",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100027817.1.22",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100027817.1.23",
          "form": "어쩌다보니 저도 모르게 포도를",
          "original_form": "어쩌다보니 저도 모르게 포도를",
          "speaker_id": "2",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100027817.1.24",
          "form": "아니 뭐 포도 싫어하진 않습니다",
          "original_form": "아니 뭐 포도 싫어하진 않습니다",
          "speaker_id": "2",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100027817.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ괜찮아요 저도 씁쓸한 맛은 안좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ괜찮아요 저도 씁쓸한 맛은 안좋아해요",
          "speaker_id": "1",
          "time": "20210902 15:34"
        }
      ]
