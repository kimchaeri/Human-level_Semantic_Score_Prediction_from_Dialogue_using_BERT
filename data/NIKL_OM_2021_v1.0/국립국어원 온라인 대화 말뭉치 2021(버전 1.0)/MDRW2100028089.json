      "utterance": [
        {
          "id": "MDRW2100028089.1.1",
          "form": "근데 군대는 왜",
          "original_form": "근데 군대는 왜",
          "speaker_id": "2",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100028089.1.2",
          "form": "안가셨나요",
          "original_form": "안가셨나요",
          "speaker_id": "2",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100028089.1.3",
          "form": "적성에 안맞나요?",
          "original_form": "적성에 안맞나요?",
          "speaker_id": "2",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100028089.1.4",
          "form": "안보내주든데요",
          "original_form": "안보내주든데요",
          "speaker_id": "1",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100028089.1.5",
          "form": "저도가고싶은데",
          "original_form": "저도가고싶은데",
          "speaker_id": "1",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100028089.1.6",
          "form": "자격요건미달",
          "original_form": "자격요건미달",
          "speaker_id": "1",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100028089.1.7",
          "form": "아 아쉽겠습니다",
          "original_form": "아 아쉽겠습니다",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028089.1.8",
          "form": "인생에서 보면 참 귀중한 시간이었다고 생각됩니다",
          "original_form": "인생에서 보면 참 귀중한 시간이었다고 생각됩니다",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028089.1.9",
          "form": "크.. 언제가셨어요 대학 1학년? 2학년?",
          "original_form": "크.. 언제가셨어요 대학 1학년? 2학년?",
          "speaker_id": "1",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028089.1.10",
          "form": "대학교 1학년하고 갓네요",
          "original_form": "대학교 1학년하고 갓네요",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028089.1.11",
          "form": "지금으로부터 16년전입니다",
          "original_form": "지금으로부터 16년전입니다",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028089.1.12",
          "form": "와웅",
          "original_form": "와웅",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028089.1.13",
          "form": "길었을때군요",
          "original_form": "길었을때군요",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028089.1.14",
          "form": "요즘 아이들 눈뜨면 전역하던데",
          "original_form": "요즘 아이들 눈뜨면 전역하던데",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028089.1.15",
          "form": "안에 있으면 시간이 너무 안가서 돌아버립니다 ㅋㅋ",
          "original_form": "안에 있으면 시간이 너무 안가서 돌아버립니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100028089.1.16",
          "form": "ㅋㅋㅋㅋ요즘은 폰도 쓰고",
          "original_form": "ㅋㅋㅋㅋ요즘은 폰도 쓰고",
          "speaker_id": "1",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100028089.1.17",
          "form": "갈만하겟더라구요",
          "original_form": "갈만하겟더라구요",
          "speaker_id": "1",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100028089.1.18",
          "form": "폰도 쓰나요?",
          "original_form": "폰도 쓰나요?",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100028089.1.19",
          "form": "폰을 왜?",
          "original_form": "폰을 왜?",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
