      "utterance": [
        {
          "id": "MDRW2100035659.1.1",
          "form": "아뇨ㅠ",
          "original_form": "아뇨ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100035659.1.2",
          "form": "네페쓰세요?",
          "original_form": "네페쓰세요?",
          "speaker_id": "2",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100035659.1.3",
          "form": "저는 자주 사용해요",
          "original_form": "저는 자주 사용해요",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100035659.1.4",
          "form": "혜택이좋다던데",
          "original_form": "혜택이좋다던데",
          "speaker_id": "2",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100035659.1.5",
          "form": "왜안쓰게되는지 ,,",
          "original_form": "왜안쓰게되는지 ,,",
          "speaker_id": "2",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100035659.1.6",
          "form": "ㅋㅋㅋㅋ 포인트가 쌓이긴 하지만 안 맞을 수도 있으니까요ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 포인트가 쌓이긴 하지만 안 맞을 수도 있으니까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035659.1.7",
          "form": "구매는 대부분 네이버페이쓰시는거에요??",
          "original_form": "구매는 대부분 네이버페이쓰시는거에요??",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100035659.1.8",
          "form": "네네! 6만원쯤 구매하면 보통 몇 천원은 적립받을 수 있어서요",
          "original_form": "네네! 6만원쯤 구매하면 보통 몇 천원은 적립받을 수 있어서요",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035659.1.9",
          "form": "우와 적립많이해주네요",
          "original_form": "우와 적립많이해주네요",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035659.1.10",
          "form": "최근에 사신건 뭐에요?!",
          "original_form": "최근에 사신건 뭐에요?!",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035659.1.11",
          "form": "알코올 스왑이랑 무드등이요!",
          "original_form": "알코올 스왑이랑 무드등이요!",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035659.1.12",
          "form": "무드등도사셨군요!!",
          "original_form": "무드등도사셨군요!!",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035659.1.13",
          "form": "선물용으로요ㅎㅎ",
          "original_form": "선물용으로요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035659.1.14",
          "form": "무드등은 자기 직전까지 켜놓는건가요? 켜놓고자는건가요?",
          "original_form": "무드등은 자기 직전까지 켜놓는건가요? 켜놓고자는건가요?",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035659.1.15",
          "form": "앗 선물용ㅋㄱㅋㅋ",
          "original_form": "앗 선물용ㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035659.1.16",
          "form": "켜 놓고 자기도 하고 원하는 대로 하지 않을까요?",
          "original_form": "켜 놓고 자기도 하고 원하는 대로 하지 않을까요?",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
