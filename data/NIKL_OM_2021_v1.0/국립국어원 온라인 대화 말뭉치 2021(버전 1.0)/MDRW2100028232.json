      "utterance": [
        {
          "id": "MDRW2100028232.1.1",
          "form": "자몽피지오 좋아하시나요?ㅋㅋㅋㅋ",
          "original_form": "자몽피지오 좋아하시나요?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100028232.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100028232.1.3",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100028232.1.4",
          "form": "네 올 여름에 먹어봣어요",
          "original_form": "네 올 여름에 먹어봣어요",
          "speaker_id": "1",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100028232.1.5",
          "form": "자몽을 원래 좋아해서ㅋㅋㅋㅋ",
          "original_form": "자몽을 원래 좋아해서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100028232.1.6",
          "form": "저도요ㅎㅎ",
          "original_form": "저도요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100028232.1.7",
          "form": "개인적으론 피지오보다 맛있었어요",
          "original_form": "개인적으론 피지오보다 맛있었어요",
          "speaker_id": "1",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100028232.1.8",
          "form": "근데 어느순간부터 없어졌더라구요...",
          "original_form": "근데 어느순간부터 없어졌더라구요...",
          "speaker_id": "2",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100028232.1.9",
          "form": "너무슬퍼요",
          "original_form": "너무슬퍼요",
          "speaker_id": "2",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100028232.1.10",
          "form": "그니까요.. ㅠㅠ 스벅이 잘 그러긴하지만요.. 내년에 부활을 외쳐봅니다",
          "original_form": "그니까요.. ㅠㅠ 스벅이 잘 그러긴하지만요.. 내년에 부활을 외쳐봅니다",
          "speaker_id": "1",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100028232.1.11",
          "form": "그래서 요즘은 라임피지오마시고있어요ㅎㅎ",
          "original_form": "그래서 요즘은 라임피지오마시고있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100028232.1.12",
          "form": "name1님은 뭐좋어허세요?",
          "original_form": "&name1&님은 뭐좋어허세요?",
          "speaker_id": "2",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100028232.1.13",
          "form": "저는 자허블이요! 자몽허니블랙티요ㅋㅋㅋㅋ",
          "original_form": "저는 자허블이요! 자몽허니블랙티요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028232.1.14",
          "form": "스벅에서 커피안마실때는요 ㅋㅋ\r\n차갑게 먹어도 따뜻하게 먹어도 좋더라구요",
          "original_form": "스벅에서 커피안마실때는요 ㅋㅋ\r\n차갑게 먹어도 따뜻하게 먹어도 좋더라구요",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028232.1.15",
          "form": "맞아요 자허블 맛있죠",
          "original_form": "맞아요 자허블 맛있죠",
          "speaker_id": "2",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100028232.1.16",
          "form": "커피는 잘 안드세요?",
          "original_form": "커피는 잘 안드세요?",
          "speaker_id": "2",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100028232.1.17",
          "form": "아니오 잘마셔요 ㅎㅎ",
          "original_form": "아니오 잘마셔요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028232.1.18",
          "form": "따뜻한 아메리카노나 라떼도 자주 마시는데\r\n스벅에서 커피마시는 건 왠지 아쉬워서 ㅋㅋㅋㅋ",
          "original_form": "따뜻한 아메리카노나 라떼도 자주 마시는데\r\n스벅에서 커피마시는 건 왠지 아쉬워서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028232.1.19",
          "form": "아 알거같아요ㅎㅎㅎ",
          "original_form": "아 알거같아요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028232.1.20",
          "form": "일부러 콜드브루마시고 그래요",
          "original_form": "일부러 콜드브루마시고 그래요",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028232.1.21",
          "form": "콜드브루도 마시긴해요ㅎㅎ 따뜻하게가 안되서 그렇죠 ㅎㅎ",
          "original_form": "콜드브루도 마시긴해요ㅎㅎ 따뜻하게가 안되서 그렇죠 ㅎㅎ",
