      "utterance": [
        {
          "id": "MDRW2100023441.1.1",
          "form": "먹지않을까? 깨어있다면",
          "original_form": "먹지않을까? 깨어있다면",
          "speaker_id": "2",
          "time": "20210830 02:40"
        },
        {
          "id": "MDRW2100023441.1.2",
          "form": "ㅎㅎ나도 일찍 일나서 아침밥 먹고싶당",
          "original_form": "ㅎㅎ나도 일찍 일나서 아침밥 먹고싶당",
          "speaker_id": "1",
          "time": "20210830 02:59"
        },
        {
          "id": "MDRW2100023441.1.3",
          "form": "아침밥 먹었어??",
          "original_form": "아침밥 먹었어??",
          "speaker_id": "2",
          "time": "20210830 11:09"
        },
        {
          "id": "MDRW2100023441.1.4",
          "form": "불고기샐러드랑 씨리얼ㅎ",
          "original_form": "불고기샐러드랑 씨리얼ㅎ",
          "speaker_id": "1",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100023441.1.5",
          "form": "헐 맛있었겠다 ㅋㅋ",
          "original_form": "헐 맛있었겠다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100023441.1.6",
          "form": "인자 곧 점심시간",
          "original_form": "인자 곧 점심시간",
          "speaker_id": "1",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100023441.1.7",
          "form": "응 샐러드 맛있더라",
          "original_form": "응 샐러드 맛있더라",
          "speaker_id": "1",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100023441.1.8",
          "form": "드레싱없어도 불고기가 짭조롬이라 괜찮았어",
          "original_form": "드레싱없어도 불고기가 짭조롬이라 괜찮았어",
          "speaker_id": "1",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100023441.1.9",
          "form": "나도불고기샐러드 먹어보고싶당!",
          "original_form": "나도불고기샐러드 먹어보고싶당!",
          "speaker_id": "2",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100023441.1.10",
          "form": "근데 드레싱 없었구나",
          "original_form": "근데 드레싱 없었구나",
          "speaker_id": "2",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100023441.1.11",
          "form": "있는데 내가 안먹었어\n난 안먹어도 괜찮더라고ㅎ",
          "original_form": "있는데 내가 안먹었어\n난 안먹어도 괜찮더라고ㅎ",
          "speaker_id": "1",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100023441.1.12",
          "form": "아 맞나 ㅋㅋ 암튼 맛있었게따",
          "original_form": "아 맞나 ㅋㅋ 암튼 맛있었게따",
          "speaker_id": "2",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100023441.1.13",
          "form": "난 밥 고추참치 김 김치 이렇게 먹었네",
          "original_form": "난 밥 고추참치 김 김치 이렇게 먹었네",
          "speaker_id": "2",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023441.1.14",
          "form": "김치랑 김 맛있디ㅋㅋ",
          "original_form": "김치랑 김 맛있디ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:10"
        },
        {
          "id": "MDRW2100023441.1.15",
          "form": "점심 먹었어?",
          "original_form": "점심 먹었어?",
          "speaker_id": "1",
          "time": "20210830 12:11"
        },
        {
          "id": "MDRW2100023441.1.16",
          "form": "아침 늦게 먹어서 엄빠 신라면볶음면 먹을때 꼽사리로 조금먹음",
          "original_form": "아침 늦게 먹어서 엄빠 신라면볶음면 먹을때 꼽사리로 조금먹음",
          "speaker_id": "2",
          "time": "20210830 12:11"
        },
        {
          "id": "MDRW2100023441.1.17",
          "form": "난 아침 늦께먹고는 점심도 잘먹어버림ㅋ\n뜨끈하게 나와서 디기 잘먹었네",
          "original_form": "난 아침 늦께먹고는 점심도 잘먹어버림ㅋ\n뜨끈하게 나와서 디기 잘먹었네",
          "speaker_id": "1",
          "time": "20210830 12:12"
        },
        {
          "id": "MDRW2100023441.1.18",
          "form": "김치전, 순대국밥 먹었어ㅎ",
          "original_form": "김치전, 순대국밥 먹었어ㅎ",
          "speaker_id": "1",
          "time": "20210830 12:12"
        },
        {
          "id": "MDRW2100023441.1.19",
          "form": "와 니가 좋아하는거네",
          "original_form": "와 니가 좋아하는거네",
          "speaker_id": "2",
          "time": "20210830 12:13"
        },
        {
          "id": "MDRW2100023441.1.20",
          "form": "근데 며칠전에도 순대국밥 나오지 않음?",
          "original_form": "근데 며칠전에도 순대국밥 나오지 않음?",
          "speaker_id": "2",
          "time": "20210830 12:13"
        },
        {
          "id": "MDRW2100023441.1.21",
          "form": "응 저녁에 먹음\n오늘점심도 나왔던데 조금 다른게\n저번 저녁땐 들깨가루 줬는데\n오늘 점심땐 없더라ㅎ",
          "original_form": "응 저녁에 먹음\n오늘점심도 나왔던데 조금 다른게\n저번 저녁땐 들깨가루 줬는데\n오늘 점심땐 없더라ㅎ",
          "speaker_id": "1",
          "time": "20210830 12:29"
        },
        {
          "id": "MDRW2100023441.1.22",
          "form": "아 그렇구나 순대가 남았나?",
          "original_form": "아 그렇구나 순대가 남았나?",
          "speaker_id": "2",
          "time": "20210830 12:42"
        },
        {
          "id": "MDRW2100023441.1.23",
          "form": "남아? 회사식당에서 남아서 또 나오냐고?",
          "original_form": "남아? 회사식당에서 남아서 또 나오냐고?",
          "speaker_id": "1",
          "time": "20210830 12:43"
        },
        {
          "id": "MDRW2100023441.1.24",
          "form": "사놓은 순대가 많아서 소진하려고 그런거같은데",
          "original_form": "사놓은 순대가 많아서 소진하려고 그런거같은데",
          "speaker_id": "2",
          "time": "20210830 12:45"
        },
        {
          "id": "MDRW2100023441.1.25",
          "form": "너무 짧은시간에 순대국이 나와서 ㅋㅋ",
          "original_form": "너무 짧은시간에 순대국이 나와서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:45"
        },
        {
          "id": "MDRW2100023441.1.26",
          "form": "조리한 순대가 남은게아니고 사놓은순대",
          "original_form": "조리한 순대가 남은게아니고 사놓은순대",
          "speaker_id": "2",
          "time": "20210830 12:45"
        },
        {
          "id": "MDRW2100023441.1.27",
          "form": "아~그러지 모르겠다ㅎㅎ",
          "original_form": "아~그러지 모르겠다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 12:47"
        },
        {
          "id": "MDRW2100023441.1.28",
          "form": "오늘 맛 괜찮더라ㅎ",
          "original_form": "오늘 맛 괜찮더라ㅎ",
          "speaker_id": "1",
          "time": "20210830 12:47"
        },
        {
          "id": "MDRW2100023441.1.29",
          "form": "바베큐 넘 맛나보여서 구입함",
          "original_form": "바베큐 넘 맛나보여서 구입함",
          "speaker_id": "2",
          "time": "20210830 12:48"
        }
      ]
    }
