      "utterance": [
        {
          "id": "MDRW2100002246.1.1",
          "form": "여행계획있니?ㅋㅋ",
          "original_form": "여행계획있니?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 01:18"
        },
        {
          "id": "MDRW2100002246.1.2",
          "form": "10월에 담양갈까 생각중임ㅋㅋ",
          "original_form": "10월에 담양갈까 생각중임ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:19"
        },
        {
          "id": "MDRW2100002246.1.3",
          "form": "오 담양 ㅋㅋ 담양에는 뭐있지? 나는 여수 가볼라하는데 ㅋㅋ",
          "original_form": "오 담양 ㅋㅋ 담양에는 뭐있지? 나는 여수 가볼라하는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 01:19"
        },
        {
          "id": "MDRW2100002246.1.4",
          "form": "대나무 숲이 있어!",
          "original_form": "대나무 숲이 있어!",
          "speaker_id": "2",
          "time": "20210820 01:19"
        },
        {
          "id": "MDRW2100002246.1.5",
          "form": "나 여수 한번도 안 가봄ㅋㅋ",
          "original_form": "나 여수 한번도 안 가봄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002246.1.6",
          "form": "글쿠나 ㅋㅋㅋ 나도 첨가보ㅓ",
          "original_form": "글쿠나 ㅋㅋㅋ 나도 첨가보ㅓ",
          "speaker_id": "1",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002246.1.7",
          "form": "첨가봐",
          "original_form": "첨가봐",
          "speaker_id": "1",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002246.1.8",
          "form": "여수에는 밤바다있겠지..?ㅋㅋㅋㅋ",
          "original_form": "여수에는 밤바다있겠지..?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 01:21"
        },
        {
          "id": "MDRW2100002246.1.9",
          "form": "ㅋㅋㅋㅋㅋ아 노래가 그렇게 유명한데 있겠지ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아 노래가 그렇게 유명한데 있겠지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:21"
        },
        {
          "id": "MDRW2100002246.1.10",
          "form": "바다 얘기하니까 강원도 가고싶어진다ㅜ",
          "original_form": "바다 얘기하니까 강원도 가고싶어진다ㅜ",
          "speaker_id": "2",
          "time": "20210820 01:22"
        },
        {
          "id": "MDRW2100002246.1.11",
          "form": "맞아 ㅋㅋㅋㅋ 게장도 유명한거같더라 기대돼",
          "original_form": "맞아 ㅋㅋㅋㅋ 게장도 유명한거같더라 기대돼",
          "speaker_id": "1",
          "time": "20210820 01:22"
        },
        {
          "id": "MDRW2100002246.1.12",
          "form": "아 동해바다 좋아하는구나",
          "original_form": "아 동해바다 좋아하는구나",
          "speaker_id": "1",
          "time": "20210820 01:22"
        },
        {
          "id": "MDRW2100002246.1.13",
          "form": "맞아 게장 꼭 먹고와 친구들이 맛있댔음ㅋㅋㅋ",
          "original_form": "맞아 게장 꼭 먹고와 친구들이 맛있댔음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:23"
        },
        {
          "id": "MDRW2100002246.1.14",
          "form": "응 전에 정동진 간 적 있는데 엄청 좋았어ㅋㅋ",
          "original_form": "응 전에 정동진 간 적 있는데 엄청 좋았어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:23"
        },
        {
          "id": "MDRW2100002246.1.15",
          "form": "ㅋㅋㅋㅋ정동진이면 해돋이보고왔겠네",
          "original_form": "ㅋㅋㅋㅋ정동진이면 해돋이보고왔겠네",
          "speaker_id": "1",
          "time": "20210820 01:24"
        },
        {
          "id": "MDRW2100002246.1.16",
          "form": "좋았겠다 ㅠㅠㅋㅋㅋ",
          "original_form": "좋았겠다 ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 01:24"
        },
        {
          "id": "MDRW2100002246.1.17",
          "form": "엉 진짜 엄청 좋았어ㅠㅜ겨울에 갔는데도 해돋이 보겠다고 새벽부터 나가있었어ㅋㅋ",
          "original_form": "엉 진짜 엄청 좋았어ㅠㅜ겨울에 갔는데도 해돋이 보겠다고 새벽부터 나가있었어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:24"
        },
        {
          "id": "MDRW2100002246.1.18",
          "form": "ㅋㅋㅋㅋㅋ안추웠나",
          "original_form": "ㅋㅋㅋㅋㅋ안추웠나",
          "speaker_id": "1",
          "time": "20210820 01:25"
        },
        {
          "id": "MDRW2100002246.1.19",
          "form": "그래도 해돋이보면 뭔가 탁트이고 기분되게 좋았겠다",
          "original_form": "그래도 해돋이보면 뭔가 탁트이고 기분되게 좋았겠다",
          "speaker_id": "1",
          "time": "20210820 01:25"
        },
        {
          "id": "MDRW2100002246.1.20",
          "form": "응 조금 감동했음ㅋㅋㅋ",
          "original_form": "응 조금 감동했음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:26"
        },
        {
          "id": "MDRW2100002246.1.21",
          "form": "ㅋㅋㅋㅋ정동진에서 보면 뭔가 다른가?",
          "original_form": "ㅋㅋㅋㅋ정동진에서 보면 뭔가 다른가?",
          "speaker_id": "1",
          "time": "20210820 01:27"
        },
        {
          "id": "MDRW2100002246.1.22",
          "form": "나도 가보고싶네 ㅠㅠㅋㅋㅋ",
          "original_form": "나도 가보고싶네 ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 01:27"
        },
        {
          "id": "MDRW2100002246.1.23",
          "form": "걍,,,똑같아 바다위로 해뜨는 게 보일 뿐ㅋㅋㅋ",
          "original_form": "걍,,,똑같아 바다위로 해뜨는 게 보일 뿐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:28"
        },
        {
          "id": "MDRW2100002246.1.24",
          "form": "그냥 그날 하루 한국서 젤 먼저 해뜨는 걸 보는 게 다른것뿐ㅋㅋㄲ",
          "original_form": "그냥 그날 하루 한국서 젤 먼저 해뜨는 걸 보는 게 다른것뿐ㅋㅋㄲ",
          "speaker_id": "2",
