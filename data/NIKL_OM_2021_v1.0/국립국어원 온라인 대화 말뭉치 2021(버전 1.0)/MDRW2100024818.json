      "utterance": [
        {
          "id": "MDRW2100024818.1.1",
          "form": "저도 마라탕좋아해요!!",
          "original_form": "저도 마라탕좋아해요!!",
          "speaker_id": "1",
          "time": "20210831 19:07"
        },
        {
          "id": "MDRW2100024818.1.2",
          "form": "마라탕 맛있어요 저도 좋아해요",
          "original_form": "마라탕 맛있어요 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210831 19:12"
        },
        {
          "id": "MDRW2100024818.1.3",
          "form": "마라탕에 뭐 넣는 거 좋아하시나요",
          "original_form": "마라탕에 뭐 넣는 거 좋아하시나요",
          "speaker_id": "2",
          "time": "20210831 19:12"
        },
        {
          "id": "MDRW2100024818.1.4",
          "form": "숙주",
          "original_form": "숙주",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024818.1.5",
          "form": "팽이버섯",
          "original_form": "팽이버섯",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024818.1.6",
          "form": "중국당면",
          "original_form": "중국당면",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024818.1.7",
          "form": "분모자",
          "original_form": "분모자",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024818.1.8",
          "form": "고구마",
          "original_form": "고구마",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024818.1.9",
          "form": "이정도 생각나요ㅋㅋㅋ",
          "original_form": "이정도 생각나요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024818.1.10",
          "form": "하나같이 다 맛있는 것들이네요",
          "original_form": "하나같이 다 맛있는 것들이네요",
          "speaker_id": "2",
          "time": "20210831 19:14"
        },
        {
          "id": "MDRW2100024818.1.11",
          "form": "전 중국당면 엄청 좋아해요 ㅎㅎ",
          "original_form": "전 중국당면 엄청 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 19:14"
        },
        {
          "id": "MDRW2100024818.1.12",
          "form": "버섯도 맛있고요",
          "original_form": "버섯도 맛있고요",
          "speaker_id": "2",
          "time": "20210831 19:14"
        },
        {
          "id": "MDRW2100024818.1.13",
          "form": "엄청 쫀득해요 중국당면ㅋㅋㅋ",
          "original_form": "엄청 쫀득해요 중국당면ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:15"
        },
        {
          "id": "MDRW2100024818.1.14",
          "form": "요즘은 배달을 많이 시켜먹어용",
          "original_form": "요즘은 배달을 많이 시켜먹어용",
          "speaker_id": "2",
          "time": "20210831 19:15"
        },
        {
          "id": "MDRW2100024818.1.15",
          "form": "아 그리고 전 소고기추가 꼭 해요",
          "original_form": "아 그리고 전 소고기추가 꼭 해요",
          "speaker_id": "1",
          "time": "20210831 19:15"
        },
        {
          "id": "MDRW2100024818.1.16",
          "form": "오오 배달도 재료선택 가능해요?",
          "original_form": "오오 배달도 재료선택 가능해요?",
          "speaker_id": "1",
          "time": "20210831 19:15"
        },
        {
          "id": "MDRW2100024818.1.17",
          "form": "그냥 기본적인  거 오는데 제가 버섯이랑 청경채 정도 따로 사서 넣어요ㅎㅎ",
          "original_form": "그냥 기본적인  거 오는데 제가 버섯이랑 청경채 정도 따로 사서 넣어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100024818.1.18",
          "form": "매운 거는 잘 드시나요",
          "original_form": "매운 거는 잘 드시나요",
          "speaker_id": "2",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100024818.1.19",
          "form": "전 항상 얼큰 정도로 먹어요ㅎㅎ",
          "original_form": "전 항상 얼큰 정도로 먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100024818.1.20",
          "form": "네네 매운거좋아해요",
          "original_form": "네네 매운거좋아해요",
          "speaker_id": "1",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100024818.1.21",
          "form": "가끔 위랑 장이 감당못하기도해요ㅋㅋ",
          "original_form": "가끔 위랑 장이 감당못하기도해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100024818.1.22",
          "form": "전 맵찔이라 겸손하게 가장 안 맵게 먹어용ㅋㄱㅂ",
          "original_form": "전 맵찔이라 겸손하게 가장 안 맵게 먹어용ㅋㄱㅂ",
          "speaker_id": "2",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100024818.1.23",
          "form": "맵게 먹고 항상 아이스크림으로 마무리해요",
          "original_form": "맵게 먹고 항상 아이스크림으로 마무리해요",
          "speaker_id": "2",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100024818.1.24",
          "form": "아이스크림ㅜㅜ 맛있겠다",
          "original_form": "아이스크림ㅜㅜ 맛있겠다",
          "speaker_id": "1",
          "time": "20210831 19:23"
        },
        {
          "id": "MDRW2100024818.1.25",
          "form": "완벽한 마무리네요",
          "original_form": "완벽한 마무리네요",
          "speaker_id": "1",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100024818.1.26",
          "form": "매운 거 먹고 아이스크림으로 내려주면 딱이에요",
          "original_form": "매운 거 먹고 아이스크림으로 내려주면 딱이에요",
          "speaker_id": "2",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100024818.1.27",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100024818.1.28",
          "form": "아니요 아직요ㅜㅠ 배고파요",
          "original_form": "아니요 아직요ㅜㅠ 배고파요",
          "speaker_id": "1",
