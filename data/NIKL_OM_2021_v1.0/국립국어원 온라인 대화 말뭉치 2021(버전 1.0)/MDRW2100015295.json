      "utterance": [
        {
          "id": "MDRW2100015295.1.1",
          "form": "오오 쇼핑 좀 해야겠네",
          "original_form": "오오 쇼핑 좀 해야겠네",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015295.1.2",
          "form": "그걸로 작은 아들 문제집 사려고",
          "original_form": "그걸로 작은 아들 문제집 사려고",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015295.1.3",
          "form": "ㅎㅎ 좋으넹",
          "original_form": "ㅎㅎ 좋으넹",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015295.1.4",
          "form": "네이버페이 모으는 재미가 쏠쏠해",
          "original_form": "네이버페이 모으는 재미가 쏠쏠해",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015295.1.5",
          "form": "요즘 이벤트로 많이 모이나보네",
          "original_form": "요즘 이벤트로 많이 모이나보네",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015295.1.6",
          "form": "ㅎㅎ 그냥 이것저것 참여하고 누르다 보면",
          "original_form": "ㅎㅎ 그냥 이것저것 참여하고 누르다 보면",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015295.1.7",
          "form": "물건 사고 상품평 남겨도 주고",
          "original_form": "물건 사고 상품평 남겨도 주고",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015295.1.8",
          "form": "온라인 쇼핑할 때는 사용하기 편하잖아",
          "original_form": "온라인 쇼핑할 때는 사용하기 편하잖아",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015295.1.9",
          "form": "글치 ㅎㅎ 아까 말한 연산 문제집 사야겠어",
          "original_form": "글치 ㅎㅎ 아까 말한 연산 문제집 사야겠어",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015295.1.10",
          "form": "연산 어떤 걸로 사게?",
          "original_form": "연산 어떤 걸로 사게?",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015295.1.11",
          "form": "좀 알아보니 빅터 연산 많이 하더라고",
          "original_form": "좀 알아보니 빅터 연산 많이 하더라고",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015295.1.12",
          "form": "들어본 것 같아",
          "original_form": "들어본 것 같아",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015295.1.13",
          "form": "문제도 지겹지 않고 구성이 괜찮은가봐 ㅎㅎ",
          "original_form": "문제도 지겹지 않고 구성이 괜찮은가봐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015295.1.14",
          "form": "ㅎㅎ 그거 사면 되겠네",
          "original_form": "ㅎㅎ 그거 사면 되겠네",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015295.1.15",
