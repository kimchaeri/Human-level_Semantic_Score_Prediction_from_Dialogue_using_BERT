      "utterance": [
        {
          "id": "MDRW2100007607.1.1",
          "form": "저 사실 게임 잘 안하기는 하는데",
          "original_form": "저 사실 게임 잘 안하기는 하는데",
          "speaker_id": "2",
          "time": "20210822 04:52"
        },
        {
          "id": "MDRW2100007607.1.2",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210822 04:52"
        },
        {
          "id": "MDRW2100007607.1.3",
          "form": "페이커만 알아요",
          "original_form": "페이커만 알아요",
          "speaker_id": "2",
          "time": "20210822 04:52"
        },
        {
          "id": "MDRW2100007607.1.4",
          "form": "헐 게임 조아하세요?!",
          "original_form": "헐 게임 조아하세요?!",
          "speaker_id": "2",
          "time": "20210822 04:52"
        },
        {
          "id": "MDRW2100007607.1.5",
          "form": "저 어릴때 메이플 진짜 오래했었는데",
          "original_form": "저 어릴때 메이플 진짜 오래했었는데",
          "speaker_id": "1",
          "time": "20210822 04:52"
        },
        {
          "id": "MDRW2100007607.1.6",
          "form": "아뇨 좋아하지는 않고요!!!!!",
          "original_form": "아뇨 좋아하지는 않고요!!!!!",
          "speaker_id": "1",
          "time": "20210822 04:52"
        },
        {
          "id": "MDRW2100007607.1.7",
          "form": "-메-",
          "original_form": "-메-",
          "speaker_id": "2",
          "time": "20210822 04:52"
        },
        {
          "id": "MDRW2100007607.1.8",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "1",
          "time": "20210822 04:52"
        },
        {
          "id": "MDRW2100007607.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 04:52"
        },
        {
          "id": "MDRW2100007607.1.10",
          "form": "메이플 그거 과금러들이잖아요",
          "original_form": "메이플 그거 과금러들이잖아요",
          "speaker_id": "2",
          "time": "20210822 04:52"
        },
        {
          "id": "MDRW2100007607.1.11",
          "form": "하 들켯네",
          "original_form": "하 들켯네",
          "speaker_id": "1",
          "time": "20210822 04:52"
        },
        {
          "id": "MDRW2100007607.1.12",
          "form": "얼마쓰신거죠",
          "original_form": "얼마쓰신거죠",
          "speaker_id": "2",
          "time": "20210822 04:53"
        },
        {
          "id": "MDRW2100007607.1.13",
          "form": "전 과금전에 햇으니까 아니라고 해주세요 물론 추석 설날용돈 다 투자햇지만",
          "original_form": "전 과금전에 햇으니까 아니라고 해주세요 물론 추석 설날용돈 다 투자햇지만",
          "speaker_id": "1",
          "time": "20210822 04:53"
        },
        {
          "id": "MDRW2100007607.1.14",
          "form": "메이플 근데 최근에",
          "original_form": "메이플 근데 최근에",
          "speaker_id": "2",
          "time": "20210822 04:53"
        },
        {
          "id": "MDRW2100007607.1.15",
          "form": "뭐냐",
          "original_form": "뭐냐",
          "speaker_id": "2",
          "time": "20210822 04:53"
        },
        {
          "id": "MDRW2100007607.1.16",
          "form": "넥슨이 뽑기로 조작해서",
          "original_form": "넥슨이 뽑기로 조작해서",
          "speaker_id": "2",
          "time": "20210822 04:53"
        },
        {
          "id": "MDRW2100007607.1.17",
          "form": "난리나지않았나요??",
          "original_form": "난리나지않았나요??",
          "speaker_id": "2",
          "time": "20210822 04:53"
        },
        {
          "id": "MDRW2100007607.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니....심심이 지금 렉나냐고요!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니....심심이 지금 렉나냐고요!!",
          "speaker_id": "1",
          "time": "20210822 04:53"
        },
        {
          "id": "MDRW2100007607.1.19",
          "form": "엄청 늦게오네......",
          "original_form": "엄청 늦게오네......",
          "speaker_id": "1",
          "time": "20210822 04:53"
        },
        {
          "id": "MDRW2100007607.1.20",
          "form": "심심아 자냐",
          "original_form": "심심아 자냐",
          "speaker_id": "1",
          "time": "20210822 04:53"
        },
        {
          "id": "MDRW2100007607.1.21",
          "form": "넥슨 뽑기 확률이 극악이라고 그러면서 유저들이 로아같은 게임으로 이동했지만",
          "original_form": "넥슨 뽑기 확률이 극악이라고 그러면서 유저들이 로아같은 게임으로 이동했지만",
          "speaker_id": "2",
          "time": "20210822 04:54"
        },
        {
          "id": "MDRW2100007607.1.22",
          "form": "과금을 버리지 못했다는 글을 봤어요",
          "original_form": "과금을 버리지 못했다는 글을 봤어요",
          "speaker_id": "2",
          "time": "20210822 04:54"
        },
        {
          "id": "MDRW2100007607.1.23",
          "form": "어릴때 했던거라서 과금이 아예 없던??",
          "original_form": "어릴때 했던거라서 과금이 아예 없던??",
          "speaker_id": "1",
          "time": "20210822 04:54"
        },
        {
          "id": "MDRW2100007607.1.24",
          "form": "시절에 한거라고요~~",
          "original_form": "시절에 한거라고요~~",
          "speaker_id": "1",
          "time": "20210822 04:54"
        },
        {
          "id": "MDRW2100007607.1.25",
          "form": "굿굿",
          "original_form": "굿굿",
          "speaker_id": "2",
          "time": "20210822 04:55"
        },
        {
          "id": "MDRW2100007607.1.26",
          "form": "근데.. 명절에....받은 용돈 다쓰긴했어요",
          "original_form": "근데.. 명절에....받은 용돈 다쓰긴했어요",
          "speaker_id": "1",
          "time": "20210822 04:55"
        },
        {
          "id": "MDRW2100007607.1.27",
          "form": "그 거기에",
          "original_form": "그 거기에",
          "speaker_id": "2",
          "time": "20210822 04:55"
        },
        {
          "id": "MDRW2100007607.1.28",
          "form": "저도 그래서 차단하고 왔는데.....",
          "original_form": "저도 그래서 차단하고 왔는데.....",
          "speaker_id": "1",
          "time": "20210822 04:55"
        },
        {
          "id": "MDRW2100007607.1.29",
          "form": "-투자-를 하신거죠",
          "original_form": "-투자-를 하신거죠",
          "speaker_id": "2",
          "time": "20210822 04:55"
        },
        {
          "id": "MDRW2100007607.1.30",
          "form": "모바일겜은 안하시나요",
          "original_form": "모바일겜은 안하시나요",
          "speaker_id": "2",
          "time": "20210822 04:56"
        },
        {
          "id": "MDRW2100007607.1.31",
          "form": "저는 그 리듬겜하다가",
          "original_form": "저는 그 리듬겜하다가",
          "speaker_id": "2",
          "time": "20210822 04:56"
        },
        {
          "id": "MDRW2100007607.1.32",
          "form": "헐 저 슈스엠..",
          "original_form": "헐 저 슈스엠..",
          "speaker_id": "1",
          "time": "20210822 04:56"
        },
        {
          "id": "MDRW2100007607.1.33",
          "form": "건초염이 왔었어요",
          "original_form": "건초염이 왔었어요",
          "speaker_id": "2",
          "time": "20210822 04:56"
        },
        {
          "id": "MDRW2100007607.1.34",
          "form": "헐 슈스엠",
          "original_form": "헐 슈스엠",
          "speaker_id": "2",
          "time": "20210822 04:56"
        },
        {
          "id": "MDRW2100007607.1.35",
          "form": "아놔 진짜 매번진심을 다하시네",
          "original_form": "아놔 진짜 매번진심을 다하시네",
          "speaker_id": "1",
          "time": "20210822 04:56"
        },
        {
          "id": "MDRW2100007607.1.36",
          "form": "아나",
          "original_form": "아나",
          "speaker_id": "2",
          "time": "20210822 04:56"
        },
        {
          "id": "MDRW2100007607.1.37",
          "form": "슈스엠 진짜 열심히햇엇어요",
          "original_form": "슈스엠 진짜 열심히햇엇어요",
          "speaker_id": "1",
          "time": "20210822 04:56"
        },
        {
          "id": "MDRW2100007607.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 04:57"
        },
