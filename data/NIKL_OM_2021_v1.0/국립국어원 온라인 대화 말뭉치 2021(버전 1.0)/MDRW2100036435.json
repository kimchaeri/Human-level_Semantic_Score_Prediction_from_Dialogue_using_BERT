      "utterance": [
        {
          "id": "MDRW2100036435.1.1",
          "form": "타이머콘센트?",
          "original_form": "타이머콘센트?",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100036435.1.2",
          "form": "그거 어떤물건이야?",
          "original_form": "그거 어떤물건이야?",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100036435.1.3",
          "form": "아아니~~~",
          "original_form": "아아니~~~",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100036435.1.4",
          "form": "휴대폰 무선 충전기 말이야",
          "original_form": "휴대폰 무선 충전기 말이야",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100036435.1.5",
          "form": "아아 그거",
          "original_form": "아아 그거",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100036435.1.6",
          "form": "너 타이머콘센트를 참 좋아하는구나!",
          "original_form": "너 타이머콘센트를 참 좋아하는구나!",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100036435.1.7",
          "form": "되게 귀엽게생겼던 충전기 말하는구나",
          "original_form": "되게 귀엽게생겼던 충전기 말하는구나",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100036435.1.8",
          "form": "아무튼, 그 충전기 너무 귀엽지않아?",
          "original_form": "아무튼, 그 충전기 너무 귀엽지않아?",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100036435.1.9",
          "form": "맞아 의자모양이 귀엽지",
          "original_form": "맞아 의자모양이 귀엽지",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100036435.1.10",
          "form": "응응 너무 귀여워",
          "original_form": "응응 너무 귀여워",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100036435.1.11",
          "form": "근데 가격 봤어?",
          "original_form": "근데 가격 봤어?",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100036435.1.12",
          "form": "혹시 가격이 얼마나해?",
          "original_form": "혹시 가격이 얼마나해?",
          "speaker_id": "2",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100036435.1.13",
          "form": "거의 오만원이라니 너무 사악해",
          "original_form": "거의 오만원이라니 너무 사악해",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100036435.1.14",
          "form": "금칠을 한걸까?",
          "original_form": "금칠을 한걸까?",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100036435.1.15",
          "form": "세상에...진짜 금을 사는게 나을 것 같아",
          "original_form": "세상에...진짜 금을 사는게 나을 것 같아",
          "speaker_id": "2",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100036435.1.16",
          "form": "정말 돈낭비가 아닐 수 없어",
          "original_form": "정말 돈낭비가 아닐 수 없어",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100036435.1.17",
          "form": "그거 좋은 생각이다",
          "original_form": "그거 좋은 생각이다",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100036435.1.18",
          "form": "금테크를 해보자",
          "original_form": "금테크를 해보자",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100036435.1.19",
          "form": "응 조금만 참아서 진짜 금을 사자",
          "original_form": "응 조금만 참아서 진짜 금을 사자",
          "speaker_id": "2",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100036435.1.20",
          "form": "두번만 참으면 살 수 있어",
          "original_form": "두번만 참으면 살 수 있어",
          "speaker_id": "2",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100036435.1.21",
          "form": "내가 어제 갖고싶다고 한 거 기억나니?",
          "original_form": "내가 어제 갖고싶다고 한 거 기억나니?",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100036435.1.22",
          "form": "음 딱 하루 전이네.",
          "original_form": "음 딱 하루 전이네.",
          "speaker_id": "1",
          "time": "20210830 23:05"
        }
      ]
    }
