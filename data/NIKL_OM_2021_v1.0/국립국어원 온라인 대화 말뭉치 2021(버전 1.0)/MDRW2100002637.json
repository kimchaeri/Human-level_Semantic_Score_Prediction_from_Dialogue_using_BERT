      "utterance": [
        {
          "id": "MDRW2100002637.1.1",
          "form": "요새 저는 취업준비중이에요",
          "original_form": "요새 저는 취업준비중이에요",
          "speaker_id": "2",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002637.1.2",
          "form": "아 이직하시는건가요?",
          "original_form": "아 이직하시는건가요?",
          "speaker_id": "1",
          "time": "20210820 14:39"
        },
        {
          "id": "MDRW2100002637.1.3",
          "form": "네에! 새로운 일을 하려고 합니다",
          "original_form": "네에! 새로운 일을 하려고 합니다",
          "speaker_id": "2",
          "time": "20210820 14:57"
        },
        {
          "id": "MDRW2100002637.1.4",
          "form": "그래서 이력서를 쓰고있는데 어렵네요 ㅎㅎ",
          "original_form": "그래서 이력서를 쓰고있는데 어렵네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 14:57"
        },
        {
          "id": "MDRW2100002637.1.5",
          "form": "오늘 이력서 얼마나 쓰셨나요?",
          "original_form": "오늘 이력서 얼마나 쓰셨나요?",
          "speaker_id": "1",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002637.1.6",
          "form": "저도 다른일로 이직은 아니고 겸업을 하려는데 쉽지않네요",
          "original_form": "저도 다른일로 이직은 아니고 겸업을 하려는데 쉽지않네요",
          "speaker_id": "1",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002637.1.7",
          "form": "요슴 티오가 잘 없는거같아요",
          "original_form": "요슴 티오가 잘 없는거같아요",
          "speaker_id": "1",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002637.1.8",
          "form": "오늘 아직 시작도 못했어요....",
          "original_form": "오늘 아직 시작도 못했어요....",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002637.1.9",
          "form": "이제 시작하려구요",
          "original_form": "이제 시작하려구요",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002637.1.10",
          "form": "자소서쓰는게 어렵더라구요 ㅠㅠ",
          "original_form": "자소서쓰는게 어렵더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002637.1.11",
          "form": "ㅠㅠㅠㅠ쓰기 싫어도 딱 삼십분만 집중하면 술술 써질꺼예요",
          "original_form": "ㅠㅠㅠㅠ쓰기 싫어도 딱 삼십분만 집중하면 술술 써질꺼예요",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002637.1.12",
          "form": "처음 그 집중까지가 어렵더라구요",
          "original_form": "처음 그 집중까지가 어렵더라구요",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002637.1.13",
          "form": "급여높은곳으로 갈수있게!",
          "original_form": "급여높은곳으로 갈수있게!",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002637.1.14",
          "form": "맞아요 ㅠㅠ 처음이 어렵지 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ 처음이 어렵지 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002637.1.15",
          "form": "취준생분들 자소서 참고하면서 작성해보려구요 ㅠㅠ",
          "original_form": "취준생분들 자소서 참고하면서 작성해보려구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002637.1.16",
          "form": "할수있어요!!!! 취뽀!!!이직 화이팅",
          "original_form": "할수있어요!!!! 취뽀!!!이직 화이팅",
          "speaker_id": "1",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002637.1.17",
          "form": "감사합니다!!!!",
          "original_form": "감사합니다!!!!",
          "speaker_id": "2",
          "time": "20210820 18:19"
        },
        {
          "id": "MDRW2100002637.1.18",
          "form": "취업 해낼수있다!!!",
          "original_form": "취업 해낼수있다!!!",
          "speaker_id": "2",
          "time": "20210820 18:19"
        },
        {
          "id": "MDRW2100002637.1.19",
          "form": "저는 곧 겸업에 성공할꺼같아요!",
          "original_form": "저는 곧 겸업에 성공할꺼같아요!",
          "speaker_id": "1",
          "time": "20210820 19:04"
        },
        {
          "id": "MDRW2100002637.1.20",
          "form": "오 진짜요??!!! 잘됬네요!!!",
          "original_form": "오 진짜요??!!! 잘됬네요!!!",
          "speaker_id": "2",
          "time": "20210820 19:05"
        },
        {
          "id": "MDRW2100002637.1.21",
          "form": "티오가 안나서 계속 기다렸어요",
          "original_form": "티오가 안나서 계속 기다렸어요",
