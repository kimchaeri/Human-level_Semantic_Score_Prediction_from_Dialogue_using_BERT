        {
          "id": "MDRW2100002722.1.1",
          "form": "형제가 어떻게되세요",
          "original_form": "형제가 어떻게되세요",
          "speaker_id": "1",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002722.1.2",
          "form": "딸3명이요~",
          "original_form": "딸3명이요~",
          "speaker_id": "2",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002722.1.3",
          "form": "오 전 딸2인데",
          "original_form": "오 전 딸2인데",
          "speaker_id": "1",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002722.1.4",
          "form": "가족분들과 같이사세요?",
          "original_form": "가족분들과 같이사세요?",
          "speaker_id": "1",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002722.1.5",
          "form": "아니요 저는 20대후반이라 결혼했어요",
          "original_form": "아니요 저는 20대후반이라 결혼했어요",
          "speaker_id": "2",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002722.1.6",
          "form": "언니가 신혼집 근처에 살아서 자주 만나요",
          "original_form": "언니가 신혼집 근처에 살아서 자주 만나요",
          "speaker_id": "2",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002722.1.7",
          "form": "오 저랑같네요 ㅎㅎ",
          "original_form": "오 저랑같네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002722.1.8",
          "form": "저도 결혼해서 남편이랑 엄마랑 같이살아용 ~",
          "original_form": "저도 결혼해서 남편이랑 엄마랑 같이살아용 ~",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002722.1.9",
          "form": "엄마요? 친정엄마요?",
          "original_form": "엄마요? 친정엄마요?",
          "speaker_id": "2",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002722.1.10",
          "form": "부러워요!!",
          "original_form": "부러워요!!",
          "speaker_id": "2",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002722.1.11",
          "form": "엄마가해주는 집밥 먹을 수 있겠네요!",
          "original_form": "엄마가해주는 집밥 먹을 수 있겠네요!",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002722.1.12",
          "form": "자취하다가 결혼한거라 안좋은습관을 고대로 가져와서 요리는 거의 안해요",
          "original_form": "자취하다가 결혼한거라 안좋은습관을 고대로 가져와서 요리는 거의 안해요",
          "speaker_id": "2",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002722.1.13",
          "form": "아뇨 엄마가 편찮으셔서 옆에있어야해서용 ㅎㅎ",
          "original_form": "아뇨 엄마가 편찮으셔서 옆에있어야해서용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002722.1.14",
          "form": "집안일은 남편이랑 분담용",
          "original_form": "집안일은 남편이랑 분담용",
          "speaker_id": "1",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002722.1.15",
          "form": "애기없어요?",
          "original_form": "애기없어요?",
          "speaker_id": "2",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002722.1.16",
          "form": "결혼한지오래됬어요?",
          "original_form": "결혼한지오래됬어요?",
          "speaker_id": "2",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002722.1.17",
          "form": "아뇨 작년에했어요 ~ 없구요",
          "original_form": "아뇨 작년에했어요 ~ 없구요",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002722.1.18",
          "form": "name2님은요 ?",
          "original_form": "&name2&님은요 ?",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002722.1.19",
          "form": "저는 애기 돌 됬어요",
          "original_form": "저는 애기 돌 됬어요",
          "speaker_id": "2",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002722.1.20",
          "form": "딸이예요~~",
          "original_form": "딸이예요~~",
          "speaker_id": "2",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002722.1.21",
          "form": "오 결혼하신지 좀되셨어욘 ?",
          "original_form": "오 결혼하신지 좀되셨어욘 ?",
          "speaker_id": "1",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002722.1.22",
          "form": "6년사귀고 결혼해서 바로 애기가졌어요~~",
          "original_form": "6년사귀고 결혼해서 바로 애기가졌어요~~",
          "speaker_id": "2",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002722.1.23",
          "form": "우와 .. 멋지네요 6년이라",
          "original_form": "우와 .. 멋지네요 6년이라",
