      "utterance": [
        {
          "id": "MDRW2100023365.1.1",
          "form": "진짜 경제공부해야하는것같애요ㅎ",
          "original_form": "진짜 경제공부해야하는것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:54"
        },
        {
          "id": "MDRW2100023365.1.2",
          "form": "저도 아직은  해야져",
          "original_form": "저도 아직은  해야져",
          "speaker_id": "1",
          "time": "20210830 00:54"
        },
        {
          "id": "MDRW2100023365.1.3",
          "form": "오늘 자기소개 하는데ㅎ 뭔가 반성을 하게되더라구요ㅎ",
          "original_form": "오늘 자기소개 하는데ㅎ 뭔가 반성을 하게되더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:54"
        },
        {
          "id": "MDRW2100023365.1.4",
          "form": "전 기본적인거  아끼기 이런건 하는데",
          "original_form": "전 기본적인거  아끼기 이런건 하는데",
          "speaker_id": "1",
          "time": "20210830 00:54"
        },
        {
          "id": "MDRW2100023365.1.5",
          "form": "투자가 안되서 ㅎㅎ",
          "original_form": "투자가 안되서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:54"
        },
        {
          "id": "MDRW2100023365.1.6",
          "form": "진짜 계속 공부는 해야하는듯ㅎ",
          "original_form": "진짜 계속 공부는 해야하는듯ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:54"
        },
        {
          "id": "MDRW2100023365.1.7",
          "form": "투자는 지키는게 투자래요ㅎ",
          "original_form": "투자는 지키는게 투자래요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:54"
        },
        {
          "id": "MDRW2100023365.1.8",
          "form": "내자산을 지키고ㅎ 그시드를 불려서 하는게 투자ㅎㅎ",
          "original_form": "내자산을 지키고ㅎ 그시드를 불려서 하는게 투자ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:55"
        },
        {
          "id": "MDRW2100023365.1.9",
          "form": "그건 그래요  ㅎㅎ",
          "original_form": "그건 그래요  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:55"
        },
        {
          "id": "MDRW2100023365.1.10",
          "form": "음 제가 투자이야기하니깐 제가 번돈 이야기해줄까요?",
          "original_form": "음 제가 투자이야기하니깐 제가 번돈 이야기해줄까요?",
          "speaker_id": "2",
          "time": "20210830 00:55"
        },
        {
          "id": "MDRW2100023365.1.11",
          "form": "네네 ~",
          "original_form": "네네 ~",
          "speaker_id": "1",
          "time": "20210830 00:55"
        },
        {
          "id": "MDRW2100023365.1.12",
          "form": "가정을 이루기전에ㅎ",
          "original_form": "가정을 이루기전에ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:55"
        },
        {
          "id": "MDRW2100023365.1.13",
          "form": "제가주식을 장기투자로했었어요ㅎ",
          "original_form": "제가주식을 장기투자로했었어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:56"
        },
        {
          "id": "MDRW2100023365.1.14",
          "form": "우선은 배타면서 처음했구요ㅎ",
          "original_form": "우선은 배타면서 처음했구요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:56"
        },
        {
          "id": "MDRW2100023365.1.15",
          "form": "그때 2천 투자해서 450벌고ㅎ",
          "original_form": "그때 2천 투자해서 450벌고ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:56"
        },
        {
          "id": "MDRW2100023365.1.16",
          "form": "우앙~~",
          "original_form": "우앙~~",
          "speaker_id": "1",
          "time": "20210830 00:56"
        },
        {
          "id": "MDRW2100023365.1.17",
          "form": "내려서 5천 투자해서 1500만원 벌었어요",
          "original_form": "내려서 5천 투자해서 1500만원 벌었어요",
          "speaker_id": "2",
          "time": "20210830 00:56"
        },
        {
          "id": "MDRW2100023365.1.18",
          "form": "다 어떻게 했냐면",
          "original_form": "다 어떻게 했냐면",
          "speaker_id": "2",
          "time": "20210830 00:56"
        },
        {
          "id": "MDRW2100023365.1.19",
          "form": "10대 기업으로 했어요ㅎ",
          "original_form": "10대 기업으로 했어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100023365.1.20",
          "form": "아함 투자 잘하신듯 ㅎ",
          "original_form": "아함 투자 잘하신듯 ㅎ",
          "speaker_id": "1",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100023365.1.21",
          "form": "저는 음 주식고 몰랏었네요 ㅎㅎ",
          "original_form": "저는 음 주식고 몰랏었네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100023365.1.22",
          "form": "무조건 장기는 탄탄한대 위주로 하니깐 원금 손실은없고",
          "original_form": "무조건 장기는 탄탄한대 위주로 하니깐 원금 손실은없고",
          "speaker_id": "2",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100023365.1.23",
          "form": "대부분 3년정도 거치하면 오르더라구여ㅎ 만약에 그주식을 지금팔았다면 2.5배 이상갔을꺼에요ㅎ",
          "original_form": "대부분 3년정도 거치하면 오르더라구여ㅎ 만약에 그주식을 지금팔았다면 2.5배 이상갔을꺼에요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100023365.1.24",
          "form": "근데 제가 왜 그렇게 안하냐면ㅎ 원금손실이 싫어서ㅎㅎㅎㅎㅎ",
          "original_form": "근데 제가 왜 그렇게 안하냐면ㅎ 원금손실이 싫어서ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:58"
        },
        {
          "id": "MDRW2100023365.1.25",
          "form": "원금마이너스되시면 그냥 냅두세여ㅎㅎ 우량주같은경우는 많이 떨어져도 상장폐지까지는안가고 기다리면 올라요ㅎ",
          "original_form": "원금마이너스되시면 그냥 냅두세여ㅎㅎ 우량주같은경우는 많이 떨어져도 상장폐지까지는안가고 기다리면 올라요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:58"
        },
        {
          "id": "MDRW2100023365.1.26",
          "form": "아예 해본적도 없어요",
          "original_form": "아예 해본적도 없어요",
          "speaker_id": "1",
          "time": "20210830 00:59"
        },
        {
          "id": "MDRW2100023365.1.27",
          "form": "가정경제에 도움이되는거~^^ㅎ",
          "original_form": "가정경제에 도움이되는거~^^ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:59"
        },
        {
          "id": "MDRW2100023365.1.28",
          "form": "그래도 하셔야해여ㅎ",
          "original_form": "그래도 하셔야해여ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:59"
        },
        {
          "id": "MDRW2100023365.1.29",
          "form": "그냥 무료로 주는 주식만 ㅎㅎ 들고 있는데",
          "original_form": "그냥 무료로 주는 주식만 ㅎㅎ 들고 있는데",
          "speaker_id": "1",
          "time": "20210830 00:59"
        },
        {
          "id": "MDRW2100023365.1.30",
          "form": "저도 어제 200넣어서 5만원벌았어요ㅎ",
          "original_form": "저도 어제 200넣어서 5만원벌았어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:59"
        },
        {
