      "utterance": [
        {
          "id": "MDRW2100005405.1.1",
          "form": "마라탕은 근데 정말 중독인 것 같아요",
          "original_form": "마라탕은 근데 정말 중독인 것 같아요",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005405.1.2",
          "form": "어제에 먹었는데 또 먹고 싶네요...",
          "original_form": "어제에 먹었는데 또 먹고 싶네요...",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005405.1.3",
          "form": "마라탕이 좀 그런 거 같아요",
          "original_form": "마라탕이 좀 그런 거 같아요",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005405.1.4",
          "form": "오늘 먹었는데 내일 또 생각나고",
          "original_form": "오늘 먹었는데 내일 또 생각나고",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005405.1.5",
          "form": "저도 한 때 푹 빠져서 다음날되면 아 또 먹고 싶다 했었어요",
          "original_form": "저도 한 때 푹 빠져서 다음날되면 아 또 먹고 싶다 했었어요",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005405.1.6",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005405.1.7",
          "form": "마약... 아닐까요",
          "original_form": "마약... 아닐까요",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005405.1.8",
          "form": "이것은 좀 합리적인 의심입니다",
          "original_form": "이것은 좀 합리적인 의심입니다",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005405.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요ㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요ㅜ",
          "speaker_id": "1",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005405.1.10",
          "form": "전에 자주가던 마라탕 집이 주인이 바뀌면서 맛이 바뀌어가지고 슬펐어요 ㅠㅠ",
          "original_form": "전에 자주가던 마라탕 집이 주인이 바뀌면서 맛이 바뀌어가지고 슬펐어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005405.1.11",
          "form": "그런 거 너무 슬퍼요ㅠㅠ",
          "original_form": "그런 거 너무 슬퍼요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005405.1.12",
          "form": "저도 알던 맛집이 맛 변해가지고 속상하더라고요",
          "original_form": "저도 알던 맛집이 맛 변해가지고 속상하더라고요",
          "speaker_id": "1",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005405.1.13",
          "form": "제발 변하지 말아주세요 저희가 열심히 갈게요 ㅠㅠ",
          "original_form": "제발 변하지 말아주세요 저희가 열심히 갈게요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005405.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이렇게 빕니닷 사장님들 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이렇게 빕니닷 사장님들 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005405.1.15",
          "form": "그래서 근처 마라탕집 갔다가 매운맛만 잔뜩보고 돌아왔답니다 ;ㅅ; 순한 맛이었는데도 맵기만 하더라구여",
          "original_form": "그래서 근처 마라탕집 갔다가 매운맛만 잔뜩보고 돌아왔답니다 ;ㅅ; 순한 맛이었는데도 맵기만 하더라구여",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005405.1.16",
          "form": "어우 그럼 너무 괴로워요ㅠㅠ 맵찔이로서,,",
          "original_form": "어우 그럼 너무 괴로워요ㅠㅠ 맵찔이로서,,",
          "speaker_id": "1",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005405.1.17",
          "form": "왜 한국인의 입맛이 매운 맛인 건지 맵찔이로서 누리지 못하는 음식이 많아요 ㅜㅜ",
          "original_form": "왜 한국인의 입맛이 매운 맛인 건지 맵찔이로서 누리지 못하는 음식이 많아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005405.1.18",
          "form": "맞아여ㅠ 다 빨간 음식이라서",
          "original_form": "맞아여ㅠ 다 빨간 음식이라서",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005405.1.19",
          "form": "떡볶이도 매운맛이 기본이라서 ㅠㅠ 맛있는데 매워요 ㅠㅠㅠ",
          "original_form": "떡볶이도 매운맛이 기본이라서 ㅠㅠ 맛있는데 매워요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005405.1.20",
          "form": "그쳐ㅜㅜ 신라면 맵기라고",
          "original_form": "그쳐ㅜㅜ 신라면 맵기라고",
          "speaker_id": "1",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005405.1.21",
          "form": "안 맵다고 하는데",
          "original_form": "안 맵다고 하는데",
          "speaker_id": "1",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005405.1.22",
          "form": "그것도 매운...ㅋ",
          "original_form": "그것도 매운...ㅋ",
          "speaker_id": "1",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005405.1.23",
          "form": "맞아요 전 신라면도 좀 쉬었다 먹어야 하는데 매운 거 잘 드시는 분들은 신라면에서 매움을 못 느끼시더라구여 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 전 신라면도 좀 쉬었다 먹어야 하는데 매운 거 잘 드시는 분들은 신라면에서 매움을 못 느끼시더라구여 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:46"
        }
      ]
    }
