      "utterance": [
        {
          "id": "MDRW2100033034.1.1",
          "form": "며칠 사이에 주제가 다양해졌어요",
          "original_form": "며칠 사이에 주제가 다양해졌어요",
          "speaker_id": "2",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100033034.1.2",
          "form": "그니까여",
          "original_form": "그니까여",
          "speaker_id": "1",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100033034.1.3",
          "form": "전 대책으로 집에 안가는걸",
          "original_form": "전 대책으로 집에 안가는걸",
          "speaker_id": "1",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100033034.1.4",
          "form": "할 예정이죠 ㅎㅎ",
          "original_form": "할 예정이죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100033034.1.5",
          "form": "부디 안 모여야 되는데",
          "original_form": "부디 안 모여야 되는데",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033034.1.6",
          "form": "그런말이 있지요",
          "original_form": "그런말이 있지요",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033034.1.7",
          "form": "불효자는 옵니다 ㅜㅜ",
          "original_form": "불효자는 옵니다 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033034.1.8",
          "form": "엄마는 제가 음식도와주시니까 오시길 바라는데",
          "original_form": "엄마는 제가 음식도와주시니까 오시길 바라는데",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033034.1.9",
          "form": "큰집이라",
          "original_form": "큰집이라",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033034.1.10",
          "form": "이번에도 많이들 모이시나요",
          "original_form": "이번에도 많이들 모이시나요",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033034.1.11",
          "form": "큰 집이면 어쩔 수가 없네요ㅠ",
          "original_form": "큰 집이면 어쩔 수가 없네요ㅠ",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033034.1.12",
          "form": "근데 명절분위기 안난지 오래예요",
          "original_form": "근데 명절분위기 안난지 오래예요",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100033034.1.13",
          "form": "기독교시면 제사 없죠?",
          "original_form": "기독교시면 제사 없죠?",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100033034.1.14",
          "form": "네 전혀 없어요",
          "original_form": "네 전혀 없어요",
          "speaker_id": "2",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100033034.1.15",
          "form": "대신에 어머니 납골당에 가요",
          "original_form": "대신에 어머니 납골당에 가요",
          "speaker_id": "2",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100033034.1.16",
          "form": "그러면 행사 끝^^",
          "original_form": "그러면 행사 끝^^",
          "speaker_id": "2",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100033034.1.17",
          "form": "엄니 납골당시면 아버지랑 두분이시구나",
          "original_form": "엄니 납골당시면 아버지랑 두분이시구나",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033034.1.18",
          "form": "기도하는 것도 좋은 것 같아요",
          "original_form": "기도하는 것도 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033034.1.19",
          "form": "제도 제사 없앴음 ㅎㅎ",
          "original_form": "제도 제사 없앴음 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033034.1.20",
          "form": "부디 사람들이 마스크를 잘 쓰고 다니길 바래요 안 만날 수는 없고",
          "original_form": "부디 사람들이 마스크를 잘 쓰고 다니길 바래요 안 만날 수는 없고",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033034.1.21",
          "form": "너무 장기전이 되다 보니까 다들 힘들죠",
          "original_form": "너무 장기전이 되다 보니까 다들 힘들죠",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100033034.1.22",
          "form": "접종 대략 하구 10월부텀은 나아지길",
          "original_form": "접종 대략 하구 10월부텀은 나아지길",
