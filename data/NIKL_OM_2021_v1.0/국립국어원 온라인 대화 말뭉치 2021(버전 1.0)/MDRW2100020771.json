      "utterance": [
        {
          "id": "MDRW2100020771.1.1",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020771.1.2",
          "form": "직접 하는 건 안좋아하는데 보는 건 좋아해요 ㅎㅎ",
          "original_form": "직접 하는 건 안좋아하는데 보는 건 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020771.1.3",
          "form": "주로 어떤경기보세요?",
          "original_form": "주로 어떤경기보세요?",
          "speaker_id": "2",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020771.1.4",
          "form": "따로 챙겨보진 않는데",
          "original_form": "따로 챙겨보진 않는데",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020771.1.5",
          "form": "올림픽이나 월드컵은 우리나라 선수 출전하는 것만 챙겨봐요 ㅋㅋㅋ",
          "original_form": "올림픽이나 월드컵은 우리나라 선수 출전하는 것만 챙겨봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020771.1.6",
          "form": "아 이번에 올림픽 저도 재밌게봤어요",
          "original_form": "아 이번에 올림픽 저도 재밌게봤어요",
          "speaker_id": "2",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100020771.1.7",
          "form": "요새 배구시즌이라 그것도 종종 봐요ㅎㅎ",
          "original_form": "요새 배구시즌이라 그것도 종종 봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100020771.1.8",
          "form": "분명 이시국에 무슨 올림픽이냐 취소해라 너네들끼리 해라",
          "original_form": "분명 이시국에 무슨 올림픽이냐 취소해라 너네들끼리 해라",
          "speaker_id": "1",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020771.1.9",
          "form": "하고 욕했던 것 같은데 막상 개최하니까 재밌게 본 ㅋㅋ",
          "original_form": "하고 욕했던 것 같은데 막상 개최하니까 재밌게 본 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020771.1.10",
          "form": "맞아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020771.1.11",
          "form": "그렇게 재밌는게 없고..",
          "original_form": "그렇게 재밌는게 없고..",
          "speaker_id": "2",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020771.1.12",
          "form": "티비만 틀면 중계해주니 그것만봤네요",
          "original_form": "티비만 틀면 중계해주니 그것만봤네요",
          "speaker_id": "2",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020771.1.13",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100020771.1.14",
          "form": "그 다음 올림픽 내년이라 내년도 기대됩니다 ㅎㅎ",
          "original_form": "그 다음 올림픽 내년이라 내년도 기대됩니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100020771.1.15",
          "form": "그러게요 또 동계가 기다리네요",
          "original_form": "그러게요 또 동계가 기다리네요",
          "speaker_id": "2",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100020771.1.16",
          "form": "이시국에 어디가기도 그렇고 그거나ㅎㅎ",
          "original_form": "이시국에 어디가기도 그렇고 그거나ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100020771.1.17",
          "form": "이번 동계 올림픽은 어디서 열리죠?",
          "original_form": "이번 동계 올림픽은 어디서 열리죠?",
          "speaker_id": "1",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100020771.1.18",
          "form": "베이징 아닌가요?",
          "original_form": "베이징 아닌가요?",
          "speaker_id": "2",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100020771.1.19",
          "form": "저번에 베이징 올림픽 열었는데",
          "original_form": "저번에 베이징 올림픽 열었는데",
          "speaker_id": "1",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020771.1.20",
          "form": "그때 관중들 진짜 비매너 아니였나요?ㅠ",
          "original_form": "그때 관중들 진짜 비매너 아니였나요?ㅠ",
          "speaker_id": "1",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020771.1.21",
          "form": "그랬었나요? 어휴...",
          "original_form": "그랬었나요? 어휴...",
          "speaker_id": "2",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100020771.1.22",
          "form": "왠지 중국인들하면 좀 안좋은 인식이",
          "original_form": "왠지 중국인들하면 좀 안좋은 인식이",
          "speaker_id": "2",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100020771.1.23",
          "form": "아마 2008 하계 올림픽이였던 것 같은데 ㅋㅋㅋ",
          "original_form": "아마 2008 하계 올림픽이였던 것 같은데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100020771.1.24",
          "form": "근데 선수들 양궁 쏘는데 뒤에서 빵빠레 불고 난리쳤다던데요",
          "original_form": "근데 선수들 양궁 쏘는데 뒤에서 빵빠레 불고 난리쳤다던데요",
          "speaker_id": "1",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100020771.1.25",
          "form": "근데 자국 선수가 하면 조용해지고 ㅋㅋㅋ",
          "original_form": "근데 자국 선수가 하면 조용해지고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:05"
        }
      ]
    }
