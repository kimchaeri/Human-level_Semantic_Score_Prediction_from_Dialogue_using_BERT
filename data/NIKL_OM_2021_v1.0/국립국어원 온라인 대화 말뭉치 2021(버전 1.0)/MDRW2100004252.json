      "utterance": [
        {
          "id": "MDRW2100004252.1.1",
          "form": "저는 중국 드라마 보는거 좋아해용",
          "original_form": "저는 중국 드라마 보는거 좋아해용",
          "speaker_id": "2",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004252.1.2",
          "form": "아 저도 봤었어요",
          "original_form": "아 저도 봤었어요",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004252.1.3",
          "form": "채널이 바껴서 못봤어요",
          "original_form": "채널이 바껴서 못봤어요",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004252.1.4",
          "form": "삼생삼세 랑",
          "original_form": "삼생삼세 랑",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004252.1.5",
          "form": "아 생각이 안나네요 제목이 그 포도정령하고 봉황이 사랑하는건데",
          "original_form": "아 생각이 안나네요 제목이 그 포도정령하고 봉황이 사랑하는건데",
          "speaker_id": "1",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004252.1.6",
          "form": "진짜 잼났어요",
          "original_form": "진짜 잼났어요",
          "speaker_id": "1",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004252.1.7",
          "form": "한때 중드 매일 봤어요 ㅎㅎ",
          "original_form": "한때 중드 매일 봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004252.1.8",
          "form": "뭐 보세요?",
          "original_form": "뭐 보세요?",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004252.1.9",
          "form": "준드 남주 너무 잘생겼어요 ㅎㅎ",
          "original_form": "준드 남주 너무 잘생겼어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004252.1.10",
          "form": "중드",
          "original_form": "중드",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004252.1.11",
          "form": "헐 대박!!! 저 지금 삼생삼세 보고있거등요!!! 지금 12화째예욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐 대박!!! 저 지금 삼생삼세 보고있거등요!!! 지금 12화째예욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004252.1.12",
          "form": "향밀침침신여상",
          "original_form": "향밀침침신여상",
          "speaker_id": "1",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004252.1.13",
          "form": "와 향밀이랑 유리미인살 다보고 너뮤 재밌어거",
          "original_form": "와 향밀이랑 유리미인살 다보고 너뮤 재밌어거",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004252.1.14",
          "form": "아남주가 아주 조각입니다",
          "original_form": "아남주가 아주 조각입니다",
          "speaker_id": "1",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004252.1.15",
          "form": "삼생삼세 보고있어요...",
          "original_form": "삼생삼세 보고있어요...",
          "speaker_id": "2",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004252.1.16",
          "form": "3개 약간 비슷한 느김의 재미래요",
          "original_form": "3개 약간 비슷한 느김의 재미래요",
          "speaker_id": "2",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004252.1.17",
          "form": "삼생삼세 넘잼나요",
          "original_form": "삼생삼세 넘잼나요",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004252.1.18",
          "form": "네 저도 요런거 좋아요 ㅎㅎ",
          "original_form": "네 저도 요런거 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004252.1.19",
          "form": "야화 너무 좋아요......",
          "original_form": "야화 너무 좋아요......",
          "speaker_id": "2",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004252.1.20",
          "form": "삼생삼세 그남주 적인걸 영화도 봤어요",
          "original_form": "삼생삼세 그남주 적인걸 영화도 봤어요",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004252.1.21",
          "form": "근데 드라마에서 더 멋쪄요",
          "original_form": "근데 드라마에서 더 멋쪄요",
          "speaker_id": "1",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004252.1.22",
          "form": "맞아요 아 매력적이예요 앙",
          "original_form": "맞아요 아 매력적이예요 앙",
          "speaker_id": "1",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004252.1.23",
          "form": "미남이라기보단 분위기가 아주 매력적입니다",
          "original_form": "미남이라기보단 분위기가 아주 매력적입니다",
          "speaker_id": "1",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004252.1.24",
          "form": "판타지 중드 넘잼나요",
          "original_form": "판타지 중드 넘잼나요",
          "speaker_id": "1",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004252.1.25",
          "form": "마자요 얼굴은 향밀주인공이 좀더 미남같아요... 윤옥... 봉황...",
          "original_form": "마자요 얼굴은 향밀주인공이 좀더 미남같아요... 윤옥... 봉황...",
          "speaker_id": "2",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004252.1.26",
          "form": "진정령도 보셨어요??",
          "original_form": "진정령도 보셨어요??",
          "speaker_id": "2",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004252.1.27",
          "form": "맞아요 미남이죠",
          "original_form": "맞아요 미남이죠",
          "speaker_id": "1",
          "time": "20210821 16:06"
        },
