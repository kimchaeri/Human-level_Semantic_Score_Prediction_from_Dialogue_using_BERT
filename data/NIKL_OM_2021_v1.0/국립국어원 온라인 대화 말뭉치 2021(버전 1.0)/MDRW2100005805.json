      "utterance": [
        {
          "id": "MDRW2100005805.1.1",
          "form": "엥 머리 아파? 기온 차 때문에 그런갑다",
          "original_form": "엥 머리 아파? 기온 차 때문에 그런갑다",
          "speaker_id": "1",
          "time": "20210907 13:00"
        },
        {
          "id": "MDRW2100005805.1.2",
          "form": "점심 아파?",
          "original_form": "점심 아파?",
          "speaker_id": "1",
          "time": "20210907 13:00"
        },
        {
          "id": "MDRW2100005805.1.3",
          "form": "아니뭐래",
          "original_form": "아니뭐래",
          "speaker_id": "1",
          "time": "20210907 13:00"
        },
        {
          "id": "MDRW2100005805.1.4",
          "form": "먹었어?",
          "original_form": "먹었어?",
          "speaker_id": "1",
          "time": "20210907 13:00"
        },
        {
          "id": "MDRW2100005805.1.5",
          "form": "ㅋㄱㅋㅋㅋ점심 아파 이러네",
          "original_form": "ㅋㄱㅋㅋㅋ점심 아파 이러네",
          "speaker_id": "2",
          "time": "20210907 13:01"
        },
        {
          "id": "MDRW2100005805.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅠㅠㅠㅠㅠ실스입니다.",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅠㅠㅠㅠㅠ실스입니다.",
          "speaker_id": "1",
          "time": "20210907 13:01"
        },
        {
          "id": "MDRW2100005805.1.7",
          "form": "아직.. 나 좀 전에 강의 끝나서 이제 먹어야혀",
          "original_form": "아직.. 나 좀 전에 강의 끝나서 이제 먹어야혀",
          "speaker_id": "2",
          "time": "20210907 13:01"
        },
        {
          "id": "MDRW2100005805.1.8",
          "form": "아 실강이야? 헉 뭐먹을거양",
          "original_form": "아 실강이야? 헉 뭐먹을거양",
          "speaker_id": "1",
          "time": "20210907 13:02"
        },
        {
          "id": "MDRW2100005805.1.9",
          "form": "녹강하는거 하나뿐인듯. 아직 고민중인데 짜파게티 먹을까 싶기도",
          "original_form": "녹강하는거 하나뿐인듯. 아직 고민중인데 짜파게티 먹을까 싶기도",
          "speaker_id": "2",
          "time": "20210907 13:02"
        },
        {
          "id": "MDRW2100005805.1.10",
          "form": "2시에 또 실강있어서 타임택이다",
          "original_form": "2시에 또 실강있어서 타임택이다",
          "speaker_id": "2",
          "time": "20210907 13:02"
        },
        {
          "id": "MDRW2100005805.1.11",
          "form": "타임어택",
          "original_form": "타임어택",
          "speaker_id": "2",
          "time": "20210907 13:02"
        },
        {
          "id": "MDRW2100005805.1.12",
          "form": "omg 에반데 얼른 먹어야겠다",
          "original_form": "omg 에반데 얼른 먹어야겠다",
          "speaker_id": "1",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100005805.1.13",
          "form": "짜파게티 끓이좌 지금",
          "original_form": "짜파게티 끓이좌 지금",
          "speaker_id": "1",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100005805.1.14",
          "form": "화요일 왜케 빡세냐",
          "original_form": "화요일 왜케 빡세냐",
          "speaker_id": "2",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100005805.1.15",
          "form": "넌 짜파게티 어떻게 끓여?",
          "original_form": "넌 짜파게티 어떻게 끓여?",
          "speaker_id": "2",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100005805.1.16",
          "form": "나 짜파게티 하나만 끓인 건 꽤 오래돼서 기억이 안 난다.. 맨날 불닭이랑 같이 끓여서..",
          "original_form": "나 짜파게티 하나만 끓인 건 꽤 오래돼서 기억이 안 난다.. 맨날 불닭이랑 같이 끓여서..",
          "speaker_id": "1",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100005805.1.17",
          "form": "물을 안 버리는거였나?",
          "original_form": "물을 안 버리는거였나?",
          "speaker_id": "1",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100005805.1.18",
          "form": "아 졸여서 끓이는거?",
          "original_form": "아 졸여서 끓이는거?",
          "speaker_id": "2",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100005805.1.19",
          "form": "불닭짜파게티 맛있겠다",
          "original_form": "불닭짜파게티 맛있겠다",
          "speaker_id": "2",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100005805.1.20",
          "form": "웅 졸여서 끓여!",
          "original_form": "웅 졸여서 끓여!",
          "speaker_id": "1",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100005805.1.21",
          "form": "개개개개개맛있어",
          "original_form": "개개개개개맛있어",
          "speaker_id": "1",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100005805.1.22",
          "form": "졸여서 끓이면 설거지 넘 힘들지 않음? ㅜ",
          "original_form": "졸여서 끓이면 설거지 넘 힘들지 않음? ㅜ",
          "speaker_id": "2",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100005805.1.23",
          "form": "나 그거땜에 엄두도 못내는중",
          "original_form": "나 그거땜에 엄두도 못내는중",
          "speaker_id": "2",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100005805.1.24",
          "form": "헉 맞아 딱 달라붙어서 안 지워지더라... 그래도 그릇에 옮겨 먹으면 그나마 괜찮더라구! 먹을 때도 물 조금씩 넣어야되고..",
          "original_form": "헉 맞아 딱 달라붙어서 안 지워지더라... 그래도 그릇에 옮겨 먹으면 그나마 괜찮더라구! 먹을 때도 물 조금씩 넣어야되고..",
          "speaker_id": "1",
          "time": "20210907 13:08"
        },
        {
          "id": "MDRW2100005805.1.25",
          "form": "귀찮다 걍 물버려야겠다ㅋㅋㄱㅋㅋㄱ",
          "original_form": "귀찮다 걍 물버려야겠다ㅋㅋㄱㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100005805.1.26",
          "form": "짜파게티에 반숙 올리면 진짜 극락",
          "original_form": "짜파게티에 반숙 올리면 진짜 극락",
          "speaker_id": "2",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100005805.1.27",
          "form": "아놔ㅋㅋㅋㅋㅋㅋㅋㅋ뭐 먹게ㅋㅋㅋㅋㅋ",
          "original_form": "아놔ㅋㅋㅋㅋㅋㅋㅋㅋ뭐 먹게ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100005805.1.28",
          "form": "내가 먹어야겠네",
          "original_form": "내가 먹어야겠네",
          "speaker_id": "1",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100005805.1.29",
          "form": "ㅎㅎ 부산시 어쩌고 오세요",
          "original_form": "ㅎㅎ 부산시 어쩌고 오세요",
          "speaker_id": "2",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100005805.1.30",
          "form": "한...4시간만 기다려봐... 뛰어감..",
          "original_form": "한...4시간만 기다려봐... 뛰어감..",
          "speaker_id": "1",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100005805.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그때면 저녁이에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그때면 저녁이에요",
          "speaker_id": "2",
