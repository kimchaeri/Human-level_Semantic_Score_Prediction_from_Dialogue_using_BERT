      "utterance": [
        {
          "id": "MDRW2100008139.1.1",
          "form": "그전에 던킨 쿠폰 까먹고 그냥 먹었다 ㅡㅡ",
          "original_form": "그전에 던킨 쿠폰 까먹고 그냥 먹었다 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008139.1.2",
          "form": "지금도 후회중 ㅜㅜ",
          "original_form": "지금도 후회중 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008139.1.3",
          "form": "진짜 넘 차이나",
          "original_form": "진짜 넘 차이나",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008139.1.4",
          "form": "20프로 이상 세일이라",
          "original_form": "20프로 이상 세일이라",
          "speaker_id": "1",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008139.1.5",
          "form": "요새 생각해보니 막는고만 샀네",
          "original_form": "요새 생각해보니 막는고만 샀네",
          "speaker_id": "1",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008139.1.6",
          "form": "ㅋㅋㅋㅋ나도 그래ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ나도 그래ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008139.1.7",
          "form": "근데 후회해봐야 뭐 안달라진다 치면!!",
          "original_form": "근데 후회해봐야 뭐 안달라진다 치면!!",
          "speaker_id": "2",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008139.1.8",
          "form": "앞으로 뭘 더 할인받을것인가!!",
          "original_form": "앞으로 뭘 더 할인받을것인가!!",
          "speaker_id": "2",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008139.1.9",
          "form": "고민해야지ㅋㅋㅋ",
          "original_form": "고민해야지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008139.1.10",
          "form": "맞아 근데도 후회됨",
          "original_form": "맞아 근데도 후회됨",
          "speaker_id": "1",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008139.1.11",
          "form": "난받을때 실수도해",
          "original_form": "난받을때 실수도해",
          "speaker_id": "1",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008139.1.12",
          "form": "그래서 쇼핑을 못해",
          "original_form": "그래서 쇼핑을 못해",
          "speaker_id": "1",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008139.1.13",
          "form": "얼마전 복숭아 공짜",
          "original_form": "얼마전 복숭아 공짜",
          "speaker_id": "1",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008139.1.14",
          "form": "맛있더라 ㅎㅎ",
          "original_form": "맛있더라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008139.1.15",
          "form": "공짜라 맛난가봐 ㅋㅋ",
          "original_form": "공짜라 맛난가봐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:46"
        },
        {
          "id": "MDRW2100008139.1.16",
          "form": "쿠팡은 그럴때 있잖아",
          "original_form": "쿠팡은 그럴때 있잖아",
          "speaker_id": "1",
          "time": "20210822 13:47"
        },
        {
          "id": "MDRW2100008139.1.17",
          "form": "나가기 싫고 당장 간장 식자재 떨어졌을때 넘좋아",
          "original_form": "나가기 싫고 당장 간장 식자재 떨어졌을때 넘좋아",
          "speaker_id": "1",
          "time": "20210822 13:47"
        },
        {
          "id": "MDRW2100008139.1.18",
          "form": "비마트가 여긴 안되서 시골이라 아쉬워",
          "original_form": "비마트가 여긴 안되서 시골이라 아쉬워",
          "speaker_id": "1",
          "time": "20210822 13:48"
        },
        {
          "id": "MDRW2100008139.1.19",
          "form": "난 쿠팡은 안써봤어ㅋㅋㅋ그 무슨 와우회원? 이런거 신청도 했어?",
          "original_form": "난 쿠팡은 안써봤어ㅋㅋㅋ그 무슨 와우회원? 이런거 신청도 했어?",
          "speaker_id": "2",
          "time": "20210822 18:32"
        },
        {
          "id": "MDRW2100008139.1.20",
          "form": "응 했지",
          "original_form": "응 했지",
          "speaker_id": "1",
          "time": "20210822 18:35"
        },
        {
          "id": "MDRW2100008139.1.21",
          "form": "2900원내면 한달내내 배송비 없으니",
          "original_form": "2900원내면 한달내내 배송비 없으니",
          "speaker_id": "1",
          "time": "20210822 18:35"
        },
        {
          "id": "MDRW2100008139.1.22",
          "form": "편해 뭐하니시켜도 부담없고 사실",
          "original_form": "편해 뭐하니시켜도 부담없고 사실",
          "speaker_id": "1",
          "time": "20210822 18:36"
        },
        {
          "id": "MDRW2100008139.1.23",
          "form": "조그만거 사면 배송비가 더 비쌀때 많잖아",
          "original_form": "조그만거 사면 배송비가 더 비쌀때 많잖아",
          "speaker_id": "1",
          "time": "20210822 18:36"
        },
        {
          "id": "MDRW2100008139.1.24",
          "form": "ㅋㅋㅋㅋ아 배송비가 아예없어? 그건 너무 저렴한거 아닌가",
          "original_form": "ㅋㅋㅋㅋ아 배송비가 아예없어? 그건 너무 저렴한거 아닌가",
          "speaker_id": "2",
          "time": "20210822 18:36"
        },
        {
          "id": "MDRW2100008139.1.25",
          "form": "그렇때좋아",
          "original_form": "그렇때좋아",
          "speaker_id": "1",
          "time": "20210822 18:36"
        },
        {
          "id": "MDRW2100008139.1.26",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210822 18:36"
        },
        {
          "id": "MDRW2100008139.1.27",
          "form": "원래 1회 배송비가 2500이자나",
          "original_form": "원래 1회 배송비가 2500이자나",
          "speaker_id": "2",
          "time": "20210822 18:36"
        },
        {
          "id": "MDRW2100008139.1.28",
          "form": "거기 직배송은 없어",
          "original_form": "거기 직배송은 없어",
          "speaker_id": "1",
          "time": "20210822 18:36"
        },
        {
          "id": "MDRW2100008139.1.29",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210822 18:36"
        },
        {
          "id": "MDRW2100008139.1.30",
          "form": "헐ㅋㅋㅋㅋㅋㅋ나는 그 쿠팡플레이? 이거때문에 가입할까 고민하다가 봤는데",
          "original_form": "헐ㅋㅋㅋㅋㅋㅋ나는 그 쿠팡플레이? 이거때문에 가입할까 고민하다가 봤는데",
          "speaker_id": "2",
          "time": "20210822 18:37"
        },
        {
          "id": "MDRW2100008139.1.31",
          "form": "거기 신선은 15000원 이상인데",
          "original_form": "거기 신선은 15000원 이상인데",
          "speaker_id": "1",
          "time": "20210822 18:37"
        },
        {
          "id": "MDRW2100008139.1.32",
          "form": "간장 소독솜",
          "original_form": "간장 소독솜",
          "speaker_id": "1",
          "time": "20210822 18:37"
        },
        {
          "id": "MDRW2100008139.1.33",
          "form": "뭐이런 공산품",
          "original_form": "뭐이런 공산품",
          "speaker_id": "1",
          "time": "20210822 18:37"
        },
        {
          "id": "MDRW2100008139.1.34",
          "form": "쿠팡센타에서 오는거 전부 무배",
          "original_form": "쿠팡센타에서 오는거 전부 무배",
          "speaker_id": "1",
          "time": "20210822 18:37"
        },
        {
          "id": "MDRW2100008139.1.35",
          "form": "진짜 편해",
          "original_form": "진짜 편해",
          "speaker_id": "1",
          "time": "20210822 18:37"
        },
        {
          "id": "MDRW2100008139.1.36",
          "form": "조건은 좋아",
          "original_form": "조건은 좋아",
          "speaker_id": "1",
          "time": "20210822 18:37"
        },
        {
          "id": "MDRW2100008139.1.37",
          "form": "근데 요새 시킨게 없네 ㅋㅋ",
          "original_form": "근데 요새 시킨게 없네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:38"
        },
        {
          "id": "MDRW2100008139.1.38",
          "form": "아 소독솜 시켰다",
          "original_form": "아 소독솜 시켰다",
          "speaker_id": "1",
          "time": "20210822 18:38"
        },
