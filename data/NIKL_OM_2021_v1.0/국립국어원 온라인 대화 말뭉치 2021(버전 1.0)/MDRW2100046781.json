        },
        {
          "id": "MDRW2100046781.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211104 07:01"
        },
        {
          "id": "MDRW2100046781.1.3",
          "form": "아침은 드셨나요?",
          "original_form": "아침은 드셨나요?",
          "speaker_id": "2",
          "time": "20211104 09:02"
        },
        {
          "id": "MDRW2100046781.1.4",
          "form": "네 먹었어요",
          "original_form": "네 먹었어요",
          "speaker_id": "1",
          "time": "20211105 11:58"
        },
        {
          "id": "MDRW2100046781.1.5",
          "form": "오 ㅋㅋ 저는 방금 점심먹구 왔습니다!",
          "original_form": "오 ㅋㅋ 저는 방금 점심먹구 왔습니다!",
          "speaker_id": "2",
          "time": "20211106 00:51"
        },
        {
          "id": "MDRW2100046781.1.6",
          "form": "점심 뭐먹었어요??",
          "original_form": "점심 뭐먹었어요??",
          "speaker_id": "1",
          "time": "20211105 13:27"
        },
        {
          "id": "MDRW2100046781.1.7",
          "form": "저는 육계장 먹었어요",
          "original_form": "저는 육계장 먹었어요",
          "speaker_id": "1",
          "time": "20211105 13:27"
        },
        {
          "id": "MDRW2100046781.1.8",
          "form": "저는 마라탕 먹었습니다!!",
          "original_form": "저는 마라탕 먹었습니다!!",
          "speaker_id": "2",
          "time": "20211105 13:27"
        },
        {
          "id": "MDRW2100046781.1.9",
          "form": "마라탕 맛있겠다",
          "original_form": "마라탕 맛있겠다",
          "speaker_id": "1",
          "time": "20211105 13:27"
        },
        {
          "id": "MDRW2100046781.1.10",
          "form": "육개장도 맛있으셨겠는데요ㅋㅋ",
          "original_form": "육개장도 맛있으셨겠는데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 13:27"
        },
        {
          "id": "MDRW2100046781.1.11",
          "form": "마라탕에 밥말아 먹어보셨나요",
          "original_form": "마라탕에 밥말아 먹어보셨나요",
          "speaker_id": "1",
          "time": "20211105 13:27"
        },
        {
          "id": "MDRW2100046781.1.12",
          "form": "넵 한국인은 국물에 밥말아 먹는거 아닌가요",
          "original_form": "넵 한국인은 국물에 밥말아 먹는거 아닌가요",
          "speaker_id": "2",
          "time": "20211105 13:28"
        },
        {
          "id": "MDRW2100046781.1.13",
          "form": "저도 마라탕에 밥말아먹는데",
          "original_form": "저도 마라탕에 밥말아먹는데",
          "speaker_id": "1",
          "time": "20211105 13:28"
        },
        {
          "id": "MDRW2100046781.1.14",
          "form": "그치만 너무 매워서 많이는 못먹었어요 ㅋㅋ",
          "original_form": "그치만 너무 매워서 많이는 못먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 13:28"
        },
        {
          "id": "MDRW2100046781.1.15",
          "form": "정말 맛있더라구요",
          "original_form": "정말 맛있더라구요",
          "speaker_id": "1",
          "time": "20211105 13:28"
        },
        {
          "id": "MDRW2100046781.1.16",
          "form": "땅콩소스 넣어먹어면",
          "original_form": "땅콩소스 넣어먹어면",
          "speaker_id": "1",
          "time": "20211105 13:28"
        },
        {
          "id": "MDRW2100046781.1.17",
          "form": "그렇게 안맵더라구요",
          "original_form": "그렇게 안맵더라구요",
          "speaker_id": "1",
          "time": "20211105 13:28"
        },
        {
          "id": "MDRW2100046781.1.18",
          "form": "앗 그런가요",
          "original_form": "앗 그런가요",
          "speaker_id": "2",
          "time": "20211105 13:28"
        },
