      "utterance": [
        {
          "id": "MDRW2100043956.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043956.1.2",
          "form": "학교 다니시나요?",
          "original_form": "학교 다니시나요?",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043956.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043956.1.4",
          "form": "저는 졸업생이에요",
          "original_form": "저는 졸업생이에요",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100043956.1.5",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100043956.1.6",
          "form": "아뇨 저도 회사다니고 있어요 ㅋㅋㅋ",
          "original_form": "아뇨 저도 회사다니고 있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100043956.1.7",
          "form": "학교 다닐때가 젤 맘 편했던거같아요",
          "original_form": "학교 다닐때가 젤 맘 편했던거같아요",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100043956.1.8",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100043956.1.9",
          "form": "그땐 시험보는게 너무 싫어서",
          "original_form": "그땐 시험보는게 너무 싫어서",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043956.1.10",
          "form": "얼른 졸업하고 싶었는데 말이에요",
          "original_form": "얼른 졸업하고 싶었는데 말이에요",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043956.1.11",
          "form": "맞아요 ㅋㅋㅋ 저도 빨리 돈벌고싶었는대",
          "original_form": "맞아요 ㅋㅋㅋ 저도 빨리 돈벌고싶었는대",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043956.1.12",
          "form": "정말 세상 바보같은 일이었어요 ㅠ",
          "original_form": "정말 세상 바보같은 일이었어요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043956.1.13",
          "form": "아휴 그러니까요!",
          "original_form": "아휴 그러니까요!",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100043956.1.14",
          "form": "동아리 활동도 하셨나요?",
          "original_form": "동아리 활동도 하셨나요?",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100043956.1.15",
          "form": "전 동아리는 안하고 학회 했었어요",
          "original_form": "전 동아리는 안하고 학회 했었어요",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100043956.1.16",
          "form": "동아리 하셨나요?",
          "original_form": "동아리 하셨나요?",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100043956.1.17",
          "form": "오 저도 동아리는 안했어요",
          "original_form": "오 저도 동아리는 안했어요",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100043956.1.18",
          "form": "다른거 하신거 있나요?",
          "original_form": "다른거 하신거 있나요?",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100043956.1.19",
          "form": "소모임이 있어서 소모임 활동만 잠깐 했어요",
          "original_form": "소모임이 있어서 소모임 활동만 잠깐 했어요",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100043956.1.20",
          "form": "다시 돌아갈 수 있다면 동아리 해보고 싶어요!",
          "original_form": "다시 돌아갈 수 있다면 동아리 해보고 싶어요!",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100043956.1.21",
          "form": "어떤 동아리 하고싶으세요?",
          "original_form": "어떤 동아리 하고싶으세요?",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100043956.1.22",
          "form": "음..체육 활동 하는 곳이나 창업 동아리 재미있을 것 같아요",
          "original_form": "음..체육 활동 하는 곳이나 창업 동아리 재미있을 것 같아요",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100043956.1.23",
          "form": "창업동아리 특이하네요",
          "original_form": "창업동아리 특이하네요",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100043956.1.24",
          "form": "저도 해보고싶어요",
          "original_form": "저도 해보고싶어요",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100043956.1.25",
          "form": "돈 주고 운동 배우는 직장인의 작은 후회 입니다..",
          "original_form": "돈 주고 운동 배우는 직장인의 작은 후회 입니다..",
          "speaker_id": "2",
          "time": "20210907 23:16"
        }
      ]
    }
