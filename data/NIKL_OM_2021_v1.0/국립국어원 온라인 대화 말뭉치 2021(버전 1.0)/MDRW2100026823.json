      "utterance": [
        {
          "id": "MDRW2100026823.1.1",
          "form": "영화관내에서 먹는건 금지에요",
          "original_form": "영화관내에서 먹는건 금지에요",
          "speaker_id": "1",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100026823.1.2",
          "form": "정모해보신적있나요",
          "original_form": "정모해보신적있나요",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026823.1.3",
          "form": "한 20년전?",
          "original_form": "한 20년전?",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026823.1.4",
          "form": "오래됐군요",
          "original_form": "오래됐군요",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026823.1.5",
          "form": "가물가물합니다",
          "original_form": "가물가물합니다",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026823.1.6",
          "form": "요즘은 코로나라서 정모하기 어렵겠지요",
          "original_form": "요즘은 코로나라서 정모하기 어렵겠지요",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026823.1.7",
          "form": "커뮤니티는 온라인으로 충분합니다",
          "original_form": "커뮤니티는 온라인으로 충분합니다",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026823.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026823.1.9",
          "form": "그래도 새로운 사람을 만나는 기회가 되니 좋지요",
          "original_form": "그래도 새로운 사람을 만나는 기회가 되니 좋지요",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026823.1.10",
          "form": "개고양이 짤 얻기엔 커뮤니티가 짱이지요",
          "original_form": "개고양이 짤 얻기엔 커뮤니티가 짱이지요",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026823.1.11",
          "form": "정모해본적은 없습니다",
          "original_form": "정모해본적은 없습니다",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026823.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026823.1.13",
          "form": "반려동물 짤 좋아요",
          "original_form": "반려동물 짤 좋아요",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026823.1.14",
          "form": "정모는 이 시대에 썩 필요치않습니다",
          "original_form": "정모는 이 시대에 썩 필요치않습니다",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026823.1.15",
          "form": "메타버스 시대이지요",
          "original_form": "메타버스 시대이지요",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026823.1.16",
          "form": "랜선집사여서 커뮤니티에서 짤 모으네요",
          "original_form": "랜선집사여서 커뮤니티에서 짤 모으네요",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026823.1.17",
          "form": "네 저는 랜선이모입니다",
          "original_form": "네 저는 랜선이모입니다",
          "speaker_id": "2",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026823.1.18",
          "form": "고양이 강아지 모으시나요",
          "original_form": "고양이 강아지 모으시나요",
          "speaker_id": "1",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026823.1.19",
          "form": "커뮤니티로 다복한 냥멍을 키우고 있습니다",
          "original_form": "커뮤니티로 다복한 냥멍을 키우고 있습니다",
          "speaker_id": "2",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026823.1.20",
          "form": "전 가끔 토끼나 삵도 저장해요",
          "original_form": "전 가끔 토끼나 삵도 저장해요",
          "speaker_id": "1",
