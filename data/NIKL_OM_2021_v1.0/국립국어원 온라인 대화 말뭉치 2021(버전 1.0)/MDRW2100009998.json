      "utterance": [
        {
          "id": "MDRW2100009998.1.1",
          "form": "크리스마스날 성당갔는데",
          "original_form": "크리스마스날 성당갔는데",
          "speaker_id": "1",
          "time": "20210823 12:58"
        },
        {
          "id": "MDRW2100009998.1.2",
          "form": "영어를많이쓰더라",
          "original_form": "영어를많이쓰더라",
          "speaker_id": "1",
          "time": "20210823 12:58"
        },
        {
          "id": "MDRW2100009998.1.3",
          "form": "요즘 성당도 코로나라서 안갔는데",
          "original_form": "요즘 성당도 코로나라서 안갔는데",
          "speaker_id": "2",
          "time": "20210823 18:32"
        },
        {
          "id": "MDRW2100009998.1.4",
          "form": "반성되네여",
          "original_form": "반성되네여",
          "speaker_id": "2",
          "time": "20210823 18:32"
        },
        {
          "id": "MDRW2100009998.1.5",
          "form": "ㅋㅋ 요샌 갈수없지",
          "original_form": "ㅋㅋ 요샌 갈수없지",
          "speaker_id": "1",
          "time": "20210823 18:34"
        },
        {
          "id": "MDRW2100009998.1.6",
          "form": "다 온라인으로 전환된지 꽤 됐오",
          "original_form": "다 온라인으로 전환된지 꽤 됐오",
          "speaker_id": "1",
          "time": "20210823 18:35"
        },
        {
          "id": "MDRW2100009998.1.7",
          "form": "요새 절도 그럼 폐쇄? 인가요?",
          "original_form": "요새 절도 그럼 폐쇄? 인가요?",
          "speaker_id": "2",
          "time": "20210823 18:35"
        },
        {
          "id": "MDRW2100009998.1.8",
          "form": "ㅇㅇ외부인 출입금지여",
          "original_form": "ㅇㅇ외부인 출입금지여",
          "speaker_id": "1",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100009998.1.9",
          "form": "뭔가 신부님과 스님들 심심하실지도",
          "original_form": "뭔가 신부님과 스님들 심심하실지도",
          "speaker_id": "2",
          "time": "20210823 18:38"
        },
        {
          "id": "MDRW2100009998.1.10",
          "form": "ㅋㅋ 심심해도 쩔수엄징ㅋㅋ",
          "original_form": "ㅋㅋ 심심해도 쩔수엄징ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100009998.1.11",
          "form": "신고된다궁",
          "original_form": "신고된다궁",
          "speaker_id": "1",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100009998.1.12",
          "form": "108배는 매일하세요?",
          "original_form": "108배는 매일하세요?",
          "speaker_id": "2",
          "time": "20210823 18:41"
        },
        {
          "id": "MDRW2100009998.1.13",
          "form": "ㅇㅇ 매일하징 오늘도 다해서 좋아",
          "original_form": "ㅇㅇ 매일하징 오늘도 다해서 좋아",
          "speaker_id": "1",
          "time": "20210823 18:43"
        },
        {
          "id": "MDRW2100009998.1.14",
          "form": "울엄마도 매일 묵주기도하심",
          "original_form": "울엄마도 매일 묵주기도하심",
          "speaker_id": "2",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100009998.1.15",
          "form": "오오 어머니는 대단하시당",
          "original_form": "오오 어머니는 대단하시당",
          "speaker_id": "1",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100009998.1.16",
          "form": "사랑의교횐가 서울에 전광훈목사네",
          "original_form": "사랑의교횐가 서울에 전광훈목사네",
          "speaker_id": "1",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100009998.1.17",
          "form": "거기 철거된대",
          "original_form": "거기 철거된대",
          "speaker_id": "1",
          "time": "20210823 18:50"
        },
        {
          "id": "MDRW2100009998.1.18",
          "form": "저런 어쩌다가?",
          "original_form": "저런 어쩌다가?",
          "speaker_id": "2",
          "time": "20210823 18:51"
        },
        {
          "id": "MDRW2100009998.1.19",
          "form": "계속 대면 모임하고 시위해서 ㅋㅋㅋ",
          "original_form": "계속 대면 모임하고 시위해서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:53"
        },
        {
          "id": "MDRW2100009998.1.20",
          "form": "에라이 그건 진짜 종교인으로서 부끄러워 해야할일인데",
          "original_form": "에라이 그건 진짜 종교인으로서 부끄러워 해야할일인데",
          "speaker_id": "2",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100009998.1.21",
          "form": "난 종교는 정치색을 띄면 안된다고 생가함",
          "original_form": "난 종교는 정치색을 띄면 안된다고 생가함",
          "speaker_id": "2",
          "time": "20210823 20:16"
        },
