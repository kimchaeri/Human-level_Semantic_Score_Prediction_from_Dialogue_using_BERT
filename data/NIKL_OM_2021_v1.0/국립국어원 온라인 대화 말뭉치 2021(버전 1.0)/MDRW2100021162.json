      "utterance": [
        {
          "id": "MDRW2100021162.1.1",
          "form": "여행조아하시나요",
          "original_form": "여행조아하시나요",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100021162.1.2",
          "form": "자주는 못가지만 좋아해요",
          "original_form": "자주는 못가지만 좋아해요",
          "speaker_id": "2",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100021162.1.3",
          "form": "어디가보셨나요",
          "original_form": "어디가보셨나요",
          "speaker_id": "1",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100021162.1.4",
          "form": "제주도 가봤어요!",
          "original_form": "제주도 가봤어요!",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021162.1.5",
          "form": "속초나 강릉도 가보고",
          "original_form": "속초나 강릉도 가보고",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021162.1.6",
          "form": "주로 바다나 폭포 근처로 갔었네요",
          "original_form": "주로 바다나 폭포 근처로 갔었네요",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021162.1.7",
          "form": "해외는요?",
          "original_form": "해외는요?",
          "speaker_id": "1",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100021162.1.8",
          "form": "해외는 말레이시어 한번 갔었너요",
          "original_form": "해외는 말레이시어 한번 갔었너요",
          "speaker_id": "2",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100021162.1.9",
          "form": "근데 예전이라 기억이 잘 안나요ㅋㅋㅋ",
          "original_form": "근데 예전이라 기억이 잘 안나요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100021162.1.10",
          "form": "오말레이시아 많이가더라구요",
          "original_form": "오말레이시아 많이가더라구요",
          "speaker_id": "1",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100021162.1.11",
          "form": "근데 예전에 간거라 기억은 잘 안나요",
          "original_form": "근데 예전에 간거라 기억은 잘 안나요",
          "speaker_id": "2",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100021162.1.12",
          "form": "싼 물가가 매력적이라 많이 가는것 같더라구요",
          "original_form": "싼 물가가 매력적이라 많이 가는것 같더라구요",
          "speaker_id": "2",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100021162.1.13",
          "form": "코로나가 어서 끝나야ㅜ",
          "original_form": "코로나가 어서 끝나야ㅜ",
          "speaker_id": "1",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100021162.1.14",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "2",
          "time": "20210829 13:36"
        },
        {
          "id": "MDRW2100021162.1.15",
          "form": "이렇게 아무것도 못하게될줄은 몰랐어요",
          "original_form": "이렇게 아무것도 못하게될줄은 몰랐어요",
          "speaker_id": "2",
          "time": "20210829 13:36"
        },
        {
          "id": "MDRW2100021162.1.16",
          "form": "어디가보고 싶으신가요",
          "original_form": "어디가보고 싶으신가요",
          "speaker_id": "1",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100021162.1.17",
          "form": "외국은 알레스카요",
          "original_form": "외국은 알레스카요",
          "speaker_id": "2",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100021162.1.18",
          "form": "별이랑 오로라 보고싶더라구요",
          "original_form": "별이랑 오로라 보고싶더라구요",
          "speaker_id": "2",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100021162.1.19",
          "form": "오로라 보기 힘들다던데 꼭 눈으로 보고싶어욤",
          "original_form": "오로라 보기 힘들다던데 꼭 눈으로 보고싶어욤",
          "speaker_id": "2",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100021162.1.20",
          "form": "!",
          "original_form": "!",
          "speaker_id": "2",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100021162.1.21",
          "form": "와 알래스카!!!",
          "original_form": "와 알래스카!!!",
          "speaker_id": "1",
