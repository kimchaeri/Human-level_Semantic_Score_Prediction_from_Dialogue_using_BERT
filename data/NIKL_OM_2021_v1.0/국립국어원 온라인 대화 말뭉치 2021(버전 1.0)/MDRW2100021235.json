      "utterance": [
        {
          "id": "MDRW2100021235.1.1",
          "form": "밤새대화하는건가요!",
          "original_form": "밤새대화하는건가요!",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021235.1.2",
          "form": "ㅋㅋ계속 하고 있긴 해요",
          "original_form": "ㅋㅋ계속 하고 있긴 해요",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021235.1.3",
          "form": "동물 키우시나요",
          "original_form": "동물 키우시나요",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021235.1.4",
          "form": "어 음 저는 키우지않아요!!!",
          "original_form": "어 음 저는 키우지않아요!!!",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021235.1.5",
          "form": "하지만 고양이키우고싶어요",
          "original_form": "하지만 고양이키우고싶어요",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021235.1.6",
          "form": "저두요. 고양이 귀엽더라구요",
          "original_form": "저두요. 고양이 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021235.1.7",
          "form": "그죠그죠",
          "original_form": "그죠그죠",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021235.1.8",
          "form": "뭔가 도도한맛",
          "original_form": "뭔가 도도한맛",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021235.1.9",
          "form": "털때문에 키우기 힘들 거 같아요",
          "original_form": "털때문에 키우기 힘들 거 같아요",
          "speaker_id": "2",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100021235.1.10",
          "form": "그건 강아지도그렇지 않을까녀??!!",
          "original_form": "그건 강아지도그렇지 않을까녀??!!",
          "speaker_id": "1",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100021235.1.11",
          "form": "그렇긴 하겠네요ㅋㅋ",
          "original_form": "그렇긴 하겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021235.1.12",
          "form": "귀여운데 키울 자신은 없더라구요",
          "original_form": "귀여운데 키울 자신은 없더라구요",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100021235.1.13",
          "form": "그죠그조",
          "original_form": "그죠그조",
          "speaker_id": "1",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100021235.1.14",
          "form": "그래서 사진으로만 보는 편이에요",
          "original_form": "그래서 사진으로만 보는 편이에요",
          "speaker_id": "2",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100021235.1.15",
          "form": "아아 저두그래여!!@",
          "original_form": "아아 저두그래여!!@",
          "speaker_id": "1",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100021235.1.16",
          "form": "인터넷짤보면서",
          "original_form": "인터넷짤보면서",
          "speaker_id": "1",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100021235.1.17",
          "form": "유튜브 영상도요ㅋㅋㅋ",
          "original_form": "유튜브 영상도요ㅋㅋㅋ",
          "speaker_id": "2",
