      "utterance": [
        {
          "id": "MDRW2100039964.1.1",
          "form": "헐 나두",
          "original_form": "헐 나두",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100039964.1.2",
          "form": "1키로그램 사서 같이 먹을까?",
          "original_form": "1키로그램 사서 같이 먹을까?",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100039964.1.3",
          "form": "그건 너무 많나?",
          "original_form": "그건 너무 많나?",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100039964.1.4",
          "form": "아냐 식구가 몇인데. 다 먹을 수 있어",
          "original_form": "아냐 식구가 몇인데. 다 먹을 수 있어",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100039964.1.5",
          "form": "회로도 먹고 연어장도 담그면 돼",
          "original_form": "회로도 먹고 연어장도 담그면 돼",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100039964.1.6",
          "form": "연어장 집에서 만들 수 있어??",
          "original_form": "연어장 집에서 만들 수 있어??",
          "speaker_id": "2",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100039964.1.7",
          "form": "그럼. 간장이랑 양파랑 끓인 다음 식혀서 연어 숙성시키면 끝이야",
          "original_form": "그럼. 간장이랑 양파랑 끓인 다음 식혀서 연어 숙성시키면 끝이야",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100039964.1.8",
          "form": "올~~ 그럼 연어장도 만들어 먹자!!",
          "original_form": "올~~ 그럼 연어장도 만들어 먹자!!",
          "speaker_id": "2",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100039964.1.9",
          "form": "그래. 얼마 전부터 계속 연어가 먹고 싶었어",
          "original_form": "그래. 얼마 전부터 계속 연어가 먹고 싶었어",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100039964.1.10",
          "form": "나두 나두~~ 난 회가 너무 먹고 싶었어ㅠㅠ",
          "original_form": "나두 나두~~ 난 회가 너무 먹고 싶었어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100039964.1.11",
          "form": "혼자 배달시켜 먹긴 좀 애매해서 참고 잇었는데",
          "original_form": "혼자 배달시켜 먹긴 좀 애매해서 참고 잇었는데",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100039964.1.12",
          "form": "엄마랑 같이 먹으면 되겠다 >.<",
          "original_form": "엄마랑 같이 먹으면 되겠다 >.<",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100039964.1.13",
          "form": "더 먹고 싶은 건 없어?",
          "original_form": "더 먹고 싶은 건 없어?",
          "speaker_id": "1",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100039964.1.14",
          "form": "해주든가 사줄게",
          "original_form": "해주든가 사줄게",
          "speaker_id": "1",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100039964.1.15",
          "form": "나 얼마 전부터 마리탕이 너무 땡겨..",
          "original_form": "나 얼마 전부터 마리탕이 너무 땡겨..",
          "speaker_id": "2",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100039964.1.16",
          "form": "근데 그건 친구들이랑 같이 먹으면 돼",
          "original_form": "근데 그건 친구들이랑 같이 먹으면 돼",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100039964.1.17",
          "form": "엄마는 짜장면을 먹고 싶다",
          "original_form": "엄마는 짜장면을 먹고 싶다",
          "speaker_id": "1",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100039964.1.18",
          "form": "헐 나두.. 꿔바로우도 먹고 싶어",
          "original_form": "헐 나두.. 꿔바로우도 먹고 싶어",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100039964.1.19",
          "form": "평일에 점심으로 같이 시켜먹자",
          "original_form": "평일에 점심으로 같이 시켜먹자",
          "speaker_id": "1",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100039964.1.20",
          "form": "오키 집 주변에 맛있는 중식당 있나 알아보겠으..",
          "original_form": "오키 집 주변에 맛있는 중식당 있나 알아보겠으..",
