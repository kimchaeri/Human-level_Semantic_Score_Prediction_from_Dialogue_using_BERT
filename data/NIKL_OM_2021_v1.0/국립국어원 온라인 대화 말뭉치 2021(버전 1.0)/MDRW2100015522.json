      "utterance": [
        {
          "id": "MDRW2100015522.1.1",
          "form": "오늘 집안일은 괜찮았나요",
          "original_form": "오늘 집안일은 괜찮았나요",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015522.1.2",
          "form": "오늘 안했어요",
          "original_form": "오늘 안했어요{emoji:♡}",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015522.1.3",
          "form": "아 상자 버리로가다가 엉덩방아 찧었어요ㅠ",
          "original_form": "아 상자 버리로가다가 엉덩방아 찧었어요ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015522.1.4",
          "form": "해방되었네요",
          "original_form": "해방되었네요",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015522.1.5",
          "form": "빗길이 미끄러워서?",
          "original_form": "빗길이 미끄러워서?",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015522.1.6",
          "form": "아 봄이를 앞에 가방 메고",
          "original_form": "아 봄이를 앞에 가방 메고",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015522.1.7",
          "form": "떨너진상자줍다다",
          "original_form": "떨너진상자줍다다",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015522.1.8",
          "form": "엉덩이가 무거워서 엉덩방앜",
          "original_form": "엉덩이가 무거워서 엉덩방앜",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015522.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015522.1.10",
          "form": "아 무게중심을 잃었군여 조심하세요",
          "original_form": "아 무게중심을 잃었군여 조심하세요",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015522.1.11",
          "form": "사실 저는 집안일을 거의 안합니다",
          "original_form": "사실 저는 집안일을 거의 안합니다",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015522.1.12",
          "form": "고생이 많으시군요",
          "original_form": "고생이 많으시군요",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015522.1.13",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015522.1.14",
          "form": "놈팽이에요 내방만 치워요",
          "original_form": "놈팽이에요 내방만 치워요",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015522.1.15",
          "form": "ㅋㅋㅋㅋ놈팽잌ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ놈팽잌ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015522.1.16",
          "form": "이제 동생은 올라가나요",
          "original_form": "이제 동생은 올라가나요",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015522.1.17",
          "form": "저도 이제 놈팽이할레요",
          "original_form": "저도 이제 놈팽이할레요",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015522.1.18",
          "form": "다음주쯤에요",
          "original_form": "다음주쯤에요",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015522.1.19",
          "form": "좋아요 마음을 내려놓으셔야합니다",
          "original_form": "좋아요 마음을 내려놓으셔야합니다",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015522.1.20",
          "form": "또 가고나면 못해준것만 생각날테니",
          "original_form": "또 가고나면 못해준것만 생각날테니",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015522.1.21",
          "form": "아직도 멀었군요",
          "original_form": "아직도 멀었군요",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015522.1.22",
          "form": "잘해주려고요ㅠㅠ휴ㅠㅠㅠ",
          "original_form": "잘해주려고요ㅠㅠ휴ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015522.1.23",
          "form": "너무피곤합니다",
          "original_form": "너무피곤합니다",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015522.1.24",
          "form": "여러분",
          "original_form": "여러분",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015522.1.25",
          "form": "맞아요 늘 지나고나면 마음아파하는 name2님",
          "original_form": "맞아요 늘 지나고나면 마음아파하는 &name2&님",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015522.1.26",
          "form": "그쵸 하지만 당시엔 너무 다들 배려없고 힘들게해요",
          "original_form": "그쵸 하지만 당시엔 너무 다들 배려없고 힘들게해요",
          "speaker_id": "2",
          "time": "20210825 23:39"
        }
      ]
