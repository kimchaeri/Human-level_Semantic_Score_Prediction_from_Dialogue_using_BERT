      "utterance": [
        {
          "id": "MDRW2100020166.1.1",
          "form": "카페인에 약해?",
          "original_form": "카페인에 약해?",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020166.1.2",
          "form": "나 카페인 엄청  약해",
          "original_form": "나 카페인 엄청  약해",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020166.1.3",
          "form": "의왠데?",
          "original_form": "의왠데?",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020166.1.4",
          "form": "의외지?",
          "original_form": "의외지?",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020166.1.5",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020166.1.6",
          "form": "가슴이 엄청 두근두근 하고 손도 떨리고 이상한 각성 상태가 돼",
          "original_form": "가슴이 엄청 두근두근 하고 손도 떨리고 이상한 각성 상태가 돼",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020166.1.7",
          "form": "나도 약간 두근거리기는 한데 손은 안떨림",
          "original_form": "나도 약간 두근거리기는 한데 손은 안떨림",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020166.1.8",
          "form": "아까 밥 먹으면서 깜짝 놀랐잖아 손 떨려서",
          "original_form": "아까 밥 먹으면서 깜짝 놀랐잖아 손 떨려서",
          "speaker_id": "1",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020166.1.9",
          "form": "나 못봤어",
          "original_form": "나 못봤어",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020166.1.10",
          "form": "보고 놀랄까봐 내가 잘 숨겼어",
          "original_form": "보고 놀랄까봐 내가 잘 숨겼어",
          "speaker_id": "1",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020166.1.11",
          "form": "그래서 커피 잘 안먹는구나",
          "original_form": "그래서 커피 잘 안먹는구나",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020166.1.12",
          "form": "응 밖에서도 디카페인 많이 팔았으면 좋겠어",
          "original_form": "응 밖에서도 디카페인 많이 팔았으면 좋겠어",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020166.1.13",
          "form": "그거 인정",
          "original_form": "그거 인정",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020166.1.14",
          "form": "어떤 날은 매운 것 잘 먹겠는데 어떤 날은 매운 거 너무 못 먹겠어",
          "original_form": "어떤 날은 매운 것 잘 먹겠는데 어떤 날은 매운 거 너무 못 먹겠어",
          "speaker_id": "1",
          "time": "20210829 00:29"
        }
      ]
