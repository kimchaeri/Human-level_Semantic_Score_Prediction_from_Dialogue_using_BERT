      "utterance": [
        {
          "id": "MDRW2100030267.1.1",
          "form": "가족들은 잘 지내나요??",
          "original_form": "가족들은 잘 지내나요??",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030267.1.2",
          "form": "잘몰라요",
          "original_form": "잘몰라요",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030267.1.3",
          "form": "엄마랑 싸우고 잘연락안해요",
          "original_form": "엄마랑 싸우고 잘연락안해요",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030267.1.4",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030267.1.5",
          "form": "곧 추석인데오",
          "original_form": "곧 추석인데오",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030267.1.6",
          "form": "시댁은 감기기운있다고 안간지 오래댔어요",
          "original_form": "시댁은 감기기운있다고 안간지 오래댔어요",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030267.1.7",
          "form": "역시 가족은 떨어져 살아야 좋은 거 같아오....",
          "original_form": "역시 가족은 떨어져 살아야 좋은 거 같아오....",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030267.1.8",
          "form": "이제 준비해야죠",
          "original_form": "이제 준비해야죠",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030267.1.9",
          "form": "아 역시 코로나가....",
          "original_form": "아 역시 코로나가....",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030267.1.10",
          "form": "가족분들은 다들 백신 맞으셨어???",
          "original_form": "가족분들은 다들 백신 맞으셨어???",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030267.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030267.1.12",
          "form": "멀리 인것같은 가까운거리",
          "original_form": "멀리 인것같은 가까운거리",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030267.1.13",
          "form": "가좋아요",
          "original_form": "가좋아요",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030267.1.14",
          "form": "부모님은 맞으신것같은데",
          "original_form": "부모님은 맞으신것같은데",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030267.1.15",
          "form": "동생들은 모르겠어요",
          "original_form": "동생들은 모르겠어요",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030267.1.16",
          "form": "ㅁㅈㅁㅈ 멀고도 가까운 게 최고....",
          "original_form": "ㅁㅈㅁㅈ 멀고도 가까운 게 최고....",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030267.1.17",
          "form": "가까운곳이어야 도움을 얻고",
          "original_form": "가까운곳이어야 도움을 얻고",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030267.1.18",
          "form": "멀어야 잔소리를 안듣고ㅋㅋㅋㄱ",
          "original_form": "멀어야 잔소리를 안듣고ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030267.1.19",
          "form": "따로 살이야 좋습니다~",
          "original_form": "따로 살이야 좋습니다~",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030267.1.20",
          "form": "맞아요!!@",
          "original_form": "맞아요!!@",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030267.1.21",
          "form": "가족간에도 거리감이 필요하죠",
          "original_form": "가족간에도 거리감이 필요하죠",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030267.1.22",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030267.1.23",
          "form": "옳소~~~~~~~~~~",
          "original_form": "옳소~~~~~~~~~~",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030267.1.24",
          "form": "멀수록 가족을 더챙기게되는것같습니다",
          "original_form": "멀수록 가족을 더챙기게되는것같습니다",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030267.1.25",
