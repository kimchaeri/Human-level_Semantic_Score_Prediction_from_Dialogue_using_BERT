      "utterance": [
        {
          "id": "MDRW2100015420.1.1",
          "form": "저도 많이 안가봐서 경복궁 창덕궁 남산타워??",
          "original_form": "저도 많이 안가봐서 경복궁 창덕궁 남산타워??",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015420.1.2",
          "form": "ㅋㅋㅋㅋ좋은곳있나요?",
          "original_form": "ㅋㅋㅋㅋ좋은곳있나요?",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015420.1.3",
          "form": "유명한 데는 다 가봤어요 ㅜㅜ",
          "original_form": "유명한 데는 다 가봤어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015420.1.4",
          "form": "궁궐 야간개장은 진짜 너무 좋더라구요",
          "original_form": "궁궐 야간개장은 진짜 너무 좋더라구요",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015420.1.5",
          "form": "가보셨어요 ?",
          "original_form": "가보셨어요 ?",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015420.1.6",
          "form": "아 진짜요? 어디가 제일 좋았어요?",
          "original_form": "아 진짜요? 어디가 제일 좋았어요?",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015420.1.7",
          "form": "네 저 야간개장",
          "original_form": "네 저 야간개장",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015420.1.8",
          "form": "갔어요!!",
          "original_form": "갔어요!!",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015420.1.9",
          "form": "창 덕궁이요 ㅋㅋ",
          "original_form": "창 덕궁이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015420.1.10",
          "form": "경복궁 너무 좋았어요. 거기서 국악공연 하는데 와..!",
          "original_form": "경복궁 너무 좋았어요. 거기서 국악공연 하는데 와..!",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015420.1.11",
          "form": "창덕궁 덕수궁도 아름다워요 ㅜㅜ",
          "original_form": "창덕궁 덕수궁도 아름다워요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015420.1.12",
          "form": "와 그렇구나",
          "original_form": "와 그렇구나",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015420.1.13",
          "form": "저도 엽서도 받고",
          "original_form": "저도 엽서도 받고",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015420.1.14",
          "form": "그뭐지 창소리",
          "original_form": "그뭐지 창소리",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015420.1.15",
          "form": "내는것도 들었네요 ㅋㅋ",
          "original_form": "내는것도 들었네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015420.1.16",
          "form": "판소리요?",
          "original_form": "판소리요?",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015420.1.17",
          "form": "아 창 소리? 챙~?",
          "original_form": "아 창 소리? 챙~?",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015420.1.18",
          "form": "헐 맞아요!!!!!!!!!",
          "original_form": "헐 맞아요!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015420.1.19",
          "form": "판소리요",
          "original_form": "판소리요",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015420.1.20",
          "form": "판소리요?",
          "original_form": "판소리요?",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015420.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015420.1.22",
          "form": "그뭐지 심청이!!",
          "original_form": "그뭐지 심청이!!",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015420.1.23",
          "form": "그것도 국악의 일부이긴 하죠",
          "original_form": "그것도 국악의 일부이긴 하죠",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015420.1.24",
          "form": "들었네요",
          "original_form": "들었네요",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015420.1.25",
          "form": "요즘 코로나 때문에 그런 공연도 못보니 너무 삶이 재미없어요",
          "original_form": "요즘 코로나 때문에 그런 공연도 못보니 너무 삶이 재미없어요",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015420.1.26",
          "form": "진짜 창소리하시는분 엄청 이쁘더라구요 ㅋㅋ",
          "original_form": "진짜 창소리하시는분 엄청 이쁘더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015420.1.27",
          "form": "작년엔 청계천 등 축제도 취소 됐던 것 같은데 ㅜㅜ",
          "original_form": "작년엔 청계천 등 축제도 취소 됐던 것 같은데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015420.1.28",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015420.1.29",
          "form": "진짜 국내여행도 못가고",
          "original_form": "진짜 국내여행도 못가고",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015420.1.30",
          "form": "차 없어서 더 서러워요",
          "original_form": "차 없어서 더 서러워요",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015420.1.31",
          "form": "제주도나 거제도 강릉바다 이런데 가고싶었는데 ㅋㅋㅋㅋ",
          "original_form": "제주도나 거제도 강릉바다 이런데 가고싶었는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015420.1.32",
          "form": "면허는 있으세요?",
          "original_form": "면허는 있으세요?",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015420.1.33",
          "form": "네 면허 있어요 !",
          "original_form": "네 면허 있어요 !",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015420.1.34",
          "form": "근데 남의차 박살 낼까봐 좀 무서워요",
          "original_form": "근데 남의차 박살 낼까봐 좀 무서워요",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015420.1.35",
          "form": "아하 맞아요 저도 그게 걱정이네요",
          "original_form": "아하 맞아요 저도 그게 걱정이네요",
          "speaker_id": "2",
          "time": "20210825 23:04"
        }
      ]
