        },
        {
          "id": "MDRW2100041480.1.2",
          "form": "나 오늘 현대백화점   갔다왔어",
          "original_form": "나 오늘 현대백화점   갔다왔어",
          "speaker_id": "1",
          "time": "20210906 01:12"
        },
        {
          "id": "MDRW2100041480.1.3",
          "form": "가서 뭐했어",
          "original_form": "가서 뭐했어",
          "speaker_id": "2",
          "time": "20210906 01:12"
        },
        {
          "id": "MDRW2100041480.1.4",
          "form": "면세점을 들르러 갔어",
          "original_form": "면세점을 들르러 갔어",
          "speaker_id": "1",
          "time": "20210906 01:12"
        },
        {
          "id": "MDRW2100041480.1.5",
          "form": "뭐야 너 어디 여행가니",
          "original_form": "뭐야 너 어디 여행가니",
          "speaker_id": "2",
          "time": "20210906 01:12"
        },
        {
          "id": "MDRW2100041480.1.6",
          "form": "그랬으면 좋겠다",
          "original_form": "그랬으면 좋겠다",
          "speaker_id": "1",
          "time": "20210906 01:12"
        },
        {
          "id": "MDRW2100041480.1.7",
          "form": "그게 아니라면 어 왜 갔는데",
          "original_form": "그게 아니라면 어 왜 갔는데",
          "speaker_id": "2",
          "time": "20210906 01:12"
        },
        {
          "id": "MDRW2100041480.1.8",
          "form": "내가 대한항공에서 받을 게 좀 있어서",
          "original_form": "내가 대한항공에서 받을 게 좀 있어서",
          "speaker_id": "1",
          "time": "20210906 01:12"
        },
        {
          "id": "MDRW2100041480.1.9",
          "form": "우와 좋았겠다",
          "original_form": "우와 좋았겠다",
          "speaker_id": "2",
          "time": "20210906 01:13"
        },
        {
          "id": "MDRW2100041480.1.10",
          "form": "현대백화점 면세점 만원 쿠폰을 받았어",
          "original_form": "현대백화점 면세점 만원 쿠폰을 받았어",
          "speaker_id": "1",
          "time": "20210906 01:13"
        },
        {
          "id": "MDRW2100041480.1.11",
          "form": "여행가면 꼭 써야겠네",
          "original_form": "여행가면 꼭 써야겠네",
          "speaker_id": "2",
          "time": "20210906 01:13"
        },
        {
          "id": "MDRW2100041480.1.12",
          "form": "일년 내로 코로나가 멀쩡해질지는 모르겠는데",
          "original_form": "일년 내로 코로나가 멀쩡해질지는 모르겠는데",
          "speaker_id": "1",
          "time": "20210906 01:13"
        },
        {
          "id": "MDRW2100041480.1.13",
          "form": "그러게 해외를 일년 안에 갈 수 있으려나",
          "original_form": "그러게 해외를 일년 안에 갈 수 있으려나",
          "speaker_id": "2",
          "time": "20210906 01:13"
        },
        {
          "id": "MDRW2100041480.1.14",
          "form": "그래도 백신을 많이 받고 있으니 괜찮지 않을까",
          "original_form": "그래도 백신을 많이 받고 있으니 괜찮지 않을까",
          "speaker_id": "1",
          "time": "20210906 01:13"
        },
        {
          "id": "MDRW2100041480.1.15",
          "form": "맞아 분명 그렇게 될 거야",
          "original_form": "맞아 분명 그렇게 될 거야",
          "speaker_id": "2",
