      "utterance": [
        {
          "id": "MDRW2100016482.1.1",
          "form": "자신들은 뭐 합법적 소비자다 이거죠",
          "original_form": "자신들은 뭐 합법적 소비자다 이거죠",
          "speaker_id": "2",
          "time": "20210826 13:07"
        },
        {
          "id": "MDRW2100016482.1.2",
          "form": "여자를 나노단위로 나눠서 평가하더라구요",
          "original_form": "여자를 나노단위로 나눠서 평가하더라구요",
          "speaker_id": "1",
          "time": "20210826 13:07"
        },
        {
          "id": "MDRW2100016482.1.3",
          "form": "역겨워요 진짜",
          "original_form": "역겨워요 진짜",
          "speaker_id": "2",
          "time": "20210826 13:07"
        },
        {
          "id": "MDRW2100016482.1.4",
          "form": "그런사상이라 세상이 어휴",
          "original_form": "그런사상이라 세상이 어휴",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016482.1.5",
          "form": "소비-판매 그 도식을",
          "original_form": "소비-판매 그 도식을",
          "speaker_id": "1",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016482.1.6",
          "form": "어떻게 인간한테 적용하는지 모르겟어요",
          "original_form": "어떻게 인간한테 적용하는지 모르겟어요",
          "speaker_id": "1",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016482.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016482.1.8",
          "form": "같은 인격체가 아니라는거죠",
          "original_form": "같은 인격체가 아니라는거죠",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016482.1.9",
          "form": "그런주제에 피해는 다 받았단식으로",
          "original_form": "그런주제에 피해는 다 받았단식으로",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016482.1.10",
          "form": "무슨 가축을 사고팔듯..",
          "original_form": "무슨 가축을 사고팔듯..",
          "speaker_id": "1",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016482.1.11",
          "form": "굴땐 진짜 역겨워요",
          "original_form": "굴땐 진짜 역겨워요",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016482.1.12",
          "form": "피해 대체 뭐 받았나 궁금해요",
          "original_form": "피해 대체 뭐 받았나 궁금해요",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016482.1.13",
          "form": "저 얼마전에 운전면허 땄는데",
          "original_form": "저 얼마전에 운전면허 땄는데",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016482.1.14",
          "form": "여성ㅇㅇ 붙인것들만 보면",
          "original_form": "여성ㅇㅇ 붙인것들만 보면",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016482.1.15",
          "form": "발악하는 꼴이",
          "original_form": "발악하는 꼴이",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016482.1.16",
          "form": "오 축하드려요!",
          "original_form": "오 축하드려요!",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016482.1.17",
          "form": "운전 연수 비용도 비싸잖아요\r\n근데 군대가면 군대에서 운전병 무제한 연수해주고..",
          "original_form": "운전 연수 비용도 비싸잖아요\r\n근데 군대가면 군대에서 운전병 무제한 연수해주고..",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016482.1.18",
          "form": "부럽습니다..",
          "original_form": "부럽습니다..",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016482.1.19",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016482.1.20",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016482.1.21",
          "form": "이런식으로 혜택이란 혜택은 다 가져가네요",
          "original_form": "이런식으로 혜택이란 혜택은 다 가져가네요",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016482.1.22",
          "form": "운전병으로 한번 군대다녀오면",
          "original_form": "운전병으로 한번 군대다녀오면",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016482.1.23",
          "form": "운전 엄청 잘하게 되어서 나오고",
          "original_form": "운전 엄청 잘하게 되어서 나오고",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016482.1.24",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016482.1.25",
          "form": "운전하는 일자리 구하기 쉬워요",
          "original_form": "운전하는 일자리 구하기 쉬워요",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016482.1.26",
          "form": "아 그것도 생각나요",
          "original_form": "아 그것도 생각나요",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016482.1.27",
          "form": "경찰 공무원 되려면 1종 따야하는거요",
          "original_form": "경찰 공무원 되려면 1종 따야하는거요",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016482.1.28",
          "form": "아 맞아요 맞아요",
          "original_form": "아 맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016482.1.29",
          "form": "경찰 잘 모르지만",
          "original_form": "경찰 잘 모르지만",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016482.1.30",
          "form": "순찰차 타고다니지않나요?",
          "original_form": "순찰차 타고다니지않나요?",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016482.1.31",
          "form": "저도 경찰 잘 몰라섴ㅋㅋ",
          "original_form": "저도 경찰 잘 몰라섴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016482.1.32",
          "form": "하긴ㅋㅋㅋ 그렇네요",
          "original_form": "하긴ㅋㅋㅋ 그렇네요",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016482.1.33",
          "form": "어쨌든 공공연하게 운전면허가 필요한 직업이 많은데",
          "original_form": "어쨌든 공공연하게 운전면허가 필요한 직업이 많은데",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016482.1.34",
          "form": "지원해준다니 진짜 부럽네요",
          "original_form": "지원해준다니 진짜 부럽네요",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016482.1.35",
          "form": "아 운전면허를 따야 운전병이 될 수 있는데",
          "original_form": "아 운전면허를 따야 운전병이 될 수 있는데",
          "speaker_id": "1",
          "time": "20210826 13:14"
        },
        {
          "id": "MDRW2100016482.1.36",
          "form": "군대에 운전병으로 들어가면 운전 연수를 시켜줘요",
          "original_form": "군대에 운전병으로 들어가면 운전 연수를 시켜줘요",
          "speaker_id": "1",
          "time": "20210826 13:14"
        },
        {
          "id": "MDRW2100016482.1.37",
          "form": "장롱면허여도 운전이 능숙해지는..",
          "original_form": "장롱면허여도 운전이 능숙해지는..",
          "speaker_id": "1",
          "time": "20210826 13:14"
        },
        {
          "id": "MDRW2100016482.1.38",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016482.1.39",
          "form": "직업훈련을 시켜주는 느낌이엿어ㅓ요",
          "original_form": "직업훈련을 시켜주는 느낌이엿어ㅓ요",
          "speaker_id": "1",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016482.1.40",
          "form": "군대도 들어간 사람들 핸드폰 다 하더라구요",
          "original_form": "군대도 들어간 사람들 핸드폰 다 하더라구요",
          "speaker_id": "2",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016482.1.41",
          "form": "맞아요 핸드폰 요새 다하고",
          "original_form": "맞아요 핸드폰 요새 다하고",
          "speaker_id": "1",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016482.1.42",
          "form": "어디가서 이런말 하면 욕먹겠지만요",
          "original_form": "어디가서 이런말 하면 욕먹겠지만요",
          "speaker_id": "2",
          "time": "20210826 13:15"
        }
      ]
    }
