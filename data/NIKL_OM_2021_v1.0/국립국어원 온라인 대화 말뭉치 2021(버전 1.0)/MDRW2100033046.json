        {
          "id": "MDRW2100033046.1.2",
          "form": "그 분",
          "original_form": "그 분",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100033046.1.3",
          "form": "넵넵",
          "original_form": "넵넵",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100033046.1.4",
          "form": "돌아가셔서 안타깝기는 하지만",
          "original_form": "돌아가셔서 안타깝기는 하지만",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100033046.1.5",
          "form": "아 진짜 너무 이기적인거아닌가요?",
          "original_form": "아 진짜 너무 이기적인거아닌가요?",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100033046.1.6",
          "form": "애초에 여자인 기분이라는게 뭔지 ㅋㅋㅋ",
          "original_form": "애초에 여자인 기분이라는게 뭔지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100033046.1.7",
          "form": "죽었나요?",
          "original_form": "죽었나요?",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100033046.1.8",
          "form": "진짜 말이 안돼요",
          "original_form": "진짜 말이 안돼요",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100033046.1.9",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100033046.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100033046.1.11",
          "form": "자 살 하셨어요",
          "original_form": "자 살 하셨어요",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100033046.1.12",
          "form": "억울해서요",
          "original_form": "억울해서요",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100033046.1.13",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100033046.1.14",
          "form": "여자로 인정 안해준다고;",
          "original_form": "여자로 인정 안해준다고;",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100033046.1.15",
          "form": "여자취급안해줘서요?",
          "original_form": "여자취급안해줘서요?",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100033046.1.16",
          "form": "아니근데 트젠들 진짜",
          "original_form": "아니근데 트젠들 진짜",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100033046.1.17",
          "form": "이기적인게",
          "original_form": "이기적인게",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100033046.1.18",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100033046.1.19",
          "form": "아니 그러면서",
          "original_form": "아니 그러면서",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100033046.1.20",
          "form": "아니 입장바꿔서생각이안되나",
          "original_form": "아니 입장바꿔서생각이안되나",
          "speaker_id": "2",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100033046.1.21",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100033046.1.22",
          "form": "그 미국에서도",
          "original_form": "그 미국에서도",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100033046.1.23",
          "form": "수술 다 안한 mtf가",
          "original_form": "수술 다 안한 mtf가",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100033046.1.24",
          "form": "자꾸 여자 욕탕에 들어와서",
          "original_form": "자꾸 여자 욕탕에 들어와서",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100033046.1.25",
          "form": "아 이거 우리나라였나...?",
          "original_form": "아 이거 우리나라였나...?",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100033046.1.26",
          "form": "아 미쳤어 정말",
          "original_form": "아 미쳤어 정말",
