      "utterance": [
        {
          "id": "MDRW2100037259.1.1",
          "form": "비와서 그런듯",
          "original_form": "비와서 그런듯",
          "speaker_id": "2",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100037259.1.2",
          "form": "넘 추워서 겨울옷 빼입고 싶어짐",
          "original_form": "넘 추워서 겨울옷 빼입고 싶어짐",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100037259.1.3",
          "form": "그정도는 아닌게",
          "original_form": "그정도는 아닌게",
          "speaker_id": "2",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100037259.1.4",
          "form": "아닌데",
          "original_form": "아닌데",
          "speaker_id": "2",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100037259.1.5",
          "form": "맞다 ㅠㅠ엄마 나가봐유",
          "original_form": "맞다 ㅠㅠ엄마 나가봐유",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100037259.1.6",
          "form": "추운가",
          "original_form": "추운가",
          "speaker_id": "2",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100037259.1.7",
          "form": "웅웅 롱패딩 입고있으면 딱 맞을듯",
          "original_form": "웅웅 롱패딩 입고있으면 딱 맞을듯",
          "speaker_id": "1",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100037259.1.8",
          "form": "ㅋㅋ 온도가 많이 낮아졌나보네",
          "original_form": "ㅋㅋ 온도가 많이 낮아졌나보네",
          "speaker_id": "2",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100037259.1.9",
          "form": "그런듯 ㅠㅠ언제까지 비오는지",
          "original_form": "그런듯 ㅠㅠ언제까지 비오는지",
          "speaker_id": "1",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100037259.1.10",
          "form": "이번주 계속 내릴껄",
          "original_form": "이번주 계속 내릴껄",
          "speaker_id": "2",
          "time": "20210831 22:00"
        },
        {
