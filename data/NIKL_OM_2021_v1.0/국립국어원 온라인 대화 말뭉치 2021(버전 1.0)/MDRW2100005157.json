      "utterance": [
        {
          "id": "MDRW2100005157.1.1",
          "form": "안녕하세요. 혹시 재택근무 하시나요.",
          "original_form": "안녕하세요. 혹시 재택근무 하시나요.",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005157.1.2",
          "form": "재택근무 하고파요",
          "original_form": "재택근무 하고파요",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005157.1.3",
          "form": "저두요. 계속 버스타고 다니기 불안하네요.",
          "original_form": "저두요. 계속 버스타고 다니기 불안하네요.",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005157.1.4",
          "form": "작년에 잠깐 한적 있었는데 편하긴 하더라구요",
          "original_form": "작년에 잠깐 한적 있었는데 편하긴 하더라구요",
          "speaker_id": "1",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005157.1.5",
          "form": "우와 재택근무를 해보셨다니",
          "original_form": "우와 재택근무를 해보셨다니",
          "speaker_id": "2",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005157.1.6",
          "form": "코로나 끝나면 재택근무 시켜주려나요,",
          "original_form": "코로나 끝나면 재택근무 시켜주려나요,",
          "speaker_id": "2",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005157.1.7",
          "form": "끝나면 더 안시켜주지 않을까요 ㅠㅠ 또했음 좋겠는데",
          "original_form": "끝나면 더 안시켜주지 않을까요 ㅠㅠ 또했음 좋겠는데",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005157.1.8",
          "form": "확실히 효율성은 떨어지나요?",
          "original_form": "확실히 효율성은 떨어지나요?",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005157.1.9",
          "form": "네 아무래도 그래요 ㅋㅋㅋ 저는 좀 방탕하게 사는편이라 더그런거같아요",
          "original_form": "네 아무래도 그래요 ㅋㅋㅋ 저는 좀 방탕하게 사는편이라 더그런거같아요",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005157.1.10",
          "form": "오 욜로신가요?",
          "original_form": "오 욜로신가요?",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005157.1.11",
          "form": "최근까진 그랬어요. 집값을 보기전까진",
          "original_form": "최근까진 그랬어요. 집값을 보기전까진{emoji:…}",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005157.1.12",
          "form": "헉....집값을ㅈ보고 더 욜로되는건 아닌가유",
          "original_form": "헉....집값을ㅈ보고 더 욜로되는건 아닌가유",
          "speaker_id": "2",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005157.1.13",
          "form": "아니면 재택투잡?",
          "original_form": "아니면 재택투잡?",
          "speaker_id": "2",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005157.1.14",
          "form": "아뇨 일단 아끼는것부터요",
          "original_form": "아뇨 일단 아끼는것부터요",
          "speaker_id": "1",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005157.1.15",
          "form": "투잡도 해볼까 하는데 아직 못구했어요",
          "original_form": "투잡도 해볼까 하는데 아직 못구했어요",
          "speaker_id": "1",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005157.1.16",
          "form": "재택 가능한 일 있으면 저도 하고싶네요",
          "original_form": "재택 가능한 일 있으면 저도 하고싶네요",
          "speaker_id": "2",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005157.1.17",
          "form": "전 프리랜서 친구한테 일거리있으면 소개시켜달라고 했는데 아직 소식이없네요",
          "original_form": "전 프리랜서 친구한테 일거리있으면 소개시켜달라고 했는데 아직 소식이없네요",
          "speaker_id": "1",
          "time": "20210821 21:22"
        }
