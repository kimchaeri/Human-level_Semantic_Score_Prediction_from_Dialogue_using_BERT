      "utterance": [
        {
          "id": "MDRW2100045743.1.1",
          "form": "멍게는 어떤 맛이예요?",
          "original_form": "멍게는 어떤 맛이예요?",
          "speaker_id": "2",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045743.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아니에요 호불호갈리는음식은 보통 정해져잇길래 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아니에요 호불호갈리는음식은 보통 정해져잇길래 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045743.1.3",
          "form": "굴보다 더 비린맛이에요 저한테는 ㅋㅋㅋㅋㅋ",
          "original_form": "굴보다 더 비린맛이에요 저한테는 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100045743.1.4",
          "form": "오우...으어우....진짜 비린가보네요",
          "original_form": "오우...으어우....진짜 비린가보네요",
          "speaker_id": "2",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045743.1.5",
          "form": "전 생굴만 해도 너무 비리던데",
          "original_form": "전 생굴만 해도 너무 비리던데",
          "speaker_id": "2",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045743.1.6",
          "form": "멍게 식감은 어때요? 대체 어딜 먹는 건가요",
          "original_form": "멍게 식감은 어때요? 대체 어딜 먹는 건가요",
          "speaker_id": "2",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045743.1.7",
          "form": "굴같아요 물렁물렁",
          "original_form": "굴같아요 물렁물렁",
          "speaker_id": "1",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045743.1.8",
          "form": "우욱 안 먹을래요",
          "original_form": "우욱 안 먹을래요",
          "speaker_id": "2",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045743.1.9",
          "form": "ㅋㅋㅋㅋㅌㅌㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅌㅌㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045743.1.10",
          "form": "굴도 물컹한데 멍게마저 그러다니",
          "original_form": "굴도 물컹한데 멍게마저 그러다니",
          "speaker_id": "2",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045743.1.11",
          "form": "이단의 무리다!",
          "original_form": "이단의 무리다!",
          "speaker_id": "2",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045743.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ네 호불호가 완전 갈릴 맛과식감이에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ네 호불호가 완전 갈릴 맛과식감이에요",
          "speaker_id": "1",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045743.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ이번에는 뭘 맞추기할까요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ이번에는 뭘 맞추기할까요",
          "speaker_id": "1",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045743.1.14",
          "form": "어제먹은 점심메뉴?",
          "original_form": "어제먹은 점심메뉴?",
          "speaker_id": "1",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045743.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ가물가물한디 좋아요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ가물가물한디 좋아요!",
          "speaker_id": "2",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045743.1.16",
          "form": "제가 먼저 물어볼게요",
          "original_form": "제가 먼저 물어볼게요",
          "speaker_id": "2",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045743.1.17",
          "form": "네 좋아요",
          "original_form": "네 좋아요",
          "speaker_id": "1",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045743.1.18",
          "form": "한식이예요?",
          "original_form": "한식이예요?",
          "speaker_id": "2",
          "time": "20210908 06:39"
        },
        {
          "id": "MDRW2100045743.1.19",
          "form": "아니요 양식이에여",
          "original_form": "아니요 양식이에여",
          "speaker_id": "1",
          "time": "20210908 06:39"
        },
        {
          "id": "MDRW2100045743.1.20",
          "form": "조리할 때 기름이 들어갔나요?",
          "original_form": "조리할 때 기름이 들어갔나요?",
          "speaker_id": "2",
          "time": "20210908 06:39"
        },
        {
          "id": "MDRW2100045743.1.21",
          "form": "안들어갓어요",
          "original_form": "안들어갓어요",
          "speaker_id": "1",
          "time": "20210908 06:40"
        },
        {
          "id": "MDRW2100045743.1.22",
          "form": "야채가 들어가나요?",
          "original_form": "야채가 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045743.1.23",
          "form": "네 들어가요",
          "original_form": "네 들어가요",
          "speaker_id": "1",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045743.1.24",
          "form": "생야채가 들어가나요?",
          "original_form": "생야채가 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045743.1.25",
          "form": "네 생야채가 들어가요",
          "original_form": "네 생야채가 들어가요",
          "speaker_id": "1",
          "time": "20210908 06:45"
        },
        {
          "id": "MDRW2100045743.1.26",
          "form": "야채 들어가요",
          "original_form": "야채 들어가요",
          "speaker_id": "1",
          "time": "20210908 06:46"
        }
      ]
    }
