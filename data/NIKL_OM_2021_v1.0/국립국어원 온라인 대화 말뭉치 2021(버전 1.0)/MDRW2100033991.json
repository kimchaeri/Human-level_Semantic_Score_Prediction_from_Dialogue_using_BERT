      "utterance": [
        {
          "id": "MDRW2100033991.1.1",
          "form": "보면 알껀데 마크 월버그",
          "original_form": "보면 알껀데 마크 월버그",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100033991.1.2",
          "form": "잠시만 보고 오께",
          "original_form": "잠시만 보고 오께",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100033991.1.3",
          "form": "프랜스포머에 나오는 새로 바뀐 주인공",
          "original_form": "프랜스포머에 나오는 새로 바뀐 주인공",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100033991.1.4",
          "form": "봐도 모르겠다ㅋ",
          "original_form": "봐도 모르겠다ㅋ",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100033991.1.5",
          "form": "여기도 넷플렉스 완전 상위는 다 넷플릭스네",
          "original_form": "여기도 넷플렉스 완전 상위는 다 넷플릭스네",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100033991.1.6",
          "form": "코로나로 개봉을 안히니 어쩔수 없네",
          "original_form": "코로나로 개봉을 안히니 어쩔수 없네",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100033991.1.7",
          "form": "퍼펙트 스톰 영화 봤나 거기 나오는데",
          "original_form": "퍼펙트 스톰 영화 봤나 거기 나오는데",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100033991.1.8",
          "form": "영화제목은 들어봤는데... 다음 순위는?",
          "original_form": "영화제목은 들어봤는데... 다음 순위는?",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100033991.1.9",
          "form": "세상에 또 넷플릭스다",
          "original_form": "세상에 또 넷플릭스다",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100033991.1.10",
          "form": "그냥 다  넷플 영화구만",
          "original_form": "그냥 다  넷플 영화구만",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100033991.1.11",
          "form": "레오나르도 디카프리오 3000만",
          "original_form": "레오나르도 디카프리오 3000만",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100033991.1.12",
          "form": "디카프리오도 넷플 영화 찍었군",
          "original_form": "디카프리오도 넷플 영화 찍었군",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100033991.1.13",
          "form": "오 드디어 나왔다 3위 덴젤 워싱턴 4000만",
          "original_form": "오 드디어 나왔다 3위 덴젤 워싱턴 4000만",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100033991.1.14",
          "form": "와 많이 받구나.  근데 아카데미 못 받았지",
          "original_form": "와 많이 받구나.  근데 아카데미 못 받았지",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100033991.1.15",
          "form": "4000만이면 한 450억 정도는 되겠다 그쟈?",
          "original_form": "4000만이면 한 450억 정도는 되겠다 그쟈?",
