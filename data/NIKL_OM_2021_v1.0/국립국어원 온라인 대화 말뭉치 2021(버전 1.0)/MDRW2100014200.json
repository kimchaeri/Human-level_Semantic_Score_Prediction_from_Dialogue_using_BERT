      "utterance": [
        {
          "id": "MDRW2100014200.1.1",
          "form": "드라마 보시나요?",
          "original_form": "드라마 보시나요?",
          "speaker_id": "1",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014200.1.2",
          "form": "네 초반에 보고 재미있으면 계속봐요",
          "original_form": "네 초반에 보고 재미있으면 계속봐요",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014200.1.3",
          "form": "어떤거 보세요?",
          "original_form": "어떤거 보세요?",
          "speaker_id": "1",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014200.1.4",
          "form": "요즘은 슬의생이요\n그거 보시나요?",
          "original_form": "요즘은 슬의생이요\n그거 보시나요?",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014200.1.5",
          "form": "네 저도 슬의생 챙겨보고 있어요ㅋㅋ",
          "original_form": "네 저도 슬의생 챙겨보고 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014200.1.6",
          "form": "요즘 그거안보는 사람 진짜 거의 없네요",
          "original_form": "요즘 그거안보는 사람 진짜 거의 없네요",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014200.1.7",
          "form": "ㅋㅋ제주변도요ㅎㅎ보면볼수록 재밌는거 같아요ㅋㅋ",
          "original_form": "ㅋㅋ제주변도요ㅎㅎ보면볼수록 재밌는거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014200.1.8",
          "form": "일주일에 한번하는게 아쉬워요",
          "original_form": "일주일에 한번하는게 아쉬워요",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014200.1.9",
          "form": "맞아요 2번은 했으면 좋겠는데ㅜㅜ",
          "original_form": "맞아요 2번은 했으면 좋겠는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014200.1.10",
          "form": "벌써 내일 10회네요ㅎㅎ",
          "original_form": "벌써 내일 10회네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014200.1.11",
          "form": "거의끝났네요 이제ㅠ",
          "original_form": "거의끝났네요 이제ㅠ",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014200.1.12",
          "form": "시즌2를 일년을 기다렷는데 흑",
          "original_form": "시즌2를 일년을 기다렷는데 흑",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014200.1.13",
          "form": "시즌3도 할까요?ㅎㅎ",
          "original_form": "시즌3도 할까요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014200.1.14",
          "form": "글세요 근데 하더라도 또 일년 기다려야하나요 ㄷㄷ",
          "original_form": "글세요 근데 하더라도 또 일년 기다려야하나요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014200.1.15",
          "form": "ㄷㄷ일년이라니ㅜ 유튜브에서 우리동네산부인과라는 채널에서 슬의생 리뷰 해주는데 그거도 재밌어요ㅋㅋ",
          "original_form": "ㄷㄷ일년이라니ㅜ 유튜브에서 우리동네산부인과라는 채널에서 슬의생 리뷰 해주는데 그거도 재밌어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014200.1.16",
          "form": "앗 저도 그거보는데ㅋㅋㅋㅋ",
          "original_form": "앗 저도 그거보는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014200.1.17",
          "form": "우리동네 산부인과 채널 재밌죠?ㅋㅋㅋㅋ",
          "original_form": "우리동네 산부인과 채널 재밌죠?ㅋㅋㅋㅋ",
