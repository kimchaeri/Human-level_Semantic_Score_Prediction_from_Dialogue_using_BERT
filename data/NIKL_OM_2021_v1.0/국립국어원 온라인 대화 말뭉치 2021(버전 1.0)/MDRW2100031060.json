      "utterance": [
        {
          "id": "MDRW2100031060.1.1",
          "form": "요즘 밥 챙겨먹는게 너무 귀찮아서 그냥 간단하게 식사 대체되는 식품이 나왔음 좋겠다",
          "original_form": "요즘 밥 챙겨먹는게 너무 귀찮아서 그냥 간단하게 식사 대체되는 식품이 나왔음 좋겠다",
          "speaker_id": "2",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100031060.1.2",
          "form": "근데 맛도 있엇음 좋겟어ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 맛도 있엇음 좋겟어ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:02"
        },
        {
          "id": "MDRW2100031060.1.3",
          "form": "나 아침 못먹을 때 편의점에서 뭐 비건라떼인가 뭔가 사먹는데",
          "original_form": "나 아침 못먹을 때 편의점에서 뭐 비건라떼인가 뭔가 사먹는데",
          "speaker_id": "1",
          "time": "20210904 16:02"
        },
        {
          "id": "MDRW2100031060.1.4",
          "form": "빈 속을 채워주더라",
          "original_form": "빈 속을 채워주더라",
          "speaker_id": "1",
          "time": "20210904 16:02"
        },
        {
          "id": "MDRW2100031060.1.5",
          "form": "곡물라떼였나",
          "original_form": "곡물라떼였나",
          "speaker_id": "1",
          "time": "20210904 16:02"
        },
        {
          "id": "MDRW2100031060.1.6",
          "form": "비건라떼는 뭐야?",
          "original_form": "비건라떼는 뭐야?",
          "speaker_id": "2",
          "time": "20210904 16:02"
        },
        {
          "id": "MDRW2100031060.1.7",
          "form": "비건인데 어떻게 라떼가 들어가지 라떼는 우유 아님?",
          "original_form": "비건인데 어떻게 라떼가 들어가지 라떼는 우유 아님?",
          "speaker_id": "2",
          "time": "20210904 16:02"
        },
        {
          "id": "MDRW2100031060.1.8",
          "form": "고기 안들어간건가봐",
          "original_form": "고기 안들어간건가봐",
          "speaker_id": "1",
          "time": "20210904 16:02"
        },
        {
          "id": "MDRW2100031060.1.9",
          "form": "띠용 유제품은 괜찮은건가",
          "original_form": "띠용 유제품은 괜찮은건가",
          "speaker_id": "2",
          "time": "20210904 16:02"
        },
        {
          "id": "MDRW2100031060.1.10",
          "form": "그냥 곡물갈아서 우유랑 섞은거 아닌가",
          "original_form": "그냥 곡물갈아서 우유랑 섞은거 아닌가",
          "speaker_id": "1",
          "time": "20210904 16:02"
        },
        {
          "id": "MDRW2100031060.1.11",
          "form": "모르겠어",
          "original_form": "모르겠어",
          "speaker_id": "1",
          "time": "20210904 16:02"
        },
        {
          "id": "MDRW2100031060.1.12",
          "form": "그런것도 비건라떼인가? 비건식음료의 세계란..",
          "original_form": "그런것도 비건라떼인가? 비건식음료의 세계란..",
          "speaker_id": "2",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100031060.1.13",
          "form": "음료의 세계란~~",
          "original_form": "음료의 세계란~~",
          "speaker_id": "1",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100031060.1.14",
          "form": "근데 난 마시는 것도 귀찮아 그냥 사탕처럼 간단한 식품을 원해",
          "original_form": "근데 난 마시는 것도 귀찮아 그냥 사탕처럼 간단한 식품을 원해",
          "speaker_id": "2",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100031060.1.15",
          "form": "이번에 차 산거 잘 먹구 계십니까",
          "original_form": "이번에 차 산거 잘 먹구 계십니까",
          "speaker_id": "1",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100031060.1.16",
          "form": "아 도밍고 꿀홍차 그거",
          "original_form": "아 도밍고 꿀홍차 그거",
          "speaker_id": "2",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100031060.1.17",
          "form": "우유 냉침하니까 맛잇음",
          "original_form": "우유 냉침하니까 맛잇음",
          "speaker_id": "2",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100031060.1.18",
          "form": "맞아 맞아",
          "original_form": "맞아 맞아",
          "speaker_id": "1",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100031060.1.19",
          "form": "근데 넘 오래 담아놓으면 쓰다",
          "original_form": "근데 넘 오래 담아놓으면 쓰다",
          "speaker_id": "2",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100031060.1.20",
          "form": "나도 생각나서 지금 민트 만들어 먹어야겠다",
          "original_form": "나도 생각나서 지금 민트 만들어 먹어야겠다",
          "speaker_id": "1",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100031060.1.21",
          "form": "사이다 냉침도 된다고 써있던데 이건 무서워서 안해봄",
          "original_form": "사이다 냉침도 된다고 써있던데 이건 무서워서 안해봄",
          "speaker_id": "2",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100031060.1.22",
          "form": "민트도 도밍고꺼야??",
          "original_form": "민트도 도밍고꺼야??",
          "speaker_id": "2",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100031060.1.23",
          "form": "사이다???",
          "original_form": "사이다???",
          "speaker_id": "1",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100031060.1.24",
          "form": "민트 맞어",
          "original_form": "민트 맞어",
          "speaker_id": "1",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100031060.1.25",
          "form": "도밍고꺼",
          "original_form": "도밍고꺼",
          "speaker_id": "1",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100031060.1.26",
          "form": "어 설명서에 사이다냉침도 가능하다는 듯??",
          "original_form": "어 설명서에 사이다냉침도 가능하다는 듯??",
          "speaker_id": "2",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100031060.1.27",
          "form": "탄산수 냉침이던가...",
          "original_form": "탄산수 냉침이던가...",
          "speaker_id": "2",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100031060.1.28",
          "form": "탄산수에 홍차라",
          "original_form": "탄산수에 홍차라",
          "speaker_id": "1",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100031060.1.29",
          "form": "여튼 민트도 꿀들어간거야?",
          "original_form": "여튼 민트도 꿀들어간거야?",
          "speaker_id": "2",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100031060.1.30",
          "form": "그냥 도밍고 자체가 꿀이 다 들어간거같기도",
          "original_form": "그냥 도밍고 자체가 꿀이 다 들어간거같기도",
          "speaker_id": "1",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100031060.1.31",
          "form": "아 그래? 신기하다 진짴ㅋㅋㅋㅋ꿀과 민트라니 괴식아니냐고",
          "original_form": "아 그래? 신기하다 진짴ㅋㅋㅋㅋ꿀과 민트라니 괴식아니냐고",
          "speaker_id": "2",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100031060.1.32",
          "form": "물 끓었다 후후 민트 우려야지",
          "original_form": "물 끓었다 후후 민트 우려야지",
          "speaker_id": "1",
          "time": "20210904 16:06"
        }
      ]
