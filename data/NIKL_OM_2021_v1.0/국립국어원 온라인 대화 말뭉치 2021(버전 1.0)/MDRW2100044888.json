        {
          "id": "MDRW2100044888.1.1",
          "form": "산모기 심할텐데 방비 잘해야겠어요",
          "original_form": "산모기 심할텐데 방비 잘해야겠어요",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044888.1.2",
          "form": "네네 ㅠㅠ 가을 옷 준비 하셧나요??",
          "original_form": "네네 ㅠㅠ 가을 옷 준비 하셧나요??",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044888.1.3",
          "form": "전 더워서 아직 반팔반바지예요",
          "original_form": "전 더워서 아직 반팔반바지예요",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044888.1.4",
          "form": "서울은 쌀쌀해서 가을옷 입으셨나요?",
          "original_form": "서울은 쌀쌀해서 가을옷 입으셨나요?",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044888.1.5",
          "form": "요즘 반팔에 얇은 가디건 챙겨요!!!",
          "original_form": "요즘 반팔에 얇은 가디건 챙겨요!!!",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044888.1.6",
          "form": "이제 옷장 정리 해야할거같아요 ㅋㅋ",
          "original_form": "이제 옷장 정리 해야할거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044888.1.7",
          "form": "ㅋㅋㅋ저 요 근래 이틀 혹시나싶어 가디건 들고다녔지만",
          "original_form": "ㅋㅋㅋ저 요 근래 이틀 혹시나싶어 가디건 들고다녔지만",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100044888.1.8",
          "form": "짐이였어요ㅋㅋ 오늘 퇴근길 생각했죠 들고다니지말자고ㅋ",
          "original_form": "짐이였어요ㅋㅋ 오늘 퇴근길 생각했죠 들고다니지말자고ㅋ",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100044888.1.9",
          "form": "오 맞아옄ㅋㅋㅋ 제친구도",
          "original_form": "오 맞아옄ㅋㅋㅋ 제친구도",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100044888.1.10",
          "form": "겉옷 들고나왓다가",
          "original_form": "겉옷 들고나왓다가",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044888.1.11",
          "form": "팔에 걸치고 다녓어요 ㅋㅋ",
          "original_form": "팔에 걸치고 다녓어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044888.1.12",
          "form": "이틀 그러고 낼부터 안갖고갈꺼예요\n계속 들고만 다녔어요ㅋ",
          "original_form": "이틀 그러고 낼부터 안갖고갈꺼예요\n계속 들고만 다녔어요ㅋ",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044888.1.13",
          "form": "비올땐 좀 내려가나봐요 온도가 ㅋㅋㅋㅋ",
          "original_form": "비올땐 좀 내려가나봐요 온도가 ㅋㅋㅋㅋ",
