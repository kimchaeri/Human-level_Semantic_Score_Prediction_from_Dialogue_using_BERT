      "utterance": [
        {
          "id": "MDRW2100022084.1.1",
          "form": "예전부터 그래와서 놀랍지도않아요ㅎㅎ",
          "original_form": "예전부터 그래와서 놀랍지도않아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100022084.1.2",
          "form": "저 진짜 3년내내 돈걱정만하고 산듯ㅠ",
          "original_form": "저 진짜 3년내내 돈걱정만하고 산듯ㅠ",
          "speaker_id": "2",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100022084.1.3",
          "form": "에공 토닥토닥요",
          "original_form": "에공 토닥토닥요",
          "speaker_id": "1",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100022084.1.4",
          "form": "원래 대출받는것도 싫어하고 돈도 별로 안쓰는 스타일이었는데ㅠ 결혼생활 하면서 정말 어마무시하게 썼네요ㅠ",
          "original_form": "원래 대출받는것도 싫어하고 돈도 별로 안쓰는 스타일이었는데ㅠ 결혼생활 하면서 정말 어마무시하게 썼네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100022084.1.5",
          "form": "연봉이 5천이 조금 넘어요ㅎ",
          "original_form": "연봉이 5천이 조금 넘어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100022084.1.6",
          "form": "근데 쓸데가 그렇게 많아요?",
          "original_form": "근데 쓸데가 그렇게 많아요?",
          "speaker_id": "1",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100022084.1.7",
          "form": "근데 그걸 다쓰고도 모자라서 마이너스로써요ㅎ",
          "original_form": "근데 그걸 다쓰고도 모자라서 마이너스로써요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100022084.1.8",
          "form": "ㅇ어떻게 쓰면 그렇게 써요",
          "original_form": "ㅇ어떻게 쓰면 그렇게 써요",
          "speaker_id": "1",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100022084.1.9",
          "form": "급궁금",
          "original_form": "급궁금",
          "speaker_id": "1",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100022084.1.10",
          "form": "대부분 식비랑 육아용품 아이를위해 쓰는거라",
          "original_form": "대부분 식비랑 육아용품 아이를위해 쓰는거라",
          "speaker_id": "2",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100022084.1.11",
          "form": "그래서 이해는하는데...",
          "original_form": "그래서 이해는하는데...",
          "speaker_id": "2",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100022084.1.12",
          "form": "해도 너무하네ㅎㅎㅎ",
          "original_form": "해도 너무하네ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100022084.1.13",
          "form": "잘됫어요ㅎㅎ 내가 이혼하자고 말하지도 않았지만",
          "original_form": "잘됫어요ㅎㅎ 내가 이혼하자고 말하지도 않았지만",
          "speaker_id": "2",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100022084.1.14",
          "form": "보니 소비패턴이 너무 다른거 같아요",
          "original_form": "보니 소비패턴이 너무 다른거 같아요",
          "speaker_id": "1",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100022084.1.15",
          "form": "그러니깐 자꾸싸우게되더라구요",
          "original_form": "그러니깐 자꾸싸우게되더라구요",
          "speaker_id": "2",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100022084.1.16",
          "form": "전에 이야기했었을꺼에요ㅎ",
          "original_form": "전에 이야기했었을꺼에요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100022084.1.17",
          "form": "전 어떻게든 줄여보고 부수입을 늘릴려고 스벅 쿠폰받아서 커피사주는데",
          "original_form": "전 어떻게든 줄여보고 부수입을 늘릴려고 스벅 쿠폰받아서 커피사주는데",
          "speaker_id": "2",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100022084.1.18",
          "form": "그모습을보고 저보고 쿠폰남이라네요ㅠ",
          "original_form": "그모습을보고 저보고 쿠폰남이라네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100022084.1.19",
          "form": "에공",
          "original_form": "에공",
          "speaker_id": "1",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100022084.1.20",
          "form": "에휴ㅠ 죄송해요ㅠ 괜한이야기를하네요",
          "original_form": "에휴ㅠ 죄송해요ㅠ 괜한이야기를하네요",
          "speaker_id": "2",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100022084.1.21",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100022084.1.22",
          "form": "어따 이야기 하실때조 없엇을건데",
          "original_form": "어따 이야기 하실때조 없엇을건데",
          "speaker_id": "1",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100022084.1.23",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100022084.1.24",
          "form": "솔직히 이혼하는게 좋은건 아니잖아요",
          "original_form": "솔직히 이혼하는게 좋은건 아니잖아요",
          "speaker_id": "2",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100022084.1.25",
          "form": "사람은 누구한테 말하는거 만으로도 위안을 받는데요",
          "original_form": "사람은 누구한테 말하는거 만으로도 위안을 받는데요",
          "speaker_id": "1",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100022084.1.26",
          "form": "제가 뭐 바람을 폈나요? 아니면 뭐 도박을해ㅛ나요?",
          "original_form": "제가 뭐 바람을 폈나요? 아니면 뭐 도박을해ㅛ나요?",
          "speaker_id": "2",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100022084.1.27",
          "form": "이혼사유는 아내가 충분한대",
          "original_form": "이혼사유는 아내가 충분한대",
          "speaker_id": "2",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100022084.1.28",
          "form": "아이를 위해서 싸우기 싫고 관계를 유지해야만 하기때문에",
          "original_form": "아이를 위해서 싸우기 싫고 관계를 유지해야만 하기때문에",
          "speaker_id": "2",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100022084.1.29",
          "form": "그냥 이혼원하니깐 하는건데..",
          "original_form": "그냥 이혼원하니깐 하는건데..",
          "speaker_id": "2",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100022084.1.30",
          "form": "술먹고 이상한 소리하네요ㅎㅎㅎ",
          "original_form": "술먹고 이상한 소리하네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100022084.1.31",
          "form": "ㅎㅎ 괜찮아요 ㅎㅎ",
          "original_form": "ㅎㅎ 괜찮아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100022084.1.32",
          "form": "주제랑 너무 다르네요ㅎ",
          "original_form": "주제랑 너무 다르네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100022084.1.33",
          "form": "오죽 답답하면",
          "original_form": "오죽 답답하면",
          "speaker_id": "1",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100022084.1.34",
          "form": "요건 대화변경으로 넘길까봐요 ㅎㅎ",
          "original_form": "요건 대화변경으로 넘길까봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100022084.1.35",
          "form": "저 원래 앱테크도 몰랐고 안해요ㅎ",
          "original_form": "저 원래 앱테크도 몰랐고 안해요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100022084.1.36",
          "form": "식음료사려고 하는것도아니고?ㅎㅎ",
          "original_form": "식음료사려고 하는것도아니고?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100022084.1.37",
          "form": "그래도 제출은 해야하니~^^",
          "original_form": "그래도 제출은 해야하니~^^",
          "speaker_id": "2",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100022084.1.38",
          "form": "ㅎㅎㅎ 못살아요",
          "original_form": "ㅎㅎㅎ 못살아요",
          "speaker_id": "1",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100022084.1.39",
          "form": "스타벅스도 잘안가고요ㅎ",
          "original_form": "스타벅스도 잘안가고요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100022084.1.40",
          "form": "스벅가면 700원 결제하더라구요ㅎ",
          "original_form": "스벅가면 700원 결제하더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100022084.1.41",
          "form": "나머지는 쿠폰발급받으면ㅎ 그렇게 열씸히살고있는데....",
          "original_form": "나머지는 쿠폰발급받으면ㅎ 그렇게 열씸히살고있는데....",
          "speaker_id": "2",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100022084.1.42",
          "form": "쿠폰남이라고하고 뭐 맨날 핸드폰만 본다고 하더라구요ㅎ",
          "original_form": "쿠폰남이라고하고 뭐 맨날 핸드폰만 본다고 하더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100022084.1.43",
          "form": "에고ㅎㅎ 술취했나봐요ㅎ 하소연을 하고있음ㅎㅎㅎㅎㅎ",
          "original_form": "에고ㅎㅎ 술취했나봐요ㅎ 하소연을 하고있음ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
