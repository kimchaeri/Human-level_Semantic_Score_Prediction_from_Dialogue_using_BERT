        },
        {
          "id": "MDRW2100046728.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20211104 11:28"
        },
        {
          "id": "MDRW2100046728.1.3",
          "form": "취미 있으신가요?",
          "original_form": "취미 있으신가요?",
          "speaker_id": "2",
          "time": "20211104 11:28"
        },
        {
          "id": "MDRW2100046728.1.4",
          "form": "영화보는게 취미였는데",
          "original_form": "영화보는게 취미였는데",
          "speaker_id": "1",
          "time": "20211104 11:29"
        },
        {
          "id": "MDRW2100046728.1.5",
          "form": "지금 코로나로인해ㅜ못본지ㅜ꽤ㅜ됐네요",
          "original_form": "지금 코로나로인해ㅜ못본지ㅜ꽤ㅜ됐네요",
          "speaker_id": "1",
          "time": "20211104 11:29"
        },
        {
          "id": "MDRW2100046728.1.6",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211104 11:32"
        },
        {
          "id": "MDRW2100046728.1.7",
          "form": "저도 영화관 간지 꽤 오래 됐어요",
          "original_form": "저도 영화관 간지 꽤 오래 됐어요",
          "speaker_id": "2",
          "time": "20211104 11:32"
        },
        {
          "id": "MDRW2100046728.1.8",
          "form": "그쳐 ㅠㅠ",
          "original_form": "그쳐 ㅠㅠ",
          "speaker_id": "1",
          "time": "20211104 11:33"
        },
        {
          "id": "MDRW2100046728.1.9",
          "form": "마스크 없는 하루 살고 싶어용",
          "original_form": "마스크 없는 하루 살고 싶어용",
          "speaker_id": "1",
          "time": "20211104 11:33"
        },
        {
          "id": "MDRW2100046728.1.10",
          "form": "마지막으로 영화관에서 본 영화 기억나시나요? ㅎㅎ",
          "original_form": "마지막으로 영화관에서 본 영화 기억나시나요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 11:34"
        },
        {
          "id": "MDRW2100046728.1.11",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "1",
          "time": "20211104 11:35"
        },
        {
          "id": "MDRW2100046728.1.12",
          "form": "조정석이랑 윤아 나왔던 ㅋㅋ",
          "original_form": "조정석이랑 윤아 나왔던 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211104 11:36"
        },
        {
          "id": "MDRW2100046728.1.13",
          "form": "엑시트?",
          "original_form": "엑시트?",
          "speaker_id": "1",
          "time": "20211104 11:36"
        },
        {
          "id": "MDRW2100046728.1.14",
          "form": "ㅋㅋ너무 오래됐네요",
          "original_form": "ㅋㅋ너무 오래됐네요",
          "speaker_id": "1",
          "time": "20211104 11:36"
        },
        {
          "id": "MDRW2100046728.1.15",
          "form": "헉 ㅋㅋㅋ 진짜 오래 됐네요",
          "original_form": "헉 ㅋㅋㅋ 진짜 오래 됐네요",
          "speaker_id": "2",
          "time": "20211104 11:36"
        },
        {
          "id": "MDRW2100046728.1.16",
          "form": "저도 그거 영화관에서 봤어요!",
          "original_form": "저도 그거 영화관에서 봤어요!",
          "speaker_id": "2",
          "time": "20211104 11:37"
        },
        {
          "id": "MDRW2100046728.1.17",
          "form": "중간에 뭐 다른영화 본거 같은데",
          "original_form": "중간에 뭐 다른영화 본거 같은데",
          "speaker_id": "1",
          "time": "20211104 11:37"
        },
        {
          "id": "MDRW2100046728.1.18",
          "form": "기억이 잘 안나네요 ㅎㅎ",
          "original_form": "기억이 잘 안나네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211104 11:37"
        },
        {
          "id": "MDRW2100046728.1.19",
          "form": "저도 기억이 잘 안나네요..",
          "original_form": "저도 기억이 잘 안나네요..",
          "speaker_id": "2",
          "time": "20211104 11:38"
        },
        {
          "id": "MDRW2100046728.1.20",
          "form": "매드맥스 4D 재개봉해서 보고.. 그게 마지막인 것 같아요",
          "original_form": "매드맥스 4D 재개봉해서 보고.. 그게 마지막인 것 같아요",
          "speaker_id": "2",
          "time": "20211104 11:38"
        },
        {
          "id": "MDRW2100046728.1.21",
          "form": "하루빨리 다들 편하게 영화보고 놀러가는 날이 오면 좋겠네요 ㅎㅎ",
          "original_form": "하루빨리 다들 편하게 영화보고 놀러가는 날이 오면 좋겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211104 11:39"
        },
        {
          "id": "MDRW2100046728.1.22",
          "form": "그쵸 ㅋㅋㅋ 이제 다들 백신도 맞고 해서 좀 편해질 것 같긴 하지만...",
          "original_form": "그쵸 ㅋㅋㅋ 이제 다들 백신도 맞고 해서 좀 편해질 것 같긴 하지만...",
          "speaker_id": "2",
          "time": "20211104 11:40"
        },
        {
          "id": "MDRW2100046728.1.23",
          "form": "코로나가 아예 끝나는 날이 얼른 왔으면 좋겠어요",
          "original_form": "코로나가 아예 끝나는 날이 얼른 왔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20211104 11:41"
        },
        {
          "id": "MDRW2100046728.1.24",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "1",
