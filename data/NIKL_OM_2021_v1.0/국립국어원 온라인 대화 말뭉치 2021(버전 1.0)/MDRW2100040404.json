      "utterance": [
        {
          "id": "MDRW2100040404.1.1",
          "form": "아 디자인! 멋있어요",
          "original_form": "아 디자인! 멋있어요",
          "speaker_id": "2",
          "time": "20210904 18:19"
        },
        {
          "id": "MDRW2100040404.1.2",
          "form": "유망한 직종 아닌가요ㅋㅋㅋ",
          "original_form": "유망한 직종 아닌가요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:19"
        },
        {
          "id": "MDRW2100040404.1.3",
          "form": "네 일자리는 많죠",
          "original_form": "네 일자리는 많죠",
          "speaker_id": "1",
          "time": "20210904 18:19"
        },
        {
          "id": "MDRW2100040404.1.4",
          "form": "일이 힘들어서 그렇지 ᆢ",
          "original_form": "일이 힘들어서 그렇지 ᆢ",
          "speaker_id": "1",
          "time": "20210904 18:19"
        },
        {
          "id": "MDRW2100040404.1.5",
          "form": "일하신지 얼마나 되셨어요?",
          "original_form": "일하신지 얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210904 18:21"
        },
        {
          "id": "MDRW2100040404.1.6",
          "form": "저는 이제 5년차쯤 됐어요",
          "original_form": "저는 이제 5년차쯤 됐어요",
          "speaker_id": "2",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100040404.1.7",
          "form": "5년차인데도 만족하시면 정말 직업이 잘맞나봐요",
          "original_form": "5년차인데도 만족하시면 정말 직업이 잘맞나봐요",
          "speaker_id": "1",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100040404.1.8",
          "form": "넹 매년 새로워서ㅋㅋ",
          "original_form": "넹 매년 새로워서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:53"
        },
        {
          "id": "MDRW2100040404.1.9",
          "form": "좋겠네요ㅜ",
          "original_form": "좋겠네요ㅜ",
          "speaker_id": "1",
          "time": "20210904 19:41"
        },
        {
          "id": "MDRW2100040404.1.10",
          "form": "그래도 창의적인 업무라 좀 재밌지않나용",
          "original_form": "그래도 창의적인 업무라 좀 재밌지않나용",
          "speaker_id": "2",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100040404.1.11",
          "form": "재미가 없는건 아닌데 야근많고 일이 많으니까 좀 지쳐요",
          "original_form": "재미가 없는건 아닌데 야근많고 일이 많으니까 좀 지쳐요",
          "speaker_id": "1",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100040404.1.12",
          "form": "제가 체력이 없어서 그런거 같아요",
          "original_form": "제가 체력이 없어서 그런거 같아요",
          "speaker_id": "1",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100040404.1.13",
          "form": "아 너무 근무시간이 길군여",
          "original_form": "아 너무 근무시간이 길군여",
          "speaker_id": "2",
          "time": "20210904 20:12"
        },
        {
          "id": "MDRW2100040404.1.14",
          "form": "그쵸 영상쪽이 아무래도 3d업종이죠",
          "original_form": "그쵸 영상쪽이 아무래도 3d업종이죠",
          "speaker_id": "1",
          "time": "20210904 20:12"
        },
        {
          "id": "MDRW2100040404.1.15",
          "form": "야근많고 ᆢ ㅜㅜㅜ",
          "original_form": "야근많고 ᆢ ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 20:13"
        },
        {
          "id": "MDRW2100040404.1.16",
          "form": "name2님은 야근많이 없으세요?",
          "original_form": "&name2&님은 야근많이 없으세요?",
          "speaker_id": "1",
          "time": "20210904 20:13"
        },
        {
          "id": "MDRW2100040404.1.17",
          "form": "넹 칼퇴가능해서 좋아요 ㅋㅋ",
          "original_form": "넹 칼퇴가능해서 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100040404.1.18",
          "form": "부럽네요ㅜㅜㅜ",
          "original_form": "부럽네요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 21:02"
        },
        {
          "id": "MDRW2100040404.1.19",
          "form": "저도 칼퇴하는곳 가고싶네요",
          "original_form": "저도 칼퇴하는곳 가고싶네요",
          "speaker_id": "1",
          "time": "20210904 21:02"
        },
        {
          "id": "MDRW2100040404.1.20",
          "form": "넹ㅜ 급여가 큰 편은 아니라도 워라밸이 최고라 ㅠ",
          "original_form": "넹ㅜ 급여가 큰 편은 아니라도 워라밸이 최고라 ㅠ",
          "speaker_id": "2",
          "time": "20210904 21:14"
        },
        {
          "id": "MDRW2100040404.1.21",
          "form": "맞인요 워라벨 중요하죠",
          "original_form": "맞인요 워라벨 중요하죠",
          "speaker_id": "1",
