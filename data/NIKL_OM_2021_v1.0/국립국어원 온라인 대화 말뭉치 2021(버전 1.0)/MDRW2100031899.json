      "utterance": [
        {
          "id": "MDRW2100031899.1.1",
          "form": "잘 할 줄 모르면 밀키트 좋은 거 같아요",
          "original_form": "잘 할 줄 모르면 밀키트 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210905 07:53"
        },
        {
          "id": "MDRW2100031899.1.2",
          "form": "오 짱좋아요",
          "original_form": "오 짱좋아요",
          "speaker_id": "2",
          "time": "20210905 07:53"
        },
        {
          "id": "MDRW2100031899.1.3",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:53"
        },
        {
          "id": "MDRW2100031899.1.4",
          "form": "아 궁금한게 있어요",
          "original_form": "아 궁금한게 있어요",
          "speaker_id": "2",
          "time": "20210905 07:54"
        },
        {
          "id": "MDRW2100031899.1.5",
          "form": "문과 박사들은 어디로 취직하나요?",
          "original_form": "문과 박사들은 어디로 취직하나요?",
          "speaker_id": "2",
          "time": "20210905 07:54"
        },
        {
          "id": "MDRW2100031899.1.6",
          "form": "젤 하고 싶은 건 교수 아닐까요?",
          "original_form": "젤 하고 싶은 건 교수 아닐까요?",
          "speaker_id": "1",
          "time": "20210905 07:54"
        },
        {
          "id": "MDRW2100031899.1.7",
          "form": "제 아는 동생은 성균관대  경제학과 박사가 정",
          "original_form": "제 아는 동생은 성균관대  경제학과 박사가 정",
          "speaker_id": "2",
          "time": "20210905 07:54"
        },
        {
          "id": "MDRW2100031899.1.8",
          "form": "그거 아니면 연구하거나",
          "original_form": "그거 아니면 연구하거나",
          "speaker_id": "1",
          "time": "20210905 07:54"
        },
        {
          "id": "MDRW2100031899.1.9",
          "form": "하고 나서 카이스트 갔거든요",
          "original_form": "하고 나서 카이스트 갔거든요",
          "speaker_id": "2",
          "time": "20210905 07:54"
        },
        {
          "id": "MDRW2100031899.1.10",
          "form": "그런 거 말고",
          "original_form": "그런 거 말고",
          "speaker_id": "2",
          "time": "20210905 07:54"
        },
        {
          "id": "MDRW2100031899.1.11",
          "form": "일반적으로 취직하면 어디로 가나요",
          "original_form": "일반적으로 취직하면 어디로 가나요",
          "speaker_id": "2",
          "time": "20210905 07:55"
        },
        {
          "id": "MDRW2100031899.1.12",
          "form": "잘 모르겠어요",
          "original_form": "잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210905 07:55"
        },
        {
          "id": "MDRW2100031899.1.13",
          "form": "그럼 지금은 어디에서 일하십니까",
          "original_form": "그럼 지금은 어디에서 일하십니까",
          "speaker_id": "2",
          "time": "20210905 07:55"
        },
        {
          "id": "MDRW2100031899.1.14",
          "form": "할거없으면 다시 다른공부하거나 하기도하고...",
          "original_form": "할거없으면 다시 다른공부하거나 하기도하고...",
          "speaker_id": "1",
          "time": "20210905 07:55"
        },
        {
          "id": "MDRW2100031899.1.15",
          "form": "지금은 일안하고 쉬고있어요",
          "original_form": "지금은 일안하고 쉬고있어요",
          "speaker_id": "1",
          "time": "20210905 07:55"
        },
        {
          "id": "MDRW2100031899.1.16",
          "form": "음 재도약 준비중이시네요",
          "original_form": "음 재도약 준비중이시네요",
          "speaker_id": "2",
          "time": "20210905 07:56"
        },
        {
          "id": "MDRW2100031899.1.17",
          "form": "네 뭐 그렇다고 볼수있죠",
          "original_form": "네 뭐 그렇다고 볼수있죠",
          "speaker_id": "1",
          "time": "20210905 07:56"
        },
        {
          "id": "MDRW2100031899.1.18",
          "form": "잘되시길 바랍니다",
          "original_form": "잘되시길 바랍니다",
          "speaker_id": "2",
          "time": "20210905 07:56"
        },
        {
          "id": "MDRW2100031899.1.19",
          "form": "넵 감사해요",
          "original_form": "넵 감사해요",
          "speaker_id": "1",
          "time": "20210905 07:57"
        },
        {
          "id": "MDRW2100031899.1.20",
          "form": "준비하시는거 있으세요?",
          "original_form": "준비하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210905 07:57"
        }
      ]
