      "utterance": [
        {
          "id": "MDRW2100011682.1.1",
          "form": "연애안하냐",
          "original_form": "연애안하냐",
          "speaker_id": "1",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011682.1.2",
          "form": "네 아직이요",
          "original_form": "네 아직이요",
          "speaker_id": "2",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011682.1.3",
          "form": "안하는게아니고 몬하는거아니가",
          "original_form": "안하는게아니고 몬하는거아니가",
          "speaker_id": "1",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011682.1.4",
          "form": "그런 것도 있겠죠?ㅋㅋㅋ",
          "original_form": "그런 것도 있겠죠?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011682.1.5",
          "form": "근데 생각이 아직 없는걸유",
          "original_form": "근데 생각이 아직 없는걸유",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011682.1.6",
          "form": "지금도 늦었거든",
          "original_form": "지금도 늦었거든",
          "speaker_id": "1",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011682.1.7",
          "form": "때가 되면 가겠쥬",
          "original_form": "때가 되면 가겠쥬",
          "speaker_id": "2",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011682.1.8",
          "form": "근데 안가도 될거 같기도 하고",
          "original_form": "근데 안가도 될거 같기도 하고",
          "speaker_id": "2",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011682.1.9",
          "form": "연애를 많이해봐야 좋은남자를 만나지",
          "original_form": "연애를 많이해봐야 좋은남자를 만나지",
          "speaker_id": "1",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011682.1.10",
          "form": "시작 안하면 만날 필요 없잖아요",
          "original_form": "시작 안하면 만날 필요 없잖아요",
          "speaker_id": "2",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011682.1.11",
          "form": "안만나고 혼자 살지 뭐",
          "original_form": "안만나고 혼자 살지 뭐",
          "speaker_id": "2",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011682.1.12",
          "form": "결혼은안해도 연애는해야쥐",
          "original_form": "결혼은안해도 연애는해야쥐",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011682.1.13",
          "form": "혼자가 편한걸...",
          "original_form": "혼자가 편한걸...",
          "speaker_id": "2",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011682.1.14",
          "form": "으이그",
          "original_form": "으이그",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011682.1.15",
          "form": "있어도 걸리적거릴거 같아요ㅋㅋㅋ",
          "original_form": "있어도 걸리적거릴거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011682.1.16",
          "form": "사랑을 받아본적이있어야 하지",
          "original_form": "사랑을 받아본적이있어야 하지",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011682.1.17",
          "form": "결혼안하면 좋아할사람은 너네 부모삐없다",
          "original_form": "결혼안하면 좋아할사람은 너네 부모삐없다",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011682.1.18",
          "form": "그건 그렇겠네요ㅋㅋㅋ",
          "original_form": "그건 그렇겠네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011682.1.19",
          "form": "얼른 집을 나가야겠네",
          "original_form": "얼른 집을 나가야겠네",
          "speaker_id": "2",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011682.1.20",
          "form": "제발 독립이라도",
          "original_form": "제발 독립이라도",
          "speaker_id": "1",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011682.1.21",
          "form": "독립은 얼른 해야하는데...",
          "original_form": "독립은 얼른 해야하는데...",
          "speaker_id": "2",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011682.1.22",
          "form": "집을 구할 생각에 머리부터 아프네요",
          "original_form": "집을 구할 생각에 머리부터 아프네요",
          "speaker_id": "2",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011682.1.23",
          "form": "그래 나오면 돈이 많이들긴하지",
          "original_form": "그래 나오면 돈이 많이들긴하지",
          "speaker_id": "1",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011682.1.24",
          "form": "그쵸ㅠㅠ",
          "original_form": "그쵸ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 15:17"
        },
        {
          "id": "MDRW2100011682.1.25",
          "form": "모아둔 돈이라도 있으면 좋겠지만 남은 돈이 얼마 없네요",
          "original_form": "모아둔 돈이라도 있으면 좋겠지만 남은 돈이 얼마 없네요",
          "speaker_id": "2",
          "time": "20210824 15:17"
        },
        {
          "id": "MDRW2100011682.1.26",
          "form": "에휴 그건 그렇네",
          "original_form": "에휴 그건 그렇네",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011682.1.27",
          "form": "아니다 요새 대출 잘 되어있다 잘 찾아봐라",
          "original_form": "아니다 요새 대출 잘 되어있다 잘 찾아봐라",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011682.1.28",
          "form": "대출...그렇죠...찾아보면 있겠죠...",
          "original_form": "대출...그렇죠...찾아보면 있겠죠...",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011682.1.29",
          "form": "품팔이를 해야하는데 어렵구먼유",
          "original_form": "품팔이를 해야하는데 어렵구먼유",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011682.1.30",
          "form": "행복주택 이런 것도 있지않나",
          "original_form": "행복주택 이런 것도 있지않나",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011682.1.31",
          "form": "전에 보니까 괜찮던데",
          "original_form": "전에 보니까 괜찮던데",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011682.1.32",
          "form": "찾아봤는데 자격요건이 안 맞더라구요",
          "original_form": "찾아봤는데 자격요건이 안 맞더라구요",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011682.1.33",
          "form": "직장에서도 거리가 애매하고",
          "original_form": "직장에서도 거리가 애매하고",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011682.1.34",
          "form": "하긴 차 없으면 움직이기 힘들겠더라",
          "original_form": "하긴 차 없으면 움직이기 힘들겠더라",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011682.1.35",
          "form": "그래도 집에서 나와야 마음 편하게 연애라도 해볼텐데",
          "original_form": "그래도 집에서 나와야 마음 편하게 연애라도 해볼텐데",
          "speaker_id": "1",
          "time": "20210824 15:25"
        },
        {
          "id": "MDRW2100011682.1.36",
          "form": "ㅎㅎㅎ...맞네요...독립하고 생각이 바뀔지도 모르겠네요",
          "original_form": "ㅎㅎㅎ...맞네요...독립하고 생각이 바뀔지도 모르겠네요",
          "speaker_id": "2",
          "time": "20210824 15:25"
        },
        {
          "id": "MDRW2100011682.1.37",
          "form": "독립하고 외로우면 말해라",
          "original_form": "독립하고 외로우면 말해라",
          "speaker_id": "1",
          "time": "20210824 15:30"
        },
        {
          "id": "MDRW2100011682.1.38",
          "form": "아니다 지금 외로워도 말해라",
          "original_form": "아니다 지금 외로워도 말해라",
          "speaker_id": "1",
          "time": "20210824 15:30"
        },
        {
          "id": "MDRW2100011682.1.39",
          "form": "괜찮은 놈 찾아줄게",
          "original_form": "괜찮은 놈 찾아줄게",
          "speaker_id": "1",
          "time": "20210824 15:31"
        },
        {
          "id": "MDRW2100011682.1.40",
          "form": "ㅋㅋㅋ아이고 마음만으로도 감사합니다",
          "original_form": "ㅋㅋㅋ아이고 마음만으로도 감사합니다",
          "speaker_id": "2",
          "time": "20210824 15:31"
        }
      ]
    }
  ]
