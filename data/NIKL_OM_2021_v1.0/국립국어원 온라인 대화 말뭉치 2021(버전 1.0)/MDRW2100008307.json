      "utterance": [
        {
          "id": "MDRW2100008307.1.1",
          "form": "최근이면 오늘 아침에 동물농장 봤고 어제 전참시랑 놀면뭐하니 봤어요ㅋㅋㅋ",
          "original_form": "최근이면 오늘 아침에 동물농장 봤고 어제 전참시랑 놀면뭐하니 봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008307.1.2",
          "form": "동물농장 너무 재밌어요",
          "original_form": "동물농장 너무 재밌어요",
          "speaker_id": "1",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008307.1.3",
          "form": "일요일아침은 동농이랑 함께 시작하는 기분",
          "original_form": "일요일아침은 동농이랑 함께 시작하는 기분",
          "speaker_id": "1",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008307.1.4",
          "form": "그쵸 오늘 햄스터가 귀여웠어요ㅋㅋㅋ",
          "original_form": "그쵸 오늘 햄스터가 귀여웠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100008307.1.5",
          "form": "맞아요ㅋㅋ 우연이요",
          "original_form": "맞아요ㅋㅋ 우연이요",
          "speaker_id": "1",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100008307.1.6",
          "form": "너무 깜찍하더라는",
          "original_form": "너무 깜찍하더라는",
          "speaker_id": "1",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008307.1.7",
          "form": "아 전참시는 오상욱선수 나온 거 보셨나요?",
          "original_form": "아 전참시는 오상욱선수 나온 거 보셨나요?",
          "speaker_id": "1",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008307.1.8",
          "form": "오 맞아요 ㅋㅋㅋ",
          "original_form": "오 맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008307.1.9",
          "form": "재밌더라구요 선수촌 내부도 보고 신기했어요",
          "original_form": "재밌더라구요 선수촌 내부도 보고 신기했어요",
          "speaker_id": "1",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008307.1.10",
          "form": "엌ㅋㅋㅋ뭔가 이상하다 싶어서 생각해보니",
          "original_form": "엌ㅋㅋㅋ뭔가 이상하다 싶어서 생각해보니",
          "speaker_id": "2",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008307.1.11",
          "form": "전참시는 아니고 나혼산에 나오셨었어요ㅋㅋㅋ",
          "original_form": "전참시는 아니고 나혼산에 나오셨었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008307.1.12",
          "form": "엇",
          "original_form": "엇",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008307.1.13",
          "form": "저는 둘 다 봤고요",
          "original_form": "저는 둘 다 봤고요",
          "speaker_id": "2",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008307.1.14",
          "form": "앗ㅋㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008307.1.15",
          "form": "뭐지 왜 착각했지",
          "original_form": "뭐지 왜 착각했지",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008307.1.16",
          "form": "ㅋㅋㅋ같은 방송사라 순간 헷갈렸나봐요",
          "original_form": "ㅋㅋㅋ같은 방송사라 순간 헷갈렸나봐요",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008307.1.17",
          "form": "저도 그런줄 알았네요 순간ㅋㅋㅋ",
          "original_form": "저도 그런줄 알았네요 순간ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008307.1.18",
          "form": "전참시에는 어제 원슈타인님이랑 박하선님 나오시더라고요",
          "original_form": "전참시에는 어제 원슈타인님이랑 박하선님 나오시더라고요",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008307.1.19",
          "form": "두 분다 매력있는 분들이시죠",
          "original_form": "두 분다 매력있는 분들이시죠",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008307.1.20",
          "form": "어땠나요?",
          "original_form": "어땠나요?",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008307.1.21",
          "form": "완전 순박한 청년 느낌이셨고 원슈타인님은",
          "original_form": "완전 순박한 청년 느낌이셨고 원슈타인님은",
          "speaker_id": "2",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008307.1.22",
          "form": "박하선님은 진짜 뭔가 도덕책에 나올법한 인생 같았어요ㅋㅋㅋ",
          "original_form": "박하선님은 진짜 뭔가 도덕책에 나올법한 인생 같았어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008307.1.23",
          "form": "재밌었을거 같아요ㅋㅋ 저도 재방으로 봐야겠어요",
          "original_form": "재밌었을거 같아요ㅋㅋ 저도 재방으로 봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 15:13"
        }
      ]
    }
