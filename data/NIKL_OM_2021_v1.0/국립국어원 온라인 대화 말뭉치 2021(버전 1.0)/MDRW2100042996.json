      "utterance": [
        {
          "id": "MDRW2100042996.1.1",
          "form": "음..  딱히 없어요",
          "original_form": "음..  딱히 없어요",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100042996.1.2",
          "form": "그냥 이슈 있는것들 유트브로 검색해서 봐요",
          "original_form": "그냥 이슈 있는것들 유트브로 검색해서 봐요",
          "speaker_id": "2",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100042996.1.3",
          "form": "아ㅋㅋ저도 그러다가 집콕하면서 이것저것 보고있어요!",
          "original_form": "아ㅋㅋ저도 그러다가 집콕하면서 이것저것 보고있어요!",
          "speaker_id": "1",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100042996.1.4",
          "form": "유투브는 구독하는 채널 있으세요?",
          "original_form": "유투브는 구독하는 채널 있으세요?",
          "speaker_id": "1",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100042996.1.5",
          "form": "저는 주로 여행유트버 구독중이에요",
          "original_form": "저는 주로 여행유트버 구독중이에요",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100042996.1.6",
          "form": "재밌는거 아세요?",
          "original_form": "재밌는거 아세요?",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100042996.1.7",
          "form": "뭔데요???",
          "original_form": "뭔데요???",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100042996.1.8",
          "form": "뜨랑킬로랑 빠니보틀 백지수표 등등요",
          "original_form": "뜨랑킬로랑 빠니보틀 백지수표 등등요",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100042996.1.9",
          "form": "아ㅋㅋ 그거 저번에 비교된다고 영상 올라온거 봤어요ㅋㅋ",
          "original_form": "아ㅋㅋ 그거 저번에 비교된다고 영상 올라온거 봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100042996.1.10",
          "form": "그래요? 넷플 혹시보세요?",
          "original_form": "그래요? 넷플 혹시보세요?",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100042996.1.11",
          "form": "아뇨ㅋㅋ 그건 따로 안봐요ㅎㅎ",
          "original_form": "아뇨ㅋㅋ 그건 따로 안봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100042996.1.12",
          "form": "아~ 시간있으면 뭐보세요",
          "original_form": "아~ 시간있으면 뭐보세요",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100042996.1.13",
          "form": "ㅋㅋ 유투브로 숨은 사람 찾기나 홈트나 베이킹영상 봐요ㅋㅋ",
          "original_form": "ㅋㅋ 유투브로 숨은 사람 찾기나 홈트나 베이킹영상 봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100042996.1.14",
          "form": "숨은사람찾기 뭐에요?",
          "original_form": "숨은사람찾기 뭐에요?",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100042996.1.15",
          "form": "부지런하실것같아요",
          "original_form": "부지런하실것같아요",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100042996.1.16",
          "form": "이십대카톡방에서 삼십대찾기 일반인사이에서 힙합가수찾기",
          "original_form": "이십대카톡방에서 삼십대찾기 일반인사이에서 힙합가수찾기",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100042996.1.17",
          "form": "요 컨텐츠 웃겨요",
          "original_form": "요 컨텐츠 웃겨요",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100042996.1.18",
          "form": "추천!",
          "original_form": "추천!",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100042996.1.19",
          "form": "방금 삼십대찾기보고왔어여",
          "original_form": "방금 삼십대찾기보고왔어여",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100042996.1.20",
          "form": "웃픈데요^^",
          "original_form": "웃픈데요^^",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100042996.1.21",
          "form": "ㅋㅋㅋㅋㅋ20대였을때 봤다면 어땠을까 궁금해지네요",
          "original_form": "ㅋㅋㅋㅋㅋ20대였을때 봤다면 어땠을까 궁금해지네요",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100042996.1.22",
          "form": "ㅋㅋ 30대분 너무 공감가서ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ 30대분 너무 공감가서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100042996.1.23",
          "form": "랩퍼편도 되게 웃겨요!",
          "original_form": "랩퍼편도 되게 웃겨요!",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100042996.1.24",
          "form": "ㅋㅋㅋㅋ긍깐요 저도 금방 티날것같아요",
          "original_form": "ㅋㅋㅋㅋ긍깐요 저도 금방 티날것같아요",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100042996.1.25",
          "form": "좀따봐볼께요!",
          "original_form": "좀따봐볼께요!",
