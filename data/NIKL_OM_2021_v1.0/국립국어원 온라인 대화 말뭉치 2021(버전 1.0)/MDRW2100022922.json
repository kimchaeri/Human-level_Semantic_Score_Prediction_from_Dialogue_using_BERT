      "utterance": [
        {
          "id": "MDRW2100022922.1.1",
          "form": "나 이번에 패브릭포스터 샀다",
          "original_form": "나 이번에 패브릭포스터 샀다",
          "speaker_id": "2",
          "time": "20210901 15:32"
        },
        {
          "id": "MDRW2100022922.1.2",
          "form": "오 어떤거?",
          "original_form": "오 어떤거?",
          "speaker_id": "1",
          "time": "20210901 15:33"
        },
        {
          "id": "MDRW2100022922.1.3",
          "form": "앙리 마티스 작품 디자인!",
          "original_form": "앙리 마티스 작품 디자인!",
          "speaker_id": "2",
          "time": "20210901 15:34"
        },
        {
          "id": "MDRW2100022922.1.4",
          "form": "나도 샀어",
          "original_form": "나도 샀어",
          "speaker_id": "1",
          "time": "20210901 15:35"
        },
        {
          "id": "MDRW2100022922.1.5",
          "form": "열무가 자꾸 벽타고 올라가려고해서...그거 막을라고 사서 달았어",
          "original_form": "열무가 자꾸 벽타고 올라가려고해서...그거 막을라고 사서 달았어",
          "speaker_id": "2",
          "time": "20210901 15:35"
        },
        {
          "id": "MDRW2100022922.1.6",
          "form": "오 언니는 뭐 샀어?",
          "original_form": "오 언니는 뭐 샀어?",
          "speaker_id": "2",
          "time": "20210901 15:35"
        },
        {
          "id": "MDRW2100022922.1.7",
          "form": "헐 엄청 크겠네?",
          "original_form": "헐 엄청 크겠네?",
          "speaker_id": "1",
          "time": "20210901 15:35"
        },
        {
          "id": "MDRW2100022922.1.8",
          "form": "아니 한 40-50? 정도야 그리 안커!",
          "original_form": "아니 한 40-50? 정도야 그리 안커!",
          "speaker_id": "2",
          "time": "20210901 15:36"
        },
        {
          "id": "MDRW2100022922.1.9",
          "form": "나는 하이큐...오타쿠...패브릭..포스터...",
          "original_form": "나는 하이큐...오타쿠...패브릭..포스터...",
          "speaker_id": "1",
          "time": "20210901 15:36"
        },
        {
          "id": "MDRW2100022922.1.10",
          "form": "오 궁금하다 어떤 디자인인지ㅋㅋ",
          "original_form": "오 궁금하다 어떤 디자인인지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:36"
        },
        {
          "id": "MDRW2100022922.1.11",
          "form": "그냥 일러스트 재탕이야 ㅋㅋㅋ",
          "original_form": "그냥 일러스트 재탕이야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:36"
        },
        {
          "id": "MDRW2100022922.1.12",
          "form": "그거하니까 벽타고 못올라가?",
          "original_form": "그거하니까 벽타고 못올라가?",
          "speaker_id": "1",
          "time": "20210901 15:37"
        },
        {
          "id": "MDRW2100022922.1.13",
          "form": "언니가 좋아하는 캐릭터야? 방금 달아가지고ㅋㅋ 못타는지는 모르겠옹",
          "original_form": "언니가 좋아하는 캐릭터야? 방금 달아가지고ㅋㅋ 못타는지는 모르겠옹",
          "speaker_id": "2",
          "time": "20210901 15:37"
        },
        {
          "id": "MDRW2100022922.1.14",
          "form": "응응 내가 좋아하는 캐릭터 ㅋㅋ아고 캣타워를 사줘야겠다",
          "original_form": "응응 내가 좋아하는 캐릭터 ㅋㅋ아고 캣타워를 사줘야겠다",
          "speaker_id": "1",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100022922.1.15",
          "form": "웅 이사가면 캣타워 하나 해줘야지.... 일단은 패브릭포스터로 막아야지..",
          "original_form": "웅 이사가면 캣타워 하나 해줘야지.... 일단은 패브릭포스터로 막아야지..",
          "speaker_id": "2",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100022922.1.16",
          "form": "앙리마티스 그런거 약간 인테리어용으로 나온거지?",
          "original_form": "앙리마티스 그런거 약간 인테리어용으로 나온거지?",
          "speaker_id": "1",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100022922.1.17",
          "form": "이사가면 뭐가 제일 사고싶어?",
          "original_form": "이사가면 뭐가 제일 사고싶어?",
          "speaker_id": "1",
          "time": "20210901 15:42"
        }
      ]
