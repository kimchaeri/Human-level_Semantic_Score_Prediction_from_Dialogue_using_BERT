      "utterance": [
        {
          "id": "MDRW2100016220.1.1",
          "form": "컴퓨터 사고싶어요",
          "original_form": "컴퓨터 사고싶어요",
          "speaker_id": "2",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016220.1.2",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016220.1.3",
          "form": "게임  잘 돌아가는 거요",
          "original_form": "게임  잘 돌아가는 거요",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016220.1.4",
          "form": "게임을좋아하시나봐요ㅋㅋ",
          "original_form": "게임을좋아하시나봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016220.1.5",
          "form": "넹ㅋㅋ",
          "original_form": "넹ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016220.1.6",
          "form": "전 스벅 텀블러 예쁜 거 나오면 사려구요",
          "original_form": "전 스벅 텀블러 예쁜 거 나오면 사려구요",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016220.1.7",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016220.1.8",
          "form": "시즌 바뀌어서 나올 거같아요",
          "original_form": "시즌 바뀌어서 나올 거같아요",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016220.1.9",
          "form": "그러네요ㅋㅋ 텀블러 맨날 사모았었는데",
          "original_form": "그러네요ㅋㅋ 텀블러 맨날 사모았었는데",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016220.1.10",
          "form": "어느순간 흥미가 없네요ㅋㅋ",
          "original_form": "어느순간 흥미가 없네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016220.1.11",
          "form": "저도 사모으다가 이렇게 많을 필요가 있나싶더라구요",
          "original_form": "저도 사모으다가 이렇게 많을 필요가 있나싶더라구요",
          "speaker_id": "1",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100016220.1.12",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100016220.1.13",
          "form": "이번에 좋은 거사면 전에 거 처분하려구요",
          "original_form": "이번에 좋은 거사면 전에 거 처분하려구요",
          "speaker_id": "1",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100016220.1.14",
          "form": "좋은 생각이네요",
          "original_form": "좋은 생각이네요",
          "speaker_id": "2",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100016220.1.15",
          "form": "중고 판매도 괜찮겠어요!",
          "original_form": "중고 판매도 괜찮겠어요!",
          "speaker_id": "2",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100016220.1.16",
          "form": "오 좋은데요?",
          "original_form": "오 좋은데요?",
          "speaker_id": "1",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100016220.1.17",
          "form": "조금이라도 생활비에 보탬이될겁니당ㅎㅎ",
          "original_form": "조금이라도 생활비에 보탬이될겁니당ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:15"
        }
      ]
