      "utterance": [
        {
          "id": "MDRW2100006559.1.1",
          "form": "사라지진않을 것 같아",
          "original_form": "사라지진않을 것 같아",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100006559.1.2",
          "form": "어쨌든 레벨 낮은 친구들을 원하면 이제 잘 안하는 게임이라",
          "original_form": "어쨌든 레벨 낮은 친구들을 원하면 이제 잘 안하는 게임이라",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100006559.1.3",
          "form": "탈퇴했다 가입하면 돼",
          "original_form": "탈퇴했다 가입하면 돼",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100006559.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100006559.1.5",
          "form": "오 친절하네 근데",
          "original_form": "오 친절하네 근데",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100006559.1.6",
          "form": "사기아냐?",
          "original_form": "사기아냐?",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100006559.1.7",
          "form": "사기까지는 아니겠지 뭐ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "사기까지는 아니겠지 뭐ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100006559.1.8",
          "form": "나도 뭐 그닥 잘하지 않아서 이제",
          "original_form": "나도 뭐 그닥 잘하지 않아서 이제",
          "speaker_id": "2",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100006559.1.9",
          "form": "게임을 어 막 그짓말로 하고 그래도 되는거야?!",
          "original_form": "게임을 어 막 그짓말로 하고 그래도 되는거야?!",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100006559.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ반성해??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ반성해??",
          "speaker_id": "2",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100006559.1.11",
          "form": "손 들어??",
          "original_form": "손 들어??",
          "speaker_id": "2",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100006559.1.12",
          "form": "어 !",
          "original_form": "어 !",
          "speaker_id": "1",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100006559.1.13",
          "form": "ㅋㅋㅋ안할거면서 하여튼 옵치 안망하길 기도해야겠다",
          "original_form": "ㅋㅋㅋ안할거면서 하여튼 옵치 안망하길 기도해야겠다",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100006559.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나중에 앞에서 인증할게ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나중에 앞에서 인증할게ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100006559.1.15",
          "form": "그래그래",
          "original_form": "그래그래",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100006559.1.16",
          "form": "그래도 옵치 아직 한 만원하나?",
          "original_form": "그래도 옵치 아직 한 만원하나?",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100006559.1.17",
          "form": "아근데 몹치 사도 할 컴터가 없네",
          "original_form": "아근데 몹치 사도 할 컴터가 없네",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100006559.1.18",
          "form": "바쁜건 둘째치구",
          "original_form": "바쁜건 둘째치구",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100006559.1.19",
          "form": "이만원인데",
          "original_form": "이만원인데",
          "speaker_id": "2",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100006559.1.20",
          "form": "추석이나 이럴 때 자주 할인하니까 만원대로 봐야지 뭐",
          "original_form": "추석이나 이럴 때 자주 할인하니까 만원대로 봐야지 뭐",
          "speaker_id": "2",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100006559.1.21",
          "form": "ㅋㅋㅋ아닝 세일하먄 만원 언저리 할거아냐 그래 맞자 그거 말이여",
          "original_form": "ㅋㅋㅋ아닝 세일하먄 만원 언저리 할거아냐 그래 맞자 그거 말이여",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100006559.1.22",
          "form": "응응!! 이제 다 아네",
          "original_form": "응응!! 이제 다 아네",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100006559.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100006559.1.24",
          "form": "그정돈 기억하지 ㅎㅎ",
          "original_form": "그정돈 기억하지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100006559.1.25",
          "form": "옵치 2는 안나오나보네?",
          "original_form": "옵치 2는 안나오나보네?",
          "speaker_id": "1",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100006559.1.26",
          "form": "좋아좋아",
          "original_form": "좋아좋아",
          "speaker_id": "2",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100006559.1.27",
          "form": "나왔는데 소리소문없이 사라졌을걸",
          "original_form": "나왔는데 소리소문없이 사라졌을걸",
          "speaker_id": "2",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100006559.1.28",
          "form": "아 아니구나",
          "original_form": "아 아니구나",
          "speaker_id": "2",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100006559.1.29",
          "form": "나도 한동안 관심없어서 몰랐는데 계속 연기됐네",
          "original_form": "나도 한동안 관심없어서 몰랐는데 계속 연기됐네",
          "speaker_id": "2",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100006559.1.30",
          "form": "아 글쿠만 !! 아주 신상 정보 고마워 ㅎㅎ",
          "original_form": "아 글쿠만 !! 아주 신상 정보 고마워 ㅎㅎ",
