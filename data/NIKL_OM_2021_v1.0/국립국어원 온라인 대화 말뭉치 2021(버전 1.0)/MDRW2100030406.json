      "utterance": [
        {
          "id": "MDRW2100030406.1.1",
          "form": "나머지 책들은 1권이 흥하니까 시리즈로 집필한 거거든요",
          "original_form": "나머지 책들은 1권이 흥하니까 시리즈로 집필한 거거든요",
          "speaker_id": "2",
          "time": "20210904 00:38"
        },
        {
          "id": "MDRW2100030406.1.2",
          "form": "개미 끝까지 다 읽으셨어요?ㅎ",
          "original_form": "개미 끝까지 다 읽으셨어요?ㅎ",
          "speaker_id": "1",
          "time": "20210904 00:38"
        },
        {
          "id": "MDRW2100030406.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210904 00:38"
        },
        {
          "id": "MDRW2100030406.1.4",
          "form": "워낙 한국에서 흥한 작가라",
          "original_form": "워낙 한국에서 흥한 작가라",
          "speaker_id": "2",
          "time": "20210904 00:39"
        },
        {
          "id": "MDRW2100030406.1.5",
          "form": "4권부터는 한국인 캐릭터도 나와요ㅋㅋ",
          "original_form": "4권부터는 한국인 캐릭터도 나와요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:39"
        },
        {
          "id": "MDRW2100030406.1.6",
          "form": "원래 시리즈물이 아녔던걸까요?",
          "original_form": "원래 시리즈물이 아녔던걸까요?",
          "speaker_id": "1",
          "time": "20210904 00:39"
        },
        {
          "id": "MDRW2100030406.1.7",
          "form": "아녔던거 같아요.",
          "original_form": "아녔던거 같아요.",
          "speaker_id": "2",
          "time": "20210904 00:39"
        },
        {
          "id": "MDRW2100030406.1.8",
          "form": "아 진짜요?ㅎㅎ한국인 캐릭터 나온다니 신기하네요",
          "original_form": "아 진짜요?ㅎㅎ한국인 캐릭터 나온다니 신기하네요",
          "speaker_id": "1",
          "time": "20210904 00:39"
        },
        {
          "id": "MDRW2100030406.1.9",
          "form": "1권만으로 충분히 완결성을 가지기때문에",
          "original_form": "1권만으로 충분히 완결성을 가지기때문에",
          "speaker_id": "2",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030406.1.10",
          "form": "약간 미드같은거죠. 미드도 흥하면 시즌이 막 20까지 나오고 그러잖아요",
          "original_form": "약간 미드같은거죠. 미드도 흥하면 시즌이 막 20까지 나오고 그러잖아요",
          "speaker_id": "2",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030406.1.11",
          "form": "네네 그렇게 나온 시리즈물은 항상...",
          "original_form": "네네 그렇게 나온 시리즈물은 항상...",
          "speaker_id": "1",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030406.1.12",
          "form": "개미도 봐야겠어요",
          "original_form": "개미도 봐야겠어요",
          "speaker_id": "1",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030406.1.13",
          "form": "읽을게 많네요ㅎㅎ잠중록 추천해줘서 보려고 대기중이었는데..",
          "original_form": "읽을게 많네요ㅎㅎ잠중록 추천해줘서 보려고 대기중이었는데..",
          "speaker_id": "1",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030406.1.14",
          "form": "저도 잠중록 읽어볼까 하긴 했었는데",
          "original_form": "저도 잠중록 읽어볼까 하긴 했었는데",
          "speaker_id": "2",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030406.1.15",
          "form": "추리물을 좋아하는 편이라서요",
          "original_form": "추리물을 좋아하는 편이라서요",
          "speaker_id": "2",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030406.1.16",
          "form": "저도 추리물 좋아해요ㅎㅎ드라마나 영화도 그렇고",
          "original_form": "저도 추리물 좋아해요ㅎㅎ드라마나 영화도 그렇고",
          "speaker_id": "1",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030406.1.17",
          "form": "다만 추리물도 읽다보면 패턴이 느껴져서 ㅠㅠ",
          "original_form": "다만 추리물도 읽다보면 패턴이 느껴져서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030406.1.18",
          "form": "휴지기를 가져야해요 ㅎㅎ",
          "original_form": "휴지기를 가져야해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030406.1.19",
          "form": "맞아요! 공감ㅋㅋ",
          "original_form": "맞아요! 공감ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030406.1.20",
          "form": "어릴땐 김진명도 자주 읽었었네요ㅋㅋ",
          "original_form": "어릴땐 김진명도 자주 읽었었네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030406.1.21",
          "form": "아... 그분것도 재밌긴하죠...",
          "original_form": "아... 그분것도 재밌긴하죠...",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030406.1.22",
          "form": "황태자비 납치사건인가 재밌게 봤던 기억이..",
          "original_form": "황태자비 납치사건인가 재밌게 봤던 기억이..",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030406.1.23",
          "form": "근데 어느순간 질리더라구요",
          "original_form": "근데 어느순간 질리더라구요",
          "speaker_id": "1",
          "time": "20210904 00:48"
        }
      ]
    }
