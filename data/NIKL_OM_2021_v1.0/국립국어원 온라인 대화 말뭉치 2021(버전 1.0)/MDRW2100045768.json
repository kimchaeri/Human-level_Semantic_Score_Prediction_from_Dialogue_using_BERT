      "utterance": [
        {
          "id": "MDRW2100045768.1.1",
          "form": "이제 좀 춥죠?",
          "original_form": "이제 좀 춥죠?",
          "speaker_id": "1",
          "time": "20210908 06:51"
        },
        {
          "id": "MDRW2100045768.1.2",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "2",
          "time": "20210908 06:54"
        },
        {
          "id": "MDRW2100045768.1.3",
          "form": "가을옷 꺼내셨나요?",
          "original_form": "가을옷 꺼내셨나요?",
          "speaker_id": "2",
          "time": "20210908 06:54"
        },
        {
          "id": "MDRW2100045768.1.4",
          "form": "이제 진짜 반팔이",
          "original_form": "이제 진짜 반팔이",
          "speaker_id": "1",
          "time": "20210908 06:55"
        },
        {
          "id": "MDRW2100045768.1.5",
          "form": "춥게 느껴져요",
          "original_form": "춥게 느껴져요",
          "speaker_id": "1",
          "time": "20210908 06:55"
        },
        {
          "id": "MDRW2100045768.1.6",
          "form": "옷정리 한번해야하는데...아직 미루고 있어요",
          "original_form": "옷정리 한번해야하는데...아직 미루고 있어요",
          "speaker_id": "1",
          "time": "20210908 06:55"
        },
        {
          "id": "MDRW2100045768.1.7",
          "form": "ㅋㅋㅋㅋ저두욬ㅋㅋㅋ아직 여름옷을 못넣어놔서",
          "original_form": "ㅋㅋㅋㅋ저두욬ㅋㅋㅋ아직 여름옷을 못넣어놔서",
          "speaker_id": "2",
          "time": "20210908 06:56"
        },
        {
          "id": "MDRW2100045768.1.8",
          "form": "못꺼내고 있어요!",
          "original_form": "못꺼내고 있어요!",
          "speaker_id": "2",
          "time": "20210908 06:56"
        },
        {
          "id": "MDRW2100045768.1.9",
          "form": "이러다 리빙박스에서 꺼내입다가 겨울올듯요 ㅎ",
          "original_form": "이러다 리빙박스에서 꺼내입다가 겨울올듯요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 06:56"
        },
        {
          "id": "MDRW2100045768.1.10",
          "form": "가을옷은 진짜 미리 준비해둬야겠어요",
          "original_form": "가을옷은 진짜 미리 준비해둬야겠어요",
          "speaker_id": "2",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045768.1.11",
          "form": "이렇게 금방 추워질줄은!",
          "original_form": "이렇게 금방 추워질줄은!",
          "speaker_id": "2",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045768.1.12",
          "form": "거긴 비오나요?",
          "original_form": "거긴 비오나요?",
          "speaker_id": "1",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045768.1.13",
          "form": "여긴 어제는 오더니 지금은 안오네요",
          "original_form": "여긴 어제는 오더니 지금은 안오네요",
          "speaker_id": "1",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045768.1.14",
          "form": "여기도요! 어제까지 비 스멀스멀 내리더니",
          "original_form": "여기도요! 어제까지 비 스멀스멀 내리더니",
          "speaker_id": "2",
          "time": "20210908 07:00"
        },
        {
          "id": "MDRW2100045768.1.15",
          "form": "오늘은 그냥 흐려요",
          "original_form": "오늘은 그냥 흐려요",
          "speaker_id": "2",
          "time": "20210908 07:00"
        },
        {
          "id": "MDRW2100045768.1.16",
          "form": "그래도 방심을 놓지 않고 우산 가져가려구요!",
          "original_form": "그래도 방심을 놓지 않고 우산 가져가려구요!",
          "speaker_id": "2",
          "time": "20210908 07:00"
        },
        {
          "id": "MDRW2100045768.1.17",
          "form": "태풍온데요",
          "original_form": "태풍온데요",
          "speaker_id": "1",
          "time": "20210908 07:00"
        },
        {
          "id": "MDRW2100045768.1.18",
          "form": "가을엔 트렌치도 좀 입어줘야 하는데",
          "original_form": "가을엔 트렌치도 좀 입어줘야 하는데",
          "speaker_id": "1",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045768.1.19",
          "form": "가을이 점점 짧아져서 트렌치 두번입고 나면",
          "original_form": "가을이 점점 짧아져서 트렌치 두번입고 나면",
          "speaker_id": "1",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045768.1.20",
          "form": "겨울이 오더라구요",
          "original_form": "겨울이 오더라구요",
          "speaker_id": "1",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045768.1.21",
          "form": "가을옷사기가 가장 애매한거 같아요",
          "original_form": "가을옷사기가 가장 애매한거 같아요",
          "speaker_id": "1",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045768.1.22",
          "form": "너무 짧아져서요",
          "original_form": "너무 짧아져서요",
          "speaker_id": "1",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045768.1.23",
          "form": "맞아요 트렌치",
          "original_form": "맞아요 트렌치",
          "speaker_id": "2",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100045768.1.24",
          "form": "1년중 딱 2주 입을 수 있는 옷이라는 별명도 있잖아욬ㅋㅋㅋ",
          "original_form": "1년중 딱 2주 입을 수 있는 옷이라는 별명도 있잖아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045768.1.25",
          "form": "근데 너무 예뻐서 또 사게되고",
          "original_form": "근데 너무 예뻐서 또 사게되고",
          "speaker_id": "2",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045768.1.26",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045768.1.27",
          "form": "그게 딱이네요 2주짜리 ㅋㅋㅋ",
          "original_form": "그게 딱이네요 2주짜리 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045768.1.28",
          "form": "글쵸...매년 계절바뀌면",
          "original_form": "글쵸...매년 계절바뀌면",
          "speaker_id": "1",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045768.1.29",
          "form": "입을게 없는거 같고",
          "original_form": "입을게 없는거 같고",
          "speaker_id": "1",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045768.1.30",
          "form": "작년에 머입었나 싶고요 ㅎ",
          "original_form": "작년에 머입었나 싶고요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045768.1.31",
          "form": "아 맞아요 이거 진짴ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 맞아요 이거 진짴ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045768.1.32",
          "form": "분명히 가을에 옷을 입었으니까 겨울이 오고 봄이 온 거일텐데",
          "original_form": "분명히 가을에 옷을 입었으니까 겨울이 오고 봄이 온 거일텐데",
          "speaker_id": "2",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045768.1.33",
          "form": "옷장이 텅 비었어요 다시 확인해보면",
          "original_form": "옷장이 텅 비었어요 다시 확인해보면",
          "speaker_id": "2",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100045768.1.34",
          "form": "또 사러나가야겠어요",
          "original_form": "또 사러나가야겠어요",
          "speaker_id": "2",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100045768.1.35",
          "form": "그러고 또사죠",
          "original_form": "그러고 또사죠",
          "speaker_id": "1",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100045768.1.36",
          "form": "이번에는 지름신 강림하면 안되는데 ㅎ",
          "original_form": "이번에는 지름신 강림하면 안되는데 ㅎ",
          "speaker_id": "1",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100045768.1.37",
          "form": "계절바뀌었다고 옷 하나 사줘야 제맛인데 ㅎ",
          "original_form": "계절바뀌었다고 옷 하나 사줘야 제맛인데 ㅎ",
          "speaker_id": "1",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100045768.1.38",
          "form": "ㅋㅋㅋㅋㅋ또 그 맛에 살아가는 것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋ또 그 맛에 살아가는 것 같아요",
          "speaker_id": "2",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100045768.1.39",
          "form": "계절바뀐 기념으로 옷 하나씩 사고 새로운 기분 내고",
          "original_form": "계절바뀐 기념으로 옷 하나씩 사고 새로운 기분 내고",
          "speaker_id": "2",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100045768.1.40",
          "form": "격하게 공감해요",
          "original_form": "격하게 공감해요",
          "speaker_id": "1",
          "time": "20210908 07:11"
        }
      ]
    }
