      "utterance": [
        {
          "id": "MDRW2100018728.1.1",
          "form": "다행히 비 안온다",
          "original_form": "다행히 비 안온다",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018728.1.2",
          "form": "ㅎㅎ 꼬맹이 교과서 받으러 갔어?",
          "original_form": "ㅎㅎ 꼬맹이 교과서 받으러 갔어?",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018728.1.3",
          "form": "ㅇㅇ 12시 반~40분 사이에 오라고 하셔서",
          "original_form": "ㅇㅇ 12시 반~40분 사이에 오라고 하셔서",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018728.1.4",
          "form": "시간 텀이 너무 빡빡하네 ㅎㅎ",
          "original_form": "시간 텀이 너무 빡빡하네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018728.1.5",
          "form": "많이 흐리네",
          "original_form": "많이 흐리네",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018728.1.6",
          "form": "그래도 비 안와서 어디야 ㅎㅎ",
          "original_form": "그래도 비 안와서 어디야 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018728.1.7",
          "form": "ㅇㅇ 진짜 다행이야",
          "original_form": "ㅇㅇ 진짜 다행이야",
          "speaker_id": "1",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018728.1.8",
          "form": "색시의 기도를 들어주셨나보다 ㅋ",
          "original_form": "색시의 기도를 들어주셨나보다 ㅋ",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018728.1.9",
          "form": "큰아들은 2시 이후에 오라고 하셔서 오후 내내 비 안와야 해 ㅎㅎ",
          "original_form": "큰아들은 2시 이후에 오라고 하셔서 오후 내내 비 안와야 해 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018728.1.10",
          "form": "이런 한 고비가 더 남았구만 ㅋ",
          "original_form": "이런 한 고비가 더 남았구만 ㅋ",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018728.1.11",
          "form": "ㅇㅇ 사실 큰아들이 더 걱정이야 힘이 없어서",
          "original_form": "ㅇㅇ 사실 큰아들이 더 걱정이야 힘이 없어서",
          "speaker_id": "1",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018728.1.12",
          "form": "ㅎㅎ 잘 가지고 오겠지 뭐",
          "original_form": "ㅎㅎ 잘 가지고 오겠지 뭐",
          "speaker_id": "2",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018728.1.13",
          "form": "부디 지금처럼만.. 비 오면 안돼 ㅠ",
          "original_form": "부디 지금처럼만.. 비 오면 안돼 ㅠ",
          "speaker_id": "1",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018728.1.14",
