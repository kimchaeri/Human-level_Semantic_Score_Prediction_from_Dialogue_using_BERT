      "utterance": [
        {
          "id": "MDRW2100014682.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014682.1.2",
          "form": "혹시 정기결제하는 ott 있으신가요.",
          "original_form": "혹시 정기결제하는 ott 있으신가요.",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014682.1.3",
          "form": "저는 유튜브 프리미엄이랑 왓챠랑 티빙이랑 웨이브요 ㅋㅋㅋㅋ",
          "original_form": "저는 유튜브 프리미엄이랑 왓챠랑 티빙이랑 웨이브요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014682.1.4",
          "form": "우와 많이 보시네요",
          "original_form": "우와 많이 보시네요",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014682.1.5",
          "form": "가격이 부담되시진 않으세요?",
          "original_form": "가격이 부담되시진 않으세요?",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014682.1.6",
          "form": "보통 4인팟 그런거 하니까",
          "original_form": "보통 4인팟 그런거 하니까",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014682.1.7",
          "form": "별로 부담되지는 않능거 같아여",
          "original_form": "별로 부담되지는 않능거 같아여",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014682.1.8",
          "form": "아 그럴 수 있겠어요",
          "original_form": "아 그럴 수 있겠어요",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014682.1.9",
          "form": "요즘에 뭐 재밌게 보셨어요?",
          "original_form": "요즘에 뭐 재밌게 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014682.1.10",
          "form": "저는 란티모스 감독 더 랍스타....",
          "original_form": "저는 란티모스 감독 더 랍스타....",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014682.1.11",
          "form": "이게 왓챠밖에 안 보여서 왓챠 끊었지 뭐예요....",
          "original_form": "이게 왓챠밖에 안 보여서 왓챠 끊었지 뭐예요....",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014682.1.12",
          "form": "저는 대탈출이여",
          "original_form": "저는 대탈출이여",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014682.1.13",
          "form": "저는 그거 보진 않는데 그거 이번에 레전드 찍었다는 기사는 봤어요",
          "original_form": "저는 그거 보진 않는데 그거 이번에 레전드 찍었다는 기사는 봤어요",
          "speaker_id": "1",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014682.1.14",
          "form": "맞아여 1234 다봤는ㄷ",
          "original_form": "맞아여 1234 다봤는ㄷ",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014682.1.15",
          "form": "점점 더 재밌어져여",
          "original_form": "점점 더 재밌어져여",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014682.1.16",
          "form": "어렸을 때 티비에 공중파 채널들만 딱 나오던 때",
          "original_form": "어렸을 때 티비에 공중파 채널들만 딱 나오던 때",
          "speaker_id": "1",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014682.1.17",
          "form": "어떻게 살았나 모르겠어요",
          "original_form": "어떻게 살았나 모르겠어요",
          "speaker_id": "1",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014682.1.18",
          "form": "채널 많아지니 프로그램 많아져서 좋아요",
          "original_form": "채널 많아지니 프로그램 많아져서 좋아요",
          "speaker_id": "1",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014682.1.19",
          "form": "마자요 ㅋㅋㅋㅋㅋ",
          "original_form": "마자요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014682.1.20",
          "form": "유튜브는 뭐 구독해서 보는 채널 잇으세영",
          "original_form": "유튜브는 뭐 구독해서 보는 채널 잇으세영",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014682.1.21",
          "form": "저는 옛날 드라마나 예능 짧게 압축해서 보여주는 채널들 좋아해요",
          "original_form": "저는 옛날 드라마나 예능 짧게 압축해서 보여주는 채널들 좋아해요",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014682.1.22",
          "form": "봐도 봐도 웃긴 장면들 보고 또 웃고 ㅋㅋㅋ",
          "original_form": "봐도 봐도 웃긴 장면들 보고 또 웃고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014682.1.23",
          "form": "추억팔이도 하고 ㅋㅋㅋ",
          "original_form": "추억팔이도 하고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014682.1.24",
          "form": "뭐 추천해주실 채널 있나요?",
          "original_form": "뭐 추천해주실 채널 있나요?",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014682.1.25",
          "form": "아녀!!!",
          "original_form": "아녀!!!",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014682.1.26",
          "form": "ㅋㅋㅋㅋㅋ 저는 런닝맨이나 무도 클립 같은거 봐여",
          "original_form": "ㅋㅋㅋㅋㅋ 저는 런닝맨이나 무도 클립 같은거 봐여",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014682.1.27",
          "form": "맞아요 무도 진짜 레전드 ㅋㅋㅋㅋ",
          "original_form": "맞아요 무도 진짜 레전드 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014682.1.28",
          "form": "다신 그런 예능 안 나올 것 같아요",
          "original_form": "다신 그런 예능 안 나올 것 같아요",
