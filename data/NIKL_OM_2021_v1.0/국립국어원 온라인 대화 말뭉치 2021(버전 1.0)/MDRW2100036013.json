      "utterance": [
        {
          "id": "MDRW2100036013.1.1",
          "form": "저는 잠깨려고",
          "original_form": "저는 잠깨려고",
          "speaker_id": "1",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100036013.1.2",
          "form": "커피 사러 왔어요",
          "original_form": "커피 사러 왔어요",
          "speaker_id": "1",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100036013.1.3",
          "form": "ㅋㅋ저는 커피를 마시고있어요",
          "original_form": "ㅋㅋ저는 커피를 마시고있어요",
          "speaker_id": "2",
          "time": "20210830 15:25"
        },
        {
          "id": "MDRW2100036013.1.4",
          "form": "ㅋㅋㅋㅋ다들 비슷하게 사는군요,.",
          "original_form": "ㅋㅋㅋㅋ다들 비슷하게 사는군요,.",
          "speaker_id": "1",
          "time": "20210830 16:36"
        },
        {
          "id": "MDRW2100036013.1.5",
          "form": "저 스타벅스 오늘의 커피+텀블러 할인받아서 3500원에 있다가 왔답니다",
          "original_form": "저 스타벅스 오늘의 커피+텀블러 할인받아서 3500원에 있다가 왔답니다",
          "speaker_id": "1",
          "time": "20210830 17:00"
        },
        {
          "id": "MDRW2100036013.1.6",
          "form": "오 3500원이면싼거군요",
          "original_form": "오 3500원이면싼거군요",
          "speaker_id": "2",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100036013.1.7",
          "form": "저는 많이파는데",
          "original_form": "저는 많이파는데",
          "speaker_id": "2",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100036013.1.8",
          "form": "한번도 3500원에판적없어요ㅜ",
          "original_form": "한번도 3500원에판적없어요ㅜ",
          "speaker_id": "2",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100036013.1.9",
          "form": "호구당해서 매번 3200원?",
          "original_form": "호구당해서 매번 3200원?",
          "speaker_id": "2",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100036013.1.10",
          "form": "아 커피를 파세요?",
          "original_form": "아 커피를 파세요?",
          "speaker_id": "1",
          "time": "20210830 17:40"
        },
        {
          "id": "MDRW2100036013.1.11",
          "form": "아 스벅같은 기프트콘은 좀 팔아요",
          "original_form": "아 스벅같은 기프트콘은 좀 팔아요",
          "speaker_id": "2",
          "time": "20210830 18:26"
        },
        {
          "id": "MDRW2100036013.1.12",
          "form": "저는 싼 프랜차이즈 마시고 ㅋㅋ",
          "original_form": "저는 싼 프랜차이즈 마시고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:27"
        },
        {
          "id": "MDRW2100036013.1.13",
          "form": "아 기프티콘을 파셨군요",
          "original_form": "아 기프티콘을 파셨군요",
          "speaker_id": "1",
          "time": "20210830 18:31"
        },
        {
          "id": "MDRW2100036013.1.14",
          "form": "네 근데 다들 어캐 비싸게 파시나여 싶어요 ㅋㅋ",
          "original_form": "네 근데 다들 어캐 비싸게 파시나여 싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100036013.1.15",
          "form": "앜ㅋㅋㅋㅋㅋ 알뜰하시다",
          "original_form": "앜ㅋㅋㅋㅋㅋ 알뜰하시다",
          "speaker_id": "1",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036013.1.16",
          "form": "저는 걍 기프티스타에 팔아서 어케 비싸게파는지 모르겟네요",
          "original_form": "저는 걍 기프티스타에 팔아서 어케 비싸게파는지 모르겟네요",
          "speaker_id": "1",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036013.1.17",
          "form": "저는 당근이나 중나에 보통올려요 ㅋㅋ",
          "original_form": "저는 당근이나 중나에 보통올려요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100036013.1.18",
          "form": "아 라면 먹고싶네요ㅜㅜ",
          "original_form": "아 라면 먹고싶네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100036013.1.19",
          "form": "야밤에 라면땡겨요",
          "original_form": "야밤에 라면땡겨요",
          "speaker_id": "1",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100036013.1.20",
          "form": "저 방금 부대찌개에",
          "original_form": "저 방금 부대찌개에",
          "speaker_id": "1",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100036013.1.21",
          "form": "라면 넣어 먹엇어요",
          "original_form": "라면 넣어 먹엇어요",
          "speaker_id": "1",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100036013.1.22",
          "form": "악ㅜㅜ 저는 진짜 다이어트만 성공하면 먹고싶은거 넘많아요ㅜㅜ",
          "original_form": "악ㅜㅜ 저는 진짜 다이어트만 성공하면 먹고싶은거 넘많아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 08:44"
        },
        {
          "id": "MDRW2100036013.1.23",
          "form": "악 다이어트면 식이조절하시나요",
          "original_form": "악 다이어트면 식이조절하시나요",
