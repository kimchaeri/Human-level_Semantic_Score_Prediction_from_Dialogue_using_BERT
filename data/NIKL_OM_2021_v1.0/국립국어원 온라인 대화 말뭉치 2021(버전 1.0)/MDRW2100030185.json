      "utterance": [
        {
          "id": "MDRW2100030185.1.1",
          "form": "오 name1님",
          "original_form": "오 &name1&님",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030185.1.2",
          "form": "무슨 일 하세요~??",
          "original_form": "무슨 일 하세요~??",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030185.1.3",
          "form": "동아리얘기하신거 보면 학생이신가요?",
          "original_form": "동아리얘기하신거 보면 학생이신가요?",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030185.1.4",
          "form": "네네 저 학생이에요!",
          "original_form": "네네 저 학생이에요!",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030185.1.5",
          "form": "직업이라고 하긴 그렇고 알바 느낌으로 과외하고 있어요!",
          "original_form": "직업이라고 하긴 그렇고 알바 느낌으로 과외하고 있어요!",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030185.1.6",
          "form": "name2님은 아르바이크 하시나요??",
          "original_form": "&name2&님은 아르바이크 하시나요??",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030185.1.7",
          "form": "음 지금은 아무것도 안하고 있어요!",
          "original_form": "음 지금은 아무것도 안하고 있어요!",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030185.1.8",
          "form": "원래 서빙알바와 인턴 하다가 이번 학기엔",
          "original_form": "원래 서빙알바와 인턴 하다가 이번 학기엔",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030185.1.9",
          "form": "좀 쉬려고요ㅎㅎㅜ",
          "original_form": "좀 쉬려고요ㅎㅎㅜ",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030185.1.10",
          "form": "과외는 어떠세요?",
          "original_form": "과외는 어떠세요?",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030185.1.11",
          "form": "학생이 말 잘 듣나요?ㅎㅎㅎ",
          "original_form": "학생이 말 잘 듣나요?ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030185.1.12",
          "form": "우와 인턴... 멋있네요",
          "original_form": "우와 인턴... 멋있네요",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030185.1.13",
          "form": "저도 인턴 해보고싶어요..!",
          "original_form": "저도 인턴 해보고싶어요..!",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030185.1.14",
          "form": "학생이 말을... 매우 안들어서 그만 두고 싶네요ㅜㅜㅜㅜ",
          "original_form": "학생이 말을... 매우 안들어서 그만 두고 싶네요ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030185.1.15",
          "form": "ㅜㅜㅠ 저도 과외할 때 참 힘들었던 기억이 있네요",
          "original_form": "ㅜㅜㅠ 저도 과외할 때 참 힘들었던 기억이 있네요",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030185.1.16",
          "form": "숙제도 안해오고 집중도 안하고....",
          "original_form": "숙제도 안해오고 집중도 안하고....",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030185.1.17",
          "form": "어휴",
          "original_form": "어휴",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030185.1.18",
          "form": "과외가 은근 스트레스 너무 받아요",
          "original_form": "과외가 은근 스트레스 너무 받아요",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030185.1.19",
          "form": "차라리 그냥 편의점이나 카페 이런데서 하는게 정신적 스트레스는 덜한거같아요ㅜㅜ",
          "original_form": "차라리 그냥 편의점이나 카페 이런데서 하는게 정신적 스트레스는 덜한거같아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030185.1.20",
          "form": "맞아요 ㅜㅠㅠ",
          "original_form": "맞아요 ㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030185.1.21",
          "form": "아 근데 또 서빙알바할때 생각하면",
          "original_form": "아 근데 또 서빙알바할때 생각하면",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030185.1.22",
          "form": "음......",
          "original_form": "음......",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030185.1.23",
          "form": "진상 아재들때문에 너무 스트레스 받았어요",
          "original_form": "진상 아재들때문에 너무 스트레스 받았어요",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030185.1.24",
          "form": "어우... 그거는 그거대로 스트레스 많이 받겠네요",
          "original_form": "어우... 그거는 그거대로 스트레스 많이 받겠네요",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030185.1.25",
          "form": "네ㅜㅠㅠ",
          "original_form": "네ㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030185.1.26",
          "form": "심심2님은 뭔가 저학년이실것같은데",
          "original_form": "심심2님은 뭔가 저학년이실것같은데",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030185.1.27",
          "form": "맞나요??",
          "original_form": "맞나요??",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030185.1.28",
          "form": "앗 저 4학년이에요ㅋㅋㅋㅋ",
          "original_form": "앗 저 4학년이에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030185.1.29",
          "form": "앗ㅋㅋㅋㅋㅋㅋㅋㅋ죄송해요",
          "original_form": "앗ㅋㅋㅋㅋㅋㅋㅋㅋ죄송해요",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030185.1.30",
          "form": "저도 4학년입니닷ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 4학년입니닷ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030185.1.31",
          "form": "근데 제가 다니는 과가 4년제는 아니라 졸업은 꽤 남았어요ㅋㅋㅋ",
          "original_form": "근데 제가 다니는 과가 4년제는 아니라 졸업은 꽤 남았어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030185.1.32",
          "form": "오오 그렇군요!!",
          "original_form": "오오 그렇군요!!",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030185.1.33",
          "form": "전 졸업을 앞두고 있네요....하핫...ㅠ",
          "original_form": "전 졸업을 앞두고 있네요....하핫...ㅠ",
          "speaker_id": "2",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030185.1.34",
          "form": "졸업 앞두시고 바쁘겠네요ㅜㅜ",
          "original_form": "졸업 앞두시고 바쁘겠네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030185.1.35",
          "form": "주위 친구들 취준하느라 바쁘더라구요ㅜㅜ",
          "original_form": "주위 친구들 취준하느라 바쁘더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030185.1.36",
          "form": "네ㅜㅜㅠ 뭔가 쫓기는 느낌이 들어요",
          "original_form": "네ㅜㅜㅠ 뭔가 쫓기는 느낌이 들어요",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030185.1.37",
          "form": "빨리 취업해야 할 것 같고, 자격증 학점 다 챙겨야할 것 같고ㅎㅎ휴ㅠㅠㅠㅠㅠ",
