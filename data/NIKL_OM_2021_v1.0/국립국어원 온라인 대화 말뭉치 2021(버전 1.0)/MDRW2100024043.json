      "utterance": [
        {
          "id": "MDRW2100024043.1.1",
          "form": "요즘 커뮤 자주 하시나요?",
          "original_form": "요즘 커뮤 자주 하시나요?",
          "speaker_id": "2",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100024043.1.2",
          "form": "자주 안해요",
          "original_form": "자주 안해요",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024043.1.3",
          "form": "저도요 요즘 심심이에 빠져있어서요",
          "original_form": "저도요 요즘 심심이에 빠져있어서요",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024043.1.4",
          "form": "오 글쿤여",
          "original_form": "오 글쿤여",
          "speaker_id": "1",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100024043.1.5",
          "form": "그래도 가끔은 들어가야해요.",
          "original_form": "그래도 가끔은 들어가야해요.",
          "speaker_id": "1",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100024043.1.6",
          "form": "그건 그래요 정보를 알아야 해서",
          "original_form": "그건 그래요 정보를 알아야 해서",
          "speaker_id": "2",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024043.1.7",
          "form": "안들어가면 손해보는느낌이에요.\r\n오늘은 양파를 3킬로 중짜리 700원에삿어요",
          "original_form": "안들어가면 손해보는느낌이에요.\r\n오늘은 양파를 3킬로 중짜리 700원에삿어요",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024043.1.8",
          "form": "와 저희집은 그거 오천원에 샀었대요",
          "original_form": "와 저희집은 그거 오천원에 샀었대요",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024043.1.9",
          "form": "오... 아쉽다",
          "original_form": "오... 아쉽다",
          "speaker_id": "1",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100024043.1.10",
          "form": "양파로 만드실거 있나요?",
          "original_form": "양파로 만드실거 있나요?",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100024043.1.11",
          "form": "양파 감자 슾?",
          "original_form": "양파 감자 슾?",
          "speaker_id": "1",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100024043.1.12",
          "form": "엥.. 양파슾 시러요",
          "original_form": "엥.. 양파슾 시러요",
          "speaker_id": "2",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100024043.1.13",
          "form": "감자슾인데",
          "original_form": "감자슾인데",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100024043.1.14",
          "form": "전 양파 절임 좋아해요",
          "original_form": "전 양파 절임 좋아해요",
