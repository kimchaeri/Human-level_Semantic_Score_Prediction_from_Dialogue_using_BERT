      "utterance": [
        {
          "id": "MDRW2100026619.1.1",
          "form": "좋아하는 메뉴 있어?",
          "original_form": "좋아하는 메뉴 있어?",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026619.1.2",
          "form": "나는 피자!",
          "original_form": "나는 피자!",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026619.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026619.1.4",
          "form": "난 떡볶이!",
          "original_form": "난 떡볶이!",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026619.1.5",
          "form": "피자 브랜드는 좋아하는데 있어?",
          "original_form": "피자 브랜드는 좋아하는데 있어?",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026619.1.6",
          "form": "아 맛있겟다 나 아직도 배떡 못먹어봣어ㅜㅜ",
          "original_form": "아 맛있겟다 나 아직도 배떡 못먹어봣어ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026619.1.7",
          "form": "나 류길상이랑 도미노!",
          "original_form": "나 류길상이랑 도미노!",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026619.1.8",
          "form": "혼자먹을땐 피자스쿨이 젤 만만하고!",
          "original_form": "혼자먹을땐 피자스쿨이 젤 만만하고!",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026619.1.9",
          "form": "도미노에 비해 피자스쿨은 약간 친근한 느낌이지 않아?",
          "original_form": "도미노에 비해 피자스쿨은 약간 친근한 느낌이지 않아?",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026619.1.10",
          "form": "그치그치 사이즈도 작고 좀 저렴해서",
          "original_form": "그치그치 사이즈도 작고 좀 저렴해서",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026619.1.11",
          "form": "맞아 도미노는 비싸....ㅠㅠ",
          "original_form": "맞아 도미노는 비싸....ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026619.1.12",
          "form": "너무 비싸지ㅜㅜ",
          "original_form": "너무 비싸지ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026619.1.13",
          "form": "치킨도 그렇고 요즘 배달음식 너무 비싸지 않아?",
          "original_form": "치킨도 그렇고 요즘 배달음식 너무 비싸지 않아?",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026619.1.14",
          "form": "맞아 특히 떡볶이 좀 양심없어",
          "original_form": "맞아 특히 떡볶이 좀 양심없어",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026619.1.15",
          "form": "맞아맞아 맛은 있지만... 너무비싸",
          "original_form": "맞아맞아 맛은 있지만... 너무비싸",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026619.1.16",
          "form": "ㅜㅜ시켜먹기에 부담스런 가격이여ㅜㅜ",
          "original_form": "ㅜㅜ시켜먹기에 부담스런 가격이여ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026619.1.17",
          "form": "음식가격도 게다가 비싼데 배달비도 받지...",
          "original_form": "음식가격도 게다가 비싼데 배달비도 받지...",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026619.1.18",
          "form": "맞아 ㅋㅋㅋㅋㅋ 차라리 픽업해와서 먹을래ㅜ",
          "original_form": "맞아 ㅋㅋㅋㅋㅋ 차라리 픽업해와서 먹을래ㅜ",
          "speaker_id": "2",
          "time": "20210902 07:28"
        },
        {
          "id": "MDRW2100026619.1.19",
          "form": "나도 이젠 픽업은 하는데 배달은 안시켜ㅋㅋㅋㅋ",
          "original_form": "나도 이젠 픽업은 하는데 배달은 안시켜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100026619.1.20",
          "form": "ㅋㅋㅋㅋ그럴만도ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋ그럴만도ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100026619.1.21",
          "form": "포장할인도해주는데",
          "original_form": "포장할인도해주는데",
          "speaker_id": "2",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100026619.1.22",
          "form": "맞아 포장은 오히려 할인까지 해준다고!",
          "original_form": "맞아 포장은 오히려 할인까지 해준다고!",
          "speaker_id": "1",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100026619.1.23",
          "form": "그치그치 ㅋㅋㅋ",
          "original_form": "그치그치 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100026619.1.24",
          "form": "배달의 민족 나왔던 초창기엔 거의 배달료 없어서 잘썼는데",
          "original_form": "배달의 민족 나왔던 초창기엔 거의 배달료 없어서 잘썼는데",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100026619.1.25",
          "form": "이제는...오히려 잘 안쓰게 되는거 같아",
          "original_form": "이제는...오히려 잘 안쓰게 되는거 같아",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100026619.1.26",
          "form": "ㅠ맞아ㅠ수수료도 너무 많이떼더라 (가게쪽에서",
          "original_form": "ㅠ맞아ㅠ수수료도 너무 많이떼더라 (가게쪽에서",
          "speaker_id": "2",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100026619.1.27",
          "form": "처음엔 좋았는데 이젠 시국만 아니면 식당가서 먹게되는거 같아ㅠ",
          "original_form": "처음엔 좋았는데 이젠 시국만 아니면 식당가서 먹게되는거 같아ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100026619.1.28",
          "form": "그치ㅠㅜ 안그래도 나도 오늘 시장에서 칼국수 먹었어ㅠㅠㅋㅋ",
          "original_form": "그치ㅠㅜ 안그래도 나도 오늘 시장에서 칼국수 먹었어ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100026619.1.29",
          "form": "오 칼국수!! 맛있었겠다!!",
          "original_form": "오 칼국수!! 맛있었겠다!!",
          "speaker_id": "1",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100026619.1.30",
          "form": "ㅎㅎ 너무많이먹어서 배터질뻔 ㅋㅋ 넌 뭐 먹었어?",
          "original_form": "ㅎㅎ 너무많이먹어서 배터질뻔 ㅋㅋ 넌 뭐 먹었어?",
          "speaker_id": "2",
          "time": "20210902 23:04"
        }
      ]
