      "utterance": [
        {
          "id": "MDRW2100011778.1.1",
          "form": "재택 근무 해보셨나요",
          "original_form": "재택 근무 해보셨나요",
          "speaker_id": "1",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011778.1.2",
          "form": "아니요 전 취업을 아직 못해서요 재택 근무 해보셨나요?",
          "original_form": "아니요 전 취업을 아직 못해서요 재택 근무 해보셨나요?",
          "speaker_id": "2",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011778.1.3",
          "form": "요즘은 아니고",
          "original_form": "요즘은 아니고",
          "speaker_id": "1",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011778.1.4",
          "form": "회사가 확장이전한다고",
          "original_form": "회사가 확장이전한다고",
          "speaker_id": "1",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011778.1.5",
          "form": "재택근무 한적있었어요",
          "original_form": "재택근무 한적있었어요",
          "speaker_id": "1",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011778.1.6",
          "form": "오오 며칠동안 했나요?",
          "original_form": "오오 며칠동안 했나요?",
          "speaker_id": "2",
          "time": "20210824 16:06"
        },
        {
          "id": "MDRW2100011778.1.7",
          "form": "2개월 정도 했어요",
          "original_form": "2개월 정도 했어요",
          "speaker_id": "1",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011778.1.8",
          "form": "와 엄청 길었네요 재택 근무 할 때 불편한 점이 있었나요?",
          "original_form": "와 엄청 길었네요 재택 근무 할 때 불편한 점이 있었나요?",
          "speaker_id": "2",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011778.1.9",
          "form": "집중이 잘 안돼요",
          "original_form": "집중이 잘 안돼요",
          "speaker_id": "1",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011778.1.10",
          "form": "자꾸 눕고 싶고ㅋㅋ",
          "original_form": "자꾸 눕고 싶고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011778.1.11",
          "form": "내가 일하는건지 쉬는건지",
          "original_form": "내가 일하는건지 쉬는건지",
          "speaker_id": "1",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011778.1.12",
          "form": "구분이 안됩니다",
          "original_form": "구분이 안됩니다",
          "speaker_id": "1",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011778.1.13",
          "form": "아ㅋㅋㅋㅋㅋ 일터와 집의 경계가 사라지는군요",
          "original_form": "아ㅋㅋㅋㅋㅋ 일터와 집의 경계가 사라지는군요",
          "speaker_id": "2",
          "time": "20210824 16:19"
        },
        {
          "id": "MDRW2100011778.1.14",
          "form": "그럼 그럴 땐 어떻게 하셨나요?",
          "original_form": "그럼 그럴 땐 어떻게 하셨나요?",
          "speaker_id": "2",
          "time": "20210824 16:19"
        },
        {
          "id": "MDRW2100011778.1.15",
          "form": "오은영 박사님 영상보고",
          "original_form": "오은영 박사님 영상보고",
          "speaker_id": "1",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011778.1.16",
          "form": "정신차립니다",
          "original_form": "정신차립니다",
          "speaker_id": "1",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011778.1.17",
          "form": "ㅋㅋㅋㅋ아 그렇군요",
          "original_form": "ㅋㅋㅋㅋ아 그렇군요",
          "speaker_id": "2",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100011778.1.18",
          "form": "그래도 재택근무의 장점이 더 많죠?",
          "original_form": "그래도 재택근무의 장점이 더 많죠?",
          "speaker_id": "2",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100011778.1.19",
          "form": "있죠",
          "original_form": "있죠",
          "speaker_id": "1",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100011778.1.20",
          "form": "상사 안봐도됨",
          "original_form": "상사 안봐도됨",
          "speaker_id": "1",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100011778.1.21",
          "form": "넘사벽 장점이죠ㅋㅋ",
          "original_form": "넘사벽 장점이죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100011778.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 최고로 중요한 장점이죠ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 최고로 중요한 장점이죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100011778.1.23",
          "form": "그리고 통근에 필요한 시간이 줄어드는 것도 좋을 거 같아요!",
          "original_form": "그리고 통근에 필요한 시간이 줄어드는 것도 좋을 거 같아요!",
          "speaker_id": "2",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100011778.1.24",
          "form": "생각해보니까",
          "original_form": "생각해보니까",
          "speaker_id": "1",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100011778.1.25",
          "form": "사람들이랑 안부대껴서 좋았어요",
          "original_form": "사람들이랑 안부대껴서 좋았어요",
          "speaker_id": "1",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100011778.1.26",
          "form": "대중교통에서",
          "original_form": "대중교통에서",
          "speaker_id": "1",
          "time": "20210824 16:49"
        }
      ]
    }
  ]
