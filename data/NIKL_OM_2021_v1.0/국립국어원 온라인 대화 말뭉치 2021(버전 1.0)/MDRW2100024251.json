      "utterance": [
        {
          "id": "MDRW2100024251.1.1",
          "form": "님 요새 뭐 산 거 있음?",
          "original_form": "님 요새 뭐 산 거 있음?",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024251.1.2",
          "form": "나는 매트리스 사고 싶더라",
          "original_form": "나는 매트리스 사고 싶더라",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024251.1.3",
          "form": "아니면 모니터.. 중고로 싸게 하나 사고싶음",
          "original_form": "아니면 모니터.. 중고로 싸게 하나 사고싶음",
          "speaker_id": "1",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100024251.1.4",
          "form": "나 지금 한복 펀딩 때문에 고민 중님",
          "original_form": "나 지금 한복 펀딩 때문에 고민 중님",
          "speaker_id": "2",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100024251.1.5",
          "form": "사려고 점 찍어둔 한복원피스가 있는데 로브도 같이 되거든ㅋㅋㅋ",
          "original_form": "사려고 점 찍어둔 한복원피스가 있는데 로브도 같이 되거든ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100024251.1.6",
          "form": "사라사",
          "original_form": "사라사",
          "speaker_id": "1",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100024251.1.7",
          "form": "오늘까지 펀딩인데",
          "original_form": "오늘까지 펀딩인데",
          "speaker_id": "2",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100024251.1.8",
          "form": "어서 사",
          "original_form": "어서 사",
          "speaker_id": "1",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100024251.1.9",
          "form": "3벌+덧치마3벌에 45만임",
          "original_form": "3벌+덧치마3벌에 45만임",
          "speaker_id": "2",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100024251.1.10",
          "form": "히익",
          "original_form": "히익",
          "speaker_id": "1",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100024251.1.11",
          "form": "한벌만 하면 20만원이라ㅋㅋㅋ",
          "original_form": "한벌만 하면 20만원이라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100024251.1.12",
          "form": "너무 비싸요",
          "original_form": "너무 비싸요",
          "speaker_id": "1",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100024251.1.13",
          "form": "그럴바엔 3벌이 훨씬 낫다 싶어서",
          "original_form": "그럴바엔 3벌이 훨씬 낫다 싶어서",
          "speaker_id": "2",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100024251.1.14",
          "form": "근데 그렇게 보면 또",
          "original_form": "근데 그렇게 보면 또",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024251.1.15",
          "form": "45만원이 나은 거 같기도",
          "original_form": "45만원이 나은 거 같기도",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024251.1.16",
          "form": "한복은 원단 때문에 어쩔 수 없겠다 생각은 하는데",
          "original_form": "한복은 원단 때문에 어쩔 수 없겠다 생각은 하는데",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024251.1.17",
          "form": "고민되는건 어쩔 수 없다",
          "original_form": "고민되는건 어쩔 수 없다",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024251.1.18",
          "form": "할부면 아마 고민 덜 했을 듯",
          "original_form": "할부면 아마 고민 덜 했을 듯",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024251.1.19",
          "form": "한복지가 특히 비싸진 않을텐데",
          "original_form": "한복지가 특히 비싸진 않을텐데",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024251.1.20",
          "form": "뭐 비단으로 만드는 것도 아니고",
          "original_form": "뭐 비단으로 만드는 것도 아니고",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024251.1.21",
          "form": "할부 안됨?",
          "original_form": "할부 안됨?",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024251.1.22",
          "form": "ㅇㅇ텀블벅이라",
          "original_form": "ㅇㅇ텀블벅이라",
          "speaker_id": "2",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100024251.1.23",
          "form": "하긴 할부가 될 리가 없네",
          "original_form": "하긴 할부가 될 리가 없네",
          "speaker_id": "1",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100024251.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100024251.1.25",
          "form": "텀블벅은 할인도 안하고",
          "original_form": "텀블벅은 할인도 안하고",
          "speaker_id": "1",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100024251.1.26",
          "form": "얼리버드래봤자 별로 안쌈",
          "original_form": "얼리버드래봤자 별로 안쌈",
          "speaker_id": "1",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100024251.1.27",
          "form": "나는 모니터 사고싶다",
          "original_form": "나는 모니터 사고싶다",
          "speaker_id": "1",
          "time": "20210831 16:55"
        },
        {
          "id": "MDRW2100024251.1.28",
          "form": "노트북 넘 작아서 화면 큰걸로 보고싶음",
          "original_form": "노트북 넘 작아서 화면 큰걸로 보고싶음",
          "speaker_id": "1",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100024251.1.29",
          "form": "맞제 8ㅁ8 그래서 고민 중",
          "original_form": "맞제 8ㅁ8 그래서 고민 중",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024251.1.30",
          "form": "모니터 좋지",
          "original_form": "모니터 좋지",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024251.1.31",
          "form": "나는 동생 컴퓨터 내 방으로 옮기고 싶은데",
          "original_form": "나는 동생 컴퓨터 내 방으로 옮기고 싶은데",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024251.1.32",
          "form": "아예 모니터 새걸로 살까도 했는데",
          "original_form": "아예 모니터 새걸로 살까도 했는데",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024251.1.33",
          "form": "십만원씩 하더라고...??ㅇ",
          "original_form": "십만원씩 하더라고...??ㅇ",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024251.1.34",
          "form": "원래 이렇게 비쌌나 싶고",
          "original_form": "원래 이렇게 비쌌나 싶고",
          "speaker_id": "1",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100024251.1.35",
          "form": "내 예상보단 싼거 같은데",
          "original_form": "내 예상보단 싼거 같은데",
          "speaker_id": "2",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100024251.1.36",
          "form": "요새 워낙 값이 비싸져서",
          "original_form": "요새 워낙 값이 비싸져서",
          "speaker_id": "2",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100024251.1.37",
          "form": "하긴 그래",
          "original_form": "하긴 그래",
          "speaker_id": "1",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100024251.1.38",
          "form": "컴퓨터 맞출래도 힘듬 비싸서",
          "original_form": "컴퓨터 맞출래도 힘듬 비싸서",
          "speaker_id": "1",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100024251.1.39",
          "form": "어느게 적정인지 모르겠음",
          "original_form": "어느게 적정인지 모르겠음",
          "speaker_id": "2",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100024251.1.40",
          "form": "맞아 적정가가 얼만지 모르겠음",
          "original_form": "맞아 적정가가 얼만지 모르겠음",
          "speaker_id": "1",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100024251.1.41",
          "form": "이렇게 가격대가 날뛰는 물건인지 몰랐네....",
          "original_form": "이렇게 가격대가 날뛰는 물건인지 몰랐네....",
          "speaker_id": "1",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100024251.1.42",
          "form": "그러네",
          "original_form": "그러네",
          "speaker_id": "2",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100024251.1.43",
          "form": "나도 다음에 살 때 잘 골라야겠다",
          "original_form": "나도 다음에 살 때 잘 골라야겠다",
          "speaker_id": "2",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100024251.1.44",
          "form": "그리고 갑자기인데 요거트 머신 사고싶네",
          "original_form": "그리고 갑자기인데 요거트 머신 사고싶네",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100024251.1.45",
          "form": "있긴 한데 용량이 작아서 큰 거 사고싶음",
          "original_form": "있긴 한데 용량이 작아서 큰 거 사고싶음",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100024251.1.46",
          "form": "사자사자",
          "original_form": "사자사자",
          "speaker_id": "2",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100024251.1.47",
          "form": "나도 최근에 샀는데",
          "original_form": "나도 최근에 샀는데",
          "speaker_id": "2",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100024251.1.48",
          "form": "아빠가 안 만들어주네",
          "original_form": "아빠가 안 만들어주네",
          "speaker_id": "2",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100024251.1.49",
          "form": "얼마나 하지 사고는 싶은대 가격은 안알아봄",
          "original_form": "얼마나 하지 사고는 싶은대 가격은 안알아봄",
          "speaker_id": "1",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100024251.1.50",
          "form": "2만원 안쪽으로 살 수 있을걸",
          "original_form": "2만원 안쪽으로 살 수 있을걸",
          "speaker_id": "2",
          "time": "20210831 19:01"
        },
        {
          "id": "MDRW2100024251.1.51",
          "form": "나는 아까 말한 한복 안사기로 함",
          "original_form": "나는 아까 말한 한복 안사기로 함",
          "speaker_id": "2",
          "time": "20210831 19:01"
        },
        {
          "id": "MDRW2100024251.1.52",
          "form": "아니 못 삼...사이즈가 안 맞더라...8ㅁ8",
          "original_form": "아니 못 삼...사이즈가 안 맞더라...8ㅁ8",
          "speaker_id": "2",
          "time": "20210831 19:02"
        },
        {
          "id": "MDRW2100024251.1.53",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210831 19:50"
        },
        {
          "id": "MDRW2100024251.1.54",
          "form": "내일 요거트 만들려고 산 우유 오는데 요거트 겁나 만들어서 쟁여놓고 싶다",
          "original_form": "내일 요거트 만들려고 산 우유 오는데 요거트 겁나 만들어서 쟁여놓고 싶다",
