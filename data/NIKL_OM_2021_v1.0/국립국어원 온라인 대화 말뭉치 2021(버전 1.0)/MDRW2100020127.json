      "utterance": [
        {
          "id": "MDRW2100020127.1.1",
          "form": "키우시는 반려동물 있으세요?",
          "original_form": "키우시는 반려동물 있으세요?",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020127.1.2",
          "form": "키우진 않지만 강아지 좋아해요",
          "original_form": "키우진 않지만 강아지 좋아해요",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020127.1.3",
          "form": "저도 강아지 너무 좋아해요.",
          "original_form": "저도 강아지 너무 좋아해요.",
          "speaker_id": "1",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020127.1.4",
          "form": "귀엽죠! 리트리버가 가장 좋아요",
          "original_form": "귀엽죠! 리트리버가 가장 좋아요",
          "speaker_id": "2",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100020127.1.5",
          "form": "리트리버 똑똑하고 순하죠ㅎㅎ 전 친한 친구가 슈나우저 두 마리 기르는데 그 아이둘도 너무 귀엽더라구요",
          "original_form": "리트리버 똑똑하고 순하죠ㅎㅎ 전 친한 친구가 슈나우저 두 마리 기르는데 그 아이둘도 너무 귀엽더라구요",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020127.1.6",
          "form": "비숑도 귀엽지않나요",
          "original_form": "비숑도 귀엽지않나요",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020127.1.7",
          "form": "귀엽죠ㅎㅎ 만약 반려동물이 생긴다면 어떤 종으로 데려오고 싶으세요?",
          "original_form": "귀엽죠ㅎㅎ 만약 반려동물이 생긴다면 어떤 종으로 데려오고 싶으세요?",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020127.1.8",
          "form": "비숑과 리트리버중에 고민되네요",
          "original_form": "비숑과 리트리버중에 고민되네요",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020127.1.9",
          "form": "비숑 산책하는 걸 얼마전에 우연히 봤는데 생각보다 되게 크더라구요ㅎㅎ 전 소형견만 있는 줄 알았어요ㅎㅎ",
          "original_form": "비숑 산책하는 걸 얼마전에 우연히 봤는데 생각보다 되게 크더라구요ㅎㅎ 전 소형견만 있는 줄 알았어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020127.1.10",
          "form": "비숑자체가 똥글똥글한게 귀엽더라고요",
          "original_form": "비숑자체가 똥글똥글한게 귀엽더라고요",
          "speaker_id": "2",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100020127.1.11",
          "form": "맞아요~ 만약 반려동물 키우신다면 이름운 뭘로 짓고 싶으세요?",
          "original_form": "맞아요~ 만약 반려동물 키우신다면 이름운 뭘로 짓고 싶으세요?",
          "speaker_id": "1",
          "time": "20210829 21:03"
        },
        {
          "id": "MDRW2100020127.1.12",
          "form": "몽이나 빵이처럼 귀여운 이름이요",
          "original_form": "몽이나 빵이처럼 귀여운 이름이요",
          "speaker_id": "2",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100020127.1.13",
          "form": "ㅎㅎ귀여운 이름이네요",
          "original_form": "ㅎㅎ귀여운 이름이네요",
          "speaker_id": "1",
          "time": "20210902 02:17"
        },
        {
          "id": "MDRW2100020127.1.14",
          "form": "전 요즘 마당 있는 집에서 대형견 키우고 싶다는 생각 자주 해요",
          "original_form": "전 요즘 마당 있는 집에서 대형견 키우고 싶다는 생각 자주 해요",
          "speaker_id": "1",
          "time": "20210902 02:18"
        }
      ]
