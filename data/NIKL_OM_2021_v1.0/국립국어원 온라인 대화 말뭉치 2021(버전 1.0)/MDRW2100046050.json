        {
          "id": "MDRW2100046050.1.1",
          "form": "내 친구가 최근에 헤어졌어",
          "original_form": "내 친구가 최근에 헤어졌어",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046050.1.2",
          "form": "올해 사기 언니",
          "original_form": "올해 사기 언니",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046050.1.3",
          "form": "오래 만났니",
          "original_form": "오래 만났니",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046050.1.4",
          "form": "삼년 넘게 만나지",
          "original_form": "삼년 넘게 만나지",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046050.1.5",
          "form": "어쩌다가 헤어졌대",
          "original_form": "어쩌다가 헤어졌대",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046050.1.6",
          "form": "정확히는 말을 안 해주는데",
          "original_form": "정확히는 말을 안 해주는데",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046050.1.7",
          "form": "이유가 있긴 할 텐데",
          "original_form": "이유가 있긴 할 텐데",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046050.1.8",
          "form": "서로 바빠서 그랬다더라",
          "original_form": "서로 바빠서 그랬다더라",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046050.1.9",
          "form": "바쁘면 아무래도 만나기 힘들지",
          "original_form": "바쁘면 아무래도 만나기 힘들지",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046050.1.10",
          "form": "맞아 서로 감정도 씻고",
          "original_form": "맞아 서로 감정도 씻고",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046050.1.11",
          "form": "연애라는 거 오래하기 정말 어렵군",
          "original_form": "연애라는 거 오래하기 정말 어렵군",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046050.1.12",
          "form": "맞아 그래서 친구를 많이 위로 해줬어",
          "original_form": "맞아 그래서 친구를 많이 위로 해줬어",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046050.1.13",
          "form": "잘했어",
          "original_form": "잘했어",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046050.1.14",
          "form": "너나 오래 만났으니",
