        },
        {
          "id": "MDRW2100007057.1.2",
          "form": "오 안녕하세요~.~!!",
          "original_form": "오 안녕하세요~.~!!",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007057.1.3",
          "form": "오 신기하네요. ㅋㅋ",
          "original_form": "오 신기하네요. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007057.1.4",
          "form": "동호회 같은거 하시나요",
          "original_form": "동호회 같은거 하시나요",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007057.1.5",
          "form": "아니요ㅠㅠ 소모임 들으려구 앱 깔았다가",
          "original_form": "아니요ㅠㅠ 소모임 들으려구 앱 깔았다가",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007057.1.6",
          "form": "코로나 터져서 쓸모없어졌다는",
          "original_form": "코로나 터져서 쓸모없어졌다는",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007057.1.7",
          "form": "슬픈 사연만 갖고있어요...",
          "original_form": "슬픈 사연만 갖고있어요...",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007057.1.8",
          "form": "name1님은요?!",
          "original_form": "&name1&님은요?!",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007057.1.9",
          "form": "코로나가 진짜 모임 다 가로막네요....",
          "original_form": "코로나가 진짜 모임 다 가로막네요....",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007057.1.10",
          "form": "누구 만날래도 무서워서ㅋㅋ",
          "original_form": "누구 만날래도 무서워서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007057.1.11",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007057.1.12",
          "form": "그때까지만 해도 이게... 이렇게나 오래갈줄은",
          "original_form": "그때까지만 해도 이게... 이렇게나 오래갈줄은",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007057.1.13",
          "form": "몰랐어요ㅠㅜㅠ",
          "original_form": "몰랐어요ㅠㅜㅠ",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007057.1.14",
          "form": "그러게요ㅜㅠ",
          "original_form": "그러게요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007057.1.15",
          "form": "모임 가지기 힘드네요ㅋㅋ",
          "original_form": "모임 가지기 힘드네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007057.1.16",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007057.1.17",
          "form": "그러면 name1님은 코로나 전에는",
          "original_form": "그러면 &name1&님은 코로나 전에는",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007057.1.18",
          "form": "동호회나 모임 하셨던거 있으세요?",
          "original_form": "동호회나 모임 하셨던거 있으세요?",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007057.1.19",
          "form": "그냥 회사 동기들하고 모임 자주 가졌어요ㅎㅎ name2님은요?",
          "original_form": "그냥 회사 동기들하고 모임 자주 가졌어요ㅎㅎ &name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007057.1.20",
          "form": "헉 회사에서 모임갖는거 진짜 대단해요...",
          "original_form": "헉 회사에서 모임갖는거 진짜 대단해요...",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007057.1.21",
          "form": "저는 체력 쓰레기라... 학생신분인데도 원래도 동아리같은거 다 피해다녔거든요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "저는 체력 쓰레기라... 학생신분인데도 원래도 동아리같은거 다 피해다녔거든요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007057.1.22",
          "form": "ㅎㅎ 말이 모임이지 그냥 친한 사람들끼리 노는거에요!",
          "original_form": "ㅎㅎ 말이 모임이지 그냥 친한 사람들끼리 노는거에요!",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007057.1.23",
          "form": "회식이려나요 역시 회사라면!ㅋㅋㅋㅋㅋ",
          "original_form": "회식이려나요 역시 회사라면!ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007057.1.24",
          "form": "그러고보면 진짜 요즘은 인원제한이 엄청 빡세져서",
          "original_form": "그러고보면 진짜 요즘은 인원제한이 엄청 빡세져서",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007057.1.25",
          "form": "쉽게 뭘 할 수가 없는 것 같아요...",
          "original_form": "쉽게 뭘 할 수가 없는 것 같아요...",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007057.1.26",
          "form": "맞아요. . 그래도 이제 백신접종이면 4인까지 되기는 해서",
          "original_form": "맞아요. . 그래도 이제 백신접종이면 4인까지 되기는 해서",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007057.1.27",
          "form": "헉 아 그게 그렇게 바꼈군요!",
          "original_form": "헉 아 그게 그렇게 바꼈군요!",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007057.1.28",
          "form": "1차만 맞아도 되는건가요, 아님 2차까지 완전접종 되어야하는건가요?!",
          "original_form": "1차만 맞아도 되는건가요, 아님 2차까지 완전접종 되어야하는건가요?!",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007057.1.29",
          "form": "2차까지인데 저는 얀센 맞아서 ^^;;",
          "original_form": "2차까지인데 저는 얀센 맞아서 ^^;;",
