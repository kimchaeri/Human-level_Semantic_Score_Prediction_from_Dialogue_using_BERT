      "utterance": [
        {
          "id": "MDRW2100031824.1.1",
          "form": "name3앞에서 땅콩 언급 금지야?",
          "original_form": "&name3&앞에서 땅콩 언급 금지야?",
          "speaker_id": "1",
          "time": "20210905 01:30"
        },
        {
          "id": "MDRW2100031824.1.2",
          "form": "풀어서 말해주는 거 진짜 웃기네",
          "original_form": "풀어서 말해주는 거 진짜 웃기네",
          "speaker_id": "2",
          "time": "20210905 01:30"
        },
        {
          "id": "MDRW2100031824.1.3",
          "form": "그 정도 머리는 안 되는 듯...",
          "original_form": "그 정도 머리는 안 되는 듯...",
          "speaker_id": "2",
          "time": "20210905 01:31"
        },
        {
          "id": "MDRW2100031824.1.4",
          "form": "ㅋㄱㅋㅋㅋㄱㅋㅋㄱㅋㅋㅋㄱㅋㄱㄱ그저 귀엽?",
          "original_form": "ㅋㄱㅋㅋㅋㄱㅋㅋㄱㅋㅋㅋㄱㅋㄱㄱ그저 귀엽?",
          "speaker_id": "1",
          "time": "20210905 01:31"
        },
        {
          "id": "MDRW2100031824.1.5",
          "form": "씩씩하고... 그런 매력... 응...",
          "original_form": "씩씩하고... 그런 매력... 응...",
          "speaker_id": "2",
          "time": "20210905 01:31"
        },
        {
          "id": "MDRW2100031824.1.6",
          "form": "살많이쪘어?",
          "original_form": "살많이쪘어?",
          "speaker_id": "1",
          "time": "20210905 01:31"
        },
        {
          "id": "MDRW2100031824.1.7",
          "form": "내친구네 애옹들은 살빼야된대",
          "original_form": "내친구네 애옹들은 살빼야된대",
          "speaker_id": "1",
          "time": "20210905 01:32"
        },
        {
          "id": "MDRW2100031824.1.8",
          "form": "거세하고 진짜 많이 쪘어",
          "original_form": "거세하고 진짜 많이 쪘어",
          "speaker_id": "2",
          "time": "20210905 01:32"
        },
        {
          "id": "MDRW2100031824.1.9",
          "form": "ㅋㄱㄱㅋㄱㅋㅋㄱㅋㄱ거세ㅋㅋㄱㅋㅋㅋㄱㄱㅈ",
          "original_form": "ㅋㄱㄱㅋㄱㅋㅋㄱㅋㄱ거세ㅋㅋㄱㅋㅋㅋㄱㄱㅈ",
          "speaker_id": "1",
          "time": "20210905 01:32"
        },
        {
          "id": "MDRW2100031824.1.10",
          "form": "직접적이네,,.",
          "original_form": "직접적이네,,.",
          "speaker_id": "1",
          "time": "20210905 01:32"
        },
        {
          "id": "MDRW2100031824.1.11",
          "form": "수치스럽겠다 name3,",
          "original_form": "수치스럽겠다 &name3&,",
          "speaker_id": "1",
          "time": "20210905 01:32"
        },
        {
          "id": "MDRW2100031824.1.12",
          "form": "이게 있는 거하고 없는 거하고 칼로리 소비량이 팍 차이 나더라고",
          "original_form": "이게 있는 거하고 없는 거하고 칼로리 소비량이 팍 차이 나더라고",
          "speaker_id": "2",
          "time": "20210905 01:32"
        },
        {
          "id": "MDRW2100031824.1.13",
          "form": "친구 고양이들은 다이어트 어케 해??",
          "original_form": "친구 고양이들은 다이어트 어케 해??",
          "speaker_id": "2",
          "time": "20210905 01:33"
        },
        {
          "id": "MDRW2100031824.1.14",
          "form": "친구는 간식을 줄였대",
          "original_form": "친구는 간식을 줄였대",
          "speaker_id": "1",
          "time": "20210905 01:35"
        },
        {
          "id": "MDRW2100031824.1.15",
          "form": "근데 애옹이 뭘알겠어 화를 잔뜩낸대",
          "original_form": "근데 애옹이 뭘알겠어 화를 잔뜩낸대",
          "speaker_id": "1",
          "time": "20210905 01:36"
        },
        {
          "id": "MDRW2100031824.1.16",
          "form": "에궁...",
          "original_form": "에궁...",
          "speaker_id": "2",
          "time": "20210905 01:36"
        },
        {
          "id": "MDRW2100031824.1.17",
          "form": "그래도 조금 빠졌어",
          "original_form": "그래도 조금 빠졌어",
          "speaker_id": "1",
          "time": "20210905 01:37"
        },
        {
          "id": "MDRW2100031824.1.18",
          "form": "맛없는 다이어트 사료보단 나을 텐데...",
          "original_form": "맛없는 다이어트 사료보단 나을 텐데...",
          "speaker_id": "2",
          "time": "20210905 01:37"
        },
        {
          "id": "MDRW2100031824.1.19",
          "form": "운동도안하니까 빼기쉽지않대",
          "original_form": "운동도안하니까 빼기쉽지않대",
          "speaker_id": "1",
          "time": "20210905 01:37"
        },
        {
          "id": "MDRW2100031824.1.20",
          "form": "캣휠을 사줬는데 안탄대,,",
          "original_form": "캣휠을 사줬는데 안탄대,,",
          "speaker_id": "1",
          "time": "20210905 01:37"
        },
        {
          "id": "MDRW2100031824.1.21",
          "form": "그러게... 고양이가 움직이려면 결국 사람도 같이 움직여야 돼서......",
          "original_form": "그러게... 고양이가 움직이려면 결국 사람도 같이 움직여야 돼서......",
          "speaker_id": "2",
          "time": "20210905 01:38"
        },
        {
          "id": "MDRW2100031824.1.22",
          "form": "남일 같지 않아서 조금 눈물이",
          "original_form": "남일 같지 않아서 조금 눈물이",
          "speaker_id": "2",
          "time": "20210905 01:38"
        },
        {
          "id": "MDRW2100031824.1.23",
          "form": "name3도 스트릿이었어?",
          "original_form": "&name3&도 스트릿이었어?",
          "speaker_id": "1",
          "time": "20210905 01:39"
        },
        {
          "id": "MDRW2100031824.1.24",
          "form": "응 울 언니가 다니는 회사 상사가 애기 고양이 줏었는데 애가 애교도 없고 사납다고 다시 버리려는 거 우리가 데려옴...",
          "original_form": "응 울 언니가 다니는 회사 상사가 애기 고양이 줏었는데 애가 애교도 없고 사납다고 다시 버리려는 거 우리가 데려옴...",
          "speaker_id": "2",
          "time": "20210905 01:40"
        }
      ]
