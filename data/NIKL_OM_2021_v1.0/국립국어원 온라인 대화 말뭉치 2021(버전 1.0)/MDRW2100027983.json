      "utterance": [
        {
          "id": "MDRW2100027983.1.1",
          "form": "근데 셋트 뽕 뽑으려며 까지 해야한대요",
          "original_form": "근데 셋트 뽕 뽑으려며 까지 해야한대요",
          "speaker_id": "2",
          "time": "20210902 16:34"
        },
        {
          "id": "MDRW2100027983.1.2",
          "form": "제생각엔 2로 뽑고도 남았을거같아요",
          "original_form": "제생각엔 2로 뽑고도 남았을거같아요",
          "speaker_id": "1",
          "time": "20210902 16:34"
        },
        {
          "id": "MDRW2100027983.1.3",
          "form": "ㅋㅋ 하긴요",
          "original_form": "ㅋㅋ 하긴요",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027983.1.4",
          "form": "아스달은 더 했는데 1로 손해는 안봤다고 했거든요",
          "original_form": "아스달은 더 했는데 1로 손해는 안봤다고 했거든요",
          "speaker_id": "1",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027983.1.5",
          "form": "슬의는 아스달보다 돈 안들었을거고",
          "original_form": "슬의는 아스달보다 돈 안들었을거고",
          "speaker_id": "1",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027983.1.6",
          "form": "완전 흥해서",
          "original_form": "완전 흥해서",
          "speaker_id": "1",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027983.1.7",
          "form": "헐 아스달 손해 안봤대요? 그 셋트하고?",
          "original_form": "헐 아스달 손해 안봤대요? 그 셋트하고?",
          "speaker_id": "2",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027983.1.8",
          "form": "그럼 3하지 말고 끝냈음",
          "original_form": "그럼 3하지 말고 끝냈음",
          "speaker_id": "2",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027983.1.9",
          "form": "기사로는 그렇다 하더라고요",
          "original_form": "기사로는 그렇다 하더라고요",
          "speaker_id": "1",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027983.1.10",
          "form": "3이 기대되는건 결사곡뿐이예요",
          "original_form": "3이 기대되는건 결사곡뿐이예요",
          "speaker_id": "2",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027983.1.11",
          "form": "근데 수익이 안났으니까요",
          "original_form": "근데 수익이 안났으니까요",
          "speaker_id": "1",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027983.1.12",
          "form": "펜하3은 진짜ㅡㅡ",
          "original_form": "펜하3은 진짜ㅡㅡ",
          "speaker_id": "2",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027983.1.13",
          "form": "결사곡ㅋㅋㅋㅋ",
          "original_form": "결사곡ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027983.1.14",
          "form": "내년이나되야하니까요",
          "original_form": "내년이나되야하니까요",
          "speaker_id": "1",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027983.1.15",
          "form": "제발 불륜녀들 벌 받았으면하는데",
          "original_form": "제발 불륜녀들 벌 받았으면하는데",
          "speaker_id": "1",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027983.1.16",
          "form": "둘이나 결혼하고 있고요",
          "original_form": "둘이나 결혼하고 있고요",
          "speaker_id": "1",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027983.1.17",
          "form": "제발요",
          "original_form": "제발요",
          "speaker_id": "2",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027983.1.18",
          "form": "성훈 상간녀는 진짜 짜증",
          "original_form": "성훈 상간녀는 진짜 짜증",
          "speaker_id": "2",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027983.1.19",
          "form": "ㅇㅇ이민영역할이요",
          "original_form": "ㅇㅇ이민영역할이요",
          "speaker_id": "1",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027983.1.20",
          "form": "아니 유뷰남 애를 임신했는데",
          "original_form": "아니 유뷰남 애를 임신했는데",
          "speaker_id": "1",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027983.1.21",
          "form": "세성 귀한 며느리되고",
          "original_form": "세성 귀한 며느리되고",
          "speaker_id": "1",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027983.1.22",
          "form": "세상",
          "original_form": "세상",
          "speaker_id": "1",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027983.1.23",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027983.1.24",
          "form": "벌도 안받으면 너무 하잖아요",
          "original_form": "벌도 안받으면 너무 하잖아요",
          "speaker_id": "1",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027983.1.25",
          "form": "나쁜말 안썼는데",
          "original_form": "나쁜말 안썼는데",
          "speaker_id": "2",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027983.1.26",
          "form": "그 세계관 킹카랑 결혼이라뇨",
          "original_form": "그 세계관 킹카랑 결혼이라뇨",
          "speaker_id": "1",
          "time": "20210902 16:40"
        },
        {
          "id": "MDRW2100027983.1.27",
          "form": "부장님ㅋㅋㅋㅋ",
          "original_form": "부장님ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:40"
        },
        {
          "id": "MDRW2100027983.1.28",
          "form": "진짜 그건 용서할수 없어요",
          "original_form": "진짜 그건 용서할수 없어요",
          "speaker_id": "2",
          "time": "20210902 16:40"
        },
        {
          "id": "MDRW2100027983.1.29",
          "form": "임성한 사상이상한거나 유령나오는거는 특색으로 넘길수도 있는데",
          "original_form": "임성한 사상이상한거나 유령나오는거는 특색으로 넘길수도 있는데",
          "speaker_id": "1",
          "time": "20210902 16:41"
        },
        {
          "id": "MDRW2100027983.1.30",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 16:41"
        },
        {
          "id": "MDRW2100027983.1.31",
          "form": "제발 권선징악이요",
          "original_form": "제발 권선징악이요",
          "speaker_id": "1",
          "time": "20210902 16:41"
        },
        {
          "id": "MDRW2100027983.1.32",
          "form": "제발요ㅡㅡ\n사람들이 양심이 있어야죠",
          "original_form": "제발요ㅡㅡ\n사람들이 양심이 있어야죠",
          "speaker_id": "2",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100027983.1.33",
          "form": "이민영 말투 넘 싫구요",
          "original_form": "이민영 말투 넘 싫구요",
          "speaker_id": "2",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100027983.1.34",
          "form": "불륜 어벤져스",
          "original_form": "불륜 어벤져스",
          "speaker_id": "2",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100027983.1.35",
          "form": "불륜녀들이 자기 잘못한것도 모르고",
          "original_form": "불륜녀들이 자기 잘못한것도 모르고",
          "speaker_id": "1",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100027983.1.36",
          "form": "글구 손녀몸 빙의는...",
          "original_form": "글구 손녀몸 빙의는...",
          "speaker_id": "2",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027983.1.37",
          "form": "막장드라마에 원하는건 권선징악인데",
          "original_form": "막장드라마에 원하는건 권선징악인데",
          "speaker_id": "1",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027983.1.38",
          "form": "애비야 골프가자",
          "original_form": "애비야 골프가자",
          "speaker_id": "2",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027983.1.39",
          "form": "아 그건 좀ㅇㅇ 지아가 불쌍",
          "original_form": "아 그건 좀ㅇㅇ 지아가 불쌍",
          "speaker_id": "1",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027983.1.40",
          "form": "펜하는 이지아가 과연 살아남았을지",
          "original_form": "펜하는 이지아가 과연 살아남았을지",
          "speaker_id": "2",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027983.1.41",
          "form": "이지아 죽어요?",
          "original_form": "이지아 죽어요?",
          "speaker_id": "1",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100027983.1.42",
          "form": "아 걔도 지아네요 ㅎㅎ",
          "original_form": "아 걔도 지아네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100027983.1.43",
          "form": "펜하는 넘 띄엄띄엄봐서 잘 모르겠어요",
          "original_form": "펜하는 넘 띄엄띄엄봐서 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100027983.1.44",
          "form": "김소연이 치메걸린건 알구요",
          "original_form": "김소연이 치메걸린건 알구요",
          "speaker_id": "1",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100027983.1.45",
          "form": "엄기준이 떨어지면서 폭탄스위치 눌러서 건물이 무너지는데",
          "original_form": "엄기준이 떨어지면서 폭탄스위치 눌러서 건물이 무너지는데",
          "speaker_id": "2",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100027983.1.46",
          "form": "그때 이지아가 펜트하우스에 있어서",
          "original_form": "그때 이지아가 펜트하우스에 있어서",
          "speaker_id": "2",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100027983.1.47",
          "form": "과연 탈출이 가능할지",
          "original_form": "과연 탈출이 가능할지",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100027983.1.48",
          "form": "엄마랑 얘기하는데 이지아 총 너무 주섬주섬 꺼낸다고 ㅎㅎㅎ",
          "original_form": "엄마랑 얘기하는데 이지아 총 너무 주섬주섬 꺼낸다고 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100027983.1.49",
          "form": "찍찍이 ㅎㅎ",
          "original_form": "찍찍이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100027983.1.50",
          "form": "아ㅋㅋ",
          "original_form": "아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100027983.1.51",
          "form": "ㅎㅎㅎ뭔가 촥 꺼내야하는데",
          "original_form": "ㅎㅎㅎ뭔가 촥 꺼내야하는데",
          "speaker_id": "2",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100027983.1.52",
          "form": "주섬주섬",
          "original_form": "주섬주섬",
          "speaker_id": "2",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100027983.1.53",
          "form": "무슨 홍콩영화 패러디 같다고",
          "original_form": "무슨 홍콩영화 패러디 같다고",
          "speaker_id": "2",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100027983.1.54",
          "form": "짤",
          "original_form": "짤",
          "speaker_id": "1",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100027983.1.55",
          "form": "보셨어요? 짤 돌고 있다고 하는데",
          "original_form": "보셨어요? 짤 돌고 있다고 하는데",
          "speaker_id": "2",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100027983.1.56",
          "form": "주섬주섬",
          "original_form": "주섬주섬",
          "speaker_id": "2",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100027983.1.57",
          "form": "아 아니요ㅋㅋ 김소연 그와중에 플립 쓴다는 짤이요",
          "original_form": "아 아니요ㅋㅋ 김소연 그와중에 플립 쓴다는 짤이요",
          "speaker_id": "1",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100027983.1.58",
          "form": "예판구매ㅋㅋㅋ",
          "original_form": "예판구매ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100027983.1.59",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "2",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100027983.1.60",
          "form": "못봤어요",
          "original_form": "못봤어요",
          "speaker_id": "2",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100027983.1.61",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100027983.1.62",
          "form": "플립 피피엘 들어갔나보더라구요",
          "original_form": "플립 피피엘 들어갔나보더라구요",
          "speaker_id": "1",
          "time": "20210902 16:48"
        },
        {
          "id": "MDRW2100027983.1.63",
          "form": "크림색",
          "original_form": "크림색",
          "speaker_id": "1",
          "time": "20210902 16:48"
        },
        {
          "id": "MDRW2100027983.1.64",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210902 16:48"
        },
        {
          "id": "MDRW2100027983.1.65",
