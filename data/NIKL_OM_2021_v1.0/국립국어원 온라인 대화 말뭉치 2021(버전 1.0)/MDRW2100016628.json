      "utterance": [
        {
          "id": "MDRW2100016628.1.1",
          "form": "점심 식사는 하셨나요?",
          "original_form": "점심 식사는 하셨나요?",
          "speaker_id": "1",
          "time": "20210826 14:15"
        },
        {
          "id": "MDRW2100016628.1.2",
          "form": "네! 고기구워먹엇어요 ㅋㅋㅋㅋ",
          "original_form": "네! 고기구워먹엇어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:16"
        },
        {
          "id": "MDRW2100016628.1.3",
          "form": "점심 뭐 드셧어요?",
          "original_form": "점심 뭐 드셧어요?",
          "speaker_id": "2",
          "time": "20210826 14:16"
        },
        {
          "id": "MDRW2100016628.1.4",
          "form": "점심부터요?",
          "original_form": "점심부터요?",
          "speaker_id": "1",
          "time": "20210826 14:16"
        },
        {
          "id": "MDRW2100016628.1.5",
          "form": "전 점심은 항상 면류에요",
          "original_form": "전 점심은 항상 면류에요",
          "speaker_id": "1",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016628.1.6",
          "form": "ㅋㅋㅋㅋㅋ네 저는 아침부터도 고기 가능해요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ네 저는 아침부터도 고기 가능해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016628.1.7",
          "form": "어떤 면류 보통 드세요?",
          "original_form": "어떤 면류 보통 드세요?",
          "speaker_id": "2",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016628.1.8",
          "form": "라면 우동이요..",
          "original_form": "라면 우동이요..",
          "speaker_id": "1",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016628.1.9",
          "form": "간편하게 먹는걸 좋아해서요",
          "original_form": "간편하게 먹는걸 좋아해서요",
          "speaker_id": "1",
          "time": "20210826 14:19"
        },
        {
          "id": "MDRW2100016628.1.10",
          "form": "아아 그럼 점심은 주로 그렇게 드시는거에요?",
          "original_form": "아아 그럼 점심은 주로 그렇게 드시는거에요?",
          "speaker_id": "2",
          "time": "20210826 14:19"
        },
        {
          "id": "MDRW2100016628.1.11",
          "form": "네.. 저도 점심에 고기먹어보고 싶네요..",
          "original_form": "네.. 저도 점심에 고기먹어보고 싶네요..",
          "speaker_id": "1",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016628.1.12",
          "form": "삼겹살 같은거 구워드신거에요?",
          "original_form": "삼겹살 같은거 구워드신거에요?",
          "speaker_id": "1",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016628.1.13",
          "form": "목살 구워먹엇어요..!",
          "original_form": "목살 구워먹엇어요..!",
          "speaker_id": "2",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016628.1.14",
          "form": "자취하시는거에요?",
          "original_form": "자취하시는거에요?",
          "speaker_id": "2",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016628.1.15",
          "form": "우와.. 목살.. ㅎㄷㄷㄷㄷ",
          "original_form": "우와.. 목살.. ㅎㄷㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016628.1.16",
          "form": "네 자취라 간편한거 찾다보니.. 면류로..",
          "original_form": "네 자취라 간편한거 찾다보니.. 면류로..",
          "speaker_id": "1",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016628.1.17",
          "form": "한식은 넘 번거로워요",
          "original_form": "한식은 넘 번거로워요",
          "speaker_id": "1",
          "time": "20210826 14:24"
        },
        {
          "id": "MDRW2100016628.1.18",
          "form": "네네 번거로울거 같아요!",
          "original_form": "네네 번거로울거 같아요!",
          "speaker_id": "2",
          "time": "20210826 14:26"
        },
        {
          "id": "MDRW2100016628.1.19",
          "form": "그래두 건강생각해서 매일 라면은 자제해보세요..ㅠ",
          "original_form": "그래두 건강생각해서 매일 라면은 자제해보세요..ㅠ",
          "speaker_id": "2",
          "time": "20210826 14:26"
        },
        {
          "id": "MDRW2100016628.1.20",
          "form": "ㅋㅋ 저도 성공해서 3끼 고기먹고 싶어요.. ㅎㅎ",
          "original_form": "ㅋㅋ 저도 성공해서 3끼 고기먹고 싶어요.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:27"
        },
        {
          "id": "MDRW2100016628.1.21",
          "form": "근데 라면이 몸에 안좋은건지도 의문이에요.. ㅋㅋ",
          "original_form": "근데 라면이 몸에 안좋은건지도 의문이에요.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:27"
        },
        {
          "id": "MDRW2100016628.1.22",
          "form": "저두성공해서 종일 고기만 먹고싶네요 ㅠㅠ",
          "original_form": "저두성공해서 종일 고기만 먹고싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100016628.1.23",
          "form": "살 잘 안찌는체질이세요!?",
          "original_form": "살 잘 안찌는체질이세요!?",
          "speaker_id": "2",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100016628.1.24",
          "form": "잘 찌는 체질이라.. 식단은 항상 신경써요..",
          "original_form": "잘 찌는 체질이라.. 식단은 항상 신경써요..",
          "speaker_id": "1",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016628.1.25",
          "form": "대충 먹으면 금장 찌더라구요",
          "original_form": "대충 먹으면 금장 찌더라구요",
          "speaker_id": "1",
          "time": "20210826 14:32"
        }
      ]
