      "utterance": [
        {
          "id": "MDRW2100018775.1.1",
          "form": "좋아하는 운동화 브랜드 있으세요?",
          "original_form": "좋아하는 운동화 브랜드 있으세요?",
          "speaker_id": "1",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100018775.1.2",
          "form": "저 나이키 운동화 좋아해요",
          "original_form": "저 나이키 운동화 좋아해요",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018775.1.3",
          "form": "어렸을 때부터 그것만 신었어요",
          "original_form": "어렸을 때부터 그것만 신었어요",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018775.1.4",
          "form": "오~ 저도요",
          "original_form": "오~ 저도요",
          "speaker_id": "1",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018775.1.5",
          "form": "사고보면 나이키꺼만 있어요ㅋㅋ",
          "original_form": "사고보면 나이키꺼만 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018775.1.6",
          "form": "나이키 라인업이 다양하더라구요",
          "original_form": "나이키 라인업이 다양하더라구요",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018775.1.7",
          "form": "전 에어맥스 위주로 샀는데.. 다른 건 모르겠어요",
          "original_form": "전 에어맥스 위주로 샀는데.. 다른 건 모르겠어요",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018775.1.8",
          "form": "새로운거 사봤는데 결국엔 나이키네요",
          "original_form": "새로운거 사봤는데 결국엔 나이키네요",
          "speaker_id": "1",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018775.1.9",
          "form": "맞아요 나이키가 제일 편하더라구요",
          "original_form": "맞아요 나이키가 제일 편하더라구요",
          "speaker_id": "2",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018775.1.10",
          "form": "다른 브랜드 사봤는데 금방 낡아지거나 밑창 닳거나 ㅠㅠ",
          "original_form": "다른 브랜드 사봤는데 금방 낡아지거나 밑창 닳거나 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018775.1.11",
          "form": "맞아요ㅠ 신다보면 아픈경우도 많았어요",
          "original_form": "맞아요ㅠ 신다보면 아픈경우도 많았어요",
          "speaker_id": "1",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018775.1.12",
          "form": "운동환데 뒷꿈치 나간것도 있었구요ㅠ",
          "original_form": "운동환데 뒷꿈치 나간것도 있었구요ㅠ",
          "speaker_id": "1",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100018775.1.13",
          "form": "어 맞아요 아픈 게 제일 컸어요",
          "original_form": "어 맞아요 아픈 게 제일 컸어요",
          "speaker_id": "2",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100018775.1.14",
          "form": "저 발볼 큰 편도 아닌데 안맞기도 하고 ㅠㅠ",
          "original_form": "저 발볼 큰 편도 아닌데 안맞기도 하고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100018775.1.15",
          "form": "그런곳이 많죠ㅠ 그런거보면 나이키가 괜찮아요",
          "original_form": "그런곳이 많죠ㅠ 그런거보면 나이키가 괜찮아요",
          "speaker_id": "1",
          "time": "20210827 12:11"
        },
        {
          "id": "MDRW2100018775.1.16",
          "form": "나이키 어떤 라인업 선호하세요?",
          "original_form": "나이키 어떤 라인업 선호하세요?",
          "speaker_id": "2",
          "time": "20210827 12:14"
        },
        {
          "id": "MDRW2100018775.1.17",
          "form": "저도 에어맥스로 자주 보게 되네요",
          "original_form": "저도 에어맥스로 자주 보게 되네요",
          "speaker_id": "1",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018775.1.18",
          "form": "걸을때 편한게 좋더라구요",
          "original_form": "걸을때 편한게 좋더라구요",
          "speaker_id": "1",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018775.1.19",
          "form": "그쵸 ㅎㅎ 특히 러닝할 때 발이 편해요",
          "original_form": "그쵸 ㅎㅎ 특히 러닝할 때 발이 편해요",
          "speaker_id": "2",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018775.1.20",
          "form": "쿠션감 차이가 진짜 어마어마 하더라구요",
          "original_form": "쿠션감 차이가 진짜 어마어마 하더라구요",
          "speaker_id": "2",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018775.1.21",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100018775.1.22",
          "form": "신어보면 계속 찾게되요",
          "original_form": "신어보면 계속 찾게되요",
