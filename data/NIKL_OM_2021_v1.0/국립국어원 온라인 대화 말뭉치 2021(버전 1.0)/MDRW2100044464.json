      "utterance": [
        {
          "id": "MDRW2100044464.1.1",
          "form": "요새 진짜",
          "original_form": "요새 진짜",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044464.1.2",
          "form": "원신말곤 할게 없네",
          "original_form": "원신말곤 할게 없네",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044464.1.3",
          "form": "190개중에 150개정도는",
          "original_form": "190개중에 150개정도는",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044464.1.4",
          "form": "원신얘긴듯",
          "original_form": "원신얘긴듯",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044464.1.5",
          "form": "ㅇㅇ 나도 원신밖에 안해",
          "original_form": "ㅇㅇ 나도 원신밖에 안해",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044464.1.6",
          "form": "신야근 미접한지 한참됨",
          "original_form": "신야근 미접한지 한참됨",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044464.1.7",
          "form": "ㅇㅇ걔네 출석체크도 없잖아",
          "original_form": "ㅇㅇ걔네 출석체크도 없잖아",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044464.1.8",
          "form": "인디겜 수준",
          "original_form": "인디겜 수준",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044464.1.9",
          "form": "ㅇㅇ 만들 예정이라고는 써놨는데",
          "original_form": "ㅇㅇ 만들 예정이라고는 써놨는데",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044464.1.10",
          "form": "못믿음 ㅋㅋㅋ",
          "original_form": "못믿음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044464.1.11",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044464.1.12",
          "form": "그리고 진짜",
          "original_form": "그리고 진짜",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044464.1.13",
          "form": "저기 공식계정 관리하는 거 볼수록 실망이야",
          "original_form": "저기 공식계정 관리하는 거 볼수록 실망이야",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044464.1.14",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044464.1.15",
          "form": "지난번에 점검하고서",
          "original_form": "지난번에 점검하고서",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044464.1.16",
          "form": "8.30일에 보상 준다고 했는데",
          "original_form": "8.30일에 보상 준다고 했는데",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044464.1.17",
          "form": "자기들끼리 뭔 오류가 났는지",
          "original_form": "자기들끼리 뭔 오류가 났는지",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044464.1.18",
          "form": "안줬어??",
          "original_form": "안줬어??",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044464.1.19",
          "form": "8.30일부터 앱 접속이 안됨 ㅋ",
          "original_form": "8.30일부터 앱 접속이 안됨 ㅋ",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044464.1.20",
          "form": "???",
          "original_form": "???",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044464.1.21",
          "form": "지금도?",
          "original_form": "지금도?",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044464.1.22",
          "form": "아니 그건 아닌데",
          "original_form": "아니 그건 아닌데",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044464.1.23",
          "form": "결국 그래서 8.31 저녁 늦게 접속이 됐는데",
          "original_form": "결국 그래서 8.31 저녁 늦게 접속이 됐는데",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044464.1.24",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044464.1.25",
          "form": "보상ㅈ은?",
          "original_form": "보상ㅈ은?",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044464.1.26",
          "form": "그럼 공지를 고쳐야하잖아?",
          "original_form": "그럼 공지를 고쳐야하잖아?",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044464.1.27",
          "form": "일단 그 보상은 없었늠 ㅋ",
          "original_form": "일단 그 보상은 없었늠 ㅋ",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044464.1.28",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:46"
        }
