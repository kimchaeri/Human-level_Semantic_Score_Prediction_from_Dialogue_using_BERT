      "utterance": [
        {
          "id": "MDRW2100041997.1.1",
          "form": "안녕하세요! 혹시 반려동물 키우시나요?!",
          "original_form": "안녕하세요! 혹시 반려동물 키우시나요?!",
          "speaker_id": "1",
          "time": "20210906 18:22"
        },
        {
          "id": "MDRW2100041997.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041997.1.3",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041997.1.4",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041997.1.5",
          "form": "저도 안키우고 있어요",
          "original_form": "저도 안키우고 있어요",
          "speaker_id": "1",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100041997.1.6",
          "form": "동물이 좋긴하지만 키우기엔 부담감이 좀 들더라구요..",
          "original_form": "동물이 좋긴하지만 키우기엔 부담감이 좀 들더라구요..",
          "speaker_id": "1",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041997.1.7",
          "form": "그쵸 비용이나 시간측면에서",
          "original_form": "그쵸 비용이나 시간측면에서",
          "speaker_id": "2",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041997.1.8",
          "form": "평범한 직장인은 좀 부담스러운 느낌이에요",
          "original_form": "평범한 직장인은 좀 부담스러운 느낌이에요",
          "speaker_id": "2",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041997.1.9",
          "form": "맞아요 긴 시간 같이 있기 힘드니까요ㅠㅠ",
          "original_form": "맞아요 긴 시간 같이 있기 힘드니까요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100041997.1.10",
          "form": "만약 키운다면 어떤동물 키우고싶으세요?",
          "original_form": "만약 키운다면 어떤동물 키우고싶으세요?",
          "speaker_id": "2",
          "time": "20210906 18:52"
        },
        {
          "id": "MDRW2100041997.1.11",
          "form": "저는 고양이요!",
          "original_form": "저는 고양이요!",
          "speaker_id": "1",
          "time": "20210906 18:58"
        },
        {
          "id": "MDRW2100041997.1.12",
          "form": "고양이 좋아하세ㅇ",
          "original_form": "고양이 좋아하세ㅇ",
          "speaker_id": "2",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100041997.1.13",
          "form": "세요?",
          "original_form": "세요?",
          "speaker_id": "2",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100041997.1.14",
          "form": "네! 친구네 집에서 고양이를 키우는데",
          "original_form": "네! 친구네 집에서 고양이를 키우는데",
          "speaker_id": "1",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100041997.1.15",
          "form": "그거 보고 좋아졌어용",
          "original_form": "그거 보고 좋아졌어용",
          "speaker_id": "1",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100041997.1.16",
          "form": "아 친구가 키우면 정말 귀엽죠",
          "original_form": "아 친구가 키우면 정말 귀엽죠",
          "speaker_id": "2",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100041997.1.17",
          "form": "어떤 종인가요?",
          "original_form": "어떤 종인가요?",
          "speaker_id": "2",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100041997.1.18",
          "form": "먼치킨이에요! 작고 귀여워요ㅠㅠ",
          "original_form": "먼치킨이에요! 작고 귀여워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100041997.1.19",
          "form": "와 진짜 귀엽겠네요",
          "original_form": "와 진짜 귀엽겠네요",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100041997.1.20",
          "form": "막 할퀴거나 그러진 않나요?",
          "original_form": "막 할퀴거나 그러진 않나요?",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100041997.1.21",
          "form": "네! 엄청 순했어요!!",
          "original_form": "네! 엄청 순했어요!!",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100041997.1.22",
          "form": "와 부럽네요",
          "original_form": "와 부럽네요",
          "speaker_id": "2",
          "time": "20210906 22:13"
        }
      ]
