      "utterance": [
        {
          "id": "MDRW2100042282.1.1",
          "form": "글쎄 비온 뒤에 더워진단 말을 들었거든요",
          "original_form": "글쎄 비온 뒤에 더워진단 말을 들었거든요",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042282.1.2",
          "form": "비온 다음 더위라니",
          "original_form": "비온 다음 더위라니",
          "speaker_id": "1",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042282.1.3",
          "form": "보통 이 맘때 비는",
          "original_form": "보통 이 맘때 비는",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042282.1.4",
          "form": "가을을 재촉하는 비 아닌가요",
          "original_form": "가을을 재촉하는 비 아닌가요",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042282.1.5",
          "form": "그러게요 추석도 오고 있는데 더위라고 해서",
          "original_form": "그러게요 추석도 오고 있는데 더위라고 해서",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042282.1.6",
          "form": "의아하는 중이었어요",
          "original_form": "의아하는 중이었어요",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042282.1.7",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042282.1.8",
          "form": "이젠 점점 기온이 떨어질 것 같은데",
          "original_form": "이젠 점점 기온이 떨어질 것 같은데",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042282.1.9",
          "form": "부디 저게 헛소리길 바래요 ㅎㅎㅎ",
          "original_form": "부디 저게 헛소리길 바래요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042282.1.10",
          "form": "더이상 더운건 사절이에요 ㅠ",
          "original_form": "더이상 더운건 사절이에요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042282.1.11",
          "form": "네 더위는 이제 그만",
          "original_form": "네 더위는 이제 그만",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042282.1.12",
          "form": "이제는 눈도 보고 싶어져요",
          "original_form": "이제는 눈도 보고 싶어져요",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100042282.1.13",
          "form": "아 겨울 좋아하시나요",
          "original_form": "아 겨울 좋아하시나요",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100042282.1.14",
          "form": "가을을 제일 좋아하시만",
          "original_form": "가을을 제일 좋아하시만",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042282.1.15",
          "form": "겨울도 좋아해요!",
          "original_form": "겨울도 좋아해요!",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042282.1.16",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042282.1.17",
          "form": "더위보단 추위쪽이시군요",
          "original_form": "더위보단 추위쪽이시군요",
          "speaker_id": "1",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042282.1.18",
          "form": "전 봄가을 좋아해요",
          "original_form": "전 봄가을 좋아해요",
          "speaker_id": "1",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042282.1.19",
          "form": "맞아요.. 이번 여름에 진짜 지옥을 맛봐서",
          "original_form": "맞아요.. 이번 여름에 진짜 지옥을 맛봐서",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042282.1.20",
          "form": "추위쪽이 더 나은 것 같아요",
          "original_form": "추위쪽이 더 나은 것 같아요",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042282.1.21",
          "form": "이번 여름 힘드셨군요",
          "original_form": "이번 여름 힘드셨군요",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042282.1.22",
          "form": "정말..  이번 생에서 겪은 더위중 최고였어요",
          "original_form": "정말..  이번 생에서 겪은 더위중 최고였어요",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042282.1.23",
          "form": "그러셨구나ㅜ",
          "original_form": "그러셨구나ㅜ",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042282.1.24",
          "form": "그럼 선선해진 날씨가 더 반가우시겠어요",
          "original_form": "그럼 선선해진 날씨가 더 반가우시겠어요",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042282.1.25",
          "form": "지금 날씨가 너무 좋아요!",
          "original_form": "지금 날씨가 너무 좋아요!",
          "speaker_id": "2",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042282.1.26",
          "form": "name1님은 추위보단 더위가 더 나으신가요??",
          "original_form": "&name1&님은 추위보단 더위가 더 나으신가요??",
          "speaker_id": "2",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042282.1.27",
          "form": "음 둘 다 힘들긴한데",
          "original_form": "음 둘 다 힘들긴한데",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042282.1.28",
          "form": "저도 굳이 고르라면 추위요",
          "original_form": "저도 굳이 고르라면 추위요",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042282.1.29",
          "form": "추운 건 옷을 껴입으면 되는데",
          "original_form": "추운 건 옷을 껴입으면 되는데",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042282.1.30",
          "form": "더운 건 넘 지쳐요ㅜ",
          "original_form": "더운 건 넘 지쳐요ㅜ",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042282.1.31",
          "form": "오늘 아침도 여전히 비가 와요ㅜ",
          "original_form": "오늘 아침도 여전히 비가 와요ㅜ",
          "speaker_id": "1",
          "time": "20210907 08:10"
        }
      ]
