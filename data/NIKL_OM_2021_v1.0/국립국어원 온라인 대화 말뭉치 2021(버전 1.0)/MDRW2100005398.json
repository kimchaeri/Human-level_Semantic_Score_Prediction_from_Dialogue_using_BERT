      "utterance": [
        {
          "id": "MDRW2100005398.1.1",
          "form": "혹시 좋아하는 책 있으세요?",
          "original_form": "혹시 좋아하는 책 있으세요?",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005398.1.2",
          "form": "책은 많이 안읽어요",
          "original_form": "책은 많이 안읽어요",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005398.1.3",
          "form": "실용서위주로 보네요",
          "original_form": "실용서위주로 보네요",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005398.1.4",
          "form": "아 만화책도 안좋아하세요?",
          "original_form": "아 만화책도 안좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005398.1.5",
          "form": "좋아했는데 요즘은 거의 웹툰으로 보네요",
          "original_form": "좋아했는데 요즘은 거의 웹툰으로 보네요",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005398.1.6",
          "form": "아 웹툰 어느웹툰보세요?",
          "original_form": "아 웹툰 어느웹툰보세요?",
          "speaker_id": "1",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005398.1.7",
          "form": "네이버웹툰 이용하는데 이것저것 많이봐요",
          "original_form": "네이버웹툰 이용하는데 이것저것 많이봐요",
          "speaker_id": "2",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005398.1.8",
          "form": "베스트셀러같은건 안읽으세요?",
          "original_form": "베스트셀러같은건 안읽으세요?",
          "speaker_id": "1",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005398.1.9",
          "form": "네ㅎㅎ 최근 보신것있음 소개해주세요",
          "original_form": "네ㅎㅎ 최근 보신것있음 소개해주세요",
          "speaker_id": "2",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005398.1.10",
          "form": "저도 만화책이나 철학책 같은거 이외에는 잘 안읽어서..",
          "original_form": "저도 만화책이나 철학책 같은거 이외에는 잘 안읽어서..",
          "speaker_id": "1",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005398.1.11",
          "form": "그리고 아무도 없었다 재밌어보이더라구요",
          "original_form": "그리고 아무도 없었다 재밌어보이더라구요",
          "speaker_id": "1",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005398.1.12",
          "form": "그렇군요 웹툰도 인기있음 단행본 나오더라구요",
          "original_form": "그렇군요 웹툰도 인기있음 단행본 나오더라구요",
          "speaker_id": "2",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005398.1.13",
          "form": "근데 컬러라그런지 하나당 되게 비싸게 느껴지더라구요",
          "original_form": "근데 컬러라그런지 하나당 되게 비싸게 느껴지더라구요",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005398.1.14",
          "form": "맞아요..책값들도 많이 올랐죠",
          "original_form": "맞아요..책값들도 많이 올랐죠",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005398.1.15",
          "form": "저번에 하나 샀었는데 웹툰 단행본 하나가 만오천원 비슷하더라구요..",
          "original_form": "저번에 하나 샀었는데 웹툰 단행본 하나가 만오천원 비슷하더라구요..",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005398.1.16",
          "form": "어떤책인가요?",
          "original_form": "어떤책인가요?",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005398.1.17",
          "form": "마음의소리인데",
          "original_form": "마음의소리인데",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005398.1.18",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005398.1.19",
          "form": "네ㅋ 들어봤어요 조석",
          "original_form": "네ㅋ 들어봤어요 조석",
          "speaker_id": "2",
          "time": "20210821 22:41"
        }
      ]
