      "utterance": [
        {
          "id": "MDRW2100023310.1.1",
          "form": "최근에 구매한것 중 가장 마음에 드는것이 무었일까요?",
          "original_form": "최근에 구매한것 중 가장 마음에 드는것이 무었일까요?",
          "speaker_id": "2",
          "time": "20210829 23:57"
        },
        {
          "id": "MDRW2100023310.1.2",
          "form": "저는ㅋㅋㅋ애들 옷이요ㅋㅋㅋ",
          "original_form": "저는ㅋㅋㅋ애들 옷이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100023310.1.3",
          "form": "계절마다 애들옷 사기 바빠요",
          "original_form": "계절마다 애들옷 사기 바빠요",
          "speaker_id": "1",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100023310.1.4",
          "form": "쑥쑥 크거든요",
          "original_form": "쑥쑥 크거든요",
          "speaker_id": "1",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100023310.1.5",
          "form": "오 애기옷은 살때마다 행복해지더라구요",
          "original_form": "오 애기옷은 살때마다 행복해지더라구요",
          "speaker_id": "2",
          "time": "20210830 11:35"
        },
        {
          "id": "MDRW2100023310.1.6",
          "form": "금방 커서 못입는 옷도 있어서 안타까워요 ㅎㅎ",
          "original_form": "금방 커서 못입는 옷도 있어서 안타까워요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 11:35"
        },
        {
          "id": "MDRW2100023310.1.7",
          "form": "네 맞아요. 금방 크니깐 진짜 한 철 있고 또 사고 그러는것같아요",
          "original_form": "네 맞아요. 금방 크니깐 진짜 한 철 있고 또 사고 그러는것같아요",
          "speaker_id": "1",
          "time": "20210830 18:12"
        },
        {
          "id": "MDRW2100023310.1.8",
          "form": "맞습니다 당근하시나요?",
          "original_form": "맞습니다 당근하시나요?",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023310.1.9",
          "form": "네 당근하죠ㅋㅋ좋아해요ㅋ",
          "original_form": "네 당근하죠ㅋㅋ좋아해요ㅋ",
          "speaker_id": "1",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100023310.1.10",
          "form": "방금 당근해서  식탁을 팔려고 했는데 불발되었습니다",
          "original_form": "방금 당근해서  식탁을 팔려고 했는데 불발되었습니다",
          "speaker_id": "2",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100023310.1.11",
          "form": "한샘 식탁  확장형",
          "original_form": "한샘 식탁  확장형",
          "speaker_id": "2",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100023310.1.12",
          "form": "5만원에 올렸는데",
          "original_form": "5만원에 올렸는데",
          "speaker_id": "2",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100023310.1.13",
          "form": "갑자기 안  산다다 하네요",
          "original_form": "갑자기 안  산다다 하네요",
          "speaker_id": "2",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100023310.1.14",
          "form": "헉 한샘식탁 ㅜ5만원",
          "original_form": "헉 한샘식탁 ㅜ5만원",
          "speaker_id": "1",
          "time": "20210831 08:39"
        },
        {
          "id": "MDRW2100023310.1.15",
          "form": "저렴하게파시는데ㅜ",
          "original_form": "저렴하게파시는데ㅜ",
          "speaker_id": "1",
          "time": "20210831 08:39"
        },
        {
          "id": "MDRW2100023310.1.16",
          "form": "불발이라니ㅜ속상하겠어요ㅜ",
          "original_form": "불발이라니ㅜ속상하겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210831 08:39"
        },
        {
          "id": "MDRW2100023310.1.17",
          "form": "아 아침되니까 3명이 산다고 연락왔네요",
          "original_form": "아 아침되니까 3명이 산다고 연락왔네요",
          "speaker_id": "2",
          "time": "20210831 09:24"
        },
        {
          "id": "MDRW2100023310.1.18",
          "form": "거래하기로 했습니다 ㅎㅎ",
          "original_form": "거래하기로 했습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 09:24"
        },
        {
          "id": "MDRW2100023310.1.19",
          "form": "공감 해주셔서 감사합니다",
          "original_form": "공감 해주셔서 감사합니다",
          "speaker_id": "2",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100023310.1.20",
          "form": "성격 짱좋으신듯",
          "original_form": "성격 짱좋으신듯",
          "speaker_id": "2",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100023310.1.21",
          "form": "오오오 ㅋㅋ 좋으네요 ㅋㅋㅋ",
          "original_form": "오오오 ㅋㅋ 좋으네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:36"
        },
        {
          "id": "MDRW2100023310.1.22",
          "form": "저는 이제 당근에 팔게 없어요 ㅋㅋㅋㅋㅋㅋㅋ 지난달에 엄청 팔았었거든요 ㅋㅋ",
          "original_form": "저는 이제 당근에 팔게 없어요 ㅋㅋㅋㅋㅋㅋㅋ 지난달에 엄청 팔았었거든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:36"
        },
        {
          "id": "MDRW2100023310.1.23",
          "form": "오 당근으로 중고거래 하면 뭔가 뿌듯하고 집도 넓어지고 좋아요",
          "original_form": "오 당근으로 중고거래 하면 뭔가 뿌듯하고 집도 넓어지고 좋아요",
          "speaker_id": "2",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100023310.1.24",
          "form": "잘하셨습니다",
          "original_form": "잘하셨습니다",
          "speaker_id": "2",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100023310.1.25",
          "form": "네 맞아요 그래서 부수입 쏠쏠했어요 ㅋㅋ",
          "original_form": "네 맞아요 그래서 부수입 쏠쏠했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:21"
        },
        {
          "id": "MDRW2100023310.1.26",
          "form": "명절다가오는데 뭐사시나요?",
          "original_form": "명절다가오는데 뭐사시나요?",
          "speaker_id": "1",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100023310.1.27",
          "form": "아 바빠서 답장을 못드렸어요 ㅠ 명절에 음식해야되서 과일도 사고",
          "original_form": "아 바빠서 답장을 못드렸어요 ㅠ 명절에 음식해야되서 과일도 사고",
          "speaker_id": "2",
