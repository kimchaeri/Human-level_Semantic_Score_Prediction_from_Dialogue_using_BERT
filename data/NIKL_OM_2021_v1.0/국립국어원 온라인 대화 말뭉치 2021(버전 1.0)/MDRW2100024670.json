      "utterance": [
        {
          "id": "MDRW2100024670.1.1",
          "form": "최근 구매하신 물건이 뭔가요~? 저는 비비고 왕교자 만두요",
          "original_form": "최근 구매하신 물건이 뭔가요~? 저는 비비고 왕교자 만두요",
          "speaker_id": "1",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100024670.1.2",
          "form": "왕교자 ㅋㅋ",
          "original_form": "왕교자 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100024670.1.3",
          "form": "최근에 바디로션샀어요",
          "original_form": "최근에 바디로션샀어요",
          "speaker_id": "2",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100024670.1.4",
          "form": "ㅋㅋㅋ체해서 며칠 못먹었더니 먹는거만 찾아다녔네요",
          "original_form": "ㅋㅋㅋ체해서 며칠 못먹었더니 먹는거만 찾아다녔네요",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100024670.1.5",
          "form": "오 어떤제품이 좋아요?",
          "original_form": "오 어떤제품이 좋아요?",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100024670.1.6",
          "form": "향 있는거 좋아해서 비욘드꺼 토탈어쩌구 샀어요",
          "original_form": "향 있는거 좋아해서 비욘드꺼 토탈어쩌구 샀어요",
          "speaker_id": "2",
          "time": "20210831 17:14"
        },
        {
          "id": "MDRW2100024670.1.7",
          "form": "아이구 체하면 진짜 고생인데",
          "original_form": "아이구 체하면 진짜 고생인데",
          "speaker_id": "2",
          "time": "20210831 17:14"
        },
        {
          "id": "MDRW2100024670.1.8",
          "form": "비욘드~ 예전에 보라색  써봤던거같아여 냄새 좋아서",
          "original_form": "비욘드~ 예전에 보라색  써봤던거같아여 냄새 좋아서",
          "speaker_id": "1",
          "time": "20210831 17:15"
        },
        {
          "id": "MDRW2100024670.1.9",
          "form": "ㅠ...그니깐요 어제 응급실 다녀왔는데 여전하네요",
          "original_form": "ㅠ...그니깐요 어제 응급실 다녀왔는데 여전하네요",
          "speaker_id": "1",
          "time": "20210831 17:15"
        },
        {
          "id": "MDRW2100024670.1.10",
          "form": "근데 비비고라고요? ㅋ",
          "original_form": "근데 비비고라고요? ㅋ",
          "speaker_id": "2",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024670.1.11",
          "form": "이온음료 계속마시고 탄산수 마시면 좀 나아지더라구요",
          "original_form": "이온음료 계속마시고 탄산수 마시면 좀 나아지더라구요",
          "speaker_id": "2",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024670.1.12",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024670.1.13",
          "form": "비비고 아닌가여 ㅋㅋ 그 씨제이 왕교자 만두",
          "original_form": "비비고 아닌가여 ㅋㅋ 그 씨제이 왕교자 만두",
          "speaker_id": "1",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024670.1.14",
          "form": "아하 탄산수ㅎ 집에가서 마셔봐야겠네요",
          "original_form": "아하 탄산수ㅎ 집에가서 마셔봐야겠네요",
          "speaker_id": "1",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024670.1.15",
          "form": "저는 체하면 두통도 있더라고요",
          "original_form": "저는 체하면 두통도 있더라고요",
          "speaker_id": "1",
          "time": "20210831 17:19"
        },
        {
          "id": "MDRW2100024670.1.16",
          "form": "마자요 비비고 왕교자",
          "original_form": "마자요 비비고 왕교자",
          "speaker_id": "2",
          "time": "20210831 17:19"
        },
        {
          "id": "MDRW2100024670.1.17",
          "form": "체끼 가시면 드세요",
          "original_form": "체끼 가시면 드세요",
          "speaker_id": "2",
          "time": "20210831 17:19"
        },
        {
          "id": "MDRW2100024670.1.18",
          "form": "체아면 꼬일듯 아픈 기분나쁜 복통과 식은땀나는 두통",
          "original_form": "체아면 꼬일듯 아픈 기분나쁜 복통과 식은땀나는 두통",
          "speaker_id": "2",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100024670.1.19",
          "form": "진짜 싫어요",
          "original_form": "진짜 싫어요",
          "speaker_id": "2",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100024670.1.20",
          "form": "맞아요 진짜싫어요",
          "original_form": "맞아요 진짜싫어요",
          "speaker_id": "1",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024670.1.21",
          "form": "근데 엄청 배고프네요...",
          "original_form": "근데 엄청 배고프네요...",
          "speaker_id": "1",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024670.1.22",
          "form": "여기서 또 뭐 먹으면 소화안될까 무서워서 참고있네요",
          "original_form": "여기서 또 뭐 먹으면 소화안될까 무서워서 참고있네요",
          "speaker_id": "1",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024670.1.23",
          "form": "다 나으면 꼭 라면 끓여서 비비고 퐁당해서 드세요",
          "original_form": "다 나으면 꼭 라면 끓여서 비비고 퐁당해서 드세요",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024670.1.24",
          "form": "지금은 참는걸로",
          "original_form": "지금은 참는걸로",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024670.1.25",
          "form": "헉 대박 라면에 ㅠㅠ 왜 그생각을 못했는지",
          "original_form": "헉 대박 라면에 ㅠㅠ 왜 그생각을 못했는지",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024670.1.26",
          "form": "더 배고파지네요 ㅋㅋㅋ",
          "original_form": "더 배고파지네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024670.1.27",
          "form": "제가 오히려 식욕에 불을 지폈나요?",
          "original_form": "제가 오히려 식욕에 불을 지폈나요?",
          "speaker_id": "2",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024670.1.28",
          "form": "ㅋㅋㅋ활활 타오르네요",
          "original_form": "ㅋㅋㅋ활활 타오르네요",
          "speaker_id": "1",
          "time": "20210831 17:44"
        },
        {
          "id": "MDRW2100024670.1.29",
          "form": "퇴근때 되니까 더 그런듯요",
          "original_form": "퇴근때 되니까 더 그런듯요",
          "speaker_id": "1",
          "time": "20210831 17:44"
        },
        {
          "id": "MDRW2100024670.1.30",
          "form": "쾌차하시길",
          "original_form": "쾌차하시길",
          "speaker_id": "2",
          "time": "20210831 17:45"
        }
      ]
    }
