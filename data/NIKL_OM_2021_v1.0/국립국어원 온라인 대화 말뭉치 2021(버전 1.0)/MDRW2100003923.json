      "utterance": [
        {
          "id": "MDRW2100003923.1.1",
          "form": "티비봐요?",
          "original_form": "티비봐요?",
          "speaker_id": "1",
          "time": "20210821 13:52"
        },
        {
          "id": "MDRW2100003923.1.2",
          "form": "저 예능 많이 봐요",
          "original_form": "저 예능 많이 봐요",
          "speaker_id": "2",
          "time": "20210821 13:53"
        },
        {
          "id": "MDRW2100003923.1.3",
          "form": "드라마는 슬기로운 의사생활 보고",
          "original_form": "드라마는 슬기로운 의사생활 보고",
          "speaker_id": "2",
          "time": "20210821 13:53"
        },
        {
          "id": "MDRW2100003923.1.4",
          "form": "예능은 나혼자산다 맛있는녀석들",
          "original_form": "예능은 나혼자산다 맛있는녀석들",
          "speaker_id": "2",
          "time": "20210821 13:54"
        },
        {
          "id": "MDRW2100003923.1.5",
          "form": "런닝맨 잼나요",
          "original_form": "런닝맨 잼나요",
          "speaker_id": "1",
          "time": "20210821 13:54"
        },
        {
          "id": "MDRW2100003923.1.6",
          "form": "그것도 봄",
          "original_form": "그것도 봄",
          "speaker_id": "2",
          "time": "20210821 13:54"
        },
        {
          "id": "MDRW2100003923.1.7",
          "form": "오프닝에 멤버들끼리 떠드는게 제일 재밌어요",
          "original_form": "오프닝에 멤버들끼리 떠드는게 제일 재밌어요",
          "speaker_id": "2",
          "time": "20210821 13:54"
        },
        {
          "id": "MDRW2100003923.1.8",
          "form": "광수 없어도 잼나요",
          "original_form": "광수 없어도 잼나요",
          "speaker_id": "1",
          "time": "20210821 13:55"
        },
        {
          "id": "MDRW2100003923.1.9",
          "form": "광수가 에이스긴 한데",
          "original_form": "광수가 에이스긴 한데",
          "speaker_id": "2",
          "time": "20210821 13:55"
        },
        {
          "id": "MDRW2100003923.1.10",
          "form": "없어도 토크가 비질않음",
          "original_form": "없어도 토크가 비질않음",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003923.1.11",
          "form": "지석진 잼나요",
          "original_form": "지석진 잼나요",
          "speaker_id": "1",
          "time": "20210821 13:57"
        },
        {
          "id": "MDRW2100003923.1.12",
          "form": "지석진 자기 까는거 받아주니까 재밌어졌어요",
          "original_form": "지석진 자기 까는거 받아주니까 재밌어졌어요",
          "speaker_id": "2",
          "time": "20210821 13:58"
        },
        {
          "id": "MDRW2100003923.1.13",
          "form": "캐미가 정말",
          "original_form": "캐미가 정말",
          "speaker_id": "1",
          "time": "20210821 13:58"
        },
        {
          "id": "MDRW2100003923.1.14",
          "form": "송지효가 요즘은 너무 재미없어졌는데",
          "original_form": "송지효가 요즘은 너무 재미없어졌는데",
          "speaker_id": "2",
          "time": "20210821 13:59"
        },
        {
          "id": "MDRW2100003923.1.15",
          "form": "전소민 캐릭터가 송지효까지 다 커버함",
          "original_form": "전소민 캐릭터가 송지효까지 다 커버함",
          "speaker_id": "2",
          "time": "20210821 13:59"
        },
        {
          "id": "MDRW2100003923.1.16",
          "form": "돌아이",
          "original_form": "돌아이",
          "speaker_id": "1",
          "time": "20210821 14:00"
        },
        {
          "id": "MDRW2100003923.1.17",
          "form": "진짜 전소민같은 캐릭터 어디서 났나 싶음",
          "original_form": "진짜 전소민같은 캐릭터 어디서 났나 싶음",
          "speaker_id": "2",
          "time": "20210821 14:01"
        },
        {
          "id": "MDRW2100003923.1.18",
          "form": "광수도 연기자로는 대성하긴 힘들것 같은데",
          "original_form": "광수도 연기자로는 대성하긴 힘들것 같은데",
          "speaker_id": "2",
          "time": "20210821 14:01"
        },
        {
          "id": "MDRW2100003923.1.19",
          "form": "예능인 이광수만큼",
          "original_form": "예능인 이광수만큼",
          "speaker_id": "2",
          "time": "20210821 14:01"
        },
        {
          "id": "MDRW2100003923.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
