      "utterance": [
        {
          "id": "MDRW2100015913.1.1",
          "form": "어떤 일 하시는지 뭉어봐도 될까요?",
          "original_form": "어떤 일 하시는지 뭉어봐도 될까요?",
          "speaker_id": "1",
          "time": "20210826 02:48"
        },
        {
          "id": "MDRW2100015913.1.2",
          "form": "앗 저 백수인거 어떻게 아시구..!",
          "original_form": "앗 저 백수인거 어떻게 아시구..!",
          "speaker_id": "2",
          "time": "20210826 02:49"
        },
        {
          "id": "MDRW2100015913.1.3",
          "form": "무역쪽에서 4년 일하다가",
          "original_form": "무역쪽에서 4년 일하다가",
          "speaker_id": "2",
          "time": "20210826 02:49"
        },
        {
          "id": "MDRW2100015913.1.4",
          "form": "퇴사했어요유ㅠ",
          "original_form": "퇴사했어요유ㅠ",
          "speaker_id": "2",
          "time": "20210826 02:49"
        },
        {
          "id": "MDRW2100015913.1.5",
          "form": "헐ㅋㅋㅋㅋㅋㅋㅋㅋㅠㅠ",
          "original_form": "헐ㅋㅋㅋㅋㅋㅋㅋㅋㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 02:49"
        },
        {
          "id": "MDRW2100015913.1.6",
          "form": "저는 아직 학생이에요",
          "original_form": "저는 아직 학생이에요",
          "speaker_id": "1",
          "time": "20210826 02:49"
        },
        {
          "id": "MDRW2100015913.1.7",
          "form": "아핫",
          "original_form": "아핫",
          "speaker_id": "2",
          "time": "20210826 02:50"
        },
        {
          "id": "MDRW2100015913.1.8",
          "form": "근데 저도 과외 선생 6개월하다..",
          "original_form": "근데 저도 과외 선생 6개월하다..",
          "speaker_id": "1",
          "time": "20210826 02:50"
        },
        {
          "id": "MDRW2100015913.1.9",
          "form": "어제부로 짤렸어요",
          "original_form": "어제부로 짤렸어요",
          "speaker_id": "1",
          "time": "20210826 02:50"
        },
        {
          "id": "MDRW2100015913.1.10",
          "form": "어머ㅠㅠㅠ",
          "original_form": "어머ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 02:50"
        },
        {
          "id": "MDRW2100015913.1.11",
          "form": "아이구...",
          "original_form": "아이구...",
          "speaker_id": "2",
          "time": "20210826 02:50"
        },
        {
          "id": "MDRW2100015913.1.12",
          "form": "남은 3주정도하고 끝나요^.^..",
          "original_form": "남은 3주정도하고 끝나요^.^..",
          "speaker_id": "1",
          "time": "20210826 02:50"
        },
        {
          "id": "MDRW2100015913.1.13",
          "form": "속상하시겠네요ㅠㅠ",
          "original_form": "속상하시겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 02:50"
        },
        {
          "id": "MDRW2100015913.1.14",
          "form": "조금 미묘해요ㅠㅠㅋㅋㅠ",
          "original_form": "조금 미묘해요ㅠㅠㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210826 02:50"
        },
        {
          "id": "MDRW2100015913.1.15",
          "form": "요즘 일 구하는게 넘 어렵죠ㅠㅠ..",
          "original_form": "요즘 일 구하는게 넘 어렵죠ㅠㅠ..",
          "speaker_id": "2",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100015913.1.16",
          "form": "숙제도 안해오고 수업하기 힘든 친구라 과외할땐 힘들었는데",
          "original_form": "숙제도 안해오고 수업하기 힘든 친구라 과외할땐 힘들었는데",
          "speaker_id": "1",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100015913.1.17",
          "form": "또 짤리게됐으니까",
          "original_form": "또 짤리게됐으니까",
          "speaker_id": "1",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100015913.1.18",
          "form": "학생이 넘 적극적이지 않으면 선생님이 너무 힘들것같아요",
          "original_form": "학생이 넘 적극적이지 않으면 선생님이 너무 힘들것같아요",
          "speaker_id": "2",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100015913.1.19",
          "form": "정말 그런것같아요ㅎㅎ.. 그래서 조금 휴식기를 가지려고요",
          "original_form": "정말 그런것같아요ㅎㅎ.. 그래서 조금 휴식기를 가지려고요",
          "speaker_id": "1",
          "time": "20210826 02:52"
        },
        {
          "id": "MDRW2100015913.1.20",
          "form": "첫과외였는데 너무 힘들었어서요",
          "original_form": "첫과외였는데 너무 힘들었어서요",
          "speaker_id": "1",
          "time": "20210826 02:52"
        },
        {
          "id": "MDRW2100015913.1.21",
          "form": "무역일은 어떤 일인지 여쭤봐도될까요?",
          "original_form": "무역일은 어떤 일인지 여쭤봐도될까요?",
          "speaker_id": "1",
          "time": "20210826 02:52"
        },
        {
          "id": "MDRW2100015913.1.22",
          "form": "고생많으셨어요ㅠㅠ",
          "original_form": "고생많으셨어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 02:53"
        },
        {
          "id": "MDRW2100015913.1.23",
          "form": "아 국내 로컬 업체에서 물건 매입해서",
          "original_form": "아 국내 로컬 업체에서 물건 매입해서",
          "speaker_id": "2",
          "time": "20210826 02:53"
        },
        {
          "id": "MDRW2100015913.1.24",
          "form": "적절한 단가를 설정해서 대량으로 해외 고객사로 보내는거에용!",
          "original_form": "적절한 단가를 설정해서 대량으로 해외 고객사로 보내는거에용!",
          "speaker_id": "2",
          "time": "20210826 02:53"
        },
        {
          "id": "MDRW2100015913.1.25",
          "form": "수출이랑 겹치죵..?",
          "original_form": "수출이랑 겹치죵..?",
          "speaker_id": "2",
          "time": "20210826 02:53"
        },
        {
          "id": "MDRW2100015913.1.26",
          "form": "아 그런거군요 !!",
