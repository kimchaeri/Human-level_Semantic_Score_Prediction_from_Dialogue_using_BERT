      "utterance": [
        {
          "id": "MDRW2100027896.1.1",
          "form": "이제 가을이네요!",
          "original_form": "이제 가을이네요!",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100027896.1.2",
          "form": "가을 좋아하시나요?",
          "original_form": "가을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027896.1.3",
          "form": "네 가을 좋아해요!",
          "original_form": "네 가을 좋아해요!",
          "speaker_id": "2",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100027896.1.4",
          "form": "시원따숩을 느낄수있는..",
          "original_form": "시원따숩을 느낄수있는..",
          "speaker_id": "2",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100027896.1.5",
          "form": "저도 좋아해요ㅋㅋㅋㅋ",
          "original_form": "저도 좋아해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100027896.1.6",
          "form": "사실 겨울을 좋아하긴 해요",
          "original_form": "사실 겨울을 좋아하긴 해요",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100027896.1.7",
          "form": "여름이 제일 싫어요ㅋㅋㅋ",
          "original_form": "여름이 제일 싫어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100027896.1.8",
          "form": "헉 사실 저도 사계절 중에",
          "original_form": "헉 사실 저도 사계절 중에",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100027896.1.9",
          "form": "겨울 젤 좋아해요 춥지만 벌레도 없고",
          "original_form": "겨울 젤 좋아해요 춥지만 벌레도 없고",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100027896.1.10",
          "form": "겨울-가을-봄-여름 순일까요",
          "original_form": "겨울-가을-봄-여름 순일까요",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100027896.1.11",
          "form": "헐 대박 진짜요?? 우왕",
          "original_form": "헐 대박 진짜요?? 우왕",
          "speaker_id": "1",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100027896.1.12",
          "form": "동지네요 저두 똑같이 좋아해요",
          "original_form": "동지네요 저두 똑같이 좋아해요",
          "speaker_id": "1",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100027896.1.13",
          "form": "원래는 봄을 더 좋아했는데",
          "original_form": "원래는 봄을 더 좋아했는데",
          "speaker_id": "1",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100027896.1.14",
          "form": "미세먼지때매 가을한테 밀렸어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "미세먼지때매 가을한테 밀렸어요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100027896.1.15",
          "form": "겨울은 부동의 1위!!",
          "original_form": "겨울은 부동의 1위!!",
          "speaker_id": "1",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100027896.1.16",
          "form": "벌레없는 거 너무 좋구 눈도 솔직히 좋아해요ㅋㅋㅋ",
          "original_form": "벌레없는 거 너무 좋구 눈도 솔직히 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100027896.1.17",
          "form": "헐 저도에요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐 저도에요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100027896.1.18",
          "form": "봄 따스해서 좋은데ㅜ 미세먼지랑 황사콤보로 아웃...",
          "original_form": "봄 따스해서 좋은데ㅜ 미세먼지랑 황사콤보로 아웃...",
          "speaker_id": "2",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100027896.1.19",
          "form": "꽃구경 하는건 좋지만요!",
          "original_form": "꽃구경 하는건 좋지만요!",
          "speaker_id": "2",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100027896.1.20",
          "form": "맞아요ㅠㅠ 눈 싫어하는 사람도 있지만... 눈 좋아해요 밟는것두",
          "original_form": "맞아요ㅠㅠ 눈 싫어하는 사람도 있지만... 눈 좋아해요 밟는것두",
          "speaker_id": "2",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100027896.1.21",
          "form": "저도요 뽀득뽀듯 너무 좋아요",
          "original_form": "저도요 뽀득뽀듯 너무 좋아요",
          "speaker_id": "1",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100027896.1.22",
          "form": "겨울 옷 무거운 것만 빼면 정말 다 좋은 거 같아요ㅠㅠ",
          "original_form": "겨울 옷 무거운 것만 빼면 정말 다 좋은 거 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100027896.1.23",
          "form": "옷 무게가 은근 상당하단 걸... 근육 다 빠지고 깨달았지 뭐예요..",
          "original_form": "옷 무게가 은근 상당하단 걸... 근육 다 빠지고 깨달았지 뭐예요..",
          "speaker_id": "1",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100027896.1.24",
          "form": "맞아요ㅜ 특히 아무도 안 밟은 눈을 내가 처음으로 밟을때...!",
          "original_form": "맞아요ㅜ 특히 아무도 안 밟은 눈을 내가 처음으로 밟을때...!",
          "speaker_id": "2",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100027896.1.25",
          "form": "그쵸 패딩부터...가벼운것도 있지만",
          "original_form": "그쵸 패딩부터...가벼운것도 있지만",
          "speaker_id": "2",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100027896.1.26",
          "form": "겨울은 옷이 두껍다보니 무게가 있죠",
          "original_form": "겨울은 옷이 두껍다보니 무게가 있죠",
          "speaker_id": "2",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100027896.1.27",
          "form": "마자요 두꺼우니까 여름보다는...",
          "original_form": "마자요 두꺼우니까 여름보다는...",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100027896.1.28",
          "form": "몇년 전까진 예쁜 옷 찾았는데",
          "original_form": "몇년 전까진 예쁜 옷 찾았는데",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100027896.1.29",
          "form": "작년부터 급격히 가벼운 옷 찾는 저 자신을 발견하고 웃었네요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "작년부터 급격히 가벼운 옷 찾는 저 자신을 발견하고 웃었네요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100027896.1.30",
          "form": "ㅋㅋㅋㅋ예쁜옷 입어도 패딩이 가려주니깐",
          "original_form": "ㅋㅋㅋㅋ예쁜옷 입어도 패딩이 가려주니깐",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100027896.1.31",
          "form": "가볍지만 따뜻한 옷 걸치는것도 좋죠ㅋㅋㅋㅋ",
          "original_form": "가볍지만 따뜻한 옷 걸치는것도 좋죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100027896.1.32",
          "form": "조만간 옷 쇼핑해야겠어요",
          "original_form": "조만간 옷 쇼핑해야겠어요",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100027896.1.33",
          "form": "맞아요 다가올 겨울에 대비해야하니까요ㅋㅋㅋㅋ",
          "original_form": "맞아요 다가올 겨울에 대비해야하니까요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100027896.1.34",
          "form": "저도 그 핑ㄱㅖ로 마음껏 지르는 중입니다ㅋㅋㅋㅋ",
          "original_form": "저도 그 핑ㄱㅖ로 마음껏 지르는 중입니다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100027896.1.35",
          "form": "겨울오기전에 가을에 예쁜 옷 많이 입어야죠!",
          "original_form": "겨울오기전에 가을에 예쁜 옷 많이 입어야죠!",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100027896.1.36",
          "form": "가을에 입을 거 고르면 베이지가 눈이 가요",
          "original_form": "가을에 입을 거 고르면 베이지가 눈이 가요",
          "speaker_id": "2",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100027896.1.37",
          "form": "마자요마자요 베이지~ 요즘엔 빈티지한 컬러가 많이 나와서",
          "original_form": "마자요마자요 베이지~ 요즘엔 빈티지한 컬러가 많이 나와서",
          "speaker_id": "1",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100027896.1.38",
          "form": "커피색에 가까운 베이지랑 차분한 네이비가 끌리더라구용",
          "original_form": "커피색에 가까운 베이지랑 차분한 네이비가 끌리더라구용",
          "speaker_id": "1",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100027896.1.39",
          "form": "가을옷 예쁜 거 많아요ㅠㅠ",
          "original_form": "가을옷 예쁜 거 많아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100027896.1.40",
          "form": "오 가을하면 빈티지죠ㅠㅠ!!",
          "original_form": "오 가을하면 빈티지죠ㅠㅠ!!",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100027896.1.41",
          "form": "베이지 이즈 뭔들...!",
          "original_form": "베이지 이즈 뭔들...!",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100027896.1.42",
          "form": "네이비쪽은 잘 안 입지만 예쁠거같아요!!",
          "original_form": "네이비쪽은 잘 안 입지만 예쁠거같아요!!",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100027896.1.43",
          "form": "이번주...인터넷 쇼핑해야겠다",
          "original_form": "이번주...인터넷 쇼핑해야겠다",
