        {
          "id": "MDRW2100036653.1.1",
          "form": "안녕라새요",
          "original_form": "안녕라새요",
          "speaker_id": "2",
          "time": "20210831 04:14"
        },
        {
          "id": "MDRW2100036653.1.2",
          "form": "안녕하세욥",
          "original_form": "안녕하세욥",
          "speaker_id": "1",
          "time": "20210831 04:16"
        },
        {
          "id": "MDRW2100036653.1.3",
          "form": "우유 좋아하세요?",
          "original_form": "우유 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 04:16"
        },
        {
          "id": "MDRW2100036653.1.4",
          "form": "네.. 저는 부산우유",
          "original_form": "네.. 저는 부산우유{emoji:♥}",
          "speaker_id": "1",
          "time": "20210831 04:17"
        },
        {
          "id": "MDRW2100036653.1.5",
          "form": "부산우유를 따로 파나요?",
          "original_form": "부산우유를 따로 파나요?",
          "speaker_id": "2",
          "time": "20210831 04:17"
        },
        {
          "id": "MDRW2100036653.1.6",
          "form": "gs수퍼에서 1974우유,\r\n이마트에서도 부산우유에서 만든 제품이잇어용",
          "original_form": "gs수퍼에서 1974우유,\r\n이마트에서도 부산우유에서 만든 제품이잇어용",
          "speaker_id": "1",
          "time": "20210831 04:18"
        },
        {
          "id": "MDRW2100036653.1.7",
          "form": "피코크꺼인가",
          "original_form": "피코크꺼인가",
          "speaker_id": "1",
          "time": "20210831 04:18"
        },
        {
          "id": "MDRW2100036653.1.8",
          "form": "왕 자세히 아시네요!",
          "original_form": "왕 자세히 아시네요!",
          "speaker_id": "2",
          "time": "20210831 04:19"
        },
        {
          "id": "MDRW2100036653.1.9",
          "form": "네..찾아봣죱. 확인해보세요 숫자체크",
          "original_form": "네..찾아봣죱. 확인해보세요 숫자체크",
          "speaker_id": "1",
          "time": "20210831 04:20"
        },
        {
          "id": "MDRW2100036653.1.10",
          "form": "서울우유보다 맛있나요?",
          "original_form": "서울우유보다 맛있나요?",
          "speaker_id": "2",
          "time": "20210831 04:20"
        },
        {
          "id": "MDRW2100036653.1.11",
          "form": "모루겟네요",
          "original_form": "모루겟네요",
          "speaker_id": "1",
          "time": "20210831 04:24"
        },
        {
          "id": "MDRW2100036653.1.12",
          "form": "테스트좀하고 후기좀",
          "original_form": "테스트좀하고 후기좀",
          "speaker_id": "1",
          "time": "20210831 04:24"
        },
        {
          "id": "MDRW2100036653.1.13",
          "form": "해외우유는 어땨요?",
          "original_form": "해외우유는 어땨요?",
          "speaker_id": "2",
          "time": "20210831 04:25"
        },
        {
          "id": "MDRW2100036653.1.14",
          "form": "저는 맛잇다고생각해요",
          "original_form": "저는 맛잇다고생각해요",
          "speaker_id": "1",
          "time": "20210831 04:26"
        },
        {
          "id": "MDRW2100036653.1.15",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 04:26"
        },
        {
          "id": "MDRW2100036653.1.16",
          "form": "멸균말고용",
          "original_form": "멸균말고용",
          "speaker_id": "1",
          "time": "20210831 04:26"
        },
        {
          "id": "MDRW2100036653.1.17",
          "form": "저는 멸균 맛으로 안먹어요",
          "original_form": "저는 멸균 맛으로 안먹어요",
