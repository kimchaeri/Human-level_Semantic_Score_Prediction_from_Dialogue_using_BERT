      "utterance": [
        {
          "id": "MDRW2100007580.1.1",
          "form": "여행가고싶은곳있어요?",
          "original_form": "여행가고싶은곳있어요?",
          "speaker_id": "1",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007580.1.2",
          "form": "국내해외",
          "original_form": "국내해외",
          "speaker_id": "1",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007580.1.3",
          "form": "최근에 다녀온곳있어오?",
          "original_form": "최근에 다녀온곳있어오?",
          "speaker_id": "1",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007580.1.4",
          "form": "음 남미쪽을 가보고 싶어요",
          "original_form": "음 남미쪽을 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100007580.1.5",
          "form": "남미요?",
          "original_form": "남미요?",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100007580.1.6",
          "form": "특이하다",
          "original_form": "특이하다",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100007580.1.7",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100007580.1.8",
          "form": "남미 대륙은 아예 한번도 안가봐서",
          "original_form": "남미 대륙은 아예 한번도 안가봐서",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100007580.1.9",
          "form": "한번 가보고 싶어요",
          "original_form": "한번 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100007580.1.10",
          "form": "저도 안가봤는데 진작에 많이갈걸그랬어요",
          "original_form": "저도 안가봤는데 진작에 많이갈걸그랬어요",
          "speaker_id": "1",
          "time": "20210822 13:51"
        },
        {
          "id": "MDRW2100007580.1.11",
          "form": "그러게요 코로나가 올지 몰랐는데ㅠ",
          "original_form": "그러게요 코로나가 올지 몰랐는데ㅠ",
          "speaker_id": "2",
          "time": "20210822 13:53"
        },
        {
          "id": "MDRW2100007580.1.12",
          "form": "터키준비하다가 코로나터져서 못갔어요ㅋㅋ",
          "original_form": "터키준비하다가 코로나터져서 못갔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100007580.1.13",
          "form": "헉 정말 아쉬우셨겠어요",
          "original_form": "헉 정말 아쉬우셨겠어요",
          "speaker_id": "2",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100007580.1.14",
          "form": "터키가면 어디 가려고 하셨나요?",
          "original_form": "터키가면 어디 가려고 하셨나요?",
          "speaker_id": "2",
          "time": "20210822 14:17"
        },
        {
          "id": "MDRW2100007580.1.15",
          "form": "남편이 준비한거라 잘 모르겠네요\r\n칄발만 했으면됬는데",
          "original_form": "남편이 준비한거라 잘 모르겠네요\r\n칄발만 했으면됬는데",
          "speaker_id": "1",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100007580.1.16",
          "form": "나중에 가면되지 하고 말았는데",
          "original_form": "나중에 가면되지 하고 말았는데",
          "speaker_id": "1",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100007580.1.17",
          "form": "코로나가 이렇게 길어질지몰랐어요",
          "original_form": "코로나가 이렇게 길어질지몰랐어요",
          "speaker_id": "1",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100007580.1.18",
          "form": "그러게요 전 터키하면 터키.아이스크림밖에 몰라서ㅠ",
          "original_form": "그러게요 전 터키하면 터키.아이스크림밖에 몰라서ㅠ",
          "speaker_id": "2",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100007580.1.19",
          "form": "앜ㅋㅋㅋㅋ 진짜 오랜만에 들어요",
          "original_form": "앜ㅋㅋㅋㅋ 진짜 오랜만에 들어요",
          "speaker_id": "1",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100007580.1.20",
          "form": "여행가면 꼭 사오는거있어요?",
          "original_form": "여행가면 꼭 사오는거있어요?",
          "speaker_id": "1",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100007580.1.21",
          "form": "음 저는 열쇠고리요",
          "original_form": "음 저는 열쇠고리요",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100007580.1.22",
          "form": "특이하네요",
          "original_form": "특이하네요",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100007580.1.23",
          "form": "저는 자물쇠를 국내해외 다 달고와요ㅋㅋ",
          "original_form": "저는 자물쇠를 국내해외 다 달고와요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100007580.1.24",
          "form": "하트자물쇠",
          "original_form": "하트자물쇠",
          "speaker_id": "1",
