      "utterance": [
        {
          "id": "MDRW2100024071.1.1",
          "form": "무슨 일 하석요?",
          "original_form": "무슨 일 하석요?",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024071.1.2",
          "form": "구직 활동 중 입니다",
          "original_form": "구직 활동 중 입니다",
          "speaker_id": "1",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024071.1.3",
          "form": "서비스업이 힘들다고 알고있는데 대단하세요",
          "original_form": "서비스업이 힘들다고 알고있는데 대단하세요",
          "speaker_id": "1",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024071.1.4",
          "form": "호텔 일이 적성에 맞아서 괜찬아요",
          "original_form": "호텔 일이 적성에 맞아서 괜찬아요",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024071.1.5",
          "form": "적성에 맞는 직업을 찾으셔서 부러워요",
          "original_form": "적성에 맞는 직업을 찾으셔서 부러워요",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024071.1.6",
          "form": "업무 스트레스는 좀 있어도 괜찮아요",
          "original_form": "업무 스트레스는 좀 있어도 괜찮아요",
          "speaker_id": "2",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024071.1.7",
          "form": "저는 생산직을 생각하고있어요",
          "original_form": "저는 생산직을 생각하고있어요",
          "speaker_id": "1",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024071.1.8",
          "form": "힘들겠지만 응원해요",
          "original_form": "힘들겠지만 응원해요",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024071.1.9",
          "form": "생산직이 몸은 힘들겠지만 보람있을 것 같아요",
          "original_form": "생산직이 몸은 힘들겠지만 보람있을 것 같아요",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024071.1.10",
          "form": "직업은 귀천이 없으니까요",
          "original_form": "직업은 귀천이 없으니까요",
          "speaker_id": "2",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024071.1.11",
          "form": "맞아요 어떤 직업이든 본인이 만족하면되니까요",
          "original_form": "맞아요 어떤 직업이든 본인이 만족하면되니까요",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024071.1.12",
          "form": "그렇죠 구직활동은 잘 되가시나요",
          "original_form": "그렇죠 구직활동은 잘 되가시나요",
          "speaker_id": "2",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024071.1.13",
          "form": "열심히 하는 중이긴해요",
          "original_form": "열심히 하는 중이긴해요",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024071.1.14",
          "form": "저는 이직생각이 조금 들긴해요",
          "original_form": "저는 이직생각이 조금 들긴해요",
          "speaker_id": "2",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024071.1.15",
          "form": "낼출근!",
          "original_form": "낼출근!",
