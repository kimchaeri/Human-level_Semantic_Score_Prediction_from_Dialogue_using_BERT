        {
          "id": "MDRW2100037254.1.2",
          "form": "술좋아해?",
          "original_form": "술좋아해?",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037254.1.3",
          "form": "맥주를 제일 좋아해",
          "original_form": "맥주를 제일 좋아해",
          "speaker_id": "1",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037254.1.4",
          "form": "어떤맥주?",
          "original_form": "어떤맥주?",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037254.1.5",
          "form": "에 일류를 좋아해",
          "original_form": "에 일류를 좋아해",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037254.1.6",
          "form": "세계맥주는?",
          "original_form": "세계맥주는?",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037254.1.7",
          "form": "독일 거 좋아해",
          "original_form": "독일 거 좋아해",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037254.1.8",
          "form": "무슨브랜드?",
          "original_form": "무슨브랜드?",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037254.1.9",
          "form": "파울라너",
          "original_form": "파울라너",
          "speaker_id": "1",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100037254.1.10",
          "form": "우리나라 맥주도맛있어",
          "original_form": "우리나라 맥주도맛있어",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
