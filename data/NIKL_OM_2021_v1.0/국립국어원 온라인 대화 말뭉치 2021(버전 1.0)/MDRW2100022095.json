        },
        {
          "id": "MDRW2100022095.1.7",
          "form": "name1님은 게임 좋아하세요?",
          "original_form": "&name1&님은 게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 03:02"
        },
        {
          "id": "MDRW2100022095.1.8",
          "form": "넹 저는 요즘도 해요",
          "original_form": "넹 저는 요즘도 해요",
          "speaker_id": "1",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100022095.1.9",
          "form": "저도 서든은 해봤는데 ㅎㅅㄴ",
          "original_form": "저도 서든은 해봤는데 ㅎㅅㄴ",
          "speaker_id": "1",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100022095.1.10",
          "form": "어떤게임하세요?",
          "original_form": "어떤게임하세요?",
          "speaker_id": "2",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100022095.1.11",
          "form": "어렵더라구요ㅋㅋㅠ",
          "original_form": "어렵더라구요ㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100022095.1.12",
          "form": "전 rpg게임해요",
          "original_form": "전 rpg게임해요",
          "speaker_id": "1",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100022095.1.13",
          "form": "친구가 추천해줘서 지금은 파판하고 잇어요!",
          "original_form": "친구가 추천해줘서 지금은 파판하고 잇어요!",
          "speaker_id": "1",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100022095.1.14",
          "form": "재밌더라구요",
          "original_form": "재밌더라구요",
          "speaker_id": "1",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100022095.1.15",
          "form": "해보셨어요?",
          "original_form": "해보셨어요?",
          "speaker_id": "1",
          "time": "20210829 03:07"
        },
        {
          "id": "MDRW2100022095.1.16",
          "form": "파판 온라인이요?",
          "original_form": "파판 온라인이요?",
          "speaker_id": "2",
          "time": "20210829 03:07"
        },
        {
          "id": "MDRW2100022095.1.17",
          "form": "파판 추억의 게임이네요",
          "original_form": "파판 추억의 게임이네요",
          "speaker_id": "2",
          "time": "20210829 03:07"
        },
        {
          "id": "MDRW2100022095.1.18",
          "form": "ㅋㅋ추억이에요?",
          "original_form": "ㅋㅋ추억이에요?",
          "speaker_id": "1",
          "time": "20210829 03:08"
        },
        {
          "id": "MDRW2100022095.1.19",
          "form": "지금도 해보시는게 어때요?",
          "original_form": "지금도 해보시는게 어때요?",
          "speaker_id": "1",
          "time": "20210829 03:08"
        },
        {
          "id": "MDRW2100022095.1.20",
          "form": "요즘은 퇴근하고 운동하고 재테크 공부하는라ㅎㅎㅎ",
          "original_form": "요즘은 퇴근하고 운동하고 재테크 공부하는라ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 03:08"
        },
        {
          "id": "MDRW2100022095.1.21",
          "form": "오~~",
          "original_form": "오~~",
          "speaker_id": "1",
          "time": "20210829 03:08"
        },
        {
          "id": "MDRW2100022095.1.22",
          "form": "대단해요ㅋㅋㅠ",
          "original_form": "대단해요ㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100022095.1.23",
          "form": "전 아직ㅎㅎ...",
          "original_form": "전 아직ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100022095.1.24",
          "form": "운동좋아하세요?",
          "original_form": "운동좋아하세요?",
