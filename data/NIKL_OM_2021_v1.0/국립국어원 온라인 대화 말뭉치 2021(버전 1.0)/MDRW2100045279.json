      "utterance": [
        {
          "id": "MDRW2100045279.1.1",
          "form": "저는 돈가스 그 소스가 맛잇어요 ㅋㅋ",
          "original_form": "저는 돈가스 그 소스가 맛잇어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045279.1.2",
          "form": "라면 먹고싶네용",
          "original_form": "라면 먹고싶네용",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045279.1.3",
          "form": "맞아요 ㅋㅋㅋ 뿌려져있는데 살짝 바삭하고요",
          "original_form": "맞아요 ㅋㅋㅋ 뿌려져있는데 살짝 바삭하고요",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045279.1.4",
          "form": "분식점 라면도 맛있죠 ㅜㅜ",
          "original_form": "분식점 라면도 맛있죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045279.1.5",
          "form": "맞아요 ㅠㅠ 집에서 끓이면 그맛이 안나요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ 집에서 끓이면 그맛이 안나요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045279.1.6",
          "form": "그니까요.. 비법이 뭔가 따로 있나봐요 ㅜ",
          "original_form": "그니까요.. 비법이 뭔가 따로 있나봐요 ㅜ",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045279.1.7",
          "form": "물양이 중요한가바여... ㅠㅜ",
          "original_form": "물양이 중요한가바여... ㅠㅜ",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045279.1.8",
          "form": "그런가봐요 ... 갑자기 라면 땡기네요..",
          "original_form": "그런가봐요 ... 갑자기 라면 땡기네요..",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045279.1.9",
          "form": "비도 오고 ㅠㅠ 끓이기엔 너무 늦어서 ㅋㅋㅋ",
          "original_form": "비도 오고 ㅠㅠ 끓이기엔 너무 늦어서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045279.1.10",
          "form": "맞아여 .. 낼 먹어야겠어요",
          "original_form": "맞아여 .. 낼 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045279.1.11",
          "form": "어떤 라면 젤 좋아하세요?",
          "original_form": "어떤 라면 젤 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045279.1.12",
          "form": "저는 너구리요!! 오동통통 !!",
          "original_form": "저는 너구리요!! 오동통통 !!",
          "speaker_id": "1",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045279.1.13",
          "form": "오 맛있죠ㅎㅎ 저는 열라면 제일 좋아해요",
          "original_form": "오 맛있죠ㅎㅎ 저는 열라면 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045279.1.14",
          "form": "헉 그라면 맵지않나요",
          "original_form": "헉 그라면 맵지않나요",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045279.1.15",
          "form": "매운거 좋아해서요..ㅎㅎ 맛있어요!!",
          "original_form": "매운거 좋아해서요..ㅎㅎ 맛있어요!!",
