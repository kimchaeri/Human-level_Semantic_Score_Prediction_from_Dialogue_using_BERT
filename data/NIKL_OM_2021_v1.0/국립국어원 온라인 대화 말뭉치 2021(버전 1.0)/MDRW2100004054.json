      "utterance": [
        {
          "id": "MDRW2100004054.1.1",
          "form": "사고 싶은 게 있으세요?",
          "original_form": "사고 싶은 게 있으세요?",
          "speaker_id": "2",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004054.1.2",
          "form": "전 가전제품들 바꾸고 싶어요",
          "original_form": "전 가전제품들 바꾸고 싶어요",
          "speaker_id": "1",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004054.1.3",
          "form": "뭐 사고 싶으세요?",
          "original_form": "뭐 사고 싶으세요?",
          "speaker_id": "1",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004054.1.4",
          "form": "저도 노트북 새로 사고 싶네요",
          "original_form": "저도 노트북 새로 사고 싶네요",
          "speaker_id": "2",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004054.1.5",
          "form": "아 노트북 필수죠",
          "original_form": "아 노트북 필수죠",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004054.1.6",
          "form": "요샌 노트북이 편해요",
          "original_form": "요샌 노트북이 편해요",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004054.1.7",
          "form": "요즘 티트리 오일 뭐 살까 찾고 있어요",
          "original_form": "요즘 티트리 오일 뭐 살까 찾고 있어요",
          "speaker_id": "2",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004054.1.8",
          "form": "들고 다니기 좋고요",
          "original_form": "들고 다니기 좋고요",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004054.1.9",
          "form": "요즘은 탭 많이 쓰더라구요",
          "original_form": "요즘은 탭 많이 쓰더라구요",
          "speaker_id": "2",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004054.1.10",
          "form": "저도 티트리 예전에 사봤는데 잘모르겠네요",
          "original_form": "저도 티트리 예전에 사봤는데 잘모르겠네요",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004054.1.11",
          "form": "어떤 브랜드요?",
          "original_form": "어떤 브랜드요?",
          "speaker_id": "1",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004054.1.12",
          "form": "어떤 제품 바꾸고 싶나요?",
          "original_form": "어떤 제품 바꾸고 싶나요?",
          "speaker_id": "2",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004054.1.13",
          "form": "갤럭시 탭요",
          "original_form": "갤럭시 탭요",
          "speaker_id": "2",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004054.1.14",
          "form": "전 냉장고 세탁기요",
          "original_form": "전 냉장고 세탁기요",
          "speaker_id": "1",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004054.1.15",
          "form": "아 전 아이패드랑 hp 노트북 써요",
          "original_form": "아 전 아이패드랑 hp 노트북 써요",
          "speaker_id": "1",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004054.1.16",
          "form": "전 티트리 오일 맞는데 싼게 잘 없네요",
          "original_form": "전 티트리 오일 맞는데 싼게 잘 없네요",
          "speaker_id": "2",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004054.1.17",
          "form": "노트북은 삼성 쓰고 있어요",
          "original_form": "노트북은 삼성 쓰고 있어요",
          "speaker_id": "2",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004054.1.18",
          "form": "싼거 없어요 오일 비싸요",
          "original_form": "싼거 없어요 오일 비싸요",
          "speaker_id": "1",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004054.1.19",
          "form": "제 아들 삼성써요",
          "original_form": "제 아들 삼성써요",
          "speaker_id": "1",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004054.1.20",
          "form": "요번에 갤럭시북고 샀어요",
          "original_form": "요번에 갤럭시북고 샀어요",
          "speaker_id": "1",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004054.1.21",
          "form": "그싼거요 문서 작성용요",
          "original_form": "그싼거요 문서 작성용요",
          "speaker_id": "1",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004054.1.22",
          "form": "용돈 털어서요 ㅎㅎ기특해요",
          "original_form": "용돈 털어서요 ㅎㅎ기특해요",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004054.1.23",
          "form": "노트북 탭도 비싸서 엄두가 안나요",
          "original_form": "노트북 탭도 비싸서 엄두가 안나요",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004054.1.24",
          "form": "삼성 행사해서 싸게 샀어요",
          "original_form": "삼성 행사해서 싸게 샀어요",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004054.1.25",
          "form": "맞아요 오일이 비싸요 \r\n전 그래서 쿠폰이랑 포인트 털어서 사요",
          "original_form": "맞아요 오일이 비싸요 \r\n전 그래서 쿠폰이랑 포인트 털어서 사요",
          "speaker_id": "2",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004054.1.26",
          "form": "저도 맨날 털고 정보 얻어서 사요",
          "original_form": "저도 맨날 털고 정보 얻어서 사요",
          "speaker_id": "1",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004054.1.27",
          "form": "입학, 개학 시기엔 쌀거예요",
          "original_form": "입학, 개학 시기엔 쌀거예요",
          "speaker_id": "2",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004054.1.28",
          "form": "그렇죠 ㅎㅎ",
          "original_form": "그렇죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004054.1.29",
          "form": "안털면 넘 비싸요 ㅜㅜ",
          "original_form": "안털면 넘 비싸요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004054.1.30",
          "form": "그리고 왠지 비싸게 산거 같고요",
          "original_form": "그리고 왠지 비싸게 산거 같고요",
          "speaker_id": "1",
          "time": "20210821 14:58"
        }
      ]
    }
