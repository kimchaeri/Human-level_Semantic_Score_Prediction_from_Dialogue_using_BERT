      "utterance": [
        {
          "id": "MDRW2100009711.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210823 07:39"
        },
        {
          "id": "MDRW2100009711.1.2",
          "form": "아뇨 한번도 안해봤어요",
          "original_form": "아뇨 한번도 안해봤어요",
          "speaker_id": "2",
          "time": "20210823 08:02"
        },
        {
          "id": "MDRW2100009711.1.3",
          "form": "해보셨나요?",
          "original_form": "해보셨나요?",
          "speaker_id": "2",
          "time": "20210823 08:02"
        },
        {
          "id": "MDRW2100009711.1.4",
          "form": "네 당근",
          "original_form": "네 당근",
          "speaker_id": "1",
          "time": "20210823 08:05"
        },
        {
          "id": "MDRW2100009711.1.5",
          "form": "중고거래 자주하세요?",
          "original_form": "중고거래 자주하세요?",
          "speaker_id": "2",
          "time": "20210823 09:57"
        },
        {
          "id": "MDRW2100009711.1.6",
          "form": "가끔씩",
          "original_form": "가끔씩",
          "speaker_id": "1",
          "time": "20210823 09:57"
        },
        {
          "id": "MDRW2100009711.1.7",
          "form": "부지런하시네요",
          "original_form": "부지런하시네요",
          "speaker_id": "2",
          "time": "20210823 09:58"
        },
        {
          "id": "MDRW2100009711.1.8",
          "form": "최근에 구매한 물품은 있으신가요?",
          "original_form": "최근에 구매한 물품은 있으신가요?",
          "speaker_id": "2",
          "time": "20210823 09:59"
        },
        {
          "id": "MDRW2100009711.1.9",
          "form": "팔기만요",
          "original_form": "팔기만요",
          "speaker_id": "1",
          "time": "20210823 09:59"
        },
        {
          "id": "MDRW2100009711.1.10",
          "form": "아 중고거래 말고 구매한 물건 없으신가요?",
          "original_form": "아 중고거래 말고 구매한 물건 없으신가요?",
          "speaker_id": "2",
          "time": "20210823 10:02"
        },
        {
          "id": "MDRW2100009711.1.11",
          "form": "아.. 제습기요",
          "original_form": "아.. 제습기요",
          "speaker_id": "1",
          "time": "20210823 10:02"
        },
        {
          "id": "MDRW2100009711.1.12",
          "form": "습해서",
          "original_form": "습해서",
          "speaker_id": "1",
          "time": "20210823 10:02"
        },
        {
          "id": "MDRW2100009711.1.13",
          "form": "제습기 좋죠 몇년동안 쓰고있는데 습기없애는데는 진짜 좋은 것 같아요",
          "original_form": "제습기 좋죠 몇년동안 쓰고있는데 습기없애는데는 진짜 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210823 10:11"
        },
        {
          "id": "MDRW2100009711.1.14",
          "form": "물통에 물 가득 차 있을 때 버리면 괜히 뿌듯하고",
          "original_form": "물통에 물 가득 차 있을 때 버리면 괜히 뿌듯하고",
          "speaker_id": "2",
          "time": "20210823 10:11"
        },
        {
          "id": "MDRW2100009711.1.15",
          "form": "그런 기분이 들어요ㅎㅍ",
          "original_form": "그런 기분이 들어요ㅎㅍ",
          "speaker_id": "2",
          "time": "20210823 10:12"
        },
        {
          "id": "MDRW2100009711.1.16",
          "form": "근데 틀면 온도가 올라가더라구요",
          "original_form": "근데 틀면 온도가 올라가더라구요",
          "speaker_id": "1",
          "time": "20210823 10:14"
        },
        {
          "id": "MDRW2100009711.1.17",
          "form": "맞아요 그래서 전 제습기사용할때 1-2시간만 예약해서 강하게 빠짝 돌리고 꺼놔요 안그럼 온도가 너무 올라가요",
          "original_form": "맞아요 그래서 전 제습기사용할때 1-2시간만 예약해서 강하게 빠짝 돌리고 꺼놔요 안그럼 온도가 너무 올라가요",
          "speaker_id": "2",
          "time": "20210823 10:24"
        },
