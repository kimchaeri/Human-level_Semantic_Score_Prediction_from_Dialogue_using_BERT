        },
        {
          "id": "MDRW2100039791.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039791.1.3",
          "form": "반려동물 키워본적 있으신가요?",
          "original_form": "반려동물 키워본적 있으신가요?",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039791.1.4",
          "form": "아니요! 하지만 키우고 싶어요",
          "original_form": "아니요! 하지만 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039791.1.5",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039791.1.6",
          "form": "키운다면 어떤 동물 키워보고 싶으세요?",
          "original_form": "키운다면 어떤 동물 키워보고 싶으세요?",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039791.1.7",
          "form": "저는 햄스터요",
          "original_form": "저는 햄스터요",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039791.1.8",
          "form": "친구가 키워서 관심이 생겼어요",
          "original_form": "친구가 키워서 관심이 생겼어요",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039791.1.9",
          "form": "오 햄스터 귀엽죠",
          "original_form": "오 햄스터 귀엽죠",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039791.1.10",
          "form": "저도 어렸을 때 키워본적 있어요",
          "original_form": "저도 어렸을 때 키워본적 있어요",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039791.1.11",
          "form": "털이 어떤 색이었나요",
          "original_form": "털이 어떤 색이었나요",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039791.1.12",
          "form": "완전 어렸을때 키웠어서 기억이 잘 안나는데",
          "original_form": "완전 어렸을때 키웠어서 기억이 잘 안나는데",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039791.1.13",
          "form": "아마 흰색이었을거에요",
          "original_form": "아마 흰색이었을거에요",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039791.1.14",
          "form": "귀여웠겠네요",
          "original_form": "귀여웠겠네요",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039791.1.15",
          "form": "이름은 뭐였나요",
          "original_form": "이름은 뭐였나요",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039791.1.16",
          "form": "이름도 기억이 안나네요 6살인가? 7살쯤 키웠거든요",
          "original_form": "이름도 기억이 안나네요 6살인가? 7살쯤 키웠거든요",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039791.1.17",
          "form": "햄스터는 귀여우니 아마 귀여운 이름이지 않았을까 싶어요!",
          "original_form": "햄스터는 귀여우니 아마 귀여운 이름이지 않았을까 싶어요!",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039791.1.18",
          "form": "다른 반려동물은 키워보신 적 있나요",
          "original_form": "다른 반려동물은 키워보신 적 있나요",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039791.1.19",
          "form": "털 달린 동물은 아닌데",
          "original_form": "털 달린 동물은 아닌데",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039791.1.20",
          "form": "금붕어랑 미꾸라지...를 키워봤어요",
          "original_form": "금붕어랑 미꾸라지...를 키워봤어요",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039791.1.21",
          "form": "ㅋㄴㅋㅋ 미꾸라지를 어쩌다 키우셨나요",
          "original_form": "ㅋㄴㅋㅋ 미꾸라지를 어쩌다 키우셨나요",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039791.1.22",
          "form": "ㅋㅋㅋㅋㅋ 어릴적에",
          "original_form": "ㅋㅋㅋㅋㅋ 어릴적에",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039791.1.23",
          "form": "문화센터에서",
          "original_form": "문화센터에서",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039791.1.24",
          "form": "미꾸라지로 어떤 활동을 했는지는 기억이 진짜 1도 안나는데",
          "original_form": "미꾸라지로 어떤 활동을 했는지는 기억이 진짜 1도 안나는데",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039791.1.25",
          "form": "여튼 받아와서...",
          "original_form": "여튼 받아와서...",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039791.1.26",
          "form": "금붕어를 마침 키워 어항에 같이 키우게 됐죠...",
          "original_form": "금붕어를 마침 키워 어항에 같이 키우게 됐죠...",
          "speaker_id": "1",
          "time": "20210903 21:46"
        }
      ]
