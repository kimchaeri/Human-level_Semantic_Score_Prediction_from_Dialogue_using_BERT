      "utterance": [
        {
          "id": "MDRW2100032625.1.1",
          "form": "아니 나는 이번에 망했어 ㅠ",
          "original_form": "아니 나는 이번에 망했어 ㅠ",
          "speaker_id": "2",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100032625.1.2",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100032625.1.3",
          "form": "전공이 두개나 실시간이야",
          "original_form": "전공이 두개나 실시간이야",
          "speaker_id": "2",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100032625.1.4",
          "form": "설마 1교시야?",
          "original_form": "설마 1교시야?",
          "speaker_id": "1",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100032625.1.5",
          "form": "응 맞아....9시",
          "original_form": "응 맞아....9시",
          "speaker_id": "2",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100032625.1.6",
          "form": "헐 정정기간에 정정할 수 있잖아",
          "original_form": "헐 정정기간에 정정할 수 있잖아",
          "speaker_id": "1",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100032625.1.7",
          "form": "정정할 수 있는데 다른 전공은 지금 수강가능인원이 0명이라 못해ㅠ",
          "original_form": "정정할 수 있는데 다른 전공은 지금 수강가능인원이 0명이라 못해ㅠ",
          "speaker_id": "2",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100032625.1.8",
          "form": "인기많은 과야?",
          "original_form": "인기많은 과야?",
          "speaker_id": "1",
          "time": "20210905 21:38"
        },
        {
          "id": "MDRW2100032625.1.9",
          "form": "응 인기가 너무 많아서 에타 보니깐 못 잡은 사람이 꽤 있더라구",
          "original_form": "응 인기가 너무 많아서 에타 보니깐 못 잡은 사람이 꽤 있더라구",
          "speaker_id": "2",
          "time": "20210905 21:39"
        },
        {
          "id": "MDRW2100032625.1.10",
          "form": "에타가 뭐야?",
          "original_form": "에타가 뭐야?",
          "speaker_id": "1",
          "time": "20210905 21:40"
        },
        {
          "id": "MDRW2100032625.1.11",
          "form": "에타는 같은 대학교에 다니는 사람들끼리 익명으로 학교 정보를 공유하거나 얘기를 자유롭게 하는 곳이야.",
          "original_form": "에타는 같은 대학교에 다니는 사람들끼리 익명으로 학교 정보를 공유하거나 얘기를 자유롭게 하는 곳이야.",
          "speaker_id": "2",
          "time": "20210905 21:41"
        },
        {
          "id": "MDRW2100032625.1.12",
          "form": "오 신기하다",
          "original_form": "오 신기하다",
          "speaker_id": "1",
          "time": "20210905 21:41"
        },
        {
          "id": "MDRW2100032625.1.13",
          "form": "그러면 교양은 몇과목 들어?",
          "original_form": "그러면 교양은 몇과목 들어?",
          "speaker_id": "1",
          "time": "20210905 21:42"
        },
        {
          "id": "MDRW2100032625.1.14",
          "form": "교양은 두 과목!",
          "original_form": "교양은 두 과목!",
          "speaker_id": "2",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032625.1.15",
          "form": "나는 이번에 6전공 하려고",
          "original_form": "나는 이번에 6전공 하려고",
          "speaker_id": "1",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100032625.1.16",
          "form": "헉 6 전공? 그게 가능해?",
          "original_form": "헉 6 전공? 그게 가능해?",
          "speaker_id": "2",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100032625.1.17",
          "form": "엉 비대면일때 최대한 많이 들어 놓는게 좋거든",
          "original_form": "엉 비대면일때 최대한 많이 들어 놓는게 좋거든",
          "speaker_id": "1",
          "time": "20210905 23:16"
        },
        {
          "id": "MDRW2100032625.1.18",
          "form": "다 녹강이야?",
          "original_form": "다 녹강이야?",
          "speaker_id": "2",
          "time": "20210906 11:24"
        },
        {
          "id": "MDRW2100032625.1.19",
          "form": "한과목은 실강 3번 있고 나머지는 다 녹강!",
          "original_form": "한과목은 실강 3번 있고 나머지는 다 녹강!",
          "speaker_id": "1",
          "time": "20210906 12:06"
        },
        {
          "id": "MDRW2100032625.1.20",
          "form": "와 진짜 좋겠다",
          "original_form": "와 진짜 좋겠다",
          "speaker_id": "2",
          "time": "20210906 19:14"
        }
      ]
