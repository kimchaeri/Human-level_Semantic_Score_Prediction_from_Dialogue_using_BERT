      "utterance": [
        {
          "id": "MDRW2100011268.1.1",
          "form": "여기는 천둥번개 안치던데",
          "original_form": "여기는 천둥번개 안치던데",
          "speaker_id": "2",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100011268.1.2",
          "form": "그냥 비랑 바람만 좀 붐",
          "original_form": "그냥 비랑 바람만 좀 붐",
          "speaker_id": "2",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100011268.1.3",
          "form": "원래 이때쯤 태풍이 왔었던것같기도ㅠ",
          "original_form": "원래 이때쯤 태풍이 왔었던것같기도ㅠ",
          "speaker_id": "2",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100011268.1.4",
          "form": "꼭 여름 다 지나갈 즈음에 오더라",
          "original_form": "꼭 여름 다 지나갈 즈음에 오더라",
          "speaker_id": "1",
          "time": "20210824 11:25"
        },
        {
          "id": "MDRW2100011268.1.5",
          "form": "날씨가 우릴 놀리나",
          "original_form": "날씨가 우릴 놀리나",
          "speaker_id": "1",
          "time": "20210824 11:25"
        },
        {
          "id": "MDRW2100011268.1.6",
          "form": "아까보니 온천천 쪽은 또 강이 넘쳤던데...",
          "original_form": "아까보니 온천천 쪽은 또 강이 넘쳤던데...",
          "speaker_id": "1",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100011268.1.7",
          "form": "거기는 툭하면 넘치네ㅠㅠ",
          "original_form": "거기는 툭하면 넘치네ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 11:39"
        },
        {
          "id": "MDRW2100011268.1.8",
          "form": "어떻게 안되는건가",
          "original_form": "어떻게 안되는건가",
          "speaker_id": "2",
          "time": "20210824 11:39"
        },
        {
          "id": "MDRW2100011268.1.9",
          "form": "그러게...",
          "original_form": "그러게...",
          "speaker_id": "1",
          "time": "20210824 11:41"
        },
        {
          "id": "MDRW2100011268.1.10",
          "form": "참, 얼마 전만 해도 사람 녹을듯이 덥더니 날씨 너무 확확 바뀐다",
          "original_form": "참, 얼마 전만 해도 사람 녹을듯이 덥더니 날씨 너무 확확 바뀐다",
          "speaker_id": "1",
          "time": "20210824 11:41"
        },
        {
          "id": "MDRW2100011268.1.11",
          "form": "여름이 점점 더 더워지는 것 같아",
          "original_form": "여름이 점점 더 더워지는 것 같아",
          "speaker_id": "1",
          "time": "20210824 11:42"
        },
        {
          "id": "MDRW2100011268.1.12",
          "form": "맞아유",
          "original_form": "맞아유",
          "speaker_id": "2",
          "time": "20210824 12:11"
        },
        {
          "id": "MDRW2100011268.1.13",
          "form": "에어컨없이 못 살겠어ㅠ",
          "original_form": "에어컨없이 못 살겠어ㅠ",
          "speaker_id": "2",
          "time": "20210824 12:11"
        },
        {
          "id": "MDRW2100011268.1.14",
          "form": "근데 한 번은 날씨 괜찮길래 창문을 열었거든?",
          "original_form": "근데 한 번은 날씨 괜찮길래 창문을 열었거든?",
          "speaker_id": "2",
          "time": "20210824 12:12"
        },
        {
          "id": "MDRW2100011268.1.15",
          "form": "근데 벌레가 불빛보고 막 들어오는거야",
          "original_form": "근데 벌레가 불빛보고 막 들어오는거야",
          "speaker_id": "2",
          "time": "20210824 12:12"
        },
        {
          "id": "MDRW2100011268.1.16",
          "form": "식겁해서 문닫고 다시 에어컨 돌림ㅠ",
          "original_form": "식겁해서 문닫고 다시 에어컨 돌림ㅠ",
          "speaker_id": "2",
          "time": "20210824 12:12"
        },
        {
          "id": "MDRW2100011268.1.17",
          "form": "에어컨 안키고싶어도 벌레가 도와주질않네ㅠ",
          "original_form": "에어컨 안키고싶어도 벌레가 도와주질않네ㅠ",
          "speaker_id": "2",
          "time": "20210824 12:13"
        },
        {
          "id": "MDRW2100011268.1.18",
          "form": "아 벌레ㅠ",
          "original_form": "아 벌레ㅠ",
          "speaker_id": "1",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100011268.1.19",
          "form": "에어컨 없이는 살 수 없는 그런 기후가 되어 버린것같아",
          "original_form": "에어컨 없이는 살 수 없는 그런 기후가 되어 버린것같아",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100011268.1.20",
          "form": "지구온난화 때문이겠지?",
          "original_form": "지구온난화 때문이겠지?",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100011268.1.21",
          "form": "이렇게 비정상적인 여름이라니ㅠ",
          "original_form": "이렇게 비정상적인 여름이라니ㅠ",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100011268.1.22",
          "form": "슬픔ㅠ",
          "original_form": "슬픔ㅠ",
          "speaker_id": "2",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100011268.1.23",
          "form": "어서 가을이나 왔으면 좋겧다",
          "original_form": "어서 가을이나 왔으면 좋겧다",
          "speaker_id": "2",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100011268.1.24",
          "form": "응 어서 가을이나 왔으면 좋겠당ㅋㅋㅋ",
          "original_form": "응 어서 가을이나 왔으면 좋겠당ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100011268.1.25",
          "form": "슬슬 긴팔 옷 꺼내놔야지",
          "original_form": "슬슬 긴팔 옷 꺼내놔야지",
          "speaker_id": "1",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100011268.1.26",
          "form": "근데 가을 진짜 빨리지나감",
          "original_form": "근데 가을 진짜 빨리지나감",
          "speaker_id": "2",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100011268.1.27",
          "form": "프랜치코트 꺼내서 입고 몇 일뒤 패딩꺼냄",
          "original_form": "프랜치코트 꺼내서 입고 몇 일뒤 패딩꺼냄",
          "speaker_id": "2",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100011268.1.28",
          "form": "슬픔ㅜ",
          "original_form": "슬픔ㅜ",
          "speaker_id": "2",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100011268.1.29",
          "form": "봄가을이 너무 짧아진 느낌이야ㅠ",
          "original_form": "봄가을이 너무 짧아진 느낌이야ㅠ",
          "speaker_id": "1",
          "time": "20210824 12:46"
        },
        {
          "id": "MDRW2100011268.1.30",
          "form": "난 가을이 제일 좋은데",
          "original_form": "난 가을이 제일 좋은데",
          "speaker_id": "1",
          "time": "20210824 12:46"
        },
        {
          "id": "MDRW2100011268.1.31",
          "form": "나도ㅠ",
          "original_form": "나도ㅠ",
          "speaker_id": "2",
          "time": "20210824 12:47"
        },
        {
          "id": "MDRW2100011268.1.32",
          "form": "나도 가을이 좋음",
          "original_form": "나도 가을이 좋음",
          "speaker_id": "2",
          "time": "20210824 12:47"
        },
        {
          "id": "MDRW2100011268.1.33",
          "form": "봄은 황사랑 비 많이 내려서 별로임",
          "original_form": "봄은 황사랑 비 많이 내려서 별로임",
          "speaker_id": "2",
          "time": "20210824 12:48"
        },
        {
          "id": "MDRW2100011268.1.34",
          "form": "가을이 선선하고 좋다ㅎㅎ",
          "original_form": "가을이 선선하고 좋다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 12:48"
        },
        {
          "id": "MDRW2100011268.1.35",
          "form": "덥지도 않고 춥지도 않고ㅋㅋ",
          "original_form": "덥지도 않고 춥지도 않고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:48"
        },
        {
          "id": "MDRW2100011268.1.36",
          "form": "가을에 핑크뮬리 필걸?",
          "original_form": "가을에 핑크뮬리 필걸?",
          "speaker_id": "2",
          "time": "20210824 12:49"
        },
        {
          "id": "MDRW2100011268.1.37",
          "form": "이쁨",
          "original_form": "이쁨",
          "speaker_id": "2",
          "time": "20210824 12:49"
        },
        {
          "id": "MDRW2100011268.1.38",
          "form": "가을 전어도 맛있고",
          "original_form": "가을 전어도 맛있고",
          "speaker_id": "2",
          "time": "20210824 12:49"
        },
        {
          "id": "MDRW2100011268.1.39",
          "form": "가을 전어가 집나간 며느리 돌아오게 한다던가?",
          "original_form": "가을 전어가 집나간 며느리 돌아오게 한다던가?",
          "speaker_id": "2",
          "time": "20210824 12:50"
        },
        {
          "id": "MDRW2100011268.1.40",
          "form": "전어맞나?",
          "original_form": "전어맞나?",
          "speaker_id": "2",
          "time": "20210824 12:50"
        },
        {
          "id": "MDRW2100011268.1.41",
          "form": "어어 전어 맞아ㅋㅋ",
          "original_form": "어어 전어 맞아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:50"
        }
      ]
    }
