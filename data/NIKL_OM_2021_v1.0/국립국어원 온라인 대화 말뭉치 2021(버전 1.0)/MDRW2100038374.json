      "utterance": [
        {
          "id": "MDRW2100038374.1.1",
          "form": "주로 뭘 보시나요?",
          "original_form": "주로 뭘 보시나요?",
          "speaker_id": "1",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100038374.1.2",
          "form": "오 저는 영드 일드 미드같은 외국 드라마 많이봐요 ㅎ",
          "original_form": "오 저는 영드 일드 미드같은 외국 드라마 많이봐요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100038374.1.3",
          "form": "오 옛날 일드 많이 봤었는데",
          "original_form": "오 옛날 일드 많이 봤었는데",
          "speaker_id": "1",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100038374.1.4",
          "form": "최근 외국꺼는 브리저튼 봤어요",
          "original_form": "최근 외국꺼는 브리저튼 봤어요",
          "speaker_id": "1",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100038374.1.5",
          "form": "오 그건 장르가 뭔가요?",
          "original_form": "오 그건 장르가 뭔가요?",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100038374.1.6",
          "form": "로맨스일까요 ㅎㄹ",
          "original_form": "로맨스일까요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100038374.1.7",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100038374.1.8",
          "form": "여자들 옷이 너무 예뻣어요",
          "original_form": "여자들 옷이 너무 예뻣어요",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100038374.1.9",
          "form": "앗 영드는 좀 그런거 보는 재미가있어요 ㅎㅎ",
          "original_form": "앗 영드는 좀 그런거 보는 재미가있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100038374.1.10",
          "form": "요즘 중드도 많이 보더라구요",
          "original_form": "요즘 중드도 많이 보더라구요",
          "speaker_id": "1",
          "time": "20210902 12:45"
        },
        {
          "id": "MDRW2100038374.1.11",
          "form": "볼만은 했는데 집중을 못해서 끊은게 많아요 ㅜㅜㅋ",
          "original_form": "볼만은 했는데 집중을 못해서 끊은게 많아요 ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210902 12:45"
        },
        {
          "id": "MDRW2100038374.1.12",
          "form": "전 일드중에 고독한 미식가아세요?",
          "original_form": "전 일드중에 고독한 미식가아세요?",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100038374.1.13",
          "form": "헐 그거 완전 잼죠",
          "original_form": "헐 그거 완전 잼죠",
          "speaker_id": "1",
          "time": "20210902 13:16"
        },
        {
          "id": "MDRW2100038374.1.14",
          "form": "아저씨 완전 잘드심",
          "original_form": "아저씨 완전 잘드심",
          "speaker_id": "1",
          "time": "20210902 13:16"
        },
        {
          "id": "MDRW2100038374.1.15",
          "form": "거기 한번씩 다가보고싶어오",
          "original_form": "거기 한번씩 다가보고싶어오",
          "speaker_id": "1",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100038374.1.16",
          "form": "저도요ㅜ",
          "original_form": "저도요ㅜ",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100038374.1.17",
          "form": "뭔가 일본이 뭐 먹는드라마는 진짜 잘만드는거같아요 ㅋㅋ",
          "original_form": "뭔가 일본이 뭐 먹는드라마는 진짜 잘만드는거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100038374.1.18",
          "form": "근데 심야식당? 그거는 흥미가 안생기더라구요",
          "original_form": "근데 심야식당? 그거는 흥미가 안생기더라구요",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100038374.1.19",
          "form": "아하 저도요",
          "original_form": "아하 저도요",
          "speaker_id": "2",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100038374.1.20",
          "form": "그식당 있는동네가 좀 우울한 동네그라그런가",
          "original_form": "그식당 있는동네가 좀 우울한 동네그라그런가",
          "speaker_id": "2",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100038374.1.21",
          "form": "혹시 웨이브보세요?",
          "original_form": "혹시 웨이브보세요?",
          "speaker_id": "2",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100038374.1.22",
          "form": "웨이브 라이브티비볼때 자주봐요ㅎㄹ",
          "original_form": "웨이브 라이브티비볼때 자주봐요ㅎㄹ",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100038374.1.23",
          "form": "웨이브에서 얼마전에 무슨 초밥왕 일드봤는데",
          "original_form": "웨이브에서 얼마전에 무슨 초밥왕 일드봤는데",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100038374.1.24",
          "form": "스토리는 걍대충보고 초밥나오는부분만 일시정지 시키고 그랬어요 ㅋㅋ",
          "original_form": "스토리는 걍대충보고 초밥나오는부분만 일시정지 시키고 그랬어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100038374.1.25",
          "form": "초밥왕 듣기만해도",
          "original_form": "초밥왕 듣기만해도",
          "speaker_id": "1",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100038374.1.26",
          "form": "맛있겠어요 ㅋㅋㅋㅋ",
          "original_form": "맛있겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:34"
        }
      ]
