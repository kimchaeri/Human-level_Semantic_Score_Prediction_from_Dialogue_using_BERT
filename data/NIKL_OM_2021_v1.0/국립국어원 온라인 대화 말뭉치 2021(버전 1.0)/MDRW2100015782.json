      "utterance": [
        {
          "id": "MDRW2100015782.1.1",
          "form": "왜 아직 안 자?",
          "original_form": "왜 아직 안 자?",
          "speaker_id": "1",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015782.1.2",
          "form": "요새 잠이 잘 안 와",
          "original_form": "요새 잠이 잘 안 와",
          "speaker_id": "2",
          "time": "20210826 01:02"
        },
        {
          "id": "MDRW2100015782.1.3",
          "form": "불면증인가",
          "original_form": "불면증인가",
          "speaker_id": "2",
          "time": "20210826 01:02"
        },
        {
          "id": "MDRW2100015782.1.4",
          "form": "아이구 그러면 어째",
          "original_form": "아이구 그러면 어째",
          "speaker_id": "1",
          "time": "20210826 01:02"
        },
        {
          "id": "MDRW2100015782.1.5",
          "form": "불면증엔 따듯한 차가 좋대",
          "original_form": "불면증엔 따듯한 차가 좋대",
          "speaker_id": "1",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015782.1.6",
          "form": "차... 보이차 마셔봐야겠다",
          "original_form": "차... 보이차 마셔봐야겠다",
          "speaker_id": "2",
          "time": "20210826 01:06"
        },
        {
          "id": "MDRW2100015782.1.7",
          "form": "잠울 잘 못자서 그런가 피부도 안 좋아졌어",
          "original_form": "잠울 잘 못자서 그런가 피부도 안 좋아졌어",
          "speaker_id": "2",
          "time": "20210826 01:06"
        },
        {
          "id": "MDRW2100015782.1.8",
          "form": "여드름 나고 그래?",
          "original_form": "여드름 나고 그래?",
          "speaker_id": "1",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015782.1.9",
          "form": "자꾸 피부에 손 대지마. 더 심해져.",
          "original_form": "자꾸 피부에 손 대지마. 더 심해져.",
          "speaker_id": "1",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015782.1.10",
          "form": "엉 자꾸 뾰루지 나네ㅠ",
          "original_form": "엉 자꾸 뾰루지 나네ㅠ",
          "speaker_id": "2",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015782.1.11",
          "form": "어제 마스크팩도 했는데 안 가라 앉아ㅠㅠ",
          "original_form": "어제 마스크팩도 했는데 안 가라 앉아ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 01:08"
        },
        {
          "id": "MDRW2100015782.1.12",
          "form": "생활습관을 건강하게 바꿔야 몸도 건강해지지.",
          "original_form": "생활습관을 건강하게 바꿔야 몸도 건강해지지.",
          "speaker_id": "1",
          "time": "20210826 01:08"
        },
        {
          "id": "MDRW2100015782.1.13",
          "form": "일찍 자고 일찍 일어나고.",
          "original_form": "일찍 자고 일찍 일어나고.",
          "speaker_id": "1",
          "time": "20210826 01:09"
        },
        {
          "id": "MDRW2100015782.1.14",
          "form": "ㅋㅋㅋ알겠엉",
          "original_form": "ㅋㅋㅋ알겠엉",
          "speaker_id": "2",
          "time": "20210826 01:09"
        },
        {
          "id": "MDRW2100015782.1.15",
          "form": "엄마는 오늘 사이클 좀 탔어?",
          "original_form": "엄마는 오늘 사이클 좀 탔어?",
          "speaker_id": "2",
          "time": "20210826 01:09"
        },
        {
          "id": "MDRW2100015782.1.16",
          "form": "응 티비 보면서 한 시간 탔어.",
          "original_form": "응 티비 보면서 한 시간 탔어.",
          "speaker_id": "1",
          "time": "20210826 01:09"
        },
        {
          "id": "MDRW2100015782.1.17",
          "form": "이것도 오래하면 엉덩이 아파.",
          "original_form": "이것도 오래하면 엉덩이 아파.",
          "speaker_id": "1",
          "time": "20210826 01:09"
        },
        {
          "id": "MDRW2100015782.1.18",
          "form": "맞아 그러더라! 수건 같은 거 깔고 해",
          "original_form": "맞아 그러더라! 수건 같은 거 깔고 해",
          "speaker_id": "2",
          "time": "20210826 01:10"
        },
        {
          "id": "MDRW2100015782.1.19",
          "form": "넘 빨리 타면 무릎에 안 좋으니까 무리하지 말고",
          "original_form": "넘 빨리 타면 무릎에 안 좋으니까 무리하지 말고",
          "speaker_id": "2",
          "time": "20210826 01:11"
        },
        {
          "id": "MDRW2100015782.1.20",
          "form": "살빼려고 하는 건데 싸이클 타고 나면 더 배고파.",
          "original_form": "살빼려고 하는 건데 싸이클 타고 나면 더 배고파.",
          "speaker_id": "1",
          "time": "20210826 01:12"
        },
        {
          "id": "MDRW2100015782.1.21",
          "form": "참아야지 어떡해ㅠ",
          "original_form": "참아야지 어떡해ㅠ",
          "speaker_id": "2",
          "time": "20210826 01:12"
        },
        {
          "id": "MDRW2100015782.1.22",
          "form": "다이어트는 운동 10 식이 90이랬어",
          "original_form": "다이어트는 운동 10 식이 90이랬어",
          "speaker_id": "2",
          "time": "20210826 01:12"
        },
        {
          "id": "MDRW2100015782.1.23",
          "form": "알겠어. 너는 살 뺀다고 무리하지 말고 운동하면서 밥 잘 챙겨 먹어.",
          "original_form": "알겠어. 너는 살 뺀다고 무리하지 말고 운동하면서 밥 잘 챙겨 먹어.",
          "speaker_id": "1",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015782.1.24",
          "form": "오키!! 내일 운동하고 후기 알려줄게ㅋㅋㅋ",
          "original_form": "오키!! 내일 운동하고 후기 알려줄게ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:14"
        }
      ]
    }
  ]
