      "utterance": [
        {
          "id": "MDRW2100009848.1.1",
          "form": "치킨도 가족과 같이 먹어야 행복이에요",
          "original_form": "치킨도 가족과 같이 먹어야 행복이에요",
          "speaker_id": "2",
          "time": "20210823 10:57"
        },
        {
          "id": "MDRW2100009848.1.2",
          "form": "형제가있나요",
          "original_form": "형제가있나요",
          "speaker_id": "1",
          "time": "20210823 10:57"
        },
        {
          "id": "MDRW2100009848.1.3",
          "form": "전 혼자입니다ㅠㅜㄴ",
          "original_form": "전 혼자입니다ㅠㅜㄴ",
          "speaker_id": "2",
          "time": "20210823 10:57"
        },
        {
          "id": "MDRW2100009848.1.4",
          "form": "있으신가요?",
          "original_form": "있으신가요?",
          "speaker_id": "2",
          "time": "20210823 10:58"
        },
        {
          "id": "MDRW2100009848.1.5",
          "form": "외동이시구나",
          "original_form": "외동이시구나",
          "speaker_id": "1",
          "time": "20210823 10:58"
        },
        {
          "id": "MDRW2100009848.1.6",
          "form": "저는 남동생있어요",
          "original_form": "저는 남동생있어요",
          "speaker_id": "1",
          "time": "20210823 10:58"
        },
        {
          "id": "MDRW2100009848.1.7",
          "form": "외동은 어떤가요",
          "original_form": "외동은 어떤가요",
          "speaker_id": "1",
          "time": "20210823 10:58"
        },
        {
          "id": "MDRW2100009848.1.8",
          "form": "많이 싸우시나요?",
          "original_form": "많이 싸우시나요?",
          "speaker_id": "2",
          "time": "20210823 10:58"
        },
        {
          "id": "MDRW2100009848.1.9",
          "form": "외동은.. 좋습니다ㅋㅋ",
          "original_form": "외동은.. 좋습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:58"
        },
        {
          "id": "MDRW2100009848.1.10",
          "form": "적응 잘 하면ㄱㄲ",
          "original_form": "적응 잘 하면ㄱㄲ",
          "speaker_id": "2",
          "time": "20210823 10:58"
        },
        {
          "id": "MDRW2100009848.1.11",
          "form": "네 둘 중 하나가 사라지기 직전까지 싸워요",
          "original_form": "네 둘 중 하나가 사라지기 직전까지 싸워요",
          "speaker_id": "1",
          "time": "20210823 10:58"
        },
        {
          "id": "MDRW2100009848.1.12",
          "form": "음 외동이어서 힘든점은 없나요",
          "original_form": "음 외동이어서 힘든점은 없나요",
          "speaker_id": "1",
          "time": "20210823 10:59"
        },
        {
          "id": "MDRW2100009848.1.13",
          "form": "ㅋㅋ 사라지기 직전ㅋㅋ",
          "original_form": "ㅋㅋ 사라지기 직전ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:59"
        },
        {
          "id": "MDRW2100009848.1.14",
          "form": "가족중 누가 크게 아플때 걱정이  크지요ㅠ",
          "original_form": "가족중 누가 크게 아플때 걱정이  크지요ㅠ",
          "speaker_id": "2",
          "time": "20210823 10:59"
        },
        {
          "id": "MDRW2100009848.1.15",
          "form": "아.. 그러네요",
          "original_form": "아.. 그러네요",
          "speaker_id": "1",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009848.1.16",
          "form": "슬픔을 못나눠서ㅠ",
          "original_form": "슬픔을 못나눠서ㅠ",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009848.1.17",
          "form": "그것도 그렇고 혼자면 케어도 힘들고 하니까",
          "original_form": "그것도 그렇고 혼자면 케어도 힘들고 하니까",
          "speaker_id": "1",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009848.1.18",
          "form": "맞아요.",
          "original_form": "맞아요.",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009848.1.19",
          "form": "생각지 못했던 부분이에요",
          "original_form": "생각지 못했던 부분이에요",
          "speaker_id": "1",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009848.1.20",
          "form": "하지만 일상이 평화로운 편이에요",
          "original_form": "하지만 일상이 평화로운 편이에요",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009848.1.21",
          "form": "그것 부럽다",
          "original_form": "그것 부럽다",
          "speaker_id": "1",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009848.1.22",
          "form": "저희는 항상 시끄럽거든요..ㅋㅋ",
          "original_form": "저희는 항상 시끄럽거든요..ㅋㅋ",
