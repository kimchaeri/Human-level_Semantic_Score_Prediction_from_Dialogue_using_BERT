      "utterance": [
        {
          "id": "MDRW2100043316.1.1",
          "form": "운동 해요?",
          "original_form": "운동 해요?",
          "speaker_id": "2",
          "time": "20210907 17:36"
        },
        {
          "id": "MDRW2100043316.1.2",
          "form": "저 다이어트중이에요",
          "original_form": "저 다이어트중이에요",
          "speaker_id": "1",
          "time": "20210907 17:37"
        },
        {
          "id": "MDRW2100043316.1.3",
          "form": "아하 다른 운동 하는거 잇어요?",
          "original_form": "아하 다른 운동 하는거 잇어요?",
          "speaker_id": "2",
          "time": "20210907 17:37"
        },
        {
          "id": "MDRW2100043316.1.4",
          "form": "버피테스트 30개...정도만ㅋㅋㅋ",
          "original_form": "버피테스트 30개...정도만ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100043316.1.5",
          "form": "홈트요",
          "original_form": "홈트요",
          "speaker_id": "1",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100043316.1.6",
          "form": "우와 너뮤 대단하시다",
          "original_form": "우와 너뮤 대단하시다",
          "speaker_id": "2",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100043316.1.7",
          "form": "저는 그냥 산책만 해요 ㅋㅋ",
          "original_form": "저는 그냥 산책만 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100043316.1.8",
          "form": "아 댄스레슨도 받아요",
          "original_form": "아 댄스레슨도 받아요",
          "speaker_id": "2",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100043316.1.9",
          "form": "댄스레슨은 뭐하는거에요?",
          "original_form": "댄스레슨은 뭐하는거에요?",
          "speaker_id": "1",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100043316.1.10",
          "form": "아 아이돌 댄스 가르쳐 주는 레슨이에요",
          "original_form": "아 아이돌 댄스 가르쳐 주는 레슨이에요",
          "speaker_id": "2",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100043316.1.11",
          "form": "댄스 학원에서 2대 1로 받아요",
          "original_form": "댄스 학원에서 2대 1로 받아요",
          "speaker_id": "2",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100043316.1.12",
          "form": "우와!!신기해요",
          "original_form": "우와!!신기해요",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100043316.1.13",
          "form": "그럼 되게 운동되겠네요",
          "original_form": "그럼 되게 운동되겠네요",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100043316.1.14",
          "form": "네네 맞아요 재밌어요",
          "original_form": "네네 맞아요 재밌어요",
          "speaker_id": "2",
          "time": "20210907 17:43"
        },
        {
          "id": "MDRW2100043316.1.15",
          "form": "노래 크게 틀고 스트레칭만 해도 좋아요",
          "original_form": "노래 크게 틀고 스트레칭만 해도 좋아요",
          "speaker_id": "2",
          "time": "20210907 17:43"
        },
        {
          "id": "MDRW2100043316.1.16",
          "form": "저도 운동할때 노래틀고해요",
          "original_form": "저도 운동할때 노래틀고해요",
          "speaker_id": "1",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100043316.1.17",
          "form": "근데 너무 힘들면 노래가 안들려여",
          "original_form": "근데 너무 힘들면 노래가 안들려여",
          "speaker_id": "1",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100043316.1.18",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100043316.1.19",
          "form": "힘들면 빠른 노래도 느려져요",
          "original_form": "힘들면 빠른 노래도 느려져요",
          "speaker_id": "2",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100043316.1.20",
          "form": "전 그래서 운동할때 락음악들어요",
          "original_form": "전 그래서 운동할때 락음악들어요",
