      "utterance": [
        {
          "id": "MDRW2100015725.1.1",
          "form": "우왕 또?",
          "original_form": "우왕 또?",
          "speaker_id": "2",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015725.1.2",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015725.1.3",
          "form": "맛있었어?ㅋㅋㅋㅋ",
          "original_form": "맛있었어?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015725.1.4",
          "form": "아니 너무 맛없었어..",
          "original_form": "아니 너무 맛없었어..",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015725.1.5",
          "form": "엥 왜??",
          "original_form": "엥 왜??",
          "speaker_id": "2",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015725.1.6",
          "form": "이상한 맛이 나고 심지어 간장까지 맛없었어",
          "original_form": "이상한 맛이 나고 심지어 간장까지 맛없었어",
          "speaker_id": "1",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015725.1.7",
          "form": "??상한 거 아냐?",
          "original_form": "??상한 거 아냐?",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015725.1.8",
          "form": "상한건 아닌데 그냥 맛이 이상해",
          "original_form": "상한건 아닌데 그냥 맛이 이상해",
          "speaker_id": "1",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015725.1.9",
          "form": "간장은 또 왜",
          "original_form": "간장은 또 왜",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015725.1.10",
          "form": "몰라.. 리뷰 이벤트 참여해서 받은 새우튀김이 제일 맛있었어",
          "original_form": "몰라.. 리뷰 이벤트 참여해서 받은 새우튀김이 제일 맛있었어",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015725.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ저런 주객전도네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ저런 주객전도네",
          "speaker_id": "2",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015725.1.12",
          "form": "맞아..",
          "original_form": "맞아..",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015725.1.13",
          "form": "리뷰는 어떻게 썼어?",
          "original_form": "리뷰는 어떻게 썼어?",
          "speaker_id": "2",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015725.1.14",
          "form": "어쩔수 없이 별 5개 줬는데 그때 다른 사람들이 쓴것도 봤는데 진작 볼걸그랬어",
          "original_form": "어쩔수 없이 별 5개 줬는데 그때 다른 사람들이 쓴것도 봤는데 진작 볼걸그랬어",
          "speaker_id": "1",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015725.1.15",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015725.1.16",
          "form": "담부터는 리뷰 꼼꼼하게 확인행",
          "original_form": "담부터는 리뷰 꼼꼼하게 확인행",
          "speaker_id": "2",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015725.1.17",
          "form": "그래야겟어 다들 이상한 맛 난다고 했더라고",
          "original_form": "그래야겟어 다들 이상한 맛 난다고 했더라고",
          "speaker_id": "1",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015725.1.18",
          "form": "또 뭐 먹었어?",
          "original_form": "또 뭐 먹었어?",
          "speaker_id": "2",
          "time": "20210826 01:01"
        },
        {
          "id": "MDRW2100015725.1.19",
          "form": "난 명란아보카도 덮밥 먹었어",
          "original_form": "난 명란아보카도 덮밥 먹었어",
          "speaker_id": "2",
          "time": "20210826 01:01"
        },
        {
          "id": "MDRW2100015725.1.20",
          "form": "장국도 왔는데 장국도 별로였어",
          "original_form": "장국도 왔는데 장국도 별로였어",
          "speaker_id": "1",
          "time": "20210826 01:01"
        },
        {
          "id": "MDRW2100015725.1.21",
          "form": "장국은 또 왴ㅋㅋㅋㅋ",
          "original_form": "장국은 또 왴ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:08"
        },
        {
          "id": "MDRW2100015725.1.22",
          "form": "장국에서도 이상한맛이 나더라고..",
          "original_form": "장국에서도 이상한맛이 나더라고..",
          "speaker_id": "1",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100015725.1.23",
          "form": "무서워서 이제 배달 못시키겠다",
          "original_form": "무서워서 이제 배달 못시키겠다",
          "speaker_id": "1",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100015725.1.24",
          "form": "에이 맛있었던 연어맛집은 있었어?",
          "original_form": "에이 맛있었던 연어맛집은 있었어?",
          "speaker_id": "2",
          "time": "20210826 02:56"
        },
        {
          "id": "MDRW2100015725.1.25",
          "form": "당연히 있었지",
          "original_form": "당연히 있었지",
          "speaker_id": "1",
          "time": "20210826 02:57"
        },
        {
          "id": "MDRW2100015725.1.26",
          "form": "나는 겐코가 맛있더라",
          "original_form": "나는 겐코가 맛있더라",
          "speaker_id": "1",
          "time": "20210826 02:57"
        },
        {
          "id": "MDRW2100015725.1.27",
          "form": "겐코?",
          "original_form": "겐코?",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100015725.1.28",
          "form": "어디있는데야?",
          "original_form": "어디있는데야?",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100015725.1.29",
