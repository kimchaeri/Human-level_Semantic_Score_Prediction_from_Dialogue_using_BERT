      "utterance": [
        {
          "id": "MDRW2100006576.1.1",
          "form": "경기 좋아하세뇨?",
          "original_form": "경기 좋아하세뇨?",
          "speaker_id": "1",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100006576.1.2",
          "form": "여자배구 보셨어요?",
          "original_form": "여자배구 보셨어요?",
          "speaker_id": "2",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100006576.1.3",
          "form": "아 ㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100006576.1.4",
          "form": "여자배구 !",
          "original_form": "여자배구 !",
          "speaker_id": "2",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100006576.1.5",
          "form": "아뇨.. 전안좋아함",
          "original_form": "아뇨.. 전안좋아함",
          "speaker_id": "1",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100006576.1.6",
          "form": "이번 도쿄 올림픽때 진짜",
          "original_form": "이번 도쿄 올림픽때 진짜",
          "speaker_id": "2",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100006576.1.7",
          "form": "양궁은 유명파던데",
          "original_form": "양궁은 유명파던데",
          "speaker_id": "1",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006576.1.8",
          "form": "제 치킨의 베스트 프렌드",
          "original_form": "제 치킨의 베스트 프렌드",
          "speaker_id": "2",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006576.1.9",
          "form": "양궁도 좋아해요",
          "original_form": "양궁도 좋아해요",
          "speaker_id": "2",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006576.1.10",
          "form": "안산인가?",
          "original_form": "안산인가?",
          "speaker_id": "1",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006576.1.11",
          "form": "안산선수",
          "original_form": "안산선수",
          "speaker_id": "2",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006576.1.12",
          "form": "심박수 보세요~~",
          "original_form": "심박수 보세요~~",
          "speaker_id": "2",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006576.1.13",
          "form": "3금메달 멋져요",
          "original_form": "3금메달 멋져요",
          "speaker_id": "1",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006576.1.14",
          "form": "신기해요 저보다 낮음 ㅋㅋ",
          "original_form": "신기해요 저보다 낮음 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006576.1.15",
          "form": "여기가 집 안방 인가요~",
          "original_form": "여기가 집 안방 인가요~",
          "speaker_id": "2",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006576.1.16",
          "form": "제가 더 무섭",
          "original_form": "제가 더 무섭",
          "speaker_id": "1",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006576.1.17",
          "form": "딸 수 있는거 다 딴거래요",
          "original_form": "딸 수 있는거 다 딴거래요",
          "speaker_id": "2",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006576.1.18",
          "form": "진짜 미쳤지 않나요?",
          "original_form": "진짜 미쳤지 않나요?",
          "speaker_id": "2",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006576.1.19",
          "form": "와 간지나는 인터뷰다",
          "original_form": "와 간지나는 인터뷰다",
          "speaker_id": "1",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006576.1.20",
