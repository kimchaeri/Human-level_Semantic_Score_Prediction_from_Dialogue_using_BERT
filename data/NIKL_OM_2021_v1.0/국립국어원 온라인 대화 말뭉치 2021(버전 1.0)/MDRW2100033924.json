      "utterance": [
        {
          "id": "MDRW2100033924.1.1",
          "form": "나도ㅠㅠ",
          "original_form": "나도ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100033924.1.2",
          "form": "벌써 2달 반이 지났어",
          "original_form": "벌써 2달 반이 지났어",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100033924.1.3",
          "form": "말이 돼?",
          "original_form": "말이 돼?",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100033924.1.4",
          "form": "아 그러네 실화야...",
          "original_form": "아 그러네 실화야...",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100033924.1.5",
          "form": "방학에 뭐했어?",
          "original_form": "방학에 뭐했어?",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100033924.1.6",
          "form": "소모임하고 운전면허 따고",
          "original_form": "소모임하고 운전면허 따고",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100033924.1.7",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100033924.1.8",
          "form": "나는 운동했어",
          "original_form": "나는 운동했어",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100033924.1.9",
          "form": "학기 시작하면 많이 못 하니까",
          "original_form": "학기 시작하면 많이 못 하니까",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100033924.1.10",
          "form": "이번에 교양 과목 몇 개 들어?",
          "original_form": "이번에 교양 과목 몇 개 들어?",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100033924.1.11",
          "form": "4개 정도",
          "original_form": "4개 정도",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100033924.1.12",
          "form": "아 교양 2개",
          "original_form": "아 교양 2개",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100033924.1.13",
          "form": "나는 1개만 들어",
          "original_form": "나는 1개만 들어",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100033924.1.14",
          "form": "대면 수업이 좋아 비대면이 나아?",
          "original_form": "대면 수업이 좋아 비대면이 나아?",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100033924.1.15",
          "form": "나는 그래도 대면",
          "original_form": "나는 그래도 대면",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100033924.1.16",
          "form": "근데 코로나 진정되기 전까지는 그냥 비대면이 나아",
          "original_form": "근데 코로나 진정되기 전까지는 그냥 비대면이 나아",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100033924.1.17",
          "form": "나도... 너무 위험하잖아",
          "original_form": "나도... 너무 위험하잖아",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100033924.1.18",
          "form": "이번 학기 비대면이야?",
          "original_form": "이번 학기 비대면이야?",
          "speaker_id": "1",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100033924.1.19",
          "form": "섞여 있어",
          "original_form": "섞여 있어",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100033924.1.20",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100033924.1.21",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100033924.1.22",
          "form": "3개 정도는 대면",
          "original_form": "3개 정도는 대면",
          "speaker_id": "1",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100033924.1.23",
          "form": "나도 그정도 하는 거 같아",
          "original_form": "나도 그정도 하는 거 같아",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100033924.1.24",
          "form": "이번 학기도 파이팅",
          "original_form": "이번 학기도 파이팅",
          "speaker_id": "1",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100033924.1.25",
          "form": "너도 파이팅",
          "original_form": "너도 파이팅",
          "speaker_id": "2",
          "time": "20210825 15:56"
        }
      ]
