      "utterance": [
        {
          "id": "MDRW2100022577.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ사실 큐크다스 잘 안사요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ사실 큐크다스 잘 안사요",
          "speaker_id": "2",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022577.1.2",
          "form": "원래 그런거에요~",
          "original_form": "원래 그런거에요~",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022577.1.3",
          "form": "요기요 써요 어플?",
          "original_form": "요기요 써요 어플?",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022577.1.4",
          "form": "저는 과자중에 화이트하임을 구매해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 과자중에 화이트하임을 구매해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022577.1.5",
          "form": "저는 배민 이용해요",
          "original_form": "저는 배민 이용해요",
          "speaker_id": "2",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022577.1.6",
          "form": "아 배민이요 ㅎㅎ",
          "original_form": "아 배민이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022577.1.7",
          "form": "요기요에서 파리바게트 할인하는 거 알아요?",
          "original_form": "요기요에서 파리바게트 할인하는 거 알아요?",
          "speaker_id": "1",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022577.1.8",
          "form": "네네 ㅋㅋㅋㅋㅋ한번 사먹엇어요",
          "original_form": "네네 ㅋㅋㅋㅋㅋ한번 사먹엇어요",
          "speaker_id": "2",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022577.1.9",
          "form": "크 만원 이상 오천원 할인 대박이에요",
          "original_form": "크 만원 이상 오천원 할인 대박이에요",
          "speaker_id": "1",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022577.1.10",
          "form": "완전 저렴해지더라구용",
          "original_form": "완전 저렴해지더라구용",
          "speaker_id": "2",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022577.1.11",
          "form": "ㅋㅋㅋ그쵸 가보니까 포장 엄청 밀려있더라구요",
          "original_form": "ㅋㅋㅋ그쵸 가보니까 포장 엄청 밀려있더라구요",
          "speaker_id": "1",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022577.1.12",
          "form": "그거 오늘이 마지막이래요 ㅜㅜ",
          "original_form": "그거 오늘이 마지막이래요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022577.1.13",
          "form": "어쩌다 딱 만원담앗는데",
          "original_form": "어쩌다 딱 만원담앗는데",
          "speaker_id": "2",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022577.1.14",
          "form": "할인율최고",
          "original_form": "할인율최고",
          "speaker_id": "2",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022577.1.15",
          "form": "엇 저도 딱 만원 채웠어요 ㅋㅋ",
          "original_form": "엇 저도 딱 만원 채웠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022577.1.16",
          "form": "뭐뭐 사셨어요?",
          "original_form": "뭐뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022577.1.17",
          "form": "헐 그래요?? 오늘 또 구매하러가야겟는데..",
          "original_form": "헐 그래요?? 오늘 또 구매하러가야겟는데..",
          "speaker_id": "2",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022577.1.18",
          "form": "이미 다 쓸어갔을 것 같긴한데 고민이에요 저도",
          "original_form": "이미 다 쓸어갔을 것 같긴한데 고민이에요 저도",
          "speaker_id": "1",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022577.1.19",
          "form": "낼 점심으로 먹기 딱인데 파바 빵..",
          "original_form": "낼 점심으로 먹기 딱인데 파바 빵..",
          "speaker_id": "1",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022577.1.20",
          "form": "저는 식빵이랑 도넛류 샀어요!",
          "original_form": "저는 식빵이랑 도넛류 샀어요!",
          "speaker_id": "2",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022577.1.21",
          "form": "생각보다 도넛류 맛잇어서 구매햇어요 ㅋㅋㅋ",
          "original_form": "생각보다 도넛류 맛잇어서 구매햇어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022577.1.22",
          "form": "오 ㅋㅋ 저도 비슷한데 저는 치즈케잌이랑 팔도빵, 도넛 하나요",
          "original_form": "오 ㅋㅋ 저도 비슷한데 저는 치즈케잌이랑 팔도빵, 도넛 하나요",
          "speaker_id": "1",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022577.1.23",
          "form": "그쵸? ㅋㅋ 도넛 의외였어요 던킨 뺨치던데요",
          "original_form": "그쵸? ㅋㅋ 도넛 의외였어요 던킨 뺨치던데요",
          "speaker_id": "1",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022577.1.24",
          "form": "사먹어보셧군요 ㅋㅋㅋㅋ",
          "original_form": "사먹어보셧군요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022577.1.25",
          "form": "네 ㅎㅎ 도넛 하니까 하나 떠오르는 게 있네요",
          "original_form": "네 ㅎㅎ 도넛 하니까 하나 떠오르는 게 있네요",
          "speaker_id": "1",
          "time": "20210829 15:54"
        }
      ]
