      "utterance": [
        {
          "id": "MDRW2100027774.1.1",
          "form": "근래에 뭐 사신거 있으세요?",
          "original_form": "근래에 뭐 사신거 있으세요?",
          "speaker_id": "1",
          "time": "20210902 14:56"
        },
        {
          "id": "MDRW2100027774.1.2",
          "form": "저는 향수를 샀어요!",
          "original_form": "저는 향수를 샀어요!",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027774.1.3",
          "form": "가을 겨울 대비 향수를 구입했습니다 ㅠ너무신나요",
          "original_form": "가을 겨울 대비 향수를 구입했습니다 ㅠ너무신나요",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027774.1.4",
          "form": "구입한 물품 있으신가요?!",
          "original_form": "구입한 물품 있으신가요?!",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027774.1.5",
          "form": "전 자켓을 샀어요.",
          "original_form": "전 자켓을 샀어요.",
          "speaker_id": "1",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027774.1.6",
          "form": "슬슬 취업준비도 해야하고...",
          "original_form": "슬슬 취업준비도 해야하고...",
          "speaker_id": "1",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027774.1.7",
          "form": "이건  핑계지만요 ㅋㅋ",
          "original_form": "이건  핑계지만요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027774.1.8",
          "form": "그렇군요 하지만 자켓은 유행타지않고 꾸준히 입을 수 있으니까요!",
          "original_form": "그렇군요 하지만 자켓은 유행타지않고 꾸준히 입을 수 있으니까요!",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027774.1.9",
          "form": "좋은 선택이죠!",
          "original_form": "좋은 선택이죠!",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027774.1.10",
          "form": "취업준비는 정말 힘들죠 이럴때 잘먹고 건강해야 정신도 건강한것같아요",
          "original_form": "취업준비는 정말 힘들죠 이럴때 잘먹고 건강해야 정신도 건강한것같아요",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027774.1.11",
          "form": "중고거래 해보신적 있나요?",
          "original_form": "중고거래 해보신적 있나요?",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027774.1.12",
          "form": "옛날에 바자회로 중고거래를 한적은 있는데요",
          "original_form": "옛날에 바자회로 중고거래를 한적은 있는데요",
          "speaker_id": "1",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027774.1.13",
          "form": "온라인카페 통해서는 안해봤어요",
          "original_form": "온라인카페 통해서는 안해봤어요",
          "speaker_id": "1",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027774.1.14",
          "form": "그렇군요!",
          "original_form": "그렇군요!",
          "speaker_id": "2",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100027774.1.15",
          "form": "저는 당근마켓 구매자로 이용하기는 하는데 팔아본적은 없어요 ㅎㅎ",
          "original_form": "저는 당근마켓 구매자로 이용하기는 하는데 팔아본적은 없어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100027774.1.16",
          "form": "장바구니에 넣어놓은 물건있나요?",
          "original_form": "장바구니에 넣어놓은 물건있나요?",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100027774.1.17",
          "form": "에어프라이어 각을 재고 있긴해요",
          "original_form": "에어프라이어 각을 재고 있긴해요",
          "speaker_id": "1",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100027774.1.18",
          "form": "근데 잘쓸까 싶기도하고 생각보다 크기도 하고",
          "original_form": "근데 잘쓸까 싶기도하고 생각보다 크기도 하고",
          "speaker_id": "1",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100027774.1.19",
          "form": "사면 냉동만 먹게되는거 아닐까 싶기도하고",
          "original_form": "사면 냉동만 먹게되는거 아닐까 싶기도하고",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100027774.1.20",
          "form": "에어프라이어 너무 좋죠!",
          "original_form": "에어프라이어 너무 좋죠!",
          "speaker_id": "2",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100027774.1.21",
          "form": "냉동으로 하기 너무 간편해서 그럴 수 있지만",
          "original_form": "냉동으로 하기 너무 간편해서 그럴 수 있지만",
          "speaker_id": "2",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100027774.1.22",
          "form": "쿠키도 만들고 빵도굽고 여러가지 활용도가 높은 것 같아요 ㅎㅎ",
          "original_form": "쿠키도 만들고 빵도굽고 여러가지 활용도가 높은 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100027774.1.23",
          "form": "하... 정말 끌리기는 하는데",
          "original_form": "하... 정말 끌리기는 하는데",
          "speaker_id": "1",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100027774.1.24",
          "form": "사놓고 몇번만 쓰고 안쓸까봐 고민중이에요",
          "original_form": "사놓고 몇번만 쓰고 안쓸까봐 고민중이에요",
          "speaker_id": "1",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100027774.1.25",
          "form": "작은게 아니라 그럴것같아요 ㅜ",
          "original_form": "작은게 아니라 그럴것같아요 ㅜ",
          "speaker_id": "2",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100027774.1.26",
          "form": "가걱대는 어느정도 생각중이신가요?",
          "original_form": "가걱대는 어느정도 생각중이신가요?",
          "speaker_id": "2",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100027774.1.27",
          "form": "쌀수록 좋긴하지만",
          "original_form": "쌀수록 좋긴하지만",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100027774.1.28",
          "form": "엥간하면 스뎅으로 된게 좋다고해서 그쪽으로 생각하고 있어요",
          "original_form": "엥간하면 스뎅으로 된게 좋다고해서 그쪽으로 생각하고 있어요",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100027774.1.29",
          "form": "맞아요 그리고 에어프라리어에 넣는 실리콘사시면 씻기도 편하고 좋아요!",
          "original_form": "맞아요 그리고 에어프라리어에 넣는 실리콘사시면 씻기도 편하고 좋아요!",
          "speaker_id": "2",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100027774.1.30",
          "form": "앗, 많이 쓰시나봐요!",
          "original_form": "앗, 많이 쓰시나봐요!",
          "speaker_id": "1",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100027774.1.31",
          "form": "네! 전 정말 편리해서 잘 쓰게 되더라구요 ㅎㅎ",
          "original_form": "네! 전 정말 편리해서 잘 쓰게 되더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100027774.1.32",
          "form": "예전에 실리콘 없이 쓸때 씻기 힘들었는데 실리콘있으니까",
          "original_form": "예전에 실리콘 없이 쓸때 씻기 힘들었는데 실리콘있으니까",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100027774.1.33",
          "form": "더 잘쓰게 되더라구요 ㅎㅎ",
          "original_form": "더 잘쓰게 되더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:30"
        }
      ]
