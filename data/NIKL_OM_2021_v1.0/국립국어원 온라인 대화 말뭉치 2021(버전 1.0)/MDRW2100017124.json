        {
          "id": "MDRW2100017124.1.1",
          "form": "새우튀김 좋아해요",
          "original_form": "새우튀김 좋아해요",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017124.1.2",
          "form": "새우튀김 맛있어요",
          "original_form": "새우튀김 맛있어요",
          "speaker_id": "1",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017124.1.3",
          "form": "근데 비싸서 자주 못먹어요 ㅠㅠ",
          "original_form": "근데 비싸서 자주 못먹어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017124.1.4",
          "form": "맞아요 아무래도 단가가비싸니까",
          "original_form": "맞아요 아무래도 단가가비싸니까",
          "speaker_id": "1",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017124.1.5",
          "form": "후우... 왕새우 튀김 너무 비싸다...",
          "original_form": "후우... 왕새우 튀김 너무 비싸다...",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017124.1.6",
          "form": "통새우튀김 좋아하세요",
          "original_form": "통새우튀김 좋아하세요",
          "speaker_id": "1",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017124.1.7",
          "form": "10개에 17000원?",
          "original_form": "10개에 17000원?",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017124.1.8",
          "form": "좋아하죠",
          "original_form": "좋아하죠",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017124.1.9",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017124.1.10",
          "form": "어디서 그렇게 비싸게 파나요",
          "original_form": "어디서 그렇게 비싸게 파나요",
          "speaker_id": "1",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017124.1.11",
          "form": "제 주변에 또 새우튀김 맛집이 있어서",
          "original_form": "제 주변에 또 새우튀김 맛집이 있어서",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017124.1.12",
          "form": "맛집이라서 비싼가?",
          "original_form": "맛집이라서 비싼가?",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017124.1.13",
          "form": "아 그런가봐요ㅋㅋㅋ",
          "original_form": "아 그런가봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017124.1.14",
          "form": "아 국물떡볶이랑 세트예요",
          "original_form": "아 국물떡볶이랑 세트예요",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017124.1.15",
          "form": "17000원이면 거의 치킨값인데",
          "original_form": "17000원이면 거의 치킨값인데",
          "speaker_id": "1",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017124.1.16",
          "form": "그쵸...",
