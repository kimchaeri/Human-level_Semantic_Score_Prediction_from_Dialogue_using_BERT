      "utterance": [
        {
          "id": "MDRW2100016982.1.1",
          "form": "나도 만들어줘",
          "original_form": "나도 만들어줘",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100016982.1.2",
          "form": "난 1인분밖에 안해",
          "original_form": "난 1인분밖에 안해",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100016982.1.3",
          "form": "계량을 못해서",
          "original_form": "계량을 못해서",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100016982.1.4",
          "form": "1인분 두 번 해",
          "original_form": "1인분 두 번 해",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100016982.1.5",
          "form": "토끼는 뭐해",
          "original_form": "토끼는 뭐해",
          "speaker_id": "1",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100016982.1.6",
          "form": "자요",
          "original_form": "자요",
          "speaker_id": "2",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100016982.1.7",
          "form": "자는 토끼 귀여워",
          "original_form": "자는 토끼 귀여워",
          "speaker_id": "1",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100016982.1.8",
          "form": "눈뜨고자다가",
          "original_form": "눈뜨고자다가",
          "speaker_id": "2",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100016982.1.9",
          "form": "엄마가 간식주네",
          "original_form": "엄마가 간식주네",
          "speaker_id": "2",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100016982.1.10",
          "form": "토끼 간식 뭐 먹어",
          "original_form": "토끼 간식 뭐 먹어",
          "speaker_id": "1",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100016982.1.11",
          "form": "알약같은거",
          "original_form": "알약같은거",
          "speaker_id": "2",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100016982.1.12",
          "form": "알약이 간식이야?",
          "original_form": "알약이 간식이야?",
          "speaker_id": "1",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100016982.1.13",
          "form": "불쌍해",
          "original_form": "불쌍해",
          "speaker_id": "1",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100016982.1.14",
          "form": "사과즙으로 만든 알약인디",
          "original_form": "사과즙으로 만든 알약인디",
          "speaker_id": "2",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100016982.1.15",
          "form": "그건 맛있겠다",
          "original_form": "그건 맛있겠다",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100016982.1.16",
          "form": "사과즙 간식",
          "original_form": "사과즙 간식",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100016982.1.17",
          "form": "귀여운 토끼는 간식 다 먹었어?",
          "original_form": "귀여운 토끼는 간식 다 먹었어?",
