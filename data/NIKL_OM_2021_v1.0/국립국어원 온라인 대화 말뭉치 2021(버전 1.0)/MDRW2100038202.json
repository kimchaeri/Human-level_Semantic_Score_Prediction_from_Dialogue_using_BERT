      "utterance": [
        {
          "id": "MDRW2100038202.1.1",
          "form": "나 낚시 튜토만 했는대",
          "original_form": "나 낚시 튜토만 했는대",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100038202.1.2",
          "form": "관상어 난이도 무엇...",
          "original_form": "관상어 난이도 무엇...",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100038202.1.3",
          "form": "분명 이쯤하면 물고기가 잡혀야하는데 안잡혀 ㅋㅋㅋㅋ",
          "original_form": "분명 이쯤하면 물고기가 잡혀야하는데 안잡혀 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100038202.1.4",
          "form": "대체 뭘 시키길래",
          "original_form": "대체 뭘 시키길래",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100038202.1.5",
          "form": "난 4성 회ㅣ복밥이나",
          "original_form": "난 4성 회ㅣ복밥이나",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100038202.1.6",
          "form": "주면",
          "original_form": "주면",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100038202.1.7",
          "form": "좋겠어",
          "original_form": "좋겠어",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100038202.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ ㅁㅈㅁㅈ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ ㅁㅈㅁㅈ",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100038202.1.9",
          "form": "이제 슬슬 3성은 좀 안차",
          "original_form": "이제 슬슬 3성은 좀 안차",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100038202.1.10",
          "form": "ㅇㅇ 포만도때문에 3성 먹어도 돌아서면 또 먹어야함",
          "original_form": "ㅇㅇ 포만도때문에 3성 먹어도 돌아서면 또 먹어야함",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100038202.1.11",
          "form": "ㅇㅇ....그래서 이제",
          "original_form": "ㅇㅇ....그래서 이제",
          "speaker_id": "1",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100038202.1.12",
          "form": "한번 빨리 죽였다 살려서",
          "original_form": "한번 빨리 죽였다 살려서",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100038202.1.13",
          "form": "포만도 리셋도 시키고",
          "original_form": "포만도 리셋도 시키고",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100038202.1.14",
          "form": "살리는 음식 쿨타임동안 다시 포만도 들고 버티고",
          "original_form": "살리는 음식 쿨타임동안 다시 포만도 들고 버티고",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100038202.1.15",
          "form": "그러고있어....",
          "original_form": "그러고있어....",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100038202.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 갈수록 꼼수만 늘어나는 유저들..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 갈수록 꼼수만 늘어나는 유저들..",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100038202.1.17",
          "form": "죽었다 살아나면",
          "original_form": "죽었다 살아나면",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100038202.1.18",
          "form": "배가 꺼지나봐.....",
          "original_form": "배가 꺼지나봐.....",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100038202.1.19",
          "form": "스킬 쿨타임도 다시 채워야하잖아 ㅎ",
          "original_form": "스킬 쿨타임도 다시 채워야하잖아 ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100038202.1.20",
          "form": "다 쓰고 죽어야지 뭐",
          "original_form": "다 쓰고 죽어야지 뭐",
          "speaker_id": "1",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100038202.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100038202.1.22",
          "form": "이제부터",
          "original_form": "이제부터",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100038202.1.23",
          "form": "성실하게 출첵을 하는 유저가 되기로 햇어",
          "original_form": "성실하게 출첵을 하는 유저가 되기로 햇어",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100038202.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038202.1.25",
          "form": "출첵 언제부터 한대?",
          "original_form": "출첵 언제부터 한대?",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038202.1.26",
          "form": "홈페이지 출석은 오늘부터",
          "original_form": "홈페이지 출석은 오늘부터",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038202.1.27",
          "form": "이것저것 좀 주는 출석은 이달 말부터인듯?",
          "original_form": "이것저것 좀 주는 출석은 이달 말부터인듯?",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038202.1.28",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038202.1.29",
          "form": "그거는 진짜",
          "original_form": "그거는 진짜",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038202.1.30",
          "form": "손 놓게 되더라",
          "original_form": "손 놓게 되더라",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038202.1.31",
          "form": "ㅇㅇ 나도 지난달에 완전 까먹어서 ㅋㅋㅋ",
          "original_form": "ㅇㅇ 나도 지난달에 완전 까먹어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038202.1.32",
          "form": "열심히 계산을 해봣믄데",
          "original_form": "열심히 계산을 해봣믄데",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100038202.1.33",
          "form": "일단 소가 2.3에 복각된다고 가정하면",
          "original_form": "일단 소가 2.3에 복각된다고 가정하면",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100038202.1.34",
          "form": "대충 최소 60일에서 최대 80일의 여유기간이 생기고",
          "original_form": "대충 최소 60일에서 최대 80일의 여유기간이 생기고",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100038202.1.35",
          "form": "일퀘 원석이 하루에 20개니까 60일하면 1200개",
          "original_form": "일퀘 원석이 하루에 20개니까 60일하면 1200개",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100038202.1.36",
          "form": "ㄴㄴ일퀘 원석",
          "original_form": "ㄴㄴ일퀘 원석",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100038202.1.37",
          "form": "중간에 2.2 업데이트 하면 점검보상 300개",
          "original_form": "중간에 2.2 업데이트 하면 점검보상 300개",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100038202.1.38",
          "form": "일퀘 하나에 10원석",
          "original_form": "일퀘 하나에 10원석",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100038202.1.39",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100038202.1.40",
          "form": "4개 다하면 20원석 추가",
          "original_form": "4개 다하면 20원석 추가",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100038202.1.41",
          "form": "총 60이지",
          "original_form": "총 60이지",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100038202.1.42",
          "form": "그럼 최소 3600개",
          "original_form": "그럼 최소 3600개",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100038202.1.43",
          "form": "2.2 점검보상+오류수정 총 400이라 치고",
          "original_form": "2.2 점검보상+오류수정 총 400이라 치고",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100038202.1.44",
          "form": "이벤트같은거 뭐 안하려나",
          "original_form": "이벤트같은거 뭐 안하려나",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100038202.1.45",
          "form": "원석주는거",
          "original_form": "원석주는거",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100038202.1.46",
          "form": "출석보상으로 10연=1600원석어치 주고",
          "original_form": "출석보상으로 10연=1600원석어치 주고",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100038202.1.47",
          "form": "매달 상점교환 가능한 뽑기권이 5개니까 10월&11월 두달간 10연=1600원석어치 교환가능",
          "original_form": "매달 상점교환 가능한 뽑기권이 5개니까 10월&11월 두달간 10연=1600원석어치 교환가능",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100038202.1.48",
          "form": "그럼 대략 일퀘 3600+업뎃보상400+이벤트 출석1600+상점교환 1600 하니까",
          "original_form": "그럼 대략 일퀘 3600+업뎃보상400+이벤트 출석1600+상점교환 1600 하니까",
          "speaker_id": "2",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100038202.1.49",
          "form": "총 7200원석은 그닥 노력없이 얻을 수 잇어",
          "original_form": "총 7200원석은 그닥 노력없이 얻을 수 잇어",
