      "utterance": [
        {
          "id": "MDRW2100001929.1.1",
          "form": "큰일이네.. 그냥 지나가기엔 큰 행사인데",
          "original_form": "큰일이네.. 그냥 지나가기엔 큰 행사인데",
          "speaker_id": "1",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001929.1.2",
          "form": "모여서 여행도 못가고 음식도 못먹으니 할 수 있는게 없어 ㅠ",
          "original_form": "모여서 여행도 못가고 음식도 못먹으니 할 수 있는게 없어 ㅠ",
          "speaker_id": "2",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001929.1.3",
          "form": "용돈이라도 두둑하게 드려야지..",
          "original_form": "용돈이라도 두둑하게 드려야지..",
          "speaker_id": "1",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001929.1.4",
          "form": "용돈은 드리는데 그냥.. 마음이 안좋네",
          "original_form": "용돈은 드리는데 그냥.. 마음이 안좋네",
          "speaker_id": "2",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001929.1.5",
          "form": "지금 상황이 어쩔 수 없으니 이해하실거야..",
          "original_form": "지금 상황이 어쩔 수 없으니 이해하실거야..",
          "speaker_id": "1",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001929.1.6",
          "form": "엄마아빠 두 분 다 칠순이 올해인데 이렇게 허무하게 보내다니.. 슬퍼",
          "original_form": "엄마아빠 두 분 다 칠순이 올해인데 이렇게 허무하게 보내다니.. 슬퍼",
          "speaker_id": "2",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001929.1.7",
          "form": "코로나 좋아지면 여행이라도 가자",
          "original_form": "코로나 좋아지면 여행이라도 가자",
          "speaker_id": "1",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001929.1.8",
          "form": "웅.. 그래야겠어 조촐한 가족모임이라도",
          "original_form": "웅.. 그래야겠어 조촐한 가족모임이라도",
          "speaker_id": "2",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001929.1.9",
          "form": "가족사진 찍고 싶어하지 않으셨어?",
          "original_form": "가족사진 찍고 싶어하지 않으셨어?",
          "speaker_id": "1",
          "time": "20210819 22:21"
        },
        {
          "id": "MDRW2100001929.1.10",
          "form": "맞아.. 칠순에 같은 옷 입고 사진찍고 싶다고 하셨는데",
          "original_form": "맞아.. 칠순에 같은 옷 입고 사진찍고 싶다고 하셨는데",
          "speaker_id": "2",
          "time": "20210819 22:21"
        },
        {
          "id": "MDRW2100001929.1.11",
          "form": "사진관이라도 알아볼까봐",
          "original_form": "사진관이라도 알아볼까봐",
          "speaker_id": "1",
          "time": "20210819 22:21"
        },
        {
          "id": "MDRW2100001929.1.12",
          "form": "진짜 그래야하나..",
          "original_form": "진짜 그래야하나..",
          "speaker_id": "2",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001929.1.13",
          "form": "뭐라도 정성을 보여야지",
          "original_form": "뭐라도 정성을 보여야지",
          "speaker_id": "1",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001929.1.14",
          "form": "용돈만 드리기도 좀 그렇고...",
          "original_form": "용돈만 드리기도 좀 그렇고...",
          "speaker_id": "1",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001929.1.15",
          "form": "고민 좀 더 해봐야겠어",
          "original_form": "고민 좀 더 해봐야겠어",
