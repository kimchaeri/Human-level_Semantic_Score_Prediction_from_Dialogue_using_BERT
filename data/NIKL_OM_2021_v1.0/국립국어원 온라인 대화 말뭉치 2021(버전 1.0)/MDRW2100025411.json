      "utterance": [
        {
          "id": "MDRW2100025411.1.1",
          "form": "드라마 좋아해요",
          "original_form": "드라마 좋아해요",
          "speaker_id": "2",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025411.1.2",
          "form": "근데 보통 다시보기로 해서 봐요",
          "original_form": "근데 보통 다시보기로 해서 봐요",
          "speaker_id": "2",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025411.1.3",
          "form": "ㅎㅎ 저도 티비로 드라마 본지 오래된것 같네요",
          "original_form": "ㅎㅎ 저도 티비로 드라마 본지 오래된것 같네요",
          "speaker_id": "1",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025411.1.4",
          "form": "애초에 채널권이 저한테 없는지라 ㅋㅋ",
          "original_form": "애초에 채널권이 저한테 없는지라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025411.1.5",
          "form": "앜ㅋㅋㅋㅋ왜요",
          "original_form": "앜ㅋㅋㅋㅋ왜요",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025411.1.6",
          "form": "요즘 왓챠 넷플 웨이브 티빙",
          "original_form": "요즘 왓챠 넷플 웨이브 티빙",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025411.1.7",
          "form": "다시보기 많잖아요",
          "original_form": "다시보기 많잖아요",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025411.1.8",
          "form": "전 티빙있어요ㅋㅋ",
          "original_form": "전 티빙있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100025411.1.9",
          "form": "하.. 웨이브랑 티빙이랑 통합됐으면",
          "original_form": "하.. 웨이브랑 티빙이랑 통합됐으면",
          "speaker_id": "1",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100025411.1.10",
          "form": "진심.....",
          "original_form": "진심.....",
          "speaker_id": "2",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025411.1.11",
          "form": "넷플이랑 디즈니에 대응하려면",
          "original_form": "넷플이랑 디즈니에 대응하려면",
          "speaker_id": "2",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025411.1.12",
          "form": "우리나라끼리는 뭉치라고요....",
          "original_form": "우리나라끼리는 뭉치라고요....",
          "speaker_id": "2",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025411.1.13",
          "form": "그리고 원래 jtbc가 웨이브에있었거든요??근데",
          "original_form": "그리고 원래 jtbc가 웨이브에있었거든요??근데",
          "speaker_id": "2",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025411.1.14",
          "form": "언제부턴가 jtbc가 티빙으로",
          "original_form": "언제부턴가 jtbc가 티빙으로",
          "speaker_id": "2",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025411.1.15",
          "form": "옮겼더라고요....ㅋㅋㅋㅋ",
          "original_form": "옮겼더라고요....ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025411.1.16",
          "form": "그래서 결국 다 보네요...",
          "original_form": "그래서 결국 다 보네요...",
          "speaker_id": "2",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025411.1.17",
          "form": "지상파는 웨이브 그외는 티빙인거 같아요",
          "original_form": "지상파는 웨이브 그외는 티빙인거 같아요",
          "speaker_id": "1",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025411.1.18",
          "form": "그나마 다행인건",
          "original_form": "그나마 다행인건",
          "speaker_id": "2",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025411.1.19",
          "form": "전 skt써서 웨이브 공짜예요ㅎㅎ",
          "original_form": "전 skt써서 웨이브 공짜예요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025411.1.20",
          "form": "우와 그래요?",
          "original_form": "우와 그래요?",
          "speaker_id": "1",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025411.1.21",
          "form": "전 lg인데 디즈니플러스랑 협약했대서 기다리고있는데",
          "original_form": "전 lg인데 디즈니플러스랑 협약했대서 기다리고있는데",
          "speaker_id": "1",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025411.1.22",
          "form": "오질 않네요...ㅎ",
          "original_form": "오질 않네요...ㅎ",
          "speaker_id": "1",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025411.1.23",
          "form": "디즈니 올해 오픈할거같던데요!",
          "original_form": "디즈니 올해 오픈할거같던데요!",
          "speaker_id": "2",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025411.1.24",
          "form": "사이트도 만들고!",
          "original_form": "사이트도 만들고!",
          "speaker_id": "2",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025411.1.25",
          "form": "부럽네여 그게더ㅜㅜ",
          "original_form": "부럽네여 그게더ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025411.1.26",
          "form": "디즈니 플러스 원래 올해 어린이날에 오픈한댔는데ㅠㅠ",
          "original_form": "디즈니 플러스 원래 올해 어린이날에 오픈한댔는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025411.1.27",
          "form": "이러다 내년에 오픈할듯요",
          "original_form": "이러다 내년에 오픈할듯요",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025411.1.28",
          "form": "12월을 기대해봐요~~",
          "original_form": "12월을 기대해봐요~~",
          "speaker_id": "2",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100025411.1.29",
          "form": "디즈니에도 드라마쪽 들어갈거라했는데.... 그것도",
          "original_form": "디즈니에도 드라마쪽 들어갈거라했는데.... 그것도",
          "speaker_id": "2",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100025411.1.30",
          "form": "결제할듯.....",
          "original_form": "결제할듯.....",
          "speaker_id": "2",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100025411.1.31",
          "form": "미드도 좋아해서",
          "original_form": "미드도 좋아해서",
          "speaker_id": "2",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100025411.1.32",
          "form": "아, 저는 미드가 취향이 아니라 넷플도 구독하다 말았어요.",
          "original_form": "아, 저는 미드가 취향이 아니라 넷플도 구독하다 말았어요.",
          "speaker_id": "1",
          "time": "20210901 12:04"
        },
        {
          "id": "MDRW2100025411.1.33",
          "form": "한국에 너무 길들여졌어... ㅎㅎ",
          "original_form": "한국에 너무 길들여졌어... ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 12:04"
        },
        {
          "id": "MDRW2100025411.1.34",
          "form": "재밌게 본 미드가 있나요?",
          "original_form": "재밌게 본 미드가 있나요?",
          "speaker_id": "1",
          "time": "20210901 14:45"
        },
        {
          "id": "MDRW2100025411.1.35",
          "form": "저는 미드보다 영드를 더 보는데",
          "original_form": "저는 미드보다 영드를 더 보는데",
          "speaker_id": "2",
          "time": "20210901 15:50"
        },
        {
          "id": "MDRW2100025411.1.36",
          "form": "미드는 그레이아나토미",
          "original_form": "미드는 그레이아나토미",
          "speaker_id": "2",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025411.1.37",
          "form": "제일 좋아해요!",
          "original_form": "제일 좋아해요!",
          "speaker_id": "2",
          "time": "20210901 15:51"
        }
      ]
