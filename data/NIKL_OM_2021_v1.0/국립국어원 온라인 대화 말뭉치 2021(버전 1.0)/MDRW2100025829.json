      "utterance": [
        {
          "id": "MDRW2100025829.1.1",
          "form": "학생일때는 돌돌 말아서 모자에 집어넣었는데",
          "original_form": "학생일때는 돌돌 말아서 모자에 집어넣었는데",
          "speaker_id": "1",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025829.1.2",
          "form": "집에서 그렇게 보관할 수는 없었죠...ㅋㅋ",
          "original_form": "집에서 그렇게 보관할 수는 없었죠...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025829.1.3",
          "form": "전 의자에 걸어놨던거 같아요",
          "original_form": "전 의자에 걸어놨던거 같아요",
          "speaker_id": "2",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025829.1.4",
          "form": "저는 의자에 걸면 의자가 힘들어하더라구요",
          "original_form": "저는 의자에 걸면 의자가 힘들어하더라구요",
          "speaker_id": "1",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025829.1.5",
          "form": "그렇긴해요",
          "original_form": "그렇긴해요",
          "speaker_id": "2",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025829.1.6",
          "form": "의자가 낡은 건지 롱패딩이 무거운건지...",
          "original_form": "의자가 낡은 건지 롱패딩이 무거운건지...",
          "speaker_id": "1",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025829.1.7",
          "form": "근데 옷걸이가 더 힘들어해서...ㅎ",
          "original_form": "근데 옷걸이가 더 힘들어해서...ㅎ",
          "speaker_id": "2",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025829.1.8",
          "form": "그리고 세탁도 좀 힘들고..ㅠ",
          "original_form": "그리고 세탁도 좀 힘들고..ㅠ",
          "speaker_id": "2",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100025829.1.9",
          "form": "맞아요 얇은 옷걸이는 버티지도 못하죠",
          "original_form": "맞아요 얇은 옷걸이는 버티지도 못하죠",
          "speaker_id": "1",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100025829.1.10",
          "form": "세탁은 세탁소에 맡기고 있어요",
          "original_form": "세탁은 세탁소에 맡기고 있어요",
          "speaker_id": "1",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025829.1.11",
          "form": "아, 전 물빨래해야하는건 제가해야 속 편하달까",
          "original_form": "아, 전 물빨래해야하는건 제가해야 속 편하달까",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025829.1.12",
          "form": "물빨래 하기에는 너무 힘들거 같아서 저는 포기했어요ㅠ",
          "original_form": "물빨래 하기에는 너무 힘들거 같아서 저는 포기했어요ㅠ",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025829.1.13",
          "form": "힘들긴 해요",
          "original_form": "힘들긴 해요",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025829.1.14",
          "form": "세탁자체는 세탁기가 하는데",
          "original_form": "세탁자체는 세탁기가 하는데",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025829.1.15",
          "form": "그거 말리고 솜 부풀리는게 힘들죠",
          "original_form": "그거 말리고 솜 부풀리는게 힘들죠",
          "speaker_id": "2",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100025829.1.16",
          "form": "물빨래 하고 걸어놓을 데가 애매할거 같아요",
          "original_form": "물빨래 하고 걸어놓을 데가 애매할거 같아요",
          "speaker_id": "1",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100025829.1.17",
          "form": "전 쇼파가 방수라",
          "original_form": "전 쇼파가 방수라",
          "speaker_id": "2",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025829.1.18",
          "form": "거기 등받이에 엎어놔요",
          "original_form": "거기 등받이에 엎어놔요",
          "speaker_id": "2",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025829.1.19",
          "form": "방수 쇼파 말만 들어도 든든하네요",
          "original_form": "방수 쇼파 말만 들어도 든든하네요",
          "speaker_id": "1",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025829.1.20",
          "form": "네, 거기다가 이불도 널기도 하고 그래요 ㅋㅋ",
          "original_form": "네, 거기다가 이불도 널기도 하고 그래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:27"
        }
      ]
