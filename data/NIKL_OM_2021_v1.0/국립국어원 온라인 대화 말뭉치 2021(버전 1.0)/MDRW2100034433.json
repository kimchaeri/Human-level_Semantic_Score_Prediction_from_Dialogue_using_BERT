      "utterance": [
        {
          "id": "MDRW2100034433.1.1",
          "form": "점심 드셨나요",
          "original_form": "점심 드셨나요",
          "speaker_id": "2",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100034433.1.2",
          "form": "지금 먹었어요!",
          "original_form": "지금 먹었어요!",
          "speaker_id": "1",
          "time": "20210828 13:01"
        },
        {
          "id": "MDRW2100034433.1.3",
          "form": "메뉴는요?^^",
          "original_form": "메뉴는요?^^",
          "speaker_id": "2",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100034433.1.4",
          "form": "떡볶이 먹고 빈대떡 먹으러 왔어요!",
          "original_form": "떡볶이 먹고 빈대떡 먹으러 왔어요!",
          "speaker_id": "1",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100034433.1.5",
          "form": "와 외식인가요? 맛나겠어요",
          "original_form": "와 외식인가요? 맛나겠어요",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100034433.1.6",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100034433.1.7",
          "form": "달리고 있어요ㅋㅋ",
          "original_form": "달리고 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100034433.1.8",
          "form": "저는 밀키트로 간단히 먹었어요",
          "original_form": "저는 밀키트로 간단히 먹었어요",
          "speaker_id": "2",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100034433.1.9",
          "form": "밀키트는 어떤거에요?",
          "original_form": "밀키트는 어떤거에요?",
          "speaker_id": "1",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100034433.1.10",
          "form": "아 재료.양념 계량 손질되어있으면 그냥 조리만 간단히 하는거요",
          "original_form": "아 재료.양념 계량 손질되어있으면 그냥 조리만 간단히 하는거요",
          "speaker_id": "2",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100034433.1.11",
          "form": "오 요즘엔 그런 제품이 많은가봐요",
          "original_form": "오 요즘엔 그런 제품이 많은가봐요",
          "speaker_id": "1",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100034433.1.12",
          "form": "네 밀키트 이용 안해보셨나봐요~^^",
          "original_form": "네 밀키트 이용 안해보셨나봐요~^^",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034433.1.13",
          "form": "네 안먹어봐서 몰랐넹ᆢ",
          "original_form": "네 안먹어봐서 몰랐넹ᆢ",
          "speaker_id": "1",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100034433.1.14",
          "form": "네요",
          "original_form": "네요",
          "speaker_id": "1",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100034433.1.15",
          "form": "저녁은 된장국을 끓였어요",
          "original_form": "저녁은 된장국을 끓였어요",
          "speaker_id": "2",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100034433.1.16",
          "form": "우와 된장국 맛있겠어요~",
          "original_form": "우와 된장국 맛있겠어요~",
          "speaker_id": "1",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100034433.1.17",
          "form": "느끼한걸 먹었더니 깔끔한게 먹고싶네요",
          "original_form": "느끼한걸 먹었더니 깔끔한게 먹고싶네요",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034433.1.18",
          "form": "그죠 그럴땐 김치찌개같은게 당기죠",
          "original_form": "그죠 그럴땐 김치찌개같은게 당기죠",
          "speaker_id": "2",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034433.1.19",
          "form": "아 더 먹고싶어지네요",
          "original_form": "아 더 먹고싶어지네요",
