        },
        {
          "id": "MDRW2100013107.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 12:33"
        },
        {
          "id": "MDRW2100013107.1.3",
          "form": "점심식사는 하셨나요?",
          "original_form": "점심식사는 하셨나요?",
          "speaker_id": "2",
          "time": "20210825 12:33"
        },
        {
          "id": "MDRW2100013107.1.4",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210825 12:39"
        },
        {
          "id": "MDRW2100013107.1.5",
          "form": "치킨좋아하세요?",
          "original_form": "치킨좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 12:39"
        },
        {
          "id": "MDRW2100013107.1.6",
          "form": "네 좋아합니다",
          "original_form": "네 좋아합니다",
          "speaker_id": "2",
          "time": "20210825 12:42"
        },
        {
          "id": "MDRW2100013107.1.7",
          "form": "오늘 친구랑 먹을건데 맛있는 치킨이 있으면 추천해주세요!",
          "original_form": "오늘 친구랑 먹을건데 맛있는 치킨이 있으면 추천해주세요!",
          "speaker_id": "2",
          "time": "20210825 12:43"
        },
        {
          "id": "MDRW2100013107.1.8",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210825 12:50"
        },
        {
          "id": "MDRW2100013107.1.9",
          "form": "뿌링클 어떠세요?",
          "original_form": "뿌링클 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 12:50"
        },
        {
          "id": "MDRW2100013107.1.10",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "2",
          "time": "20210825 12:53"
        },
        {
          "id": "MDRW2100013107.1.11",
          "form": "저녁은 어떤거 드실 예정이신가요?",
          "original_form": "저녁은 어떤거 드실 예정이신가요?",
          "speaker_id": "2",
          "time": "20210825 12:54"
        },
        {
          "id": "MDRW2100013107.1.12",
          "form": "저녁은 아직이요ㅋㅋ",
          "original_form": "저녁은 아직이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:00"
        },
        {
          "id": "MDRW2100013107.1.13",
          "form": "떡볶이는 좋아하세요?",
          "original_form": "떡볶이는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 13:00"
        },
        {
          "id": "MDRW2100013107.1.14",
          "form": "떡볶이 좋아합니다",
          "original_form": "떡볶이 좋아합니다",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013107.1.15",
          "form": "요즘 로제가 맛있더라구요",
          "original_form": "요즘 로제가 맛있더라구요",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013107.1.16",
          "form": "오우 맞아요",
          "original_form": "오우 맞아요",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100013107.1.17",
          "form": "배떡드셔보셨어요?",
          "original_form": "배떡드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100013107.1.18",
          "form": "네 먹어봤습니다!",
          "original_form": "네 먹어봤습니다!",
          "speaker_id": "2",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013107.1.19",
          "form": "맛있더라구요",
          "original_form": "맛있더라구요",
          "speaker_id": "2",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013107.1.20",
          "form": "오 떡군이네는",
          "original_form": "오 떡군이네는",
          "speaker_id": "1",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013107.1.21",
          "form": "드셔보셨나요?",
          "original_form": "드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013107.1.22",
          "form": "아 거기가 원조죠?",
          "original_form": "아 거기가 원조죠?",
          "speaker_id": "2",
          "time": "20210825 13:47"
        },
        {
          "id": "MDRW2100013107.1.23",
          "form": "아니요 아직 못먹어봤습니다",
          "original_form": "아니요 아직 못먹어봤습니다",
          "speaker_id": "2",
          "time": "20210825 13:47"
        },
        {
          "id": "MDRW2100013107.1.24",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "2",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100013107.1.25",
          "form": "ㅋㅋㅋㅋ존맛이예요",
          "original_form": "ㅋㅋㅋㅋ존맛이예요",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013107.1.26",
          "form": "스파게티면 추가필수요",
          "original_form": "스파게티면 추가필수요",
          "speaker_id": "1",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013107.1.27",
          "form": "배떡은 스파게티면추가가없더라고요",
          "original_form": "배떡은 스파게티면추가가없더라고요",
          "speaker_id": "1",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013107.1.28",
          "form": "오 스파게티면 신박하네요ㅋㅋㅋ",
          "original_form": "오 스파게티면 신박하네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:25"
        }
      ]
    }
