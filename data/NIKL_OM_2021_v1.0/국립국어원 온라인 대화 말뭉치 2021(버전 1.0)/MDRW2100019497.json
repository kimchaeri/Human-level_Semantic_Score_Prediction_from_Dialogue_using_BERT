      "utterance": [
        {
          "id": "MDRW2100019497.1.1",
          "form": "지금 인천 여행을 가고 있어요",
          "original_form": "지금 인천 여행을 가고 있어요",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019497.1.2",
          "form": "검암역?이네요",
          "original_form": "검암역?이네요",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019497.1.3",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019497.1.4",
          "form": "저는 지금 인천 가고 있어요",
          "original_form": "저는 지금 인천 가고 있어요",
          "speaker_id": "1",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019497.1.5",
          "form": "인천 밤바다~",
          "original_form": "인천 밤바다~",
          "speaker_id": "1",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019497.1.6",
          "form": "검암이에요!",
          "original_form": "검암이에요!",
          "speaker_id": "1",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019497.1.7",
          "form": "여행",
          "original_form": "여행",
          "speaker_id": "2",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019497.1.8",
          "form": "젛조",
          "original_form": "젛조",
          "speaker_id": "2",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019497.1.9",
          "form": "인천여행이면",
          "original_form": "인천여행이면",
          "speaker_id": "2",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019497.1.10",
          "form": "어디가세여?",
          "original_form": "어디가세여?",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019497.1.11",
          "form": "청라 쪽 가보려규요",
          "original_form": "청라 쪽 가보려규요",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019497.1.12",
          "form": "역이 되게 좋네요 역 안에 빵집도 있고",
          "original_form": "역이 되게 좋네요 역 안에 빵집도 있고",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019497.1.13",
          "form": "지하철 여행 좋아흐세요?",
          "original_form": "지하철 여행 좋아흐세요?",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019497.1.14",
          "form": "지하철 안좋아해요...",
          "original_form": "지하철 안좋아해요...",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019497.1.15",
          "form": "여행",
          "original_form": "여행",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019497.1.16",
          "form": "지하철보다는",
          "original_form": "지하철보다는",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019497.1.17",
          "form": "비행기여행",
          "original_form": "비행기여행",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019497.1.18",
          "form": "좋아헤요",
          "original_form": "좋아헤요",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019497.1.19",
          "form": "공항파군요",
          "original_form": "공항파군요",
          "speaker_id": "1",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019497.1.20",
          "form": "저는 버스 여행도 좋아해요",
          "original_form": "저는 버스 여행도 좋아해요",
          "speaker_id": "1",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019497.1.21",
          "form": "자차만 아니면 아무래도",
          "original_form": "자차만 아니면 아무래도",
          "speaker_id": "1",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019497.1.22",
          "form": "자차여행좋지않아요?",
          "original_form": "자차여행좋지않아요?",
          "speaker_id": "2",
          "time": "20210827 19:57"
        },
        {
          "id": "MDRW2100019497.1.23",
          "form": "편할거같은데",
          "original_form": "편할거같은데",
          "speaker_id": "2",
          "time": "20210827 19:57"
        },
        {
          "id": "MDRW2100019497.1.24",
          "form": "항상 사고날까봐 불안한 초보라서요",
          "original_form": "항상 사고날까봐 불안한 초보라서요",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100019497.1.25",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100019497.1.26",
          "form": "안전여행하세요",
          "original_form": "안전여행하세요",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100019497.1.27",
          "form": "저 요즘 관심 잇는 게",
          "original_form": "저 요즘 관심 잇는 게",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100019497.1.28",
          "form": "해안선 따라 자차로 돌기? 이런 게 있더라고요",
          "original_form": "해안선 따라 자차로 돌기? 이런 게 있더라고요",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100019497.1.29",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100019497.1.30",
          "form": "그거 트위터에서 봤어요",
          "original_form": "그거 트위터에서 봤어요",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100019497.1.31",
          "form": "도전?",
          "original_form": "도전?",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100019497.1.32",
          "form": "전 도전! 아마 올겨울이나... 내년 여름으로요",
          "original_form": "전 도전! 아마 올겨울이나... 내년 여름으로요",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100019497.1.33",
          "form": "동승하실래요?",
          "original_form": "동승하실래요?",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100019497.1.34",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100019497.1.35",
          "form": "예스아이두",
          "original_form": "예스아이두",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100019497.1.36",
          "form": "이미 하신 분 말씀 들어보면 목적 자체가 해안선 돌기 도전!이엇던 것 같은데",
          "original_form": "이미 하신 분 말씀 들어보면 목적 자체가 해안선 돌기 도전!이엇던 것 같은데",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100019497.1.37",
          "form": "저희는 넉넉하게 잡아서 사우나에서 자고 맛있는 집 있으면 멈춰서 먹고.. 해보죠",
          "original_form": "저희는 넉넉하게 잡아서 사우나에서 자고 맛있는 집 있으면 멈춰서 먹고.. 해보죠",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100019497.1.38",
          "form": "ㅋㅋㅋㅋ구럼요",
          "original_form": "ㅋㅋㅋㅋ구럼요",
          "speaker_id": "2",
          "time": "20210901 23:06"
        }
      ]
    }
