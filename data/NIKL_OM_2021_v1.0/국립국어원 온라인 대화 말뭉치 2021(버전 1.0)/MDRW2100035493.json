        {
          "id": "MDRW2100035493.1.1",
          "form": "name1 님 저녁 드셨나요?",
          "original_form": "&name1& 님 저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100035493.1.2",
          "form": "아니오 좀 늦게 먹을거에요 name2님은 드셨나요?",
          "original_form": "아니오 좀 늦게 먹을거에요 &name2&님은 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100035493.1.3",
          "form": "네 밥이랑 집에 있는 반찬 먹었어요",
          "original_form": "네 밥이랑 집에 있는 반찬 먹었어요",
          "speaker_id": "2",
          "time": "20210829 18:35"
        },
        {
          "id": "MDRW2100035493.1.4",
          "form": "집밥이 돈도 안나가고 참 좋아요",
          "original_form": "집밥이 돈도 안나가고 참 좋아요",
          "speaker_id": "1",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100035493.1.5",
          "form": "그쵸ㅋㅋ 근데 먹고 났는데도 빵이 먹고 싶어요",
          "original_form": "그쵸ㅋㅋ 근데 먹고 났는데도 빵이 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100035493.1.6",
          "form": "먹다 보면 또 배불러서 안 먹힐거에요",
          "original_form": "먹다 보면 또 배불러서 안 먹힐거에요",
          "speaker_id": "1",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100035493.1.7",
          "form": "맞아요 참아야죠ㅋㅋㅋ",
          "original_form": "맞아요 참아야죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100035493.1.8",
          "form": "귀퉁이만 잘라서 샬샬 드셔보세요",
          "original_form": "귀퉁이만 잘라서 샬샬 드셔보세요",
          "speaker_id": "1",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100035493.1.9",
          "form": "와 스알스알이 금칙어네 진자 필터링이상하네요",
          "original_form": "와 스알스알이 금칙어네 진자 필터링이상하네요",
          "speaker_id": "1",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100035493.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100035493.1.11",
          "form": "와 진짜네요ㅋㅋㅋㅋㅋ",
          "original_form": "와 진짜네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100035493.1.12",
          "form": "빵 먹으려면 나가서 사와야해서 너무 귀찮아요ㅠ",
          "original_form": "빵 먹으려면 나가서 사와야해서 너무 귀찮아요ㅠ",
          "speaker_id": "2",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100035493.1.13",
          "form": "저 아까 님한테 답한줄 알고 가만히 있는데 답이 안와서 ㅋㅋㅋ",
          "original_form": "저 아까 님한테 답한줄 알고 가만히 있는데 답이 안와서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100035493.1.14",
          "form": "다시보니깐 스알스알이 걸려있더라고요 ㅋㅋ",
          "original_form": "다시보니깐 스알스알이 걸려있더라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100035493.1.15",
          "form": "나가야 되는거면 안먹어야죠 나갈려면 씻어야되고,,,",
          "original_form": "나가야 되는거면 안먹어야죠 나갈려면 씻어야되고,,,",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100035493.1.16",
          "form": "아휴 그러니까요",
          "original_form": "아휴 그러니까요",
          "speaker_id": "2",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100035493.1.17",
          "form": "내일 나갈떄 씻음되는데 지금 싯기 싫어요,, 게을,,게을,,",
          "original_form": "내일 나갈떄 씻음되는데 지금 싯기 싫어요,, 게을,,게을,,",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100035493.1.18",
          "form": "ㅋㅋㅋ 단거 먹는 것도 끊어야 하는데...",
          "original_form": "ㅋㅋㅋ 단거 먹는 것도 끊어야 하는데...",
          "speaker_id": "2",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100035493.1.19",
          "form": "ㅎㅎㅎ 단거도 끊어야 할 당위는 있지만,,",
          "original_form": "ㅎㅎㅎ 단거도 끊어야 할 당위는 있지만,,",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100035493.1.20",
          "form": "솔직히 당뇨 정도의 위기가 안오면 자발적으로 끊긴 좀 힘드룢",
