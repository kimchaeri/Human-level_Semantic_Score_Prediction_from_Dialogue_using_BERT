      "utterance": [
        {
          "id": "MDRW2100013962.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013962.1.2",
          "form": "저녘 드셨나요?",
          "original_form": "저녘 드셨나요?",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100013962.1.3",
          "form": "네네 다먹었어요ㅋㅋㅋ",
          "original_form": "네네 다먹었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100013962.1.4",
          "form": "야식 드셧아요? ㅋㅋㅋ",
          "original_form": "야식 드셧아요? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100013962.1.5",
          "form": "ㅋㅋㅋ아뇨",
          "original_form": "ㅋㅋㅋ아뇨",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100013962.1.6",
          "form": "저녁으로요",
          "original_form": "저녁으로요",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100013962.1.7",
          "form": "야식은 안드시나요? 지금이 치킨시킬 마지막 시간입니다.. 째깍째깍",
          "original_form": "야식은 안드시나요? 지금이 치킨시킬 마지막 시간입니다.. 째깍째깍",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100013962.1.8",
          "form": "전 지금",
          "original_form": "전 지금",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100013962.1.9",
          "form": "너무너무",
          "original_form": "너무너무",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100013962.1.10",
          "form": "배부르상태입니다ㅋㅋㅋ",
          "original_form": "배부르상태입니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100013962.1.11",
          "form": "원래 야식은 잘 안드세요?",
          "original_form": "원래 야식은 잘 안드세요?",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100013962.1.12",
          "form": "잘먹죠ㅋㅋㅋ",
          "original_form": "잘먹죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100013962.1.13",
          "form": "근데 지금은 너무배불러서요",
          "original_form": "근데 지금은 너무배불러서요",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100013962.1.14",
          "form": "아.. 야식 먹을때 주로 모 드세여?",
          "original_form": "아.. 야식 먹을때 주로 모 드세여?",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100013962.1.15",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100013962.1.16",
          "form": "전 회나 초밥이요 아님떡볶이요ㅋㅋㅋ",
          "original_form": "전 회나 초밥이요 아님떡볶이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100013962.1.17",
          "form": "초밥은 배달 만족라시나요? 왠지 시키기두려워요..",
          "original_form": "초밥은 배달 만족라시나요? 왠지 시키기두려워요..",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100013962.1.18",
          "form": "ㅋㅋㅋㅋ 네네배달시켜요",
          "original_form": "ㅋㅋㅋㅋ 네네배달시켜요",
