      "utterance": [
        {
          "id": "MDRW2100044157.1.1",
          "form": "네 ㅎ 귀여워요",
          "original_form": "네 ㅎ 귀여워요",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044157.1.2",
          "form": "맞아요 연기도잘하고요!!",
          "original_form": "맞아요 연기도잘하고요!!",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044157.1.3",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044157.1.4",
          "form": "이번에 드라마 들어가는데 알고 계세요?",
          "original_form": "이번에 드라마 들어가는데 알고 계세요?",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044157.1.5",
          "form": "몰랐네요",
          "original_form": "몰랐네요",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044157.1.6",
          "form": "유미의 세포들이라고 드라마 들어가는데 꼭 보세요ㅠ 웹툰이 원작이랍니다!",
          "original_form": "유미의 세포들이라고 드라마 들어가는데 꼭 보세요ㅠ 웹툰이 원작이랍니다!",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044157.1.7",
          "form": "웹툰이 재밌았나봐요",
          "original_form": "웹툰이 재밌았나봐요",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044157.1.8",
          "form": "네! 웹툰도 완전 명작이에요ㅠ",
          "original_form": "네! 웹툰도 완전 명작이에요ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044157.1.9",
          "form": "그런가요 저는 김고응을 도깨비 보고 알았어요",
          "original_form": "그런가요 저는 김고응을 도깨비 보고 알았어요",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044157.1.10",
          "form": "아 도깨비 재밌죠!",
          "original_form": "아 도깨비 재밌죠!",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044157.1.11",
          "form": "제가 공유를 좋아해서요 ㅎ",
          "original_form": "제가 공유를 좋아해서요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044157.1.12",
          "form": "그러시군요 공유도 연기잘하죠",
          "original_form": "그러시군요 공유도 연기잘하죠",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044157.1.13",
          "form": "네! 그래서 더 좋아요!!일단 잘생겼으니까 ㅎㅅㅎ",
          "original_form": "네! 그래서 더 좋아요!!일단 잘생겼으니까 ㅎㅅㅎ",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044157.1.14",
          "form": "맞아요 같이 나온 이동욱도 괜찮더라고요",
          "original_form": "맞아요 같이 나온 이동욱도 괜찮더라고요",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044157.1.15",
          "form": "네 맞아요!!",
          "original_form": "네 맞아요!!",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
