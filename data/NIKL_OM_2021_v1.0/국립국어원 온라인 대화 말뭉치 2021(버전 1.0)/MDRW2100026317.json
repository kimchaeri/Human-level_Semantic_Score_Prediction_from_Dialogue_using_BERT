      "utterance": [
        {
          "id": "MDRW2100026317.1.1",
          "form": "질문할거 있어서 팀장님 부르면서 쳐다보니까 졸고있더라고",
          "original_form": "질문할거 있어서 팀장님 부르면서 쳐다보니까 졸고있더라고",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026317.1.2",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026317.1.3",
          "form": "하",
          "original_form": "하",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026317.1.4",
          "form": "그분은 왜 팀장이에요?",
          "original_form": "그분은 왜 팀장이에요?",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026317.1.5",
          "form": "본인이 먹고 싶은 찜닭에",
          "original_form": "본인이 먹고 싶은 찜닭에",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026317.1.6",
          "form": "마카롱에",
          "original_form": "마카롱에",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026317.1.7",
          "form": "포도까지 먹으니까",
          "original_form": "포도까지 먹으니까",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026317.1.8",
          "form": "배가 불렀어 아주",
          "original_form": "배가 불렀어 아주",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026317.1.9",
          "form": "그래서 졸고앉았지",
          "original_form": "그래서 졸고앉았지",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026317.1.10",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026317.1.11",
          "form": "식곤증 오실만도",
          "original_form": "식곤증 오실만도",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026317.1.12",
          "form": "하네요",
          "original_form": "하네요",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026317.1.13",
          "form": "그치 근데 그게 다 본인맘대로 하고",
          "original_form": "그치 근데 그게 다 본인맘대로 하고",
          "speaker_id": "2",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026317.1.14",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026317.1.15",
          "form": "내 핑계대고",
          "original_form": "내 핑계대고",
          "speaker_id": "2",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026317.1.16",
          "form": "그런것들이라서 짜증난거지",
          "original_form": "그런것들이라서 짜증난거지",
          "speaker_id": "2",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026317.1.17",
          "form": "아니 하고싶어서 한거였으면서",
          "original_form": "아니 하고싶어서 한거였으면서",
          "speaker_id": "1",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026317.1.18",
          "form": "어이가 없네요",
          "original_form": "어이가 없네요",
          "speaker_id": "1",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026317.1.19",
          "form": "그렇게 했으면 본인 업무라도 잘 해야될거 아니냐구",
          "original_form": "그렇게 했으면 본인 업무라도 잘 해야될거 아니냐구",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026317.1.20",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100026317.1.21",
          "form": "본사에서는 이상황 알아요?",
          "original_form": "본사에서는 이상황 알아요?",
          "speaker_id": "1",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100026317.1.22",
          "form": "모르겠지~",
          "original_form": "모르겠지~",
          "speaker_id": "2",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026317.1.23",
          "form": "일 잘하는 줄 알겟징ㅋ",
          "original_form": "일 잘하는 줄 알겟징ㅋ",
          "speaker_id": "2",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026317.1.24",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026317.1.25",
          "form": "언니가 2인분 이상하는구만",
          "original_form": "언니가 2인분 이상하는구만",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026317.1.26",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026317.1.27",
          "form": "그리고 졸다가 깨고는 폰게임 하더라고",
          "original_form": "그리고 졸다가 깨고는 폰게임 하더라고",
          "speaker_id": "2",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026317.1.28",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026317.1.29",
          "form": "가지가지하네요",
          "original_form": "가지가지하네요",
          "speaker_id": "1",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026317.1.30",
          "form": "그러니까.. 내가 일할맛이 뚝뚝 떨어짐",
          "original_form": "그러니까.. 내가 일할맛이 뚝뚝 떨어짐",
          "speaker_id": "2",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026317.1.31",
          "form": "언니가 나가면 회사가 휘청거릴듯",
          "original_form": "언니가 나가면 회사가 휘청거릴듯",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026317.1.32",
          "form": "그렇진 않을거야...",
          "original_form": "그렇진 않을거야...",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100026317.1.33",
          "form": "날 대체할 사람을 또 구하겠지...",
          "original_form": "날 대체할 사람을 또 구하겠지...",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026317.1.34",
          "form": "지금 혼자서 몇인분어치를 하시는건지",
          "original_form": "지금 혼자서 몇인분어치를 하시는건지",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026317.1.35",
          "form": "허리가 휜다 ㅋㅋ",
          "original_form": "허리가 휜다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026317.1.36",
          "form": "ㅜㅜ그니까요",
          "original_form": "ㅜㅜ그니까요",
          "speaker_id": "1",
          "time": "20210901 21:42"
        }
      ]
    }
