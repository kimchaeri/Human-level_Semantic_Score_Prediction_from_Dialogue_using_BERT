      "utterance": [
        {
          "id": "MDRW2100021477.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021477.1.2",
          "form": "콘서트나 뮤지컬 보러 가시는거 좋아하세요?",
          "original_form": "콘서트나 뮤지컬 보러 가시는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100021477.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100021477.1.4",
          "form": "네 너무 좋아해요!!!",
          "original_form": "네 너무 좋아해요!!!",
          "speaker_id": "2",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100021477.1.5",
          "form": "콘서트 많이 가보셨나요??",
          "original_form": "콘서트 많이 가보셨나요??",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021477.1.6",
          "form": "저는 콘서트는 한번정도 가봤고",
          "original_form": "저는 콘서트는 한번정도 가봤고",
          "speaker_id": "2",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021477.1.7",
          "form": "뮤지컬을 좋아해요",
          "original_form": "뮤지컬을 좋아해요",
          "speaker_id": "2",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021477.1.8",
          "form": "아~ 뮤지컬 어떤거 보셨어요?",
          "original_form": "아~ 뮤지컬 어떤거 보셨어요?",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021477.1.9",
          "form": "뮤지컬을 본적이 없어서 궁금해요!",
          "original_form": "뮤지컬을 본적이 없어서 궁금해요!",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021477.1.10",
          "form": "저는 대극장은",
          "original_form": "저는 대극장은",
          "speaker_id": "2",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021477.1.11",
          "form": "팬텀 헤드윅 몬테크리스토 등등 봤구",
          "original_form": "팬텀 헤드윅 몬테크리스토 등등 봤구",
          "speaker_id": "2",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021477.1.12",
          "form": "대학로 뮤지컬 좋아해서 대학로꺼 많이 봤어요!",
          "original_form": "대학로 뮤지컬 좋아해서 대학로꺼 많이 봤어요!",
          "speaker_id": "2",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021477.1.13",
          "form": "오오 문화인이시네요!",
          "original_form": "오오 문화인이시네요!",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021477.1.14",
          "form": "하하ㅠ 요즘은 코로나때문에 잘 못봐요",
          "original_form": "하하ㅠ 요즘은 코로나때문에 잘 못봐요",
          "speaker_id": "2",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021477.1.15",
          "form": "아ㅠ 그렇겠군요ㅠ",
          "original_form": "아ㅠ 그렇겠군요ㅠ",
          "speaker_id": "1",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100021477.1.16",
          "form": "아쉬우시겠어요",
          "original_form": "아쉬우시겠어요",
          "speaker_id": "1",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100021477.1.17",
          "form": "ㅠㅠ 속상하지만 어쩔수없죠",
          "original_form": "ㅠㅠ 속상하지만 어쩔수없죠",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021477.1.18",
          "form": "연극은 보신적있으세요?",
          "original_form": "연극은 보신적있으세요?",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021477.1.19",
          "form": "클래식만 본 것 같아요..!",
          "original_form": "클래식만 본 것 같아요..!",
          "speaker_id": "1",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100021477.1.20",
          "form": "와 대박 멋져요",
          "original_form": "와 대박 멋져요",
          "speaker_id": "2",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100021477.1.21",
          "form": "저 클래식은 한번도 본 적 없어요!",
          "original_form": "저 클래식은 한번도 본 적 없어요!",
          "speaker_id": "2",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100021477.1.22",
          "form": "오 저도 과제때문에 보게됐는데 생각보다 좋았어요",
          "original_form": "오 저도 과제때문에 보게됐는데 생각보다 좋았어요",
          "speaker_id": "1",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100021477.1.23",
          "form": "오 과제!!",
          "original_form": "오 과제!!",
          "speaker_id": "2",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100021477.1.24",
          "form": "저는 발레 한번 봐보고싶어요",
          "original_form": "저는 발레 한번 봐보고싶어요",
          "speaker_id": "2",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100021477.1.25",
          "form": "저도요!!",
          "original_form": "저도요!!",
          "speaker_id": "1",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100021477.1.26",
          "form": "엄청 멋지다던데 가본적이 없네요",
          "original_form": "엄청 멋지다던데 가본적이 없네요",
          "speaker_id": "1",
          "time": "20210828 19:43"
        }
      ]
    }
