      "utterance": [
        {
          "id": "MDRW2100001800.1.1",
          "form": "저 오늘 쇼핑했어요",
          "original_form": "저 오늘 쇼핑했어요",
          "speaker_id": "1",
          "time": "20210819 21:19"
        },
        {
          "id": "MDRW2100001800.1.2",
          "form": "단백질 쉐이크인데 할인행사하더라구요.",
          "original_form": "단백질 쉐이크인데 할인행사하더라구요.",
          "speaker_id": "1",
          "time": "20210819 21:19"
        },
        {
          "id": "MDRW2100001800.1.3",
          "form": "오 얼마주고 샀음?",
          "original_form": "오 얼마주고 샀음?",
          "speaker_id": "2",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001800.1.4",
          "form": "1+1인데 개당 1650원쯤 산거같아여",
          "original_form": "1+1인데 개당 1650원쯤 산거같아여",
          "speaker_id": "1",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001800.1.5",
          "form": "원래는 개당 거의 3000원 꼴인데",
          "original_form": "원래는 개당 거의 3000원 꼴인데",
          "speaker_id": "1",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001800.1.6",
          "form": "할인행사안하면 못사먹겠더라구요",
          "original_form": "할인행사안하면 못사먹겠더라구요",
          "speaker_id": "1",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001800.1.7",
          "form": "싸게샀네잉",
          "original_form": "싸게샀네잉",
          "speaker_id": "2",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001800.1.8",
          "form": "개비싸여 우쒸",
          "original_form": "개비싸여 우쒸",
          "speaker_id": "1",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001800.1.9",
          "form": "근데 성분이 좋고 맛도 좋아서",
          "original_form": "근데 성분이 좋고 맛도 좋아서",
          "speaker_id": "1",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001800.1.10",
          "form": "단백질 쉐이크는 이것만 사여",
          "original_form": "단백질 쉐이크는 이것만 사여",
          "speaker_id": "1",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100001800.1.11",
          "form": "뭔맛이여?",
          "original_form": "뭔맛이여?",
          "speaker_id": "2",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100001800.1.12",
          "form": "일단 초코랑 고구마맛이여",
          "original_form": "일단 초코랑 고구마맛이여",
          "speaker_id": "1",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001800.1.13",
          "form": "초코 맛있겠다",
          "original_form": "초코 맛있겠다",
          "speaker_id": "2",
          "time": "20210820 08:50"
        },
        {
          "id": "MDRW2100001800.1.14",
          "form": "고구마는 약간 상상이 안가\r\n고구마라떼같은 맛이려나",
          "original_form": "고구마는 약간 상상이 안가\r\n고구마라떼같은 맛이려나",
          "speaker_id": "2",
          "time": "20210820 08:51"
        },
        {
          "id": "MDRW2100001800.1.15",
          "form": "비슷하긴해여",
          "original_form": "비슷하긴해여",
          "speaker_id": "1",
          "time": "20210820 08:53"
        },
        {
          "id": "MDRW2100001800.1.16",
          "form": "우유타먹으면 맛나여ㅋㅋㅋ",
          "original_form": "우유타먹으면 맛나여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:53"
        },
        {
          "id": "MDRW2100001800.1.17",
          "form": "미끄덩한 맛은 별로 없어??",
          "original_form": "미끄덩한 맛은 별로 없어??",
          "speaker_id": "2",
          "time": "20210820 08:54"
        },
        {
          "id": "MDRW2100001800.1.18",
          "form": "아 이건 그런맛 없어여",
          "original_form": "아 이건 그런맛 없어여",
          "speaker_id": "1",
          "time": "20210820 08:56"
        },
        {
          "id": "MDRW2100001800.1.19",
          "form": "언니 만날때 한팩 가져가보께요ㅋㅋ",
          "original_form": "언니 만날때 한팩 가져가보께요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:57"
        },
        {
          "id": "MDRW2100001800.1.20",
          "form": "이전에 안줬나",
          "original_form": "이전에 안줬나",
          "speaker_id": "1",
          "time": "20210820 08:57"
        },
        {
          "id": "MDRW2100001800.1.21",
          "form": "그려",
          "original_form": "그려",
          "speaker_id": "2",
          "time": "20210820 08:57"
        },
        {
          "id": "MDRW2100001800.1.22",
          "form": "옹 이전에는 안 줬으",
          "original_form": "옹 이전에는 안 줬으",
          "speaker_id": "2",
          "time": "20210820 08:58"
        },
        {
          "id": "MDRW2100001800.1.23",
          "form": "언니는 최근 산거 없어여?",
          "original_form": "언니는 최근 산거 없어여?",
          "speaker_id": "1",
          "time": "20210820 08:59"
        },
        {
          "id": "MDRW2100001800.1.24",
          "form": "나 샤오미핏인가?",
          "original_form": "나 샤오미핏인가?",
          "speaker_id": "2",
          "time": "20210820 09:00"
        },
        {
          "id": "MDRW2100001800.1.25",
          "form": "그 시계되는거 샀으",
          "original_form": "그 시계되는거 샀으",
          "speaker_id": "2",
          "time": "20210820 09:00"
        },
        {
          "id": "MDRW2100001800.1.26",
          "form": "이전버전 고장나가지고~",
          "original_form": "이전버전 고장나가지고~",
          "speaker_id": "2",
          "time": "20210820 09:00"
        }
      ]
    }
