      "utterance": [
        {
          "id": "MDRW2100047499.1.1",
          "form": "저는 그러고보니 와디즈에서 본 베개 사고싶어요",
          "original_form": "저는 그러고보니 와디즈에서 본 베개 사고싶어요",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100047499.1.2",
          "form": "사실쇼핑을 잘안해서ㅎ 필요한건만 사는스타일이라 잘모르는게많네요",
          "original_form": "사실쇼핑을 잘안해서ㅎ 필요한건만 사는스타일이라 잘모르는게많네요",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100047499.1.3",
          "form": "저도 늘 텅장인데 사고 싶은건 많아요",
          "original_form": "저도 늘 텅장인데 사고 싶은건 많아요",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100047499.1.4",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100047499.1.5",
          "form": "ㅋㅋ 짠테크해야되서ㅜ",
          "original_form": "ㅋㅋ 짠테크해야되서ㅜ",
          "speaker_id": "2",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100047499.1.6",
          "form": "저도요 그래서 앱테크도 많이하고",
          "original_form": "저도요 그래서 앱테크도 많이하고",
          "speaker_id": "1",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100047499.1.7",
          "form": "맨날 할인 찾아봐요 ㅋㅋ",
          "original_form": "맨날 할인 찾아봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100047499.1.8",
          "form": "핫딜나오면ㅋㅋ",
          "original_form": "핫딜나오면ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100047499.1.9",
          "form": "고민하다가사죠ㅎ",
          "original_form": "고민하다가사죠ㅎ",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100047499.1.10",
          "form": "저는 떠리몰 이유몰도 맨날 구경해요",
          "original_form": "저는 떠리몰 이유몰도 맨날 구경해요",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100047499.1.11",
          "form": "이미 아실지도 ㅎㅎ",
          "original_form": "이미 아실지도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100047499.1.12",
          "form": "헉 처음들어요ㅋㅋ",
          "original_form": "헉 처음들어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100047499.1.13",
          "form": "젊은분이신가보네용ㅎㅎ 저는 뭔지 하나도모림ㅎ",
          "original_form": "젊은분이신가보네용ㅎㅎ 저는 뭔지 하나도모림ㅎ",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100047499.1.14",
          "form": "인터넷 사이트인데요",
          "original_form": "인터넷 사이트인데요",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100047499.1.15",
          "form": "이유몰 떠리몰 치면 나오고요",
          "original_form": "이유몰 떠리몰 치면 나오고요",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100047499.1.16",
          "form": "유통기한임박이나 재고 상품들 진짜 싸게 팔아요",
          "original_form": "유통기한임박이나 재고 상품들 진짜 싸게 팔아요",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100047499.1.17",
          "form": "팔구십 프로도 많고요",
          "original_form": "팔구십 프로도 많고요",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100047499.1.18",
          "form": "좋은거 하나 배우네요ㅎ 고마워용ㅎㅎ",
          "original_form": "좋은거 하나 배우네요ㅎ 고마워용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100047499.1.19",
          "form": "이유몰 쳐봐야겠어요ㅎ",
          "original_form": "이유몰 쳐봐야겠어요ㅎ",
          "speaker_id": "2",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100047499.1.20",
          "form": "저는 당근도 많이는 아니지만 간간히 해요 네이버카페 중고나라도요 ㅎㅎ",
          "original_form": "저는 당근도 많이는 아니지만 간간히 해요 네이버카페 중고나라도요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 10:24"
        },
        {
          "id": "MDRW2100047499.1.21",
          "form": "네이버스토어는 저도 이용하긴하네요",
          "original_form": "네이버스토어는 저도 이용하긴하네요",
          "speaker_id": "2",
          "time": "20210827 11:14"
        },
        {
          "id": "MDRW2100047499.1.22",
          "form": "기프트콘은 중고나라가 참 싼데",
          "original_form": "기프트콘은 중고나라가 참 싼데",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100047499.1.23",
          "form": "매번 사기 당할까봐 불안해하면서 거래해요ㅜ",
          "original_form": "매번 사기 당할까봐 불안해하면서 거래해요ㅜ",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100047499.1.24",
          "form": "저는 기프트콘은 안해봤어요ㅜ",
          "original_form": "저는 기프트콘은 안해봤어요ㅜ",
          "speaker_id": "2",
          "time": "20210827 13:46"
        }
      ]
    }
  ]
