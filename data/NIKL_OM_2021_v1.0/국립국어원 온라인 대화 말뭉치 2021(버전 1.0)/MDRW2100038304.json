        },
        {
          "id": "MDRW2100038304.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100038304.1.3",
          "form": "아침 뭐 드셨나요",
          "original_form": "아침 뭐 드셨나요",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038304.1.4",
          "form": "오늘아침이 커피네요ㅎ",
          "original_form": "오늘아침이 커피네요ㅎ",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038304.1.5",
          "form": "뭐드셨어용?",
          "original_form": "뭐드셨어용?",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038304.1.6",
          "form": "어제 남은 카레 마저 먹었어요",
          "original_form": "어제 남은 카레 마저 먹었어요",
          "speaker_id": "1",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100038304.1.7",
          "form": "네ㅎ 맛있는거먹었군요ㅎ",
          "original_form": "네ㅎ 맛있는거먹었군요ㅎ",
          "speaker_id": "2",
          "time": "20210902 09:52"
        },
        {
          "id": "MDRW2100038304.1.8",
          "form": "커피는 어떤 커피 드셨나요?",
          "original_form": "커피는 어떤 커피 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 09:54"
        },
        {
          "id": "MDRW2100038304.1.9",
          "form": "아아 좋아해요ㅎ",
          "original_form": "아아 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210902 09:56"
        },
        {
          "id": "MDRW2100038304.1.10",
          "form": "요즘커피가 워낙 다양하니ㅎ",
          "original_form": "요즘커피가 워낙 다양하니ㅎ",
          "speaker_id": "2",
          "time": "20210902 09:56"
        },
        {
          "id": "MDRW2100038304.1.11",
          "form": "아아 마시기에는 날씨가 추워지지 않았나요",
          "original_form": "아아 마시기에는 날씨가 추워지지 않았나요",
          "speaker_id": "1",
          "time": "20210902 09:57"
        },
        {
          "id": "MDRW2100038304.1.12",
          "form": "전 이제 얼음 꼭 빼요",
          "original_form": "전 이제 얼음 꼭 빼요",
          "speaker_id": "1",
          "time": "20210902 09:57"
        },
        {
          "id": "MDRW2100038304.1.13",
          "form": "ㅎ아직은 괜찮네요 겨울아닌이상 아아 먹는편이예요",
          "original_form": "ㅎ아직은 괜찮네요 겨울아닌이상 아아 먹는편이예요",
          "speaker_id": "2",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100038304.1.14",
          "form": "와 진짜 아아 좋아하시는군요",
          "original_form": "와 진짜 아아 좋아하시는군요",
          "speaker_id": "1",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100038304.1.15",
          "form": "ㅎ네 요즘 커피집 많아서 골라먹네요ㅎ",
          "original_form": "ㅎ네 요즘 커피집 많아서 골라먹네요ㅎ",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038304.1.16",
          "form": "맞아요 프랜차이즈도 다양하고",
          "original_form": "맞아요 프랜차이즈도 다양하고",
          "speaker_id": "1",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038304.1.17",
          "form": "동네 카페도 늘어났죠",
          "original_form": "동네 카페도 늘어났죠",
          "speaker_id": "1",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038304.1.18",
          "form": "그렇네요ㅎ 다른 음식 뭐좋아해요?",
          "original_form": "그렇네요ㅎ 다른 음식 뭐좋아해요?",
          "speaker_id": "2",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100038304.1.19",
          "form": "떡볶이 좋아해요",
          "original_form": "떡볶이 좋아해요",
          "speaker_id": "1",
          "time": "20210902 10:20"
        }
      ]
