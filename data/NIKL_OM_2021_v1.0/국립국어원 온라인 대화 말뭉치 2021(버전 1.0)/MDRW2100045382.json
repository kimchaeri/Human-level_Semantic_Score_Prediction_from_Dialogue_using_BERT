      "utterance": [
        {
          "id": "MDRW2100045382.1.1",
          "form": "로오히 로드 렙 몇이예요?",
          "original_form": "로오히 로드 렙 몇이예요?",
          "speaker_id": "1",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045382.1.2",
          "form": "저는 35 정도예요",
          "original_form": "저는 35 정도예요",
          "speaker_id": "2",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045382.1.3",
          "form": "오 높으시네요!! 애들 각성 많이 하셨어요?",
          "original_form": "오 높으시네요!! 애들 각성 많이 하셨어요?",
          "speaker_id": "1",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045382.1.4",
          "form": "한 6캐릭 정도 2각 해준것 같아요",
          "original_form": "한 6캐릭 정도 2각 해준것 같아요",
          "speaker_id": "2",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045382.1.5",
          "form": "오 저도 그정도 한것 같은데 왜 전 아직 35아닐까요",
          "original_form": "오 저도 그정도 한것 같은데 왜 전 아직 35아닐까요",
          "speaker_id": "1",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045382.1.6",
          "form": "ㅠ 현질 하셨나요?",
          "original_form": "ㅠ 현질 하셨나요?",
          "speaker_id": "1",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045382.1.7",
          "form": "명성말고.. 코튬에 좀 했어요",
          "original_form": "명성말고.. 코튬에 좀 했어요",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045382.1.8",
          "form": "아 저도 이번 코튬에 좀 지를 것 같아요",
          "original_form": "아 저도 이번 코튬에 좀 지를 것 같아요",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045382.1.9",
          "form": "자이라랑 크롬이었나요?",
          "original_form": "자이라랑 크롬이었나요?",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045382.1.10",
          "form": "네 맞아요 너무 멋지지 않나요?",
          "original_form": "네 맞아요 너무 멋지지 않나요?",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045382.1.11",
          "form": "아 저는 자이라랑 크롬은 좀 안 끌려서",
          "original_form": "아 저는 자이라랑 크롬은 좀 안 끌려서",
