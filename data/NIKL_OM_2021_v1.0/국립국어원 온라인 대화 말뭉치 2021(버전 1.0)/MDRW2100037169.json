      "utterance": [
        {
          "id": "MDRW2100037169.1.1",
          "form": "기프티스타 자주 이용하세요?",
          "original_form": "기프티스타 자주 이용하세요?",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100037169.1.2",
          "form": "서너번 이용해봤어요 얼마 전 알아 가지고요",
          "original_form": "서너번 이용해봤어요 얼마 전 알아 가지고요",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100037169.1.3",
          "form": "아 가입해서 팔아야하는거죠?",
          "original_form": "아 가입해서 팔아야하는거죠?",
          "speaker_id": "1",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100037169.1.4",
          "form": "네 스타벅스 아메리카노 3150 원 정도 주는 거 같아요",
          "original_form": "네 스타벅스 아메리카노 3150 원 정도 주는 거 같아요",
          "speaker_id": "2",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100037169.1.5",
          "form": "아 찾아봐야겠어요 그건 업체에 파는거죠? 개인간 거래가 아니라",
          "original_form": "아 찾아봐야겠어요 그건 업체에 파는거죠? 개인간 거래가 아니라",
          "speaker_id": "1",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100037169.1.6",
          "form": "네 업체에 팔고 업체가 소비자한테 이익을 남겨요",
          "original_form": "네 업체에 팔고 업체가 소비자한테 이익을 남겨요",
          "speaker_id": "2",
          "time": "20210831 21:19"
        },
        {
          "id": "MDRW2100037169.1.7",
          "form": "기프티스타 네이버에서 몇번 구매는 해봤어요",
          "original_form": "기프티스타 네이버에서 몇번 구매는 해봤어요",
          "speaker_id": "1",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100037169.1.8",
          "form": "그래도 신뢰는 더가겠네요",
          "original_form": "그래도 신뢰는 더가겠네요",
          "speaker_id": "1",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100037169.1.9",
          "form": "그냥 편해서요 기프티스타 니콘내콘 두 군데가 있다고 들었어요",
          "original_form": "그냥 편해서요 기프티스타 니콘내콘 두 군데가 있다고 들었어요",
          "speaker_id": "2",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100037169.1.10",
          "form": "네네 둘다 들어는 봤는데 항상 당근에다 팔았었어요",
          "original_form": "네네 둘다 들어는 봤는데 항상 당근에다 팔았었어요",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100037169.1.11",
          "form": "실제 물건도 팔아보셨어요?",
          "original_form": "실제 물건도 팔아보셨어요?",
          "speaker_id": "1",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100037169.1.12",
          "form": "자취하고 이사할 때 책장 같은 거 동네컴퓨터 팔았어요",
          "original_form": "자취하고 이사할 때 책장 같은 거 동네컴퓨터 팔았어요",
          "speaker_id": "2",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100037169.1.13",
          "form": "커뮤니티에",
          "original_form": "커뮤니티에",
          "speaker_id": "2",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100037169.1.14",
          "form": "아 대단해요! 이사할땐 진짜 골치더라고요",
          "original_form": "아 대단해요! 이사할땐 진짜 골치더라고요",
          "speaker_id": "1",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100037169.1.15",
          "form": "네 빨리 팔아야 짐이 줄어드니까요",
          "original_form": "네 빨리 팔아야 짐이 줄어드니까요",
          "speaker_id": "2",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100037169.1.16",
          "form": "진짜 필요한 사람은 싸게 사서 좋고 참 긍정적인 것 같아요",
          "original_form": "진짜 필요한 사람은 싸게 사서 좋고 참 긍정적인 것 같아요",
          "speaker_id": "1",
          "time": "20210831 21:28"
        }
      ]
