      "utterance": [
        {
          "id": "MDRW2100002340.1.1",
          "form": "안녕하세요 선생님",
          "original_form": "안녕하세요 선생님",
          "speaker_id": "1",
          "time": "20210820 09:56"
        },
        {
          "id": "MDRW2100002340.1.2",
          "form": "가내 두루 평안하신지요",
          "original_form": "가내 두루 평안하신지요",
          "speaker_id": "1",
          "time": "20210820 09:57"
        },
        {
          "id": "MDRW2100002340.1.3",
          "form": "안녕하세요 멋진 아이디에요",
          "original_form": "안녕하세요 멋진 아이디에요",
          "speaker_id": "2",
          "time": "20210820 09:57"
        },
        {
          "id": "MDRW2100002340.1.4",
          "form": "그럼요 가족관계가 어떻게되세요?",
          "original_form": "그럼요 가족관계가 어떻게되세요?",
          "speaker_id": "2",
          "time": "20210820 09:57"
        },
        {
          "id": "MDRW2100002340.1.5",
          "form": "ㅋㅋㅋㅋㅋ별안간 가족 관계를 묻는다고요???당황스럽군요",
          "original_form": "ㅋㅋㅋㅋㅋ별안간 가족 관계를 묻는다고요???당황스럽군요",
          "speaker_id": "1",
          "time": "20210820 09:57"
        },
        {
          "id": "MDRW2100002340.1.6",
          "form": "말해주세요!!",
          "original_form": "말해주세요!!",
          "speaker_id": "2",
          "time": "20210820 09:58"
        },
        {
          "id": "MDRW2100002340.1.7",
          "form": "등본상의 가족은 엄마아빠 동생1, 2입니다",
          "original_form": "등본상의 가족은 엄마아빠 동생1, 2입니다",
          "speaker_id": "1",
          "time": "20210820 09:58"
        },
        {
          "id": "MDRW2100002340.1.8",
          "form": "하지만 현재는 동생 1이 타지로 떠나 있어요",
          "original_form": "하지만 현재는 동생 1이 타지로 떠나 있어요",
          "speaker_id": "1",
          "time": "20210820 09:59"
        },
        {
          "id": "MDRW2100002340.1.9",
          "form": "동생이 보고싶으시겠어요",
          "original_form": "동생이 보고싶으시겠어요",
          "speaker_id": "2",
          "time": "20210820 09:59"
        },
        {
          "id": "MDRW2100002340.1.10",
          "form": "왜 타지로 떠났죠?",
          "original_form": "왜 타지로 떠났죠?",
          "speaker_id": "2",
          "time": "20210820 09:59"
        },
        {
          "id": "MDRW2100002340.1.11",
          "form": "서울로 상경한 제 동생을 위해서 박수를 쳐 주시겠어요?",
          "original_form": "서울로 상경한 제 동생을 위해서 박수를 쳐 주시겠어요?",
          "speaker_id": "1",
          "time": "20210820 09:59"
        },
        {
          "id": "MDRW2100002340.1.12",
          "form": "짝 짝 짝..",
          "original_form": "짝 짝 짝..",
          "speaker_id": "2",
          "time": "20210820 09:59"
        },
        {
          "id": "MDRW2100002340.1.13",
          "form": "타지로왜떠났어요?",
          "original_form": "타지로왜떠났어요?",
          "speaker_id": "2",
          "time": "20210820 10:00"
        },
        {
          "id": "MDRW2100002340.1.14",
          "form": "직장에서 서울지사로 발령받았습니다",
          "original_form": "직장에서 서울지사로 발령받았습니다",
          "speaker_id": "1",
          "time": "20210820 10:02"
        },
        {
          "id": "MDRW2100002340.1.15",
          "form": "동생 2는 지금 뭐해요",
          "original_form": "동생 2는 지금 뭐해요",
          "speaker_id": "2",
          "time": "20210820 10:02"
        },
        {
          "id": "MDRW2100002340.1.16",
          "form": "그리고 대학에 진학했기 때문에 겸사겸사이지요",
          "original_form": "그리고 대학에 진학했기 때문에 겸사겸사이지요",
          "speaker_id": "1",
          "time": "20210820 10:02"
        },
        {
          "id": "MDRW2100002340.1.17",
          "form": "동생에게 몹시 잘 된 일이라고 생각합니다",
          "original_form": "동생에게 몹시 잘 된 일이라고 생각합니다",
          "speaker_id": "1",
          "time": "20210820 10:02"
        },
        {
          "id": "MDRW2100002340.1.18",
          "form": "저두글케생각합니다",
          "original_form": "저두글케생각합니다",
          "speaker_id": "2",
          "time": "20210820 10:03"
        },
        {
          "id": "MDRW2100002340.1.19",
          "form": "동생 2는 방에서 배깔고 자고 있어요",
          "original_form": "동생 2는 방에서 배깔고 자고 있어요",
          "speaker_id": "1",
          "time": "20210820 10:03"
        },
        {
