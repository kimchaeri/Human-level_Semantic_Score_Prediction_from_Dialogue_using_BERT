      "utterance": [
        {
          "id": "MDRW2100023978.1.1",
          "form": "오늘 아침 점심 저녁 다 챙겨드셨어요?",
          "original_form": "오늘 아침 점심 저녁 다 챙겨드셨어요?",
          "speaker_id": "1",
          "time": "20210830 20:50"
        },
        {
          "id": "MDRW2100023978.1.2",
          "form": "네. 적당히 챙겨먹었어요",
          "original_form": "네. 적당히 챙겨먹었어요",
          "speaker_id": "2",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023978.1.3",
          "form": "ㅎㅎ 보통 밥은 사먹는 편이에요?",
          "original_form": "ㅎㅎ 보통 밥은 사먹는 편이에요?",
          "speaker_id": "1",
          "time": "20210830 20:57"
        },
        {
          "id": "MDRW2100023978.1.4",
          "form": "보통 도시락싸서 다녀요",
          "original_form": "보통 도시락싸서 다녀요",
          "speaker_id": "2",
          "time": "20210830 21:00"
        },
        {
          "id": "MDRW2100023978.1.5",
          "form": "돈도 아끼고 사실 나가서 사먹는게 귀찮아서요",
          "original_form": "돈도 아끼고 사실 나가서 사먹는게 귀찮아서요",
          "speaker_id": "2",
          "time": "20210830 21:00"
        },
        {
          "id": "MDRW2100023978.1.6",
          "form": "우와 그럼 도시락 직접 준비해요?",
          "original_form": "우와 그럼 도시락 직접 준비해요?",
          "speaker_id": "1",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023978.1.7",
          "form": "아무래도요",
          "original_form": "아무래도요",
          "speaker_id": "2",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023978.1.8",
          "form": "입맛이 까다롭진않아서 걍 적당히 해요",
          "original_form": "입맛이 까다롭진않아서 걍 적당히 해요",
          "speaker_id": "2",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023978.1.9",
          "form": "주로 어떤 메뉴 준비하시는데요?",
          "original_form": "주로 어떤 메뉴 준비하시는데요?",
          "speaker_id": "1",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100023978.1.10",
          "form": "그냥 밥에 김치 및 밑반찬 싸가고",
          "original_form": "그냥 밥에 김치 및 밑반찬 싸가고",
          "speaker_id": "2",
          "time": "20210830 21:05"
        },
        {
          "id": "MDRW2100023978.1.11",
          "form": "대패삼겹을 많이 활용하는데요",
          "original_form": "대패삼겹을 많이 활용하는데요",
          "speaker_id": "2",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100023978.1.12",
          "form": "걔를 김치랑  볶거나 양념해서 콩나물과 볶거나",
          "original_form": "걔를 김치랑  볶거나 양념해서 콩나물과 볶거나",
          "speaker_id": "2",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100023978.1.13",
          "form": "아니면 계란말이 같은거",
          "original_form": "아니면 계란말이 같은거",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100023978.1.14",
          "form": "오~ 대패삼겹살이 진짜 활용도가 높네요.",
          "original_form": "오~ 대패삼겹살이 진짜 활용도가 높네요.",
          "speaker_id": "1",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023978.1.15",
          "form": "저도 갈수록 도시락 메뉴가 고민인데 그렇게 한 번 해봐야겠어요",
          "original_form": "저도 갈수록 도시락 메뉴가 고민인데 그렇게 한 번 해봐야겠어요",
          "speaker_id": "1",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023978.1.16",
          "form": "그쵸,",
          "original_form": "그쵸,",
          "speaker_id": "2",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100023978.1.17",
          "form": "근데 요즘 갈수록 대패삼겹 가격이 올라서 걱정이에요",
          "original_form": "근데 요즘 갈수록 대패삼겹 가격이 올라서 걱정이에요",
          "speaker_id": "2",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100023978.1.18",
          "form": "사실 고기값이 다 올랐죠..",
          "original_form": "사실 고기값이 다 올랐죠..",
          "speaker_id": "2",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100023978.1.19",
          "form": "아이구.. 왜 안떨어질까요",
          "original_form": "아이구.. 왜 안떨어질까요",
          "speaker_id": "1",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100023978.1.20",
          "form": "요새 대패삼겹 100그램당 얼마 정도 해요?",
          "original_form": "요새 대패삼겹 100그램당 얼마 정도 해요?",
          "speaker_id": "1",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100023978.1.21",
          "form": "보통  마트에서 예전엔 1키로에 만원정도였는데",
          "original_form": "보통  마트에서 예전엔 1키로에 만원정도였는데",
          "speaker_id": "2",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100023978.1.22",
          "form": "지금은 만오천원 가까이해요",
          "original_form": "지금은 만오천원 가까이해요",
          "speaker_id": "2",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100023978.1.23",
          "form": "헐.. 수입산이요..?",
          "original_form": "헐.. 수입산이요..?",
          "speaker_id": "1",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100023978.1.24",
          "form": "네....",
          "original_form": "네....",
          "speaker_id": "2",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100023978.1.25",
          "form": "주로 유럽산인데",
          "original_form": "주로 유럽산인데",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100023978.1.26",
          "form": "아니 진짜 많이 비싸졌네요;;",
          "original_form": "아니 진짜 많이 비싸졌네요;;",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100023978.1.27",
          "form": "뭐 야채랑 같이 볶으면 한 백그램으로도 하루반찬은 되기 때문에",
          "original_form": "뭐 야채랑 같이 볶으면 한 백그램으로도 하루반찬은 되기 때문에",
          "speaker_id": "2",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100023978.1.28",
          "form": "그치만 야채도 싸진 않죠...",
          "original_form": "그치만 야채도 싸진 않죠...",
          "speaker_id": "2",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100023978.1.29",
          "form": "맞아요 그래도 요새 야채는 가격이 안정적이더라구요",
          "original_form": "맞아요 그래도 요새 야채는 가격이 안정적이더라구요",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100023978.1.30",
          "form": "계란도 며칠전에 한판 7500원에 샀더니 일주일 뒤 6500원.. ㅠㅠ",
          "original_form": "계란도 며칠전에 한판 7500원에 샀더니 일주일 뒤 6500원.. ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100023978.1.31",
          "form": "계란...",
          "original_form": "계란...",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100023978.1.32",
          "form": "가장  만만한게 계란인데 가격이 많이 올랐죠",
          "original_form": "가장  만만한게 계란인데 가격이 많이 올랐죠",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100023978.1.33",
          "form": "저번달엔가는 한 9천원해서 손이 부들부들 떨렸는데",
          "original_form": "저번달엔가는 한 9천원해서 손이 부들부들 떨렸는데",
          "speaker_id": "2",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100023978.1.34",
          "form": "헉 ㅋㅋㅋ 진짜 공감",
          "original_form": "헉 ㅋㅋㅋ 진짜 공감",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100023978.1.35",
          "form": "저희 동네는 9900원까지도 올랐었네요",
          "original_form": "저희 동네는 9900원까지도 올랐었네요",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100023978.1.36",
          "form": "어찌나 떨어질 생각을 6개월 넘게 안하는지;;",
          "original_form": "어찌나 떨어질 생각을 6개월 넘게 안하는지;;",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100023978.1.37",
          "form": "으악!",
          "original_form": "으악!",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100023978.1.38",
          "form": "그래서 더더욱 냉동식품 자주 먹네요",
          "original_form": "그래서 더더욱 냉동식품 자주 먹네요",
          "speaker_id": "1",
          "time": "20210830 21:31"
        }
      ]
