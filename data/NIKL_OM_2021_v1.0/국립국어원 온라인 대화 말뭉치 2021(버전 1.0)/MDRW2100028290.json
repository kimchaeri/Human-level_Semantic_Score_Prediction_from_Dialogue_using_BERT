      "utterance": [
        {
          "id": "MDRW2100028290.1.1",
          "form": "오늘 저녁은 뭐먹어",
          "original_form": "오늘 저녁은 뭐먹어",
          "speaker_id": "1",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100028290.1.2",
          "form": "오늘저녁은 돼지갈비와 냉면이 예정되어있답니다~",
          "original_form": "오늘저녁은 돼지갈비와 냉면이 예정되어있답니다~",
          "speaker_id": "2",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028290.1.3",
          "form": "헐 뭐야 나도 줘",
          "original_form": "헐 뭐야 나도 줘",
          "speaker_id": "1",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028290.1.4",
          "form": "고기냉면은 최고의조합",
          "original_form": "고기냉면은 최고의조합",
          "speaker_id": "2",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028290.1.5",
          "form": "나도 냉면~!!!",
          "original_form": "나도 냉면~!!!",
          "speaker_id": "1",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028290.1.6",
          "form": "후후",
          "original_form": "후후",
          "speaker_id": "2",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028290.1.7",
          "form": "아냐 난 내일 크로플 먹을거인걸 참아 나 자신",
          "original_form": "아냐 난 내일 크로플 먹을거인걸 참아 나 자신",
          "speaker_id": "1",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028290.1.8",
          "form": "우리집 냉면에 근데 별거다들어가는데 괜찮겠어",
          "original_form": "우리집 냉면에 근데 별거다들어가는데 괜찮겠어",
          "speaker_id": "2",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028290.1.9",
          "form": "쌈무나 토마토가 들어갈때도있다",
          "original_form": "쌈무나 토마토가 들어갈때도있다",
          "speaker_id": "2",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028290.1.10",
          "form": "크로플 마싯겠다....",
          "original_form": "크로플 마싯겠다....",
          "speaker_id": "2",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028290.1.11",
          "form": "나도 괴식이라면 지지않아",
          "original_form": "나도 괴식이라면 지지않아",
          "speaker_id": "1",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100028290.1.12",
          "form": "엥?",
          "original_form": "엥?",
          "speaker_id": "1",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100028290.1.13",
          "form": "쌈무는 냉면 먹을때 들어가곤 하잖아",
          "original_form": "쌈무는 냉면 먹을때 들어가곤 하잖아",
          "speaker_id": "1",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100028290.1.14",
          "form": "엥?졌어?",
          "original_form": "엥?졌어?",
          "speaker_id": "2",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100028290.1.15",
          "form": "그러고보니그렇네",
          "original_form": "그러고보니그렇네",
          "speaker_id": "2",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100028290.1.16",
          "form": "토마토는 넣어본적 없지만",
          "original_form": "토마토는 넣어본적 없지만",
          "speaker_id": "1",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100028290.1.17",
          "form": "있으면",
          "original_form": "있으면",
          "speaker_id": "2",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100028290.1.18",
