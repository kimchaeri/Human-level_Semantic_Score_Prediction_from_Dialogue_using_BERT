      "utterance": [
        {
          "id": "MDRW2100021626.1.1",
          "form": "종교로 바뀌었네요",
          "original_form": "종교로 바뀌었네요",
          "speaker_id": "1",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100021626.1.2",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "2",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100021626.1.3",
          "form": "아니오 ㅋㅋ",
          "original_form": "아니오 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100021626.1.4",
          "form": "무교에요",
          "original_form": "무교에요",
          "speaker_id": "1",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100021626.1.5",
          "form": "저도요 ㅎㅎ",
          "original_form": "저도요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100021626.1.6",
          "form": "주변에 기독교인 친구들은 많아요",
          "original_form": "주변에 기독교인 친구들은 많아요",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100021626.1.7",
          "form": "저는 친구들도 다 무교에요 ㅋㅋ",
          "original_form": "저는 친구들도 다 무교에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100021626.1.8",
          "form": "오 그럼 교회도 안가보셨겠네요!",
          "original_form": "오 그럼 교회도 안가보셨겠네요!",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021626.1.9",
          "form": "넹ㅋㅋㅋㅋ 들어가본 적 없어요",
          "original_form": "넹ㅋㅋㅋㅋ 들어가본 적 없어요",
          "speaker_id": "1",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100021626.1.10",
          "form": "성당은 유적지 탐방하는 느낌으로 들어가 본 적은 있어요",
          "original_form": "성당은 유적지 탐방하는 느낌으로 들어가 본 적은 있어요",
          "speaker_id": "1",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100021626.1.11",
          "form": "오 저는 절이랑 교회는 가봤는데 성당은 안가봤어요",
          "original_form": "오 저는 절이랑 교회는 가봤는데 성당은 안가봤어요",
          "speaker_id": "2",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100021626.1.12",
          "form": "동네에 엄청 예전에 지어서 한옥으로 지은 성당이 있거든요 거기 가봤어요",
          "original_form": "동네에 엄청 예전에 지어서 한옥으로 지은 성당이 있거든요 거기 가봤어요",
          "speaker_id": "1",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100021626.1.13",
          "form": "우와 신기하다",
          "original_form": "우와 신기하다",
          "speaker_id": "2",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100021626.1.14",
          "form": "되게 멋질거같아요",
          "original_form": "되게 멋질거같아요",
          "speaker_id": "2",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100021626.1.15",
          "form": "그렇게 크지 않아서 겉에서 보면 그냥 집 같아요",
          "original_form": "그렇게 크지 않아서 겉에서 보면 그냥 집 같아요",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021626.1.16",
          "form": "오호...",
          "original_form": "오호...",
          "speaker_id": "2",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021626.1.17",
          "form": "동네에 그런데가 있다니 좋네용",
          "original_form": "동네에 그런데가 있다니 좋네용",
          "speaker_id": "2",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021626.1.18",
          "form": "교회는 친구 따라 같이 가셨나요?",
          "original_form": "교회는 친구 따라 같이 가셨나요?",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100021626.1.19",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021626.1.20",
          "form": "한번만 같이가자... 이런 친구들",
          "original_form": "한번만 같이가자... 이런 친구들",
          "speaker_id": "2",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021626.1.21",
          "form": "아니면 어렸을때 할머니 따라 가봤어요 ㅋㅋ",
          "original_form": "아니면 어렸을때 할머니 따라 가봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021626.1.22",
          "form": "오 어릴 때면 여름 성경 학교 같은 것도 가셨나요?",
          "original_form": "오 어릴 때면 여름 성경 학교 같은 것도 가셨나요?",
          "speaker_id": "1",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021626.1.23",
          "form": "아우 아녀아녀!!",
          "original_form": "아우 아녀아녀!!",
          "speaker_id": "2",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100021626.1.24",
          "form": "초등학생 때 교문 앞에서 홍보하는 건 봤는데",
          "original_form": "초등학생 때 교문 앞에서 홍보하는 건 봤는데",
          "speaker_id": "1",
          "time": "20210828 21:55"
        }
      ]
    }
