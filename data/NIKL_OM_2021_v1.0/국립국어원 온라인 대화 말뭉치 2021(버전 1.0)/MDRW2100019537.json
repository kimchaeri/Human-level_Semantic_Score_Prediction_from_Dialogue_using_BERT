      "utterance": [
        {
          "id": "MDRW2100019537.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100019537.1.2",
          "form": "아니요ㅋㅋ 세나개만 봐요ㅋㅋ",
          "original_form": "아니요ㅋㅋ 세나개만 봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019537.1.3",
          "form": "저도 예전에 햄스터 한번 키우고 지금은 안키우고있어요",
          "original_form": "저도 예전에 햄스터 한번 키우고 지금은 안키우고있어요",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019537.1.4",
          "form": "햄스터!",
          "original_form": "햄스터!",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019537.1.5",
          "form": "세나개 저도 가끔 보네요ㅋㅋ",
          "original_form": "세나개 저도 가끔 보네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019537.1.6",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019537.1.7",
          "form": "생각해보니까 저도 햄스터 잠깐 데리고 있던 적 있었던 것 같아요",
          "original_form": "생각해보니까 저도 햄스터 잠깐 데리고 있던 적 있었던 것 같아요",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019537.1.8",
          "form": "너무 어릴 때라 기억이 희미하네요",
          "original_form": "너무 어릴 때라 기억이 희미하네요",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019537.1.9",
          "form": "햄스터 오래 키우셨어요?",
          "original_form": "햄스터 오래 키우셨어요?",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019537.1.10",
          "form": "저도 완전 어렸을때 키웠는데 기간은 기억안나지만 오래 못살았어요 ㅠㅠ",
          "original_form": "저도 완전 어렸을때 키웠는데 기간은 기억안나지만 오래 못살았어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019537.1.11",
          "form": "햄스터는 손 많이 안타야 오래산다하더라구요..",
          "original_form": "햄스터는 손 많이 안타야 오래산다하더라구요..",
          "speaker_id": "2",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019537.1.12",
          "form": "작아서 그런가봐요",
          "original_form": "작아서 그런가봐요",
          "speaker_id": "1",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019537.1.13",
          "form": "병아리 키운 적 있는데 한번은 닭까지 크고",
          "original_form": "병아리 키운 적 있는데 한번은 닭까지 크고",
          "speaker_id": "1",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019537.1.14",
          "form": "한번은 금방 무지개다리 건넜었어요ㅠㅠ",
          "original_form": "한번은 금방 무지개다리 건넜었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019537.1.15",
          "form": "손 많이 타서 그랬던것 같아요",
          "original_form": "손 많이 타서 그랬던것 같아요",
          "speaker_id": "1",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019537.1.16",
          "form": "오 닭까지 키우셨다니 대단하시네요!!",
          "original_form": "오 닭까지 키우셨다니 대단하시네요!!",
          "speaker_id": "2",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019537.1.17",
          "form": "작은 동물들은 최대한 손안타는게 오래사나봐요 ㅠ",
          "original_form": "작은 동물들은 최대한 손안타는게 오래사나봐요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019537.1.18",
          "form": "그 병아리는 거의 어머니가 키우셨거든요ㅋㅋ",
          "original_form": "그 병아리는 거의 어머니가 키우셨거든요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100019537.1.19",
          "form": "어머니가 새 ",
          "original_form": "어머니가 새 ",
          "speaker_id": "1",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100019537.1.20",
          "form": "나중에 어머니 키우실 수 있게 입양할까 생각중이에요",
          "original_form": "나중에 어머니 키우실 수 있게 입양할까 생각중이에요",
          "speaker_id": "1",
          "time": "20210827 20:59"
        },
        {
          "id": "MDRW2100019537.1.21",
          "form": "오 좋네요 ..! 저는 나중에 고슴도치 한번 키워보고싶아요..!",
          "original_form": "오 좋네요 ..! 저는 나중에 고슴도치 한번 키워보고싶아요..!",
          "speaker_id": "2",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100019537.1.22",
          "form": "고슴도치 매력있어보여요",
          "original_form": "고슴도치 매력있어보여요",
          "speaker_id": "1",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100019537.1.23",
          "form": "그쵸 너무 귀엽고 매력있어요 !",
          "original_form": "그쵸 너무 귀엽고 매력있어요 !",
          "speaker_id": "2",
          "time": "20210827 21:04"
        }
      ]
