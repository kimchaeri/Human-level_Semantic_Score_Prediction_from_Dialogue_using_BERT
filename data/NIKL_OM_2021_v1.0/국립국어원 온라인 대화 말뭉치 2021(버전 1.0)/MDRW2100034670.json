      "utterance": [
        {
          "id": "MDRW2100034670.1.1",
          "form": "구매해보셨나요?",
          "original_form": "구매해보셨나요?",
          "speaker_id": "1",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100034670.1.2",
          "form": "저는 아직이요 \r\n케이크는 보통 프랜차이즈가서 많이 구매하게 되는거 같아요",
          "original_form": "저는 아직이요 \r\n케이크는 보통 프랜차이즈가서 많이 구매하게 되는거 같아요",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034670.1.3",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034670.1.4",
          "form": "파리바게트나 뚜레쥬르를 많이 가는거같아요",
          "original_form": "파리바게트나 뚜레쥬르를 많이 가는거같아요",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100034670.1.5",
          "form": "저두요ㅎㅎ",
          "original_form": "저두요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100034670.1.6",
          "form": "요새는 근데",
          "original_form": "요새는 근데",
          "speaker_id": "1",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100034670.1.7",
          "form": "빵값이 너무 비싸요 ㅜㅜ",
          "original_form": "빵값이 너무 비싸요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100034670.1.8",
          "form": "집 근처에 파바가 있어 케이크는 주로 파리바게트 이용하는 편이에요",
          "original_form": "집 근처에 파바가 있어 케이크는 주로 파리바게트 이용하는 편이에요",
          "speaker_id": "2",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100034670.1.9",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100034670.1.10",
          "form": "빵값이 계속 올라가요",
          "original_form": "빵값이 계속 올라가요",
          "speaker_id": "2",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100034670.1.11",
          "form": "접근성이 젤좋죠",
          "original_form": "접근성이 젤좋죠",
          "speaker_id": "1",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100034670.1.12",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "1",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100034670.1.13",
          "form": "우리나라 빵값이 다른나라랑 비교해서도 높더라구요..",
          "original_form": "우리나라 빵값이 다른나라랑 비교해서도 높더라구요..",
          "speaker_id": "1",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100034670.1.14",
          "form": "계란 값 오른다고 오르고, 우유/버터 값 오른다고 오르고..",
          "original_form": "계란 값 오른다고 오르고, 우유/버터 값 오른다고 오르고..",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100034670.1.15",
          "form": "하지만 다시 내려가는 경우는 절대 없죠ㅜㅜ",
          "original_form": "하지만 다시 내려가는 경우는 절대 없죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100034670.1.16",
          "form": "맞아요 너무해요 ㅠㅠ",
          "original_form": "맞아요 너무해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100034670.1.17",
          "form": "그렇다고 경제가 좋아지는거도 아니고요ㅠㅠ",
          "original_form": "그렇다고 경제가 좋아지는거도 아니고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100034670.1.18",
          "form": "그러니까요ㅠㅠ",
          "original_form": "그러니까요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034670.1.19",
          "form": "그래서 빵이나 디저트들 사는게 점점 부담스러워져요",
          "original_form": "그래서 빵이나 디저트들 사는게 점점 부담스러워져요",
          "speaker_id": "2",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034670.1.20",
          "form": "맞아요 ㅜ",
          "original_form": "맞아요 ㅜ",
          "speaker_id": "1",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034670.1.21",
          "form": "배는 안차고 비싼데 살만찌는 ㅎㅎ..",
          "original_form": "배는 안차고 비싼데 살만찌는 ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034670.1.22",
          "form": "정말 슬퍼지는 일이죠.. \r\n그래서 베이킹 직접 하시는 분들도 많은것 같아요",
          "original_form": "정말 슬퍼지는 일이죠.. \r\n그래서 베이킹 직접 하시는 분들도 많은것 같아요",
          "speaker_id": "2",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100034670.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
