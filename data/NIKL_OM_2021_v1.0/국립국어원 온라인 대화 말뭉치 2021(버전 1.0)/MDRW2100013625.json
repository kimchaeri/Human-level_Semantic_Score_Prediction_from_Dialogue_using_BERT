      "utterance": [
        {
          "id": "MDRW2100013625.1.1",
          "form": "엄마 계절어때요??",
          "original_form": "엄마 계절어때요??",
          "speaker_id": "2",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100013625.1.2",
          "form": "요즘 날씨",
          "original_form": "요즘 날씨",
          "speaker_id": "2",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013625.1.3",
          "form": "비가 와서 싫다",
          "original_form": "비가 와서 싫다",
          "speaker_id": "1",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013625.1.4",
          "form": "빨래안말라",
          "original_form": "빨래안말라",
          "speaker_id": "1",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013625.1.5",
          "form": "빨래 덜 말리면 냄새나는데 ㅠㅠ",
          "original_form": "빨래 덜 말리면 냄새나는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013625.1.6",
          "form": "빨리 가을 됐음 좋겠어요",
          "original_form": "빨리 가을 됐음 좋겠어요",
          "speaker_id": "2",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013625.1.7",
          "form": "그래나도",
          "original_form": "그래나도",
          "speaker_id": "1",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013625.1.8",
          "form": "엄마는 어느계절 좋아하세요??",
          "original_form": "엄마는 어느계절 좋아하세요??",
          "speaker_id": "2",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013625.1.9",
          "form": "봄",
          "original_form": "봄",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013625.1.10",
          "form": "꽃이 예뻐",
          "original_form": "꽃이 예뻐",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013625.1.11",
          "form": "저도 꽃 좋아하는데",
          "original_form": "저도 꽃 좋아하는데",
          "speaker_id": "2",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013625.1.12",
          "form": "근데 가을이 더 좋아요 선선하이",
          "original_form": "근데 가을이 더 좋아요 선선하이",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013625.1.13",
          "form": "그래빨리가을이왔으면좋겠다",
          "original_form": "그래빨리가을이왔으면좋겠다",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013625.1.14",
          "form": "장마같이비가계속오네",
          "original_form": "장마같이비가계속오네",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013625.1.15",
          "form": "어제밤에 진짜 무서웠는ㄷ0",
          "original_form": "어제밤에 진짜 무서웠는ㄷ0",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013625.1.16",
          "form": "천둥소리 들었어요??",
          "original_form": "천둥소리 들었어요??",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013625.1.17",
          "form": "그래 그래서 창문다닫고잤다",
          "original_form": "그래 그래서 창문다닫고잤다",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013625.1.18",
          "form": "무슨 집부서지는줄ㅋㅋㅋㅋ",
          "original_form": "무슨 집부서지는줄ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013625.1.19",
          "form": "별일없어서다행이지",
          "original_form": "별일없어서다행이지",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013625.1.20",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:26"
        }
      ]
