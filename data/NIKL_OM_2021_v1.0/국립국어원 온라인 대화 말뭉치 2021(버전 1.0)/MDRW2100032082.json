        },
        {
          "id": "MDRW2100032082.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032082.1.3",
          "form": "뮤지컬 보시는거 좋아하시나요?",
          "original_form": "뮤지컬 보시는거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032082.1.4",
          "form": "아직 뮤지컬을 본 적이 없어요",
          "original_form": "아직 뮤지컬을 본 적이 없어요",
          "speaker_id": "2",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032082.1.5",
          "form": "name1은 뮤지컬 좋아하시나요?",
          "original_form": "&name1&은 뮤지컬 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032082.1.6",
          "form": "사실 저도 공연장을 가본적은 많진 않은데",
          "original_form": "사실 저도 공연장을 가본적은 많진 않은데",
          "speaker_id": "1",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032082.1.7",
          "form": "dvd나 영화로 많이 봤어요",
          "original_form": "dvd나 영화로 많이 봤어요",
          "speaker_id": "1",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032082.1.8",
          "form": "그럼 어떤 뮤지컬 좋아하시나요?",
          "original_form": "그럼 어떤 뮤지컬 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100032082.1.9",
          "form": "해밀턴이라고 혹시 아실진 모르겠네요",
          "original_form": "해밀턴이라고 혹시 아실진 모르겠네요",
          "speaker_id": "1",
          "time": "20210905 12:16"
        },
        {
          "id": "MDRW2100032082.1.10",
          "form": "요즘은 이거 젤 좋아하고",
          "original_form": "요즘은 이거 젤 좋아하고",
          "speaker_id": "1",
          "time": "20210905 12:16"
        },
        {
          "id": "MDRW2100032082.1.11",
          "form": "어쩌면 해피엔딩이랑 팬레터도 좋아해요!",
          "original_form": "어쩌면 해피엔딩이랑 팬레터도 좋아해요!",
          "speaker_id": "1",
          "time": "20210905 12:16"
        },
        {
          "id": "MDRW2100032082.1.12",
          "form": "제가 뮤지컬을 잘 몰라서 그러는데 지금 말씀해주신 뮤지컬들이 대극장 뮤지컬들은 아니죠??",
          "original_form": "제가 뮤지컬을 잘 몰라서 그러는데 지금 말씀해주신 뮤지컬들이 대극장 뮤지컬들은 아니죠??",
          "speaker_id": "2",
          "time": "20210905 12:17"
        },
        {
          "id": "MDRW2100032082.1.13",
          "form": "어쩌면 해피엔딩은 소극장이고",
          "original_form": "어쩌면 해피엔딩은 소극장이고",
          "speaker_id": "1",
          "time": "20210905 12:18"
        },
        {
          "id": "MDRW2100032082.1.14",
          "form": "팬레터는 대극장이에요",
          "original_form": "팬레터는 대극장이에요",
          "speaker_id": "1",
          "time": "20210905 12:18"
        },
        {
          "id": "MDRW2100032082.1.15",
          "form": "해밀턴은 미국 뮤지컬인데 아직 한국에 온 적은 없어요 ㅠㅠ",
          "original_form": "해밀턴은 미국 뮤지컬인데 아직 한국에 온 적은 없어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 12:18"
        },
        {
          "id": "MDRW2100032082.1.16",
          "form": "그렇군요ㅠㅠ 그럼 팬레터는 대략 어떤 내용인가요?",
          "original_form": "그렇군요ㅠㅠ 그럼 팬레터는 대략 어떤 내용인가요?",
          "speaker_id": "2",
          "time": "20210905 12:18"
        },
        {
          "id": "MDRW2100032082.1.17",
          "form": "일제시대 시인과 소설가들을 다룬",
          "original_form": "일제시대 시인과 소설가들을 다룬",
          "speaker_id": "1",
          "time": "20210905 12:19"
        },
        {
          "id": "MDRW2100032082.1.18",
          "form": "스릴러라 해야 하나...?",
          "original_form": "스릴러라 해야 하나...?",
          "speaker_id": "1",
          "time": "20210905 12:19"
        },
        {
          "id": "MDRW2100032082.1.19",
          "form": "소설가의 죽음을 다룬 내용이에요",
          "original_form": "소설가의 죽음을 다룬 내용이에요",
          "speaker_id": "1",
          "time": "20210905 12:20"
        },
        {
          "id": "MDRW2100032082.1.20",
          "form": "오 팬레터 제목과 다른 반전내용이네여",
          "original_form": "오 팬레터 제목과 다른 반전내용이네여",
          "speaker_id": "2",
          "time": "20210905 12:20"
        },
        {
          "id": "MDRW2100032082.1.21",
          "form": "그럼 혹시 최근에 영화관에 가신 적은 있으신가요??",
          "original_form": "그럼 혹시 최근에 영화관에 가신 적은 있으신가요??",
          "speaker_id": "1",
          "time": "20210905 12:22"
        },
        {
          "id": "MDRW2100032082.1.22",
          "form": "최근은 코로나때문에 못갔네요ㅠㅠ",
          "original_form": "최근은 코로나때문에 못갔네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 12:23"
        }
      ]
