      "utterance": [
        {
          "id": "MDRW2100015333.1.1",
          "form": "남친이랑 잘맞아? ㅋㅋㅋ",
          "original_form": "남친이랑 잘맞아? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015333.1.2",
          "form": "웅ㅋㅋㅋㅋ 개그코드가 잘맞아",
          "original_form": "웅ㅋㅋㅋㅋ 개그코드가 잘맞아",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015333.1.3",
          "form": "오 어케만낫엉 ㅋㅋ",
          "original_form": "오 어케만낫엉 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015333.1.4",
          "form": "친구소개로! ㅎㅎ 넌 애인있어?",
          "original_form": "친구소개로! ㅎㅎ 넌 애인있어?",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015333.1.5",
          "form": "웅 근데 한국에없다 거의 없는거같아 걍",
          "original_form": "웅 근데 한국에없다 거의 없는거같아 걍",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015333.1.6",
          "form": "장거리연애인거야?",
          "original_form": "장거리연애인거야?",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015333.1.7",
          "form": "웅 .. 짜증나 겨울에 한국 다시와 교환학생갔어",
          "original_form": "웅 .. 짜증나 겨울에 한국 다시와 교환학생갔어",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015333.1.8",
          "form": "헉 그렇구나",
          "original_form": "헉 그렇구나",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015333.1.9",
          "form": "요즘 코로나때문에 외국생활도 힘들텐데",
          "original_form": "요즘 코로나때문에 외국생활도 힘들텐데",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015333.1.10",
          "form": "부럽당 남친이랑 잘지내는거같아섴ㅋㅋㅋ",
          "original_form": "부럽당 남친이랑 잘지내는거같아섴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015333.1.11",
          "form": "걱정많겠다",
          "original_form": "걱정많겠다",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015333.1.12",
          "form": "ㅋㅋㅋㅋ 걔가 걱정되진 않아",
          "original_form": "ㅋㅋㅋㅋ 걔가 걱정되진 않아",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015333.1.13",
          "form": "난 뭐 소박하게 사이좋게 잘 지내긴해ㅋㅋ",
          "original_form": "난 뭐 소박하게 사이좋게 잘 지내긴해ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015333.1.14",
          "form": "얼마나만났어? ㅋㅋ",
          "original_form": "얼마나만났어? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015333.1.15",
          "form": "이제 한 2년?",
          "original_form": "이제 한 2년?",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015333.1.16",
          "form": "오 꽤 만낫구낭 ㅋㅋ 결혼생각도 있엉? ㅋㅋ",
          "original_form": "오 꽤 만낫구낭 ㅋㅋ 결혼생각도 있엉? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015333.1.17",
          "form": "웅 결혼하면 좋을거같아",
          "original_form": "웅 결혼하면 좋을거같아",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015333.1.18",
          "form": "우와 보기좋당 ㅋㅋㅋ 계속 잘만났음 좋겟넹 ㅇ",
          "original_form": "우와 보기좋당 ㅋㅋㅋ 계속 잘만났음 좋겟넹 ㅇ",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015333.1.19",
          "form": "현실은 다르다지만 그래도 매일 같이지내면 더 재밌지않을까?",
          "original_form": "현실은 다르다지만 그래도 매일 같이지내면 더 재밌지않을까?",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015333.1.20",
          "form": "주위에 결혼한사람이 거의 없어서 잘 모르겠엉ㅋㅋ",
          "original_form": "주위에 결혼한사람이 거의 없어서 잘 모르겠엉ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015333.1.21",
          "form": "개그코드 맞기 어려운데 ㅋㅋ 잘맞으면 진짜 그럴거같아",
          "original_form": "개그코드 맞기 어려운데 ㅋㅋ 잘맞으면 진짜 그럴거같아",
          "speaker_id": "1",
          "time": "20210825 23:10"
        }
      ]
    }
