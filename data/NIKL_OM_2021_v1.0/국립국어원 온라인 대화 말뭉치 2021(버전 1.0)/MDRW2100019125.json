      "utterance": [
        {
          "id": "MDRW2100019125.1.1",
          "form": "점심은 먹고 나왔니?",
          "original_form": "점심은 먹고 나왔니?",
          "speaker_id": "1",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019125.1.2",
          "form": "나는 오늘 짜장밥..!! 근데 조금 짜서 밥을 다 먹었어 짜장은 남고ㅋㅋ",
          "original_form": "나는 오늘 짜장밥..!! 근데 조금 짜서 밥을 다 먹었어 짜장은 남고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019125.1.3",
          "form": "점심에 부침개 먹었어",
          "original_form": "점심에 부침개 먹었어",
          "speaker_id": "2",
          "time": "20210827 15:37"
        },
        {
          "id": "MDRW2100019125.1.4",
          "form": "오늘 비오잖아",
          "original_form": "오늘 비오잖아",
          "speaker_id": "2",
          "time": "20210827 15:37"
        },
        {
          "id": "MDRW2100019125.1.5",
          "form": "출근해야해서 막걸리 못 마신게 아쉽지ㅎㅎ",
          "original_form": "출근해야해서 막걸리 못 마신게 아쉽지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:37"
        },
        {
          "id": "MDRW2100019125.1.6",
          "form": "막걸리는 다음 기회에 ㅋㅋㄱ",
          "original_form": "막걸리는 다음 기회에 ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019125.1.7",
          "form": "부침개 맛있겠당",
          "original_form": "부침개 맛있겠당",
          "speaker_id": "1",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019125.1.8",
          "form": "파전? 부추전? 섞어서?",
          "original_form": "파전? 부추전? 섞어서?",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019125.1.9",
          "form": "버섯이랑 양파밖에 없어서 그것만 넣었어",
          "original_form": "버섯이랑 양파밖에 없어서 그것만 넣었어",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019125.1.10",
          "form": "김치부침개 할까 하다가 어제 김치볶음밥을 먹어서 과감하게 패스했지",
          "original_form": "김치부침개 할까 하다가 어제 김치볶음밥을 먹어서 과감하게 패스했지",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019125.1.11",
          "form": "버섯전이네 ㅋㅋㅋ",
          "original_form": "버섯전이네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019125.1.12",
          "form": "저녁에는 씨유가서 독도막걸리 있는지 봐봐야지",
          "original_form": "저녁에는 씨유가서 독도막걸리 있는지 봐봐야지",
          "speaker_id": "1",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019125.1.13",
          "form": "동생이 졸업하면서 군밤막걸리도 끝났어..",
          "original_form": "동생이 졸업하면서 군밤막걸리도 끝났어..",
          "speaker_id": "2",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100019125.1.14",
          "form": "왜  편의점에는 알밤막걸리만 있을까",
          "original_form": "왜  편의점에는 알밤막걸리만 있을까",
          "speaker_id": "2",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100019125.1.15",
          "form": "알밤이 유통하기 편해서?",
          "original_form": "알밤이 유통하기 편해서?",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019125.1.16",
          "form": "군밤은 자기들끼리 먹으려고?",
          "original_form": "군밤은 자기들끼리 먹으려고?",
          "speaker_id": "2",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019125.1.17",
          "form": "아니아니 맛이 달라지니까?",
          "original_form": "아니아니 맛이 달라지니까?",
          "speaker_id": "1",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019125.1.18",
          "form": "유통중에 문제가 생길수도 있고.. 괜히 지역막걸리겠어 ?",
          "original_form": "유통중에 문제가 생길수도 있고.. 괜히 지역막걸리겠어 ?",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019125.1.19",
          "form": "하긴 여기저기서 다 먹을 수 있으면 굳이 그 지역가서 또 안먹겠지",
          "original_form": "하긴 여기저기서 다 먹을 수 있으면 굳이 그 지역가서 또 안먹겠지",
          "speaker_id": "2",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019125.1.20",
          "form": "그치 그런거지",
          "original_form": "그치 그런거지",
          "speaker_id": "1",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019125.1.21",
          "form": "성심당이랑 군밤막걸리 때문에 가보고 싶다",
          "original_form": "성심당이랑 군밤막걸리 때문에 가보고 싶다",
          "speaker_id": "1",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019125.1.22",
          "form": "일단 저녁에 뭐 먹을지는 정해진건가ㅋㅋ",
          "original_form": "일단 저녁에 뭐 먹을지는 정해진건가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:05"
        },
        {
          "id": "MDRW2100019125.1.23",
          "form": "응? 막걸리는 구경하러.. 사진않을거야",
          "original_form": "응? 막걸리는 구경하러.. 사진않을거야",
          "speaker_id": "1",
          "time": "20210827 16:06"
        },
        {
          "id": "MDRW2100019125.1.24",
          "form": "넌 뭐먹을려구..?",
          "original_form": "넌 뭐먹을려구..?",
          "speaker_id": "1",
          "time": "20210827 16:07"
        },
        {
          "id": "MDRW2100019125.1.25",
          "form": "아직 생각중",
          "original_form": "아직 생각중",
          "speaker_id": "2",
          "time": "20210827 16:07"
        }
      ]
