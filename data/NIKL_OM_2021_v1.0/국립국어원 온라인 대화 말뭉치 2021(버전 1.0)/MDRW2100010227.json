        },
        {
          "id": "MDRW2100010227.1.3",
          "form": "연애중이신가요",
          "original_form": "연애중이신가요",
          "speaker_id": "2",
          "time": "20210823 16:40"
        },
        {
          "id": "MDRW2100010227.1.4",
          "form": "아니요\r\n연애중이신가요?",
          "original_form": "아니요\r\n연애중이신가요?",
          "speaker_id": "1",
          "time": "20210823 16:44"
        },
        {
          "id": "MDRW2100010227.1.5",
          "form": "아니요 ㅎㅎ 결혼했어요",
          "original_form": "아니요 ㅎㅎ 결혼했어요",
          "speaker_id": "2",
          "time": "20210823 17:25"
        },
        {
          "id": "MDRW2100010227.1.6",
          "form": "오 결혼 좋나요?",
          "original_form": "오 결혼 좋나요?",
          "speaker_id": "1",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010227.1.7",
          "form": "장단점이 있죠!",
          "original_form": "장단점이 있죠!",
          "speaker_id": "2",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100010227.1.8",
          "form": "장점은 무엇인가요?",
          "original_form": "장점은 무엇인가요?",
          "speaker_id": "1",
          "time": "20210824 10:43"
        },
        {
          "id": "MDRW2100010227.1.9",
          "form": "동반자가 있죠",
          "original_form": "동반자가 있죠",
          "speaker_id": "2",
          "time": "20210824 11:08"
        },
        {
          "id": "MDRW2100010227.1.10",
          "form": "단점은",
          "original_form": "단점은",
          "speaker_id": "2",
          "time": "20210824 11:08"
        },
        {
          "id": "MDRW2100010227.1.11",
          "form": "동반자가 있죠",
          "original_form": "동반자가 있죠",
          "speaker_id": "2",
          "time": "20210824 11:08"
        },
        {
          "id": "MDRW2100010227.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:15"
        },
        {
          "id": "MDRW2100010227.1.13",
          "form": "그래도 후회는안하시죠?",
          "original_form": "그래도 후회는안하시죠?",
          "speaker_id": "1",
          "time": "20210824 11:15"
        },
        {
          "id": "MDRW2100010227.1.14",
          "form": "네ㅋㅋ 그건 맞아요",
          "original_form": "네ㅋㅋ 그건 맞아요",
          "speaker_id": "2",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100010227.1.15",
          "form": "좋네요ㅋㅋㅋ",
          "original_form": "좋네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:34"
        },
        {
          "id": "MDRW2100010227.1.16",
          "form": "결혼하고 만족하시는분들이 더 많나요?",
          "original_form": "결혼하고 만족하시는분들이 더 많나요?",
          "speaker_id": "1",
          "time": "20210824 11:34"
        },
        {
          "id": "MDRW2100010227.1.17",
          "form": "ㅋㅋ결혼보다 이혼이 적으니...",
          "original_form": "ㅋㅋ결혼보다 이혼이 적으니...",
          "speaker_id": "2",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100010227.1.18",
          "form": "주거가 해결되면 더 늘어날텐데",
          "original_form": "주거가 해결되면 더 늘어날텐데",
          "speaker_id": "2",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100010227.1.19",
          "form": "맞아요 요즘은",
          "original_form": "맞아요 요즘은",
          "speaker_id": "1",
          "time": "20210824 12:46"
        },
        {
          "id": "MDRW2100010227.1.20",
          "form": "집값이 어마무시하죠",
