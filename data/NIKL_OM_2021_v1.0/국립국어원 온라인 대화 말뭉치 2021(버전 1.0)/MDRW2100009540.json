      "utterance": [
        {
          "id": "MDRW2100009540.1.1",
          "form": "저 자러가요 ㅎㅎ",
          "original_form": "저 자러가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:21"
        },
        {
          "id": "MDRW2100009540.1.2",
          "form": "굿반되세요 ㅎㅎ",
          "original_form": "굿반되세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009540.1.3",
          "form": "굿밤 되세요  ㅎㅎ",
          "original_form": "굿밤 되세요  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009540.1.4",
          "form": "좋은정보 감사합니다",
          "original_form": "좋은정보 감사합니다",
          "speaker_id": "1",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009540.1.5",
          "form": "또봐요 ㅎㅎ",
          "original_form": "또봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009540.1.6",
          "form": "굳밤요 ㅋㅋ이야기 잘 들어주셔서 감사해요!",
          "original_form": "굳밤요 ㅋㅋ이야기 잘 들어주셔서 감사해요!",
          "speaker_id": "2",
          "time": "20210823 00:24"
        },
        {
          "id": "MDRW2100009540.1.7",
          "form": "네 ㅎㅎ저도요 감사해요 ㅎㅎ",
          "original_form": "네 ㅎㅎ저도요 감사해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:24"
        },
        {
          "id": "MDRW2100009540.1.8",
          "form": "근데 저녁으로 단백질 쉐이크는 빈약해여",
          "original_form": "근데 저녁으로 단백질 쉐이크는 빈약해여",
          "speaker_id": "1",
          "time": "20210823 18:28"
        },
        {
          "id": "MDRW2100009540.1.9",
          "form": "안됩니다 ㅋㅋ",
          "original_form": "안됩니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:28"
        },
        {
          "id": "MDRW2100009540.1.10",
          "form": "낮에는 장어 먹었어요",
          "original_form": "낮에는 장어 먹었어요",
          "speaker_id": "2",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100009540.1.11",
          "form": "근데 그거 초코맛 진짜 맛있어요",
          "original_form": "근데 그거 초코맛 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100009540.1.12",
          "form": "저녁 딱 한끼만 참고ㅜ",
          "original_form": "저녁 딱 한끼만 참고ㅜ",
          "speaker_id": "2",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100009540.1.13",
          "form": "세상에 제대로 드셨네요",
          "original_form": "세상에 제대로 드셨네요",
          "speaker_id": "1",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100009540.1.14",
          "form": "그럼 참아도 돼요",
          "original_form": "그럼 참아도 돼요",
          "speaker_id": "1",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100009540.1.15",
          "form": "쉐이크로 바쿠려고용",
          "original_form": "쉐이크로 바쿠려고용",
          "speaker_id": "2",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100009540.1.16",
          "form": "전 전이랑 술한잔 ㅋㅋ",
          "original_form": "전 전이랑 술한잔 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100009540.1.17",
          "form": "네 무슨맛 이요?",
          "original_form": "네 무슨맛 이요?",
          "speaker_id": "1",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100009540.1.18",
          "form": "맛도 다양한데",
          "original_form": "맛도 다양한데",
          "speaker_id": "1",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100009540.1.19",
          "form": "초코 하세요",
          "original_form": "초코 하세요",
          "speaker_id": "1",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100009540.1.20",
          "form": "저도 한때 먹었어요",
          "original_form": "저도 한때 먹었어요",
          "speaker_id": "1",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100009540.1.21",
          "form": "방황하다가ㅜ 집에 얼른 가야겠어요",
          "original_form": "방황하다가ㅜ 집에 얼른 가야겠어요",
          "speaker_id": "2",
          "time": "20210823 18:30"
        },
        {
          "id": "MDRW2100009540.1.22",
          "form": "사먹고싶은게 너무많아서",
          "original_form": "사먹고싶은게 너무많아서",
          "speaker_id": "2",
          "time": "20210823 18:30"
        },
        {
          "id": "MDRW2100009540.1.23",
          "form": "안되겠네요",
          "original_form": "안되겠네요",
          "speaker_id": "2",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100009540.1.24",
          "form": "맞아요 넘 보면 사고 싶어져요",
          "original_form": "맞아요 넘 보면 사고 싶어져요",
          "speaker_id": "1",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100009540.1.25",
          "form": "쉐이크는 사셨어요?",
          "original_form": "쉐이크는 사셨어요?",
          "speaker_id": "1",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100009540.1.26",
          "form": "주위가 막집이죠?",
          "original_form": "주위가 막집이죠?",
          "speaker_id": "1",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100009540.1.27",
          "form": "맛집",
          "original_form": "맛집",
          "speaker_id": "1",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100009540.1.28",
          "form": "비도 오고 어서 가서 쉬면서 쉐이크 하세요",
          "original_form": "비도 오고 어서 가서 쉬면서 쉐이크 하세요",
          "speaker_id": "1",
          "time": "20210823 18:32"
        },
        {
          "id": "MDRW2100009540.1.29",
          "form": "혹 편의점 보임 샐러드라도 ㅋㅋ",
          "original_form": "혹 편의점 보임 샐러드라도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100009540.1.30",
          "form": "크레페가 순간 눈에들어와서",
          "original_form": "크레페가 순간 눈에들어와서",
          "speaker_id": "2",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100009540.1.31",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100009540.1.32",
          "form": "잠시 저거 하나만 사가면안되나",
          "original_form": "잠시 저거 하나만 사가면안되나",
          "speaker_id": "2",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100009540.1.33",
          "form": "했다가",
          "original_form": "했다가",
          "speaker_id": "2",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100009540.1.34",
          "form": "안돼요 ㅋㅋ",
          "original_form": "안돼요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100009540.1.35",
          "form": "다시 정신을 차렸습니다",
          "original_form": "다시 정신을 차렸습니다",
          "speaker_id": "2",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100009540.1.36",
          "form": "아 저라도 사가고 싶어요",
          "original_form": "아 저라도 사가고 싶어요",
          "speaker_id": "1",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100009540.1.37",
          "form": "대단하세요 참기 힘들어요",
          "original_form": "대단하세요 참기 힘들어요",
          "speaker_id": "1",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100009540.1.38",
          "form": "주변이 완전 맛집이네요",
          "original_form": "주변이 완전 맛집이네요",
          "speaker_id": "1",
          "time": "20210823 18:34"
        },
        {
          "id": "MDRW2100009540.1.39",
          "form": "여긴 크레페점 없어요 ㅜ",
          "original_form": "여긴 크레페점 없어요 ㅜ",
          "speaker_id": "1",
          "time": "20210823 18:34"
        }
      ]
    }
