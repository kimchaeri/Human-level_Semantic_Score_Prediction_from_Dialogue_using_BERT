        },
        {
          "id": "MDRW2100046442.1.9",
          "form": "면음식 넘 맛있죠ㅠㅠ",
          "original_form": "면음식 넘 맛있죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20211019 13:57"
        },
        {
          "id": "MDRW2100046442.1.10",
