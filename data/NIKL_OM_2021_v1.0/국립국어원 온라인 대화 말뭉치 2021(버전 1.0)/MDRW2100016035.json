      "utterance": [
        {
          "id": "MDRW2100016035.1.1",
          "form": "네네..",
          "original_form": "네네..",
          "speaker_id": "2",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100016035.1.2",
          "form": "어쩌다가..",
          "original_form": "어쩌다가..",
          "speaker_id": "1",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100016035.1.3",
          "form": "먹고 토..했어요",
          "original_form": "먹고 토..했어요",
          "speaker_id": "2",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100016035.1.4",
          "form": "네네는 피하세요ㅠ",
          "original_form": "네네는 피하세요ㅠ",
          "speaker_id": "1",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100016035.1.5",
          "form": "그..치킨이",
          "original_form": "그..치킨이",
          "speaker_id": "2",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100016035.1.6",
          "form": "너무안좋아서",
          "original_form": "너무안좋아서",
          "speaker_id": "2",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100016035.1.7",
          "form": "저 잘먹는데",
          "original_form": "저 잘먹는데",
          "speaker_id": "2",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100016035.1.8",
          "form": "처음으로 토하고 난리남",
          "original_form": "처음으로 토하고 난리남",
          "speaker_id": "2",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100016035.1.9",
          "form": "동생과 함께",
          "original_form": "동생과 함께",
          "speaker_id": "2",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100016035.1.10",
          "form": "닭상태가 안좋았나봐요ㅠ",
          "original_form": "닭상태가 안좋았나봐요ㅠ",
          "speaker_id": "1",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100016035.1.11",
          "form": "네에ㅜㅜ",
          "original_form": "네에ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100016035.1.12",
          "form": "고생하셨겠어요",
          "original_form": "고생하셨겠어요",
          "speaker_id": "1",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100016035.1.13",
          "form": "치킨먹을 때 사이다 콜라 어떤거드세요",
          "original_form": "치킨먹을 때 사이다 콜라 어떤거드세요",
          "speaker_id": "1",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100016035.1.14",
          "form": "저는 콜라요",
          "original_form": "저는 콜라요",
          "speaker_id": "2",
          "time": "20210826 08:28"
        },
        {
          "id": "MDRW2100016035.1.15",
          "form": "요즘엔 맥주",
          "original_form": "요즘엔 맥주",
          "speaker_id": "2",
          "time": "20210826 08:28"
        },
        {
          "id": "MDRW2100016035.1.16",
          "form": "그렇죠 치킨에는 맥주죠ㅎ",
          "original_form": "그렇죠 치킨에는 맥주죠ㅎ",
          "speaker_id": "1",
          "time": "20210826 08:28"
        },
        {
          "id": "MDRW2100016035.1.17",
          "form": "치킨한 입먹고 맥주한모금 완벽하네요",
          "original_form": "치킨한 입먹고 맥주한모금 완벽하네요",
          "speaker_id": "1",
          "time": "20210826 08:28"
        },
        {
          "id": "MDRW2100016035.1.18",
          "form": "치킨 무는 드세요?",
          "original_form": "치킨 무는 드세요?",
          "speaker_id": "1",
          "time": "20210826 08:29"
        },
        {
          "id": "MDRW2100016035.1.19",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210826 08:29"
        },
        {
          "id": "MDRW2100016035.1.20",
          "form": "치킨무 ㅈㅁㅌ",
          "original_form": "치킨무 ㅈㅁㅌ",
          "speaker_id": "2",
          "time": "20210826 08:29"
        },
        {
          "id": "MDRW2100016035.1.21",
          "form": "역시 치킨에는 무가 빠질 수 없죠",
          "original_form": "역시 치킨에는 무가 빠질 수 없죠",
          "speaker_id": "1",
          "time": "20210826 08:30"
        },
        {
          "id": "MDRW2100016035.1.22",
          "form": "약간 입가심하기 좋은 맛",
          "original_form": "약간 입가심하기 좋은 맛",
          "speaker_id": "1",
          "time": "20210826 08:30"
        },
        {
          "id": "MDRW2100016035.1.23",
          "form": "달고 새콤하고",
          "original_form": "달고 새콤하고",
          "speaker_id": "1",
          "time": "20210826 08:30"
        },
        {
          "id": "MDRW2100016035.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 08:30"
        }
      ]
