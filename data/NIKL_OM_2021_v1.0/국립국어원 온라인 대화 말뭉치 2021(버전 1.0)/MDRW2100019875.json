      "utterance": [
        {
          "id": "MDRW2100019875.1.1",
          "form": "제출했습니다 유투브 자주 보시나요",
          "original_form": "제출했습니다 유투브 자주 보시나요",
          "speaker_id": "1",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019875.1.2",
          "form": "출퇴근 중에 노래 들을 때 유투브로 봐요",
          "original_form": "출퇴근 중에 노래 들을 때 유투브로 봐요",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019875.1.3",
          "form": "저도 유튜브로 음악을 자주들어요",
          "original_form": "저도 유튜브로 음악을 자주들어요",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019875.1.4",
          "form": "최근에는 무한도전 보고있어요",
          "original_form": "최근에는 무한도전 보고있어요",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019875.1.5",
          "form": "아 유튜브에 무한도전 전부 다 올라와 있는거예요???",
          "original_form": "아 유튜브에 무한도전 전부 다 올라와 있는거예요???",
          "speaker_id": "2",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019875.1.6",
          "form": "대다수 올라와있어요",
          "original_form": "대다수 올라와있어요",
          "speaker_id": "1",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019875.1.7",
          "form": "아항 저 같은 경우 유투브로는 거의 노래만 듣고",
          "original_form": "아항 저 같은 경우 유투브로는 거의 노래만 듣고",
          "speaker_id": "2",
          "time": "20210827 22:39"
        },
        {
          "id": "MDRW2100019875.1.8",
          "form": "그런 영상은 주로 넷플릭스로 많이봐요",
          "original_form": "그런 영상은 주로 넷플릭스로 많이봐요",
          "speaker_id": "2",
          "time": "20210827 22:39"
        },
        {
          "id": "MDRW2100019875.1.9",
          "form": "넷플릭스 좋죠 추천할만한게 있으신가요",
          "original_form": "넷플릭스 좋죠 추천할만한게 있으신가요",
          "speaker_id": "1",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019875.1.10",
          "form": "최근에 슬기로운 의사생활 시즌2 열심히 보고있어요 ㅎ",
          "original_form": "최근에 슬기로운 의사생활 시즌2 열심히 보고있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019875.1.11",
          "form": "강추드리는거는 시그널 강추드려요!",
          "original_form": "강추드리는거는 시그널 강추드려요!",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019875.1.12",
          "form": "슬기로운 재밌나요.",
          "original_form": "슬기로운 재밌나요.",
          "speaker_id": "1",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019875.1.13",
          "form": "네 개꿀잼이예요",
          "original_form": "네 개꿀잼이예요",
          "speaker_id": "2",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019875.1.14",
          "form": "시즌1도 본방사수는 못 해도 다음날에 꾸준히 봤어요",
          "original_form": "시즌1도 본방사수는 못 해도 다음날에 꾸준히 봤어요",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019875.1.15",
          "form": "나중에 봐야겠네요!",
          "original_form": "나중에 봐야겠네요!",
          "speaker_id": "1",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019875.1.16",
          "form": "전 괴물 넷플에서",
          "original_form": "전 괴물 넷플에서",
          "speaker_id": "1",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019875.1.17",
          "form": "재미나게봤어요",
          "original_form": "재미나게봤어요",
          "speaker_id": "1",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019875.1.18",
          "form": "아 괴물 개재밌다고 하던데",
          "original_form": "아 괴물 개재밌다고 하던데",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019875.1.19",
          "form": "재밌습니다",
          "original_form": "재밌습니다",
          "speaker_id": "1",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019875.1.20",
          "form": "추천드려요",
          "original_form": "추천드려요",
          "speaker_id": "1",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019875.1.21",
          "form": "네엡 알겠습니다 시간 날 때 꼭 봐야겠네요",
          "original_form": "네엡 알겠습니다 시간 날 때 꼭 봐야겠네요",
          "speaker_id": "2",
          "time": "20210827 22:46"
        }
