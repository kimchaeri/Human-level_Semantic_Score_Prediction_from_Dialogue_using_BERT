      "utterance": [
        {
          "id": "MDRW2100033672.1.1",
          "form": "근데 과자10봉에 3천얼마밖에안해요?",
          "original_form": "근데 과자10봉에 3천얼마밖에안해요?",
          "speaker_id": "1",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100033672.1.2",
          "form": "실결제가 그렇다는거죠?",
          "original_form": "실결제가 그렇다는거죠?",
          "speaker_id": "1",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100033672.1.3",
          "form": "실결제늘3520원이구요",
          "original_form": "실결제늘3520원이구요",
          "speaker_id": "2",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100033672.1.4",
          "form": "우친배달하는건 다르긴한데",
          "original_form": "우친배달하는건 다르긴한데",
          "speaker_id": "2",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100033672.1.5",
          "form": "전가까워서2700원 나오더라구요",
          "original_form": "전가까워서2700원 나오더라구요",
          "speaker_id": "2",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100033672.1.6",
          "form": "오 누가 채가면 어째요?ㅋ",
          "original_form": "오 누가 채가면 어째요?ㅋ",
          "speaker_id": "1",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100033672.1.7",
          "form": "매장가서 해요 전ㅋㅋ",
          "original_form": "매장가서 해요 전ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100033672.1.8",
          "form": "내가 픽업해야되는데",
          "original_form": "내가 픽업해야되는데",
          "speaker_id": "1",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100033672.1.9",
          "form": "복불복ㅋㄱㅋ",
          "original_form": "복불복ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100033672.1.10",
          "form": "근데. 거기서주문 넣고 몇분뒤에",
          "original_form": "근데. 거기서주문 넣고 몇분뒤에",
          "speaker_id": "2",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100033672.1.11",
          "form": "ㅎㅎ 실패한적없어요?",
          "original_form": "ㅎㅎ 실패한적없어요?",
          "speaker_id": "1",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100033672.1.12",
          "form": "포스기에서 수락 누르면",
          "original_form": "포스기에서 수락 누르면",
          "speaker_id": "2",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100033672.1.13",
          "form": "우친에떠요",
          "original_form": "우친에떠요",
          "speaker_id": "2",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100033672.1.14",
          "form": "저도 오늘첨이라서ㅋㅋㅋ",
          "original_form": "저도 오늘첨이라서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100033672.1.15",
          "form": "아ㅎ 편의점입구서 주문하고대기해야되나요ㅋㅋ",
          "original_form": "아ㅎ 편의점입구서 주문하고대기해야되나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100033672.1.16",
          "form": "근데 첨에 뜨고 시간좀 지나면 배달비올라간다는데 그거는 누가 채갈까봐ㅎ 못하고",
          "original_form": "근데 첨에 뜨고 시간좀 지나면 배달비올라간다는데 그거는 누가 채갈까봐ㅎ 못하고",
          "speaker_id": "2",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100033672.1.17",
          "form": "바로잡았어요",
          "original_form": "바로잡았어요",
          "speaker_id": "2",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100033672.1.18",
          "form": "전 gs25. 사장님하고 친해서",
          "original_form": "전 gs25. 사장님하고 친해서",
          "speaker_id": "2",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100033672.1.19",
          "form": "안에서 이야기하면서 했어요",
          "original_form": "안에서 이야기하면서 했어요",
          "speaker_id": "2",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100033672.1.20",
          "form": "ㅎㅎ 친하셔요? 얼마나 자주가시길래",
          "original_form": "ㅎㅎ 친하셔요? 얼마나 자주가시길래",
          "speaker_id": "1",
          "time": "20210824 22:36"
        },
        {
          "id": "MDRW2100033672.1.21",
          "form": "사장님들은 싫어하시지않아요?",
          "original_form": "사장님들은 싫어하시지않아요?",
          "speaker_id": "1",
          "time": "20210824 22:36"
        },
        {
          "id": "MDRW2100033672.1.22",
          "form": "좋아하시던데요?고맙데요ㅋㅋ",
          "original_form": "좋아하시던데요?고맙데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:36"
        },
        {
          "id": "MDRW2100033672.1.23",
          "form": "본사에서 나오니까 그런가봐요",
          "original_form": "본사에서 나오니까 그런가봐요",
          "speaker_id": "2",
          "time": "20210824 22:37"
        },
        {
          "id": "MDRW2100033672.1.24",
          "form": "아 손해보시는건 없나보네요",
          "original_form": "아 손해보시는건 없나보네요",
          "speaker_id": "1",
          "time": "20210824 22:37"
        },
        {
          "id": "MDRW2100033672.1.25",
          "form": "깁콘물건 없거나 핫딜뜨면 선주문 해주시니 친해요",
          "original_form": "깁콘물건 없거나 핫딜뜨면 선주문 해주시니 친해요",
          "speaker_id": "2",
          "time": "20210824 22:37"
        },
        {
          "id": "MDRW2100033672.1.26",
          "form": "친화력 짱이시군요ㅎ",
          "original_form": "친화력 짱이시군요ㅎ",
          "speaker_id": "1",
          "time": "20210824 22:38"
        }
      ]
