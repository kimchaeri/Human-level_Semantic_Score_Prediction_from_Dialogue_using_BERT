      "utterance": [
        {
          "id": "MDRW2100018778.1.1",
          "form": "오늘 점심은 뭐 드실생각이신가요??",
          "original_form": "오늘 점심은 뭐 드실생각이신가요??",
          "speaker_id": "1",
          "time": "20210827 12:02"
        },
        {
          "id": "MDRW2100018778.1.2",
          "form": "간장게장이요! 좋아하시나요?",
          "original_form": "간장게장이요! 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100018778.1.3",
          "form": "헐 못먹어요..",
          "original_form": "헐 못먹어요..",
          "speaker_id": "1",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100018778.1.4",
          "form": "없어서 못먹어요....",
          "original_form": "없어서 못먹어요....",
          "speaker_id": "1",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100018778.1.5",
          "form": "너무 좋아합니다",
          "original_form": "너무 좋아합니다",
          "speaker_id": "1",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100018778.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ해산물못드시는줄알았어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ해산물못드시는줄알았어요",
          "speaker_id": "2",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100018778.1.7",
          "form": "전 가리지않고 다 잘먹는데",
          "original_form": "전 가리지않고 다 잘먹는데",
          "speaker_id": "1",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100018778.1.8",
          "form": "막입이라 맛없는것도 대충 잘 먹어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "막입이라 맛없는것도 대충 잘 먹어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100018778.1.9",
          "form": "오 곱창도 드세요?",
          "original_form": "오 곱창도 드세요?",
          "speaker_id": "2",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100018778.1.10",
          "form": "그럼요!아주좋아해요 ㅎㅎ",
          "original_form": "그럼요!아주좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100018778.1.11",
          "form": "어머 닭발은요?",
          "original_form": "어머 닭발은요?",
          "speaker_id": "2",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018778.1.12",
          "form": "그것역시 잘먹습니다",
          "original_form": "그것역시 잘먹습니다",
          "speaker_id": "1",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018778.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018778.1.14",
          "form": "못드시는거까지 내볼게요ㅋㅋㅋㅋ오기생기네요",
          "original_form": "못드시는거까지 내볼게요ㅋㅋㅋㅋ오기생기네요",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018778.1.15",
          "form": "가지도 드세요????",
          "original_form": "가지도 드세요????",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018778.1.16",
          "form": "가지 먹긴하지만",
          "original_form": "가지 먹긴하지만",
          "speaker_id": "1",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018778.1.17",
          "form": "안좋아해요 ㅋㅋㅋㅋ",
          "original_form": "안좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018778.1.18",
          "form": "그 흐물흐물 가지 탈락!!",
          "original_form": "그 흐물흐물 가지 탈락!!",
          "speaker_id": "1",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018778.1.19",
          "form": "호오 도라지는요?",
          "original_form": "호오 도라지는요?",
          "speaker_id": "2",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018778.1.20",
          "form": "도라지도 먹어요!!",
          "original_form": "도라지도 먹어요!!",
          "speaker_id": "1",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018778.1.21",
          "form": "민트초코는여????",
          "original_form": "민트초코는여????",
          "speaker_id": "2",
          "time": "20210827 12:09"
        },
        {
