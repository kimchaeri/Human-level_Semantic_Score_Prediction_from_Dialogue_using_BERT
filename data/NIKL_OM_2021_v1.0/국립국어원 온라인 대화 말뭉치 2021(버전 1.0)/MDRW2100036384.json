      "utterance": [
        {
          "id": "MDRW2100036384.1.1",
          "form": "완전 얼음을 꽉 채워주셔서 좋어요",
          "original_form": "완전 얼음을 꽉 채워주셔서 좋어요",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100036384.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100036384.1.3",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100036384.1.4",
          "form": "간얼음이 되는구나",
          "original_form": "간얼음이 되는구나",
          "speaker_id": "1",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100036384.1.5",
          "form": "네네 !! 간얼음을 달라하면 갈아서 넣어줘요!!!",
          "original_form": "네네 !! 간얼음을 달라하면 갈아서 넣어줘요!!!",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100036384.1.6",
          "form": "저 그거",
          "original_form": "저 그거",
          "speaker_id": "1",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100036384.1.7",
          "form": "좀지나면 ㅋㅋㅋ슬러시같아요 ㅋㅋㅋㅋ",
          "original_form": "좀지나면 ㅋㅋㅋ슬러시같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100036384.1.8",
          "form": "커피빈만 되는줄알고 살아왔어요ㅋㅋㅋㅋ",
          "original_form": "커피빈만 되는줄알고 살아왔어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100036384.1.9",
          "form": "살얼음같이 ㅋㅋㅋ",
          "original_form": "살얼음같이 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100036384.1.10",
          "form": "와 맛있겠다ㅠㅠ",
          "original_form": "와 맛있겠다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100036384.1.11",
          "form": "빽스는 큰얼음을 갈아서 넣어주는거에여!",
          "original_form": "빽스는 큰얼음을 갈아서 넣어주는거에여!",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100036384.1.12",
          "form": "근데 더벤티나 커피빈은 제빙기가 간얼음으로 나오는거같더라구요!",
          "original_form": "근데 더벤티나 커피빈은 제빙기가 간얼음으로 나오는거같더라구요!",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100036384.1.13",
          "form": "오늘은 더벤티갓는데 얼음제빙기가",
          "original_form": "오늘은 더벤티갓는데 얼음제빙기가",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100036384.1.14",
          "form": "아하 저희 집앞에 빽다방 있는데 가성비 좋아서 그런지 항상 사람 많아요",
          "original_form": "아하 저희 집앞에 빽다방 있는데 가성비 좋아서 그런지 항상 사람 많아요",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100036384.1.15",
          "form": "아에 간얼음 !",
          "original_form": "아에 간얼음 !",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100036384.1.16",
          "form": "더벤티도 간얼음이구나... 세상 좋아졌네요 ㅡㅋㅋㅋㅋ",
          "original_form": "더벤티도 간얼음이구나... 세상 좋아졌네요 ㅡㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100036384.1.17",
          "form": "맞아요! 저희동네 사장님도 친절하셔서",
          "original_form": "맞아요! 저희동네 사장님도 친절하셔서",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100036384.1.18",
          "form": "좋아요!",
          "original_form": "좋아요!",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100036384.1.19",
          "form": "이디야 돈주고 먹을바엔",
          "original_form": "이디야 돈주고 먹을바엔",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100036384.1.20",
          "form": "아아는 더벤티나 빽다방이죵ㅋㅋㅋㅋ",
          "original_form": "아아는 더벤티나 빽다방이죵ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100036384.1.21",
          "form": "이디야도 가까운데 잘안가게되요",
          "original_form": "이디야도 가까운데 잘안가게되요",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100036384.1.22",
          "form": "네ㅠㅠ 그래서 허니브레드?",
          "original_form": "네ㅠㅠ 그래서 허니브레드?",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100036384.1.23",
          "form": "그건 맛있는데",
          "original_form": "그건 맛있는데",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100036384.1.24",
          "form": "아 이거 맛있는곳이",
          "original_form": "아 이거 맛있는곳이",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100036384.1.25",
          "form": "탐앤탐스였다@",
          "original_form": "탐앤탐스였다@",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100036384.1.26",
          "form": "탐탐은 진짜안간지오래됏네요",
          "original_form": "탐탐은 진짜안간지오래됏네요",
          "speaker_id": "2",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100036384.1.27",
          "form": "프레첼 마니먹엇는데",
          "original_form": "프레첼 마니먹엇는데",
          "speaker_id": "2",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100036384.1.28",
          "form": "저복숭아깍아야해서 못햐요ㅠㅠ",
          "original_form": "저복숭아깍아야해서 못햐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100036384.1.29",
          "form": "맞아요 프레첼 맛있어용ㅎㅎ",
          "original_form": "맞아요 프레첼 맛있어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:22"
        }
      ]
    }
