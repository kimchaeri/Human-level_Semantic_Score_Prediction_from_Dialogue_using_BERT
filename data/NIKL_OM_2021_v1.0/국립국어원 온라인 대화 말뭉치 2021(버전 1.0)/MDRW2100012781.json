      "utterance": [
        {
          "id": "MDRW2100012781.1.1",
          "form": "오 잘생긴 수비수 이름이 뭐였죠?ㅋㅋㅋㅋ",
          "original_form": "오 잘생긴 수비수 이름이 뭐였죠?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:05"
        },
        {
          "id": "MDRW2100012781.1.2",
          "form": "잘생긴...",
          "original_form": "잘생긴...",
          "speaker_id": "1",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100012781.1.3",
          "form": "수비수가....",
          "original_form": "수비수가....",
          "speaker_id": "1",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100012781.1.4",
          "form": "있던가요?.......",
          "original_form": "있던가요?.......",
          "speaker_id": "1",
          "time": "20210825 01:10"
        },
        {
          "id": "MDRW2100012781.1.5",
          "form": "토비???",
          "original_form": "토비???",
          "speaker_id": "2",
          "time": "20210825 01:10"
        },
        {
          "id": "MDRW2100012781.1.6",
          "form": "알뭐시기 ㅋㅋㅋㅋ",
          "original_form": "알뭐시기 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:10"
        },
        {
          "id": "MDRW2100012781.1.7",
          "form": "토비 알데르?",
          "original_form": "토비 알데르?",
          "speaker_id": "1",
          "time": "20210825 01:14"
        },
        {
          "id": "MDRW2100012781.1.8",
          "form": "이적했죠ㅋㅋㅋ",
          "original_form": "이적했죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:14"
        },
        {
          "id": "MDRW2100012781.1.9",
          "form": "네 ㅋㅋㅋ 그래도 잘 메꿨네요 수비라인",
          "original_form": "네 ㅋㅋㅋ 그래도 잘 메꿨네요 수비라인",
          "speaker_id": "2",
          "time": "20210825 01:15"
        },
        {
          "id": "MDRW2100012781.1.10",
          "form": "탕강가가",
          "original_form": "탕강가가",
          "speaker_id": "1",
          "time": "20210825 01:19"
        },
        {
          "id": "MDRW2100012781.1.11",
          "form": "맨시티때 오졌답니다",
          "original_form": "맨시티때 오졌답니다",
          "speaker_id": "1",
          "time": "20210825 01:19"
        },
        {
          "id": "MDRW2100012781.1.12",
          "form": "그 실력만유지해도 좋을텐데요",
          "original_form": "그 실력만유지해도 좋을텐데요",
          "speaker_id": "1",
          "time": "20210825 01:19"
        },
        {
          "id": "MDRW2100012781.1.13",
          "form": "헐 근데 대체 공격수 보강은 왜 안하나 몰라요 ㅋㅋㅋㅋ 토트넘 가난해요?",
          "original_form": "헐 근데 대체 공격수 보강은 왜 안하나 몰라요 ㅋㅋㅋㅋ 토트넘 가난해요?",
          "speaker_id": "2",
          "time": "20210825 01:24"
        },
        {
          "id": "MDRW2100012781.1.14",
          "form": "사실근데 공격수만의문제도 아닌것같아요",
          "original_form": "사실근데 공격수만의문제도 아닌것같아요",
          "speaker_id": "1",
          "time": "20210825 01:31"
        },
        {
          "id": "MDRW2100012781.1.15",
          "form": "공이 안오니 넣을수가없죠ㅋㅋㅋㅋ",
          "original_form": "공이 안오니 넣을수가없죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:31"
        },
        {
          "id": "MDRW2100012781.1.16",
          "form": "중원이 문제인건가 ㅋㅋㅋㅋ",
          "original_form": "중원이 문제인건가 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100012781.1.17",
          "form": "케인에게",
          "original_form": "케인에게",
          "speaker_id": "1",
          "time": "20210825 01:37"
        },
        {
          "id": "MDRW2100012781.1.18",
          "form": "손흥민이 골을 찔러줬듯이",
          "original_form": "손흥민이 골을 찔러줬듯이",
          "speaker_id": "1",
          "time": "20210825 01:38"
        },
        {
          "id": "MDRW2100012781.1.19",
          "form": "손에게 찔러줄선수가 필요해요ㅋㅋㅋ",
          "original_form": "손에게 찔러줄선수가 필요해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:38"
        },
        {
          "id": "MDRW2100012781.1.20",
          "form": "손이 찌르고 뛰어서 받아서 넣어야...",
          "original_form": "손이 찌르고 뛰어서 받아서 넣어야...",
          "speaker_id": "2",
          "time": "20210825 01:39"
        },
        {
          "id": "MDRW2100012781.1.21",
          "form": "ㅋㅋㅋㅋㅋ 혼자요?",
          "original_form": "ㅋㅋㅋㅋㅋ 혼자요?",
          "speaker_id": "1",
          "time": "20210825 01:42"
        },
        {
          "id": "MDRW2100012781.1.22",
          "form": "참..앞으로",
          "original_form": "참..앞으로",
          "speaker_id": "1",
          "time": "20210825 01:42"
        },
        {
          "id": "MDRW2100012781.1.23",
          "form": "어떤경기일지 궁금하긴해요..ㅎ",
          "original_form": "어떤경기일지 궁금하긴해요..ㅎ",
          "speaker_id": "1",
          "time": "20210825 01:42"
        },
        {
          "id": "MDRW2100012781.1.24",
          "form": "케인 잔류 선언했네요 ㅋㅋㅋ",
          "original_form": "케인 잔류 선언했네요 ㅋㅋㅋ",
