      "utterance": [
        {
          "id": "MDRW2100019610.1.1",
          "form": "내일 또 외식하기러했는데",
          "original_form": "내일 또 외식하기러했는데",
          "speaker_id": "2",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019610.1.2",
          "form": "ㅋㅋ진짜 내일 까지만 ㅋㅋㅋ",
          "original_form": "ㅋㅋ진짜 내일 까지만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019610.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ 내일의 메뉴는 뭐예요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 내일의 메뉴는 뭐예요?",
          "speaker_id": "1",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019610.1.4",
          "form": "피자?",
          "original_form": "피자?",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019610.1.5",
          "form": "저의 소울푸드죠",
          "original_form": "저의 소울푸드죠",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019610.1.6",
          "form": "아~ 좋다",
          "original_form": "아~ 좋다",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019610.1.7",
          "form": "다이어트 콜라 먹으면 돼요",
          "original_form": "다이어트 콜라 먹으면 돼요",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019610.1.8",
          "form": "ㅋㅋㅋ 저 어제 몸무게 쟀는게 3키로 늘어서",
          "original_form": "ㅋㅋㅋ 저 어제 몸무게 쟀는게 3키로 늘어서",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019610.1.9",
          "form": "오늘부터 다이어트해요",
          "original_form": "오늘부터 다이어트해요",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019610.1.10",
          "form": "집에서 쟀을 때는 2키로 더 나왔을 뿐이라 세이프였는데...",
          "original_form": "집에서 쟀을 때는 2키로 더 나왔을 뿐이라 세이프였는데...",
          "speaker_id": "1",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019610.1.11",
          "form": "이제 다이어트 메뉴를 생각해보려구요 ㅋㅋ",
          "original_form": "이제 다이어트 메뉴를 생각해보려구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019610.1.12",
          "form": "전 너무 많이 쪘어요",
          "original_form": "전 너무 많이 쪘어요",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019610.1.13",
          "form": "진정한 확찐진",
          "original_form": "진정한 확찐진",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019610.1.14",
          "form": "확찐자 ㅋㅋ",
          "original_form": "확찐자 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019610.1.15",
          "form": "ㅋㅋㅋㅋㅋ 맛있는 못 먹고 뺀다고 생각하면 너무 슬프니까요",
          "original_form": "ㅋㅋㅋㅋㅋ 맛있는 못 먹고 뺀다고 생각하면 너무 슬프니까요",
          "speaker_id": "1",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019610.1.16",
          "form": "건강하고 맛있는 거 먹으면서 뺀다고 마인드컨트롤하려구여 ㅠㅠ",
          "original_form": "건강하고 맛있는 거 먹으면서 뺀다고 마인드컨트롤하려구여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019610.1.17",
          "form": "살 전부 이 시국 탓이지 제 탓이 아닌데 고생은 제가 하네요",
          "original_form": "살 전부 이 시국 탓이지 제 탓이 아닌데 고생은 제가 하네요",
          "speaker_id": "1",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019610.1.18",
          "form": "ㅋㅋ저는 이제 진짜 저녁으로 다이어트쉐이크 먹으려고요ㅜ",
          "original_form": "ㅋㅋ저는 이제 진짜 저녁으로 다이어트쉐이크 먹으려고요ㅜ",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100019610.1.19",
          "form": "그거 좋은 아이디어네요",
          "original_form": "그거 좋은 아이디어네요",
          "speaker_id": "1",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100019610.1.20",
          "form": "무슨 맛 먹어요?",
          "original_form": "무슨 맛 먹어요?",
          "speaker_id": "1",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100019610.1.21",
          "form": "민초맛을 샀는데",
          "original_form": "민초맛을 샀는데",
          "speaker_id": "2",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019610.1.22",
          "form": "ㅋㅋ맨날",
          "original_form": "ㅋㅋ맨날",
          "speaker_id": "2",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019610.1.23",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019610.1.24",
          "form": "밖에서 사먹고드가요",
          "original_form": "밖에서 사먹고드가요",
          "speaker_id": "2",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019610.1.25",
          "form": "월욜부터 결심했는데 ㅋㅋㅋ",
          "original_form": "월욜부터 결심했는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019610.1.26",
          "form": "ㅋㅋㅋㅋㅋ민초 쉐이크는 제가 먹어볼게요",
          "original_form": "ㅋㅋㅋㅋㅋ민초 쉐이크는 제가 먹어볼게요",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019610.1.27",
          "form": "저는 민초 들어간 음식이 상큼한 맛을 하고 칼로리는 그렇지 않다는 게",
          "original_form": "저는 민초 들어간 음식이 상큼한 맛을 하고 칼로리는 그렇지 않다는 게",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019610.1.28",
          "form": "배신감 느껴져서 싫었거든요?",
          "original_form": "배신감 느껴져서 싫었거든요?",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019610.1.29",
          "form": "근데 다이어트 쉐이크라면 너무 좋을 것 같아요",
          "original_form": "근데 다이어트 쉐이크라면 너무 좋을 것 같아요",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019610.1.30",
          "form": "ㅋㅋ 저도 민초 좋아해요",
          "original_form": "ㅋㅋ 저도 민초 좋아해요",
          "speaker_id": "2",
          "time": "20210827 21:17"
        }
      ]
