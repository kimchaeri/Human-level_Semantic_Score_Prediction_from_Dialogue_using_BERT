      "utterance": [
        {
          "id": "MDRW2100010833.1.1",
          "form": "아리에티가 흥행 못해서 그렇지",
          "original_form": "아리에티가 흥행 못해서 그렇지",
          "speaker_id": "1",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100010833.1.2",
          "form": "또 나름 철학적이었던거 같아요 ㅋㅋ",
          "original_form": "또 나름 철학적이었던거 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010833.1.3",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010833.1.4",
          "form": "지브리가 특유의 철학이 있죠",
          "original_form": "지브리가 특유의 철학이 있죠",
          "speaker_id": "2",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010833.1.5",
          "form": "그",
          "original_form": "그",
          "speaker_id": "2",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010833.1.6",
          "form": "유명한 감독이름이 뭐죠??",
          "original_form": "유명한 감독이름이 뭐죠??",
          "speaker_id": "2",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010833.1.7",
          "form": "미야자키 하야오요",
          "original_form": "미야자키 하야오요",
          "speaker_id": "1",
          "time": "20210823 22:18"
        },
        {
          "id": "MDRW2100010833.1.8",
          "form": "요새 은퇴한거 같기도하고",
          "original_form": "요새 은퇴한거 같기도하고",
          "speaker_id": "1",
          "time": "20210823 22:18"
        },
        {
          "id": "MDRW2100010833.1.9",
          "form": "전 웨이브에서 요새 영드도 많이봐요 ㅋ",
          "original_form": "전 웨이브에서 요새 영드도 많이봐요 ㅋ",
          "speaker_id": "1",
          "time": "20210823 22:18"
        },
        {
          "id": "MDRW2100010833.1.10",
          "form": "앜ㅋㅋㅋㅋ 영드 재밋죠",
          "original_form": "앜ㅋㅋㅋㅋ 영드 재밋죠",
          "speaker_id": "2",
          "time": "20210823 22:19"
        },
        {
          "id": "MDRW2100010833.1.11",
          "form": "크리미널 마인드 아세요?",
          "original_form": "크리미널 마인드 아세요?",
          "speaker_id": "2",
          "time": "20210823 22:19"
        },
        {
          "id": "MDRW2100010833.1.12",
          "form": "악 저 그거 한때 진짜 열심히봤어요",
          "original_form": "악 저 그거 한때 진짜 열심히봤어요",
          "speaker_id": "1",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010833.1.13",
          "form": "페넬로피 ㅋㅋ",
          "original_form": "페넬로피 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010833.1.14",
          "form": "ㅜㅜ 제가 시즌 1인가 2인까 까지만보고",
          "original_form": "ㅜㅜ 제가 시즌 1인가 2인까 까지만보고",
          "speaker_id": "2",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010833.1.15",
          "form": "하차해서ㅜㅜ",
          "original_form": "하차해서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010833.1.16",
          "form": "전 어느순간 그거보면서 밥먹는 나자신을보며",
          "original_form": "전 어느순간 그거보면서 밥먹는 나자신을보며",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010833.1.17",
          "form": "아 이건 아니지않나",
          "original_form": "아 이건 아니지않나",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010833.1.18",
          "form": "돌아와라 내 순수감성 ㅋㅋ했던 기억이 ㅋㅋㅋ",
          "original_form": "돌아와라 내 순수감성 ㅋㅋ했던 기억이 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010833.1.19",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 심장쫄깃한 기분을 느끼고 싶으신건가요",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 심장쫄깃한 기분을 느끼고 싶으신건가요",
          "speaker_id": "2",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010833.1.20",
          "form": "크리미널 마인드 첨볼때 진짜 손에 땀엄청나고그랫는디",
          "original_form": "크리미널 마인드 첨볼때 진짜 손에 땀엄청나고그랫는디",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010833.1.21",
          "form": "아니 뭔가 타락했다? 란 기분들었어요 ㅋㅋ",
          "original_form": "아니 뭔가 타락했다? 란 기분들었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010833.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋㅋㅋㅋㅋ 지금은 드라마 뭐보새요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋㅋㅋㅋㅋ 지금은 드라마 뭐보새요?",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010833.1.23",
          "form": "요즘은 영드요!",
          "original_form": "요즘은 영드요!",
          "speaker_id": "1",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010833.1.24",
          "form": "셜록 보셧어요??",
          "original_form": "셜록 보셧어요??",
          "speaker_id": "2",
          "time": "20210823 22:40"
        },
        {
          "id": "MDRW2100010833.1.25",
          "form": "셜록은 레전드죠",
          "original_form": "셜록은 레전드죠",
          "speaker_id": "1",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100010833.1.26",
          "form": "브라운신부 추천이요!",
          "original_form": "브라운신부 추천이요!",
          "speaker_id": "1",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100010833.1.27",
          "form": "오 무슨 내용이에요?",
          "original_form": "오 무슨 내용이에요?",
          "speaker_id": "2",
          "time": "20210824 18:45"
        },
        {
          "id": "MDRW2100010833.1.28",
          "form": "50년대 영국 시골이 배경이에요",
          "original_form": "50년대 영국 시골이 배경이에요",
          "speaker_id": "1",
          "time": "20210824 18:45"
        },
        {
          "id": "MDRW2100010833.1.29",
          "form": "오 되게 독특하네요 50년대 영국은 한번도 생각해본적이 없어서...",
          "original_form": "오 되게 독특하네요 50년대 영국은 한번도 생각해본적이 없어서...",
          "speaker_id": "2",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100010833.1.30",
          "form": "말그대로 브라운신부 이런저런 사건을 푸는 내용?",
          "original_form": "말그대로 브라운신부 이런저런 사건을 푸는 내용?",
          "speaker_id": "1",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100010833.1.31",
          "form": "브라운이라는 성을 가진 사람이 신부예요?",
          "original_form": "브라운이라는 성을 가진 사람이 신부예요?",
          "speaker_id": "2",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100010833.1.32",
          "form": "과학수사csi의 정반대 ㅋㅋ",
          "original_form": "과학수사csi의 정반대 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100010833.1.33",
          "form": "네네 성이 브라운이고",
          "original_form": "네네 성이 브라운이고",
          "speaker_id": "1",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100010833.1.34",
          "form": "다들파더 브라운 일케불러서",
          "original_form": "다들파더 브라운 일케불러서",
          "speaker_id": "1",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100010833.1.35",
          "form": "이름은 딱히 안나온거같아요",
          "original_form": "이름은 딱히 안나온거같아요",
          "speaker_id": "1",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100010833.1.36",
          "form": "ㅋㅋㅋㅋㅋ와 신기해요\r\n왜 csi랑 정반대예요???",
          "original_form": "ㅋㅋㅋㅋㅋ와 신기해요\r\n왜 csi랑 정반대예요???",
          "speaker_id": "2",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100010833.1.37",
          "form": "해리포터 위즐리아빠가 브라운신부 역할이요",
