      "utterance": [
        {
          "id": "MDRW2100001736.1.1",
          "form": "그거 해봤나 삼성헬스에서 무슨 온라인 피트니스? 시켜주는거 있던데",
          "original_form": "그거 해봤나 삼성헬스에서 무슨 온라인 피트니스? 시켜주는거 있던데",
          "speaker_id": "1",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001736.1.2",
          "form": "아 본적있다",
          "original_form": "아 본적있다",
          "speaker_id": "2",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001736.1.3",
          "form": "근데 그거 그냥 홈트영상 보는거아니가?",
          "original_form": "근데 그거 그냥 홈트영상 보는거아니가?",
          "speaker_id": "2",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001736.1.4",
          "form": "근데 그것도 월별로 끊어서 하는 거 있던데??",
          "original_form": "근데 그것도 월별로 끊어서 하는 거 있던데??",
          "speaker_id": "1",
          "time": "20210819 18:29"
        },
        {
          "id": "MDRW2100001736.1.5",
          "form": "나도 안해봤다",
          "original_form": "나도 안해봤다",
          "speaker_id": "1",
          "time": "20210819 18:29"
        },
        {
          "id": "MDRW2100001736.1.6",
          "form": "나 전에 땅끄부부?그거 따라하다가 너무 힘들었잖아....ㅎ",
          "original_form": "나 전에 땅끄부부?그거 따라하다가 너무 힘들었잖아....ㅎ",
          "speaker_id": "1",
          "time": "20210819 18:29"
        },
        {
          "id": "MDRW2100001736.1.7",
          "form": "엥 돈내야해??",
          "original_form": "엥 돈내야해??",
          "speaker_id": "2",
          "time": "20210819 18:30"
        },
        {
          "id": "MDRW2100001736.1.8",
          "form": "나 이번에 폰 사는거에",
          "original_form": "나 이번에 폰 사는거에",
          "speaker_id": "1",
          "time": "20210819 18:31"
        },
        {
          "id": "MDRW2100001736.1.9",
          "form": "무슨 2개월권?? 주던데",
          "original_form": "무슨 2개월권?? 주던데",
          "speaker_id": "1",
          "time": "20210819 18:31"
        },
        {
          "id": "MDRW2100001736.1.10",
          "form": "그걸 누가 돈주고 하지",
          "original_form": "그걸 누가 돈주고 하지",
          "speaker_id": "2",
          "time": "20210819 18:31"
        },
        {
          "id": "MDRW2100001736.1.11",
          "form": "아니다 돈되니까 파는거겠지!",
          "original_form": "아니다 돈되니까 파는거겠지!",
          "speaker_id": "2",
          "time": "20210819 18:31"
        },
        {
          "id": "MDRW2100001736.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ어어 그렇지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ어어 그렇지",
          "speaker_id": "1",
          "time": "20210819 18:32"
        },
        {
          "id": "MDRW2100001736.1.13",
          "form": "그러면 니 두달 받아서 해볼려고??",
          "original_form": "그러면 니 두달 받아서 해볼려고??",
