      "utterance": [
        {
          "id": "MDRW2100022814.1.1",
          "form": "커뮤니티하세요?",
          "original_form": "커뮤니티하세요?",
          "speaker_id": "1",
          "time": "20210829 18:08"
        },
        {
          "id": "MDRW2100022814.1.2",
          "form": "ㅋㅋㅌㅋㅋㅋㅋ맞아요 저희 자꾸 의식의 흐름대로 흘러가게 되네요",
          "original_form": "ㅋㅋㅌㅋㅋㅋㅋ맞아요 저희 자꾸 의식의 흐름대로 흘러가게 되네요",
          "speaker_id": "2",
          "time": "20210829 18:08"
        },
        {
          "id": "MDRW2100022814.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아여",
          "speaker_id": "1",
          "time": "20210829 18:08"
        },
        {
          "id": "MDRW2100022814.1.4",
          "form": "커뮤니티 눈팅도 커뮤를 한다고 해야할까요",
          "original_form": "커뮤니티 눈팅도 커뮤를 한다고 해야할까요",
          "speaker_id": "2",
          "time": "20210829 18:09"
        },
        {
          "id": "MDRW2100022814.1.5",
          "form": "고심하게되는",
          "original_form": "고심하게되는",
          "speaker_id": "2",
          "time": "20210829 18:09"
        },
        {
          "id": "MDRW2100022814.1.6",
          "form": "ㅋㅋㅋㅋ 저도 거의 눈팅해여",
          "original_form": "ㅋㅋㅋㅋ 저도 거의 눈팅해여{emoji:•}",
          "speaker_id": "1",
          "time": "20210829 18:09"
        },
        {
          "id": "MDRW2100022814.1.7",
          "form": "특정 커뮤니티에 정착하거나 그러진 않고",
          "original_form": "특정 커뮤니티에 정착하거나 그러진 않고",
          "speaker_id": "2",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100022814.1.8",
          "form": "막 요리 레시피 찾아서 돌아다니거나",
          "original_form": "막 요리 레시피 찾아서 돌아다니거나",
          "speaker_id": "2",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100022814.1.9",
          "form": "꿀팁 찾고 다니거나 그래요ㅋㅋㅋㅋ",
          "original_form": "꿀팁 찾고 다니거나 그래요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100022814.1.10",
          "form": "아 맞아요 커뮤니티가 요리레시피 찾기 좋아요",
          "original_form": "아 맞아요 커뮤니티가 요리레시피 찾기 좋아요",
          "speaker_id": "1",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100022814.1.11",
          "form": "빅데이터가 모인곳.",
          "original_form": "빅데이터가 모인곳.",
          "speaker_id": "1",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100022814.1.12",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100022814.1.13",
          "form": "얼마 전에 100%감자전 레시피를",
          "original_form": "얼마 전에 100%감자전 레시피를",
          "speaker_id": "2",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100022814.1.14",
          "form": "봐뒀는데",
          "original_form": "봐뒀는데",
          "speaker_id": "2",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100022814.1.15",
          "form": "설거지 귀찮아서 안 해먹고 있어요",
          "original_form": "설거지 귀찮아서 안 해먹고 있어요",
          "speaker_id": "2",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100022814.1.16",
          "form": "자린고비처럼",
          "original_form": "자린고비처럼",
          "speaker_id": "2",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100022814.1.17",
          "form": "아나",
          "original_form": "아나",
          "speaker_id": "1",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100022814.1.18",
          "form": "먼지 너무 이해해요",
          "original_form": "먼지 너무 이해해요",
          "speaker_id": "1",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100022814.1.19",
          "form": "상상감자전 먹는 중",
          "original_form": "상상감자전 먹는 중",
          "speaker_id": "2",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100022814.1.20",
          "form": "해먹어보고싶은데 너무 귀찮",
          "original_form": "해먹어보고싶은데 너무 귀찮",
          "speaker_id": "1",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100022814.1.21",
          "form": "ㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100022814.1.22",
          "form": "전 밀푀유나베 ...",
          "original_form": "전 밀푀유나베 ...",
          "speaker_id": "1",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100022814.1.23",
          "form": "헐 맞아요 나베도 궁금해요",
          "original_form": "헐 맞아요 나베도 궁금해요",
          "speaker_id": "2",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100022814.1.24",
          "form": "누가 쌓아둔걸로 팔았으면 좋겠어요",
