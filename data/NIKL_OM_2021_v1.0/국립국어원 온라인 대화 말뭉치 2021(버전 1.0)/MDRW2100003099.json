      "utterance": [
        {
          "id": "MDRW2100003099.1.1",
          "form": "학교다닐때 재밌으셨나요?",
          "original_form": "학교다닐때 재밌으셨나요?",
          "speaker_id": "1",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003099.1.2",
          "form": "재밌었어요",
          "original_form": "재밌었어요",
          "speaker_id": "2",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003099.1.3",
          "form": "여중 여고라서",
          "original_form": "여중 여고라서",
          "speaker_id": "2",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003099.1.4",
          "form": "당신은요?",
          "original_form": "당신은요?",
          "speaker_id": "2",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003099.1.5",
          "form": "저도요 특히 고등학교때 재밌었어요",
          "original_form": "저도요 특히 고등학교때 재밌었어요",
          "speaker_id": "1",
          "time": "20210820 18:39"
        },
        {
          "id": "MDRW2100003099.1.6",
          "form": "연락하는 친구가 있나요?",
          "original_form": "연락하는 친구가 있나요?",
          "speaker_id": "2",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003099.1.7",
          "form": "네 고등학교때 친하게 지냈던 친구랑 아직도 연락하죠 고등학교때 추억있나요?",
          "original_form": "네 고등학교때 친하게 지냈던 친구랑 아직도 연락하죠 고등학교때 추억있나요?",
          "speaker_id": "1",
          "time": "20210820 18:41"
        },
        {
          "id": "MDRW2100003099.1.8",
          "form": "저는 고1때 수련회때 장기자랑나간거요",
          "original_form": "저는 고1때 수련회때 장기자랑나간거요",
          "speaker_id": "2",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100003099.1.9",
          "form": "장기자랑에서 뭐하셨나요",
          "original_form": "장기자랑에서 뭐하셨나요",
          "speaker_id": "1",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100003099.1.10",
          "form": "걸그룹춤춘거같은데 3등은했을거예요",
          "original_form": "걸그룹춤춘거같은데 3등은했을거예요",
          "speaker_id": "2",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100003099.1.11",
          "form": "사회자가 제가 제일 못맞췄다고 한게 기억나네요",
          "original_form": "사회자가 제가 제일 못맞췄다고 한게 기억나네요",
          "speaker_id": "2",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100003099.1.12",
          "form": "ㅋㅋㅋㅋ재밌었겠네요 수학여행은 어땠어요?",
          "original_form": "ㅋㅋㅋㅋ재밌었겠네요 수학여행은 어땠어요?",
          "speaker_id": "1",
          "time": "20210820 18:48"
        },
        {
          "id": "MDRW2100003099.1.13",
          "form": "하도 오래돼서 기억이 안나네요",
          "original_form": "하도 오래돼서 기억이 안나네요",
          "speaker_id": "2",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100003099.1.14",
          "form": "대학축제 연예인 기억나는사람있나요?",
          "original_form": "대학축제 연예인 기억나는사람있나요?",
          "speaker_id": "2",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100003099.1.15",
          "form": "싸이가 왔었어요 소주 한병마시고 공연했어요",
          "original_form": "싸이가 왔었어요 소주 한병마시고 공연했어요",
          "speaker_id": "1",
          "time": "20210820 18:50"
        },
        {
          "id": "MDRW2100003099.1.16",
          "form": "저도 같네요",
          "original_form": "저도 같네요",
          "speaker_id": "2",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100003099.1.17",
          "form": "두번째 군대가기전이라 많이 뛰었나보네요",
          "original_form": "두번째 군대가기전이라 많이 뛰었나보네요",
          "speaker_id": "2",
          "time": "20210820 18:52"
        },
        {
          "id": "MDRW2100003099.1.18",
          "form": "그런가봐요 동아리활동 하셨나요",
          "original_form": "그런가봐요 동아리활동 하셨나요",
          "speaker_id": "1",
          "time": "20210820 18:53"
        },
        {
          "id": "MDRW2100003099.1.19",
          "form": "아니요 집이 멀어서요",
          "original_form": "아니요 집이 멀어서요",
          "speaker_id": "2",
          "time": "20210820 18:54"
        }
      ]
