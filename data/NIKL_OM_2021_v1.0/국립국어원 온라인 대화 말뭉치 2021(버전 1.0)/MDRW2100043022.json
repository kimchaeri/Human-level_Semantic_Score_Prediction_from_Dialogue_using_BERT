      "utterance": [
        {
          "id": "MDRW2100043022.1.1",
          "form": "나 호두파이 먹었엉",
          "original_form": "나 호두파이 먹었엉",
          "speaker_id": "1",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100043022.1.2",
          "form": "신세계에서 저번에 싸게 산거 왔넹",
          "original_form": "신세계에서 저번에 싸게 산거 왔넹",
          "speaker_id": "1",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100043022.1.3",
          "form": "그거 맛 괜찮더나?",
          "original_form": "그거 맛 괜찮더나?",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043022.1.4",
          "form": "엉 호두 많이들어서 괜찮더라 안달고",
          "original_form": "엉 호두 많이들어서 괜찮더라 안달고",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043022.1.5",
          "form": "근데 호두밑에 알수없는 밀가루풀? 암튼 그런거 들었는데 그게 별로더라 그래도 전체적으로 괜춘",
          "original_form": "근데 호두밑에 알수없는 밀가루풀? 암튼 그런거 들었는데 그게 별로더라 그래도 전체적으로 괜춘",
          "speaker_id": "1",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100043022.1.6",
          "form": "저번에 후기 안좋다 들어서\r\n어떤제품이였는지 정확한 이름 좀 알려줘ㅎ",
          "original_form": "저번에 후기 안좋다 들어서\r\n어떤제품이였는지 정확한 이름 좀 알려줘ㅎ",
          "speaker_id": "2",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100043022.1.7",
          "form": "호두파이?",
          "original_form": "호두파이?",
          "speaker_id": "1",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100043022.1.8",
          "form": "어디서 후기안좋다 들었어?",
          "original_form": "어디서 후기안좋다 들었어?",
          "speaker_id": "1",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100043022.1.9",
          "form": "유명한 호두파이 인가?",
          "original_form": "유명한 호두파이 인가?",
          "speaker_id": "1",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100043022.1.10",
          "form": "밀크앤허니 호두파이",
          "original_form": "밀크앤허니 호두파이",
          "speaker_id": "1",
          "time": "20210907 15:58"
        },
        {
          "id": "MDRW2100043022.1.11",
          "form": "땡큐",
          "original_form": "땡큐",
          "speaker_id": "2",
          "time": "20210907 15:59"
        },
        {
          "id": "MDRW2100043022.1.12",
          "form": "타르트도 있다던데 파이만 먹어봤제?",
          "original_form": "타르트도 있다던데 파이만 먹어봤제?",
          "speaker_id": "2",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100043022.1.13",
          "form": "ㅇㅇ 유명한데야?",
          "original_form": "ㅇㅇ 유명한데야?",
          "speaker_id": "1",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100043022.1.14",
          "form": "난 호두많아서 맘에들더라 안달고",
          "original_form": "난 호두많아서 맘에들더라 안달고",
          "speaker_id": "1",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100043022.1.15",
          "form": "근데 진짜 밑에바른 소스인지 밀가루풀인지가 좀 이상해",
          "original_form": "근데 진짜 밑에바른 소스인지 밀가루풀인지가 좀 이상해",
          "speaker_id": "1",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043022.1.16",
          "form": "가격이 좋아서 사는것같더라고\r\n후기는 괜찮다는 사람도 있긴한거같았어ㅎ",
          "original_form": "가격이 좋아서 사는것같더라고\r\n후기는 괜찮다는 사람도 있긴한거같았어ㅎ",
          "speaker_id": "2",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100043022.1.17",
          "form": "근데 대체로 비추했던듯해서 기억에 남았었나봐ㅎ",
          "original_form": "근데 대체로 비추했던듯해서 기억에 남았었나봐ㅎ",
          "speaker_id": "2",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100043022.1.18",
          "form": "호두많다하니 궁금",
          "original_form": "호두많다하니 궁금",
          "speaker_id": "2",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100043022.1.19",
          "form": "요즘 파이 비싸서ㅋ 토핑 적게주드라고",
          "original_form": "요즘 파이 비싸서ㅋ 토핑 적게주드라고",
          "speaker_id": "2",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100043022.1.20",
          "form": "아 나도 호두파이 많이 안먹어봐서 잘 모르겠지만 많은것 같은데",
          "original_form": "아 나도 호두파이 많이 안먹어봐서 잘 모르겠지만 많은것 같은데",
          "speaker_id": "1",
          "time": "20210907 16:16"
        },
        {
          "id": "MDRW2100043022.1.21",
          "form": "누구선물주기엔 막 고급스런맛은 아니라 별론데 가족끼리먹기엔 괜춘한듯",
          "original_form": "누구선물주기엔 막 고급스런맛은 아니라 별론데 가족끼리먹기엔 괜춘한듯",
          "speaker_id": "1",
          "time": "20210907 16:16"
        },
        {
          "id": "MDRW2100043022.1.22",
          "form": "오홍ㅎㅎ잘샀당",
          "original_form": "오홍ㅎㅎ잘샀당",
          "speaker_id": "2",
          "time": "20210907 17:00"
        },
        {
          "id": "MDRW2100043022.1.23",
          "form": "주원이 디기 쇼핑 잘한당~",
          "original_form": "주원이 디기 쇼핑 잘한당~",
          "speaker_id": "2",
          "time": "20210907 17:00"
        },
        {
          "id": "MDRW2100043022.1.24",
          "form": "나도 만오천구백원이면 선뜻 사기 그런데",
          "original_form": "나도 만오천구백원이면 선뜻 사기 그런데",
          "speaker_id": "1",
          "time": "20210907 17:02"
        },
        {
          "id": "MDRW2100043022.1.25",
          "form": "신세계 쿠폰 있어서 잘 산듯",
          "original_form": "신세계 쿠폰 있어서 잘 산듯",
          "speaker_id": "1",
          "time": "20210907 17:03"
        },
        {
          "id": "MDRW2100043022.1.26",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043022.1.27",
          "form": "쿠폰 좋다ㅋ",
          "original_form": "쿠폰 좋다ㅋ",
          "speaker_id": "2",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100043022.1.28",
          "form": "ㅋㅋ 신라면 볶음면 먹어봄?",
          "original_form": "ㅋㅋ 신라면 볶음면 먹어봄?",
          "speaker_id": "1",
          "time": "20210907 18:15"
        },
        {
          "id": "MDRW2100043022.1.29",
          "form": "나 그거 저녁으로 먹었다",
          "original_form": "나 그거 저녁으로 먹었다",
          "speaker_id": "1",
          "time": "20210907 18:15"
        },
        {
          "id": "MDRW2100043022.1.30",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210907 18:18"
        },
        {
          "id": "MDRW2100043022.1.31",
          "form": "맛있나본데?",
          "original_form": "맛있나본데?",
          "speaker_id": "2",
          "time": "20210907 18:18"
        },
        {
          "id": "MDRW2100043022.1.32",
          "form": "ㅇㅇ괜찮아!",
          "original_form": "ㅇㅇ괜찮아!",
          "speaker_id": "1",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100043022.1.33",
          "form": "나 신라면보다 이게 맛있드라",
          "original_form": "나 신라면보다 이게 맛있드라",
          "speaker_id": "1",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100043022.1.34",
          "form": "볶음면을 더 좋아해서",
          "original_form": "볶음면을 더 좋아해서",
          "speaker_id": "1",
