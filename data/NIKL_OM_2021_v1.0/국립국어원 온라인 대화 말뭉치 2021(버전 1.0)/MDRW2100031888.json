      "utterance": [
        {
          "id": "MDRW2100031888.1.1",
          "form": "일요일 날씨좋네요",
          "original_form": "일요일 날씨좋네요",
          "speaker_id": "1",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100031888.1.2",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210905 07:55"
        },
        {
          "id": "MDRW2100031888.1.3",
          "form": "어제도 날씨 화창한대 기온은 높지 않인 좋았는데",
          "original_form": "어제도 날씨 화창한대 기온은 높지 않인 좋았는데",
          "speaker_id": "2",
          "time": "20210905 07:55"
        },
        {
          "id": "MDRW2100031888.1.4",
          "form": "화창하고 하늘도깨끗",
          "original_form": "화창하고 하늘도깨끗",
          "speaker_id": "1",
          "time": "20210905 07:55"
        },
        {
          "id": "MDRW2100031888.1.5",
          "form": "오늘도 그러네요",
          "original_form": "오늘도 그러네요",
          "speaker_id": "2",
          "time": "20210905 07:55"
        },
        {
          "id": "MDRW2100031888.1.6",
          "form": "저는 그래서 어제 경주로 와이프랑 나들이 다녀왔는데",
          "original_form": "저는 그래서 어제 경주로 와이프랑 나들이 다녀왔는데",
          "speaker_id": "2",
          "time": "20210905 07:56"
        },
        {
          "id": "MDRW2100031888.1.7",
          "form": "name1님은 오늘 외출 계획 있으신가요?",
          "original_form": "&name1&님은 오늘 외출 계획 있으신가요?",
          "speaker_id": "2",
          "time": "20210905 07:56"
        },
        {
          "id": "MDRW2100031888.1.8",
          "form": "결혼식장요",
          "original_form": "결혼식장요",
          "speaker_id": "1",
          "time": "20210905 07:56"
        },
        {
          "id": "MDRW2100031888.1.9",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210905 07:56"
        },
        {
          "id": "MDRW2100031888.1.10",
          "form": "오늘도 선선해서 외출하기 좋을것 같아요",
          "original_form": "오늘도 선선해서 외출하기 좋을것 같아요",
          "speaker_id": "2",
          "time": "20210905 07:57"
        },
        {
          "id": "MDRW2100031888.1.11",
          "form": "이번주 내내 비와서 힘들었는데",
          "original_form": "이번주 내내 비와서 힘들었는데",
          "speaker_id": "2",
          "time": "20210905 07:57"
        },
        {
          "id": "MDRW2100031888.1.12",
          "form": "그러네요..너무좋아요",
          "original_form": "그러네요..너무좋아요",
          "speaker_id": "1",
          "time": "20210905 07:57"
        },
        {
          "id": "MDRW2100031888.1.13",
          "form": "내일도 좋은것같아요.화요일부터 비온다고하는것같아요",
          "original_form": "내일도 좋은것같아요.화요일부터 비온다고하는것같아요",
          "speaker_id": "1",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100031888.1.14",
          "form": "name1님은 어느 계절 좋아하세요?",
          "original_form": "&name1&님은 어느 계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100031888.1.15",
          "form": "가을요..",
          "original_form": "가을요..",
          "speaker_id": "1",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100031888.1.16",
          "form": "너무좋아해요",
          "original_form": "너무좋아해요",
          "speaker_id": "1",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100031888.1.17",
          "form": "저도 덥지도 않고 춥지도 않고",
          "original_form": "저도 덥지도 않고 춥지도 않고",
          "speaker_id": "2",
          "time": "20210905 08:00"
        },
        {
          "id": "MDRW2100031888.1.18",
          "form": "그런데 늘 너무 짧아서 슬펐는데",
          "original_form": "그런데 늘 너무 짧아서 슬펐는데",
          "speaker_id": "2",
          "time": "20210905 08:00"
        },
        {
          "id": "MDRW2100031888.1.19",
          "form": "올해는 가을이 일찍 찾아온것 같아요",
          "original_form": "올해는 가을이 일찍 찾아온것 같아요",
          "speaker_id": "2",
          "time": "20210905 08:00"
        },
        {
          "id": "MDRW2100031888.1.20",
          "form": "너무 좋아요.날씨도 기분도..",
          "original_form": "너무 좋아요.날씨도 기분도..",
          "speaker_id": "1",
          "time": "20210905 08:00"
        },
        {
          "id": "MDRW2100031888.1.21",
          "form": "옷차림도",
          "original_form": "옷차림도",
          "speaker_id": "1",
          "time": "20210905 08:01"
        },
        {
          "id": "MDRW2100031888.1.22",
          "form": "저도 가디건 좋아하는데",
          "original_form": "저도 가디건 좋아하는데",
          "speaker_id": "2",
          "time": "20210905 08:01"
        },
        {
          "id": "MDRW2100031888.1.23",
          "form": "역시 가디건은 겨울이 최고",
          "original_form": "역시 가디건은 겨울이 최고",
          "speaker_id": "2",
          "time": "20210905 08:01"
        },
        {
          "id": "MDRW2100031888.1.24",
          "form": "아니가을이최고",
          "original_form": "아니가을이최고",
          "speaker_id": "2",
          "time": "20210905 08:01"
        },
        {
          "id": "MDRW2100031888.1.25",
          "form": "생각없이 타이핑하고 있었네요",
          "original_form": "생각없이 타이핑하고 있었네요",
          "speaker_id": "2",
          "time": "20210905 08:01"
        },
        {
          "id": "MDRW2100031888.1.26",
          "form": "바파리코트도 좋구요",
          "original_form": "바파리코트도 좋구요",
