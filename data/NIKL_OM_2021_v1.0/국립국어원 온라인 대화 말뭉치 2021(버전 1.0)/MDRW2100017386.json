      "utterance": [
        {
          "id": "MDRW2100017386.1.1",
          "form": "학교다녕요,?",
          "original_form": "학교다녕요,?",
          "speaker_id": "2",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100017386.1.2",
          "form": "저는 직장인이예요ㅎㅎ",
          "original_form": "저는 직장인이예요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:40"
        },
        {
          "id": "MDRW2100017386.1.3",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100017386.1.4",
          "form": "대학원생이요",
          "original_form": "대학원생이요",
          "speaker_id": "2",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100017386.1.5",
          "form": "대학생이조앗죠",
          "original_form": "대학생이조앗죠",
          "speaker_id": "2",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100017386.1.6",
          "form": "아 대학원생이시구나",
          "original_form": "아 대학원생이시구나",
          "speaker_id": "1",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100017386.1.7",
          "form": "대학원 수업도 원격으로 진행하나요 요즘?",
          "original_form": "대학원 수업도 원격으로 진행하나요 요즘?",
          "speaker_id": "1",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100017386.1.8",
          "form": "코스웍 끝나서 모르겟네요",
          "original_form": "코스웍 끝나서 모르겟네요",
          "speaker_id": "2",
          "time": "20210904 13:52"
        },
        {
          "id": "MDRW2100017386.1.9",
          "form": "아마 그럴거애용..",
          "original_form": "아마 그럴거애용..",
          "speaker_id": "2",
          "time": "20210904 13:52"
        },
        {
          "id": "MDRW2100017386.1.10",
          "form": "대학생하고싶다",
          "original_form": "대학생하고싶다",
          "speaker_id": "2",
          "time": "20210904 13:52"
        },
        {
          "id": "MDRW2100017386.1.11",
          "form": "아 논문학기신가봐요~",
          "original_form": "아 논문학기신가봐요~",
          "speaker_id": "1",
          "time": "20210904 13:53"
        },
        {
          "id": "MDRW2100017386.1.12",
          "form": "저는 이제 대학원 가볼까 생각중이라",
          "original_form": "저는 이제 대학원 가볼까 생각중이라",
          "speaker_id": "1",
          "time": "20210904 13:53"
        },
        {
          "id": "MDRW2100017386.1.13",
          "form": "요즘 대학생들 근데 캠퍼스 생활도 못 누리고 안쓰러워요...",
          "original_form": "요즘 대학생들 근데 캠퍼스 생활도 못 누리고 안쓰러워요...",
          "speaker_id": "1",
          "time": "20210904 13:53"
        },
        {
          "id": "MDRW2100017386.1.14",
          "form": "아아 ~ 그죠ㅋㅋㅋㅋ 좀 불쌍",
          "original_form": "아아 ~ 그죠ㅋㅋㅋㅋ 좀 불쌍",
          "speaker_id": "2",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100017386.1.15",
          "form": "오 왜 학교로 다시.....",
          "original_form": "오 왜 학교로 다시.....",
          "speaker_id": "2",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100017386.1.16",
          "form": "아 전문성을 좀 키워보고 싶다는 생각이 들어서요ㅎㅎ",
          "original_form": "아 전문성을 좀 키워보고 싶다는 생각이 들어서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:56"
        },
        {
          "id": "MDRW2100017386.1.17",
          "form": "늦기 전에 좀 더 공부해보면 좋겠다 싶기도 하고",
          "original_form": "늦기 전에 좀 더 공부해보면 좋겠다 싶기도 하고",
          "speaker_id": "1",
          "time": "20210904 13:56"
        },
        {
          "id": "MDRW2100017386.1.18",
          "form": "아하 박사로요?",
          "original_form": "아하 박사로요?",
          "speaker_id": "2",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100017386.1.19",
          "form": "아뇨 일단 석사부터...ㅎㅎ",
          "original_form": "아뇨 일단 석사부터...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100017386.1.20",
          "form": "수업듣믄게 젤 폄한거같아요",
          "original_form": "수업듣믄게 젤 폄한거같아요",
          "speaker_id": "2",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100017386.1.21",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100017386.1.22",
          "form": "학생 때가 젤 편하다...",
          "original_form": "학생 때가 젤 편하다...",
          "speaker_id": "1",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100017386.1.23",
          "form": "학생 때는 공감 못했는데 지나고보니 수업듣기만 할 때가 젤 편했어요",
          "original_form": "학생 때는 공감 못했는데 지나고보니 수업듣기만 할 때가 젤 편했어요",
          "speaker_id": "1",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100017386.1.24",
          "form": "그죠 시간 돌리구시퍼요",
          "original_form": "그죠 시간 돌리구시퍼요",
          "speaker_id": "2",
          "time": "20210904 14:04"
        }
      ]
    }
  ]
