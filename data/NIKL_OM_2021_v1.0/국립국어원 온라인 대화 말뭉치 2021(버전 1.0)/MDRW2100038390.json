      "utterance": [
        {
          "id": "MDRW2100038390.1.1",
          "form": "허지웅같은 사람은 대단한거같아요 ㅋㅋ",
          "original_form": "허지웅같은 사람은 대단한거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:11"
        },
        {
          "id": "MDRW2100038390.1.2",
          "form": "여행계획있으세요",
          "original_form": "여행계획있으세요",
          "speaker_id": "2",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100038390.1.3",
          "form": "음 아니요ㅜㅜ",
          "original_form": "음 아니요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 12:32"
        },
        {
          "id": "MDRW2100038390.1.4",
          "form": "몸도 마음도 여유없는 코시국ㅜㅜ",
          "original_form": "몸도 마음도 여유없는 코시국ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 12:32"
        },
        {
          "id": "MDRW2100038390.1.5",
          "form": "구쵸ㅠ 그래도 선선해지면 조심히 다녀와보려구요",
          "original_form": "구쵸ㅠ 그래도 선선해지면 조심히 다녀와보려구요",
          "speaker_id": "2",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100038390.1.6",
          "form": "오 어디로 생각하고 계세요",
          "original_form": "오 어디로 생각하고 계세요",
          "speaker_id": "1",
          "time": "20210902 13:16"
        },
        {
          "id": "MDRW2100038390.1.7",
          "form": "저는 경주나 통영이용",
          "original_form": "저는 경주나 통영이용{emoji:♡}",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038390.1.8",
          "form": "경주 좋을것같아요 ㅎㅎ",
          "original_form": "경주 좋을것같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100038390.1.9",
          "form": "저는 최근 저희 동네 출렁다리 갔다왔어요",
          "original_form": "저는 최근 저희 동네 출렁다리 갔다왔어요",
          "speaker_id": "1",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100038390.1.10",
          "form": "오 출렁다리가있나요",
          "original_form": "오 출렁다리가있나요",
          "speaker_id": "2",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100038390.1.11",
          "form": "네네 나름 국내 최장이라고 지자체에서 광고하더라고요",
          "original_form": "네네 나름 국내 최장이라고 지자체에서 광고하더라고요",
          "speaker_id": "1",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100038390.1.12",
          "form": "오와 엄청 긴가봐요",
          "original_form": "오와 엄청 긴가봐요",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100038390.1.13",
          "form": "전  길다고 느꼈는데 강심장인 엄마는 에이 별로 안기네 ㅋㅋ",
          "original_form": "전  길다고 느꼈는데 강심장인 엄마는 에이 별로 안기네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100038390.1.14",
          "form": "이러셨어요",
          "original_form": "이러셨어요",
          "speaker_id": "1",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100038390.1.15",
          "form": "이런게 바로 사바사? ㅋㅋ",
          "original_form": "이런게 바로 사바사? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100038390.1.16",
          "form": "맞네요 사바사",
          "original_form": "맞네요 사바사",
          "speaker_id": "2",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100038390.1.17",
          "form": "집앞에 명소가 있으시군요",
          "original_form": "집앞에 명소가 있으시군요",
          "speaker_id": "2",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100038390.1.18",
          "form": "8월말까지 무료라서 시범운행으로 ㅋㅋ",
          "original_form": "8월말까지 무료라서 시범운행으로 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100038390.1.19",
          "form": "후다닥 다녀왔어요",
          "original_form": "후다닥 다녀왔어요",
          "speaker_id": "1",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100038390.1.20",
          "form": "아 정말요 ㅋㅋ그런것도 하능군요",
