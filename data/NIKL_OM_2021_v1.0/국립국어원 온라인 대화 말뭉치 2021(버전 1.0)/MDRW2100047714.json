        },
        {
          "id": "MDRW2100047714.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210827 09:32"
        },
        {
          "id": "MDRW2100047714.1.3",
          "form": "혹시 연애중 이신가요?",
          "original_form": "혹시 연애중 이신가요?",
          "speaker_id": "1",
          "time": "20210827 09:32"
        },
        {
          "id": "MDRW2100047714.1.4",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210827 09:32"
        },
        {
          "id": "MDRW2100047714.1.5",
          "form": "이상형 있으세요?",
          "original_form": "이상형 있으세요?",
          "speaker_id": "1",
          "time": "20210827 09:33"
        },
        {
          "id": "MDRW2100047714.1.6",
          "form": "저는 다정한 사람이요!",
          "original_form": "저는 다정한 사람이요!",
          "speaker_id": "2",
          "time": "20210827 09:33"
        },
        {
          "id": "MDRW2100047714.1.7",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210827 09:33"
        },
        {
          "id": "MDRW2100047714.1.8",
          "form": "저는 연애중이예요",
          "original_form": "저는 연애중이예요",
          "speaker_id": "1",
          "time": "20210827 09:34"
        },
        {
          "id": "MDRW2100047714.1.9",
          "form": "저랑 비슷한. 사람이 좋아요",
          "original_form": "저랑 비슷한. 사람이 좋아요",
          "speaker_id": "1",
          "time": "20210827 09:34"
        },
        {
          "id": "MDRW2100047714.1.10",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "2",
          "time": "20210827 09:34"
        },
        {
          "id": "MDRW2100047714.1.11",
          "form": "연애에서 다정함은 필수 인거 같아요 그렇지. 않나요?",
          "original_form": "연애에서 다정함은 필수 인거 같아요 그렇지. 않나요?",
          "speaker_id": "1",
          "time": "20210827 09:35"
        },
        {
          "id": "MDRW2100047714.1.12",
          "form": "맞아요 비슷한 사람이 편해요",
          "original_form": "맞아요 비슷한 사람이 편해요",
          "speaker_id": "2",
          "time": "20210827 09:35"
        },
        {
          "id": "MDRW2100047714.1.13",
          "form": "맞아요! 진짜 필수라고 생각해요",
          "original_form": "맞아요! 진짜 필수라고 생각해요",
          "speaker_id": "2",
          "time": "20210827 09:35"
        },
        {
          "id": "MDRW2100047714.1.14",
          "form": "그죠 상대방 배려해주고 챙겨주려는 마음이 꼭 필요해요",
          "original_form": "그죠 상대방 배려해주고 챙겨주려는 마음이 꼭 필요해요",
          "speaker_id": "1",
          "time": "20210827 09:36"
        },
        {
          "id": "MDRW2100047714.1.15",
          "form": "진짜 동의해요 그런마음조차 없다면 연애를 해서는 안된다고 생각해요",
          "original_form": "진짜 동의해요 그런마음조차 없다면 연애를 해서는 안된다고 생각해요",
          "speaker_id": "2",
          "time": "20210827 09:37"
        },
        {
          "id": "MDRW2100047714.1.16",
          "form": "name1님은 결혼하실 생각이 있으신가요?",
          "original_form": "&name1&님은 결혼하실 생각이 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 09:37"
        },
        {
          "id": "MDRW2100047714.1.17",
