      "utterance": [
        {
          "id": "MDRW2100031861.1.1",
          "form": "얼레벌레가게된유학인데",
          "original_form": "얼레벌레가게된유학인데",
          "speaker_id": "2",
          "time": "20210905 03:19"
        },
        {
          "id": "MDRW2100031861.1.2",
          "form": "뭔가",
          "original_form": "뭔가",
          "speaker_id": "2",
          "time": "20210905 03:19"
        },
        {
          "id": "MDRW2100031861.1.3",
          "form": "잘하다가",
          "original_form": "잘하다가",
          "speaker_id": "2",
          "time": "20210905 03:19"
        },
        {
          "id": "MDRW2100031861.1.4",
          "form": "네네네네",
          "original_form": "네네네네",
          "speaker_id": "1",
          "time": "20210905 03:19"
        },
        {
          "id": "MDRW2100031861.1.5",
          "form": "너무 성적이랑 멘탈난리나서",
          "original_form": "너무 성적이랑 멘탈난리나서",
          "speaker_id": "2",
          "time": "20210905 03:19"
        },
        {
          "id": "MDRW2100031861.1.6",
          "form": "이렇게 휴학을 한학기 하고있네용",
          "original_form": "이렇게 휴학을 한학기 하고있네용",
          "speaker_id": "2",
          "time": "20210905 03:20"
        },
        {
          "id": "MDRW2100031861.1.7",
          "form": "아이고..... 비대면 때문에",
          "original_form": "아이고..... 비대면 때문에",
          "speaker_id": "1",
          "time": "20210905 03:20"
        },
        {
          "id": "MDRW2100031861.1.8",
          "form": "효율이 떨어져서",
          "original_form": "효율이 떨어져서",
          "speaker_id": "1",
          "time": "20210905 03:20"
        },
        {
          "id": "MDRW2100031861.1.9",
          "form": "네네ㅠ  확실히 그렇더라구요",
          "original_form": "네네ㅠ  확실히 그렇더라구요",
          "speaker_id": "2",
          "time": "20210905 03:20"
        },
        {
          "id": "MDRW2100031861.1.10",
          "form": "힘들어지신걸까요...? 아이고ㅠㅠ 진짜",
          "original_form": "힘들어지신걸까요...? 아이고ㅠㅠ 진짜",
          "speaker_id": "1",
          "time": "20210905 03:20"
        },
        {
          "id": "MDRW2100031861.1.11",
          "form": "현장감이 안드니까",
          "original_form": "현장감이 안드니까",
          "speaker_id": "1",
          "time": "20210905 03:20"
        },
        {
          "id": "MDRW2100031861.1.12",
          "form": "긴장을 놓을 수밖에 없는거같아요ㅠㅠㅠ",
          "original_form": "긴장을 놓을 수밖에 없는거같아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 03:20"
        },
        {
          "id": "MDRW2100031861.1.13",
          "form": "역시학교는...현장에서 들어야만",
          "original_form": "역시학교는...현장에서 들어야만",
          "speaker_id": "2",
          "time": "20210905 03:20"
        },
        {
          "id": "MDRW2100031861.1.14",
          "form": "학교다니는기분과",
          "original_form": "학교다니는기분과",
          "speaker_id": "2",
          "time": "20210905 03:20"
        },
        {
          "id": "MDRW2100031861.1.15",
          "form": "집중력도향상되고",
          "original_form": "집중력도향상되고",
          "speaker_id": "2",
          "time": "20210905 03:20"
        },
        {
          "id": "MDRW2100031861.1.16",
          "form": "그런거같아요...",
          "original_form": "그런거같아요...",
          "speaker_id": "2",
          "time": "20210905 03:21"
        },
        {
          "id": "MDRW2100031861.1.17",
          "form": "긴장감도있고",
          "original_form": "긴장감도있고",
          "speaker_id": "2",
          "time": "20210905 03:21"
        },
        {
          "id": "MDRW2100031861.1.18",
          "form": "맞아요 그리고",
          "original_form": "맞아요 그리고",
          "speaker_id": "1",
          "time": "20210905 03:21"
        },
        {
          "id": "MDRW2100031861.1.19",
          "form": "그냥 비대면하니까 고3때 인강듣는것처럼",
          "original_form": "그냥 비대면하니까 고3때 인강듣는것처럼",
          "speaker_id": "2",
          "time": "20210905 03:21"
        },
        {
          "id": "MDRW2100031861.1.20",
          "form": "복작거리면서",
          "original_form": "복작거리면서",
          "speaker_id": "1",
          "time": "20210905 03:21"
        },
        {
          "id": "MDRW2100031861.1.21",
          "form": "사람들이랑",
          "original_form": "사람들이랑",
          "speaker_id": "1",
          "time": "20210905 03:21"
        },
        {
          "id": "MDRW2100031861.1.22",
          "form": "네ㅠㅠ",
          "original_form": "네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 03:21"
        },
        {
          "id": "MDRW2100031861.1.23",
          "form": "하염없이 안장있게되고",
          "original_form": "하염없이 안장있게되고",
          "speaker_id": "2",
          "time": "20210905 03:21"
        },
        {
          "id": "MDRW2100031861.1.24",
          "form": "그쵸ㅠㅠㅠ",
          "original_form": "그쵸ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 03:21"
        },
        {
          "id": "MDRW2100031861.1.25",
          "form": "사람들이랑 상호작용이 있어야하는건데....",
          "original_form": "사람들이랑 상호작용이 있어야하는건데....",
          "speaker_id": "1",
          "time": "20210905 03:21"
        },
        {
          "id": "MDRW2100031861.1.26",
          "form": "하....",
          "original_form": "하....",
          "speaker_id": "1",
          "time": "20210905 03:21"
        },
        {
          "id": "MDRW2100031861.1.27",
          "form": "그쵸그쵸...",
          "original_form": "그쵸그쵸...",
          "speaker_id": "2",
          "time": "20210905 03:21"
        },
        {
          "id": "MDRW2100031861.1.28",
          "form": "아니그나저나 유학 이야기",
          "original_form": "아니그나저나 유학 이야기",
          "speaker_id": "1",
          "time": "20210905 03:21"
        },
        {
          "id": "MDRW2100031861.1.29",
          "form": "중간중간떠들기도하고",
          "original_form": "중간중간떠들기도하고",
          "speaker_id": "2",
          "time": "20210905 03:21"
        },
        {
          "id": "MDRW2100031861.1.30",
          "form": "너무 듣고싶네요....",
          "original_form": "너무 듣고싶네요....",
          "speaker_id": "1",
          "time": "20210905 03:21"
        }
      ]
    }
