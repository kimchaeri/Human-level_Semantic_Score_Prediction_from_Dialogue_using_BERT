        },
        {
          "id": "MDRW2100036787.1.2",
          "form": "아침 드셨어요?",
          "original_form": "아침 드셨어요?",
          "speaker_id": "1",
          "time": "20210831 12:10"
        },
        {
          "id": "MDRW2100036787.1.3",
          "form": "안먹엇어요",
          "original_form": "안먹엇어요",
          "speaker_id": "2",
          "time": "20210831 12:10"
        },
        {
          "id": "MDRW2100036787.1.4",
          "form": "아침은 챙겨 드세요?",
          "original_form": "아침은 챙겨 드세요?",
          "speaker_id": "1",
          "time": "20210831 12:10"
        },
        {
          "id": "MDRW2100036787.1.5",
          "form": "안먹어요 근데 일찍일어나면 먹어요",
          "original_form": "안먹어요 근데 일찍일어나면 먹어요",
          "speaker_id": "2",
          "time": "20210831 12:10"
        },
        {
          "id": "MDRW2100036787.1.6",
          "form": "위가 놀랄텐데",
          "original_form": "위가 놀랄텐데",
          "speaker_id": "1",
          "time": "20210831 12:11"
        },
        {
          "id": "MDRW2100036787.1.7",
          "form": "그래도 먹긴해요. 일어나면 배가고파서",
          "original_form": "그래도 먹긴해요. 일어나면 배가고파서",
          "speaker_id": "2",
          "time": "20210831 12:11"
        },
        {
          "id": "MDRW2100036787.1.8",
          "form": "그쳐 저도 지금 뱌고파여",
          "original_form": "그쳐 저도 지금 뱌고파여",
          "speaker_id": "1",
          "time": "20210831 12:11"
        },
        {
          "id": "MDRW2100036787.1.9",
          "form": "일어나면 보통 뭐드세요?",
          "original_form": "일어나면 보통 뭐드세요?",
          "speaker_id": "2",
          "time": "20210831 12:11"
        },
        {
          "id": "MDRW2100036787.1.10",
          "form": "물이요",
          "original_form": "물이요",
          "speaker_id": "1",
          "time": "20210831 12:11"
        },
        {
          "id": "MDRW2100036787.1.11",
          "form": "저는 최근엔 라면먹엇어요",
          "original_form": "저는 최근엔 라면먹엇어요",
          "speaker_id": "2",
          "time": "20210831 12:12"
        },
        {
          "id": "MDRW2100036787.1.12",
          "form": "어떤 라면요?",
          "original_form": "어떤 라면요?",
          "speaker_id": "1",
          "time": "20210831 12:12"
        },
        {
          "id": "MDRW2100036787.1.13",
          "form": "집에 유통기한이 지난 오동통면먹으려고햇엇죠",
