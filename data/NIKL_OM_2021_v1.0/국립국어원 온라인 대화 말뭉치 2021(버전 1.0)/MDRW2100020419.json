      "utterance": [
        {
          "id": "MDRW2100020419.1.1",
          "form": "온라인쇼핑 자주하세요?",
          "original_form": "온라인쇼핑 자주하세요?",
          "speaker_id": "2",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100020419.1.2",
          "form": "할인쿠폰 뜨면 필요없는데도",
          "original_form": "할인쿠폰 뜨면 필요없는데도",
          "speaker_id": "1",
          "time": "20210829 06:43"
        },
        {
          "id": "MDRW2100020419.1.3",
          "form": "사게돼요ㅠ",
          "original_form": "사게돼요ㅠ",
          "speaker_id": "1",
          "time": "20210829 06:44"
        },
        {
          "id": "MDRW2100020419.1.4",
          "form": "그리고 무배 금액채우려고 괜히 더 추가하고ㅋㅋ",
          "original_form": "그리고 무배 금액채우려고 괜히 더 추가하고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:44"
        },
        {
          "id": "MDRW2100020419.1.5",
          "form": "헐 저도그래요 ㅋㅋㅋㅋㅋㅋ 아니 배송비 내고 그냥 사면 돈 덜쓰는데 왜 굳이굳이 ㅋㅋㅋㅋ 그러게 되는걸까요",
          "original_form": "헐 저도그래요 ㅋㅋㅋㅋㅋㅋ 아니 배송비 내고 그냥 사면 돈 덜쓰는데 왜 굳이굳이 ㅋㅋㅋㅋ 그러게 되는걸까요",
          "speaker_id": "2",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100020419.1.6",
          "form": "어제도ㅋㅋㅋㅋ코스트코 쇼핑갔다왔거든요",
          "original_form": "어제도ㅋㅋㅋㅋ코스트코 쇼핑갔다왔거든요",
          "speaker_id": "1",
          "time": "20210829 15:01"
        },
        {
          "id": "MDRW2100020419.1.7",
          "form": "수건이랑 요거트사러간건데",
          "original_form": "수건이랑 요거트사러간건데",
          "speaker_id": "1",
          "time": "20210829 15:01"
        },
        {
          "id": "MDRW2100020419.1.8",
          "form": "코스트코 수건 괜찮아요?",
          "original_form": "코스트코 수건 괜찮아요?",
          "speaker_id": "2",
          "time": "20210829 15:01"
        },
        {
          "id": "MDRW2100020419.1.9",
          "form": "갑~~~~자기 물티슈 사고 비타민도 두개나 산거있죠",
          "original_form": "갑~~~~자기 물티슈 사고 비타민도 두개나 산거있죠",
          "speaker_id": "1",
          "time": "20210829 15:01"
        },
        {
          "id": "MDRW2100020419.1.10",
          "form": "처음 사봤는데 좋아요~ 엄청 보드라운건 아닌데 부드러워요",
          "original_form": "처음 사봤는데 좋아요~ 엄청 보드라운건 아닌데 부드러워요",
          "speaker_id": "1",
          "time": "20210829 15:02"
        },
        {
          "id": "MDRW2100020419.1.11",
          "form": "저는 집안 행사있을때마다 수건제작해써욬ㅋㅋㅋㅋ",
          "original_form": "저는 집안 행사있을때마다 수건제작해써욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100020419.1.12",
          "form": "오~ 좋은?비싼수건으로 하세요??",
          "original_form": "오~ 좋은?비싼수건으로 하세요??",
          "speaker_id": "1",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100020419.1.13",
          "form": "아니면 적당한?!",
          "original_form": "아니면 적당한?!",
          "speaker_id": "1",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100020419.1.14",
          "form": "쓰기엔 얇은게 좋더라구요 ㅋㅋㅋ 문구 재밌게 제작해서 나눠드리면 친척어르신들 좋아하세요 ㅋㅋ",
          "original_form": "쓰기엔 얇은게 좋더라구요 ㅋㅋㅋ 문구 재밌게 제작해서 나눠드리면 친척어르신들 좋아하세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100020419.1.15",
          "form": "인터넷으로 수건 사려고 했는데",
          "original_form": "인터넷으로 수건 사려고 했는데",
          "speaker_id": "1",
          "time": "20210829 15:08"
        },
        {
          "id": "MDRW2100020419.1.16",
          "form": "무슨 종류가 그렇게 많은지ㅜ",
          "original_form": "무슨 종류가 그렇게 많은지ㅜ",
          "speaker_id": "1",
          "time": "20210829 15:08"
        },
        {
          "id": "MDRW2100020419.1.17",
          "form": "소량으로 해도 가격 괜찮아요?!",
          "original_form": "소량으로 해도 가격 괜찮아요?!",
          "speaker_id": "1",
          "time": "20210829 15:08"
        },
        {
          "id": "MDRW2100020419.1.18",
          "form": "한장당 사오천원정도 잡으면 될거같아요!",
          "original_form": "한장당 사오천원정도 잡으면 될거같아요!",
          "speaker_id": "2",
          "time": "20210829 15:09"
        },
        {
          "id": "MDRW2100020419.1.19",
          "form": "다음에 수건제작 해봐야겠어요ㅋㅋㅋ",
          "original_form": "다음에 수건제작 해봐야겠어요ㅋㅋㅋ",
