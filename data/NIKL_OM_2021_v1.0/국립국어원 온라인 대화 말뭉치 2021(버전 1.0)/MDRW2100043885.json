      "utterance": [
        {
          "id": "MDRW2100043885.1.1",
          "form": "술 좋아하세요?",
          "original_form": "술 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043885.1.2",
          "form": "아뇨ㅜ근데 사람만나는거 좋아하다보니 마시게되네요",
          "original_form": "아뇨ㅜ근데 사람만나는거 좋아하다보니 마시게되네요",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043885.1.3",
          "form": "약해서 다른사람다 괜찮을때 토해요",
          "original_form": "약해서 다른사람다 괜찮을때 토해요",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043885.1.4",
          "form": "맥주는 너무 배부르고",
          "original_form": "맥주는 너무 배부르고",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043885.1.5",
          "form": "청하? 와인?",
          "original_form": "청하? 와인?",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043885.1.6",
          "form": "아. 저는 이것저것 그냥 주는대로 다 마셔요.",
          "original_form": "아. 저는 이것저것 그냥 주는대로 다 마셔요.",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043885.1.7",
          "form": "다좋아하시는군요",
          "original_form": "다좋아하시는군요",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043885.1.8",
          "form": "그래도 그중에 뭐가좋으새요?",
          "original_form": "그래도 그중에 뭐가좋으새요?",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043885.1.9",
          "form": "맥주요. ㅎ\n맥주와 오징어",
          "original_form": "맥주요. ㅎ\n맥주와 오징어",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043885.1.10",
          "form": "아하 환상의 짝궁이죠",
          "original_form": "아하 환상의 짝궁이죠",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100043885.1.11",
          "form": "넵. 가끔은 맥주와 조미김을 먹는데 것도 맛있어요.ㅎ",
          "original_form": "넵. 가끔은 맥주와 조미김을 먹는데 것도 맛있어요.ㅎ",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100043885.1.12",
          "form": "ㅋㅋㅋ김이랑 먹으면 맛나죠",
          "original_form": "ㅋㅋㅋ김이랑 먹으면 맛나죠",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100043885.1.13",
          "form": "저는 닭발 야식 넘 좋아해요",
          "original_form": "저는 닭발 야식 넘 좋아해요",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043885.1.14",
          "form": "저도 닭발 사랑해요.\n어제도 먹었네요.ㅋ",
          "original_form": "저도 닭발 사랑해요.\n어제도 먹었네요.ㅋ",
          "speaker_id": "1",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100043885.1.15",
          "form": "근데 뼈있는건 못먹겠어요",
          "original_form": "근데 뼈있는건 못먹겠어요",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100043885.1.16",
          "form": "무뼈닭발도 맛있잖아요.\n청정원 안주야.ㅊㅋ",
          "original_form": "무뼈닭발도 맛있잖아요.\n청정원 안주야.ㅊㅋ",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100043885.1.17",
          "form": "무뼈 당근사랑하죠",
          "original_form": "무뼈 당근사랑하죠",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100043885.1.18",
          "form": "전 배달로 시켜먹어요 계란찜도 진짜 맛나잖아요",
          "original_form": "전 배달로 시켜먹어요 계란찜도 진짜 맛나잖아요",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100043885.1.19",
          "form": "전 배달맛집을 못찾았네요.;;",
          "original_form": "전 배달맛집을 못찾았네요.;;",
          "speaker_id": "1",
          "time": "20210907 21:39"
        }
      ]
