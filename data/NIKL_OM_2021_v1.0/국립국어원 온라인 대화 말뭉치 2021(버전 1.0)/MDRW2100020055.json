      "utterance": [
        {
          "id": "MDRW2100020055.1.1",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020055.1.2",
          "form": "짜장 짬뽕",
          "original_form": "짜장 짬뽕",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020055.1.3",
          "form": "짜장 좋아합니다",
          "original_form": "짜장 좋아합니다",
          "speaker_id": "1",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020055.1.4",
          "form": "설마 짬뽕?",
          "original_form": "설마 짬뽕?",
          "speaker_id": "1",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020055.1.5",
          "form": "이건 뭘 골라도 존중합니다",
          "original_form": "이건 뭘 골라도 존중합니다",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100020055.1.6",
          "form": "전 사실 둘 다 좋아하거든요",
          "original_form": "전 사실 둘 다 좋아하거든요",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100020055.1.7",
          "form": "설마 짜장입니다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "설마 짜장입니다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100020055.1.8",
          "form": "간짜장 좋아해요",
          "original_form": "간짜장 좋아해요",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100020055.1.9",
          "form": "헐 저도",
          "original_form": "헐 저도",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100020055.1.10",
          "form": "짬뽕은 짬뽕전문점이 확실히 맛잇더라구요",
          "original_form": "짬뽕은 짬뽕전문점이 확실히 맛잇더라구요",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100020055.1.11",
          "form": "일반중국집에선 잘 안시켜욬ㅋㅋㅋ",
          "original_form": "일반중국집에선 잘 안시켜욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100020055.1.12",
          "form": "맞아요 짬뽕 잘하는 집",
          "original_form": "맞아요 짬뽕 잘하는 집",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100020055.1.13",
          "form": "은근없어서",
          "original_form": "은근없어서",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100020055.1.14",
          "form": "저 중국집 처음 시키면 짬뽕국물 달라해서 국물부터 먹어봐요",
          "original_form": "저 중국집 처음 시키면 짬뽕국물 달라해서 국물부터 먹어봐요",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100020055.1.15",
          "form": "그럼 해물짬뽕 차돌짬뽕",
          "original_form": "그럼 해물짬뽕 차돌짬뽕",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100020055.1.16",
          "form": "차돌",
          "original_form": "차돌",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020055.1.17",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020055.1.18",
          "form": "맛잘알의 냄새가",
          "original_form": "맛잘알의 냄새가",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020055.1.19",
          "form": "가까워졋나요?",
          "original_form": "가까워졋나요?",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020055.1.20",
          "form": "두근두근",
          "original_form": "두근두근",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020055.1.21",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020055.1.22",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020055.1.23",
          "form": "우리 푸드메이트 1단계입니다",
          "original_form": "우리 푸드메이트 1단계입니다",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020055.1.24",
          "form": "그럼 탕수육",
          "original_form": "그럼 탕수육",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020055.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 몇단계까지잇죠?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 몇단계까지잇죠?",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020055.1.26",
          "form": "부먹 찍먹",
          "original_form": "부먹 찍먹",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020055.1.27",
          "form": "이거 550단계까지있어요",
          "original_form": "이거 550단계까지있어요",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020055.1.28",
          "form": "걱정하지마세요",
          "original_form": "걱정하지마세요",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020055.1.29",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
