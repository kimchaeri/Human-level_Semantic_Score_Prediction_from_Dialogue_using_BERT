      "utterance": [
        {
          "id": "MDRW2100028793.1.1",
          "form": "이번 쟈니 티저 사진 장난아냐",
          "original_form": "이번 쟈니 티저 사진 장난아냐",
          "speaker_id": "1",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028793.1.2",
          "form": "진심 대박",
          "original_form": "진심 대박",
          "speaker_id": "2",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100028793.1.3",
          "form": "나 완전 반해서 앨범 더사려고",
          "original_form": "나 완전 반해서 앨범 더사려고",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028793.1.4",
          "form": "또? 너 돈 없다며",
          "original_form": "또? 너 돈 없다며",
          "speaker_id": "2",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100028793.1.5",
          "form": "응... 그치 근데 약간 용돈 받으면 될 것 같기도해",
          "original_form": "응... 그치 근데 약간 용돈 받으면 될 것 같기도해",
          "speaker_id": "1",
          "time": "20210903 00:49"
        },
        {
          "id": "MDRW2100028793.1.6",
          "form": "난 용돈 아직,,,",
          "original_form": "난 용돈 아직,,,",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100028793.1.7",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100028793.1.8",
          "form": "나도 10일쯤에 받는데",
          "original_form": "나도 10일쯤에 받는데",
          "speaker_id": "1",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100028793.1.9",
          "form": "17일 컴백이니까... 그냥 바로 입금하고 궁핍하게 살려고",
          "original_form": "17일 컴백이니까... 그냥 바로 입금하고 궁핍하게 살려고",
          "speaker_id": "1",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100028793.1.10",
          "form": "아ㅋㅋㅋㅋㅋ난 모르겠어",
          "original_form": "아ㅋㅋㅋㅋㅋ난 모르겠어",
          "speaker_id": "2",
          "time": "20210903 13:50"
        },
        {
          "id": "MDRW2100028793.1.11",
          "form": "돈 언제 들어오지...",
          "original_form": "돈 언제 들어오지...",
          "speaker_id": "2",
          "time": "20210903 13:50"
        },
        {
          "id": "MDRW2100028793.1.12",
          "form": "알바라도 해야하나 싶어",
          "original_form": "알바라도 해야하나 싶어",
          "speaker_id": "1",
          "time": "20210903 13:51"
        },
        {
          "id": "MDRW2100028793.1.13",
          "form": "엔시티가 컴백하니까 나도 바빠지네...",
          "original_form": "엔시티가 컴백하니까 나도 바빠지네...",
          "speaker_id": "1",
          "time": "20210903 13:52"
        },
        {
          "id": "MDRW2100028793.1.14",
          "form": "헐 ncit 굿즈 나와",
          "original_form": "헐 ncit 굿즈 나와",
          "speaker_id": "1",
          "time": "20210903 13:53"
        },
        {
          "id": "MDRW2100028793.1.15",
          "form": "???또???",
          "original_form": "???또???",
          "speaker_id": "2",
          "time": "20210903 13:56"
        },
        {
          "id": "MDRW2100028793.1.16",
          "form": "나 텅장이야",
          "original_form": "나 텅장이야",
          "speaker_id": "2",
          "time": "20210903 13:56"
        },
        {
          "id": "MDRW2100028793.1.17",
          "form": "그만털어가",
          "original_form": "그만털어가",
          "speaker_id": "2",
          "time": "20210903 13:56"
        },
        {
          "id": "MDRW2100028793.1.18",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210903 13:56"
        },
        {
          "id": "MDRW2100028793.1.19",
          "form": "10월부터 판매한다는데",
          "original_form": "10월부터 판매한다는데",
          "speaker_id": "1",
          "time": "20210903 13:56"
        },
        {
          "id": "MDRW2100028793.1.20",
          "form": "그나마 다행이군...",
          "original_form": "그나마 다행이군...",
          "speaker_id": "1",
          "time": "20210903 13:56"
        },
