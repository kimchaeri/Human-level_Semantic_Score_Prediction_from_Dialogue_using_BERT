        },
        {
          "id": "MDRW2100047635.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210827 10:24"
        },
        {
          "id": "MDRW2100047635.1.3",
          "form": "유투브 보시나요?",
          "original_form": "유투브 보시나요?",
          "speaker_id": "1",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100047635.1.4",
          "form": "네 자주 봐요",
          "original_form": "네 자주 봐요",
          "speaker_id": "2",
          "time": "20210827 10:26"
        },
        {
          "id": "MDRW2100047635.1.5",
          "form": "어떤거요? 저는 냥이 먹방 구독해요",
          "original_form": "어떤거요? 저는 냥이 먹방 구독해요",
          "speaker_id": "1",
          "time": "20210827 10:27"
        },
        {
          "id": "MDRW2100047635.1.6",
          "form": "요리 레시피 보거나 가수들 뮤비나 무대 많이 봐요",
          "original_form": "요리 레시피 보거나 가수들 뮤비나 무대 많이 봐요",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100047635.1.7",
          "form": "오 가수누구 구독하셨나요",
          "original_form": "오 가수누구 구독하셨나요",
          "speaker_id": "1",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100047635.1.8",
          "form": "저는 해외 가수들 많이 구독하는 편이에요 한국은 남자 아이돌정도",
          "original_form": "저는 해외 가수들 많이 구독하는 편이에요 한국은 남자 아이돌정도",
          "speaker_id": "2",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100047635.1.9",
          "form": "오 해외가수누구요?",
          "original_form": "오 해외가수누구요?",
          "speaker_id": "1",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100047635.1.10",
          "form": "유명한 사람들은 거의 구독해요 아리아나그란데 포스트말론 테일러스위프트 저스틴비버 등등",
          "original_form": "유명한 사람들은 거의 구독해요 아리아나그란데 포스트말론 테일러스위프트 저스틴비버 등등",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100047635.1.11",
          "form": "저두 두아리파랑 아리아나그란데좋아해요",
          "original_form": "저두 두아리파랑 아리아나그란데좋아해요",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100047635.1.12",
          "form": "오 저도요ㅎㅎ 둘다 음색이 좋죠",
          "original_form": "오 저도요ㅎㅎ 둘다 음색이 좋죠",
          "speaker_id": "2",
          "time": "20210827 18:21"
        },
        {
          "id": "MDRW2100047635.1.13",
          "form": "두아리파가 더 취향이에요",
          "original_form": "두아리파가 더 취향이에요",
          "speaker_id": "1",
          "time": "20210827 19:04"
        },
        {
          "id": "MDRW2100047635.1.14",
          "form": "아리아나그란데와 두아리파중엔 두아리파노래가더 좋더라구요",
          "original_form": "아리아나그란데와 두아리파중엔 두아리파노래가더 좋더라구요",
