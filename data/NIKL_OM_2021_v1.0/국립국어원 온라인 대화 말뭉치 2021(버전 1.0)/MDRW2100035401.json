      "utterance": [
        {
          "id": "MDRW2100035401.1.1",
          "form": "요즘 듣는 음악 있어요?",
          "original_form": "요즘 듣는 음악 있어요?",
          "speaker_id": "2",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100035401.1.2",
          "form": "요즘 애니노래 듣고 있어요!",
          "original_form": "요즘 애니노래 듣고 있어요!",
          "speaker_id": "1",
          "time": "20210901 10:30"
        },
        {
          "id": "MDRW2100035401.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210901 10:30"
        },
        {
          "id": "MDRW2100035401.1.4",
          "form": "애닌 좋아하시는 구나",
          "original_form": "애닌 좋아하시는 구나",
          "speaker_id": "2",
          "time": "20210901 10:31"
        },
        {
          "id": "MDRW2100035401.1.5",
          "form": "요즘 전 해금 가야금 같은 전통악기 음악 듣고 있어요..ㅎㅎㅎ",
          "original_form": "요즘 전 해금 가야금 같은 전통악기 음악 듣고 있어요..ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 10:31"
        },
        {
          "id": "MDRW2100035401.1.6",
          "form": "오오 멋지네용!",
          "original_form": "오오 멋지네용!",
          "speaker_id": "1",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100035401.1.7",
          "form": "유투브로 음악 듣고 있어서 얼마전에 프리미엄 결제도 했어요..",
          "original_form": "유투브로 음악 듣고 있어서 얼마전에 프리미엄 결제도 했어요..",
          "speaker_id": "2",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100035401.1.8",
          "form": "우왕.. 유튜브로 들으시는군요!",
          "original_form": "우왕.. 유튜브로 들으시는군요!",
          "speaker_id": "1",
          "time": "20210901 12:09"
        },
        {
          "id": "MDRW2100035401.1.9",
          "form": "저는 멜론으로 듣고 있어요!",
          "original_form": "저는 멜론으로 듣고 있어요!",
          "speaker_id": "1",
          "time": "20210901 12:09"
        },
        {
          "id": "MDRW2100035401.1.10",
          "form": "멜론을 한벚도 안해봤어요",
          "original_form": "멜론을 한벚도 안해봤어요",
          "speaker_id": "2",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100035401.1.11",
          "form": "근데 유튜브에 국악 관련 노래모음집들이 있어서 듣다가",
          "original_form": "근데 유튜브에 국악 관련 노래모음집들이 있어서 듣다가",
          "speaker_id": "2",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100035401.1.12",
          "form": "광고 거슬려서 프리미엄 결재했죠",
          "original_form": "광고 거슬려서 프리미엄 결재했죠",
          "speaker_id": "2",
          "time": "20210901 12:12"
        },
        {
          "id": "MDRW2100035401.1.13",
          "form": "아항! 하긴 국악은 별로 없긴하죠ㅠ",
          "original_form": "아항! 하긴 국악은 별로 없긴하죠ㅠ",
          "speaker_id": "1",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100035401.1.14",
          "form": "유튜브.. 진짜 거슬려요",
          "original_form": "유튜브.. 진짜 거슬려요",
          "speaker_id": "1",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100035401.1.15",
          "form": "그래서 프리미엄 하고 나서 너무 좋더라구요.. 쭉 들을 수 있어서",
          "original_form": "그래서 프리미엄 하고 나서 너무 좋더라구요.. 쭉 들을 수 있어서",
          "speaker_id": "2",
          "time": "20210901 13:42"
        },
        {
          "id": "MDRW2100035401.1.16",
          "form": "진짜요 인기가 많아지니 유료되고ㅠ 그래도 좋으시다니 다행이에요",
          "original_form": "진짜요 인기가 많아지니 유료되고ㅠ 그래도 좋으시다니 다행이에요",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100035401.1.17",
          "form": "네.. 돈값을 해서요.. 저는 이정도 돈 낼 가치 있다고 봐요..ㅎㅎ",
          "original_form": "네.. 돈값을 해서요.. 저는 이정도 돈 낼 가치 있다고 봐요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100035401.1.18",
          "form": "그렇군용ㅋㅋ 돈 값하는건 드물긴하죵",
          "original_form": "그렇군용ㅋㅋ 돈 값하는건 드물긴하죵",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100035401.1.19",
          "form": "멜론은 괜찮나요? 그것도 결제하는거 아닌가요?",
          "original_form": "멜론은 괜찮나요? 그것도 결제하는거 아닌가요?",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100035401.1.20",
          "form": "맞아요! 역시 유료가..ㅋㅋ",
          "original_form": "맞아요! 역시 유료가..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100035401.1.21",
          "form": "근데 요즘엔 유튜브가 차라리 나은거같기도해요 멜론은 음원나온거 아님 못듣다보니 종종 없는게 있어 아쉽더라구요",
          "original_form": "근데 요즘엔 유튜브가 차라리 나은거같기도해요 멜론은 음원나온거 아님 못듣다보니 종종 없는게 있어 아쉽더라구요",
          "speaker_id": "1",
          "time": "20210901 21:28"
        }
      ]
