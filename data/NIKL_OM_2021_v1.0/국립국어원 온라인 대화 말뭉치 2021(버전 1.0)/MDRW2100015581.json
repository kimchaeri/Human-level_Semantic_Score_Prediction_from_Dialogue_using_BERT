        },
        {
          "id": "MDRW2100015581.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100015581.1.4",
          "form": "종교 활동하세요?",
          "original_form": "종교 활동하세요?",
          "speaker_id": "1",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100015581.1.5",
          "form": "아니요 님은요?",
          "original_form": "아니요 님은요?",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100015581.1.6",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100015581.1.7",
          "form": "무교라서",
          "original_form": "무교라서",
          "speaker_id": "1",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100015581.1.8",
          "form": "교회가보신적 있으세요?",
          "original_form": "교회가보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100015581.1.9",
          "form": "초딩때요ㅋㅋ",
          "original_form": "초딩때요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100015581.1.10",
          "form": "저도 어릴때 한번가봤어요ㅋㅋ",
          "original_form": "저도 어릴때 한번가봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100015581.1.11",
          "form": "과자받으러...",
          "original_form": "과자받으러...",
          "speaker_id": "1",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100015581.1.12",
          "form": "저는 문상 받았어요ㅋㅋ",
          "original_form": "저는 문상 받았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100015581.1.13",
          "form": "초딩때 문상 너무탐나죠ㅋㅋㅋㅋ",
          "original_form": "초딩때 문상 너무탐나죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:53"
        },
        {
          "id": "MDRW2100015581.1.14",
          "form": "절은 가보신적있나요?",
          "original_form": "절은 가보신적있나요?",
          "speaker_id": "1",
          "time": "20210826 12:53"
        },
        {
          "id": "MDRW2100015581.1.15",
          "form": "봉은사  가봤어요!",
          "original_form": "봉은사  가봤어요!",
          "speaker_id": "2",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100015581.1.16",
          "form": "절은 종교적이라기 보다 여행느낌으로 가게되는거 같아요",
          "original_form": "절은 종교적이라기 보다 여행느낌으로 가게되는거 같아요",
          "speaker_id": "1",
          "time": "20210826 13:07"
        },
        {
          "id": "MDRW2100015581.1.17",
          "form": "맞아요 스님이 종칠때마다 심신안정이와어ㅡ",
          "original_form": "맞아요 스님이 종칠때마다 심신안정이와어ㅡ",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100015581.1.18",
          "form": "어ㅡ",
          "original_form": "어ㅡ",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100015581.1.19",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100015581.1.20",
          "form": "왠만하면 풍경도 좋아서",
          "original_form": "왠만하면 풍경도 좋아서",
          "speaker_id": "1",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100015581.1.21",
          "form": "가면 마음이 편해요",
          "original_form": "가면 마음이 편해요",
          "speaker_id": "1",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100015581.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 13:14"
        }
      ]
