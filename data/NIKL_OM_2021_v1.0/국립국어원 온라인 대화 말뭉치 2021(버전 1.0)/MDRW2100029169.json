      "utterance": [
        {
          "id": "MDRW2100029169.1.1",
          "form": "피자에 콜라, 맥주?",
          "original_form": "피자에 콜라, 맥주?",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029169.1.2",
          "form": "저는 탄산 다 좋아해요",
          "original_form": "저는 탄산 다 좋아해요",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029169.1.3",
          "form": "어렵네요",
          "original_form": "어렵네요",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029169.1.4",
          "form": "둘다좋아해서ㅋㅋㅋ",
          "original_form": "둘다좋아해서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029169.1.5",
          "form": "ㅋㅋㅋ탄산 잘드시는구나",
          "original_form": "ㅋㅋㅋ탄산 잘드시는구나",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029169.1.6",
          "form": "그럼 최애 맥주로 하죠ㅎㅎ",
          "original_form": "그럼 최애 맥주로 하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029169.1.7",
          "form": "저는 칭따옵니다",
          "original_form": "저는 칭따옵니다",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029169.1.8",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029169.1.9",
          "form": "칭따오의 청량함 좋죠ㅎㅎ",
          "original_form": "칭따오의 청량함 좋죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029169.1.10",
          "form": "저는 코젤이라는 흑맥주가 원픽입니다",
          "original_form": "저는 코젤이라는 흑맥주가 원픽입니다",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029169.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029169.1.12",
          "form": "코젤다크",
          "original_form": "코젤다크",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029169.1.13",
          "form": "흑맥주였나요?",
          "original_form": "흑맥주였나요?",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029169.1.14",
          "form": "맞습니다ㅎㅎ",
          "original_form": "맞습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029169.1.15",
          "form": "안주는",
          "original_form": "안주는",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029169.1.16",
          "form": "개인적으로 기네스 흑맥보다 목넘김이 깔끔해서 좋아해요",
          "original_form": "개인적으로 기네스 흑맥보다 목넘김이 깔끔해서 좋아해요",
          "speaker_id": "2",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029169.1.17",
          "form": "뭐좋아하세요?",
          "original_form": "뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029169.1.18",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029169.1.19",
          "form": "저도먹어봐야겠어요",
          "original_form": "저도먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029169.1.20",
          "form": "보통 안주없이 마시는데",
          "original_form": "보통 안주없이 마시는데",
          "speaker_id": "2",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029169.1.21",
          "form": "코젤이 아니면 치킨이나 과자, 오징어 전기구이, 땅콩도 좋아합니다ㅋㅋ",
          "original_form": "코젤이 아니면 치킨이나 과자, 오징어 전기구이, 땅콩도 좋아합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029169.1.22",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029169.1.23",
          "form": "오징어전기구이ㅜ",
          "original_form": "오징어전기구이ㅜ",
          "speaker_id": "1",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029169.1.24",
          "form": "침고이네요",
          "original_form": "침고이네요",
          "speaker_id": "1",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029169.1.25",
          "form": "삼켜질때쯤 맥주 한모금 쭉 들이켜면 크아",
          "original_form": "삼켜질때쯤 맥주 한모금 쭉 들이켜면 크아",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029169.1.26",
          "form": "꼴깍 침넘어갑니다ㅋㅋㅋ",
          "original_form": "꼴깍 침넘어갑니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029169.1.27",
          "form": "하",
          "original_form": "하",
          "speaker_id": "1",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100029169.1.28",
          "form": "지금먹고싶어졌어요",
          "original_form": "지금먹고싶어졌어요",
          "speaker_id": "1",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100029169.1.29",
          "form": "책임져",
          "original_form": "책임져",
          "speaker_id": "1",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100029169.1.30",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100029169.1.31",
          "form": "저는 책임못집니다ㅋㅋㅋ 벌써 1년째 금주중이거든요ㅋㅋㅋㅋ",
          "original_form": "저는 책임못집니다ㅋㅋㅋ 벌써 1년째 금주중이거든요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:49"
        },
        {
          "id": "MDRW2100029169.1.32",
          "form": "얼른 외투챙기셔서 편의점 ㄱㄱ",
          "original_form": "얼른 외투챙기셔서 편의점 ㄱㄱ",
          "speaker_id": "2",
          "time": "20210903 00:49"
        },
        {
          "id": "MDRW2100029169.1.33",
          "form": "진짜로 외출하신건 아니죠..?",
          "original_form": "진짜로 외출하신건 아니죠..?",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029169.1.34",
          "form": "혹시 나가셨더라도 조심하셔요",
          "original_form": "혹시 나가셨더라도 조심하셔요",
