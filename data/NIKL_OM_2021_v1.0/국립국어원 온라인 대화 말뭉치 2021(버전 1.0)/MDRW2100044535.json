      "utterance": [
        {
          "id": "MDRW2100044535.1.1",
          "form": "그리고 손님들 너무 맛있어함",
          "original_form": "그리고 손님들 너무 맛있어함",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044535.1.2",
          "form": "고든램지 뺨치는 손목 스냅",
          "original_form": "고든램지 뺨치는 손목 스냅",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044535.1.3",
          "form": "진짜 웃겨",
          "original_form": "진짜 웃겨",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044535.1.4",
          "form": "누가봐도 어제 인사한 놈으로 만든 고기인데...",
          "original_form": "누가봐도 어제 인사한 놈으로 만든 고기인데...",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044535.1.5",
          "form": "뭐하자는건데",
          "original_form": "뭐하자는건데",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044535.1.6",
          "form": "그리고 평결도 함",
          "original_form": "그리고 평결도 함",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044535.1.7",
          "form": "완벽한 요리였어요!",
          "original_form": "완벽한 요리였어요!",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044535.1.8",
          "form": "결국 볶는거 밖에 안함",
          "original_form": "결국 볶는거 밖에 안함",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044535.1.9",
          "form": "(나:얼탱",
          "original_form": "(나:얼탱",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044535.1.10",
          "form": "어이없어 진짜",
          "original_form": "어이없어 진짜",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044535.1.11",
          "form": "ㅋㅋㅋ 아 그니까... 아주 멋지게 데코까지 하고",
          "original_form": "ㅋㅋㅋ 아 그니까... 아주 멋지게 데코까지 하고",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044535.1.12",
          "form": "님 당연히 님 좀따 죽는거 알죠?",
          "original_form": "님 당연히 님 좀따 죽는거 알죠?",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044535.1.13",
          "form": "근데 하면서 계속 궁금했던게",
          "original_form": "근데 하면서 계속 궁금했던게",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044535.1.14",
          "form": "그 동물 친구들은 자기가 도축 될거라는걸 알앗던걸까?",
          "original_form": "그 동물 친구들은 자기가 도축 될거라는걸 알앗던걸까?",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044535.1.15",
          "form": "알...지 않았을까? 모르는게 더 이상한것 같아",
          "original_form": "알...지 않았을까? 모르는게 더 이상한것 같아",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044535.1.16",
          "form": "결국 마지막에 한 명 남잖아",
          "original_form": "결국 마지막에 한 명 남잖아",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044535.1.17",
          "form": "물론 그 내용자체가 다른 것의 은유인지 진짜 있었던 일인지조차 나에겐 뭔가",
          "original_form": "물론 그 내용자체가 다른 것의 은유인지 진짜 있었던 일인지조차 나에겐 뭔가",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044535.1.18",
          "form": "그래도 담담하게 먹음 진짜 개어이없음 이런 코미디x",
          "original_form": "그래도 담담하게 먹음 진짜 개어이없음 이런 코미디x",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044535.1.19",
          "form": "분명치 않은 느낌인데",
          "original_form": "분명치 않은 느낌인데",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044535.1.20",
          "form": "응ㅇ으응ㅇ",
          "original_form": "응ㅇ으응ㅇ",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044535.1.21",
          "form": "맞와...",
          "original_form": "맞와...",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044535.1.22",
          "form": "그리고 생각한 점",
          "original_form": "그리고 생각한 점",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044535.1.23",
          "form": "그 피죤이였나???",
          "original_form": "그 피죤이였나???",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044535.1.24",
          "form": "그 친구가 뭐 화보 찍는다 뭐다 하자나",
          "original_form": "그 친구가 뭐 화보 찍는다 뭐다 하자나",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044535.1.25",
          "form": "나도... 이 시리즈 전반을 한건 아니지만 전반적으로 어떤 사건에 대한 은유라고 느꼈어 그게 실화이긴 어렵지 않을까..?",
          "original_form": "나도... 이 시리즈 전반을 한건 아니지만 전반적으로 어떤 사건에 대한 은유라고 느꼈어 그게 실화이긴 어렵지 않을까..?",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044535.1.26",
          "form": "ㅇ 맞아맞아",
          "original_form": "ㅇ 맞아맞아",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044535.1.27",
          "form": "그럼 뭐냐 장례식에 쓰는...",
          "original_form": "그럼 뭐냐 장례식에 쓰는...",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044535.1.28",
          "form": "피죤 아니고...칠면조인가? 그랬을걸",
          "original_form": "피죤 아니고...칠면조인가? 그랬을걸",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044535.1.29",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044535.1.30",
          "form": "영정사진인줄 알고",
          "original_form": "영정사진인줄 알고",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044535.1.31",
          "form": "영정이네...",
          "original_form": "영정이네...",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044535.1.32",
          "form": "찍은겨?",
          "original_form": "찍은겨?",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044535.1.33",
          "form": "응ㅇ응응",
          "original_form": "응ㅇ응응",
