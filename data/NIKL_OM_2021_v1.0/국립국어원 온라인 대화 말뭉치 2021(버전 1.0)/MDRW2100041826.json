        },
        {
          "id": "MDRW2100041826.1.3",
          "form": "저녁고름?",
          "original_form": "저녁고름?",
          "speaker_id": "1",
          "time": "20210906 16:21"
        },
        {
          "id": "MDRW2100041826.1.4",
          "form": "님은",
          "original_form": "님은",
          "speaker_id": "2",
          "time": "20210906 16:21"
        },
        {
          "id": "MDRW2100041826.1.5",
          "form": "나 굶으랴고",
          "original_form": "나 굶으랴고",
          "speaker_id": "1",
          "time": "20210906 16:21"
        },
        {
          "id": "MDRW2100041826.1.6",
          "form": "아님 넘 배고프면 프로틴",
          "original_form": "아님 넘 배고프면 프로틴",
          "speaker_id": "1",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100041826.1.7",
          "form": "프로틴 ㄱㄱ",
          "original_form": "프로틴 ㄱㄱ",
          "speaker_id": "2",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100041826.1.8",
          "form": "굶는검 별로야",
          "original_form": "굶는검 별로야",
          "speaker_id": "2",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100041826.1.9",
          "form": "근데굶으면확실히배들어가",
          "original_form": "근데굶으면확실히배들어가",
          "speaker_id": "1",
          "time": "20210906 16:24"
        },
        {
          "id": "MDRW2100041826.1.10",
          "form": "그건그래",
          "original_form": "그건그래",
          "speaker_id": "2",
          "time": "20210906 16:25"
        },
        {
          "id": "MDRW2100041826.1.11",
          "form": "탄수화물만 안먹어도",
          "original_form": "탄수화물만 안먹어도",
          "speaker_id": "2",
          "time": "20210906 16:25"
        },
        {
          "id": "MDRW2100041826.1.12",
          "form": "좀 들어가",
          "original_form": "좀 들어가",
          "speaker_id": "2",
          "time": "20210906 16:25"
        },
        {
          "id": "MDRW2100041826.1.13",
          "form": "쏙들어가더라",
          "original_form": "쏙들어가더라",
          "speaker_id": "1",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100041826.1.14",
          "form": "한 3일정도만 조이면 괜찮던데",
          "original_form": "한 3일정도만 조이면 괜찮던데",
          "speaker_id": "1",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100041826.1.15",
          "form": "마자 근데..요즘 라면부쩍먹어서 안들어가",
          "original_form": "마자 근데..요즘 라면부쩍먹어서 안들어가",
          "speaker_id": "2",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100041826.1.16",
          "form": "오늘점심두 콜레스테롤 와방 올렸어",
          "original_form": "오늘점심두 콜레스테롤 와방 올렸어",
          "speaker_id": "2",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100041826.1.17",
          "form": "뭐였니?",
          "original_form": "뭐였니?",
          "speaker_id": "1",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041826.1.18",
