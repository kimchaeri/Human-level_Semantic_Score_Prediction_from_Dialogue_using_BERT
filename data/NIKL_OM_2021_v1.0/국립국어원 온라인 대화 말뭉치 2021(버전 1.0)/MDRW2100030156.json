      "utterance": [
        {
          "id": "MDRW2100030156.1.1",
          "form": "커피 좋아하지",
          "original_form": "커피 좋아하지",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030156.1.2",
          "form": "무슨커피 좋아해?",
          "original_form": "무슨커피 좋아해?",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030156.1.3",
          "form": "아아?",
          "original_form": "아아?",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030156.1.4",
          "form": "난 아아 좋아해",
          "original_form": "난 아아 좋아해",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030156.1.5",
          "form": "케이크랑 먹으면 존맛",
          "original_form": "케이크랑 먹으면 존맛",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030156.1.6",
          "form": "나는 뜨아파",
          "original_form": "나는 뜨아파",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030156.1.7",
          "form": "뜨마도 맛있지",
          "original_form": "뜨마도 맛있지",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030156.1.8",
          "form": "드립커피도 맛남",
          "original_form": "드립커피도 맛남",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030156.1.9",
          "form": "하루를 시작하면",
          "original_form": "하루를 시작하면",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030156.1.10",
          "form": "커피를 마셔야지",
          "original_form": "커피를 마셔야지",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030156.1.11",
          "form": "커피가 진리지",
          "original_form": "커피가 진리지",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100030156.1.12",
          "form": "커피 내려마셔?",
          "original_form": "커피 내려마셔?",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100030156.1.13",
          "form": "응 드립이 맛있어",
          "original_form": "응 드립이 맛있어",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100030156.1.14",
          "form": "귀찮긴하지만",
          "original_form": "귀찮긴하지만",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100030156.1.15",
          "form": "맛이훨좋아",
          "original_form": "맛이훨좋아",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100030156.1.16",
          "form": "요즘 커피 값 오른다던데ㅜㅜ",
          "original_form": "요즘 커피 값 오른다던데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030156.1.17",
          "form": "아포카토도 맛있음",
          "original_form": "아포카토도 맛있음",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030156.1.18",
          "form": "과일은 제철과일이지",
          "original_form": "과일은 제철과일이지",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030156.1.19",
          "form": "단 쓴 단쓴 은 진리지 무한대로 들어간",
          "original_form": "단 쓴 단쓴 은 진리지 무한대로 들어간",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030156.1.20",
          "form": "지금은 복숭아 존맛",
          "original_form": "지금은 복숭아 존맛",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030156.1.21",
          "form": "딱복 물복?",
          "original_form": "딱복 물복?",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030156.1.22",
          "form": "난 물복",
          "original_form": "난 물복",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030156.1.23",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030156.1.24",
          "form": "당도 비슷하면 딱복",
          "original_form": "당도 비슷하면 딱복",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030156.1.25",
          "form": "식혜마시고싶다",
          "original_form": "식혜마시고싶다",
          "speaker_id": "1",
          "time": "20210903 22:42"
        }
      ]
