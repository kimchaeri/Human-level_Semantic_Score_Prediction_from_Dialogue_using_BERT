      "utterance": [
        {
          "id": "MDRW2100045153.1.1",
          "form": "이름이 몇글자인가요?",
          "original_form": "이름이 몇글자인가요?",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045153.1.2",
          "form": "두 글자 과일입니다 ㅎㅎ",
          "original_form": "두 글자 과일입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045153.1.3",
          "form": "힌트 다시 정리하자면",
          "original_form": "힌트 다시 정리하자면",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045153.1.4",
          "form": "따뜻한 곳에서 자라고 새콤하고",
          "original_form": "따뜻한 곳에서 자라고 새콤하고",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045153.1.5",
          "form": "껍질 못 먹어요",
          "original_form": "껍질 못 먹어요",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045153.1.6",
          "form": "두글자 열대과일은",
          "original_form": "두글자 열대과일은",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045153.1.7",
          "form": "망고밖에 생각이 안나요",
          "original_form": "망고밖에 생각이 안나요",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045153.1.8",
          "form": "어휘력이 망햇나봐요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "어휘력이 망햇나봐요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045153.1.9",
          "form": "ㅋㅋㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045153.1.10",
          "form": "아앗 아앗 좀 더 새콤해요",
          "original_form": "아앗 아앗 좀 더 새콤해요",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045153.1.11",
          "form": "셔요 셔",
          "original_form": "셔요 셔",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045153.1.12",
          "form": "레몬....ㅠ",
          "original_form": "레몬....ㅠ",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045153.1.13",
          "form": "그치만 노란색이 아니라고 햇으니까",
          "original_form": "그치만 노란색이 아니라고 햇으니까",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045153.1.14",
          "form": "레몬 사촌!",
          "original_form": "레몬 사촌!",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045153.1.15",
          "form": "아 라임이요?",
          "original_form": "아 라임이요?",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045153.1.16",
          "form": "아니 사촌까진 아니고 남이구나",
          "original_form": "아니 사촌까진 아니고 남이구나",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045153.1.17",
          "form": "ㅋㅌㅋㅋ죄송해요 혼동을 드럈어요",
          "original_form": "ㅋㅌㅋㅋ죄송해요 혼동을 드럈어요",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045153.1.18",
          "form": "ㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045153.1.19",
          "form": "귤 과는 아니예요",
          "original_form": "귤 과는 아니예요",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045153.1.20",
          "form": "순간 설렜네요",
          "original_form": "순간 설렜네요",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045153.1.21",
          "form": "거의 다 왔어요!",
          "original_form": "거의 다 왔어요!",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045153.1.22",
          "form": "깔라만시 하기엔 두글자가 아니규",
          "original_form": "깔라만시 하기엔 두글자가 아니규",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045153.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
