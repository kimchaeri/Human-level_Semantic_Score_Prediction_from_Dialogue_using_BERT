      "utterance": [
        {
          "id": "MDRW2100039410.1.1",
          "form": "식음료가 제일 무난하더라구요ㅋㅋㅋ!",
          "original_form": "식음료가 제일 무난하더라구요ㅋㅋㅋ!",
          "speaker_id": "1",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100039410.1.2",
          "form": "요새 꽂힌 음식? 같은거 있나요?",
          "original_form": "요새 꽂힌 음식? 같은거 있나요?",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039410.1.3",
          "form": "저는 그릭복숭아? 유행하길래 따라 해 먹어봤는데 맛있더라구요",
          "original_form": "저는 그릭복숭아? 유행하길래 따라 해 먹어봤는데 맛있더라구요",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039410.1.4",
          "form": "저는 마라탕이요ㅋㅋㅋ 1단계가 제일 먹기 괜찮았어요!",
          "original_form": "저는 마라탕이요ㅋㅋㅋ 1단계가 제일 먹기 괜찮았어요!",
          "speaker_id": "2",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039410.1.5",
          "form": "헐 그릭복숭아ㅠㅠㅠ",
          "original_form": "헐 그릭복숭아ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039410.1.6",
          "form": "집에 딱복 있는데 해먹어볼까 고민이에요!",
          "original_form": "집에 딱복 있는데 해먹어볼까 고민이에요!",
          "speaker_id": "2",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039410.1.7",
          "form": "그릭요거트 사서 넣으셨나요? 아니면 만들어서 넣으셨나요!",
          "original_form": "그릭요거트 사서 넣으셨나요? 아니면 만들어서 넣으셨나요!",
          "speaker_id": "2",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039410.1.8",
          "form": "그릭요거트 직접해먹었어요!",
          "original_form": "그릭요거트 직접해먹었어요!",
          "speaker_id": "1",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039410.1.9",
          "form": "체 위에 면보놓고 일반요거트 얹어서 물만 빼면 되는데",
          "original_form": "체 위에 면보놓고 일반요거트 얹어서 물만 빼면 되는데",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039410.1.10",
          "form": "헐 대박... 어떤 브랜드 요거트 드시나요ㅋㅋㅋ",
          "original_form": "헐 대박... 어떤 브랜드 요거트 드시나요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039410.1.11",
          "form": "물 빼고 나면 양이 많이 줄어서 넉넉히 만들어야겠더라구요!",
          "original_form": "물 빼고 나면 양이 많이 줄어서 넉넉히 만들어야겠더라구요!",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039410.1.12",
          "form": "대충 마트에서 제일 싼거요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "대충 마트에서 제일 싼거요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039410.1.13",
          "form": "요즘에는 그릭요거트해서 큰거 팔더라고요!",
          "original_form": "요즘에는 그릭요거트해서 큰거 팔더라고요!",
          "speaker_id": "2",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039410.1.14",
          "form": "마트 최고ㅋㅋㅋ",
          "original_form": "마트 최고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039410.1.15",
          "form": "맞아요 사실 시판이 최고에요ㅋㅋㅋㅋ",
          "original_form": "맞아요 사실 시판이 최고에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039410.1.16",
          "form": "그냥 일반 요거트는 별론가요??",
          "original_form": "그냥 일반 요거트는 별론가요??",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039410.1.17",
          "form": "전에 보니까 냉동실 넣어서 복숭아 꽝꽝 얼리신 분도 계시던데ㅋㅋㅋㅋ",
          "original_form": "전에 보니까 냉동실 넣어서 복숭아 꽝꽝 얼리신 분도 계시던데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039410.1.18",
          "form": "일반 요거트도 맛있을 것 같긴한데",
          "original_form": "일반 요거트도 맛있을 것 같긴한데",
          "speaker_id": "1",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039410.1.19",
          "form": "좀 흘러내릴 것 같긴해요",
          "original_form": "좀 흘러내릴 것 같긴해요",
          "speaker_id": "1",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039410.1.20",
          "form": "그릭요거트 대신에 마스카포네 치즈 채우시는 분도 있던데",
          "original_form": "그릭요거트 대신에 마스카포네 치즈 채우시는 분도 있던데",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039410.1.21",
          "form": "앗 그렇겠죠...",
          "original_form": "앗 그렇겠죠...",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039410.1.22",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039410.1.23",
          "form": "사실 이게 제일 맛있을 것 같아요ㅋㅋㅋㅋ",
          "original_form": "사실 이게 제일 맛있을 것 같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039410.1.24",
          "form": "마스카포네 치즈도 맛있대요..",
          "original_form": "마스카포네 치즈도 맛있대요..",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039410.1.25",
          "form": "저는 둘 다 도전해보고싶어요",
          "original_form": "저는 둘 다 도전해보고싶어요",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039410.1.26",
          "form": "맛이 없을 수가 없는 조합이죠ㅋㅋㅋㅋ",
          "original_form": "맛이 없을 수가 없는 조합이죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039410.1.27",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039410.1.28",
          "form": "저도 언제 꼭 도전해보려구요!",
          "original_form": "저도 언제 꼭 도전해보려구요!",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039410.1.29",
          "form": "물복 좋아하세요 딱복 좋아하세요??",
          "original_form": "물복 좋아하세요 딱복 좋아하세요??",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039410.1.30",
          "form": "헐 좋아요",
          "original_form": "헐 좋아요",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039410.1.31",
          "form": "복숭아시즌이 끝나기전에!",
          "original_form": "복숭아시즌이 끝나기전에!",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039410.1.32",
          "form": "후기 알려주세요..ㅋㅋㅋ",
          "original_form": "후기 알려주세요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039410.1.33",
          "form": "어렸을땐 물복이었는데",
          "original_form": "어렸을땐 물복이었는데",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039410.1.34",
          "form": "넵ㅋㅋㅋㅋ",
          "original_form": "넵ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039410.1.35",
          "form": "요즘에는 딱복?",
          "original_form": "요즘에는 딱복?",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039410.1.36",
          "form": "요즘은 딱복이 끌리더라구요ㅋㅋㄱㄴ",
          "original_form": "요즘은 딱복이 끌리더라구요ㅋㅋㄱㄴ",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039410.1.37",
          "form": "저는 다 좋아해섴ㅋㅋㅋ",
          "original_form": "저는 다 좋아해섴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039410.1.38",
          "form": "과일은 가리는거 없어요!",
          "original_form": "과일은 가리는거 없어요!",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100039410.1.39",
          "form": "근데 그릭복숭아엔 물복이 더 어울릴 것 같아요ㅋㄱㄴㅋ",
          "original_form": "근데 그릭복숭아엔 물복이 더 어울릴 것 같아요ㅋㄱㄴㅋ",
          "speaker_id": "1",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100039410.1.40",
          "form": "각자의 매력이 있죠!",
          "original_form": "각자의 매력이 있죠!",
          "speaker_id": "1",
          "time": "20210903 17:28"
        }
      ]
    }
