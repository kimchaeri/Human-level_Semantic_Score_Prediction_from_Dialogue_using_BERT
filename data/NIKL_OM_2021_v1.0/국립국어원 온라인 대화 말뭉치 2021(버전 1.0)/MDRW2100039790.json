      "utterance": [
        {
          "id": "MDRW2100039790.1.1",
          "form": "왜 반말임?",
          "original_form": "왜 반말임?",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039790.1.2",
          "form": "있음",
          "original_form": "있음",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039790.1.3",
          "form": "뭔데",
          "original_form": "뭔데",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039790.1.4",
          "form": "아니 왜 반말 !!",
          "original_form": "아니 왜 반말 !!",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039790.1.5",
          "form": "안알려줌",
          "original_form": "안알려줌",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039790.1.6",
          "form": "아ㅋ",
          "original_form": "아ㅋ",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039790.1.7",
          "form": "대화하기 싫은가봄?",
          "original_form": "대화하기 싫은가봄?",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039790.1.8",
          "form": "나 반모 안받아요",
          "original_form": "나 반모 안받아요",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039790.1.9",
          "form": "음 나의 최애노래는",
          "original_form": "음 나의 최애노래는",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039790.1.10",
          "form": "숫자송",
          "original_form": "숫자송",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039790.1.11",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039790.1.12",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039790.1.13",
          "form": "한국을 빛낸 100명의 위인들이요",
          "original_form": "한국을 빛낸 100명의 위인들이요",
          "speaker_id": "1",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039790.1.14",
          "form": "불러보세요",
          "original_form": "불러보세요",
          "speaker_id": "2",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039790.1.15",
          "form": "맞서싸운 관창!",
          "original_form": "맞서싸운 관창!",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039790.1.16",
          "form": "거기밖에 모르죠 ?",
          "original_form": "거기밖에 모르죠 ?",
          "speaker_id": "2",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039790.1.17",
          "form": "님 독도는 우리땅 노래는 아세요?",
          "original_form": "님 독도는 우리땅 노래는 아세요?",
          "speaker_id": "2",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039790.1.18",
          "form": "알죠",
          "original_form": "알죠",
          "speaker_id": "1",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039790.1.19",
          "form": "당연히",
          "original_form": "당연히",
          "speaker_id": "1",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039790.1.20",
          "form": "외로운 섬 하나 고고의 고향....",
          "original_form": "외로운 섬 하나 고고의 고향....",
          "speaker_id": "1",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039790.1.21",
          "form": "그 누가 아무리 자기네 땅이라고 우겨도 독도는 고고 땅....",
          "original_form": "그 누가 아무리 자기네 땅이라고 우겨도 독도는 고고 땅....",
          "speaker_id": "1",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039790.1.22",
          "form": "삼인칭 싫어요",
          "original_form": "삼인칭 싫어요",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039790.1.23",
          "form": "ㅋ네",
          "original_form": "ㅋ네",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039790.1.24",
          "form": "아니 진짜 님 간첩되실 뻔 했어요",
          "original_form": "아니 진짜 님 간첩되실 뻔 했어요",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039790.1.25",
          "form": "알아요 ?",
          "original_form": "알아요 ?",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039790.1.26",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039790.1.27",
          "form": "님 젛아하는 가수 있어요?",
          "original_form": "님 젛아하는 가수 있어요?",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039790.1.28",
          "form": "네 있어요",
          "original_form": "네 있어요",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039790.1.29",
          "form": "내가 맞춰볼게요",
          "original_form": "내가 맞춰볼게요",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039790.1.30",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039790.1.31",
          "form": "저죠",
          "original_form": "저죠",
          "speaker_id": "1",
          "time": "20210903 21:37"
        }
      ]
    }
