      "utterance": [
        {
          "id": "MDRW2100004839.1.1",
          "form": "옷 주록 어디서 사?",
          "original_form": "옷 주록 어디서 사?",
          "speaker_id": "1",
          "time": "20210821 18:48"
        },
        {
          "id": "MDRW2100004839.1.2",
          "form": "온라인? 지하상가?",
          "original_form": "온라인? 지하상가?",
          "speaker_id": "1",
          "time": "20210821 18:48"
        },
        {
          "id": "MDRW2100004839.1.3",
          "form": "최근에는 온라인",
          "original_form": "최근에는 온라인",
          "speaker_id": "2",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004839.1.4",
          "form": "예전에는 직접 가서 이것저것 재보고 샀었는데",
          "original_form": "예전에는 직접 가서 이것저것 재보고 샀었는데",
          "speaker_id": "2",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004839.1.5",
          "form": "이젠 귀찮아졌어",
          "original_form": "이젠 귀찮아졌어",
          "speaker_id": "2",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004839.1.6",
          "form": "맞아....",
          "original_form": "맞아....",
          "speaker_id": "1",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004839.1.7",
          "form": "그래서 요새는 퀄리티 평범한 옷가게 하나 잡아서 거기서만 쇼핑함",
          "original_form": "그래서 요새는 퀄리티 평범한 옷가게 하나 잡아서 거기서만 쇼핑함",
          "speaker_id": "2",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004839.1.8",
          "form": "적어도 실패는 안한다. 무난해",
          "original_form": "적어도 실패는 안한다. 무난해",
          "speaker_id": "2",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004839.1.9",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:03"
        },
        {
          "id": "MDRW2100004839.1.10",
          "form": "뭔지 암ㅋㅋㅋㅋ",
          "original_form": "뭔지 암ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:03"
        },
        {
          "id": "MDRW2100004839.1.11",
          "form": "근데 물건 질은 괜찮은데",
          "original_form": "근데 물건 질은 괜찮은데",
          "speaker_id": "2",
          "time": "20210821 19:04"
        },
        {
          "id": "MDRW2100004839.1.12",
          "form": "자꾸 사이즈를 못맞춰서 낭패야",
          "original_form": "자꾸 사이즈를 못맞춰서 낭패야",
          "speaker_id": "2",
          "time": "20210821 19:05"
        },
        {
          "id": "MDRW2100004839.1.13",
          "form": "저번에 할인하길래 자켓을 샀는데",
          "original_form": "저번에 할인하길래 자켓을 샀는데",
          "speaker_id": "2",
          "time": "20210821 19:05"
        },
        {
          "id": "MDRW2100004839.1.14",
          "form": "너무 커가지고, 걍 오버사이즈 패션이라고 우기면서 입고있어",
          "original_form": "너무 커가지고, 걍 오버사이즈 패션이라고 우기면서 입고있어",
          "speaker_id": "2",
          "time": "20210821 19:05"
        },
        {
          "id": "MDRW2100004839.1.15",
          "form": "아앗ㅠㅠ",
          "original_form": "아앗ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 19:06"
        },
        {
          "id": "MDRW2100004839.1.16",
          "form": "어디냐",
          "original_form": "어디냐",
          "speaker_id": "1",
          "time": "20210821 19:06"
        },
        {
          "id": "MDRW2100004839.1.17",
          "form": "나에게도",
          "original_form": "나에게도",
          "speaker_id": "1",
          "time": "20210821 19:06"
        },
        {
          "id": "MDRW2100004839.1.18",
          "form": "알려달라",
          "original_form": "알려달라",
          "speaker_id": "1",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004839.1.19",
          "form": "이거 브랜드 네임 그대로 말해도 되려나?",
          "original_form": "이거 브랜드 네임 그대로 말해도 되려나?",
          "speaker_id": "2",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004839.1.20",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004839.1.21",
          "form": "알았어ㅋㅋ",
          "original_form": "알았어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004839.1.22",
          "form": "즐거운 쇼핑 되시길...",
          "original_form": "즐거운 쇼핑 되시길...",
          "speaker_id": "2",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004839.1.23",
          "form": "고맙습니다",
          "original_form": "고맙습니다",
          "speaker_id": "1",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004839.1.24",
          "form": "가격 괜찮음?",
          "original_form": "가격 괜찮음?",
          "speaker_id": "1",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004839.1.25",
          "form": "무난해",
          "original_form": "무난해",
          "speaker_id": "2",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004839.1.26",
          "form": "어느 정도함?",
          "original_form": "어느 정도함?",
          "speaker_id": "1",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004839.1.27",
          "form": "싼건 2-3만",
          "original_form": "싼건 2-3만",
          "speaker_id": "2",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004839.1.28",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004839.1.29",
          "form": "비싼건 5-6만",
          "original_form": "비싼건 5-6만",
          "speaker_id": "2",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004839.1.30",
          "form": "싸네",
          "original_form": "싸네",
          "speaker_id": "1",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004839.1.31",
          "form": "무난하지",
          "original_form": "무난하지",
          "speaker_id": "2",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004839.1.32",
          "form": "퀄리티도 무난하고 가격도 무난해",
          "original_form": "퀄리티도 무난하고 가격도 무난해",
          "speaker_id": "2",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004839.1.33",
          "form": "감사감사",
          "original_form": "감사감사",
          "speaker_id": "1",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004839.1.34",
          "form": "가끔 주말에 배송비 무료쿠폰 주니까 잘 노려서 사",
          "original_form": "가끔 주말에 배송비 무료쿠폰 주니까 잘 노려서 사",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004839.1.35",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004839.1.36",
