      "utterance": [
        {
          "id": "MDRW2100033679.1.1",
          "form": "최근에 또 뭐 사신거. 있으세요?",
          "original_form": "최근에 또 뭐 사신거. 있으세요?",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100033679.1.2",
          "form": "책이랑 액정타블렛이요 ㅎㅎ",
          "original_form": "책이랑 액정타블렛이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100033679.1.3",
          "form": "액정타블렛은 어떤거에요?",
          "original_form": "액정타블렛은 어떤거에요?",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100033679.1.4",
          "form": "아이패드 위에 펜으로 그림그릴수있잖아요",
          "original_form": "아이패드 위에 펜으로 그림그릴수있잖아요",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100033679.1.5",
          "form": "그걸 좀더 큰 화면으로 필기감 좋게 만든거에요",
          "original_form": "그걸 좀더 큰 화면으로 필기감 좋게 만든거에요",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100033679.1.6",
          "form": "와~~~~~~~~ 좋네요 전아이패드가 없어서 근데 들어만봐도 갖고픈ㅋㅋ. 기분",
          "original_form": "와~~~~~~~~ 좋네요 전아이패드가 없어서 근데 들어만봐도 갖고픈ㅋㅋ. 기분",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100033679.1.7",
          "form": "네 그림그리기 차근차근 배워보실요량이면 한번 생각해보세요",
          "original_form": "네 그림그리기 차근차근 배워보실요량이면 한번 생각해보세요",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100033679.1.8",
          "form": "테스트해보는 매장도있다고하더라구요",
          "original_form": "테스트해보는 매장도있다고하더라구요",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100033679.1.9",
          "form": "저 똥손이라ㅋㅋ",
          "original_form": "저 똥손이라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100033679.1.10",
          "form": "저도 잘그리는건 아닌데요",
          "original_form": "저도 잘그리는건 아닌데요",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100033679.1.11",
          "form": "그게요 그리는 방법이 있던데요",
          "original_form": "그게요 그리는 방법이 있던데요",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100033679.1.12",
          "form": "그걸 차근차근 배워서 그리면 웬만큼 하겠더라구요",
          "original_form": "그걸 차근차근 배워서 그리면 웬만큼 하겠더라구요",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100033679.1.13",
          "form": "미술도 딸애 하는거 보면 기본기가 다져져야 되더라구요",
          "original_form": "미술도 딸애 하는거 보면 기본기가 다져져야 되더라구요",
          "speaker_id": "2",
          "time": "20210826 00:49"
        },
        {
          "id": "MDRW2100033679.1.14",
          "form": "책도 있던데 하루 한장 그리기 요런거 ㅋㅋ",
          "original_form": "책도 있던데 하루 한장 그리기 요런거 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:49"
        },
        {
          "id": "MDRW2100033679.1.15",
          "form": "서점에서 살수있는 책이에요?",
          "original_form": "서점에서 살수있는 책이에요?",
          "speaker_id": "1",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100033679.1.16",
          "form": "보니까요 저는 지금까지 겉모습만 봤는데",
          "original_form": "보니까요 저는 지금까지 겉모습만 봤는데",
          "speaker_id": "1",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100033679.1.17",
          "form": "그건 잘 모르겠어요 저는 인터넷에서 사줬거든요",
          "original_form": "그건 잘 모르겠어요 저는 인터넷에서 사줬거든요",
          "speaker_id": "2",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100033679.1.18",
          "form": "책제목이뭐에요? 하루 한장 그리기에요?",
          "original_form": "책제목이뭐에요? 하루 한장 그리기에요?",
          "speaker_id": "1",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100033679.1.19",
          "form": "하루에 한장 똑똑한 그리기 네요 ㅎ",
          "original_form": "하루에 한장 똑똑한 그리기 네요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:57"
        },
        {
          "id": "MDRW2100033679.1.20",
          "form": "아아 감사합니다",
          "original_form": "아아 감사합니다",
          "speaker_id": "1",
          "time": "20210826 00:57"
        },
        {
          "id": "MDRW2100033679.1.21",
          "form": "입체감이랑요",
          "original_form": "입체감이랑요",
          "speaker_id": "1",
          "time": "20210826 00:57"
        },
        {
          "id": "MDRW2100033679.1.22",
          "form": "잘그리는 사람은 덩어리를 입체로 쪼개서 보구요",
          "original_form": "잘그리는 사람은 덩어리를 입체로 쪼개서 보구요",
