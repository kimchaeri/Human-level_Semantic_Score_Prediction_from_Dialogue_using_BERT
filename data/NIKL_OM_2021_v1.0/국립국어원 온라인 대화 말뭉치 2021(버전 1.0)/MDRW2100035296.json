      "utterance": [
        {
          "id": "MDRW2100035296.1.1",
          "form": "비오는데 뭐하고 있니",
          "original_form": "비오는데 뭐하고 있니",
          "speaker_id": "2",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100035296.1.2",
          "form": "응 언니ㅋㅋ둘째재우려고",
          "original_form": "응 언니ㅋㅋ둘째재우려고",
          "speaker_id": "1",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100035296.1.3",
          "form": "언니는 집 오는길이가?",
          "original_form": "언니는 집 오는길이가?",
          "speaker_id": "1",
          "time": "20210829 14:31"
        },
        {
          "id": "MDRW2100035296.1.4",
          "form": "엉 도착해서 짐정리중ㅇ",
          "original_form": "엉 도착해서 짐정리중ㅇ",
          "speaker_id": "2",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100035296.1.5",
          "form": "햇빛나길래 다시 타프치러간다 ㅠ 말려야되서",
          "original_form": "햇빛나길래 다시 타프치러간다 ㅠ 말려야되서",
          "speaker_id": "2",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100035296.1.6",
          "form": "피곤하넹",
          "original_form": "피곤하넹",
          "speaker_id": "2",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100035296.1.7",
          "form": "고생했다잉 ㅜㅠ아고 일이다ㅋ",
          "original_form": "고생했다잉 ㅜㅠ아고 일이다ㅋ",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100035296.1.8",
          "form": "잘놀고왔나",
          "original_form": "잘놀고왔나",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100035296.1.9",
          "form": "엉 ㅎㅎ 다행히 어제 하루 안와서 물놀이는 했음",
          "original_form": "엉 ㅎㅎ 다행히 어제 하루 안와서 물놀이는 했음",
          "speaker_id": "2",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100035296.1.10",
          "form": "물놀이하면서도 불안하고;;;ㅇ",
          "original_form": "물놀이하면서도 불안하고;;;ㅇ",
          "speaker_id": "2",
          "time": "20210829 16:28"
        },
        {
          "id": "MDRW2100035296.1.11",
          "form": "올 여름 물놀이는 끝이구먼",
          "original_form": "올 여름 물놀이는 끝이구먼",
          "speaker_id": "2",
          "time": "20210829 16:28"
        },
        {
          "id": "MDRW2100035296.1.12",
          "form": "ㅋㅋ맞다 이제 물놀이는 끝인듯",
          "original_form": "ㅋㅋ맞다 이제 물놀이는 끝인듯",
          "speaker_id": "1",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100035296.1.13",
          "form": "그래도 잘놀다왔네ㅋㅋ",
          "original_form": "그래도 잘놀다왔네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100035296.1.14",
          "form": "체력이 대단하다 다들ㅋ",
          "original_form": "체력이 대단하다 다들ㅋ",
          "speaker_id": "1",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100035296.1.15",
          "form": "디다",
          "original_form": "디다",
          "speaker_id": "2",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100035296.1.16",
          "form": "오늘은 일찍 자야지",
          "original_form": "오늘은 일찍 자야지",
          "speaker_id": "2",
          "time": "20210829 16:31"
        },
        {
          "id": "MDRW2100035296.1.17",
          "form": "응 언니 어여쉬어이ㅋㅋ",
          "original_form": "응 언니 어여쉬어이ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:31"
        },
        {
          "id": "MDRW2100035296.1.18",
          "form": "충전하고 톡해ㅋ",
          "original_form": "충전하고 톡해ㅋ",
          "speaker_id": "1",
          "time": "20210829 16:31"
        },
        {
          "id": "MDRW2100035296.1.19",
          "form": "내년되면 같이 가자 ㅎㅎ",
          "original_form": "내년되면 같이 가자 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:31"
        },
        {
          "id": "MDRW2100035296.1.20",
          "form": "응 좋지 내년은 괜찮지않겠나싶다ㅋㅋ",
          "original_form": "응 좋지 내년은 괜찮지않겠나싶다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100035296.1.21",
          "form": "둘째가 좀 더 크면ㅋ",
          "original_form": "둘째가 좀 더 크면ㅋ",
          "speaker_id": "1",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100035296.1.22",
          "form": "엉 좋다 콜콜 ㅎㅎ",
          "original_form": "엉 좋다 콜콜 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 07:33"
        },
        {
          "id": "MDRW2100035296.1.23",
          "form": "어제 9시에 뻗었음",
          "original_form": "어제 9시에 뻗었음",
          "speaker_id": "2",
          "time": "20210830 07:33"
        },
        {
          "id": "MDRW2100035296.1.24",
          "form": "완전 피곤했나봐",
          "original_form": "완전 피곤했나봐",
          "speaker_id": "1",
          "time": "20210830 07:50"
        },
        {
          "id": "MDRW2100035296.1.25",
          "form": "내가 오늘은 그렇게 잘듯 ㅋㅋ",
          "original_form": "내가 오늘은 그렇게 잘듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 07:51"
        },
        {
          "id": "MDRW2100035296.1.26",
          "form": "오늘 새벽에",
          "original_form": "오늘 새벽에",
          "speaker_id": "1",
          "time": "20210830 07:51"
        },
        {
          "id": "MDRW2100035296.1.27",
          "form": "잤더니",
          "original_form": "잤더니",
          "speaker_id": "1",
          "time": "20210830 07:51"
        },
        {
          "id": "MDRW2100035296.1.28",
          "form": "피곤하네 ㅋㅋ",
          "original_form": "피곤하네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 07:51"
        },
        {
          "id": "MDRW2100035296.1.29",
          "form": "요새 왜 맨날 늦게자농",
          "original_form": "요새 왜 맨날 늦게자농",
          "speaker_id": "2",
          "time": "20210830 08:37"
        },
        {
          "id": "MDRW2100035296.1.30",
          "form": "밤에 연락할 때마다 깨 있는 듯",
          "original_form": "밤에 연락할 때마다 깨 있는 듯",
          "speaker_id": "2",
          "time": "20210830 08:37"
        },
        {
          "id": "MDRW2100035296.1.31",
          "form": "알바갔나??",
          "original_form": "알바갔나??",
          "speaker_id": "2",
          "time": "20210830 14:14"
        },
        {
          "id": "MDRW2100035296.1.32",
          "form": "웅웅 알바갔다가 볼일봤다가 이제 좀 쉰다. ㅋㅋ 애들 또 와가꼬 바빴네",
          "original_form": "웅웅 알바갔다가 볼일봤다가 이제 좀 쉰다. ㅋㅋ 애들 또 와가꼬 바빴네",
          "speaker_id": "1",
          "time": "20210830 18:10"
        },
        {
          "id": "MDRW2100035296.1.33",
          "form": "언니는 뭐해?",
          "original_form": "언니는 뭐해?",
          "speaker_id": "1",
          "time": "20210830 18:10"
        },
        {
          "id": "MDRW2100035296.1.34",
          "form": "오늘 하루 종일 바빴네",
          "original_form": "오늘 하루 종일 바빴네",
          "speaker_id": "2",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100035296.1.35",
          "form": "난 지금 애들 밥 먹이고 있음",
          "original_form": "난 지금 애들 밥 먹이고 있음",
          "speaker_id": "2",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100035296.1.36",
          "form": "ㅋㅋ 오 언니 좋넼 빠름빠름ㅋㅋ",
          "original_form": "ㅋㅋ 오 언니 좋넼 빠름빠름ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:11"
        }
      ]
    }
