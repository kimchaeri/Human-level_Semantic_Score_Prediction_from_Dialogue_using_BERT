      "utterance": [
        {
          "id": "MDRW2100037443.1.1",
          "form": "저녁은 뭐드셨나요?",
          "original_form": "저녁은 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210831 23:56"
        },
        {
          "id": "MDRW2100037443.1.2",
          "form": "네 저는 그냥 집밥에 반찬먹었어요",
          "original_form": "네 저는 그냥 집밥에 반찬먹었어요",
          "speaker_id": "1",
          "time": "20210831 23:58"
        },
        {
          "id": "MDRW2100037443.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210831 23:58"
        },
        {
          "id": "MDRW2100037443.1.4",
          "form": "저는 찜닭 먹었어용",
          "original_form": "저는 찜닭 먹었어용",
          "speaker_id": "2",
          "time": "20210901 00:08"
        },
        {
          "id": "MDRW2100037443.1.5",
          "form": "찜닭 좋아하세요?",
          "original_form": "찜닭 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 00:08"
        },
        {
          "id": "MDRW2100037443.1.6",
          "form": "네 이대앞에서 무슨 찜닥이더라",
          "original_form": "네 이대앞에서 무슨 찜닥이더라",
          "speaker_id": "1",
          "time": "20210901 00:14"
        },
        {
          "id": "MDRW2100037443.1.7",
          "form": "봉구찜닥?",
          "original_form": "봉구찜닥?",
          "speaker_id": "1",
          "time": "20210901 00:14"
        },
        {
          "id": "MDRW2100037443.1.8",
          "form": "방구찜닭?",
          "original_form": "방구찜닭?",
          "speaker_id": "1",
          "time": "20210901 00:14"
        },
        {
          "id": "MDRW2100037443.1.9",
          "form": "무슨 찜닭집에 사람이 많을때 거기서 처음 먹어봤어요",
          "original_form": "무슨 찜닭집에 사람이 많을때 거기서 처음 먹어봤어요",
          "speaker_id": "1",
          "time": "20210901 00:15"
        },
        {
          "id": "MDRW2100037443.1.10",
          "form": "봉추찜닭....? 일까요..?",
          "original_form": "봉추찜닭....? 일까요..?",
          "speaker_id": "2",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100037443.1.11",
          "form": "와 맞아요 기억력이 좋으시네요",
          "original_form": "와 맞아요 기억력이 좋으시네요",
          "speaker_id": "1",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100037443.1.12",
          "form": "그래도 ㅂ까진 맞았으니 name2님이 알아들으실 수 있었으니",
          "original_form": "그래도 ㅂ까진 맞았으니 &name2&님이 알아들으실 수 있었으니",
          "speaker_id": "1",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100037443.1.13",
          "form": "저도 기억력이 평균은 된다고 혼자 자평해서 자존감을 지키겠어요",
          "original_form": "저도 기억력이 평균은 된다고 혼자 자평해서 자존감을 지키겠어요",
          "speaker_id": "1",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100037443.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ소통됐으니 된거죠 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ소통됐으니 된거죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100037443.1.15",
          "form": "왜냐면 제가 오늘 봉추찜닭가서 먹었었거든요 그래서 찰떡같이 알아들었네요",
          "original_form": "왜냐면 제가 오늘 봉추찜닭가서 먹었었거든요 그래서 찰떡같이 알아들었네요",
          "speaker_id": "2",
          "time": "20210901 00:27"
        },
        {
          "id": "MDRW2100037443.1.16",
          "form": "ㅋㅋㅋ다행이에요",
          "original_form": "ㅋㅋㅋ다행이에요",
          "speaker_id": "1",
          "time": "20210901 00:28"
        },
        {
          "id": "MDRW2100037443.1.17",
          "form": "안동찜닭은 좀 멀다보니 봉추에서 많이 먹었는데",
          "original_form": "안동찜닭은 좀 멀다보니 봉추에서 많이 먹었는데",
          "speaker_id": "1",
          "time": "20210901 00:28"
        },
        {
          "id": "MDRW2100037443.1.18",
          "form": "동치미가 유독 맛있었어요 찜닭이 좀 맵다보니",
          "original_form": "동치미가 유독 맛있었어요 찜닭이 좀 맵다보니",
          "speaker_id": "1",
          "time": "20210901 00:28"
        },
        {
          "id": "MDRW2100037443.1.19",
          "form": "동치미가 잘넘어가더군요,,",
          "original_form": "동치미가 잘넘어가더군요,,",
          "speaker_id": "1",
          "time": "20210901 00:28"
        },
        {
          "id": "MDRW2100037443.1.20",
          "form": "오 맞아요 ㅋㅋㅋ 동치미맛집이져..",
          "original_form": "오 맞아요 ㅋㅋㅋ 동치미맛집이져..",
          "speaker_id": "2",
          "time": "20210901 00:47"
        },
        {
          "id": "MDRW2100037443.1.21",
          "form": "저도 동치미 오늘 가서 맛있게ㅜ먹구왔네요",
          "original_form": "저도 동치미 오늘 가서 맛있게ㅜ먹구왔네요",
          "speaker_id": "2",
          "time": "20210901 00:47"
        },
        {
          "id": "MDRW2100037443.1.22",
          "form": "가서 먹다보면 동치미를 리필해서 먹다가 배가차요 ㅋㅋ",
          "original_form": "가서 먹다보면 동치미를 리필해서 먹다가 배가차요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:31"
        },
        {
          "id": "MDRW2100037443.1.23",
          "form": "ㅋㅋㅋㅋㅋ맛있죠ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ맛있죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100037443.1.24",
          "form": "당면이 넘 많아서 당면만 잔뜩 먹고왔네요",
          "original_form": "당면이 넘 많아서 당면만 잔뜩 먹고왔네요",
          "speaker_id": "2",
          "time": "20210901 11:55"
        },
        {
          "id": "MDRW2100037443.1.25",
          "form": "양념맛을 당면이 다 먹으니 당면이 맛있긴한데",
          "original_form": "양념맛을 당면이 다 먹으니 당면이 맛있긴한데",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100037443.1.26",
          "form": "건강을 생각하면 당근 감자 닭도 먹어줘야 하지만 당면이 맛있죠",
          "original_form": "건강을 생각하면 당근 감자 닭도 먹어줘야 하지만 당면이 맛있죠",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100037443.1.27",
          "form": "맞아요 사실 전 찜닭먹을때 감자 당근 안좋아해서 닭이랑 당면 양파만 먹어요...",
          "original_form": "맞아요 사실 전 찜닭먹을때 감자 당근 안좋아해서 닭이랑 당면 양파만 먹어요...",
          "speaker_id": "2",
          "time": "20210901 13:26"
        },
        {
          "id": "MDRW2100037443.1.28",
          "form": "찜닭집가면 샐러드주는 집이있는데 샐러드는 먹고 감자 당근은 먹지않습니다.. ㅎ핫",
          "original_form": "찜닭집가면 샐러드주는 집이있는데 샐러드는 먹고 감자 당근은 먹지않습니다.. ㅎ핫",
          "speaker_id": "2",
          "time": "20210901 13:27"
        },
        {
          "id": "MDRW2100037443.1.29",
          "form": "익힌 당근이 좀ㅋㅋ 맛이없어요",
          "original_form": "익힌 당근이 좀ㅋㅋ 맛이없어요",
          "speaker_id": "1",
          "time": "20210901 13:38"
        },
        {
          "id": "MDRW2100037443.1.30",
          "form": "물컹하구요",
          "original_form": "물컹하구요",
          "speaker_id": "1",
          "time": "20210901 13:38"
        }
      ]
    }
