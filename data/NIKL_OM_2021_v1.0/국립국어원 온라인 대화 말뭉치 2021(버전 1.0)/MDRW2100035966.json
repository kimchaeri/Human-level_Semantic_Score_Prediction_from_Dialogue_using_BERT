      "utterance": [
        {
          "id": "MDRW2100035966.1.1",
          "form": "식사 하셨어요?",
          "original_form": "식사 하셨어요?",
          "speaker_id": "2",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100035966.1.2",
          "form": "네 점심 먹었습니다 답이 늦었네요",
          "original_form": "네 점심 먹었습니다 답이 늦었네요",
          "speaker_id": "1",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100035966.1.3",
          "form": "뭐드셨나요",
          "original_form": "뭐드셨나요",
          "speaker_id": "1",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100035966.1.4",
          "form": "저는 치킨 먹었어요!",
          "original_form": "저는 치킨 먹었어요!",
          "speaker_id": "2",
          "time": "20210830 18:12"
        },
        {
          "id": "MDRW2100035966.1.5",
          "form": "벌써 저녁 먹을 시간이네요ㅠ 메뉴 정하셨어요?",
          "original_form": "벌써 저녁 먹을 시간이네요ㅠ 메뉴 정하셨어요?",
          "speaker_id": "2",
          "time": "20210830 18:12"
        },
        {
          "id": "MDRW2100035966.1.6",
          "form": "제가 어제 끓인 소고기뭇국 이용 ㅋㅋ",
          "original_form": "제가 어제 끓인 소고기뭇국 이용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:38"
        },
        {
          "id": "MDRW2100035966.1.7",
          "form": "치킨 ㅜㅠ",
          "original_form": "치킨 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210830 18:38"
        },
        {
          "id": "MDRW2100035966.1.8",
          "form": "아 맛있조ㅡ 맛있죠!",
          "original_form": "아 맛있조ㅡ 맛있죠!",
          "speaker_id": "2",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100035966.1.9",
          "form": "뭇국은 그 시원함이 참 좋아요!",
          "original_form": "뭇국은 그 시원함이 참 좋아요!",
          "speaker_id": "2",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100035966.1.10",
          "form": "맞아요 너무맵게 끓여서",
          "original_form": "맞아요 너무맵게 끓여서",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100035966.1.11",
          "form": "어머니가 다시 끓이셨네요 ㅋㅋ",
          "original_form": "어머니가 다시 끓이셨네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100035966.1.12",
          "form": "앗ㅋㅋ 어머니 찬스가 되었네요",
          "original_form": "앗ㅋㅋ 어머니 찬스가 되었네요",
          "speaker_id": "2",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100035966.1.13",
          "form": "맛있게 드셨어요??",
          "original_form": "맛있게 드셨어요??",
          "speaker_id": "2",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100035966.1.14",
          "form": "네 잘 먹었어용",
          "original_form": "네 잘 먹었어용",
          "speaker_id": "1",
          "time": "20210830 19:24"
        },
        {
          "id": "MDRW2100035966.1.15",
          "form": "저도 다음엔 뭇국 도전해보고 싶어지네용",
          "original_form": "저도 다음엔 뭇국 도전해보고 싶어지네용",
          "speaker_id": "2",
          "time": "20210830 19:27"
        },
        {
          "id": "MDRW2100035966.1.16",
          "form": "오늘은 기름진것만 먹었더니 깔끔한게 먹고싶어요ㅋㅋ",
          "original_form": "오늘은 기름진것만 먹었더니 깔끔한게 먹고싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:28"
        },
        {
          "id": "MDRW2100035966.1.17",
          "form": "오오 내일 도전해보세요",
          "original_form": "오오 내일 도전해보세요",
          "speaker_id": "1",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100035966.1.18",
          "form": "넵!!",
          "original_form": "넵!!",
          "speaker_id": "2",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100035966.1.19",
          "form": "레시피 찾아봐야겠어용~",
          "original_form": "레시피 찾아봐야겠어용~",
          "speaker_id": "2",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100035966.1.20",
          "form": "이제배달도 질리구 직접 해먹어야겠어용",
          "original_form": "이제배달도 질리구 직접 해먹어야겠어용",
          "speaker_id": "1",
          "time": "20210830 21:00"
        },
        {
          "id": "MDRW2100035966.1.21",
          "form": "시원해지면 요리도 곧잘 할텐뎅",
          "original_form": "시원해지면 요리도 곧잘 할텐뎅",
          "speaker_id": "1",
          "time": "20210830 21:00"
        },
        {
          "id": "MDRW2100035966.1.22",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100035966.1.23",
          "form": "더우니 불앞에 있긴 싫죠ㅠ",
          "original_form": "더우니 불앞에 있긴 싫죠ㅠ",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100035966.1.24",
          "form": "맞아용",
          "original_form": "맞아용",
          "speaker_id": "1",
          "time": "20210830 21:40"
        }
      ]
