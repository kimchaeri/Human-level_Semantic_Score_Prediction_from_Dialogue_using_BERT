      "utterance": [
        {
          "id": "MDRW2100002222.1.1",
          "form": "제사랑 헷갈리네.",
          "original_form": "제사랑 헷갈리네.",
          "speaker_id": "1",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002222.1.2",
          "form": "너 경락마사지를 받아본 적이 있니?",
          "original_form": "너 경락마사지를 받아본 적이 있니?",
          "speaker_id": "1",
          "time": "20210820 01:03"
        },
        {
          "id": "MDRW2100002222.1.3",
          "form": "아니 너는??",
          "original_form": "아니 너는??",
          "speaker_id": "2",
          "time": "20210820 11:38"
        },
        {
          "id": "MDRW2100002222.1.4",
          "form": "그거 엄청 아프다던데",
          "original_form": "그거 엄청 아프다던데",
          "speaker_id": "2",
          "time": "20210820 11:38"
        },
        {
          "id": "MDRW2100002222.1.5",
          "form": "진짜로 효과가 있을까?",
          "original_form": "진짜로 효과가 있을까?",
          "speaker_id": "2",
          "time": "20210820 11:38"
        },
        {
          "id": "MDRW2100002222.1.6",
          "form": "엄마가 몇년전에 받으러 다녔었는데",
          "original_form": "엄마가 몇년전에 받으러 다녔었는데",
          "speaker_id": "1",
          "time": "20210820 11:39"
        },
        {
          "id": "MDRW2100002222.1.7",
          "form": "효과가 있다고 하더라고",
          "original_form": "효과가 있다고 하더라고",
          "speaker_id": "1",
          "time": "20210820 11:39"
        },
        {
          "id": "MDRW2100002222.1.8",
          "form": "우리동네에도 경락 마사지 하는 곳 꽤 있던데 궁금하네",
          "original_form": "우리동네에도 경락 마사지 하는 곳 꽤 있던데 궁금하네",
          "speaker_id": "1",
          "time": "20210820 11:39"
        },
        {
          "id": "MDRW2100002222.1.9",
          "form": "얼굴이 작아지나??",
          "original_form": "얼굴이 작아지나??",
          "speaker_id": "2",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100002222.1.10",
          "form": "그렇대",
          "original_form": "그렇대",
          "speaker_id": "1",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100002222.1.11",
          "form": "그리고 붓기도 빠진대",
          "original_form": "그리고 붓기도 빠진대",
          "speaker_id": "1",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100002222.1.12",
          "form": "확실히 차이 난다는 사람이 많다더라",
          "original_form": "확실히 차이 난다는 사람이 많다더라",
          "speaker_id": "1",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100002222.1.13",
          "form": "근데 너무 아플 거 같아",
          "original_form": "근데 너무 아플 거 같아",
          "speaker_id": "1",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100002222.1.14",
          "form": "궁금하긴해",
          "original_form": "궁금하긴해",
          "speaker_id": "2",
          "time": "20210820 11:49"
        },
        {
          "id": "MDRW2100002222.1.15",
          "form": "근데 가격도 비싸지??",
          "original_form": "근데 가격도 비싸지??",
          "speaker_id": "2",
          "time": "20210820 11:49"
        },
        {
          "id": "MDRW2100002222.1.16",
          "form": "비싸더라..",
          "original_form": "비싸더라..",
          "speaker_id": "1",
          "time": "20210820 12:02"
        },
        {
          "id": "MDRW2100002222.1.17",
          "form": "돈 많이 벌면 해야겠어.",
          "original_form": "돈 많이 벌면 해야겠어.",
          "speaker_id": "1",
          "time": "20210820 12:02"
        },
        {
          "id": "MDRW2100002222.1.18",
          "form": "언제쯤 그런거에 훅훅 돈 쓸수 있을까",
          "original_form": "언제쯤 그런거에 훅훅 돈 쓸수 있을까",
          "speaker_id": "2",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002222.1.19",
          "form": "돈을 많이 벌어서 건강이나 미용에 안아끼고 투자하고싶어",
          "original_form": "돈을 많이 벌어서 건강이나 미용에 안아끼고 투자하고싶어",
          "speaker_id": "2",
          "time": "20210820 14:18"
        },
        {
          "id": "MDRW2100002222.1.20",
          "form": "나도.",
          "original_form": "나도.",
          "speaker_id": "1",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002222.1.21",
          "form": "새로나온 갤럭시워치4도 건강관리하기에 유용해 보이더라.",
          "original_form": "새로나온 갤럭시워치4도 건강관리하기에 유용해 보이더라.",
          "speaker_id": "1",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002222.1.22",
          "form": "그러게 그거 너무 갖고싶다",
          "original_form": "그러게 그거 너무 갖고싶다",
          "speaker_id": "2",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002222.1.23",
          "form": "누가 나한테 사줄사람없을까",
          "original_form": "누가 나한테 사줄사람없을까",
          "speaker_id": "2",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002222.1.24",
          "form": "나도. 인바디가 된다는 게 좋네",
          "original_form": "나도. 인바디가 된다는 게 좋네",
          "speaker_id": "1",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002222.1.25",
          "form": "없을 거 같아",
          "original_form": "없을 거 같아",
          "speaker_id": "1",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002222.1.26",
          "form": "부모님께 요청해봐 :)",
          "original_form": "부모님께 요청해봐 :)",
          "speaker_id": "1",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002222.1.27",
          "form": "ㅠㅠ요청해도 안들어주실 것 같아",
          "original_form": "ㅠㅠ요청해도 안들어주실 것 같아",
          "speaker_id": "2",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002222.1.28",
          "form": "열심히 돈 모아서 스스로 장만해야지!",
          "original_form": "열심히 돈 모아서 스스로 장만해야지!",
          "speaker_id": "2",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002222.1.29",
          "form": "멋있다.",
          "original_form": "멋있다.",
          "speaker_id": "1",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002222.1.30",
          "form": "그래 스스로의 건강을 위해선 스스로 노력해야해.",
          "original_form": "그래 스스로의 건강을 위해선 스스로 노력해야해.",
          "speaker_id": "1",
          "time": "20210820 14:40"
        }
      ]
