      "utterance": [
        {
          "id": "MDRW2100000939.1.1",
          "form": "딸내미 게임 좀 그만해라",
          "original_form": "딸내미 게임 좀 그만해라",
          "speaker_id": "2",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000939.1.2",
          "form": "나 게임 별루 안하는데!",
          "original_form": "나 게임 별루 안하는데!",
          "speaker_id": "1",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000939.1.3",
          "form": "맨날 하잖아",
          "original_form": "맨날 하잖아",
          "speaker_id": "2",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000939.1.4",
          "form": "하루에 몇 시간씩",
          "original_form": "하루에 몇 시간씩",
          "speaker_id": "2",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000939.1.5",
          "form": "안하거든요~~~",
          "original_form": "안하거든요~~~",
          "speaker_id": "1",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000939.1.6",
          "form": "그냥 몇분씩만 하는건뎅!ㅎㅎ",
          "original_form": "그냥 몇분씩만 하는건뎅!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000939.1.7",
          "form": "몇 분이 아니던데",
          "original_form": "몇 분이 아니던데",
          "speaker_id": "2",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000939.1.8",
          "form": "잘못봤다 엄마가",
          "original_form": "잘못봤다 엄마가",
          "speaker_id": "1",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000939.1.9",
          "form": "참말로",
          "original_form": "참말로",
          "speaker_id": "2",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000939.1.10",
          "form": "거짓말하지 마라",
          "original_form": "거짓말하지 마라",
          "speaker_id": "2",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000939.1.11",
          "form": "ㅋㅋㅋ 몰랏! 조금밖에 안하는거다!",
          "original_form": "ㅋㅋㅋ 몰랏! 조금밖에 안하는거다!",
          "speaker_id": "1",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000939.1.12",
          "form": "스트레스 풀어야지~",
          "original_form": "스트레스 풀어야지~",
          "speaker_id": "1",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000939.1.13",
          "form": "스트레스를 운동하면서 풀어야지",
          "original_form": "스트레스를 운동하면서 풀어야지",
          "speaker_id": "2",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000939.1.14",
          "form": "게임은 쓸모가 없다",
          "original_form": "게임은 쓸모가 없다",
          "speaker_id": "2",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000939.1.15",
          "form": "아니다 ㅋ 적절한 게임은 꼭 필요함",
          "original_form": "아니다 ㅋ 적절한 게임은 꼭 필요함",
          "speaker_id": "1",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000939.1.16",
          "form": "이상한 소리를 다 하네",
          "original_form": "이상한 소리를 다 하네",
          "speaker_id": "2",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000939.1.17",
          "form": "엄마도 같이 하자~ ㅎㅎ",
          "original_form": "엄마도 같이 하자~ ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 20:51"
