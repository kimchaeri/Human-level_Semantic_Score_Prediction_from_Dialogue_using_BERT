      "utterance": [
        {
          "id": "MDRW2100005895.1.1",
          "form": "네 name1님은요?",
          "original_form": "네 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100005895.1.2",
          "form": "네 저도요!",
          "original_form": "네 저도요!",
          "speaker_id": "1",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100005895.1.3",
          "form": "일은 적성에 맞으시나요",
          "original_form": "일은 적성에 맞으시나요",
          "speaker_id": "2",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100005895.1.4",
          "form": "네 그렇긴한데 사측이 너무 양아치에요 ㅎㅎ",
          "original_form": "네 그렇긴한데 사측이 너무 양아치에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100005895.1.5",
          "form": "아 그러면 일하실때 힘드신점도 있겠엉ㅅ",
          "original_form": "아 그러면 일하실때 힘드신점도 있겠엉ㅅ",
          "speaker_id": "2",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005895.1.6",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005895.1.7",
          "form": "네 자꾸 인원 줄이려고 ㅋㅋ",
          "original_form": "네 자꾸 인원 줄이려고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100005895.1.8",
          "form": "선넘어요자꾸",
          "original_form": "선넘어요자꾸",
          "speaker_id": "1",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100005895.1.9",
          "form": "스트레스시겠어요",
          "original_form": "스트레스시겠어요",
          "speaker_id": "2",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100005895.1.10",
          "form": "시도 나쁜 놈들 투성이고 ㅎㅎ",
          "original_form": "시도 나쁜 놈들 투성이고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100005895.1.11",
          "form": "요새 자꾸 스트레스네요 ㅎㅎ",
          "original_form": "요새 자꾸 스트레스네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100005895.1.12",
          "form": "그럼 업무스트레스는 어떻게 푸시나요",
          "original_form": "그럼 업무스트레스는 어떻게 푸시나요",
          "speaker_id": "2",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100005895.1.13",
          "form": "다른 기분 좋은거로 풀어야되는데",
          "original_form": "다른 기분 좋은거로 풀어야되는데",
          "speaker_id": "1",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100005895.1.14",
          "form": "요새 자꾸 안좋은일 투성이네요....",
          "original_form": "요새 자꾸 안좋은일 투성이네요....",
          "speaker_id": "1",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100005895.1.15",
          "form": "근무환경이 좋아지시면 좋겠네요",
          "original_form": "근무환경이 좋아지시면 좋겠네요",
          "speaker_id": "2",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100005895.1.16",
          "form": "아 기분 조금 좋아졌어요 ㅋㅋ",
          "original_form": "아 기분 조금 좋아졌어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100005895.1.17",
          "form": "퇴근하면 행복하시겠어요",
          "original_form": "퇴근하면 행복하시겠어요",
          "speaker_id": "2",
          "time": "20210907 20:02"
        }
      ]
