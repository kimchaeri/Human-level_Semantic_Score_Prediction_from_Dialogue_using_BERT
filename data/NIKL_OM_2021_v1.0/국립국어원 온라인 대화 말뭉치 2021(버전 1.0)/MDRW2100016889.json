      "utterance": [
        {
          "id": "MDRW2100016889.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016889.1.2",
          "form": "저는 강아지 키워요",
          "original_form": "저는 강아지 키워요",
          "speaker_id": "2",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100016889.1.3",
          "form": "아 강아지요?",
          "original_form": "아 강아지요?",
          "speaker_id": "1",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100016889.1.4",
          "form": "네 강아지 두마리 키워요 name1님은 강아지 키우시나요?",
          "original_form": "네 강아지 두마리 키워요 &name1&님은 강아지 키우시나요?",
          "speaker_id": "2",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100016889.1.5",
          "form": "저는 안키우는데 키우고싶어요!",
          "original_form": "저는 안키우는데 키우고싶어요!",
          "speaker_id": "1",
          "time": "20210828 12:55"
        },
        {
          "id": "MDRW2100016889.1.6",
          "form": "한번 키워보세요 엄청 귀여워요",
          "original_form": "한번 키워보세요 엄청 귀여워요",
          "speaker_id": "2",
          "time": "20210828 12:56"
        },
        {
          "id": "MDRW2100016889.1.7",
          "form": "무슨 강아지 키우세요? 종류요",
          "original_form": "무슨 강아지 키우세요? 종류요",
          "speaker_id": "1",
          "time": "20210828 12:57"
        },
        {
          "id": "MDRW2100016889.1.8",
          "form": "시고르브잡종이요",
          "original_form": "시고르브잡종이요",
          "speaker_id": "2",
          "time": "20210828 12:57"
        },
        {
          "id": "MDRW2100016889.1.9",
          "form": "똥강아지 말하시는거죠?ㅋㅋ귀엽겠어요",
          "original_form": "똥강아지 말하시는거죠?ㅋㅋ귀엽겠어요",
          "speaker_id": "1",
          "time": "20210828 12:57"
        },
        {
          "id": "MDRW2100016889.1.10",
          "form": "네 엄청 귀여워요ㅠㅠ 애교쟁이들이라",
          "original_form": "네 엄청 귀여워요ㅠㅠ 애교쟁이들이라",
          "speaker_id": "2",
          "time": "20210828 12:57"
        },
        {
          "id": "MDRW2100016889.1.11",
          "form": "믹스견들이 질병도 없고 더 건강하다고 하더라고요",
          "original_form": "믹스견들이 질병도 없고 더 건강하다고 하더라고요",
          "speaker_id": "1",
          "time": "20210828 12:58"
        },
        {
          "id": "MDRW2100016889.1.12",
          "form": "아무래도 그런거 같아요",
          "original_form": "아무래도 그런거 같아요",
          "speaker_id": "2",
          "time": "20210828 12:58"
        },
        {
          "id": "MDRW2100016889.1.13",
          "form": "강아지 키우면 비용은 많이 안들어가나요?",
          "original_form": "강아지 키우면 비용은 많이 안들어가나요?",
          "speaker_id": "1",
          "time": "20210828 12:58"
        },
        {
          "id": "MDRW2100016889.1.14",
          "form": "이것저것 많이들죠",
          "original_form": "이것저것 많이들죠",
          "speaker_id": "2",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100016889.1.15",
          "form": "제일 많이 드는게 뭐에요?",
          "original_form": "제일 많이 드는게 뭐에요?",
          "speaker_id": "1",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100016889.1.16",
          "form": "음 역시 병원비려나.. 아무래도 한번 아프면 돈이 많이 들더라구요",
          "original_form": "음 역시 병원비려나.. 아무래도 한번 아프면 돈이 많이 들더라구요",
