      "utterance": [
        {
          "id": "MDRW2100029206.1.1",
          "form": "날씨어때요?",
          "original_form": "날씨어때요?",
          "speaker_id": "1",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029206.1.2",
          "form": "여긴 비가와요",
          "original_form": "여긴 비가와요",
          "speaker_id": "1",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029206.1.3",
          "form": "하루종일",
          "original_form": "하루종일",
          "speaker_id": "1",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029206.1.4",
          "form": "여기도 비오면서 많이 시원해졌어요",
          "original_form": "여기도 비오면서 많이 시원해졌어요",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029206.1.5",
          "form": "처서가 지나니 시원해요",
          "original_form": "처서가 지나니 시원해요",
          "speaker_id": "1",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029206.1.6",
          "form": "얼마전까지 많이 더웠는데",
          "original_form": "얼마전까지 많이 더웠는데",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029206.1.7",
          "form": "밤엔 살짝 추워요",
          "original_form": "밤엔 살짝 추워요",
          "speaker_id": "1",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029206.1.8",
          "form": "해가 안떠서 그런지 춥더라구요",
          "original_form": "해가 안떠서 그런지 춥더라구요",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029206.1.9",
          "form": "9월이 되니 가을인가봐요",
          "original_form": "9월이 되니 가을인가봐요",
          "speaker_id": "1",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029206.1.10",
          "form": "맞아요 얼마전까진 밤에도 공기텁텁했는데",
          "original_form": "맞아요 얼마전까진 밤에도 공기텁텁했는데",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029206.1.11",
          "form": "일기예보 보니 종일 비온다해요",
          "original_form": "일기예보 보니 종일 비온다해요",
          "speaker_id": "1",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029206.1.12",
          "form": "여름을 좋아하나요?",
          "original_form": "여름을 좋아하나요?",
          "speaker_id": "1",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029206.1.13",
          "form": "가을엔 비 한 번 올때마다 점점 추워진다는 얘기가 맞는 것 같아요",
          "original_form": "가을엔 비 한 번 올때마다 점점 추워진다는 얘기가 맞는 것 같아요",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029206.1.14",
          "form": "저는 가을이 좋아요",
          "original_form": "저는 가을이 좋아요",
          "speaker_id": "1",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029206.1.15",
          "form": "전 여름 별로 안 좋아하긴하는데",
          "original_form": "전 여름 별로 안 좋아하긴하는데",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029206.1.16",
          "form": "가을비가 계속오나봐요",
          "original_form": "가을비가 계속오나봐요",
          "speaker_id": "1",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100029206.1.17",
          "form": "이렇게 빨리 추워지니까 벌써 좀 그리워요",
          "original_form": "이렇게 빨리 추워지니까 벌써 좀 그리워요",
          "speaker_id": "2",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100029206.1.18",
          "form": "어느계절을 좋아해요?",
          "original_form": "어느계절을 좋아해요?",
          "speaker_id": "1",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100029206.1.19",
          "form": "가을옷도 다 못 꺼내놨는데",
          "original_form": "가을옷도 다 못 꺼내놨는데",
          "speaker_id": "2",
