        {
          "id": "MDRW2100029992.1.1",
          "form": "네 당근마켓 해요",
          "original_form": "네 당근마켓 해요",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100029992.1.2",
          "form": "저도 당근 종종 합니다",
          "original_form": "저도 당근 종종 합니다",
          "speaker_id": "1",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100029992.1.3",
          "form": "뭐 파셨나요",
          "original_form": "뭐 파셨나요",
          "speaker_id": "2",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100029992.1.4",
          "form": "공기청정기요",
          "original_form": "공기청정기요",
          "speaker_id": "1",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100029992.1.5",
          "form": "사신 것도 있나요?",
          "original_form": "사신 것도 있나요?",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100029992.1.6",
          "form": "팔기만 해요",
          "original_form": "팔기만 해요",
          "speaker_id": "1",
          "time": "20210903 18:55"
        },
        {
          "id": "MDRW2100029992.1.7",
          "form": "저는 사기도 했고 팔기도 했네요",
          "original_form": "저는 사기도 했고 팔기도 했네요",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100029992.1.8",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100029992.1.9",
          "form": "옷이나 화장품이요",
          "original_form": "옷이나 화장품이요",
          "speaker_id": "2",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100029992.1.10",
          "form": "잘 팔리나여",
          "original_form": "잘 팔리나여",
          "speaker_id": "1",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100029992.1.11",
          "form": "꽤? 잘 팔려요",
          "original_form": "꽤? 잘 팔려요",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100029992.1.12",
          "form": "싸게 내놨다보다요",
          "original_form": "싸게 내놨다보다요",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100029992.1.13",
          "form": "맞아요 ㅋㅋ 그리고 10~20대 여성 물건이",
          "original_form": "맞아요 ㅋㅋ 그리고 10~20대 여성 물건이",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100029992.1.14",
