        {
          "id": "MDRW2100041316.1.1",
          "form": "name2님은 낼 출근하세요?",
          "original_form": "&name2&님은 낼 출근하세요?",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100041316.1.2",
          "form": "네 ㅠㅠ 월요일 너무 싫어요 ㅠㅠ",
          "original_form": "네 ㅠㅠ 월요일 너무 싫어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100041316.1.3",
          "form": "name1님도 출근하시나요??",
          "original_form": "&name1&님도 출근하시나요??",
          "speaker_id": "2",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100041316.1.4",
          "form": "네 맞아유",
          "original_form": "네 맞아유",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100041316.1.5",
          "form": "흑 쉬는날 없는 일주일 ㅜ",
          "original_form": "흑 쉬는날 없는 일주일 ㅜ",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100041316.1.6",
          "form": "추석까지 존버입니닷",
          "original_form": "추석까지 존버입니닷",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100041316.1.7",
          "form": "저두요 ㅠㅠㅠㅠㅠ 추석 얼마 안남았는데 그래도 좀 빨리왔음 좋겠어요 ㅠㅠ",
          "original_form": "저두요 ㅠㅠㅠㅠㅠ 추석 얼마 안남았는데 그래도 좀 빨리왔음 좋겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041316.1.8",
          "form": "아 그리고 10월도 대체공휴일때문에 기다려져요 ㅎㅎ",
          "original_form": "아 그리고 10월도 대체공휴일때문에 기다려져요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041316.1.9",
          "form": "맞아요 쉬어가는게 좋은거같아요",
          "original_form": "맞아요 쉬어가는게 좋은거같아요",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100041316.1.10",
          "form": "능률을 위해서두",
          "original_form": "능률을 위해서두",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100041316.1.11",
          "form": "맞아요 확실히 쉬고나면 체력적이나 정신적으로도 좀 가벼운 느낌이 들고 좋더라구요 ㅎㅎ",
          "original_form": "맞아요 확실히 쉬고나면 체력적이나 정신적으로도 좀 가벼운 느낌이 들고 좋더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100041316.1.12",
          "form": "종종 연차는 사용하시나요??",
          "original_form": "종종 연차는 사용하시나요??",
          "speaker_id": "2",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100041316.1.13",
          "form": "연차가 없는 직업이에요...ㅋㅋㅋ",
          "original_form": "연차가 없는 직업이에요...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:15"
        },
        {
          "id": "MDRW2100041316.1.14",
          "form": "졸려요..ㅎㅎ",
          "original_form": "졸려요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 23:15"
        },
        {
          "id": "MDRW2100041316.1.15",
          "form": "출근 몇시에 하세요 name2님?",
          "original_form": "출근 몇시에 하세요 &name2&님?",
          "speaker_id": "1",
          "time": "20210905 23:15"
        },
        {
          "id": "MDRW2100041316.1.16",
          "form": "힘드시겠어요 ㅠㅠㅠ",
          "original_form": "힘드시겠어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 07:49"
        },
        {
          "id": "MDRW2100041316.1.17",
          "form": "9시까지 출근인데 보통 8시 반에 도착해요~",
          "original_form": "9시까지 출근인데 보통 8시 반에 도착해요~",
          "speaker_id": "2",
          "time": "20210906 07:50"
        },
        {
          "id": "MDRW2100041316.1.18",
          "form": "ㅎㅎ곧 퇴근하시죠",
          "original_form": "ㅎㅎ곧 퇴근하시죠",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041316.1.19",
          "form": "엇 맞아요 지금 퇴근하고 집 가는 길이에요 ㅠㅠ",
          "original_form": "엇 맞아요 지금 퇴근하고 집 가는 길이에요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100041316.1.20",
          "form": "출퇴근 왕복 얼마나 걸리시나요?",
          "original_form": "출퇴근 왕복 얼마나 걸리시나요?",
          "speaker_id": "2",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100041316.1.21",
          "form": "저는 한시간정도 걸려요",
          "original_form": "저는 한시간정도 걸려요",
          "speaker_id": "1",
          "time": "20210906 23:37"
        },
        {
          "id": "MDRW2100041316.1.22",
          "form": "아 왕복은 100분정도요 ㅎㅎ",
          "original_form": "아 왕복은 100분정도요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 23:37"
        },
        {
          "id": "MDRW2100041316.1.23",
          "form": "name1님도 오래 걸리시네요 ㅠㅠ",
          "original_form": "&name1&님도 오래 걸리시네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 07:52"
        },
        {
          "id": "MDRW2100041316.1.24",
          "form": "저도 거의 2시간 정도 걸려요ㅠㅠ 좀만 더 가까웠음 하는 마음이 있는데 이젠 많이 익숙해져서 가는길에 자거나 휴대폰 실컷만지면서 가요",
          "original_form": "저도 거의 2시간 정도 걸려요ㅠㅠ 좀만 더 가까웠음 하는 마음이 있는데 이젠 많이 익숙해져서 가는길에 자거나 휴대폰 실컷만지면서 가요",
          "speaker_id": "2",
          "time": "20210907 07:53"
        },
        {
          "id": "MDRW2100041316.1.25",
          "form": "ㅎㅎ 그니깐요 직장 코앞에 자취하고싶어요",
          "original_form": "ㅎㅎ 그니깐요 직장 코앞에 자취하고싶어요",
          "speaker_id": "1",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100041316.1.26",
          "form": "맞아요 그러면 진짜 엄청 편할 것 같은데 아쉬워요",
          "original_form": "맞아요 그러면 진짜 엄청 편할 것 같은데 아쉬워요",
          "speaker_id": "2",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100041316.1.27",
          "form": "집세는 안들어서 좋은데 진짜 시간이 너무 아까워요",
          "original_form": "집세는 안들어서 좋은데 진짜 시간이 너무 아까워요",
