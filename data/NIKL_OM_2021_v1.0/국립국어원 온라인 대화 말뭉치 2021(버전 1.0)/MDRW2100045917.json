      "utterance": [
        {
          "id": "MDRW2100045917.1.1",
          "form": "아침부터 저녁메뉴가 고민이네요",
          "original_form": "아침부터 저녁메뉴가 고민이네요",
          "speaker_id": "1",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100045917.1.2",
          "form": "먹는 메뉴 정하기가 가장 힘들죠",
          "original_form": "먹는 메뉴 정하기가 가장 힘들죠",
          "speaker_id": "2",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100045917.1.3",
          "form": "맞아요 먹으면서도 다음 끼니걱정",
          "original_form": "맞아요 먹으면서도 다음 끼니걱정",
          "speaker_id": "1",
          "time": "20210908 09:01"
        },
        {
          "id": "MDRW2100045917.1.4",
          "form": "저녁메뉴 고르셨요",
          "original_form": "저녁메뉴 고르셨요",
          "speaker_id": "2",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100045917.1.5",
          "form": "삼겹이나 구울까 하고잇어여",
          "original_form": "삼겹이나 구울까 하고잇어여",
          "speaker_id": "1",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100045917.1.6",
          "form": "고기가 제일 간단하면서 맛잇어서",
          "original_form": "고기가 제일 간단하면서 맛잇어서",
          "speaker_id": "1",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100045917.1.7",
          "form": "삼겹살 구이 넘 맛난 저녁 메뉴네요",
          "original_form": "삼겹살 구이 넘 맛난 저녁 메뉴네요",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045917.1.8",
          "form": "먹고 싶어지네요",
          "original_form": "먹고 싶어지네요",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045917.1.9",
          "form": "고기가 간단하니까 평소 자주먹게되요",
          "original_form": "고기가 간단하니까 평소 자주먹게되요",
          "speaker_id": "1",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100045917.1.10",
          "form": "저도 구이 볶음 수육등 자주 해먹어요",
          "original_form": "저도 구이 볶음 수육등 자주 해먹어요",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045917.1.11",
          "form": "수육도 맛있죠 \r\n안먹은지 오래됐네요",
          "original_form": "수육도 맛있죠 \r\n안먹은지 오래됐네요",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045917.1.12",
          "form": "가끔 먹으면 맛있죠",
          "original_form": "가끔 먹으면 맛있죠",
          "speaker_id": "2",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045917.1.13",
          "form": "씻은 묵은김치 랑 먹으면 맛나죠",
          "original_form": "씻은 묵은김치 랑 먹으면 맛나죠",
          "speaker_id": "2",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045917.1.14",
          "form": "오 오늘 삼겹살에 묵은지 씻어서 먹어야겟네요",
          "original_form": "오 오늘 삼겹살에 묵은지 씻어서 먹어야겟네요",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045917.1.15",
          "form": "맛잇을거겉아요",
          "original_form": "맛잇을거겉아요",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045917.1.16",
          "form": "간장깻잎도 곁들이면 맛있어요",
          "original_form": "간장깻잎도 곁들이면 맛있어요",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045917.1.17",
          "form": "좋은 팁이에요\r\n전 쌈만 먹엇엇는데",
          "original_form": "좋은 팁이에요\r\n전 쌈만 먹엇엇는데",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045917.1.18",
          "form": "저도 저녁메뉴로 수육 생각해 봐야 겠어요",
          "original_form": "저도 저녁메뉴로 수육 생각해 봐야 겠어요",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045917.1.19",
          "form": "네 역시 고기죠ㅋㅋ",
          "original_form": "네 역시 고기죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:27"
        }
      ]
