      "utterance": [
        {
          "id": "MDRW2100031420.1.1",
          "form": "안타깝게도",
          "original_form": "안타깝게도",
          "speaker_id": "2",
          "time": "20210904 21:30"
        },
        {
          "id": "MDRW2100031420.1.2",
          "form": "따로 하는 운동은 없네요",
          "original_form": "따로 하는 운동은 없네요",
          "speaker_id": "2",
          "time": "20210904 21:30"
        },
        {
          "id": "MDRW2100031420.1.3",
          "form": "그렇군요.. 예전에도 했던 운동도 없는건가요?",
          "original_form": "그렇군요.. 예전에도 했던 운동도 없는건가요?",
          "speaker_id": "1",
          "time": "20210904 21:35"
        },
        {
          "id": "MDRW2100031420.1.4",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "2",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031420.1.5",
          "form": "성인되고 나서는 딱히",
          "original_form": "성인되고 나서는 딱히",
          "speaker_id": "2",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031420.1.6",
          "form": "운동을 안했네요",
          "original_form": "운동을 안했네요",
          "speaker_id": "2",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031420.1.7",
          "form": "그랬군요..아직 괜찮나봅니다",
          "original_form": "그랬군요..아직 괜찮나봅니다",
          "speaker_id": "1",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100031420.1.8",
          "form": "저는 운동을 좋아해서 이것저것 해봤어요",
          "original_form": "저는 운동을 좋아해서 이것저것 해봤어요",
          "speaker_id": "1",
          "time": "20210905 09:13"
        },
        {
          "id": "MDRW2100031420.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031420.1.10",
          "form": "솔직히 운동 해야지 싶긴 한데",
          "original_form": "솔직히 운동 해야지 싶긴 한데",
          "speaker_id": "2",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031420.1.11",
          "form": "자꾸 미루네요",
          "original_form": "자꾸 미루네요",
          "speaker_id": "2",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031420.1.12",
          "form": "어떤 거 해보셨나요",
          "original_form": "어떤 거 해보셨나요",
          "speaker_id": "2",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031420.1.13",
          "form": "볼링 배워서 몇년 했었꾸요",
          "original_form": "볼링 배워서 몇년 했었꾸요",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100031420.1.14",
          "form": "수영은 거의 20년 정도 했구요",
          "original_form": "수영은 거의 20년 정도 했구요",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100031420.1.15",
          "form": "헬스도 다녔었어요",
          "original_form": "헬스도 다녔었어요",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100031420.1.16",
          "form": "오오...",
          "original_form": "오오...",
          "speaker_id": "2",
          "time": "20210905 15:08"
        },
        {
          "id": "MDRW2100031420.1.17",
          "form": "진짜 여러가지 하셨었네요",
          "original_form": "진짜 여러가지 하셨었네요",
          "speaker_id": "2",
          "time": "20210905 15:09"
        },
        {
          "id": "MDRW2100031420.1.18",
          "form": "네.. 헬스장에서 필라테스도 했었고",
          "original_form": "네.. 헬스장에서 필라테스도 했었고",
          "speaker_id": "1",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100031420.1.19",
          "form": "수영장에서 아쿠아로빅도 했었구요",
          "original_form": "수영장에서 아쿠아로빅도 했었구요",
          "speaker_id": "1",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100031420.1.20",
          "form": "운동을 좋아하시는가 봐요",
          "original_form": "운동을 좋아하시는가 봐요",
          "speaker_id": "2",
          "time": "20210906 08:41"
        },
        {
          "id": "MDRW2100031420.1.21",
          "form": "몸 쓰는 걸 좋아해요",
          "original_form": "몸 쓰는 걸 좋아해요",
          "speaker_id": "1",
          "time": "20210906 08:44"
        },
        {
          "id": "MDRW2100031420.1.22",
          "form": "외향적인 성격이라서요",
          "original_form": "외향적인 성격이라서요",
          "speaker_id": "1",
          "time": "20210906 08:44"
        },
        {
          "id": "MDRW2100031420.1.23",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210906 08:45"
        },
        {
          "id": "MDRW2100031420.1.24",
          "form": "저는 이미",
          "original_form": "저는 이미",
          "speaker_id": "2",
          "time": "20210906 08:46"
        },
        {
          "id": "MDRW2100031420.1.25",
          "form": "짐작하고 계실지도 모르겠지만",
          "original_form": "짐작하고 계실지도 모르겠지만",
          "speaker_id": "2",
          "time": "20210906 08:46"
        },
        {
          "id": "MDRW2100031420.1.26",
          "form": "인도어파라서...",
          "original_form": "인도어파라서...",
          "speaker_id": "2",
          "time": "20210906 08:46"
        },
        {
          "id": "MDRW2100031420.1.27",
          "form": "그러게요.. 저희 남편도 집을 사랑하더라구요",
          "original_form": "그러게요.. 저희 남편도 집을 사랑하더라구요",
          "speaker_id": "1",
          "time": "20210906 08:48"
        },
        {
          "id": "MDRW2100031420.1.28",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 08:48"
        },
        {
          "id": "MDRW2100031420.1.29",
          "form": "제가 헬스장 임의로 끊어놓으면 돈 아까우니까 다니긴 하는데",
          "original_form": "제가 헬스장 임의로 끊어놓으면 돈 아까우니까 다니긴 하는데",
          "speaker_id": "1",
          "time": "20210906 08:49"
        },
        {
          "id": "MDRW2100031420.1.30",
          "form": "작년부턴 그 방법도 쓰지 못했죠.. 코로나 떄문에",
