      "utterance": [
        {
          "id": "MDRW2100043848.1.1",
          "form": "저도 술마시면 심장이 뛰드라구요 ㅠㅠ",
          "original_form": "저도 술마시면 심장이 뛰드라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043848.1.2",
          "form": "name1님도 완전 술이 안맞으시나봐요 ㅠㅠ",
          "original_form": "&name1&님도 완전 술이 안맞으시나봐요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043848.1.3",
          "form": "건강응 위해 안주만 먹읍시댜",
          "original_form": "건강응 위해 안주만 먹읍시댜",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043848.1.4",
          "form": "네네 얼굴 빨개지는 타입은 먹으면 안좋다네요 ㅠ",
          "original_form": "네네 얼굴 빨개지는 타입은 먹으면 안좋다네요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043848.1.5",
          "form": "해독능력이 없데요 ㅋㅋㅋ",
          "original_form": "해독능력이 없데요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043848.1.6",
          "form": "헐 이런.. ㅠㅠ",
          "original_form": "헐 이런.. ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043848.1.7",
          "form": "그럼 양꼬치 좋아하세요?",
          "original_form": "그럼 양꼬치 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043848.1.8",
          "form": "양꼬치 안먹어봣어요 ㅠㅠ",
          "original_form": "양꼬치 안먹어봣어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043848.1.9",
          "form": "향이 독특하지않아요?",
          "original_form": "향이 독특하지않아요?",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043848.1.10",
          "form": "맞아요! 약간 그래요!",
          "original_form": "맞아요! 약간 그래요!",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043848.1.11",
          "form": "향 잇는 음식 암좋아하세요?",
          "original_form": "향 잇는 음식 암좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043848.1.12",
          "form": "네네 ㅠㅠ 마라탕도 못먹어용 ㅠ",
          "original_form": "네네 ㅠㅠ 마라탕도 못먹어용 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043848.1.13",
          "form": "마라탕 드세요?",
          "original_form": "마라탕 드세요?",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043848.1.14",
          "form": "헐..! 저 완전 좋아해요!",
          "original_form": "헐..! 저 완전 좋아해요!",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043848.1.15",
          "form": "그 사람들이 당면같은걸 좋아하나봐요",
          "original_form": "그 사람들이 당면같은걸 좋아하나봐요",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043848.1.16",
          "form": "아 여러종류의 당면이 들어가긴 하는거 같아요",
          "original_form": "아 여러종류의 당면이 들어가긴 하는거 같아요",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043848.1.17",
          "form": "떡볶이는 좋아하셍6?",
          "original_form": "떡볶이는 좋아하셍6?",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043848.1.18",
          "form": "떡볶이 최근에 로제로 먹어봣어요 ㅋㅋ",
          "original_form": "떡볶이 최근에 로제로 먹어봣어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100043848.1.19",
          "form": "드셔 보셧어요?",
          "original_form": "드셔 보셧어요?",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100043848.1.20",
          "form": "오 네!!!",
          "original_form": "오 네!!!",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100043848.1.21",
          "form": "완전 맛있었어요!",
          "original_form": "완전 맛있었어요!",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100043848.1.22",
          "form": "저는 떡볶이는 그렇개 좋아하는 편이 아닌데!",
          "original_form": "저는 떡볶이는 그렇개 좋아하는 편이 아닌데!",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100043848.1.23",
          "form": "오오 로제파스타도 맛잇드라구요 ㅠ",
          "original_form": "오오 로제파스타도 맛잇드라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100043848.1.24",
          "form": "아ㅜ저 로제파스타 너무 좋아요",
          "original_form": "아ㅜ저 로제파스타 너무 좋아요",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100043848.1.25",
          "form": "파스타 중에 어던거ㅠ가장 좋아하세요?",
          "original_form": "파스타 중에 어던거ㅠ가장 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100043848.1.26",
          "form": "오일 토마토 크림 !",
          "original_form": "오일 토마토 크림 !",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100043848.1.27",
          "form": "저는 무조건 토마토스파게티요 !! ㅋㅋ",
          "original_form": "저는 무조건 토마토스파게티요 !! ㅋㅋ",
