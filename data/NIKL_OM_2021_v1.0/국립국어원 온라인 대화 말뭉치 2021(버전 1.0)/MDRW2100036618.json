      "utterance": [
        {
          "id": "MDRW2100036618.1.1",
          "form": "뭘 사려고 하다가",
          "original_form": "뭘 사려고 하다가",
          "speaker_id": "1",
          "time": "20210831 02:25"
        },
        {
          "id": "MDRW2100036618.1.2",
          "form": "시간 좀 지나면 까먹더라구요",
          "original_form": "시간 좀 지나면 까먹더라구요",
          "speaker_id": "1",
          "time": "20210831 02:25"
        },
        {
          "id": "MDRW2100036618.1.3",
          "form": "그럼 안사도 되는 물건인건가 하는 생각을 해요",
          "original_form": "그럼 안사도 되는 물건인건가 하는 생각을 해요",
          "speaker_id": "1",
          "time": "20210831 02:26"
        },
        {
          "id": "MDRW2100036618.1.4",
          "form": "좋은거같아요.... 저는 좀 물욕이 있는거같아요 수집욕?? ㅋㅋㅋㅋㅋ 흑... 고쳐야하는데말이져..",
          "original_form": "좋은거같아요.... 저는 좀 물욕이 있는거같아요 수집욕?? ㅋㅋㅋㅋㅋ 흑... 고쳐야하는데말이져..",
          "speaker_id": "2",
          "time": "20210831 02:50"
        },
        {
          "id": "MDRW2100036618.1.5",
          "form": "어떤거 수집하세요?",
          "original_form": "어떤거 수집하세요?",
          "speaker_id": "1",
          "time": "20210831 11:39"
        },
        {
          "id": "MDRW2100036618.1.6",
          "form": "굿즈같은거도 수집하세요?",
          "original_form": "굿즈같은거도 수집하세요?",
          "speaker_id": "1",
          "time": "20210831 11:39"
        },
        {
          "id": "MDRW2100036618.1.7",
          "form": "음 ㅋㅋㅋㅋ옷도 맘에들면 깔별로 사고요 ㅋㅋㅋㅋ",
          "original_form": "음 ㅋㅋㅋㅋ옷도 맘에들면 깔별로 사고요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100036618.1.8",
          "form": "스티커 같은거도 꽂히면 엄청 사대고",
          "original_form": "스티커 같은거도 꽂히면 엄청 사대고",
          "speaker_id": "2",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100036618.1.9",
          "form": "아아 네네",
          "original_form": "아아 네네",
          "speaker_id": "1",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100036618.1.10",
          "form": "가방도 그렇게 사던데요",
          "original_form": "가방도 그렇게 사던데요",
          "speaker_id": "1",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100036618.1.11",
          "form": "컵도 모을때도 있었고 ㅌㅋㅋㅋㅋ",
          "original_form": "컵도 모을때도 있었고 ㅌㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100036618.1.12",
          "form": "티도 ㅋㅋㅋㅋ다먹지도 못할거 엄청 사댈때있었고요 ㅋㅋㅋㅋ",
          "original_form": "티도 ㅋㅋㅋㅋ다먹지도 못할거 엄청 사댈때있었고요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100036618.1.13",
          "form": "아아 네 그거 포장지 알록달록하게",
          "original_form": "아아 네 그거 포장지 알록달록하게",
          "speaker_id": "1",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100036618.1.14",
          "form": "사고싶게생겼어요",
          "original_form": "사고싶게생겼어요",
          "speaker_id": "1",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100036618.1.15",
          "form": "네 저 그 티꽁도 쓸데없는데 모아두고  ㅋㅋㅋㅋㅋ",
          "original_form": "네 저 그 티꽁도 쓸데없는데 모아두고  ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036618.1.16",
          "form": "티꽁이뭐에요??/",
          "original_form": "티꽁이뭐에요??/",
          "speaker_id": "1",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100036618.1.17",
          "form": "그 티백에 손잡이 부분 종이쪼가리요 ㅋㅋㅋㅋ 티꽁다리 ㅋㅋㅋㅋㅋ",
          "original_form": "그 티백에 손잡이 부분 종이쪼가리요 ㅋㅋㅋㅋ 티꽁다리 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:18"
        },
        {
          "id": "MDRW2100036618.1.18",
          "form": "정말...",
          "original_form": "정말...",
          "speaker_id": "2",
          "time": "20210831 13:18"
        },
        {
          "id": "MDRW2100036618.1.19",
          "form": "쓸데없죠?? ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "쓸데없죠?? ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:19"
        },
        {
          "id": "MDRW2100036618.1.20",
          "form": "아뇨 뭐 그렇게까진 ㅋㅋㅋ",
          "original_form": "아뇨 뭐 그렇게까진 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:19"
        },
        {
          "id": "MDRW2100036618.1.21",
          "form": "쓸데없다하니 맘아파요",
          "original_form": "쓸데없다하니 맘아파요",
          "speaker_id": "1",
          "time": "20210831 13:20"
        },
        {
          "id": "MDRW2100036618.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ 근데 정말 아무데도 쓸데가 없긴해요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 근데 정말 아무데도 쓸데가 없긴해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100036618.1.23",
          "form": "근데도 버리지않는 나...",
          "original_form": "근데도 버리지않는 나...",
          "speaker_id": "2",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100036618.1.24",
          "form": "그걸로 블로그나 유튜브하시면 멋질거같은데요!",
          "original_form": "그걸로 블로그나 유튜브하시면 멋질거같은데요!",
          "speaker_id": "1",
          "time": "20210831 13:26"
        }
      ]
