      "utterance": [
        {
          "id": "MDRW2100030795.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030795.1.2",
          "form": "군대 잘 아세요?",
          "original_form": "군대 잘 아세요?",
          "speaker_id": "2",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100030795.1.3",
          "form": "앗..아뇨",
          "original_form": "앗..아뇨",
          "speaker_id": "1",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100030795.1.4",
          "form": "ㅎㅎㅎ군생활쪽아시는거있나요?",
          "original_form": "ㅎㅎㅎ군생활쪽아시는거있나요?",
          "speaker_id": "1",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100030795.1.5",
          "form": "아녀ㅎ",
          "original_form": "아녀ㅎ",
          "speaker_id": "2",
          "time": "20210904 22:21"
        },
        {
          "id": "MDRW2100030795.1.6",
          "form": "강철부대보고",
          "original_form": "강철부대보고",
          "speaker_id": "2",
          "time": "20210904 22:21"
        },
        {
          "id": "MDRW2100030795.1.7",
          "form": "특전사 첨 알았어요",
          "original_form": "특전사 첨 알았어요",
          "speaker_id": "2",
          "time": "20210904 22:21"
        },
        {
          "id": "MDRW2100030795.1.8",
          "form": "ㅎㅎㅎ저도...",
          "original_form": "ㅎㅎㅎ저도...",
          "speaker_id": "1",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100030795.1.9",
          "form": "오빠가 의경생활해서",
          "original_form": "오빠가 의경생활해서",
          "speaker_id": "1",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100030795.1.10",
          "form": "그것만쪼끔알아요",
          "original_form": "그것만쪼끔알아요",
          "speaker_id": "1",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100030795.1.11",
          "form": "외박이많다는정도?",
          "original_form": "외박이많다는정도?",
          "speaker_id": "1",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100030795.1.12",
          "form": "의경도 많이 힘들다던데",
          "original_form": "의경도 많이 힘들다던데",
          "speaker_id": "2",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100030795.1.13",
          "form": "맞나요?",
          "original_form": "맞나요?",
          "speaker_id": "2",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100030795.1.14",
          "form": "ㅜㅜ힘들어보이더라고요",
          "original_form": "ㅜㅜ힘들어보이더라고요",
          "speaker_id": "1",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100030795.1.15",
          "form": "주변에군대가신분있나요?",
          "original_form": "주변에군대가신분있나요?",
          "speaker_id": "1",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100030795.1.16",
          "form": "현재는 없어요ㅎ",
          "original_form": "현재는 없어요ㅎ",
          "speaker_id": "2",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100030795.1.17",
          "form": "ㅎ그죠 쉽지않은주제에요",
          "original_form": "ㅎ그죠 쉽지않은주제에요",
          "speaker_id": "1",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100030795.1.18",
          "form": "해병대힘들어보이더라고요ㅜ",
          "original_form": "해병대힘들어보이더라고요ㅜ",
          "speaker_id": "1",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100030795.1.19",
          "form": "근데 아들이 좋아해요ㅎ",
          "original_form": "근데 아들이 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100030795.1.20",
          "form": "앗 아드님이 좋아하시는군요",
          "original_form": "앗 아드님이 좋아하시는군요",
          "speaker_id": "1",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100030795.1.21",
          "form": "아드님이군대에가셨나요?",
          "original_form": "아드님이군대에가셨나요?",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100030795.1.22",
          "form": "아직 초등생요ㅎ",
          "original_form": "아직 초등생요ㅎ",
          "speaker_id": "2",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100030795.1.23",
          "form": "ㅎㅎㅎ귀엽겠어요",
          "original_form": "ㅎㅎㅎ귀엽겠어요",
          "speaker_id": "1",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100030795.1.24",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "2",
          "time": "20210904 23:14"
        }
      ]
