        },
        {
          "id": "MDRW2100022576.1.2",
          "form": "이따가 방청소 할려구여 ㅎㅎ",
          "original_form": "이따가 방청소 할려구여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022576.1.3",
          "form": "화장실이대박이죠 락스",
          "original_form": "화장실이대박이죠 락스",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022576.1.4",
          "form": "오늘도 청소하시나요?",
          "original_form": "오늘도 청소하시나요?",
          "speaker_id": "2",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022576.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022576.1.6",
          "form": "아이구 고생하셔유 ㅠㅠ",
          "original_form": "아이구 고생하셔유 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:01"
        },
        {
          "id": "MDRW2100022576.1.7",
          "form": "머리도아프고",
          "original_form": "머리도아프고",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022576.1.8",
          "form": "그쵸 ㅠㅠ",
          "original_form": "그쵸 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100022576.1.9",
          "form": "넘  안 좋아여",
          "original_form": "넘  안 좋아여",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022576.1.10",
          "form": "냄새가 으으",
          "original_form": "냄새가 으으",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022576.1.11",
          "form": "힘들겠어요",
          "original_form": "힘들겠어요",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022576.1.12",
          "form": "네 쉬고싶어요",
          "original_form": "네 쉬고싶어요",
          "speaker_id": "1",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022576.1.13",
          "form": "한 1주일에 1번씩정도만 하는게 좋을거같은데 ㅠㅠ",
          "original_form": "한 1주일에 1번씩정도만 하는게 좋을거같은데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:06"
        },
        {
          "id": "MDRW2100022576.1.14",
          "form": "그정도요",
          "original_form": "그정도요",
          "speaker_id": "1",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022576.1.15",
          "form": "맞아요 이정도가 딱 맞는데",
          "original_form": "맞아요 이정도가 딱 맞는데",
          "speaker_id": "2",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022576.1.16",
          "form": "곰팡이걱정은요",
          "original_form": "곰팡이걱정은요",
          "speaker_id": "1",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100022576.1.17",
          "form": "관리만 잘하면 되긴하니까요",
          "original_form": "관리만 잘하면 되긴하니까요",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022576.1.18",
          "form": "환기가 중요해서",
          "original_form": "환기가 중요해서",
