      "utterance": [
        {
          "id": "MDRW2100042823.1.1",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042823.1.2",
          "form": "그래도 우리는 잘 넘어가서",
          "original_form": "그래도 우리는 잘 넘어가서",
          "speaker_id": "2",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042823.1.3",
          "form": "다 다행이다",
          "original_form": "다 다행이다",
          "speaker_id": "2",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042823.1.4",
          "form": "나 어제 성유리 엠플 세틱 샀거든",
          "original_form": "나 어제 성유리 엠플 세틱 샀거든",
          "speaker_id": "2",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042823.1.5",
          "form": "스틱",
          "original_form": "스틱",
          "speaker_id": "2",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042823.1.6",
          "form": "응 그러니깐은",
          "original_form": "응 그러니깐은",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042823.1.7",
          "form": "응으",
          "original_form": "응으",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042823.1.8",
          "form": "어때??",
          "original_form": "어때??",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042823.1.9",
          "form": "촉촉한 거 같다ㅋㅋㅋㅋ",
          "original_form": "촉촉한 거 같다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100042823.1.10",
          "form": "생각보다 끈적이지도 않고",
          "original_form": "생각보다 끈적이지도 않고",
          "speaker_id": "2",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100042823.1.11",
          "form": "오오 ㅋㅋ 좋다이 ㅋㅋ",
          "original_form": "오오 ㅋㅋ 좋다이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100042823.1.12",
          "form": "며칠 전에",
          "original_form": "며칠 전에",
          "speaker_id": "2",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100042823.1.13",
          "form": "성유리 엠플스틱 출시 기념으로",
          "original_form": "성유리 엠플스틱 출시 기념으로",
          "speaker_id": "2",
          "time": "20210907 09:43"
        },
        {
          "id": "MDRW2100042823.1.14",
          "form": "한 개를 사면",
          "original_form": "한 개를 사면",
          "speaker_id": "2",
          "time": "20210907 09:43"
        },
        {
          "id": "MDRW2100042823.1.15",
          "form": "응응 ㅋㅋ",
          "original_form": "응응 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:43"
        },
        {
          "id": "MDRW2100042823.1.16",
          "form": "마스크 팩 열 개를 주는 거야",
          "original_form": "마스크 팩 열 개를 주는 거야",
          "speaker_id": "2",
          "time": "20210907 09:43"
        },
        {
          "id": "MDRW2100042823.1.17",
          "form": "그래서",
          "original_form": "그래서",
          "speaker_id": "2",
          "time": "20210907 09:43"
        },
        {
          "id": "MDRW2100042823.1.18",
          "form": "두개 구매했당 ㅋㅋㅋㅋ",
          "original_form": "두개 구매했당 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:43"
        },
        {
          "id": "MDRW2100042823.1.19",
          "form": "이제 곧 가을이니",
          "original_form": "이제 곧 가을이니",
          "speaker_id": "2",
          "time": "20210907 09:43"
        },
        {
          "id": "MDRW2100042823.1.20",
          "form": "얼굴이 땡길때가 왔다",
          "original_form": "얼굴이 땡길때가 왔다",
          "speaker_id": "2",
          "time": "20210907 09:43"
        },
        {
          "id": "MDRW2100042823.1.21",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "1",
          "time": "20210907 09:44"
        },
        {
          "id": "MDRW2100042823.1.22",
          "form": "좋으네 ㅋㅋㅋ 마스크팩 열개라니 ㅋㅋ",
          "original_form": "좋으네 ㅋㅋㅋ 마스크팩 열개라니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:44"
        },
        {
          "id": "MDRW2100042823.1.23",
          "form": "넘 좋네잌 ㅋㅋㅋㅋ 그거 검색한번 해봐야되겠다. 촉촉하다카이",
          "original_form": "넘 좋네잌 ㅋㅋㅋㅋ 그거 검색한번 해봐야되겠다. 촉촉하다카이",
          "speaker_id": "1",
          "time": "20210907 09:45"
        },
        {
          "id": "MDRW2100042823.1.24",
          "form": "엉 ㅎㅎ 그냥 쓱쓱 바르면 되니",
          "original_form": "엉 ㅎㅎ 그냥 쓱쓱 바르면 되니",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042823.1.25",
          "form": "괜찮은듯 ㅎㅎ",
          "original_form": "괜찮은듯 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042823.1.26",
          "form": "내 얼굴 완전 악건성이라",
          "original_form": "내 얼굴 완전 악건성이라",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042823.1.27",
          "form": "응응 좋다",
          "original_form": "응응 좋다",
          "speaker_id": "1",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042823.1.28",
          "form": "계속 오일 바르잖아",
          "original_form": "계속 오일 바르잖아",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042823.1.29",
          "form": "기미에 좋은건 없는가?",
          "original_form": "기미에 좋은건 없는가?",
          "speaker_id": "1",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042823.1.30",
          "form": "기미....",
          "original_form": "기미....",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042823.1.31",
          "form": "글쎄....",
          "original_form": "글쎄....",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042823.1.32",
          "form": "기미는...레이저?? ㅋㅋㅋㅋㅋ",
          "original_form": "기미는...레이저?? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042823.1.33",
          "form": "아하....",
          "original_form": "아하....",
          "speaker_id": "1",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042823.1.34",
          "form": "레이져로 가야되나....ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "레이져로 가야되나....ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
