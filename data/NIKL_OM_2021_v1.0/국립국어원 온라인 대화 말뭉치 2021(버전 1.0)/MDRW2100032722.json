      "utterance": [
        {
          "id": "MDRW2100032722.1.1",
          "form": "집안일 잘되니",
          "original_form": "집안일 잘되니",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032722.1.2",
          "form": "ㅋㅋㅋㅋ아니",
          "original_form": "ㅋㅋㅋㅋ아니",
          "speaker_id": "1",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032722.1.3",
          "form": "점점 게을러지는 중",
          "original_form": "점점 게을러지는 중",
          "speaker_id": "1",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032722.1.4",
          "form": "ㅋㅋㅋㅋㅋ뭐가 제일 귀찮아?",
          "original_form": "ㅋㅋㅋㅋㅋ뭐가 제일 귀찮아?",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032722.1.5",
          "form": "로봇청소기 갖고싶은지경",
          "original_form": "로봇청소기 갖고싶은지경",
          "speaker_id": "1",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032722.1.6",
          "form": "머리카락...",
          "original_form": "머리카락...",
          "speaker_id": "1",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032722.1.7",
          "form": "주워도주워도 머리카락 튀어나와....ㅋㅋㅋㅋ",
          "original_form": "주워도주워도 머리카락 튀어나와....ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100032722.1.8",
          "form": "ㅋㅋㅋㅋㅋ청소기돌리는거 젤 귀찮?",
          "original_form": "ㅋㅋㅋㅋㅋ청소기돌리는거 젤 귀찮?",
          "speaker_id": "2",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100032722.1.9",
          "form": "하긴ㅋㅋㅋㅋㅋ머리카락은 답없엌ㅋㅋㅋ",
          "original_form": "하긴ㅋㅋㅋㅋㅋ머리카락은 답없엌ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100032722.1.10",
          "form": "내 머리털 왤케 빠지는지 어이없어 ㅋㅋㅋ",
          "original_form": "내 머리털 왤케 빠지는지 어이없어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100032722.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ돌돌이가 잇어서 다행이여",
          "original_form": "ㅋㅋㅋㅋㅋㅋ돌돌이가 잇어서 다행이여",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100032722.1.12",
          "form": "ㅎ ㅏ 난 돌돌이 쓰는거 더 어렵던데...",
          "original_form": "ㅎ ㅏ 난 돌돌이 쓰는거 더 어렵던데...",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100032722.1.13",
          "form": "청소기가시원하긴혀",
          "original_form": "청소기가시원하긴혀",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100032722.1.14",
          "form": "벗겨내는데 한참 걸려서...",
          "original_form": "벗겨내는데 한참 걸려서...",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100032722.1.15",
          "form": "근디 그냥 앉아서 주변은 잘 치울구잇음ㅋㅋㅋㅋ",
          "original_form": "근디 그냥 앉아서 주변은 잘 치울구잇음ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100032722.1.16",
          "form": "아 그거 요즘에 와 신세계발견",
          "original_form": "아 그거 요즘에 와 신세계발견",
          "speaker_id": "2",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100032722.1.17",
          "form": "추천템잇음",
          "original_form": "추천템잇음",
          "speaker_id": "2",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100032722.1.18",
          "form": "ㅇㅇ그건 좋긴함ㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ그건 좋긴함ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100032722.1.19",
          "form": "먼데머야",
          "original_form": "먼데머야",
          "speaker_id": "1",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100032722.1.20",
          "form": "돌돌이 사선테이프돌돌이? 사선테이프?",
          "original_form": "돌돌이 사선테이프돌돌이? 사선테이프?",
          "speaker_id": "2",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100032722.1.21",
          "form": "사선?...테이프가 사선으로 찢어져?..",
          "original_form": "사선?...테이프가 사선으로 찢어져?..",
          "speaker_id": "1",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100032722.1.22",
          "form": "ㅇㅇㅇㅇㅇ완전신세계 머리카락 잔뜩묻어도 테이프 겁나 잘뜯김",
          "original_form": "ㅇㅇㅇㅇㅇ완전신세계 머리카락 잔뜩묻어도 테이프 겁나 잘뜯김",
          "speaker_id": "2",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100032722.1.23",
          "form": "오 그런게 있나 ㅋㅋㅋ",
          "original_form": "오 그런게 있나 ㅋㅋㅋ",
