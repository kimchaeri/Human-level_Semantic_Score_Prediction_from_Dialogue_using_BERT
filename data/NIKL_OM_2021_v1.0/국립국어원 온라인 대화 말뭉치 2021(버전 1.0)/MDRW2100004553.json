      "utterance": [
        {
          "id": "MDRW2100004553.1.1",
          "form": "작년에도 하긴 했던가",
          "original_form": "작년에도 하긴 했던가",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004553.1.2",
          "form": "개막식 안하고 했던거 같다",
          "original_form": "개막식 안하고 했던거 같다",
          "speaker_id": "2",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004553.1.3",
          "form": "이번에도 그런식으로 하려나보다",
          "original_form": "이번에도 그런식으로 하려나보다",
          "speaker_id": "1",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004553.1.4",
          "form": "부국제의 빅재미는 개막식인디...다시 개막실 갈 수 있을까",
          "original_form": "부국제의 빅재미는 개막식인디...다시 개막실 갈 수 있을까",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004553.1.5",
          "form": "내년은 가능하겠지 ㅋㅋ",
          "original_form": "내년은 가능하겠지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004553.1.6",
          "form": "안 유명한 영화 다양하게 보는 맛도 있었는데",
          "original_form": "안 유명한 영화 다양하게 보는 맛도 있었는데",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004553.1.7",
          "form": "그르게 내년엔 꼭 개막식 가쟈ㅋㅋㅋ",
          "original_form": "그르게 내년엔 꼭 개막식 가쟈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004553.1.8",
          "form": "피켓팅을 뚫고 ㅋㅋㅋ",
          "original_form": "피켓팅을 뚫고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004553.1.9",
          "form": "마지막으로 갔을때 개막작 좋았었는디",
          "original_form": "마지막으로 갔을때 개막작 좋았었는디",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004553.1.10",
          "form": "뭐였는지 기억이..",
          "original_form": "뭐였는지 기억이..",
          "speaker_id": "1",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004553.1.11",
          "form": "말 도둑들",
          "original_form": "말 도둑들",
          "speaker_id": "2",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004553.1.12",
          "form": "아 그래 기억력 좋네 ㅋㅋㅋㅋ",
          "original_form": "아 그래 기억력 좋네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004553.1.13",
          "form": "안경 안껴서 자막이 안보였던 기억이ㅋㅋㅋㅋㅋ",
          "original_form": "안경 안껴서 자막이 안보였던 기억이ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004553.1.14",
          "form": "근데 좋았다고 ㅋㅋㅋㅋㅋ",
          "original_form": "근데 좋았다고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004553.1.15",
          "form": "응 자막이 잘 안보이긴 했으나 언뜻언뜻 보여서 스토리는 알겠더라ㅋㅋㅋ",
          "original_form": "응 자막이 잘 안보이긴 했으나 언뜻언뜻 보여서 스토리는 알겠더라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004553.1.16",
          "form": "극한 관람 ㅋㅋㅋㅋ",
          "original_form": "극한 관람 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004553.1.17",
          "form": "근데 그 담날 봤던 미립자들? 그 영화 완전 실망ㅋㅋㅋ",
          "original_form": "근데 그 담날 봤던 미립자들? 그 영화 완전 실망ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004553.1.18",
          "form": "근데 부국제 출품 영화들은 크게 화제 되는거 말고는 까보면 응? 싶은거 좀 있는듯 ㅋㅋ",
          "original_form": "근데 부국제 출품 영화들은 크게 화제 되는거 말고는 까보면 응? 싶은거 좀 있는듯 ㅋㅋ",
          "speaker_id": "1",
