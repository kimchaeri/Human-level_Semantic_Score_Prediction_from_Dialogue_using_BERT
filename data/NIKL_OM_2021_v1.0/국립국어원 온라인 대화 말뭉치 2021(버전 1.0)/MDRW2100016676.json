        {
          "id": "MDRW2100016676.1.1",
          "form": "연필도 더 사야할 것 같아",
          "original_form": "연필도 더 사야할 것 같아",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016676.1.2",
          "form": "연필 집에 많잖아",
          "original_form": "연필 집에 많잖아",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016676.1.3",
          "form": "너무 작아져서 ㅎㅎ 좀 긴 연필 쓰고 싶네",
          "original_form": "너무 작아져서 ㅎㅎ 좀 긴 연필 쓰고 싶네",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016676.1.4",
          "form": "삼각연필 잡기 좋던데 그거로 사줘",
          "original_form": "삼각연필 잡기 좋던데 그거로 사줘",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016676.1.5",
          "form": "안그래도 그럴라고",
          "original_form": "안그래도 그럴라고",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016676.1.6",
          "form": "ㅇㅇ 색깔도 이쁘더만",
          "original_form": "ㅇㅇ 색깔도 이쁘더만",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016676.1.7",
          "form": "HB는 좀 흐리고 계속 B써야 할 것 같아",
          "original_form": "HB는 좀 흐리고 계속 B써야 할 것 같아",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016676.1.8",
          "form": "고학년은 샤프 쓰는 애들 없나?",
          "original_form": "고학년은 샤프 쓰는 애들 없나?",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016676.1.9",
          "form": "모르겠어 ㅎㅎ 글씨가 엉망이라 아직 샤프쓰면 안될듯",
          "original_form": "모르겠어 ㅎㅎ 글씨가 엉망이라 아직 샤프쓰면 안될듯",
          "speaker_id": "2",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016676.1.10",
          "form": "글씨가 쉽게 안고쳐지지 ㅎㅎ",
          "original_form": "글씨가 쉽게 안고쳐지지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016676.1.11",
          "form": "연필만 좋은거 쓰면 뭐해 ㅋㅋ 글씨는 기어가는데",
          "original_form": "연필만 좋은거 쓰면 뭐해 ㅋㅋ 글씨는 기어가는데",
          "speaker_id": "2",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016676.1.12",
          "form": "그래도 열심히 공부하고 있으니까 좋은 걸로 사줘",
          "original_form": "그래도 열심히 공부하고 있으니까 좋은 걸로 사줘",
          "speaker_id": "1",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016676.1.13",
          "form": "ㅇㅇ 알았응",
          "original_form": "ㅇㅇ 알았응",
