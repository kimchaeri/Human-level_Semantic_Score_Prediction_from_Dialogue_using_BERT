        {
          "id": "MDRW2100046857.1.4",
          "form": "",
          "original_form": "{share:info}",
          "speaker_id": "1",
          "time": "20211107 08:12"
        },
        {
          "id": "MDRW2100046857.1.5",
          "form": "드라마 클럽하우스 말하시나요?",
          "original_form": "드라마 클럽하우스 말하시나요?",
          "speaker_id": "1",
          "time": "20211107 08:12"
        },
        {
          "id": "MDRW2100046857.1.6",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20211108 09:22"
        },
        {
          "id": "MDRW2100046857.1.7",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20211108 09:22"
        },
        {
          "id": "MDRW2100046857.1.8",
          "form": "어플 말하는건데",
          "original_form": "어플 말하는건데",
          "speaker_id": "2",
          "time": "20211108 09:22"
        },
        {
          "id": "MDRW2100046857.1.9",
          "form": "작년에 만들어진 음성 소셜미디어인데 글이나 영상으로 대화할 수 없고 오직 음성으로만 대화할 수 있더라구요 ㅎㅎ",
          "original_form": "작년에 만들어진 음성 소셜미디어인데 글이나 영상으로 대화할 수 없고 오직 음성으로만 대화할 수 있더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211108 09:23"
        },
        {
          "id": "MDRW2100046857.1.10",
          "form": "지금은 어떤지 모르겠는데",
          "original_form": "지금은 어떤지 모르겠는데",
          "speaker_id": "2",
          "time": "20211108 09:23"
        },
        {
          "id": "MDRW2100046857.1.11",
          "form": "아이폰 유저들만 사용할 수 있었어요 ㅋㅋ",
          "original_form": "아이폰 유저들만 사용할 수 있었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 09:24"
        },
        {
          "id": "MDRW2100046857.1.12",
          "form": "제가 안드로이드라서",
          "original_form": "제가 안드로이드라서",
          "speaker_id": "1",
          "time": "20211108 09:32"
        },
        {
          "id": "MDRW2100046857.1.13",
          "form": "죄송해요",
          "original_form": "죄송해요",
          "speaker_id": "1",
          "time": "20211108 09:32"
        },
        {
          "id": "MDRW2100046857.1.14",
          "form": "음성채팅 같은 거네요?",
          "original_form": "음성채팅 같은 거네요?",
          "speaker_id": "1",
          "time": "20211108 09:33"
        },
        {
          "id": "MDRW2100046857.1.15",
          "form": "아앗 죄송할건 전혀 아닌데 ㅠㅠ",
          "original_form": "아앗 죄송할건 전혀 아닌데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211108 09:37"
        },
        {
          "id": "MDRW2100046857.1.16",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20211108 09:37"
        },
        {
          "id": "MDRW2100046857.1.17",
          "form": "이게 방금",
          "original_form": "이게 방금",
          "speaker_id": "2",
          "time": "20211108 09:37"
        },
        {
          "id": "MDRW2100046857.1.18",
          "form": "찾아보니까",
          "original_form": "찾아보니까",
          "speaker_id": "2",
          "time": "20211108 09:37"
        },
        {
          "id": "MDRW2100046857.1.19",
          "form": "초대장 제도는 없어졌는데",
          "original_form": "초대장 제도는 없어졌는데",
          "speaker_id": "2",
          "time": "20211108 09:37"
        },
        {
          "id": "MDRW2100046857.1.20",
          "form": "초대장이 있어야만 가입할 수 있어서",
          "original_form": "초대장이 있어야만 가입할 수 있어서",
          "speaker_id": "2",
          "time": "20211108 09:37"
        },
        {
          "id": "MDRW2100046857.1.21",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20211108 09:37"
        },
        {
          "id": "MDRW2100046857.1.22",
          "form": "유난이라고 좀 욕 먹었던걸로 기억해요 ㅋㅋㅋㅋ",
          "original_form": "유난이라고 좀 욕 먹었던걸로 기억해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 09:38"
        },
        {
          "id": "MDRW2100046857.1.23",
          "form": "초대장 사고팔고",
          "original_form": "초대장 사고팔고",
          "speaker_id": "1",
          "time": "20211108 09:38"
        },
        {
          "id": "MDRW2100046857.1.24",
          "form": "그러지않았나요?",
          "original_form": "그러지않았나요?",
          "speaker_id": "1",
          "time": "20211108 09:38"
        },
        {
          "id": "MDRW2100046857.1.25",
          "form": "네네 막 비싼 가격에는 아니었는데 초대장 사고 팔고 그랬더라구요 ㅋㅋㅋㅋ ㅎㅎ",
          "original_form": "네네 막 비싼 가격에는 아니었는데 초대장 사고 팔고 그랬더라구요 ㅋㅋㅋㅋ ㅎㅎ",
          "speaker_id": "2",
          "time": "20211108 09:38"
        },
        {
          "id": "MDRW2100046857.1.26",
          "form": "사실",
          "original_form": "사실",
          "speaker_id": "2",
          "time": "20211108 09:38"
        },
        {
          "id": "MDRW2100046857.1.27",
          "form": "저도 안드로이드 유저였어서 그 땐",
          "original_form": "저도 안드로이드 유저였어서 그 땐",
          "speaker_id": "2",
          "time": "20211108 09:38"
        },
        {
          "id": "MDRW2100046857.1.28",
          "form": "되게 궁금했는데",
          "original_form": "되게 궁금했는데",
          "speaker_id": "2",
          "time": "20211108 09:38"
        },
        {
          "id": "MDRW2100046857.1.29",
          "form": "할 수가 없어서 ㅋㅋ ㅠㅠ",
          "original_form": "할 수가 없어서 ㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20211108 09:38"
        },
        {
          "id": "MDRW2100046857.1.30",
          "form": "아쉬웠는데",
          "original_form": "아쉬웠는데",
          "speaker_id": "2",
          "time": "20211108 09:38"
        },
        {
          "id": "MDRW2100046857.1.31",
          "form": "지금은 안드로이드도 된다네용",
          "original_form": "지금은 안드로이드도 된다네용",
          "speaker_id": "2",
          "time": "20211108 09:39"
        },
        {
          "id": "MDRW2100046857.1.32",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20211108 09:39"
        },
        {
          "id": "MDRW2100046857.1.33",
          "form": "그 때 막 연예인들 중에 몇명이",
          "original_form": "그 때 막 연예인들 중에 몇명이",
          "speaker_id": "2",
          "time": "20211108 09:39"
        },
        {
          "id": "MDRW2100046857.1.34",
          "form": "클럽하우스 관련해서",
          "original_form": "클럽하우스 관련해서",
          "speaker_id": "2",
          "time": "20211108 09:40"
        },
        {
          "id": "MDRW2100046857.1.35",
          "form": "소신발언한 게 기억이 나는ㅋㅋㅋㅋ",
          "original_form": "소신발언한 게 기억이 나는ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 09:40"
        },
        {
          "id": "MDRW2100046857.1.36",
          "form": "우월해지고 싶은 심리가 반영된 소셜미디어 같다면서",
          "original_form": "우월해지고 싶은 심리가 반영된 소셜미디어 같다면서",
          "speaker_id": "2",
          "time": "20211108 09:40"
        },
        {
          "id": "MDRW2100046857.1.37",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20211108 09:40"
        },
        {
          "id": "MDRW2100046857.1.38",
          "form": "sns나 소셜미디어나",
          "original_form": "sns나 소셜미디어나",
          "speaker_id": "1",
          "time": "20211108 09:40"
        },
        {
          "id": "MDRW2100046857.1.39",
          "form": "약간 자신을 자랑하기위한",
          "original_form": "약간 자신을 자랑하기위한",
          "speaker_id": "1",
          "time": "20211108 09:40"
        },
        {
          "id": "MDRW2100046857.1.40",
          "form": "그런게 있는거같아요",
          "original_form": "그런게 있는거같아요",
          "speaker_id": "1",
          "time": "20211108 09:40"
        },
        {
          "id": "MDRW2100046857.1.41",
          "form": "맞아요... ㅎㅎ 자기가 얼마나 잘사는지 보여주려는 ㅎㅎ",
          "original_form": "맞아요... ㅎㅎ 자기가 얼마나 잘사는지 보여주려는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211108 09:41"
        },
        {
          "id": "MDRW2100046857.1.42",
          "form": "그래서 sns 보고 상대적박탈감 느끼시는 분들도 엄청 많은거 같아요 ㅠ",
          "original_form": "그래서 sns 보고 상대적박탈감 느끼시는 분들도 엄청 많은거 같아요 ㅠ",
          "speaker_id": "2",
          "time": "20211108 09:41"
        },
        {
          "id": "MDRW2100046857.1.43",
          "form": "저도 가끔 그러기도 하고...",
          "original_form": "저도 가끔 그러기도 하고...",
          "speaker_id": "2",
          "time": "20211108 09:41"
        },
        {
          "id": "MDRW2100046857.1.44",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20211108 09:41"
        },
        {
          "id": "MDRW2100046857.1.45",
          "form": "근데 남사는 이야기 보고 자극받을때가 있어서 나름 동기부여가 되는거같기도해요",
          "original_form": "근데 남사는 이야기 보고 자극받을때가 있어서 나름 동기부여가 되는거같기도해요",
          "speaker_id": "1",
          "time": "20211108 09:41"
        },
        {
          "id": "MDRW2100046857.1.46",
          "form": "나도 조금더 노력해서 이런차 사야지 이러면서",
          "original_form": "나도 조금더 노력해서 이런차 사야지 이러면서",
          "speaker_id": "1",
          "time": "20211108 09:41"
        },
        {
          "id": "MDRW2100046857.1.47",
          "form": "맞아요 ㅋㅋㅋ 적절하게 이용하면 좋은 거 같아요 ㅎㅎ",
          "original_form": "맞아요 ㅋㅋㅋ 적절하게 이용하면 좋은 거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211108 09:41"
        },
        {
          "id": "MDRW2100046857.1.48",
          "form": "전 그렇게 아파트 샀어요!",
          "original_form": "전 그렇게 아파트 샀어요!",
          "speaker_id": "1",
          "time": "20211108 09:42"
        },
