      "utterance": [
        {
          "id": "MDRW2100028528.1.1",
          "form": "메타버스라고 아니?",
          "original_form": "메타버스라고 아니?",
          "speaker_id": "1",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028528.1.2",
          "form": "아니 그게 뭐야 설명좀",
          "original_form": "아니 그게 뭐야 설명좀",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028528.1.3",
          "form": "가상공간이랑 현실이랑 섞은 거야",
          "original_form": "가상공간이랑 현실이랑 섞은 거야",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028528.1.4",
          "form": "이번에 에스파라는 가수 뮤비가 메타버스 기술을 이용한 거야",
          "original_form": "이번에 에스파라는 가수 뮤비가 메타버스 기술을 이용한 거야",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028528.1.5",
          "form": "아 에스파 넥스트레벨",
          "original_form": "아 에스파 넥스트레벨",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028528.1.6",
          "form": "주제가 너무 어렵다",
          "original_form": "주제가 너무 어렵다",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028528.1.7",
          "form": "뭐라도 말해",
          "original_form": "뭐라도 말해",
          "speaker_id": "1",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100028528.1.8",
          "form": "이번 9월 모의고사에 메타버스 관련 지문이 나왔어",
          "original_form": "이번 9월 모의고사에 메타버스 관련 지문이 나왔어",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028528.1.9",
          "form": "그랬구나 어려웠을거같아",
          "original_form": "그랬구나 어려웠을거같아",
          "speaker_id": "2",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028528.1.10",
          "form": "맞아 그래서 정답 먼저 체크하고 풀지도 않았어",
          "original_form": "맞아 그래서 정답 먼저 체크하고 풀지도 않았어",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028528.1.11",
          "form": "가상현실은 알지?",
          "original_form": "가상현실은 알지?",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028528.1.12",
          "form": "그건 알아",
          "original_form": "그건 알아",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028528.1.13",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028528.1.14",
          "form": "메타버스 관련 주식을 사야해",
          "original_form": "메타버스 관련 주식을 사야해",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028528.1.15",
          "form": "그게 전망이 좋대?",
          "original_form": "그게 전망이 좋대?",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028528.1.16",
          "form": "관련 주가 엄청 올랐어",
          "original_form": "관련 주가 엄청 올랐어",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028528.1.17",
          "form": "너도 알아보고 메타버스 관련 주 사",
          "original_form": "너도 알아보고 메타버스 관련 주 사",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028528.1.18",
          "form": "그래 나도 메타버스에 대해 좀 알아봐야겠다",
          "original_form": "그래 나도 메타버스에 대해 좀 알아봐야겠다",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028528.1.19",
          "form": "뒤쳐지는기분이야",
          "original_form": "뒤쳐지는기분이야",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028528.1.20",
          "form": "메타버스 관련 뮤비라도 좀 봐봐",
          "original_form": "메타버스 관련 뮤비라도 좀 봐봐",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028528.1.21",
          "form": "알겠어",
          "original_form": "알겠어",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028528.1.22",
          "form": "넌 관심이 많아보여",
          "original_form": "넌 관심이 많아보여",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028528.1.23",
          "form": "맞아 요즘 트랜드니까",
          "original_form": "맞아 요즘 트랜드니까",
          "speaker_id": "1",
          "time": "20210902 21:04"
        }
      ]
