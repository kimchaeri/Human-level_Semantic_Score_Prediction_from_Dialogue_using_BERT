      "utterance": [
        {
          "id": "MDRW2100044049.1.1",
          "form": "출출하네요",
          "original_form": "출출하네요",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044049.1.2",
          "form": "야식 어떠신가요 !",
          "original_form": "야식 어떠신가요 !",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044049.1.3",
          "form": "과자먹는중이에요 헤헤",
          "original_form": "과자먹는중이에요 헤헤",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044049.1.4",
          "form": "헐 무슨과자인가요",
          "original_form": "헐 무슨과자인가요",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100044049.1.5",
          "form": "오레오요!",
          "original_form": "오레오요!",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044049.1.6",
          "form": "오레오 ! 우유랑 먹으면 엄청 맛있죠",
          "original_form": "오레오 ! 우유랑 먹으면 엄청 맛있죠",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044049.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044049.1.8",
          "form": "우유에 드시고 계신가요 ?",
          "original_form": "우유에 드시고 계신가요 ?",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044049.1.9",
          "form": "우유없어서ㅠㅜ물이랑요",
          "original_form": "우유없어서ㅠㅜ물이랑요",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044049.1.10",
          "form": "이런.. 그래도 목 안막히게 물이랑 드셔서 다행이에요",
          "original_form": "이런.. 그래도 목 안막히게 물이랑 드셔서 다행이에요",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044049.1.11",
          "form": "초코같은 단거 좋아하시나요 ?",
          "original_form": "초코같은 단거 좋아하시나요 ?",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044049.1.12",
          "form": "좋아해요~",
          "original_form": "좋아해요~",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044049.1.13",
          "form": "단거좋아하세요?",
          "original_form": "단거좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044049.1.14",
          "form": "전 단거 별로 안좋아해요ㅠ 초코같은건 한입 먹고 못먹겠더라구요",
          "original_form": "전 단거 별로 안좋아해요ㅠ 초코같은건 한입 먹고 못먹겠더라구요",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044049.1.15",
          "form": "엇!",
          "original_form": "엇!",
