      "utterance": [
        {
          "id": "MDRW2100038170.1.1",
          "form": "빵 하니까 샌드위치도 먹고싶네요 ㅎㅎ",
          "original_form": "빵 하니까 샌드위치도 먹고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100038170.1.2",
          "form": "편의점에서 인기가요 샌드위치 엄청 사먹었는데 ㅎㅎ",
          "original_form": "편의점에서 인기가요 샌드위치 엄청 사먹었는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100038170.1.3",
          "form": "헐 마자욬ㅋㅋㅋ",
          "original_form": "헐 마자욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100038170.1.4",
          "form": "한때 엄청 유행했었죠 !",
          "original_form": "한때 엄청 유행했었죠 !",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100038170.1.5",
          "form": "대만샌드위치도요 ㅋㅋㅋㄱ !",
          "original_form": "대만샌드위치도요 ㅋㅋㅋㄱ !",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100038170.1.6",
          "form": "홍루이젠 너무 맛있죠ㅠㅠ!",
          "original_form": "홍루이젠 너무 맛있죠ㅠㅠ!",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100038170.1.7",
          "form": "근처에 있던 지점들이 거의 다 사라져서 너무 아쉬워요ㅠㅠ",
          "original_form": "근처에 있던 지점들이 거의 다 사라져서 너무 아쉬워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100038170.1.8",
          "form": "맞아요 ㅜㅜ 요새는 사람들이 잘 안먹어서 ㅜㅜ",
          "original_form": "맞아요 ㅜㅜ 요새는 사람들이 잘 안먹어서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100038170.1.9",
          "form": "요즘은 또 에그슬럿? 많이 먹던데",
          "original_form": "요즘은 또 에그슬럿? 많이 먹던데",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100038170.1.10",
          "form": "또서울~이죠~",
          "original_form": "또서울~이죠~",
          "speaker_id": "1",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100038170.1.11",
          "form": "에그드롭이랑 비슷하게 생겼네요??",
          "original_form": "에그드롭이랑 비슷하게 생겼네요??",
          "speaker_id": "2",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100038170.1.12",
          "form": "그나마 에그드롭은 골고루 퍼져있어서 덜 화나네요 ㅎㅎ",
          "original_form": "그나마 에그드롭은 골고루 퍼져있어서 덜 화나네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100038170.1.13",
          "form": "비슷한데 더 맛있다는 사람도 있더라구여??!",
          "original_form": "비슷한데 더 맛있다는 사람도 있더라구여??!",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100038170.1.14",
          "form": "에그드롭도 맛있으니ㄱㅙㄴ찮아요...!!!",
          "original_form": "에그드롭도 맛있으니ㄱㅙㄴ찮아요...!!!",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100038170.1.15",
          "form": "ㅋㅋㅋㅋ 맞아요 ㅋㅋㅋㅋ 어차피 요리법 비슷한 것 같은데",
          "original_form": "ㅋㅋㅋㅋ 맞아요 ㅋㅋㅋㅋ 어차피 요리법 비슷한 것 같은데",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100038170.1.16",
          "form": "크게 차이 안날거에요 ㅋㅋㅋㅋ",
          "original_form": "크게 차이 안날거에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100038170.1.17",
          "form": "근데 에그드롭도 은근 비싸서",
          "original_form": "근데 에그드롭도 은근 비싸서",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100038170.1.18",
          "form": "자주 못먹게 되는거같아요 ㅋㅋ",
          "original_form": "자주 못먹게 되는거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100038170.1.19",
          "form": "저는 먹는게 너무 불편해서 ㅋㅋㅋㅋ",
          "original_form": "저는 먹는게 너무 불편해서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100038170.1.20",
          "form": "먹다보니 줄줄 흐르고 그러더라구요",
          "original_form": "먹다보니 줄줄 흐르고 그러더라구요",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100038170.1.21",
          "form": "그래서 잘 안먹게됐어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 잘 안먹게됐어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100038170.1.22",
          "form": "아 먹는것도 불편하죠 ㅋㅋㅋ",
          "original_form": "아 먹는것도 불편하죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100038170.1.23",
          "form": "전 그래도 서브웨이는 맛있어서 사먹어요 ㅋㅋ",
          "original_form": "전 그래도 서브웨이는 맛있어서 사먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100038170.1.24",
          "form": "서브웨이 먹을 때 야채 다 넣으시나요 ㅎㅎ",
          "original_form": "서브웨이 먹을 때 야채 다 넣으시나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038170.1.25",
          "form": "전 올리브랑",
          "original_form": "전 올리브랑",
          "speaker_id": "1",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100038170.1.26",
          "form": "피망은 꼭 빼요 !!!",
          "original_form": "피망은 꼭 빼요 !!!",
          "speaker_id": "1",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100038170.1.27",
          "form": "전 야채 다 빼고 먹고싶은데 늘 놀라셔서... 피클 하나 넣어요 ㅎㅎ",
          "original_form": "전 야채 다 빼고 먹고싶은데 늘 놀라셔서... 피클 하나 넣어요 ㅎㅎ",
