      "utterance": [
        {
          "id": "MDRW2100035469.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100035469.1.2",
          "form": "오늘 점심 뭐드셨어요?",
          "original_form": "오늘 점심 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100035469.1.3",
          "form": "순대국 먹었어요",
          "original_form": "순대국 먹었어요",
          "speaker_id": "2",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100035469.1.4",
          "form": "저도 순대국 넘 먹고싶네요ㅠ 시켜드셨나요?",
          "original_form": "저도 순대국 넘 먹고싶네요ㅠ 시켜드셨나요?",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100035469.1.5",
          "form": "포장해서 먹었어요",
          "original_form": "포장해서 먹었어요",
          "speaker_id": "2",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100035469.1.6",
          "form": "순대국 좋아하세요?",
          "original_form": "순대국 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100035469.1.7",
          "form": "네 좋아해요ㅎ name2님은 순대국에 내장 넣으시는 편인가요?",
          "original_form": "네 좋아해요ㅎ &name2&님은 순대국에 내장 넣으시는 편인가요?",
          "speaker_id": "1",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100035469.1.8",
          "form": "저는 안 넣어요",
          "original_form": "저는 안 넣어요",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100035469.1.9",
          "form": "순대만 넣어서 먹어요ㅎㅎ",
          "original_form": "순대만 넣어서 먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100035469.1.10",
          "form": "내장류를 좋아하지 않아서..",
          "original_form": "내장류를 좋아하지 않아서..",
          "speaker_id": "2",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100035469.1.11",
          "form": "그쵸 저는 내장을 매우 좋아하지만 생김새가 기괴하다고는 생각들어요ㅋㅋ",
          "original_form": "그쵸 저는 내장을 매우 좋아하지만 생김새가 기괴하다고는 생각들어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100035469.1.12",
          "form": "그쵸ㅋㅋ 그래도 좋아하고 잘 드시는 분들 많더라구요",
          "original_form": "그쵸ㅋㅋ 그래도 좋아하고 잘 드시는 분들 많더라구요",
          "speaker_id": "2",
          "time": "20210829 20:05"
        },
        {
          "id": "MDRW2100035469.1.13",
          "form": "네 저도요 전 고기류는 가리는거 없이 다 좋아요ㅋㅋ 그래서 살찌나봐요ㅎ",
          "original_form": "네 저도요 전 고기류는 가리는거 없이 다 좋아요ㅋㅋ 그래서 살찌나봐요ㅎ",
          "speaker_id": "1",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100035469.1.14",
          "form": "식욕이 많을수록 다이어트하기 힘든거 같아요",
          "original_form": "식욕이 많을수록 다이어트하기 힘든거 같아요",
          "speaker_id": "2",
          "time": "20210829 21:11"
        },
        {
          "id": "MDRW2100035469.1.15",
          "form": "그렇긴하죠 많이먹어도 살안찌는 사람이 부럽네요",
          "original_form": "그렇긴하죠 많이먹어도 살안찌는 사람이 부럽네요",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100035469.1.16",
          "form": "그런분들 보면 넘 신기하더라구요\r\n먹는 양이 어마어마하던데 그대로인분들",
          "original_form": "그런분들 보면 넘 신기하더라구요\r\n먹는 양이 어마어마하던데 그대로인분들",
          "speaker_id": "2",
          "time": "20210829 21:15"
        }
      ]
