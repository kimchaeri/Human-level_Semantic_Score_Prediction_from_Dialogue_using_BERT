      "utterance": [
        {
          "id": "MDRW2100042339.1.1",
          "form": "학교 밥은 결국 맛이 없었던 건",
          "original_form": "학교 밥은 결국 맛이 없었던 건",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042339.1.2",
          "form": "밥을 왜 잘 안 먹냐 솔직하게 얘기해 봐라",
          "original_form": "밥을 왜 잘 안 먹냐 솔직하게 얘기해 봐라",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042339.1.3",
          "form": "아니 잘 먹을 때도 있는데",
          "original_form": "아니 잘 먹을 때도 있는데",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042339.1.4",
          "form": "아 잘 안 먹을 때가 더 많다고 하더라고",
          "original_form": "아 잘 안 먹을 때가 더 많다고 하더라고",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042339.1.5",
          "form": "언니 머리 꼭대기에 올라가 있음ㅋㅋ",
          "original_form": "언니 머리 꼭대기에 올라가 있음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042339.1.6",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042339.1.7",
          "form": "나는 아침에 시리얼 자주 먹고 가도",
          "original_form": "나는 아침에 시리얼 자주 먹고 가도",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042339.1.8",
          "form": "학교에서 밥을 잘 먹으니까",
          "original_form": "학교에서 밥을 잘 먹으니까",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042339.1.9",
          "form": "한 소리 할 거 같으니까 아예 그냥 맛있다고 잘 먹는다고",
          "original_form": "한 소리 할 거 같으니까 아예 그냥 맛있다고 잘 먹는다고",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042339.1.10",
          "form": "괜찮을 거라 생각했거든",
          "original_form": "괜찮을 거라 생각했거든",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042339.1.11",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042339.1.12",
          "form": "내일 아침부터 바쁘겠네요",
          "original_form": "내일 아침부터 바쁘겠네요",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042339.1.13",
          "form": "아침에 시리얼을 먹고",
          "original_form": "아침에 시리얼을 먹고",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042339.1.14",
          "form": "점심도 대충 먹고",
          "original_form": "점심도 대충 먹고",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042339.1.15",
          "form": "밥 챙겨 먹어야 되네ㅋㅋ",
          "original_form": "밥 챙겨 먹어야 되네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042339.1.16",
          "form": "놀이터에 가서 6시 반에 집에 오는 거야",
          "original_form": "놀이터에 가서 6시 반에 집에 오는 거야",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042339.1.17",
          "form": "얼마나 배가 고프겠어",
          "original_form": "얼마나 배가 고프겠어",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042339.1.18",
          "form": "어 그래서 그다음 날부터",
          "original_form": "어 그래서 그다음 날부터",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042339.1.19",
          "form": "진짜 배고프겠는데",
          "original_form": "진짜 배고프겠는데",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042339.1.20",
          "form": "아침에 무조건 밥ㅋㅋㅋㅋㅋㄱ",
          "original_form": "아침에 무조건 밥ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042339.1.21",
          "form": "근데 중요한 건",
          "original_form": "근데 중요한 건",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042339.1.22",
          "form": "그렇게 놀고 집에 와서 밥을 잘 안 먹는다는 거",
          "original_form": "그렇게 놀고 집에 와서 밥을 잘 안 먹는다는 거",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042339.1.23",
          "form": "집에 오면",
          "original_form": "집에 오면",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042339.1.24",
          "form": "서로 피곤해지겠네ㅋㅋㅋㅋ",
          "original_form": "서로 피곤해지겠네ㅋㅋㅋㅋ",
          "speaker_id": "2",
