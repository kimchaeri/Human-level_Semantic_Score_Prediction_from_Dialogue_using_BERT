      "utterance": [
        {
          "id": "MDRW2100032746.1.1",
          "form": "거기는 지금 날씨 어때?",
          "original_form": "거기는 지금 날씨 어때?",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032746.1.2",
          "form": "선선해",
          "original_form": "선선해",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032746.1.3",
          "form": "비는 안 와?",
          "original_form": "비는 안 와?",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032746.1.4",
          "form": "긴팔 입고 다녀도 괜찮을 정도야",
          "original_form": "긴팔 입고 다녀도 괜찮을 정도야",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032746.1.5",
          "form": "비는 안오는듯??",
          "original_form": "비는 안오는듯??",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032746.1.6",
          "form": "일주일에 한 번 오나",
          "original_form": "일주일에 한 번 오나",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032746.1.7",
          "form": "여기도 비는 안 온다!",
          "original_form": "여기도 비는 안 온다!",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032746.1.8",
          "form": "거긴 아직도 비 와?",
          "original_form": "거긴 아직도 비 와?",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032746.1.9",
          "form": "아 글쿠만",
          "original_form": "아 글쿠만",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032746.1.10",
          "form": "안 와 안 와",
          "original_form": "안 와 안 와",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032746.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032746.1.12",
          "form": "낮에는 안더워?",
          "original_form": "낮에는 안더워?",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032746.1.13",
          "form": "요즘 밖에 잘 안 나가서 모르겠는데",
          "original_form": "요즘 밖에 잘 안 나가서 모르겠는데",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032746.1.14",
          "form": "일단 집 안에 있으면 예전보다 덜 더워",
          "original_form": "일단 집 안에 있으면 예전보다 덜 더워",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032746.1.15",
          "form": "오 다행이네",
          "original_form": "오 다행이네",
          "speaker_id": "2",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032746.1.16",
          "form": "이제 에어컨 안켜도",
          "original_form": "이제 에어컨 안켜도",
          "speaker_id": "2",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032746.1.17",
          "form": "충분한듯",
          "original_form": "충분한듯",
          "speaker_id": "2",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032746.1.18",
          "form": "맞어 맞어",
          "original_form": "맞어 맞어",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032746.1.19",
          "form": "근데 아직까지 좀 습한 감은 있어서",
          "original_form": "근데 아직까지 좀 습한 감은 있어서",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032746.1.20",
          "form": "밤에는 잠깐 틀어",
          "original_form": "밤에는 잠깐 틀어",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032746.1.21",
          "form": "ㅇㅎ... 여긴 건조혀",
          "original_form": "ㅇㅎ... 여긴 건조혀",
          "speaker_id": "2",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032746.1.22",
          "form": "건조?",
          "original_form": "건조?",
          "speaker_id": "2",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032746.1.23",
          "form": "적당해",
          "original_form": "적당해",
          "speaker_id": "2",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032746.1.24",
          "form": "좋네...",
          "original_form": "좋네...",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032746.1.25",
          "form": "여긴 주변이 바다라 습하다",
          "original_form": "여긴 주변이 바다라 습하다",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032746.1.26",
          "form": "낮엔 안 덥구?",
          "original_form": "낮엔 안 덥구?",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032746.1.27",
          "form": "낮에는 긴팔까지는 못입나??",
          "original_form": "낮에는 긴팔까지는 못입나??",
          "speaker_id": "2",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100032746.1.28",
          "form": "근데 좀 참으면 입을만할정도",
          "original_form": "근데 좀 참으면 입을만할정도",
          "speaker_id": "2",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100032746.1.29",
          "form": "시원해",
          "original_form": "시원해",
          "speaker_id": "2",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100032746.1.30",
          "form": "여긴 아직 긴팔은 좀 힘들 듯?",
          "original_form": "여긴 아직 긴팔은 좀 힘들 듯?",
          "speaker_id": "1",
          "time": "20210905 23:14"
        },
        {
          "id": "MDRW2100032746.1.31",
          "form": "시원해서 다행이다 드디어 가을이구나",
          "original_form": "시원해서 다행이다 드디어 가을이구나",
          "speaker_id": "1",
          "time": "20210905 23:14"
        },
        {
          "id": "MDRW2100032746.1.32",
          "form": "입추는 진작했는데",
          "original_form": "입추는 진작했는데",
          "speaker_id": "2",
          "time": "20210905 23:14"
        },
        {
          "id": "MDRW2100032746.1.33",
          "form": "더위는 너무 늦게 가셔",
          "original_form": "더위는 너무 늦게 가셔",
          "speaker_id": "2",
          "time": "20210905 23:14"
        },
        {
          "id": "MDRW2100032746.1.34",
          "form": "근데 나는... 가을보다는 겨울이 좋아",
          "original_form": "근데 나는... 가을보다는 겨울이 좋아",
          "speaker_id": "2",
          "time": "20210905 23:15"
        },
        {
          "id": "MDRW2100032746.1.35",
          "form": "그치 누가 보면 아직 여름인줄",
          "original_form": "그치 누가 보면 아직 여름인줄",
          "speaker_id": "1",
          "time": "20210905 23:16"
        },
        {
          "id": "MDRW2100032746.1.36",
          "form": "오 왜?",
          "original_form": "오 왜?",
          "speaker_id": "1",
          "time": "20210905 23:16"
        },
        {
          "id": "MDRW2100032746.1.37",
          "form": "그건 잘 모르겠는데",
          "original_form": "그건 잘 모르겠는데",
          "speaker_id": "2",
          "time": "20210905 23:16"
        },
        {
          "id": "MDRW2100032746.1.38",
          "form": "약간 계절마다 냄새가 다르잖아",
          "original_form": "약간 계절마다 냄새가 다르잖아",
          "speaker_id": "2",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100032746.1.39",
          "form": "겨울 냄새가 좋어",
          "original_form": "겨울 냄새가 좋어",
          "speaker_id": "2",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100032746.1.40",
          "form": "그리고 껑꽁 얼었다가",
          "original_form": "그리고 껑꽁 얼었다가",
          "speaker_id": "2",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100032746.1.41",
          "form": "뜨끈한 장판에 등 지지는게 조아",
          "original_form": "뜨끈한 장판에 등 지지는게 조아",
          "speaker_id": "2",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100032746.1.42",
          "form": "뭔 느낌인진 알 것 같다",
          "original_form": "뭔 느낌인진 알 것 같다",
          "speaker_id": "1",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100032746.1.43",
          "form": "맞지맞지 장판 깔고 누우면 기분 짱 조음",
          "original_form": "맞지맞지 장판 깔고 누우면 기분 짱 조음",
          "speaker_id": "1",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100032746.1.44",
          "form": "맞지 좀 뭐라고하지",
          "original_form": "맞지 좀 뭐라고하지",
          "speaker_id": "2",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100032746.1.45",
          "form": "녹는 것 같은",
          "original_form": "녹는 것 같은",
          "speaker_id": "2",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100032746.1.46",
          "form": "기분",
          "original_form": "기분",
          "speaker_id": "2",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100032746.1.47",
          "form": "ㅋㅋㅋㅋㅋ마시멜로 되는 기분...",
          "original_form": "ㅋㅋㅋㅋㅋ마시멜로 되는 기분...",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032746.1.48",
          "form": "구워지는 기분...",
          "original_form": "구워지는 기분...",
          "speaker_id": "2",
          "time": "20210905 23:26"
        }
      ]
    }
  ]
}
