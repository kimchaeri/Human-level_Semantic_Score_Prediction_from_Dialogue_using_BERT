      "utterance": [
        {
          "id": "MDRW2100045787.1.1",
          "form": "안녕하세요 최근에 관람하신거 혹시 있으신가요?",
          "original_form": "안녕하세요 최근에 관람하신거 혹시 있으신가요?",
          "speaker_id": "1",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100045787.1.2",
          "form": "안녕하세요 최근엔 코로나때문에 ㅠㅠ",
          "original_form": "안녕하세요 최근엔 코로나때문에 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100045787.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100045787.1.4",
          "form": "저도 코로나 때문에 못다녔네요.",
          "original_form": "저도 코로나 때문에 못다녔네요.",
          "speaker_id": "1",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100045787.1.5",
          "form": "원래는 공연이나 전시회 좋아하셨나요?",
          "original_form": "원래는 공연이나 전시회 좋아하셨나요?",
          "speaker_id": "1",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100045787.1.6",
          "form": "네 좋아햇어요",
          "original_form": "네 좋아햇어요",
          "speaker_id": "2",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100045787.1.7",
          "form": "영화관도 좋아하고",
          "original_form": "영화관도 좋아하고",
          "speaker_id": "2",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100045787.1.8",
          "form": "저도 공연이나 전시회보다는 영화관에 제일 많이 갔네요",
          "original_form": "저도 공연이나 전시회보다는 영화관에 제일 많이 갔네요",
          "speaker_id": "1",
          "time": "20210908 07:14"
        },
        {
          "id": "MDRW2100045787.1.9",
          "form": "영화도 최근에 본적없으신가요?",
          "original_form": "영화도 최근에 본적없으신가요?",
          "speaker_id": "1",
          "time": "20210908 07:14"
        },
        {
          "id": "MDRW2100045787.1.10",
          "form": "네 영화관에 안갓어요",
          "original_form": "네 영화관에 안갓어요",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100045787.1.11",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100045787.1.12",
          "form": "저는 한번씩은 가요",
          "original_form": "저는 한번씩은 가요",
          "speaker_id": "1",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100045787.1.13",
          "form": "오 최근에 뭐 보셧어요?",
          "original_form": "오 최근에 뭐 보셧어요?",
          "speaker_id": "2",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100045787.1.14",
          "form": "모가디슈 봤어요",
          "original_form": "모가디슈 봤어요",
          "speaker_id": "1",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100045787.1.15",
          "form": "아시너요?",
          "original_form": "아시너요?",
          "speaker_id": "1",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100045787.1.16",
          "form": "아시나요?",
          "original_form": "아시나요?",
          "speaker_id": "1",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100045787.1.17",
          "form": "보고싶엇던 영화예요",
          "original_form": "보고싶엇던 영화예요",
          "speaker_id": "2",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100045787.1.18",
          "form": "평이 괜찮던데요",
          "original_form": "평이 괜찮던데요",
          "speaker_id": "2",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100045787.1.19",
          "form": "네.재밌게 봤어요. 추천드립니다",
          "original_form": "네.재밌게 봤어요. 추천드립니다",
          "speaker_id": "1",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100045787.1.20",
          "form": "추천감사해요 나중에봐야겟어요",
          "original_form": "추천감사해요 나중에봐야겟어요",
          "speaker_id": "2",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100045787.1.21",
          "form": "인생영화 잇으세요?",
          "original_form": "인생영화 잇으세요?",
          "speaker_id": "2",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100045787.1.22",
          "form": "네 쉐이프오브워터랑 더 랍스터 라고 아실지 모르겠네요",
          "original_form": "네 쉐이프오브워터랑 더 랍스터 라고 아실지 모르겠네요",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045787.1.23",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045787.1.24",
          "form": "둘다 봣어요",
          "original_form": "둘다 봣어요",
          "speaker_id": "2",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045787.1.25",
          "form": "저는 그래비티랑 매드맥스요",
          "original_form": "저는 그래비티랑 매드맥스요",
          "speaker_id": "2",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045787.1.26",
          "form": "저도 둘다 좋아해요",
          "original_form": "저도 둘다 좋아해요",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045787.1.27",
          "form": "매드맥스는 극장에서 세번인가봤네요",
          "original_form": "매드맥스는 극장에서 세번인가봤네요",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045787.1.28",
          "form": "매드맥스 진짜 극장에서만 여섯번인가 봣어요",
          "original_form": "매드맥스 진짜 극장에서만 여섯번인가 봣어요",
          "speaker_id": "2",
