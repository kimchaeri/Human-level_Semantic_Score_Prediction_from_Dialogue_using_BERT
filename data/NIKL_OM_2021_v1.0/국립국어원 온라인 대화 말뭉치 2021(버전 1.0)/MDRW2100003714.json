      "utterance": [
        {
          "id": "MDRW2100003714.1.1",
          "form": "반려동물키우시나요?",
          "original_form": "반려동물키우시나요?",
          "speaker_id": "2",
          "time": "20210821 13:59"
        },
        {
          "id": "MDRW2100003714.1.2",
          "form": "네 지금 산책중이에요",
          "original_form": "네 지금 산책중이에요",
          "speaker_id": "1",
          "time": "20210821 13:59"
        },
        {
          "id": "MDRW2100003714.1.3",
          "form": "비 많이안오나요 비올때 산책힘드시겠어요",
          "original_form": "비 많이안오나요 비올때 산책힘드시겠어요",
          "speaker_id": "2",
          "time": "20210821 14:02"
        },
        {
          "id": "MDRW2100003714.1.4",
          "form": "여기 비 그쳤어요",
          "original_form": "여기 비 그쳤어요",
          "speaker_id": "1",
          "time": "20210821 14:05"
        },
        {
          "id": "MDRW2100003714.1.5",
          "form": "그치자마자 후다닥 산책하러 나왔어요",
          "original_form": "그치자마자 후다닥 산책하러 나왔어요",
          "speaker_id": "1",
          "time": "20210821 14:05"
        },
        {
          "id": "MDRW2100003714.1.6",
          "form": "안그러면 난리쳐가지고ㅜㅜ",
          "original_form": "안그러면 난리쳐가지고ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 14:06"
        },
        {
          "id": "MDRW2100003714.1.7",
          "form": "아 여긴 비가아직와서ㅜㅜ강쥐들은 맞아요",
          "original_form": "아 여긴 비가아직와서ㅜㅜ강쥐들은 맞아요",
          "speaker_id": "2",
          "time": "20210821 14:06"
        },
        {
          "id": "MDRW2100003714.1.8",
          "form": "산책안시켜주면 난리나니까",
          "original_form": "산책안시켜주면 난리나니까",
          "speaker_id": "2",
          "time": "20210821 14:06"
        },
        {
          "id": "MDRW2100003714.1.9",
          "form": "막 물고여있는곳으론 안가나요?",
          "original_form": "막 물고여있는곳으론 안가나요?",
          "speaker_id": "2",
          "time": "20210821 14:06"
        },
        {
          "id": "MDRW2100003714.1.10",
          "form": "이미 한바탕 물장난치고왔어요ㅋㅋㅋ",
          "original_form": "이미 한바탕 물장난치고왔어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003714.1.11",
          "form": "목욕 시키려구요ㅜㅜ",
          "original_form": "목욕 시키려구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003714.1.12",
          "form": "목욕만은 피하고싶었는데...",
          "original_form": "목욕만은 피하고싶었는데...",
          "speaker_id": "1",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003714.1.13",
          "form": "ㅋㅋㅋㅋㅋㅜ그럴거같았어요 물보면신나서 뛰댕기는 강쥐들많더라구여",
          "original_form": "ㅋㅋㅋㅋㅋㅜ그럴거같았어요 물보면신나서 뛰댕기는 강쥐들많더라구여",
          "speaker_id": "2",
          "time": "20210821 14:10"
        },
        {
          "id": "MDRW2100003714.1.14",
          "form": "강쥐들은 목욕 얼마만에 시켜주나요",
          "original_form": "강쥐들은 목욕 얼마만에 시켜주나요",
          "speaker_id": "2",
          "time": "20210821 14:10"
        },
        {
          "id": "MDRW2100003714.1.15",
          "form": "고양이는1년에 한번정도 시킬까말까라서..",
          "original_form": "고양이는1년에 한번정도 시킬까말까라서..",
          "speaker_id": "2",
          "time": "20210821 14:10"
        },
        {
          "id": "MDRW2100003714.1.16",
          "form": "아 고양이는 1년에 한번해요?",
          "original_form": "아 고양이는 1년에 한번해요?",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003714.1.17",
          "form": "강아지들은 2주에 한번은 시켜야해요",
          "original_form": "강아지들은 2주에 한번은 시켜야해요",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003714.1.18",
          "form": "고양이는 목욕 안시켜도 냄새 안나나요?",
          "original_form": "고양이는 목욕 안시켜도 냄새 안나나요?",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003714.1.19",
          "form": "네 자기들이 열심히 핥아요 ㅋㅋㅋ",
          "original_form": "네 자기들이 열심히 핥아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003714.1.20",
          "form": "헉2주에한번은해야한다니",
          "original_form": "헉2주에한번은해야한다니",
          "speaker_id": "2",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003714.1.21",
          "form": "강쥐집사님들 존경해요",
          "original_form": "강쥐집사님들 존경해요",
          "speaker_id": "2",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003714.1.22",
          "form": "냥이들 침성분이 뭐 씻는기능이있다하드라구여 일단얘네가 물을 진짜 너무싫어하기도하구여",
          "original_form": "냥이들 침성분이 뭐 씻는기능이있다하드라구여 일단얘네가 물을 진짜 너무싫어하기도하구여",
          "speaker_id": "2",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003714.1.23",
          "form": "오 신기하다",
          "original_form": "오 신기하다",
          "speaker_id": "1",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100003714.1.24",
          "form": "반려동물 키우면...",
          "original_form": "반려동물 키우면...",
          "speaker_id": "1",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100003714.1.25",
          "form": "하루가 뚝딱",
          "original_form": "하루가 뚝딱",
          "speaker_id": "1",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100003714.1.26",
          "form": "그쳐 그래도 냥이들은 반나절은 혼자 잠만자서 ㅎㅎ어쩔때는 얘가 있나싶기도해요",
          "original_form": "그쳐 그래도 냥이들은 반나절은 혼자 잠만자서 ㅎㅎ어쩔때는 얘가 있나싶기도해요",
          "speaker_id": "2",
          "time": "20210822 13:51"
        },
        {
          "id": "MDRW2100003714.1.27",
          "form": "강쥐들은 항상붙어있나요?",
          "original_form": "강쥐들은 항상붙어있나요?",
          "speaker_id": "2",
          "time": "20210822 13:51"
        },
        {
          "id": "MDRW2100003714.1.28",
          "form": "넵ㅎㅎ 지금도 제 발밑에있어요",
          "original_form": "넵ㅎㅎ 지금도 제 발밑에있어요",
          "speaker_id": "1",
          "time": "20210822 14:03"
        },
        {
          "id": "MDRW2100003714.1.29",
          "form": "ㅎㅎ귀여워라 저희집애는 불러도 들은척도안할때가많아서",
          "original_form": "ㅎㅎ귀여워라 저희집애는 불러도 들은척도안할때가많아서",
          "speaker_id": "2",
          "time": "20210822 14:13"
        },
        {
          "id": "MDRW2100003714.1.30",
          "form": "부러워여",
          "original_form": "부러워여",
          "speaker_id": "2",
          "time": "20210822 14:13"
        }
      ]
