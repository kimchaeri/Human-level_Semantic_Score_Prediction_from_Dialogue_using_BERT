      "utterance": [
        {
          "id": "MDRW2100006224.1.1",
          "form": "혹시 name2님은 법과 정치 말고",
          "original_form": "혹시 &name2&님은 법과 정치 말고",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100006224.1.2",
          "form": "좋아했던 과목 있으신가요??",
          "original_form": "좋아했던 과목 있으신가요??",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100006224.1.3",
          "form": "음.. 문과 과목은 그렇게 많이 배우진 않았어서 경제 배웠었는데 경제는 조금 어려웠었고",
          "original_form": "음.. 문과 과목은 그렇게 많이 배우진 않았어서 경제 배웠었는데 경제는 조금 어려웠었고",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100006224.1.4",
          "form": "저는 예체능 과목을 좋아해요그중에 특히 음악을 좋아하구요",
          "original_form": "저는 예체능 과목을 좋아해요그중에 특히 음악을 좋아하구요",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100006224.1.5",
          "form": "헉 많이 배우셨는데요??",
          "original_form": "헉 많이 배우셨는데요??",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100006224.1.6",
          "form": "전 경제는 배워본 적 없어요...",
          "original_form": "전 경제는 배워본 적 없어요...",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100006224.1.7",
          "form": "경제는 막 엄청 어렵진 않구요 은근하게 어려웠어요 ㅋㅋ",
          "original_form": "경제는 막 엄청 어렵진 않구요 은근하게 어려웠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100006224.1.8",
          "form": "어떤 과목 배우셨나요?",
          "original_form": "어떤 과목 배우셨나요?",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100006224.1.9",
          "form": "그냥 기본적으로 배우는 과목들에",
          "original_form": "그냥 기본적으로 배우는 과목들에",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100006224.1.10",
          "form": "사회는 사회문화 정치와법 한국지리 생활과윤리요",
          "original_form": "사회는 사회문화 정치와법 한국지리 생활과윤리요",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100006224.1.11",
          "form": "생활과 윤리 은근 어렵지 않아요?",
          "original_form": "생활과 윤리 은근 어렵지 않아요?",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006224.1.12",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006224.1.13",
          "form": "말이 아 다르고 어 다른 느낌이에요",
          "original_form": "말이 아 다르고 어 다른 느낌이에요",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006224.1.14",
          "form": "어쩜 그렇게 적절하게 표현하세요 ㅋㅋㅋ",
          "original_form": "어쩜 그렇게 적절하게 표현하세요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006224.1.15",
          "form": "딱 그 느낌이에요",
          "original_form": "딱 그 느낌이에요",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006224.1.16",
          "form": "결국 특정 구절들을 외워서 풀어야 하더라고요",
          "original_form": "결국 특정 구절들을 외워서 풀어야 하더라고요",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006224.1.17",
          "form": "예를 들어 동등 동일 이거 하나 바꾸고 틀렸다고 한다거나 그런 식으로...",
          "original_form": "예를 들어 동등 동일 이거 하나 바꾸고 틀렸다고 한다거나 그런 식으로...",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100006224.1.18",
          "form": "ㅠㅠ 꼼꼼함이 많이 요구되는 과목인 것 같아요",
          "original_form": "ㅠㅠ 꼼꼼함이 많이 요구되는 과목인 것 같아요",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100006224.1.19",
          "form": "어떤 과목이든 다 그렇지만요",
          "original_form": "어떤 과목이든 다 그렇지만요",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100006224.1.20",
          "form": "어떤 과목 선택하시기로 하셨나요?",
          "original_form": "어떤 과목 선택하시기로 하셨나요?",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100006224.1.21",
          "form": "저 사회문화랑 정치와 법이요",
          "original_form": "저 사회문화랑 정치와 법이요",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006224.1.22",
          "form": "역시 사문이 빠지지 않는군요",
          "original_form": "역시 사문이 빠지지 않는군요",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006224.1.23",
          "form": "아무래도 학교에서도 젤 미는 과목이라서요",
          "original_form": "아무래도 학교에서도 젤 미는 과목이라서요",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006224.1.24",
          "form": "동아리 활동도 하시나요?",
          "original_form": "동아리 활동도 하시나요?",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006224.1.25",
          "form": "글쓰기부에요!",
          "original_form": "글쓰기부에요!",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006224.1.26",
          "form": "아무래도 다들 선택하는 과목을 택해야 점수가 잘 나오긴 하겠죠 ㅠ",
          "original_form": "아무래도 다들 선택하는 과목을 택해야 점수가 잘 나오긴 하겠죠 ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006224.1.27",
          "form": "우와 굉장히 어울려요!",
          "original_form": "우와 굉장히 어울려요!",
          "speaker_id": "2",
          "time": "20210907 22:21"
        }
      ]
