      "utterance": [
        {
          "id": "MDRW2100017387.1.1",
          "form": "저녁 드셨나요?ㅎㅎ",
          "original_form": "저녁 드셨나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017387.1.2",
          "form": "넵 방금요",
          "original_form": "넵 방금요",
          "speaker_id": "2",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017387.1.3",
          "form": "파스타 ㅋㅋ",
          "original_form": "파스타 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017387.1.4",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017387.1.5",
          "form": "아 저는 오랜만에 보쌈",
          "original_form": "아 저는 오랜만에 보쌈",
          "speaker_id": "1",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017387.1.6",
          "form": "먹었어요",
          "original_form": "먹었어요",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017387.1.7",
          "form": "크 보쌈 ㅠㅠ",
          "original_form": "크 보쌈 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017387.1.8",
          "form": "시켜서요?",
          "original_form": "시켜서요?",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017387.1.9",
          "form": "아 부모님이 고기 삶아두셔서",
          "original_form": "아 부모님이 고기 삶아두셔서",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017387.1.10",
          "form": "쌈채소 곁들여 먹었어요ㅎㅎ",
          "original_form": "쌈채소 곁들여 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017387.1.11",
          "form": "우와ㅜㅜㅜㅜ",
          "original_form": "우와ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017387.1.12",
          "form": "부럽당 ㅎ",
          "original_form": "부럽당 ㅎ",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017387.1.13",
          "form": "김장할때 수육삶아 같이먹는거도 글케꿀맛인데.   ..갑자기 생각나네요",
          "original_form": "김장할때 수육삶아 같이먹는거도 글케꿀맛인데.   ..갑자기 생각나네요",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017387.1.14",
          "form": "ㅎㅎ김장 때 수육 먹음 진짜 꿀맛이죠",
          "original_form": "ㅎㅎ김장 때 수육 먹음 진짜 꿀맛이죠",
          "speaker_id": "1",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017387.1.15",
          "form": "또 어떤거 좋아하세요",
          "original_form": "또 어떤거 좋아하세요",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017387.1.16",
          "form": "크게 가리는 것 없이 잘먹는데",
          "original_form": "크게 가리는 것 없이 잘먹는데",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017387.1.17",
          "form": "일식이나 베트남음식 좋아해요",
          "original_form": "일식이나 베트남음식 좋아해요",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017387.1.18",
          "form": "오오 혹시 추천식당 잇으신가요",
          "original_form": "오오 혹시 추천식당 잇으신가요",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017387.1.19",
          "form": "음 그때그때 맛집 찾아서 먹는 스타일이라",
          "original_form": "음 그때그때 맛집 찾아서 먹는 스타일이라",
          "speaker_id": "1",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017387.1.20",
          "form": "name2님은 어떤 음식 좋아하세요?",
          "original_form": "&name2&님은 어떤 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017387.1.21",
          "form": "저 파스타요! ㅋㅋㅋ그리구 다 좋아해요 일식 베트남 저도 다 ㅠㅠ",
          "original_form": "저 파스타요! ㅋㅋㅋ그리구 다 좋아해요 일식 베트남 저도 다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017387.1.22",
          "form": "파스타!",
          "original_form": "파스타!",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017387.1.23",
          "form": "어떤 파스타 좋아하세요?ㅎㅎ",
          "original_form": "어떤 파스타 좋아하세요?ㅎㅎ",
