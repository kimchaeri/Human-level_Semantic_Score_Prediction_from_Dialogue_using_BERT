      "utterance": [
        {
          "id": "MDRW2100031262.1.1",
          "form": "학생이시죠?",
          "original_form": "학생이시죠?",
          "speaker_id": "1",
          "time": "20210904 19:25"
        },
        {
          "id": "MDRW2100031262.1.2",
          "form": "네 고등학생입니다!",
          "original_form": "네 고등학생입니다!",
          "speaker_id": "2",
          "time": "20210904 19:37"
        },
        {
          "id": "MDRW2100031262.1.3",
          "form": "요즘은 코로나로 단축 수업 같은거 있나요?",
          "original_form": "요즘은 코로나로 단축 수업 같은거 있나요?",
          "speaker_id": "1",
          "time": "20210904 19:38"
        },
        {
          "id": "MDRW2100031262.1.4",
          "form": "작년에는 온라인 수업도 하고 단축수업도 하고 그랬는데",
          "original_form": "작년에는 온라인 수업도 하고 단축수업도 하고 그랬는데",
          "speaker_id": "2",
          "time": "20210904 19:39"
        },
        {
          "id": "MDRW2100031262.1.5",
          "form": "올해는 전면등교 중이네요",
          "original_form": "올해는 전면등교 중이네요",
          "speaker_id": "2",
          "time": "20210904 19:39"
        },
        {
          "id": "MDRW2100031262.1.6",
          "form": "저는 대구인데 다른 지역도 그런진 모르겠어요",
          "original_form": "저는 대구인데 다른 지역도 그런진 모르겠어요",
          "speaker_id": "2",
          "time": "20210904 19:39"
        },
        {
          "id": "MDRW2100031262.1.7",
          "form": "다 비슷하지 않을까요",
          "original_form": "다 비슷하지 않을까요",
          "speaker_id": "1",
          "time": "20210904 19:40"
        },
        {
          "id": "MDRW2100031262.1.8",
          "form": "저는 직장다니는데 이제 저희도 재택근무 안하거든요",
          "original_form": "저는 직장다니는데 이제 저희도 재택근무 안하거든요",
          "speaker_id": "1",
          "time": "20210904 19:40"
        },
        {
          "id": "MDRW2100031262.1.9",
          "form": "오히려 확진자는 늘었는데",
          "original_form": "오히려 확진자는 늘었는데",
          "speaker_id": "1",
          "time": "20210904 19:41"
        },
        {
          "id": "MDRW2100031262.1.10",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210904 19:41"
        },
        {
          "id": "MDRW2100031262.1.11",
          "form": "다들 익숙해졌나봐요...",
          "original_form": "다들 익숙해졌나봐요...",
          "speaker_id": "2",
          "time": "20210904 19:41"
        },
        {
          "id": "MDRW2100031262.1.12",
          "form": "학교에서 동아리활동도 하나요?",
          "original_form": "학교에서 동아리활동도 하나요?",
          "speaker_id": "1",
          "time": "20210904 19:41"
        },
        {
          "id": "MDRW2100031262.1.13",
          "form": "1 2학년때는 많이 했는데",
          "original_form": "1 2학년때는 많이 했는데",
          "speaker_id": "2",
          "time": "20210904 19:42"
        },
        {
          "id": "MDRW2100031262.1.14",
          "form": "3학년은 거의 자습만 있어요",
          "original_form": "3학년은 거의 자습만 있어요",
          "speaker_id": "2",
          "time": "20210904 19:42"
        },
        {
          "id": "MDRW2100031262.1.15",
          "form": "아 3학년이시구나",
          "original_form": "아 3학년이시구나",
          "speaker_id": "1",
          "time": "20210904 19:43"
        },
        {
          "id": "MDRW2100031262.1.16",
          "form": "제일 힘든시기네요",
          "original_form": "제일 힘든시기네요",
          "speaker_id": "1",
          "time": "20210904 19:43"
        },
        {
          "id": "MDRW2100031262.1.17",
          "form": "그러게요...",
          "original_form": "그러게요...",
          "speaker_id": "2",
          "time": "20210904 19:43"
        },
        {
          "id": "MDRW2100031262.1.18",
          "form": "좀 힘들어야 하는데... ㅋㅋㅋ",
          "original_form": "좀 힘들어야 하는데... ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:43"
        },
        {
          "id": "MDRW2100031262.1.19",
          "form": "왜요?ㅎㅎ",
          "original_form": "왜요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 19:43"
        },
        {
          "id": "MDRW2100031262.1.20",
          "form": "아 그냥 요즘 공부를 좀 안 하는 거 같아서요...",
          "original_form": "아 그냥 요즘 공부를 좀 안 하는 거 같아서요...",
          "speaker_id": "2",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100031262.1.21",
          "form": "아 저도 3학년 여름방학때 한 2주정도 손 놓고 있었던 적 있어요",
          "original_form": "아 저도 3학년 여름방학때 한 2주정도 손 놓고 있었던 적 있어요",
          "speaker_id": "1",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100031262.1.22",
          "form": "자연스러운거라고 생각해요",
          "original_form": "자연스러운거라고 생각해요",
          "speaker_id": "1",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100031262.1.23",
          "form": "다들 고민하는",
          "original_form": "다들 고민하는",
          "speaker_id": "1",
          "time": "20210904 19:50"
        },
        {
          "id": "MDRW2100031262.1.24",
          "form": "그냥 잊고 다시 공부 시작하는 수밖에 없는것 같아요",
          "original_form": "그냥 잊고 다시 공부 시작하는 수밖에 없는것 같아요",
          "speaker_id": "1",
          "time": "20210904 19:50"
        },
        {
          "id": "MDRW2100031262.1.25",
          "form": "그래야 할 거 같네요",
          "original_form": "그래야 할 거 같네요",
          "speaker_id": "2",
          "time": "20210904 19:52"
        },
        {
          "id": "MDRW2100031262.1.26",
          "form": "혹시 실례가 안 된다면",
          "original_form": "혹시 실례가 안 된다면",
          "speaker_id": "2",
          "time": "20210904 19:52"
        },
        {
          "id": "MDRW2100031262.1.27",
          "form": "뭉댕님은 무슨 학과 나오셨는지 여쭤봐도 될까요??",
          "original_form": "뭉댕님은 무슨 학과 나오셨는지 여쭤봐도 될까요??",
          "speaker_id": "2",
          "time": "20210904 19:53"
        },
        {
          "id": "MDRW2100031262.1.28",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210904 19:53"
        },
        {
          "id": "MDRW2100031262.1.29",
          "form": "저는 전자전기컴퓨터공학이요",
          "original_form": "저는 전자전기컴퓨터공학이요",
          "speaker_id": "1",
          "time": "20210904 19:53"
        }
      ]
    }
