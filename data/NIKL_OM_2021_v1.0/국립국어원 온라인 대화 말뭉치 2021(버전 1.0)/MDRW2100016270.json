        {
          "id": "MDRW2100016270.1.2",
          "form": "넷플릭스 한달 쯤하고 싶네",
          "original_form": "넷플릭스 한달 쯤하고 싶네",
          "speaker_id": "2",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100016270.1.3",
          "form": "무료 체험하면 되지",
          "original_form": "무료 체험하면 되지",
          "speaker_id": "1",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100016270.1.4",
          "form": "아 그러고보니 그런 방법이",
          "original_form": "아 그러고보니 그런 방법이",
          "speaker_id": "2",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100016270.1.5",
          "form": "근데 막 딱히 엄청 볼게 없긴해",
          "original_form": "근데 막 딱히 엄청 볼게 없긴해",
          "speaker_id": "2",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100016270.1.6",
          "form": "보던게 새 시즌이 안 나와서",
          "original_form": "보던게 새 시즌이 안 나와서",
          "speaker_id": "2",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100016270.1.7",
          "form": "뭐 보는데",
          "original_form": "뭐 보는데",
          "speaker_id": "1",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100016270.1.8",
          "form": "마인드 헌터",
          "original_form": "마인드 헌터",
