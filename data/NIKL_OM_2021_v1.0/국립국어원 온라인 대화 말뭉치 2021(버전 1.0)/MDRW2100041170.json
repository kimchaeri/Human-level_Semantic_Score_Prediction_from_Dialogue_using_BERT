      "utterance": [
        {
          "id": "MDRW2100041170.1.1",
          "form": "쇼핑 어디서 많이 하세요?",
          "original_form": "쇼핑 어디서 많이 하세요?",
          "speaker_id": "2",
          "time": "20210905 19:23"
        },
        {
          "id": "MDRW2100041170.1.2",
          "form": "저 온라인과 집앞 마트요! ㅋㅋ",
          "original_form": "저 온라인과 집앞 마트요! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100041170.1.3",
          "form": "마트 가까이 사시나봐요",
          "original_form": "마트 가까이 사시나봐요",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100041170.1.4",
          "form": "저도 마트 가까운데 편해요 ㅎㅎ",
          "original_form": "저도 마트 가까운데 편해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100041170.1.5",
          "form": "아 대형마트아니고 동네마튼데 깨끗해요!",
          "original_form": "아 대형마트아니고 동네마튼데 깨끗해요!",
          "speaker_id": "1",
          "time": "20210905 19:30"
        },
        {
          "id": "MDRW2100041170.1.6",
          "form": "그럼 더 좋네요 ㅎㅎ",
          "original_form": "그럼 더 좋네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100041170.1.7",
          "form": "대형마트는 내려가는 길이 넘 멀어요",
          "original_form": "대형마트는 내려가는 길이 넘 멀어요",
          "speaker_id": "2",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100041170.1.8",
          "form": "더 쌀때가 많아요!",
          "original_form": "더 쌀때가 많아요!",
          "speaker_id": "1",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100041170.1.9",
          "form": "장사잘되겠어요",
          "original_form": "장사잘되겠어요",
          "speaker_id": "2",
          "time": "20210905 19:31"
        },
        {
          "id": "MDRW2100041170.1.10",
          "form": "예전에 그런 마트 알았는데요",
          "original_form": "예전에 그런 마트 알았는데요",
          "speaker_id": "2",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100041170.1.11",
          "form": "여기 진짜 돈 완전 긁어모아요",
          "original_form": "여기 진짜 돈 완전 긁어모아요",
          "speaker_id": "1",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100041170.1.12",
          "form": "주인이 어디서 싸게 구해오는 방법이 있나보더라구요",
          "original_form": "주인이 어디서 싸게 구해오는 방법이 있나보더라구요",
          "speaker_id": "2",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100041170.1.13",
          "form": "과일도 싸고 과자도 싸고..",
          "original_form": "과일도 싸고 과자도 싸고..",
          "speaker_id": "2",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100041170.1.14",
          "form": "회전율이 좋아서 물건도 신선하고요",
          "original_form": "회전율이 좋아서 물건도 신선하고요",
          "speaker_id": "1",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100041170.1.15",
          "form": "좋네요..ㅎㅎ",
          "original_form": "좋네요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 19:32"
        },
        {
          "id": "MDRW2100041170.1.16",
          "form": "운영 노하우가 있나봐요",
          "original_form": "운영 노하우가 있나봐요",
          "speaker_id": "2",
          "time": "20210905 19:33"
        },
        {
          "id": "MDRW2100041170.1.17",
          "form": "그러니까요 동네사람들 다 거기서 만나요ㅋㅋ",
          "original_form": "그러니까요 동네사람들 다 거기서 만나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:35"
        },
        {
          "id": "MDRW2100041170.1.18",
          "form": "어머 무슨 드라마같아요 훈훈하네요",
          "original_form": "어머 무슨 드라마같아요 훈훈하네요",
          "speaker_id": "2",
          "time": "20210905 19:36"
        },
        {
          "id": "MDRW2100041170.1.19",
          "form": "ㅋㅋ 그 사장님이 가장 기쁘실듯! ㅋㅋ",
          "original_form": "ㅋㅋ 그 사장님이 가장 기쁘실듯! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:40"
        }
      ]
