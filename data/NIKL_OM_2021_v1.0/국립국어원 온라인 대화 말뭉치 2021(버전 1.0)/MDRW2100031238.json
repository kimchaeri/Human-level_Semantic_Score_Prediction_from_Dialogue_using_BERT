      "utterance": [
        {
          "id": "MDRW2100031238.1.1",
          "form": "음 vod로 봐",
          "original_form": "음 vod로 봐",
          "speaker_id": "2",
          "time": "20210904 18:49"
        },
        {
          "id": "MDRW2100031238.1.2",
          "form": "재방이나",
          "original_form": "재방이나",
          "speaker_id": "2",
          "time": "20210904 18:50"
        },
        {
          "id": "MDRW2100031238.1.3",
          "form": "움 왜??",
          "original_form": "움 왜??",
          "speaker_id": "1",
          "time": "20210904 19:45"
        },
        {
          "id": "MDRW2100031238.1.4",
          "form": "나는 다봤다",
          "original_form": "나는 다봤다",
          "speaker_id": "1",
          "time": "20210904 19:45"
        },
        {
          "id": "MDRW2100031238.1.5",
          "form": "재밌ㅋㅋㅋ",
          "original_form": "재밌ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:45"
        },
        {
          "id": "MDRW2100031238.1.6",
          "form": "집에서는 아버지가 뉴스를 보시고",
          "original_form": "집에서는 아버지가 뉴스를 보시고",
          "speaker_id": "2",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100031238.1.7",
          "form": "기숙사엔 티비가 없어",
          "original_form": "기숙사엔 티비가 없어",
          "speaker_id": "2",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100031238.1.8",
          "form": "앗 ㅋㅋㅋ 웨이브 있자나",
          "original_form": "앗 ㅋㅋㅋ 웨이브 있자나",
          "speaker_id": "1",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100031238.1.9",
          "form": "아? 생각도 못했네",
          "original_form": "아? 생각도 못했네",
          "speaker_id": "2",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100031238.1.10",
          "form": "천재해 천재",
          "original_form": "천재해 천재",
          "speaker_id": "2",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100031238.1.11",
          "form": "ㅋㅋㅋ다행히 아직 웨이브는 실시간 무료야",
          "original_form": "ㅋㅋㅋ다행히 아직 웨이브는 실시간 무료야",
          "speaker_id": "1",
          "time": "20210904 19:48"
        },
        {
          "id": "MDRW2100031238.1.12",
          "form": "나도 가끔 그걸루 봐",
          "original_form": "나도 가끔 그걸루 봐",
          "speaker_id": "1",
          "time": "20210904 19:48"
        },
        {
          "id": "MDRW2100031238.1.13",
          "form": "몰랐어 그거로 봐야겠다",
          "original_form": "몰랐어 그거로 봐야겠다",
          "speaker_id": "2",
          "time": "20210904 19:48"
        },
        {
          "id": "MDRW2100031238.1.14",
          "form": "모르는게 없구만",
          "original_form": "모르는게 없구만",
          "speaker_id": "2",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100031238.1.15",
          "form": "웨이브안써봤어?",
          "original_form": "웨이브안써봤어?",
          "speaker_id": "1",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100031238.1.16",
          "form": "난 워낙티비에 관심 많으니까",
          "original_form": "난 워낙티비에 관심 많으니까",
          "speaker_id": "1",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100031238.1.17",
          "form": "응응 안 써봤어",
          "original_form": "응응 안 써봤어",
          "speaker_id": "2",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100031238.1.18",
          "form": "그렇지모",
          "original_form": "그렇지모",
          "speaker_id": "1",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100031238.1.19",
          "form": "옹 가입해두면 좋아 (?)",
          "original_form": "옹 가입해두면 좋아 (?)",
          "speaker_id": "1",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100031238.1.20",
          "form": "써볼게!! 좋아좋아",
          "original_form": "써볼게!! 좋아좋아",
          "speaker_id": "2",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100031238.1.21",
          "form": "굿굿 근데그럼 vod는 머루봐?",
          "original_form": "굿굿 근데그럼 vod는 머루봐?",
          "speaker_id": "1",
          "time": "20210904 19:50"
        },
        {
          "id": "MDRW2100031238.1.22",
          "form": "티비로? 티빈 비싸지않나",
          "original_form": "티비로? 티빈 비싸지않나",
          "speaker_id": "1",
          "time": "20210904 19:50"
        },
        {
          "id": "MDRW2100031238.1.23",
          "form": "나 폰 바꾸고 비싼 요금제 썼더니 kt에서 시즌인가 그거 줘서",
          "original_form": "나 폰 바꾸고 비싼 요금제 썼더니 kt에서 시즌인가 그거 줘서",
          "speaker_id": "2",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031238.1.24",
          "form": "일단 그거로 봐",
          "original_form": "일단 그거로 봐",
          "speaker_id": "2",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031238.1.25",
          "form": "앗 시즌에도 그럼 라이브나오지않나? 아 아악 공중파는 안나오낭",
          "original_form": "앗 시즌에도 그럼 라이브나오지않나? 아 아악 공중파는 안나오낭",
          "speaker_id": "1",
          "time": "20210904 19:54"
        },
        {
          "id": "MDRW2100031238.1.26",
          "form": "몰라 사실 그거로 vod랑 영화만 봐봤어!!",
          "original_form": "몰라 사실 그거로 vod랑 영화만 봐봤어!!",
          "speaker_id": "2",
          "time": "20210904 19:55"
        },
        {
          "id": "MDRW2100031238.1.27",
          "form": "나 시즌 포인트 받아서!",
          "original_form": "나 시즌 포인트 받아서!",
          "speaker_id": "1",
          "time": "20210904 19:55"
        },
        {
          "id": "MDRW2100031238.1.28",
          "form": "영어마법천자문을",
          "original_form": "영어마법천자문을",
          "speaker_id": "1",
          "time": "20210904 19:55"
        },
        {
          "id": "MDRW2100031238.1.29",
          "form": "구매하고 안봐서 기한이 끝나버렸어",
          "original_form": "구매하고 안봐서 기한이 끝나버렸어",
          "speaker_id": "1",
          "time": "20210904 19:55"
        },
        {
          "id": "MDRW2100031238.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:55"
        },
        {
          "id": "MDRW2100031238.1.31",
          "form": "그게뭐야??",
          "original_form": "그게뭐야??",
          "speaker_id": "2",
          "time": "20210904 19:56"
        },
        {
          "id": "MDRW2100031238.1.32",
          "form": "바쁘다보니까 간단하게는 기한끝났다는거임ㅋㅋ",
          "original_form": "바쁘다보니까 간단하게는 기한끝났다는거임ㅋㅋ",
          "speaker_id": "1",
