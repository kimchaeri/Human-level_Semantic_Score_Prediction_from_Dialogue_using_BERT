      "utterance": [
        {
          "id": "MDRW2100009246.1.1",
          "form": "반려동물있으세요?",
          "original_form": "반려동물있으세요?",
          "speaker_id": "1",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009246.1.2",
          "form": "아뇨 반려동물은 안 키웁니다..",
          "original_form": "아뇨 반려동물은 안 키웁니다..",
          "speaker_id": "2",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009246.1.3",
          "form": "키우고싶으신가요?",
          "original_form": "키우고싶으신가요?",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009246.1.4",
          "form": "막 그렇게 키우고 싶지는 않아요,,",
          "original_form": "막 그렇게 키우고 싶지는 않아요,,",
          "speaker_id": "2",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009246.1.5",
          "form": "저는 강아지가 키우고싶어요",
          "original_form": "저는 강아지가 키우고싶어요",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009246.1.6",
          "form": "안키우고싶은 이유가 있나요?",
          "original_form": "안키우고싶은 이유가 있나요?",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009246.1.7",
          "form": "동물을 아예 키워본 적이 없어서 조금 무서운 것 같아요..",
          "original_form": "동물을 아예 키워본 적이 없어서 조금 무서운 것 같아요..",
          "speaker_id": "2",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009246.1.8",
          "form": "강아지는 왜 키우고 싶으세요?",
          "original_form": "강아지는 왜 키우고 싶으세요?",
          "speaker_id": "2",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009246.1.9",
          "form": "자식같아서요!",
          "original_form": "자식같아서요!",
          "speaker_id": "1",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009246.1.10",
          "form": "근데 저도 책임감때문에 당장은 못 키우겠어요ㅠㅠ",
          "original_form": "근데 저도 책임감때문에 당장은 못 키우겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009246.1.11",
          "form": "어떤 종 키우고 싶으세요? 관심있는 종 있나요?",
          "original_form": "어떤 종 키우고 싶으세요? 관심있는 종 있나요?",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009246.1.12",
          "form": "아 책임감 중요하죠..",
          "original_form": "아 책임감 중요하죠..",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009246.1.13",
          "form": "저는 시베리안 허스키요!",
          "original_form": "저는 시베리안 허스키요!",
          "speaker_id": "1",
          "time": "20210822 22:42"
        },
        {
          "id": "MDRW2100009246.1.14",
          "form": "사실 좀 귀여운 종을 키우고싶어요",
          "original_form": "사실 좀 귀여운 종을 키우고싶어요",
          "speaker_id": "1",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100009246.1.15",
          "form": "오오 허스키..",
          "original_form": "오오 허스키..",
          "speaker_id": "2",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009246.1.16",
          "form": "아기 땐 귀여운데 나중에 엄청 커지지 않나요?",
          "original_form": "아기 땐 귀여운데 나중에 엄청 커지지 않나요?",
          "speaker_id": "2",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009246.1.17",
          "form": "네 커도 귀엽죠!",
          "original_form": "네 커도 귀엽죠!",
          "speaker_id": "1",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009246.1.18",
          "form": "하지만 병원비도 보험이안돼서 아마 못 키울 것 같네요..ㅠㅠ",
          "original_form": "하지만 병원비도 보험이안돼서 아마 못 키울 것 같네요..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009246.1.19",
          "form": "반려동물 용품 아는 것 있으신가여?",
          "original_form": "반려동물 용품 아는 것 있으신가여?",
          "speaker_id": "1",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009246.1.20",
          "form": "아아 아프면 병원비가 좀 많이 깨지긴 하겠네요..",
          "original_form": "아아 아프면 병원비가 좀 많이 깨지긴 하겠네요..",
          "speaker_id": "2",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009246.1.21",
          "form": "용품은 딱히 아는 게 없습니다..!",
          "original_form": "용품은 딱히 아는 게 없습니다..!",
          "speaker_id": "2",
          "time": "20210822 22:46"
        },
        {
          "id": "MDRW2100009246.1.22",
          "form": "저도 잘 모르는데 반려동물과 함께하는 유튜브도 많아서",
          "original_form": "저도 잘 모르는데 반려동물과 함께하는 유튜브도 많아서",
          "speaker_id": "1",
          "time": "20210822 22:46"
        },
        {
          "id": "MDRW2100009246.1.23",
          "form": "용품도 만만치않게 비싸다고하네요",
          "original_form": "용품도 만만치않게 비싸다고하네요",
          "speaker_id": "1",
          "time": "20210822 22:46"
        },
        {
          "id": "MDRW2100009246.1.24",
          "form": "아아 요즘 그런 유튜브들 많죠",
          "original_form": "아아 요즘 그런 유튜브들 많죠",
          "speaker_id": "2",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009246.1.25",
          "form": "용품도 비싸다니.. 형편이 그리 좋지 않으면 반려동물도 못키우겠네요",
          "original_form": "용품도 비싸다니.. 형편이 그리 좋지 않으면 반려동물도 못키우겠네요",
          "speaker_id": "2",
          "time": "20210822 22:48"
        },
        {
          "id": "MDRW2100009246.1.26",
          "form": "그렇죠",
