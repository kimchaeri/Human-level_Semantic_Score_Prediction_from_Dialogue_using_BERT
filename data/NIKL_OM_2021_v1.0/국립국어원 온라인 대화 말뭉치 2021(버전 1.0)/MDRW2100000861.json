      "utterance": [
        {
          "id": "MDRW2100000861.1.1",
          "form": "애들은 운동 잘하고 있어?",
          "original_form": "애들은 운동 잘하고 있어?",
          "speaker_id": "1",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000861.1.2",
          "form": "잘하고 있지",
          "original_form": "잘하고 있지",
          "speaker_id": "2",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000861.1.3",
          "form": "어떤 운동했나 오늘",
          "original_form": "어떤 운동했나 오늘",
          "speaker_id": "1",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000861.1.4",
          "form": "30분 산책하고 스쿼트랑 줄넘기",
          "original_form": "30분 산책하고 스쿼트랑 줄넘기",
          "speaker_id": "2",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000861.1.5",
          "form": "오 많이 했네",
          "original_form": "오 많이 했네",
          "speaker_id": "1",
          "time": "20210819 20:10"
        },
        {
          "id": "MDRW2100000861.1.6",
          "form": "넘 힘들어 ㅎㅎ",
          "original_form": "넘 힘들어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:10"
        },
        {
          "id": "MDRW2100000861.1.7",
          "form": "그래도 꾸준히 해야 건강해지지",
          "original_form": "그래도 꾸준히 해야 건강해지지",
          "speaker_id": "1",
          "time": "20210819 20:10"
        },
        {
          "id": "MDRW2100000861.1.8",
          "form": "ㅇㅇ 그래서 열심히 시켰어",
          "original_form": "ㅇㅇ 그래서 열심히 시켰어",
          "speaker_id": "2",
          "time": "20210819 20:10"
        },
        {
          "id": "MDRW2100000861.1.9",
          "form": "애들만 하는게 아니라 색시도 해야지 ㅎㅎ",
          "original_form": "애들만 하는게 아니라 색시도 해야지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 20:10"
        },
        {
          "id": "MDRW2100000861.1.10",
          "form": "ㅇㅇ 나도 노력은 하는데 잘 안돼",
          "original_form": "ㅇㅇ 나도 노력은 하는데 잘 안돼",
          "speaker_id": "2",
          "time": "20210819 20:10"
        },
        {
          "id": "MDRW2100000861.1.11",
          "form": "힘내요",
          "original_form": "힘내요",
          "speaker_id": "1",
          "time": "20210819 20:10"
        },
        {
          "id": "MDRW2100000861.1.12",
          "form": "조금씩이라도 하려고 노력중이야",
          "original_form": "조금씩이라도 하려고 노력중이야",
          "speaker_id": "2",
          "time": "20210819 20:11"
        },
        {
          "id": "MDRW2100000861.1.13",
          "form": "ㅇㅇ 오래오래 살아야지",
          "original_form": "ㅇㅇ 오래오래 살아야지",
          "speaker_id": "1",
          "time": "20210819 20:11"
        },
        {
          "id": "MDRW2100000861.1.14",
          "form": "걱정마용 나도 오래 살고 싶으니까 ㅎㅎ",
          "original_form": "걱정마용 나도 오래 살고 싶으니까 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:11"
        },
        {
          "id": "MDRW2100000861.1.15",
          "form": "틈틈이 줄넘기도 시켜",
          "original_form": "틈틈이 줄넘기도 시켜",
          "speaker_id": "1",
          "time": "20210819 20:11"
        },
        {
          "id": "MDRW2100000861.1.16",
          "form": "애들 300개만 해도 힘들다고 난리 ㅎㅎ",
          "original_form": "애들 300개만 해도 힘들다고 난리 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100000861.1.17",
          "form": "ㅎㅎ 그래도 해야지",
          "original_form": "ㅎㅎ 그래도 해야지",
          "speaker_id": "1",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100000861.1.18",
          "form": "요즘 마스크 쓰려고 하니 더 힘들어",
          "original_form": "요즘 마스크 쓰려고 하니 더 힘들어",
          "speaker_id": "2",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100000861.1.19",
          "form": "진짜 마스크라도 벗고 하면 나을텐데..",
          "original_form": "진짜 마스크라도 벗고 하면 나을텐데..",
          "speaker_id": "1",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100000861.1.20",
          "form": "집에서 할 땐 코가 시원함 ㅎㅎ",
          "original_form": "집에서 할 땐 코가 시원함 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100000861.1.21",
          "form": "정말 그렇겠네",
          "original_form": "정말 그렇겠네",
          "speaker_id": "1",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100000861.1.22",
          "form": "땀나면 더 힘들지 마스크",
          "original_form": "땀나면 더 힘들지 마스크",
          "speaker_id": "1",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100000861.1.23",
          "form": "얼른 마스크 안쓰고 싶음",
          "original_form": "얼른 마스크 안쓰고 싶음",
          "speaker_id": "2",
          "time": "20210819 20:13"
        },
        {
          "id": "MDRW2100000861.1.24",
          "form": "마스크 안쓰면 두 배는 더 열심히 운동할듯",
          "original_form": "마스크 안쓰면 두 배는 더 열심히 운동할듯",
          "speaker_id": "1",
          "time": "20210819 20:13"
        },
        {
          "id": "MDRW2100000861.1.25",
          "form": "내 말이 그말임 ㅎㅎ",
          "original_form": "내 말이 그말임 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:13"
        },
        {
          "id": "MDRW2100000861.1.26",
          "form": "화이팅!",
          "original_form": "화이팅!",
          "speaker_id": "1",
          "time": "20210819 20:13"
        },
        {
          "id": "MDRW2100000861.1.27",
          "form": "ㅎㅎ 신랑도 힘내",
          "original_form": "ㅎㅎ 신랑도 힘내",
          "speaker_id": "2",
          "time": "20210819 20:14"
        }
      ]
    }
