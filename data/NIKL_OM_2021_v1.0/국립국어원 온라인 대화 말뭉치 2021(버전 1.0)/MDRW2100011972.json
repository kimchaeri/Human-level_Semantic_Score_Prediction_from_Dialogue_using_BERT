      "utterance": [
        {
          "id": "MDRW2100011972.1.1",
          "form": "너무좋아요 정말",
          "original_form": "너무좋아요 정말",
          "speaker_id": "2",
          "time": "20210824 17:57"
        },
        {
          "id": "MDRW2100011972.1.2",
          "form": "어제도 먹었어요",
          "original_form": "어제도 먹었어요",
          "speaker_id": "2",
          "time": "20210824 17:57"
        },
        {
          "id": "MDRW2100011972.1.3",
          "form": "와우 떡볶이 그것은 소울푸드",
          "original_form": "와우 떡볶이 그것은 소울푸드",
          "speaker_id": "1",
          "time": "20210824 17:57"
        },
        {
          "id": "MDRW2100011972.1.4",
          "form": "어디꺼 좋아하세요",
          "original_form": "어디꺼 좋아하세요",
          "speaker_id": "1",
          "time": "20210824 17:57"
        },
        {
          "id": "MDRW2100011972.1.5",
          "form": "다 좋은데 청년다방 많이 먹어요~",
          "original_form": "다 좋은데 청년다방 많이 먹어요~",
          "speaker_id": "2",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011972.1.6",
          "form": "어디꺼 좋아하세요?",
          "original_form": "어디꺼 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011972.1.7",
          "form": "거기꺼 맛있어요",
          "original_form": "거기꺼 맛있어요",
          "speaker_id": "1",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011972.1.8",
          "form": "차돌떡볶이!",
          "original_form": "차돌떡볶이!",
          "speaker_id": "2",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011972.1.9",
          "form": "전 가리지않고 다 잘먹어요",
          "original_form": "전 가리지않고 다 잘먹어요",
          "speaker_id": "1",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011972.1.10",
          "form": "차돌 대박",
          "original_form": "차돌 대박",
          "speaker_id": "1",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011972.1.11",
          "form": "너무맛있어",
          "original_form": "너무맛있어",
          "speaker_id": "1",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011972.1.12",
          "form": "지원님 정말 배우신 분이네요",
          "original_form": "지원님 정말 배우신 분이네요",
          "speaker_id": "2",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011972.1.13",
          "form": "멋져요",
          "original_form": "멋져요",
          "speaker_id": "2",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011972.1.14",
          "form": "그럼 떡볶이는 떡을 좋아하세요 어묵을 좋아하세요",
          "original_form": "그럼 떡볶이는 떡을 좋아하세요 어묵을 좋아하세요",
          "speaker_id": "1",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011972.1.15",
          "form": "전 떡이요!",
          "original_form": "전 떡이요!",
          "speaker_id": "2",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011972.1.16",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011972.1.17",
          "form": "찾았다 내운명",
          "original_form": "찾았다 내운명",
          "speaker_id": "1",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011972.1.18",
          "form": "저 어묵좋아합니다",
          "original_form": "저 어묵좋아합니다",
          "speaker_id": "1",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011972.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ아진짴ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아진짴ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011972.1.20",
          "form": "메론빵님은 떡을 드시고",
          "original_form": "메론빵님은 떡을 드시고",
          "speaker_id": "1",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011972.1.21",
          "form": "같이 떡볶이 먹어야겠네요",
          "original_form": "같이 떡볶이 먹어야겠네요",
          "speaker_id": "2",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011972.1.22",
          "form": "저는 어묵을 먹겠습니다",
          "original_form": "저는 어묵을 먹겠습니다",
          "speaker_id": "1",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011972.1.23",
          "form": "완벽해요",
          "original_form": "완벽해요",
          "speaker_id": "2",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011972.1.24",
          "form": "운명이다 이건",
          "original_form": "운명이다 이건",
          "speaker_id": "1",
          "time": "20210824 17:59"
        }
      ]
