        },
        {
          "id": "MDRW2100035633.1.2",
          "form": "최근에 한명 나갔어요 ㅎㅎ",
          "original_form": "최근에 한명 나갔어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100035633.1.3",
          "form": "아아 아아아ㅏㅏㅏㅏㅏㅏㅏㅏㅏ",
          "original_form": "아아 아아아ㅏㅏㅏㅏㅏㅏㅏㅏㅏ",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100035633.1.4",
          "form": "이들",
          "original_form": "이들",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100035633.1.5",
          "form": "이군요!!!!!!!!!!!!",
          "original_form": "이군요!!!!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100035633.1.6",
          "form": "맞아요 !!!",
          "original_form": "맞아요 !!!",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100035633.1.7",
          "form": "좀 어려웠죸ㅋㅋㅋㅋ",
          "original_form": "좀 어려웠죸ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100035633.1.8",
          "form": "아마 저도 못맞췄을껄요 ...",
          "original_form": "아마 저도 못맞췄을껄요 ...",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100035633.1.9",
          "form": "눈감아주자 주의 발언... 철회하겠습니다",
          "original_form": "눈감아주자 주의 발언... 철회하겠습니다",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100035633.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100035633.1.11",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100035633.1.12",
          "form": "저도.. 소소한 논란은 눈감아주지만 ..",
          "original_form": "저도.. 소소한 논란은 눈감아주지만 ..",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035633.1.13",
          "form": "저는 태도논란 이런건줄알구...",
          "original_form": "저는 태도논란 이런건줄알구...",
          "speaker_id": "2",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035633.1.14",
          "form": "학폭은.. 감아주지않습니다 ...",
          "original_form": "학폭은.. 감아주지않습니다 ...",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035633.1.15",
          "form": "태도논란은 사실 팬들 아니면 잘 모르는거같아요 ㅋㅋㅋ",
          "original_form": "태도논란은 사실 팬들 아니면 잘 모르는거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035633.1.16",
          "form": "학폭은 ... 안될말이죠",
          "original_form": "학폭은 ... 안될말이죠",
          "speaker_id": "2",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035633.1.17",
          "form": "맞아요 ^^ 전 그래서 제와피 그룹들 안좋아한답니다",
          "original_form": "맞아요 ^^ 전 그래서 제와피 그룹들 안좋아한답니다",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100035633.1.18",
          "form": "제와피 디게 애들 인성",
          "original_form": "제와피 디게 애들 인성",
          "speaker_id": "2",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100035633.1.19",
          "form": "신경쓰는 척 하더니",
          "original_form": "신경쓰는 척 하더니",
          "speaker_id": "2",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100035633.1.20",
          "form": "어우 맞아요ㅋㅋ",
          "original_form": "어우 맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100035633.1.21",
          "form": "맨날 인성교육 어쩌구저쩌구",
          "original_form": "맨날 인성교육 어쩌구저쩌구",
          "speaker_id": "1",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100035633.1.22",
          "form": "저 이번에 그넘보고 실망 씨게 했어요",
          "original_form": "저 이번에 그넘보고 실망 씨게 했어요",
