      "utterance": [
        {
          "id": "MDRW2100026793.1.1",
          "form": "맞아요. 연기에 구멍인 사람이 없었어요",
          "original_form": "맞아요. 연기에 구멍인 사람이 없었어요",
          "speaker_id": "2",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026793.1.2",
          "form": "비숲2는 재미없다고해서 안보긴햇지만 ㅜ",
          "original_form": "비숲2는 재미없다고해서 안보긴햇지만 ㅜ",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026793.1.3",
          "form": "허억!!! 전... 2 괜찮게 봣어요",
          "original_form": "허억!!! 전... 2 괜찮게 봣어요",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026793.1.4",
          "form": "나름.. 나름 정교한데 ㅠ.^",
          "original_form": "나름.. 나름 정교한데 ㅠ.^",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026793.1.5",
          "form": "다들 안 좋아하더라고요",
          "original_form": "다들 안 좋아하더라고요",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026793.1.6",
          "form": "아 괜찮았나요?ㅎㅎ",
          "original_form": "아 괜찮았나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026793.1.7",
          "form": "네!!!!! 나름 큰그림을 그리던데요!!",
          "original_form": "네!!!!! 나름 큰그림을 그리던데요!!",
          "speaker_id": "1",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026793.1.8",
          "form": "전 전 추천해요!",
          "original_form": "전 전 추천해요!",
          "speaker_id": "1",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026793.1.9",
          "form": "봐야겠어요ㅎㅎ 넷플에 있었던거 같아요",
          "original_form": "봐야겠어요ㅎㅎ 넷플에 있었던거 같아요",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026793.1.10",
          "form": "네네!!!!!!!!!!!! 2 보려는 사람 많이 없어서 아쉬웠는데",
          "original_form": "네네!!!!!!!!!!!! 2 보려는 사람 많이 없어서 아쉬웠는데",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026793.1.11",
          "form": "name1이 봐주시고 저랑 말해주시면 되겠네요 ㅎ.ㅎ !",
          "original_form": "&name1&이 봐주시고 저랑 말해주시면 되겠네요 ㅎ.ㅎ !",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026793.1.12",
          "form": "네! 제가 정주행해볼게요ㅎㅎㅈ",
          "original_form": "네! 제가 정주행해볼게요ㅎㅎㅈ",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026793.1.13",
          "form": "혹시 시그널 보셨어요?",
          "original_form": "혹시 시그널 보셨어요?",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026793.1.14",
          "form": "헛 ㅠ. 아니요 제가 새드엔딩 완전 싫어하는데",
          "original_form": "헛 ㅠ. 아니요 제가 새드엔딩 완전 싫어하는데",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026793.1.15",
          "form": "그렇다는 소문을 들어서 아직 안 봤어요..",
          "original_form": "그렇다는 소문을 들어서 아직 안 봤어요..",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026793.1.16",
          "form": "새드엔딩까진 아니에요ㅎㅎ첨엔 이게 뭔가했는데 넘 재밌게 봤어요",
          "original_form": "새드엔딩까진 아니에요ㅎㅎ첨엔 이게 뭔가했는데 넘 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026793.1.17",
          "form": "김은희 작가 진짜 대단",
          "original_form": "김은희 작가 진짜 대단",
          "speaker_id": "2",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026793.1.18",
          "form": "헉 그럼 저도 한 번 봐야겠군요!!!",
          "original_form": "헉 그럼 저도 한 번 봐야겠군요!!!",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026793.1.19",
          "form": "제가 보보경삼 본 이후로는 절대 새드 안 보거든요",
          "original_form": "제가 보보경삼 본 이후로는 절대 새드 안 보거든요",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026793.1.20",
          "form": "3일내내 울어서..",
          "original_form": "3일내내 울어서..",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100026793.1.21",
          "form": "아이고ㅜㅜ 감정이입을 엄청 하시나봐요ㅜㅜ",
          "original_form": "아이고ㅜㅜ 감정이입을 엄청 하시나봐요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100026793.1.22",
          "form": "김은희 작가 좋아하세요?",
          "original_form": "김은희 작가 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100026793.1.23",
          "form": "네!!! 저 킹덤",
          "original_form": "네!!! 저 킹덤",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026793.1.24",
          "form": "엄청 재밌게 봤어요!!",
          "original_form": "엄청 재밌게 봤어요!!",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026793.1.25",
          "form": "역시 킹덤!",
          "original_form": "역시 킹덤!",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026793.1.26",
          "form": "저도 킹덤 목빠져라 다음시즌 기다리고 있어요",
          "original_form": "저도 킹덤 목빠져라 다음시즌 기다리고 있어요",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026793.1.27",
          "form": "아 그니까요!!!!! 아신전으로는 만족이 안 돼요!",
