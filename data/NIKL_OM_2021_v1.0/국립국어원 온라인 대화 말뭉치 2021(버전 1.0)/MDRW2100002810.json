      "utterance": [
        {
          "id": "MDRW2100002810.1.1",
          "form": "세상을 바꾼 변호인 너무 감동적이야.",
          "original_form": "세상을 바꾼 변호인 너무 감동적이야.",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002810.1.2",
          "form": "미국의 최초 여성 대법관의 이야기란다.",
          "original_form": "미국의 최초 여성 대법관의 이야기란다.",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002810.1.3",
          "form": "너도 본 적 있니?",
          "original_form": "너도 본 적 있니?",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002810.1.4",
          "form": "영화 말하는거지?",
          "original_form": "영화 말하는거지?",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002810.1.5",
          "form": "나도 본적있어",
          "original_form": "나도 본적있어",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002810.1.6",
          "form": "정말 대단한 분이시더라",
          "original_form": "정말 대단한 분이시더라",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002810.1.7",
          "form": "삶의 동기가 생겼어",
          "original_form": "삶의 동기가 생겼어",
          "speaker_id": "1",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002810.1.8",
          "form": "주인공도 멋있고 남편도 멋있더라",
          "original_form": "주인공도 멋있고 남편도 멋있더라",
          "speaker_id": "2",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002810.1.9",
          "form": "주인공의 꿈을 계속 지지하는 남자라 멋있는 거 같아.",
          "original_form": "주인공의 꿈을 계속 지지하는 남자라 멋있는 거 같아.",
          "speaker_id": "2",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002810.1.10",
          "form": "물론 얼굴도 잘생겼어",
          "original_form": "물론 얼굴도 잘생겼어",
          "speaker_id": "2",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002810.1.11",
          "form": "사실 잘 기억안나 외모는.",
          "original_form": "사실 잘 기억안나 외모는.",
          "speaker_id": "2",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002810.1.12",
          "form": "너는 어떤 영화릉 보았니?",
          "original_form": "너는 어떤 영화릉 보았니?",
          "speaker_id": "2",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002810.1.13",
          "form": "맞아 남자주인공의 외모는 영화의 중요한 요소야",
          "original_form": "맞아 남자주인공의 외모는 영화의 중요한 요소야",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002810.1.14",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002810.1.15",
          "form": "몰입에 중요한 역할을 해",
          "original_form": "몰입에 중요한 역할을 해",
          "speaker_id": "2",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002810.1.16",
          "form": "그런의미에서 남주가 잘생긴 영화를 좋아해",
          "original_form": "그런의미에서 남주가 잘생긴 영화를 좋아해",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002810.1.17",
          "form": "나는 최근에 싱크홀을 봤어",
          "original_form": "나는 최근에 싱크홀을 봤어",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002810.1.18",
          "form": "근데 영화가 너무 지루하더라",
          "original_form": "근데 영화가 너무 지루하더라",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002810.1.19",
          "form": "cg가 좀 게임같은 느낌이렀어",
          "original_form": "cg가 좀 게임같은 느낌이렀어",
          "speaker_id": "1",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002810.1.20",
          "form": "씽크홀에 빠지는거니?",
          "original_form": "씽크홀에 빠지는거니?",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002810.1.21",
          "form": "재목이 직관적이구나.",
          "original_form": "재목이 직관적이구나.",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002810.1.22",
          "form": "응 맞아",
          "original_form": "응 맞아",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002810.1.23",
          "form": "돈모아서 산 아파트가 일주일만에",
          "original_form": "돈모아서 산 아파트가 일주일만에",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002810.1.24",
          "form": "싱크홀로 떨어져",
          "original_form": "싱크홀로 떨어져",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002810.1.25",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002810.1.26",
          "form": "나락으로 가는구나",
          "original_form": "나락으로 가는구나",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002810.1.27",
          "form": "별점을 매긴다면 몇 점을 줄거니?",
          "original_form": "별점을 매긴다면 몇 점을 줄거니?",
          "speaker_id": "2",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002810.1.28",
          "form": "음 5점만점에 1점을 줬어",
          "original_form": "음 5점만점에 1점을 줬어",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002810.1.29",
          "form": "오 최악이었구나",
          "original_form": "오 최악이었구나",
          "speaker_id": "2",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002810.1.30",
          "form": "너는 안보는걸 추천할께!!",
          "original_form": "너는 안보는걸 추천할께!!",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002810.1.31",
          "form": "리뷰 잘 참고할게",
          "original_form": "리뷰 잘 참고할게",
          "speaker_id": "2",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002810.1.32",
          "form": "고마워!",
          "original_form": "고마워!",
          "speaker_id": "2",
