      "utterance": [
        {
          "id": "MDRW2100019561.1.1",
          "form": "요즘 읽고 있는 것이 있나요?",
          "original_form": "요즘 읽고 있는 것이 있나요?",
          "speaker_id": "1",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019561.1.2",
          "form": "저 불안한 사람들 이북은 받아놓고 아직 안읽고 있어요ㅋㅋㅋ",
          "original_form": "저 불안한 사람들 이북은 받아놓고 아직 안읽고 있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019561.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019561.1.4",
          "form": "저도 오랜만에 도서관 가서 책 대출했어요",
          "original_form": "저도 오랜만에 도서관 가서 책 대출했어요",
          "speaker_id": "1",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019561.1.5",
          "form": "오 뭐 빌리셨어요?",
          "original_form": "오 뭐 빌리셨어요?",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019561.1.6",
          "form": "귀 건강 제로웨이스트 그림심리 책 빌렸어요",
          "original_form": "귀 건강 제로웨이스트 그림심리 책 빌렸어요",
          "speaker_id": "1",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019561.1.7",
          "form": "사람들이 책 자주 대출하는지 없는 책도 많더라고요",
          "original_form": "사람들이 책 자주 대출하는지 없는 책도 많더라고요",
          "speaker_id": "1",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019561.1.8",
          "form": "와 사람들이 책을 많이 읽네요",
          "original_form": "와 사람들이 책을 많이 읽네요",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019561.1.9",
          "form": "제로웨이스트 책 저도 궁금해요ㅎㅎㅎ",
          "original_form": "제로웨이스트 책 저도 궁금해요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019561.1.10",
          "form": "오늘을 조금 바꿉니다 라는 제목이에요",
          "original_form": "오늘을 조금 바꿉니다 라는 제목이에요",
          "speaker_id": "1",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100019561.1.11",
          "form": "이북 있으면 한번 사봐야겠어요",
          "original_form": "이북 있으면 한번 사봐야겠어요",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100019561.1.12",
          "form": "쓰레기 줄인다고 줄이는데 잘 안되네요 ㅠㅠ",
          "original_form": "쓰레기 줄인다고 줄이는데 잘 안되네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019561.1.13",
          "form": "맞아요 뭐 하나만 사도 쓰레기가 많이 나오죠 기업이 잘 해야는데...",
          "original_form": "맞아요 뭐 하나만 사도 쓰레기가 많이 나오죠 기업이 잘 해야는데...",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019561.1.14",
          "form": "책 이북으로 자주 보세요?",
          "original_form": "책 이북으로 자주 보세요?",
          "speaker_id": "1",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019561.1.15",
          "form": "네 전 거의 이북으로 봐요",
          "original_form": "네 전 거의 이북으로 봐요",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019561.1.16",
          "form": "누워서 보는게 익숙해져서 책은 무겁더라구요...^^",
          "original_form": "누워서 보는게 익숙해져서 책은 무겁더라구요...^^",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019561.1.17",
          "form": "불끄고 볼수있단 장점도 있어요..",
          "original_form": "불끄고 볼수있단 장점도 있어요..",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019561.1.18",
          "form": "두꺼운 책 볼 때 좋겠네요 이동할 때도 좋고요",
          "original_form": "두꺼운 책 볼 때 좋겠네요 이동할 때도 좋고요",
          "speaker_id": "1",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019561.1.19",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019561.1.20",
          "form": "저번에 백래시 책 이북으로 읽는데 읽어도 읽어도 안끝나서 보니까 종이책은 어엄청 두껍더라구요",
          "original_form": "저번에 백래시 책 이북으로 읽는데 읽어도 읽어도 안끝나서 보니까 종이책은 어엄청 두껍더라구요",
          "speaker_id": "2",
          "time": "20210827 20:56"
        },
        {
          "id": "MDRW2100019561.1.21",
          "form": "종이책으로 봤으면 아마 쉽사리 도전 못했을거예요ㅋㅋㅋ",
          "original_form": "종이책으로 봤으면 아마 쉽사리 도전 못했을거예요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:56"
        },
        {
          "id": "MDRW2100019561.1.22",
          "form": "그런 장점도 있네요ㅎㅎ",
          "original_form": "그런 장점도 있네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019561.1.23",
          "form": "이제 가을이니 책도 열심히 읽어야 겠어요",
          "original_form": "이제 가을이니 책도 열심히 읽어야 겠어요",
          "speaker_id": "1",
          "time": "20210827 20:58"
        }
      ]
