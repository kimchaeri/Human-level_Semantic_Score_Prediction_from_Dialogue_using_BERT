      "utterance": [
        {
          "id": "MDRW2100040322.1.1",
          "form": "name2님은 커피 좋아하세요?^^",
          "original_form": "&name2&님은 커피 좋아하세요?^^",
          "speaker_id": "1",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100040322.1.2",
          "form": "저 완전 좋아하죠",
          "original_form": "저 완전 좋아하죠",
          "speaker_id": "2",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100040322.1.3",
          "form": "카페에서 일도 했었어요!",
          "original_form": "카페에서 일도 했었어요!",
          "speaker_id": "2",
          "time": "20210904 16:12"
        },
        {
          "id": "MDRW2100040322.1.4",
          "form": "와 그렇군요",
          "original_form": "와 그렇군요",
          "speaker_id": "1",
          "time": "20210904 16:15"
        },
        {
          "id": "MDRW2100040322.1.5",
          "form": "저는 커피 향이 좋아서 좋아해요",
          "original_form": "저는 커피 향이 좋아서 좋아해요",
          "speaker_id": "1",
          "time": "20210904 16:15"
        },
        {
          "id": "MDRW2100040322.1.6",
          "form": "특히 좋아하는 커피종류 있나요?",
          "original_form": "특히 좋아하는 커피종류 있나요?",
          "speaker_id": "1",
          "time": "20210904 16:15"
        },
        {
          "id": "MDRW2100040322.1.7",
          "form": "저 아이스바닐라라떼요ㅎㅎ애 입맛이죠",
          "original_form": "저 아이스바닐라라떼요ㅎㅎ애 입맛이죠",
          "speaker_id": "2",
          "time": "20210904 16:16"
        },
        {
          "id": "MDRW2100040322.1.8",
          "form": "향 좋아하시는 분들은",
          "original_form": "향 좋아하시는 분들은",
          "speaker_id": "2",
          "time": "20210904 16:16"
        },
        {
          "id": "MDRW2100040322.1.9",
          "form": "드립커피 많이드시던데!!",
          "original_form": "드립커피 많이드시던데!!",
          "speaker_id": "2",
          "time": "20210904 16:16"
        },
        {
          "id": "MDRW2100040322.1.10",
          "form": "네.. 저 드립커피 좋아해요",
          "original_form": "네.. 저 드립커피 좋아해요",
          "speaker_id": "1",
          "time": "20210904 16:17"
        },
        {
          "id": "MDRW2100040322.1.11",
          "form": "다만 안타까운건 제가 커피를 마시면 잠을 못자서 즐겨 마시진 못하고 있어요",
          "original_form": "다만 안타까운건 제가 커피를 마시면 잠을 못자서 즐겨 마시진 못하고 있어요",
          "speaker_id": "1",
          "time": "20210904 16:18"
        },
        {
          "id": "MDRW2100040322.1.12",
          "form": "아 저도 카페인에 약해요",
          "original_form": "아 저도 카페인에 약해요",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040322.1.13",
          "form": "드립이 카페인이 더 강하죠 ㅠ 조심해서 마시시길...",
          "original_form": "드립이 카페인이 더 강하죠 ㅠ 조심해서 마시시길...",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040322.1.14",
          "form": "그래서 그런가..",
          "original_form": "그래서 그런가..",
          "speaker_id": "1",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100040322.1.15",
          "form": "향이 좋아서 원두로 마시고 있는건데",
          "original_form": "향이 좋아서 원두로 마시고 있는건데",
          "speaker_id": "1",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100040322.1.16",
          "form": "집에서 아침 일찍 한잔 정도 마시고 있어요.. 어쩌다가 한번씩",
          "original_form": "집에서 아침 일찍 한잔 정도 마시고 있어요.. 어쩌다가 한번씩",
          "speaker_id": "1",
          "time": "20210904 20:28"
        },
        {
          "id": "MDRW2100040322.1.17",
          "form": "ㅠㅠ자제력이 좋으시네요",
          "original_form": "ㅠㅠ자제력이 좋으시네요",
          "speaker_id": "2",
          "time": "20210904 20:28"
        },
        {
          "id": "MDRW2100040322.1.18",
          "form": "어떤 원두 쓰세요?",
          "original_form": "어떤 원두 쓰세요?",
          "speaker_id": "2",
          "time": "20210904 20:28"
        },
        {
          "id": "MDRW2100040322.1.19",
          "form": "저는 선물받은거나 딸이 사온 걸로 해서 잘 모르겠네요",
          "original_form": "저는 선물받은거나 딸이 사온 걸로 해서 잘 모르겠네요",
          "speaker_id": "1",
          "time": "20210904 21:08"
        },
        {
          "id": "MDRW2100040322.1.20",
          "form": "아 ㅎㅎ 따님이 있으셨군요!!",
          "original_form": "아 ㅎㅎ 따님이 있으셨군요!!",
          "speaker_id": "2",
          "time": "20210904 21:26"
        },
        {
          "id": "MDRW2100040322.1.21",
          "form": "저도 아빠한테 가끔 스벅이나 그런 원두 사드려야겠어용",
          "original_form": "저도 아빠한테 가끔 스벅이나 그런 원두 사드려야겠어용",
          "speaker_id": "2",
          "time": "20210904 21:26"
        },
        {
          "id": "MDRW2100040322.1.22",
          "form": "그러면 아빠가 아주 좋아하실 거예요 :)",
          "original_form": "그러면 아빠가 아주 좋아하실 거예요 :)",
          "speaker_id": "1",
          "time": "20210904 21:29"
        },
        {
          "id": "MDRW2100040322.1.23",
          "form": "ㅎㅎ커피말구",
          "original_form": "ㅎㅎ커피말구",
          "speaker_id": "2",
          "time": "20210904 21:32"
        },
        {
          "id": "MDRW2100040322.1.24",
          "form": "디저트좋아하는거있으세요?",
          "original_form": "디저트좋아하는거있으세요?",
          "speaker_id": "2",
          "time": "20210904 21:32"
        },
        {
          "id": "MDRW2100040322.1.25",
          "form": "빵종류 좋아해요",
          "original_form": "빵종류 좋아해요",
          "speaker_id": "1",
          "time": "20210904 21:37"
        },
        {
          "id": "MDRW2100040322.1.26",
          "form": "케이크나 쿠키 같은 것보다 빵 종류가 낫더라구요 디저트로",
          "original_form": "케이크나 쿠키 같은 것보다 빵 종류가 낫더라구요 디저트로",
          "speaker_id": "1",
          "time": "20210904 21:37"
        },
        {
          "id": "MDRW2100040322.1.27",
          "form": "오 그쵸ㅋㅋㅋ막 호밀빵 이런거",
          "original_form": "오 그쵸ㅋㅋㅋ막 호밀빵 이런거",
          "speaker_id": "2",
          "time": "20210904 21:37"
        }
      ]
    }
