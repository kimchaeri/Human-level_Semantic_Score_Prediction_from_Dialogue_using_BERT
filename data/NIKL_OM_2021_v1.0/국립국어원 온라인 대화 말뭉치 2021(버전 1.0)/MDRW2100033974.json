        },
        {
          "id": "MDRW2100033974.1.2",
          "form": "엉 하이",
          "original_form": "엉 하이",
          "speaker_id": "2",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100033974.1.3",
          "form": "다음에 만나면 뭐 먹을까",
          "original_form": "다음에 만나면 뭐 먹을까",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100033974.1.4",
          "form": "마라탕 말고 다른 거 먹을까?",
          "original_form": "마라탕 말고 다른 거 먹을까?",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100033974.1.5",
          "form": "뭐먹지",
          "original_form": "뭐먹지",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100033974.1.6",
          "form": "닭갈비 어때?",
          "original_form": "닭갈비 어때?",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100033974.1.7",
          "form": "오 안 먹은지 꽤 오래됐네",
          "original_form": "오 안 먹은지 꽤 오래됐네",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100033974.1.8",
          "form": "맞아 요즘 우리 마라탕만 먹어서ㅋㅋㅋㅋ",
          "original_form": "맞아 요즘 우리 마라탕만 먹어서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100033974.1.9",
          "form": "하지만 너무 맛있는 걸~",
          "original_form": "하지만 너무 맛있는 걸~",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100033974.1.10",
          "form": "맞지맞지",
          "original_form": "맞지맞지",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100033974.1.11",
          "form": "닭갈비에 떡이랑 우동사리~",
          "original_form": "닭갈비에 떡이랑 우동사리~",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100033974.1.12",
          "form": "너무 좋아 우동사리",
          "original_form": "너무 좋아 우동사리",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100033974.1.13",
          "form": "혁명이야",
          "original_form": "혁명이야",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100033974.1.14",
          "form": "너무 맛있어ㅠㅠ",
          "original_form": "너무 맛있어ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100033974.1.15",
          "form": "그리고 볶음밥",
          "original_form": "그리고 볶음밥",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100033974.1.16",
          "form": "치즈 뿌리면 더 맛있음",
          "original_form": "치즈 뿌리면 더 맛있음",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100033974.1.17",
          "form": "아 먹고 싶다..",
