      "utterance": [
        {
          "id": "MDRW2100045493.1.1",
          "form": "오 참가 해보고 싶어요",
          "original_form": "오 참가 해보고 싶어요",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045493.1.2",
          "form": "아직 일주일도 안 살아보신 듯",
          "original_form": "아직 일주일도 안 살아보신 듯",
          "speaker_id": "1",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045493.1.3",
          "form": "그러게요 한 달은 사신줄",
          "original_form": "그러게요 한 달은 사신줄",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045493.1.4",
          "form": "오래 살기를 안 하시지",
          "original_form": "오래 살기를 안 하시지",
          "speaker_id": "2",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045493.1.5",
          "form": "그것도 재미있겠는데요",
          "original_form": "그것도 재미있겠는데요",
          "speaker_id": "1",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045493.1.6",
          "form": "운전만 계속하시는",
          "original_form": "운전만 계속하시는",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045493.1.7",
          "form": "어디 정착하지",
          "original_form": "어디 정착하지",
          "speaker_id": "1",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045493.1.8",
          "form": "내일 휴 방 하신데요",
          "original_form": "내일 휴 방 하신데요",
          "speaker_id": "2",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100045493.1.9",
          "form": "내일은 다른 분 꺼봐야겠어요",
          "original_form": "내일은 다른 분 꺼봐야겠어요",
          "speaker_id": "1",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100045493.1.10",
          "form": "체인점 등장",
          "original_form": "체인점 등장",
          "speaker_id": "2",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100045493.1.11",
          "form": "오늘 기차도 있나 봐요",
          "original_form": "오늘 기차도 있나 봐요",
          "speaker_id": "1",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045493.1.12",
          "form": "저도 첨본 ㄷㄷ",
          "original_form": "저도 첨본 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045493.1.13",
          "form": "게임이 진짜 디테일해요",
          "original_form": "게임이 진짜 디테일해요",
          "speaker_id": "1",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045493.1.14",
          "form": "진짜 꼼꼼하다는 요",
          "original_form": "진짜 꼼꼼하다는 요",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045493.1.15",
          "form": "도전이 좋으시다 네요",
          "original_form": "도전이 좋으시다 네요",
