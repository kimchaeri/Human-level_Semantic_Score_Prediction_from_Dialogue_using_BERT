      "utterance": [
        {
          "id": "MDRW2100018409.1.1",
          "form": "미용관심있으신가요?!",
          "original_form": "미용관심있으신가요?!",
          "speaker_id": "2",
          "time": "20210827 01:24"
        },
        {
          "id": "MDRW2100018409.1.2",
          "form": "요즘 관리를 안했더니 피부가 난리나서ㅋㅋㅋㅠ",
          "original_form": "요즘 관리를 안했더니 피부가 난리나서ㅋㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210827 01:27"
        },
        {
          "id": "MDRW2100018409.1.3",
          "form": "이제 관심을 가져보려구요",
          "original_form": "이제 관심을 가져보려구요",
          "speaker_id": "1",
          "time": "20210827 01:27"
        },
        {
          "id": "MDRW2100018409.1.4",
          "form": "폼클렌징 추천해주실 수 있나요",
          "original_form": "폼클렌징 추천해주실 수 있나요",
          "speaker_id": "1",
          "time": "20210827 01:28"
        },
        {
          "id": "MDRW2100018409.1.5",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210827 01:28"
        },
        {
          "id": "MDRW2100018409.1.6",
          "form": "선크림정도만 바르시나요??",
          "original_form": "선크림정도만 바르시나요??",
          "speaker_id": "2",
          "time": "20210827 01:31"
        },
        {
          "id": "MDRW2100018409.1.7",
          "form": "네 요즘엔 더워서 선크림만 바르게 되더라구요!",
          "original_form": "네 요즘엔 더워서 선크림만 바르게 되더라구요!",
          "speaker_id": "1",
          "time": "20210827 01:31"
        },
        {
          "id": "MDRW2100018409.1.8",
          "form": "마스크 쓰니까 입술도 안발라도 되고 ㅎㅎㅎ",
          "original_form": "마스크 쓰니까 입술도 안발라도 되고 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100018409.1.9",
          "form": "ㅋㅋㅋㅋ그래도 저는 화장이 취미라 눈화장 치크 꼬박꼬박해요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ그래도 저는 화장이 취미라 눈화장 치크 꼬박꼬박해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100018409.1.10",
          "form": "오 부러워요 저는 화장이 짙을 수록 더워보이는 타입...ㅠㅠㅠ",
          "original_form": "오 부러워요 저는 화장이 짙을 수록 더워보이는 타입...ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 01:35"
        },
        {
          "id": "MDRW2100018409.1.11",
          "form": "퍼스널컬러 어떤 거세요?",
          "original_form": "퍼스널컬러 어떤 거세요?",
          "speaker_id": "1",
          "time": "20210827 01:35"
        },
        {
          "id": "MDRW2100018409.1.12",
          "form": "퍼컬 안받아봤는데 확신의 웜톤이에요 ㅋㅋ",
          "original_form": "퍼컬 안받아봤는데 확신의 웜톤이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:36"
        },
        {
          "id": "MDRW2100018409.1.13",
          "form": "폼클 에뛰드거 몇년째 쓰는중이에요! ㅋㅋ",
          "original_form": "폼클 에뛰드거 몇년째 쓰는중이에요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:36"
        },
        {
          "id": "MDRW2100018409.1.14",
          "form": "엇 저는 확신의 겨울쿨..!ㅋㅋㅋ 반대네요",
          "original_form": "엇 저는 확신의 겨울쿨..!ㅋㅋㅋ 반대네요",
          "speaker_id": "1",
          "time": "20210827 01:37"
        },
        {
          "id": "MDRW2100018409.1.15",
          "form": "에뛰드 폼클 그 바다그려진 그거 맞나요",
          "original_form": "에뛰드 폼클 그 바다그려진 그거 맞나요",
          "speaker_id": "1",
          "time": "20210827 01:37"
        },
        {
          "id": "MDRW2100018409.1.16",
          "form": "본가가면 저도 그거 쓰는데!",
          "original_form": "본가가면 저도 그거 쓰는데!",
          "speaker_id": "1",
          "time": "20210827 01:37"
        },
        {
          "id": "MDRW2100018409.1.17",
          "form": "바다가 그려져있었나? 그 알갱이든거에요 비비폼클인가",
          "original_form": "바다가 그려져있었나? 그 알갱이든거에요 비비폼클인가",
          "speaker_id": "2",
          "time": "20210827 01:37"
        },
        {
          "id": "MDRW2100018409.1.18",
          "form": "알갱이 들었으면 그거맞는거 같아요",
          "original_form": "알갱이 들었으면 그거맞는거 같아요",
          "speaker_id": "1",
          "time": "20210827 01:39"
        },
        {
          "id": "MDRW2100018409.1.19",
          "form": "베이킹파우더 들어있는거에요 ㅋㅋ",
          "original_form": "베이킹파우더 들어있는거에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:39"
        },
        {
          "id": "MDRW2100018409.1.20",
          "form": "뽀득뽀득해요",
          "original_form": "뽀득뽀득해요",
          "speaker_id": "2",
          "time": "20210827 01:39"
        },
        {
          "id": "MDRW2100018409.1.21",
          "form": "싼맛에 샀는데 좋더라구요 ㅎㅎㅎ",
          "original_form": "싼맛에 샀는데 좋더라구요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 01:39"
        },
        {
          "id": "MDRW2100018409.1.22",
          "form": "파데는 뭐쓰세요?",
          "original_form": "파데는 뭐쓰세요?",
          "speaker_id": "1",
          "time": "20210827 01:40"
        },
        {
          "id": "MDRW2100018409.1.23",
          "form": "워터프루프 선제품도 잘지워지더라구요",
          "original_form": "워터프루프 선제품도 잘지워지더라구요",
          "speaker_id": "2",
          "time": "20210827 01:40"
        },
        {
          "id": "MDRW2100018409.1.24",
          "form": "지금 미샤 빨간비비랑 알엠케이 젤크리미 써요",
          "original_form": "지금 미샤 빨간비비랑 알엠케이 젤크리미 써요",
          "speaker_id": "2",
          "time": "20210827 01:40"
        },
        {
          "id": "MDRW2100018409.1.25",
          "form": "아하 미샤 빨간비비는 정말 굳건하네요",
          "original_form": "아하 미샤 빨간비비는 정말 굳건하네요",
          "speaker_id": "1",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018409.1.26",
          "form": "꾸준한 인기템!",
          "original_form": "꾸준한 인기템!",
          "speaker_id": "1",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018409.1.27",
          "form": "사실 사은품으로 받은건데... 돈주고살것같진않아요...",
          "original_form": "사실 사은품으로 받은건데... 돈주고살것같진않아요...",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018409.1.28",
          "form": "저런.....",
          "original_form": "저런.....",
          "speaker_id": "1",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018409.1.29",
          "form": "회색빛이 도나요...?",
          "original_form": "회색빛이 도나요...?",
          "speaker_id": "1",
          "time": "20210827 01:42"
        }
      ]
    }
