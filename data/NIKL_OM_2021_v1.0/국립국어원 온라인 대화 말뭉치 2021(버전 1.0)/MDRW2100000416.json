          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000416.1.11",
          "form": "꺅",
          "original_form": "꺅{emoji:♡}",
          "speaker_id": "2",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000416.1.12",
          "form": "오래 만나셨나요?",
          "original_form": "오래 만나셨나요?",
          "speaker_id": "2",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000416.1.13",
          "form": "1년정도됬어요~",
          "original_form": "1년정도됬어요~",
          "speaker_id": "1",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000416.1.14",
          "form": "오오~~ 넘 좋으시겠어요",
          "original_form": "오오~~ 넘 좋으시겠어요",
          "speaker_id": "2",
