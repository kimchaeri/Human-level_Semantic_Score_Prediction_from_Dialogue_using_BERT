        },
        {
          "id": "MDRW2100000416.1.15",
          "form": "",
          "original_form": "",
          "speaker_id": "2",
          "time": "20210819 16:03"
        },
        {
          "id": "MDRW2100000416.1.16",
          "form": "그렇죠 name2님은 결혼하신지 얼마나 되셨어요?",
          "original_form": "그렇죠 &name2&님은 결혼하신지 얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000416.1.17",
          "form": "저는 2016년에 결혼했어요!!",
          "original_form": "저는 2016년에 결혼했어요!!",
          "speaker_id": "2",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000416.1.18",
          "form": "아 벌써 좀 됐네요~ ㅋㅋㅋ",
          "original_form": "아 벌써 좀 됐네요~ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000416.1.19",
          "form": "오~ 어떻게 만나시게 되셨어요? 전 지인소개데요ㅎ",
          "original_form": "오~ 어떻게 만나시게 되셨어요? 전 지인소개데요ㅎ",
          "speaker_id": "1",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000416.1.20",
          "form": "저도 지인 소개팅이요~",
          "original_form": "저도 지인 소개팅이요~",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000416.1.21",
          "form": "한 2년반 만나고 결혼했어요~",
          "original_form": "한 2년반 만나고 결혼했어요~",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000416.1.22",
          "form": "아,그렇궇요~ 요즘 코로나라 할게 너무 없더라고요",
          "original_form": "아,그렇궇요~ 요즘 코로나라 할게 너무 없더라고요",
          "speaker_id": "1",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000416.1.23",
          "form": "앗 진짜요?!?",
          "original_form": "앗 진짜요?!?",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000416.1.24",
          "form": "와 진짜 만나면 뭐하나요ㅜㅜ",
          "original_form": "와 진짜 만나면 뭐하나요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000416.1.25",
          "form": "원래 방콕좋아하는데 못나가니 괜히 더그래요",
          "original_form": "원래 방콕좋아하는데 못나가니 괜히 더그래요",
          "speaker_id": "1",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000416.1.26",
          "form": "아하하",
          "original_form": "아하하",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000416.1.27",
          "form": "집순이 친구들도",
          "original_form": "집순이 친구들도",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000416.1.28",
          "form": "나가고싶을 정도라고 하더라고요",
          "original_form": "나가고싶을 정도라고 하더라고요",
          "speaker_id": "2",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000416.1.29",
          "form": "네~ 주로 집이나 공원 사람없는곳? 가려고 하죠",
          "original_form": "네~ 주로 집이나 공원 사람없는곳? 가려고 하죠",
          "speaker_id": "1",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000416.1.30",
          "form": "결혼하시는건 어때요ㅋㅋㄲ",
          "original_form": "결혼하시는건 어때요ㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000416.1.31",
          "form": "저는 결혼하고 더 잘 놀았어요",
          "original_form": "저는 결혼하고 더 잘 놀았어요",
          "speaker_id": "2",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000416.1.32",
          "form": "서로 애기하고 1~2년후에 할 예정이에요~",
          "original_form": "서로 애기하고 1~2년후에 할 예정이에요~",
          "speaker_id": "1",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000416.1.33",
          "form": "부럽네요ㅎㅎ",
          "original_form": "부럽네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000416.1.34",
          "form": "오 딱 좋을것 같아요!!!",
          "original_form": "오 딱 좋을것 같아요!!!",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000416.1.35",
          "form": "신혼을 꼭 가지세요!!!",
          "original_form": "신혼을 꼭 가지세요!!!",
          "speaker_id": "2",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000416.1.36",
          "form": "네, 신혼은 길게보내고싶네요~",
          "original_form": "네, 신혼은 길게보내고싶네요~",
          "speaker_id": "1",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000416.1.37",
          "form": "둘이 맨날 놀러 다니고 먹느라 살도 많이찌고ㅋㅋ",
          "original_form": "둘이 맨날 놀러 다니고 먹느라 살도 많이찌고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000416.1.38",
          "form": "추억도 많이 만들게 되더라고요",
          "original_form": "추억도 많이 만들게 되더라고요",
          "speaker_id": "2",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000416.1.39",
          "form": "맞아요 딱 저런생각했거든요~",
          "original_form": "맞아요 딱 저런생각했거든요~",
          "speaker_id": "1",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000416.1.40",
          "form": "강추합니다!!",
          "original_form": "강추합니다!!",
          "speaker_id": "2",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000416.1.41",
          "form": "제 친구들은 대부분 초반에 좋다가 지나면 하지말라고 하더라고요ㅎ",
          "original_form": "제 친구들은 대부분 초반에 좋다가 지나면 하지말라고 하더라고요ㅎ",
          "speaker_id": "1",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000416.1.42",
          "form": "악ㅋㅋㅋ  결혼을요???",
          "original_form": "악ㅋㅋㅋ  결혼을요???",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000416.1.43",
          "form": "네, 신혼때는 좋아하더니 좀 지나고 애기낳고 반이상은 하지말라고 애기하더라고요~",
          "original_form": "네, 신혼때는 좋아하더니 좀 지나고 애기낳고 반이상은 하지말라고 애기하더라고요~",
          "speaker_id": "1",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000416.1.44",
          "form": "아 진짜요??",
          "original_form": "아 진짜요??",
          "speaker_id": "2",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000416.1.45",
          "form": "저는 아기는 꼭!!!!",
          "original_form": "저는 아기는 꼭!!!!",
          "speaker_id": "2",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000416.1.46",
          "form": "아기 진짜 너무너무너무 예뻐요ㅜㅜ",
          "original_form": "아기 진짜 너무너무너무 예뻐요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000416.1.47",
          "form": "한 친구는 쌍둥이데 귀엽더라고요 (남아,여아)",
          "original_form": "한 친구는 쌍둥이데 귀엽더라고요 (남아,여아)",
          "speaker_id": "1",
          "time": "20210819 16:39"
        },
        {
          "id": "MDRW2100000416.1.48",
          "form": "아기들 진짜 너무 예뻐요",
          "original_form": "아기들 진짜 너무 예뻐요",
          "speaker_id": "2",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000416.1.49",
          "form": "진짜 너무 행복해요",
          "original_form": "진짜 너무 행복해요",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000416.1.50",
          "form": "단전에서 부터 행복이",
          "original_form": "단전에서 부터 행복이",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000416.1.51",
          "form": "아 근데 피곤해요 만성피로ㅜ",
          "original_form": "아 근데 피곤해요 만성피로ㅜ",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000416.1.52",
          "form": "그쵸 육아는 끝도 없으니깐요~ㅎ",
          "original_form": "그쵸 육아는 끝도 없으니깐요~ㅎ",
          "speaker_id": "1",
          "time": "20210819 16:44"
        },
        {
          "id": "MDRW2100000416.1.53",
          "form": "네 피곤함과 행복을 바꾼 느낌ㅜㅜ",
          "original_form": "네 피곤함과 행복을 바꾼 느낌ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 16:50"
        },
        {
          "id": "MDRW2100000416.1.54",
          "form": "아ㅠ  역시 그렇죠   친구는 주로 독박육아라 더 힘들어하던데ㅠ",
          "original_form": "아ㅠ  역시 그렇죠   친구는 주로 독박육아라 더 힘들어하던데ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:53"
        },
        {
          "id": "MDRW2100000416.1.55",
          "form": "독박 오우 노우~~~~",
          "original_form": "독박 오우 노우~~~~",
          "speaker_id": "2",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000416.1.56",
          "form": "너무 바쁜 남친은 아니죠???ㅜㅜ",
          "original_form": "너무 바쁜 남친은 아니죠???ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000416.1.57",
          "form": "그렇죠 그거때문에 자주 싸운다고 하더라고요",
          "original_form": "그렇죠 그거때문에 자주 싸운다고 하더라고요",
          "speaker_id": "1",
          "time": "20210819 17:03"
        },
        {
          "id": "MDRW2100000416.1.58",
          "form": "역할만 잘 맡으면 돼용!!",
          "original_form": "역할만 잘 맡으면 돼용!!",
          "speaker_id": "2",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000416.1.59",
          "form": "저는 제가 게으른쪽이라ㅜㅜㅋㅋㅋ",
          "original_form": "저는 제가 게으른쪽이라ㅜㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000416.1.60",
          "form": "그쵸, 서로 맞춰가면 되겠죠~",
          "original_form": "그쵸, 서로 맞춰가면 되겠죠~",
          "speaker_id": "1",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000416.1.61",
          "form": "아니면ㅋㅋㅋ애를 둘 낳으면 돼요",
          "original_form": "아니면ㅋㅋㅋ애를 둘 낳으면 돼요",
          "speaker_id": "2",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000416.1.62",
          "form": "각자 하나씩 맡으면 평화가 와요",
          "original_form": "각자 하나씩 맡으면 평화가 와요",
          "speaker_id": "2",
          "time": "20210819 17:47"
        },
