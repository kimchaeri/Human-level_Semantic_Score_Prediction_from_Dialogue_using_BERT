      "utterance": [
        {
          "id": "MDRW2100008739.1.1",
          "form": "게임 그냥 심심할때 간간히해요",
          "original_form": "게임 그냥 심심할때 간간히해요",
          "speaker_id": "1",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008739.1.2",
          "form": "퍼즐같은거",
          "original_form": "퍼즐같은거",
          "speaker_id": "1",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008739.1.3",
          "form": "아 저도 타일게임같은거 하곤 해요ㅎㅎ",
          "original_form": "아 저도 타일게임같은거 하곤 해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008739.1.4",
          "form": "타일게임은 어떤거예요?",
          "original_form": "타일게임은 어떤거예요?",
          "speaker_id": "1",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008739.1.5",
          "form": "심슨 타일이라구 짝맞추기같은 게임이에요ㅎㅎ",
          "original_form": "심슨 타일이라구 짝맞추기같은 게임이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008739.1.6",
          "form": "아 그런거 저도 좋아해요",
          "original_form": "아 그런거 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008739.1.7",
          "form": "단순게임",
          "original_form": "단순게임",
          "speaker_id": "1",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008739.1.8",
          "form": "카카오프렌즈 애니팡 이런거ㅋ",
          "original_form": "카카오프렌즈 애니팡 이런거ㅋ",
          "speaker_id": "1",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008739.1.9",
          "form": "네 시간이 아주 잘가요ㅎㅎ",
          "original_form": "네 시간이 아주 잘가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008739.1.10",
          "form": "애니팡 좋아했는데ㅋㅋ",
          "original_form": "애니팡 좋아했는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008739.1.11",
          "form": "저도요 중독됐었죠ㅎ",
          "original_form": "저도요 중독됐었죠ㅎ",
          "speaker_id": "1",
          "time": "20210822 17:58"
        },
        {
          "id": "MDRW2100008739.1.12",
          "form": "단순하다보니 끝을 모르겠더라구요ㅎㅎㅎ",
          "original_form": "단순하다보니 끝을 모르겠더라구요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:58"
        },
        {
          "id": "MDRW2100008739.1.13",
          "form": "시간도 잘가고 좋아요",
          "original_form": "시간도 잘가고 좋아요",
          "speaker_id": "1",
          "time": "20210822 17:58"
        },
        {
          "id": "MDRW2100008739.1.14",
          "form": "맞아요 의미없는 시간 보낼때 딱인 느낌ㅎㅎ",
          "original_form": "맞아요 의미없는 시간 보낼때 딱인 느낌ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:59"
        },
        {
          "id": "MDRW2100008739.1.15",
          "form": "요즘 복잡한겜이 넘많더라구요",
          "original_form": "요즘 복잡한겜이 넘많더라구요",
          "speaker_id": "1",
          "time": "20210822 18:00"
        },
        {
          "id": "MDRW2100008739.1.16",
          "form": "쉬려고 하는 게임인데 이것저것 신경써야하면 힘들죠ㅠㅠ",
          "original_form": "쉬려고 하는 게임인데 이것저것 신경써야하면 힘들죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 18:00"
        },
