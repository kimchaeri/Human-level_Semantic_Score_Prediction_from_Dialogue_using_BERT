      "utterance": [
        {
          "id": "MDRW2100026766.1.1",
          "form": "최근에 경주 갔었어요 ㅎㅎ",
          "original_form": "최근에 경주 갔었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026766.1.2",
          "form": "해바라기 보러 ㅎㅎ",
          "original_form": "해바라기 보러 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026766.1.3",
          "form": "오호 그러셨군요!! 경주에 해바라기가 유명한가요?",
          "original_form": "오호 그러셨군요!! 경주에 해바라기가 유명한가요?",
          "speaker_id": "2",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026766.1.4",
          "form": "유명한지는 모르겠어요",
          "original_form": "유명한지는 모르겠어요",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026766.1.5",
          "form": "카페가 유명한곳이 있더라구요",
          "original_form": "카페가 유명한곳이 있더라구요",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026766.1.6",
          "form": "해바라기가 있는",
          "original_form": "해바라기가 있는",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026766.1.7",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026766.1.8",
          "form": "아하 드렇군요! 신기하네용ㅋㅋㅋㅋ",
          "original_form": "아하 드렇군요! 신기하네용ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026766.1.9",
          "form": "부산에서 경주까지는 얼마나 걸리나요?",
          "original_form": "부산에서 경주까지는 얼마나 걸리나요?",
          "speaker_id": "2",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026766.1.10",
          "form": "시외버스 기준으로 한시간?",
          "original_form": "시외버스 기준으로 한시간?",
          "speaker_id": "1",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026766.1.11",
          "form": "아니여고 운전해서 가도 거의 1시간이면 가요",
          "original_form": "아니여고 운전해서 가도 거의 1시간이면 가요",
          "speaker_id": "1",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026766.1.12",
          "form": "아하 그렇구나 가깝군요! 경주는 자주 가시나요*",
          "original_form": "아하 그렇구나 가깝군요! 경주는 자주 가시나요*",
          "speaker_id": "2",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100026766.1.13",
          "form": "자주가진않아요",
          "original_form": "자주가진않아요",
          "speaker_id": "1",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100026766.1.14",
          "form": "그렇군요! 저는 경주는 수학여행으로만 한번 가봤어요!",
          "original_form": "그렇군요! 저는 경주는 수학여행으로만 한번 가봤어요!",
          "speaker_id": "2",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100026766.1.15",
          "form": "아하 그렇군요 ㅎㅎ 수학여행으로 경주 가셨군요 ㅎㅎ",
          "original_form": "아하 그렇군요 ㅎㅎ 수학여행으로 경주 가셨군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100026766.1.16",
          "form": "자주가는 여행지는 어디세요?",
          "original_form": "자주가는 여행지는 어디세요?",
          "speaker_id": "1",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100026766.1.17",
          "form": "네 많이 즐기진 못했지만 나름 재미있었어요ㅎㅎ",
          "original_form": "네 많이 즐기진 못했지만 나름 재미있었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100026766.1.18",
          "form": "저는 집은 전라도 쪽이라서 광주나 전주 많이가요!",
          "original_form": "저는 집은 전라도 쪽이라서 광주나 전주 많이가요!",
          "speaker_id": "2",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100026766.1.19",
          "form": "전주 너무 좋아요!@",
          "original_form": "전주 너무 좋아요!@",
          "speaker_id": "1",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100026766.1.20",
          "form": "너무 좋아서 3번이나갔었는데",
          "original_form": "너무 좋아서 3번이나갔었는데",
          "speaker_id": "1",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100026766.1.21",
          "form": "점점 상업화되서 아쉽더라구요 ㅠ",
          "original_form": "점점 상업화되서 아쉽더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100026766.1.22",
          "form": "아하 그러셨군요! 하늘이님운 어디를 자주 가셨나요?",
          "original_form": "아하 그러셨군요! 하늘이님운 어디를 자주 가셨나요?",
          "speaker_id": "2",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100026766.1.23",
          "form": "한옥마을말고 전주에 갈대가있나요?",
          "original_form": "한옥마을말고 전주에 갈대가있나요?",
          "speaker_id": "1",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100026766.1.24",
          "form": "신시가지 쪽이나 대학교 근처에는 가볍게ㅜ놀데는 많은거같긴해요!",
          "original_form": "신시가지 쪽이나 대학교 근처에는 가볍게ㅜ놀데는 많은거같긴해요!",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100026766.1.25",
          "form": "툭별하게 여행갈만한곳은 없는거같긴하네용..",
          "original_form": "툭별하게 여행갈만한곳은 없는거같긴하네용..",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
