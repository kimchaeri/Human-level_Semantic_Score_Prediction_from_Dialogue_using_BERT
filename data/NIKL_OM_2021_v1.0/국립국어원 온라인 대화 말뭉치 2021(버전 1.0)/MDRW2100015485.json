        },
        {
          "id": "MDRW2100015485.1.2",
          "form": "오랑우탄",
          "original_form": "오랑우탄",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015485.1.3",
          "form": "티비에서 봤는데 귀엽드랑~~",
          "original_form": "티비에서 봤는데 귀엽드랑~~",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015485.1.4",
          "form": "아 나도 티비에서 그 오랑우탄 봤는데ㅋㅋ",
          "original_form": "아 나도 티비에서 그 오랑우탄 봤는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015485.1.5",
          "form": "아 우리 보리 보고싶다 ㅠㅠ",
          "original_form": "아 우리 보리 보고싶다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015485.1.6",
          "form": "우리 통했다",
          "original_form": "우리 통했다",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015485.1.7",
          "form": "나도 보고싶다 ㅠ",
          "original_form": "나도 보고싶다 ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015485.1.8",
          "form": "보리 잘 지내겠지...",
          "original_form": "보리 잘 지내겠지...",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015485.1.9",
          "form": "보리 완전 귀여웠는데",
          "original_form": "보리 완전 귀여웠는데",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015485.1.10",
          "form": "잘 지낼거야..!",
          "original_form": "잘 지낼거야..!",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015485.1.11",
          "form": "막 애교부리고 노래부르고ㅠㅠ",
          "original_form": "막 애교부리고 노래부르고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015485.1.12",
          "form": "우리 알아보고ㅋㅋ",
          "original_form": "우리 알아보고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015485.1.13",
          "form": "응 마자 보리같은 동물 또 키우고 싶네",
          "original_form": "응 마자 보리같은 동물 또 키우고 싶네",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015485.1.14",
          "form": "근데 우리 햄스터도 귀여워",
          "original_form": "근데 우리 햄스터도 귀여워",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015485.1.15",
          "form": "볼빵빵! 너무 귀엽징",
          "original_form": "볼빵빵! 너무 귀엽징",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
