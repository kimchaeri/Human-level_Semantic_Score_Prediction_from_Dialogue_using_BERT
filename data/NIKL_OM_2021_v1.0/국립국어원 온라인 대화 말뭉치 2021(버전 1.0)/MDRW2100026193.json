      "utterance": [
        {
          "id": "MDRW2100026193.1.1",
          "form": "저도 우당도서관엘 가봐야겠네요",
          "original_form": "저도 우당도서관엘 가봐야겠네요",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100026193.1.2",
          "form": "아뇨 그냥 추리물 대표작가라 말씀드린거고요",
          "original_form": "아뇨 그냥 추리물 대표작가라 말씀드린거고요",
          "speaker_id": "1",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100026193.1.3",
          "form": "저는 위화라는 작가 좋아해요",
          "original_form": "저는 위화라는 작가 좋아해요",
          "speaker_id": "1",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100026193.1.4",
          "form": "아 중국인인가요?",
          "original_form": "아 중국인인가요?",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100026193.1.5",
          "form": "성석제님도 좋아하고",
          "original_form": "성석제님도 좋아하고",
          "speaker_id": "1",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100026193.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100026193.1.7",
          "form": "저는 초면이에욥",
          "original_form": "저는 초면이에욥",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100026193.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100026193.1.9",
          "form": "허심관매혈기같은",
          "original_form": "허심관매혈기같은",
          "speaker_id": "1",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100026193.1.10",
          "form": "유명한 소설 쓰신분인데 아직 살아계세요",
          "original_form": "유명한 소설 쓰신분인데 아직 살아계세요",
          "speaker_id": "1",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100026193.1.11",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100026193.1.12",
          "form": "요즘분이시군요",
          "original_form": "요즘분이시군요",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100026193.1.13",
          "form": "약간 풍자 위트 이런거 좋아해요",
          "original_form": "약간 풍자 위트 이런거 좋아해요",
          "speaker_id": "1",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100026193.1.14",
          "form": "조정래 님처럼",
          "original_form": "조정래 님처럼",
          "speaker_id": "2",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100026193.1.15",
          "form": "모파상 단편선",
          "original_form": "모파상 단편선",
          "speaker_id": "1",
          "time": "20210901 20:44"
