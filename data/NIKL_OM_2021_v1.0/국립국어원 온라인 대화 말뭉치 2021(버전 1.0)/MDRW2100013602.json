      "utterance": [
        {
          "id": "MDRW2100013602.1.1",
          "form": "최근에 구매하신  상품이 있으신가요",
          "original_form": "최근에 구매하신  상품이 있으신가요",
          "speaker_id": "1",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100013602.1.2",
          "form": "최근에 썬크림 샀어요 ㅋㅋ 요즘 햇빛이 강하다보니",
          "original_form": "최근에 썬크림 샀어요 ㅋㅋ 요즘 햇빛이 강하다보니",
          "speaker_id": "2",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013602.1.3",
          "form": "name1님은 최근에 뭐 사신 거 있으세요?",
          "original_form": "&name1&님은 최근에 뭐 사신 거 있으세요?",
          "speaker_id": "2",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013602.1.4",
          "form": "저는 최근에 샴푸샀어요",
          "original_form": "저는 최근에 샴푸샀어요",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013602.1.5",
          "form": "샴푸가 떨어져서 ㅎ",
          "original_form": "샴푸가 떨어져서 ㅎ",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013602.1.6",
          "form": "무슨 샴푸 사셨어용?",
          "original_form": "무슨 샴푸 사셨어용?",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013602.1.7",
          "form": "그냥 평범한 엘라스틴 구매했어요ㅎ",
          "original_form": "그냥 평범한 엘라스틴 구매했어요ㅎ",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013602.1.8",
          "form": "엘라스틴 좋져 저두 그거 써용",
          "original_form": "엘라스틴 좋져 저두 그거 써용",
          "speaker_id": "2",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013602.1.9",
          "form": "요즘 그런 것들은 인터넷에서 진짜 싸게 팔드라구용",
          "original_form": "요즘 그런 것들은 인터넷에서 진짜 싸게 팔드라구용",
          "speaker_id": "2",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013602.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013602.1.11",
          "form": "인터넷으로 사면 확실히 싼 느낌",
          "original_form": "인터넷으로 사면 확실히 싼 느낌",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013602.1.12",
          "form": "썬크림은 어디 브랜드꺼 구매하셨나요",
          "original_form": "썬크림은 어디 브랜드꺼 구매하셨나요",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013602.1.13",
          "form": "에스쁘아라는 브랜드인데",
          "original_form": "에스쁘아라는 브랜드인데",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013602.1.14",
          "form": "혹시 아시나요 ㅋㅋㅋ",
          "original_form": "혹시 아시나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013602.1.15",
          "form": "네 알아요",
          "original_form": "네 알아요",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013602.1.16",
          "form": "예전에 조이씨가 모델했었던",
          "original_form": "예전에 조이씨가 모델했었던",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013602.1.17",
          "form": "거기 썬크림이 좋은가요",
          "original_form": "거기 썬크림이 좋은가요",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013602.1.18",
          "form": "헉 조이씨가 모델 했었었나요",
          "original_form": "헉 조이씨가 모델 했었었나요",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013602.1.19",
          "form": "엄마가 잘쓰셔서 하나 더 사드렸어요 ㅋㅋㅋ",
          "original_form": "엄마가 잘쓰셔서 하나 더 사드렸어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013602.1.20",
          "form": "어..지나가다 본 거 같아서..",
          "original_form": "어..지나가다 본 거 같아서..",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013602.1.21",
          "form": "아 어머니 사드리셨구나",
          "original_form": "아 어머니 사드리셨구나",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013602.1.22",
          "form": "좋아하셨겠어요",
          "original_form": "좋아하셨겠어요",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013602.1.23",
          "form": "사실 반강제였답니다.. ^^~",
          "original_form": "사실 반강제였답니다.. ^^~",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013602.1.24",
          "form": "인터넷 세일 기다렸다가 저렴한 가격에 사드렸어요 ㅋㅋㅋ",
          "original_form": "인터넷 세일 기다렸다가 저렴한 가격에 사드렸어요 ㅋㅋㅋ",
          "speaker_id": "2",
