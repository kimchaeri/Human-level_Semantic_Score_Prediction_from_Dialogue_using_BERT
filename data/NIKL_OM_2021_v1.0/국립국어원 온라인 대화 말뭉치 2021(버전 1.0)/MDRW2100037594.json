      "utterance": [
        {
          "id": "MDRW2100037594.1.1",
          "form": "근데 우리 첫째도 좀 그렇긴 하더라",
          "original_form": "근데 우리 첫째도 좀 그렇긴 하더라",
          "speaker_id": "1",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100037594.1.2",
          "form": "아고 근데 시간이 조금 필요하긴 하겠다. ㅠㅠ",
          "original_form": "아고 근데 시간이 조금 필요하긴 하겠다. ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100037594.1.3",
          "form": "어떻게??",
          "original_form": "어떻게??",
          "speaker_id": "2",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100037594.1.4",
          "form": "마음 상하지 ㅠ",
          "original_form": "마음 상하지 ㅠ",
          "speaker_id": "1",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100037594.1.5",
          "form": "엉 잠시 쉬어가는시간..",
          "original_form": "엉 잠시 쉬어가는시간..",
          "speaker_id": "2",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100037594.1.6",
          "form": "친구관계에 계속 개입하니 피곤하다",
          "original_form": "친구관계에 계속 개입하니 피곤하다",
          "speaker_id": "2",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100037594.1.7",
          "form": "아 다른 애들이랑 있을때 더 혼내는거",
          "original_form": "아 다른 애들이랑 있을때 더 혼내는거",
          "speaker_id": "1",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100037594.1.8",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "1",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100037594.1.9",
          "form": "부모가 같이 해결해야되는",
          "original_form": "부모가 같이 해결해야되는",
          "speaker_id": "2",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100037594.1.10",
          "form": "그래 니도 그렇지",
          "original_form": "그래 니도 그렇지",
          "speaker_id": "2",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100037594.1.11",
          "form": "친구들끼리 싸우면은 부모가 나서면 안되는데",
          "original_form": "친구들끼리 싸우면은 부모가 나서면 안되는데",
          "speaker_id": "1",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100037594.1.12",
          "form": "나서게 되서",
          "original_form": "나서게 되서",
          "speaker_id": "1",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100037594.1.13",
          "form": "우리 또 버릇없는 거는 못보잖아",
          "original_form": "우리 또 버릇없는 거는 못보잖아",
          "speaker_id": "2",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100037594.1.14",
          "form": "나도 한번씩은 감정이 앞설때가 있지.",
          "original_form": "나도 한번씩은 감정이 앞설때가 있지.",
          "speaker_id": "1",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100037594.1.15",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "1",
          "time": "20210901 12:52"
        },
        {
          "id": "MDRW2100037594.1.16",
          "form": "어 이게 애랑 기싸움을 하게되니까",
          "original_form": "어 이게 애랑 기싸움을 하게되니까",
          "speaker_id": "2",
          "time": "20210901 12:52"
        },
        {
          "id": "MDRW2100037594.1.17",
          "form": "그렇지.",
          "original_form": "그렇지.",
          "speaker_id": "1",
          "time": "20210901 12:52"
        },
        {
          "id": "MDRW2100037594.1.18",
          "form": "근데 이제ㅣ애들이 안지려고 하잖아",
          "original_form": "근데 이제ㅣ애들이 안지려고 하잖아",
          "speaker_id": "2",
          "time": "20210901 12:52"
        },
        {
          "id": "MDRW2100037594.1.19",
          "form": "맞어 맞어",
          "original_form": "맞어 맞어",
          "speaker_id": "1",
          "time": "20210901 12:52"
        },
        {
          "id": "MDRW2100037594.1.20",
          "form": "전에 니랑 언니왔을때",
          "original_form": "전에 니랑 언니왔을때",
          "speaker_id": "2",
          "time": "20210901 12:52"
        },
        {
          "id": "MDRW2100037594.1.21",
          "form": "진짜 엄청싸운다",
          "original_form": "진짜 엄청싸운다",
          "speaker_id": "1",
          "time": "20210901 12:52"
        },
        {
          "id": "MDRW2100037594.1.22",
          "form": "효자손 그때도 그렇고",
          "original_form": "효자손 그때도 그렇고",
          "speaker_id": "2",
          "time": "20210901 12:52"
        },
        {
          "id": "MDRW2100037594.1.23",
          "form": "내가 그냥 한번 물러서주면 되는데",
          "original_form": "내가 그냥 한번 물러서주면 되는데",
          "speaker_id": "2",
          "time": "20210901 12:52"
        },
        {
          "id": "MDRW2100037594.1.24",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210901 12:52"
        },
        {
          "id": "MDRW2100037594.1.25",
          "form": "나도 그게 안되니 계속 신경전을",
          "original_form": "나도 그게 안되니 계속 신경전을",
          "speaker_id": "2",
          "time": "20210901 12:52"
        },
        {
          "id": "MDRW2100037594.1.26",
