      "utterance": [
        {
          "id": "MDRW2100015631.1.1",
          "form": "재택근무 중이신가요?",
          "original_form": "재택근무 중이신가요?",
          "speaker_id": "1",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015631.1.2",
          "form": "아뇨 재택근무세요???",
          "original_form": "아뇨 재택근무세요???",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015631.1.3",
          "form": "전 지금은 아니에요 ㅜㅜ 전에 해본 적은 있어요.",
          "original_form": "전 지금은 아니에요 ㅜㅜ 전에 해본 적은 있어요.",
          "speaker_id": "1",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015631.1.4",
          "form": "저희 회사는 어림도 없어요 ㅋㅋ",
          "original_form": "저희 회사는 어림도 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015631.1.5",
          "form": "절대안하더라고요",
          "original_form": "절대안하더라고요",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015631.1.6",
          "form": "와.. 단 한번도요?",
          "original_form": "와.. 단 한번도요?",
          "speaker_id": "1",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015631.1.7",
          "form": "재택근무 해보니까 좋은 점도 있는데 단점도 있더라구요 ㅜㅜ",
          "original_form": "재택근무 해보니까 좋은 점도 있는데 단점도 있더라구요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015631.1.8",
          "form": "그냥  오라는데? 사실 너무 소규모라 ㅎ",
          "original_form": "그냥  오라는데? 사실 너무 소규모라 ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015631.1.9",
          "form": "오 단점은 뭐에요?",
          "original_form": "오 단점은 뭐에요?",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015631.1.10",
          "form": "거의 작업물을 회사 컴퓨터에 저장하다보니까",
          "original_form": "거의 작업물을 회사 컴퓨터에 저장하다보니까",
          "speaker_id": "1",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015631.1.11",
          "form": "필요한 프로그램이나 파일이 없어요....",
          "original_form": "필요한 프로그램이나 파일이 없어요....",
          "speaker_id": "1",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015631.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015631.1.13",
          "form": "그러겠네요",
          "original_form": "그러겠네요",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015631.1.14",
          "form": "그리고 사내메신저 같은 게 없어서 소통이 안돼요..",
          "original_form": "그리고 사내메신저 같은 게 없어서 소통이 안돼요..",
          "speaker_id": "1",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015631.1.15",
          "form": "아 불편하겠다",
          "original_form": "아 불편하겠다",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015631.1.16",
          "form": "장점은 뭐에요 그러면?",
          "original_form": "장점은 뭐에요 그러면?",
          "speaker_id": "2",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015631.1.17",
          "form": "아침에 늦잠 잘 수 있다!",
          "original_form": "아침에 늦잠 잘 수 있다!",
          "speaker_id": "1",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015631.1.18",
          "form": "역시 이시국에 안가는거? ㅠㅠ부럽",
          "original_form": "역시 이시국에 안가는거? ㅠㅠ부럽",
          "speaker_id": "2",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015631.1.19",
          "form": "ㅋㅋ맞아요",
          "original_form": "ㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015631.1.20",
          "form": "그리고 눈치 안보면서 앞에 간식 차려놓고 먹기",
          "original_form": "그리고 눈치 안보면서 앞에 간식 차려놓고 먹기",
          "speaker_id": "1",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015631.1.21",
          "form": "저 치킨 먹으면서 한 적도 있었어요 ㅋㅋㅋ",
          "original_form": "저 치킨 먹으면서 한 적도 있었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015631.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015631.1.23",
          "form": "너무 좋다",
          "original_form": "너무 좋다",
          "speaker_id": "2",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015631.1.24",
          "form": "그리고 피씨카톡 켜놓기~!",
          "original_form": "그리고 피씨카톡 켜놓기~!",
          "speaker_id": "1",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015631.1.25",
          "form": "월루하는건 어때요?",
          "original_form": "월루하는건 어때요?",
          "speaker_id": "2",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015631.1.26",
          "form": "그쵸 그건 국롤이죠",
          "original_form": "그쵸 그건 국롤이죠",
          "speaker_id": "2",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015631.1.27",
          "form": "월루 좋죠 진짜",
          "original_form": "월루 좋죠 진짜",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015631.1.28",
          "form": "피씨카톡은 재택근무 아니라도 겨놔요",
          "original_form": "피씨카톡은 재택근무 아니라도 겨놔요",
          "speaker_id": "2",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015631.1.29",
          "form": "한 때는 월루였답니다..",
          "original_form": "한 때는 월루였답니다..",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015631.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015631.1.31",
          "form": "회사는 좀 조심해야돼요",
          "original_form": "회사는 좀 조심해야돼요",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015631.1.32",
          "form": "그건 그래요 ㅋㅋ",
          "original_form": "그건 그래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015631.1.33",
          "form": "잠깐 자리 비우고 다녀왔더니 제 자리에 누가 컴퓨터를 ...",
          "original_form": "잠깐 자리 비우고 다녀왔더니 제 자리에 누가 컴퓨터를 ...",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015631.1.34",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015631.1.35",
          "form": "손댔나요?",
          "original_form": "손댔나요?",
          "speaker_id": "2",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015631.1.36",
          "form": "진짜 당황스럽더라구요 친구들하고 별 얘기 다 했는데",
          "original_form": "진짜 당황스럽더라구요 친구들하고 별 얘기 다 했는데",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015631.1.37",
          "form": "으악",
          "original_form": "으악",
          "speaker_id": "2",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015631.1.38",
          "form": "네네 무슨 보안패치인지 뭔지 점검한다면서...",
          "original_form": "네네 무슨 보안패치인지 뭔지 점검한다면서...",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015631.1.39",
          "form": "하.........",
          "original_form": "하.........",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015631.1.40",
          "form": "헐......",
          "original_form": "헐......",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015631.1.41",
          "form": "지금은 다시 회사 가고있나요?",
          "original_form": "지금은 다시 회사 가고있나요?",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015631.1.42",
          "form": "네네 재택근무 시켜줄거면 좀",
          "original_form": "네네 재택근무 시켜줄거면 좀",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015631.1.43",
          "form": "피씨카톡조심해야겠네요",
          "original_form": "피씨카톡조심해야겠네요",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015631.1.44",
          "form": "길게 한 달 이렇게 했음 좋겠어요",
          "original_form": "길게 한 달 이렇게 했음 좋겠어요",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015631.1.45",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015631.1.46",
          "form": "일주일에 하루~이틀씩 돌아가면서 하니까 더 힘들어요",
          "original_form": "일주일에 하루~이틀씩 돌아가면서 하니까 더 힘들어요",
          "speaker_id": "1",
          "time": "20210825 23:57"
        }
      ]
