      "utterance": [
        {
          "id": "MDRW2100021220.1.1",
          "form": "왜 잘 몰라",
          "original_form": "왜 잘 몰라",
          "speaker_id": "2",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021220.1.2",
          "form": "맘터 방금도 먹어놓고!",
          "original_form": "맘터 방금도 먹어놓고!",
          "speaker_id": "2",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021220.1.3",
          "form": "배달음식 주로 머 먹어 너는",
          "original_form": "배달음식 주로 머 먹어 너는",
          "speaker_id": "2",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021220.1.4",
          "form": "그건 그런데",
          "original_form": "그건 그런데",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021220.1.5",
          "form": "피자? 연어초밥?",
          "original_form": "피자? 연어초밥?",
          "speaker_id": "2",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021220.1.6",
          "form": "그냥 끌리는 걸로 먹어서 메뉴 이름을 못위움",
          "original_form": "그냥 끌리는 걸로 먹어서 메뉴 이름을 못위움",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021220.1.7",
          "form": "나 육회 등갈비 카페 와플 탕후루 이런 좀 특이한거",
          "original_form": "나 육회 등갈비 카페 와플 탕후루 이런 좀 특이한거",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021220.1.8",
          "form": "초밥이나 피자 치킨은 아주가끔씩",
          "original_form": "초밥이나 피자 치킨은 아주가끔씩",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021220.1.9",
          "form": "ㅠㅠ나도 육회 먹고싶어",
          "original_form": "ㅠㅠ나도 육회 먹고싶어",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021220.1.10",
          "form": "난 너가 초밥이랑 피자가 주류 인줄 알았어",
          "original_form": "난 너가 초밥이랑 피자가 주류 인줄 알았어",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021220.1.11",
          "form": "육회여름에조심해야된데 자기야",
          "original_form": "육회여름에조심해야된데 자기야",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021220.1.12",
          "form": "아직..너네대해서 잘 모르는구나...",
          "original_form": "아직..너네대해서 잘 모르는구나...",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021220.1.13",
          "form": "하... 우린 결혼할 사이인데...",
          "original_form": "하... 우린 결혼할 사이인데...",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021220.1.14",
          "form": "나에대해아는것이없구나",
          "original_form": "나에대해아는것이없구나",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021220.1.15",
          "form": "ㅋㅋㅋ와플은 어디꺼? 나 와플대학 함 먹어보고싶은데",
          "original_form": "ㅋㅋㅋ와플은 어디꺼? 나 와플대학 함 먹어보고싶은데",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021220.1.16",
          "form": "ㅋㅋ이럴때하는말은겹치니까괜찮아",
          "original_form": "ㅋㅋ이럴때하는말은겹치니까괜찮아{emoji:♡}",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.17",
          "form": "대구엔 매장이 별로 없더라",
          "original_form": "대구엔 매장이 별로 없더라",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.18",
          "form": "와플은 걍 개인카페",
          "original_form": "와플은 걍 개인카페",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.19",
          "form": "나 와플기계삼",
          "original_form": "나 와플기계삼",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.20",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.21",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.22",
          "form": "정확히는우리엄마가",
          "original_form": "정확히는우리엄마가",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.23",
          "form": "좋겠다",
          "original_form": "좋겠다",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.24",
          "form": "그걸로 뭐 해먹어봤어",
          "original_form": "그걸로 뭐 해먹어봤어",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.25",
          "form": "유튜브보고",
          "original_form": "유튜브보고",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.26",
          "form": "와플",
          "original_form": "와플",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.27",
          "form": "별별거 다 만들수있던데",
          "original_form": "별별거 다 만들수있던데",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.28",
          "form": "오오ㅎㅎㅎ",
          "original_form": "오오ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.29",
          "form": "ㅋㅋㅋㅋ아 떡도누르고 그런식으로?",
          "original_form": "ㅋㅋㅋㅋ아 떡도누르고 그런식으로?",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.30",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.31",
          "form": "감튀 이런것도 누르고",
          "original_form": "감튀 이런것도 누르고",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.32",
          "form": "맛있어보이더라",
          "original_form": "맛있어보이더라",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.33",
          "form": "근데우리 산지 1달넘엇는데",
          "original_form": "근데우리 산지 1달넘엇는데",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.34",
          "form": "잘 안쓰게돼??",
          "original_form": "잘 안쓰게돼??",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.35",
          "form": "아직 와플 1번밖에안먹엇어",
          "original_form": "아직 와플 1번밖에안먹엇어",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.36",
          "form": "헉 왜",
          "original_form": "헉 왜",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021220.1.37",
          "form": "자주 해먹어보지",
          "original_form": "자주 해먹어보지",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.38",
          "form": "엄마가와플을좋아해서 메이플시럽까지삿는데",
          "original_form": "엄마가와플을좋아해서 메이플시럽까지삿는데",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.39",
          "form": "너 베이킹은 잘 하면서",
          "original_form": "너 베이킹은 잘 하면서",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.40",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.41",
          "form": "엄마담당이라엄마가안해",
          "original_form": "엄마담당이라엄마가안해",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.42",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.43",
          "form": "난 오븐담당",
          "original_form": "난 오븐담당",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.44",
          "form": "너가 좀 해도 되자나!!",
          "original_form": "너가 좀 해도 되자나!!",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.45",
          "form": "아 각자 전문분야가 있는거야?",
          "original_form": "아 각자 전문분야가 있는거야?",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.46",
          "form": "너 근데 카페 알바도 해본적잇자나",
          "original_form": "너 근데 카페 알바도 해본적잇자나",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.47",
          "form": "나가 해",
          "original_form": "나가 해",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.48",
          "form": "너가 햐",
          "original_form": "너가 햐",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.49",
          "form": "그건아니구 나는 휘낭시에굽기도바쁜거지^^",
          "original_form": "그건아니구 나는 휘낭시에굽기도바쁜거지^^",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.50",
          "form": "너가 해",
          "original_form": "너가 해",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.51",
          "form": "하...",
          "original_form": "하...",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.52",
          "form": "ㅎㅎ휘낭시에도 하고 와플도 해",
          "original_form": "ㅎㅎ휘낭시에도 하고 와플도 해",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.53",
          "form": "와플그냥 돼잇는반죽사야겟다",
          "original_form": "와플그냥 돼잇는반죽사야겟다",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.54",
          "form": "그리고 아아도 만들고",
          "original_form": "그리고 아아도 만들고",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.55",
          "form": "카페라떼도 만들고",
          "original_form": "카페라떼도 만들고",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021220.1.56",
          "form": "프라페도 만들고...",
          "original_form": "프라페도 만들고...",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021220.1.57",
          "form": "너가 해",
          "original_form": "너가 해",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021220.1.58",
          "form": "아니이거여혐이야",
          "original_form": "아니이거여혐이야",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021220.1.59",
          "form": "아 맞아 근데 와플 생지 이런거 사서 구우면",
          "original_form": "아 맞아 근데 와플 생지 이런거 사서 구우면",
