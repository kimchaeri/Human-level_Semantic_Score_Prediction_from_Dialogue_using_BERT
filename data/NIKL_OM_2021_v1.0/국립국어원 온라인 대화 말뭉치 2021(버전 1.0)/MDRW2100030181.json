      "utterance": [
        {
          "id": "MDRW2100030181.1.1",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.2",
          "form": "알지",
          "original_form": "알지",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.3",
          "form": "당연히알겠지",
          "original_form": "당연히알겠지",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.4",
          "form": "모를수도 잇지",
          "original_form": "모를수도 잇지",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.5",
          "form": "아니 민트초코 아이스크림 뿐만이아니라",
          "original_form": "아니 민트초코 아이스크림 뿐만이아니라",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.6",
          "form": "민트초코를 누가 먹어",
          "original_form": "민트초코를 누가 먹어",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.7",
          "form": "오레오 같은 과자",
          "original_form": "오레오 같은 과자",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.8",
          "form": "프라페 그런거 있자나",
          "original_form": "프라페 그런거 있자나",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.9",
          "form": "오... 웅",
          "original_form": "오... 웅",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.10",
          "form": "난 카페 가면 민트초코 프라페만 먹어",
          "original_form": "난 카페 가면 민트초코 프라페만 먹어",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.11",
          "form": "진짜 맛있거든",
          "original_form": "진짜 맛있거든",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.12",
          "form": "진짜?",
          "original_form": "진짜?",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.13",
          "form": "베라 민초도 맛있지만",
          "original_form": "베라 민초도 맛있지만",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.14",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.15",
          "form": "그냥 초코프라페먹으면",
          "original_form": "그냥 초코프라페먹으면",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.16",
          "form": "안되는거야?",
          "original_form": "안되는거야?",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.17",
          "form": "그게 더 맛잇잖아",
          "original_form": "그게 더 맛잇잖아",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.18",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030181.1.19",
          "form": "일단 2개가 섞여있으니까",
          "original_form": "일단 2개가 섞여있으니까",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030181.1.20",
          "form": "좀 더 새로운 맛?",
          "original_form": "좀 더 새로운 맛?",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030181.1.21",
          "form": "가성비?",
          "original_form": "가성비?",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030181.1.22",
          "form": "초코는 흔한맛이지만",
          "original_form": "초코는 흔한맛이지만",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030181.1.23",
          "form": "민트초코는 흔하지않다구",
          "original_form": "민트초코는 흔하지않다구",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030181.1.24",
          "form": "말도 안돼..",
          "original_form": "말도 안돼..",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030181.1.25",
          "form": "난 민트초코보단",
          "original_form": "난 민트초코보단",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030181.1.26",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030181.1.27",
          "form": "녹차 + 초코가 낫다구 생걱해",
          "original_form": "녹차 + 초코가 낫다구 생걱해",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030181.1.28",
          "form": "흠.. 하지만 녹차초코는 그렇게 유명하지얺아",
          "original_form": "흠.. 하지만 녹차초코는 그렇게 유명하지얺아",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030181.1.29",
          "form": "하지만 그게 훨씬 조화로워",
          "original_form": "하지만 그게 훨씬 조화로워",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030181.1.30",
          "form": "그냥 따로노는 느낌?",
          "original_form": "그냥 따로노는 느낌?",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030181.1.31",
          "form": "차라리 따로먹는게 더 낫아",
          "original_form": "차라리 따로먹는게 더 낫아",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030181.1.32",
          "form": "맛잘알들은 다 녹차초코 먹는다구!",
          "original_form": "맛잘알들은 다 녹차초코 먹는다구!",
          "speaker_id": "2",
          "time": "20210903 22:49"
        }
      ]
    }
  ]
