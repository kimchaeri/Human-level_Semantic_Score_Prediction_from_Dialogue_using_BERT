      "utterance": [
        {
          "id": "MDRW2100037628.1.1",
          "form": "저요 핸드믹서기 샀네요",
          "original_form": "저요 핸드믹서기 샀네요",
          "speaker_id": "2",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100037628.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100037628.1.3",
          "form": "저는 전기모기채요 ㅋㅋ",
          "original_form": "저는 전기모기채요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037628.1.4",
          "form": "오 그거 좋죠 전 다이소서 하나 샀는데",
          "original_form": "오 그거 좋죠 전 다이소서 하나 샀는데",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037628.1.5",
          "form": "돈값해요",
          "original_form": "돈값해요",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037628.1.6",
          "form": "앜ㅋㅋ빨리 고장났나요",
          "original_form": "앜ㅋㅋ빨리 고장났나요",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037628.1.7",
          "form": "아뇨 오천원 이상의 일을 해서 기특해요 ㅋㅋ",
          "original_form": "아뇨 오천원 이상의 일을 해서 기특해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037628.1.8",
          "form": "아 그런가요 ㅋㅋㅋ",
          "original_form": "아 그런가요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037628.1.9",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037628.1.10",
          "form": "다이소 최고",
          "original_form": "다이소 최고",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037628.1.11",
          "form": "네 ㅋㅋㅋ 다행이에요 내년까지 버티길",
          "original_form": "네 ㅋㅋㅋ 다행이에요 내년까지 버티길",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100037628.1.12",
          "form": "정말 다이소는 가성비짱",
          "original_form": "정말 다이소는 가성비짱",
          "speaker_id": "1",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100037628.1.13",
          "form": "네 편하고 가깝고",
          "original_form": "네 편하고 가깝고",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037628.1.14",
          "form": "어떤건 괘 비싼데 일단 가게가 가까우니",
          "original_form": "어떤건 괘 비싼데 일단 가게가 가까우니",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037628.1.15",
          "form": "웬만한건다있고",
          "original_form": "웬만한건다있고",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037628.1.16",
          "form": "그래서다이소인가봐요",
          "original_form": "그래서다이소인가봐요",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100037628.1.17",
          "form": "그런가봐욬ㅋㅋ",
          "original_form": "그런가봐욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100037628.1.18",
          "form": "최근에 홈카메라도 샀는데 좋아요",
          "original_form": "최근에 홈카메라도 샀는데 좋아요",
          "speaker_id": "1",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100037628.1.19",
          "form": "홈카메라가 뭐에요 진자 카메라에요?",
          "original_form": "홈카메라가 뭐에요 진자 카메라에요?",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100037628.1.20",
          "form": "홈카메라 샀어요",
          "original_form": "홈카메라 샀어요",
          "speaker_id": "1",
          "time": "20210901 17:37"
        }
      ]
    }
