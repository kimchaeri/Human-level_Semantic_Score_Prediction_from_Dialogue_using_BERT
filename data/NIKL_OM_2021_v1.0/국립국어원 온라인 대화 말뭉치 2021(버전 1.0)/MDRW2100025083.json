        {
          "id": "MDRW2100025083.1.1",
          "form": "스포츠 경기 좋아하세요?",
          "original_form": "스포츠 경기 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025083.1.2",
          "form": "하는 건 못하고 보는 건 좋아해요",
          "original_form": "하는 건 못하고 보는 건 좋아해요",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025083.1.3",
          "form": "무슨 스포츠 좋아해요!?",
          "original_form": "무슨 스포츠 좋아해요!?",
          "speaker_id": "2",
          "time": "20210901 09:19"
        },
        {
          "id": "MDRW2100025083.1.4",
          "form": "저는 야구 봐요ㅋㅋ",
          "original_form": "저는 야구 봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100025083.1.5",
          "form": "아~ 어느팀 응원하세요~~?",
          "original_form": "아~ 어느팀 응원하세요~~?",
          "speaker_id": "2",
          "time": "20210901 12:35"
        },
        {
          "id": "MDRW2100025083.1.6",
          "form": "두산베어스요. 야구 좋아하세요?",
          "original_form": "두산베어스요. 야구 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100025083.1.7",
          "form": "아하 두산 응원하시구낭 아뇨 저는 스포츠를 좋아하지는 않아요 ㅎ",
          "original_form": "아하 두산 응원하시구낭 아뇨 저는 스포츠를 좋아하지는 않아요 ㅎ",
          "speaker_id": "2",
          "time": "20210901 13:12"
        },
        {
          "id": "MDRW2100025083.1.8",
          "form": "야구장은 2번 가본것같아요",
          "original_form": "야구장은 2번 가본것같아요",
          "speaker_id": "2",
          "time": "20210901 13:13"
        },
        {
          "id": "MDRW2100025083.1.9",
          "form": "친구 따라서요?",
          "original_form": "친구 따라서요?",
          "speaker_id": "1",
          "time": "20210901 13:13"
        },
        {
          "id": "MDRW2100025083.1.10",
          "form": "네네 ㅎ 친구따라서요 약간 치킨먹으러 간 느낌",
          "original_form": "네네 ㅎ 친구따라서요 약간 치킨먹으러 간 느낌",
          "speaker_id": "2",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025083.1.11",
          "form": "ㅋㅋ 그거 좋죠. 잔디밭 보면서 응원가 들으면서",
          "original_form": "ㅋㅋ 그거 좋죠. 잔디밭 보면서 응원가 들으면서",
          "speaker_id": "1",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025083.1.12",
          "form": "맞아요ㅋㅋ재밌더라고요",
          "original_form": "맞아요ㅋㅋ재밌더라고요",
          "speaker_id": "2",
          "time": "20210901 13:16"
        },
        {
          "id": "MDRW2100025083.1.13",
          "form": "뭐 잘 알지는 못해도 같이 응원하고 하는게",
          "original_form": "뭐 잘 알지는 못해도 같이 응원하고 하는게",
          "speaker_id": "2",
          "time": "20210901 13:16"
        },
        {
          "id": "MDRW2100025083.1.14",
          "form": "경기보러 간건 야구랑 축구 한번 가본게 다네용",
          "original_form": "경기보러 간건 야구랑 축구 한번 가본게 다네용",
          "speaker_id": "2",
          "time": "20210901 13:17"
        },
        {
          "id": "MDRW2100025083.1.15",
          "form": "축구장은 어떠셨어요? 전 k리그 경기는 못 가봤어요",
          "original_form": "축구장은 어떠셨어요? 전 k리그 경기는 못 가봤어요",
          "speaker_id": "1",
          "time": "20210901 13:18"
        },
        {
          "id": "MDRW2100025083.1.16",
          "form": "한번 가봤는데 그날 사람도 별로없어서 그런가 재미없었어요 ㅎ",
          "original_form": "한번 가봤는데 그날 사람도 별로없어서 그런가 재미없었어요 ㅎ",
          "speaker_id": "2",
          "time": "20210901 13:21"
        },
        {
          "id": "MDRW2100025083.1.17",
          "form": "야구가 재밌었어요",
          "original_form": "야구가 재밌었어요",
          "speaker_id": "2",
          "time": "20210901 13:21"
        },
        {
          "id": "MDRW2100025083.1.18",
          "form": "야구장 응원가가 신나긴 하죠ㅎㅎ",
          "original_form": "야구장 응원가가 신나긴 하죠ㅎㅎ",
