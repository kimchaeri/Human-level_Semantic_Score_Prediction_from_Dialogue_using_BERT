      "utterance": [
        {
          "id": "MDRW2100024158.1.1",
          "form": "필라테스 가보니까",
          "original_form": "필라테스 가보니까",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100024158.1.2",
          "form": "한 다섯번째 방문까지는",
          "original_form": "한 다섯번째 방문까지는",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100024158.1.3",
          "form": "뉴비 팔찌를 끼고 수업들을 수 있게 해주더라구요...",
          "original_form": "뉴비 팔찌를 끼고 수업들을 수 있게 해주더라구요...",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100024158.1.4",
          "form": "글쿤요",
          "original_form": "글쿤요",
          "speaker_id": "1",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100024158.1.5",
          "form": "그래도 자비롭다고 해야하나..?",
          "original_form": "그래도 자비롭다고 해야하나..?",
          "speaker_id": "1",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100024158.1.6",
          "form": "그렇다고 봐야겠죠...?",
          "original_form": "그렇다고 봐야겠죠...?",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024158.1.7",
          "form": "근데 다른 분들 진짜 다 잘하시더라구요",
          "original_form": "근데 다른 분들 진짜 다 잘하시더라구요",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024158.1.8",
          "form": "어떻게 그 근력운동을 다 버티지?!",
          "original_form": "어떻게 그 근력운동을 다 버티지?!",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024158.1.9",
          "form": "그분들을 롤모델 삼아 열심히 운동해야죠ㅋㅋ",
          "original_form": "그분들을 롤모델 삼아 열심히 운동해야죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100024158.1.10",
          "form": "맞아요....",
          "original_form": "맞아요....",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024158.1.11",
          "form": "언니도 꼭 시간내서 운동 다녀보세요!!!",
          "original_form": "언니도 꼭 시간내서 운동 다녀보세요!!!",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024158.1.12",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "1",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024158.1.13",
          "form": "일단 암벽등반  할거니까요!",
          "original_form": "일단 암벽등반  할거니까요!",
          "speaker_id": "1",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024158.1.14",
          "form": "언니는 암벽등반도 잘하실거예요!!!",
          "original_form": "언니는 암벽등반도 잘하실거예요!!!",
          "speaker_id": "2",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100024158.1.15",
          "form": "암벽등반이 클라이밍이랑 같은거죠?!",
          "original_form": "암벽등반이 클라이밍이랑 같은거죠?!",
          "speaker_id": "2",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100024158.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210831 09:35"
        },
        {
          "id": "MDRW2100024158.1.17",
          "form": "같은거에요",
          "original_form": "같은거에요",
          "speaker_id": "1",
          "time": "20210831 09:35"
        },
        {
          "id": "MDRW2100024158.1.18",
          "form": "오호라",
          "original_form": "오호라",
          "speaker_id": "2",
          "time": "20210831 10:05"
        },
        {
          "id": "MDRW2100024158.1.19",
          "form": "저는 어제 필라테스 해서",
          "original_form": "저는 어제 필라테스 해서",
          "speaker_id": "2",
          "time": "20210831 10:05"
        },
        {
          "id": "MDRW2100024158.1.20",
          "form": "오늘 학교에 기어서 올 줄 알았는데",
          "original_form": "오늘 학교에 기어서 올 줄 알았는데",
          "speaker_id": "2",
          "time": "20210831 10:05"
        },
        {
          "id": "MDRW2100024158.1.21",
          "form": "생각보다 제법 말짱하네요?!",
          "original_form": "생각보다 제법 말짱하네요?!",
          "speaker_id": "2",
          "time": "20210831 10:05"
        },
        {
          "id": "MDRW2100024158.1.22",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210831 10:09"
        },
        {
          "id": "MDRW2100024158.1.23",
          "form": "선생님이 그래도 초보라고 봐주신게 맞네 ㅋㅋㅋ",
          "original_form": "선생님이 그래도 초보라고 봐주신게 맞네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:10"
        },
        {
          "id": "MDRW2100024158.1.24",
          "form": "으아아 저도 근육이 좀 생기면",
          "original_form": "으아아 저도 근육이 좀 생기면",
          "speaker_id": "2",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100024158.1.25",
          "form": "동작을 고통스럽지 않게 해낼 수 있기를...",
          "original_form": "동작을 고통스럽지 않게 해낼 수 있기를...",
          "speaker_id": "2",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100024158.1.26",
          "form": "와, 잘하면 선생도 되겠어요!",
          "original_form": "와, 잘하면 선생도 되겠어요!",
          "speaker_id": "1",
          "time": "20210831 10:58"
        },
        {
          "id": "MDRW2100024158.1.27",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100024158.1.28",
          "form": "실제로 제 친구 중에 PT 트레이너 되려고",
          "original_form": "실제로 제 친구 중에 PT 트레이너 되려고",
          "speaker_id": "2",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100024158.1.29",
          "form": "강의 듣고 자격증 따기도 하더라구요!",
          "original_form": "강의 듣고 자격증 따기도 하더라구요!",
          "speaker_id": "2",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100024158.1.30",
          "form": "인생 다모작 시대",
          "original_form": "인생 다모작 시대",
          "speaker_id": "2",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100024158.1.31",
          "form": "그쵸, 나도 암벽등반으로 선생이 되볼테야!",
          "original_form": "그쵸, 나도 암벽등반으로 선생이 되볼테야!",
          "speaker_id": "1",
          "time": "20210831 11:01"
        }
      ]
