      "utterance": [
        {
          "id": "MDRW2100011447.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100011447.1.2",
          "form": "배달 음식 시켰는데 아직 안오네요",
          "original_form": "배달 음식 시켰는데 아직 안오네요",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100011447.1.3",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100011447.1.4",
          "form": "치킨시켰어요ㅋ 감자붙어있는",
          "original_form": "치킨시켰어요ㅋ 감자붙어있는",
          "speaker_id": "2",
          "time": "20210826 06:13"
        },
        {
          "id": "MDRW2100011447.1.5",
          "form": "아 맛있겠다나겎",
          "original_form": "아 맛있겠다나겎",
          "speaker_id": "1",
          "time": "20210826 06:39"
        },
        {
          "id": "MDRW2100011447.1.6",
          "form": "코로나 없어지면 맛집 탐방 하고프네요",
          "original_form": "코로나 없어지면 맛집 탐방 하고프네요",
          "speaker_id": "2",
          "time": "20210826 08:48"
        },
        {
          "id": "MDRW2100011447.1.7",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100011447.1.8",
          "form": "회사 점심시간에 매일 배달음식 시켜먹는데",
          "original_form": "회사 점심시간에 매일 배달음식 시켜먹는데",
          "speaker_id": "2",
          "time": "20210828 20:13"
        },
        {
          "id": "MDRW2100011447.1.9",
          "form": "지겹네요ㅠㅠ",
          "original_form": "지겹네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 20:13"
        },
        {
          "id": "MDRW2100011447.1.10",
          "form": "식당가기 힘들죠",
          "original_form": "식당가기 힘들죠",
          "speaker_id": "1",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100011447.1.11",
          "form": "네 차타고 나가고 기다리고 하면 시간 얼마안남아요",
          "original_form": "네 차타고 나가고 기다리고 하면 시간 얼마안남아요",
          "speaker_id": "2",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100011447.1.12",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "1",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100011447.1.13",
          "form": "12시까지 배달음식오니 그건 좋아요",
          "original_form": "12시까지 배달음식오니 그건 좋아요",
