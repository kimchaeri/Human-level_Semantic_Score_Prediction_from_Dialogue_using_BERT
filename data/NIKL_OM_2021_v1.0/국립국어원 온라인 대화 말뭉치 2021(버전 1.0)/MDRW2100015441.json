      "utterance": [
        {
          "id": "MDRW2100015441.1.1",
          "form": "전시 좋아해요!??",
          "original_form": "전시 좋아해요!??",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015441.1.2",
          "form": "보는 거 좋아해요!",
          "original_form": "보는 거 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015441.1.3",
          "form": "전시 좋아하세요??",
          "original_form": "전시 좋아하세요??",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015441.1.4",
          "form": "저두 구경하는거 좋아해요!",
          "original_form": "저두 구경하는거 좋아해요!",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015441.1.5",
          "form": "최근에 전시 본거 있으세용?",
          "original_form": "최근에 전시 본거 있으세용?",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015441.1.6",
          "form": "저는 이건희 전시회요!",
          "original_form": "저는 이건희 전시회요!",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015441.1.7",
          "form": "오 현대미술관에서 하는거요?",
          "original_form": "오 현대미술관에서 하는거요?",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015441.1.8",
          "form": "국립중앙박물관이요!",
          "original_form": "국립중앙박물관이요!",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015441.1.9",
          "form": "아하~볼만했나용 저는 안봐서 궁금하네요",
          "original_form": "아하~볼만했나용 저는 안봐서 궁금하네요",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015441.1.10",
          "form": "사실 제가 감상할 정도까진 아니고 구경정도의 레벨이라서",
          "original_form": "사실 제가 감상할 정도까진 아니고 구경정도의 레벨이라서",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015441.1.11",
          "form": "근데그림들은 정말 신기했어요!",
          "original_form": "근데그림들은 정말 신기했어요!",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015441.1.12",
          "form": "아하! 저두 구경만 하는 정도지만ㅋㅋ시간나면 가봐야겠어요",
          "original_form": "아하! 저두 구경만 하는 정도지만ㅋㅋ시간나면 가봐야겠어요",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015441.1.13",
          "form": "저는 요시고 전시 다녀왔었어요!",
          "original_form": "저는 요시고 전시 다녀왔었어요!",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015441.1.14",
          "form": "어떠셨어오??",
          "original_form": "어떠셨어오??",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015441.1.15",
          "form": "사람이 너무 많아서 좀 힘들더라구요ㅠㅠ",
          "original_form": "사람이 너무 많아서 좀 힘들더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015441.1.16",
          "form": "대기도 넘 길구",
          "original_form": "대기도 넘 길구",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015441.1.17",
          "form": "그냥 인스타에 많이 돌아다니는 사진들..?",
          "original_form": "그냥 인스타에 많이 돌아다니는 사진들..?",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015441.1.18",
          "form": "사람이 많구나",
          "original_form": "사람이 많구나",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015441.1.19",
          "form": "ㅠㅠ 얼마나 기다리셨어요??",
          "original_form": "ㅠㅠ 얼마나 기다리셨어요??",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015441.1.20",
          "form": "12시에 가서 대기 걸어놓고",
          "original_form": "12시에 가서 대기 걸어놓고",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015441.1.21",
          "form": "점심먹고 놀다가 3시쯤?들어간거같아요",
          "original_form": "점심먹고 놀다가 3시쯤?들어간거같아요",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015441.1.22",
          "form": "사람 넘 많아요ㅍ",
          "original_form": "사람 넘 많아요ㅍ",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
