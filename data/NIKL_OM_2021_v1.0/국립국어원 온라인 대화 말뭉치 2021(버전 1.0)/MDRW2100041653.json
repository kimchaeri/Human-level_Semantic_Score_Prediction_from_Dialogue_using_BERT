      "utterance": [
        {
          "id": "MDRW2100041653.1.1",
          "form": "배가 너무 불러",
          "original_form": "배가 너무 불러",
          "speaker_id": "1",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100041653.1.2",
          "form": "위장에서",
          "original_form": "위장에서",
          "speaker_id": "1",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100041653.1.3",
          "form": "면이 불어",
          "original_form": "면이 불어",
          "speaker_id": "1",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100041653.1.4",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100041653.1.5",
          "form": "알아 알아",
          "original_form": "알아 알아",
          "speaker_id": "2",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100041653.1.6",
          "form": "그런 느낌 알어요",
          "original_form": "그런 느낌 알어요",
          "speaker_id": "2",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100041653.1.7",
          "form": "더뷰륙해",
          "original_form": "더뷰륙해",
          "speaker_id": "2",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100041653.1.8",
          "form": "난 배가 부르면 화가나",
          "original_form": "난 배가 부르면 화가나",
          "speaker_id": "1",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100041653.1.9",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100041653.1.10",
          "form": "나는 토해",
          "original_form": "나는 토해",
          "speaker_id": "2",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100041653.1.11",
          "form": "먹토가 아니고 걍 나옴",
          "original_form": "먹토가 아니고 걍 나옴",
          "speaker_id": "2",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100041653.1.12",
          "form": "토는 안해 화가 나",
          "original_form": "토는 안해 화가 나",
          "speaker_id": "1",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100041653.1.13",
          "form": "너무 화가나 왜 화나는지 모름",
          "original_form": "너무 화가나 왜 화나는지 모름",
          "speaker_id": "1",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100041653.1.14",
          "form": "구글쌤한테 물어봐야지",
          "original_form": "구글쌤한테 물어봐야지",
          "speaker_id": "1",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100041653.1.15",
          "form": "베고플때 화나는 이유만 잇네",
          "original_form": "베고플때 화나는 이유만 잇네",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100041653.1.16",
          "form": "배고플땐 화 안나",
          "original_form": "배고플땐 화 안나",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100041653.1.17",
          "form": "배고플때 화나는사람은 많아",
          "original_form": "배고플때 화나는사람은 많아",
          "speaker_id": "2",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100041653.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅌㅌ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅌㅌ",
          "speaker_id": "2",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100041653.1.19",
          "form": "배고플 때 왜 화나 이상한 사람들이네",
          "original_form": "배고플 때 왜 화나 이상한 사람들이네",
          "speaker_id": "1",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100041653.1.20",
          "form": "당을 섭취해",
          "original_form": "당을 섭취해",
          "speaker_id": "1",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100041653.1.21",
          "form": "그러기위해 화가나는거짘ㅋㅋㅋㅋ",
          "original_form": "그러기위해 화가나는거짘ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100041653.1.22",
          "form": "다이어트할때 예민해지는 거랑 같잖아",
          "original_form": "다이어트할때 예민해지는 거랑 같잖아",
          "speaker_id": "2",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100041653.1.23",
          "form": "배부르면 왜 화나지",
          "original_form": "배부르면 왜 화나지",
          "speaker_id": "2",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100041653.1.24",
          "form": "속이 꽉찬 불편감?",
          "original_form": "속이 꽉찬 불편감?",
          "speaker_id": "2",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100041653.1.25",
          "form": "난 다이어트 안해서 몰라 하지만 공복으로 예민해져본적업서",
          "original_form": "난 다이어트 안해서 몰라 하지만 공복으로 예민해져본적업서",
          "speaker_id": "1",
          "time": "20210906 14:12"
        },
        {
          "id": "MDRW2100041653.1.26",
          "form": "공복이면 집중 더 잘돼",
          "original_form": "공복이면 집중 더 잘돼",
          "speaker_id": "1",
          "time": "20210906 14:12"
        },
        {
          "id": "MDRW2100041653.1.27",
          "form": "학자들 연구 하나도 안해놨네",
          "original_form": "학자들 연구 하나도 안해놨네",
          "speaker_id": "1",
          "time": "20210906 14:12"
        },
        {
          "id": "MDRW2100041653.1.28",
          "form": "그냥 속이 찬 느낌이 아니고 드냥",
          "original_form": "그냥 속이 찬 느낌이 아니고 드냥",
          "speaker_id": "1",
          "time": "20210906 14:14"
        },
        {
          "id": "MDRW2100041653.1.29",
          "form": "머리에서 화가나",
          "original_form": "머리에서 화가나",
          "speaker_id": "1",
          "time": "20210906 14:14"
        },
        {
          "id": "MDRW2100041653.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ당과다...?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ당과다...?",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100041653.1.31",
          "form": "단 거 많이 먹는다고 화는 안난다구 힝",
          "original_form": "단 거 많이 먹는다고 화는 안난다구 힝",
          "speaker_id": "1",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100041653.1.32",
          "form": "과학적인설명이 필요해요",
          "original_form": "과학적인설명이 필요해요",
          "speaker_id": "2",
          "time": "20210906 14:23"
        },
        {
          "id": "MDRW2100041653.1.33",
          "form": "하지만 구글도 알려주지 않앗어",
          "original_form": "하지만 구글도 알려주지 않앗어",
          "speaker_id": "1",
          "time": "20210906 21:32"
        }
      ]
    }
