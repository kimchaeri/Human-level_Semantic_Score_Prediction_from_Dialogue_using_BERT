      "utterance": [
        {
          "id": "MDRW2100005540.1.1",
          "form": "내일 로제떡볶이 먹으까..",
          "original_form": "내일 로제떡볶이 먹으까..",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005540.1.2",
          "form": "로줴~~! !함도 안먹어봄",
          "original_form": "로줴~~! !함도 안먹어봄",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005540.1.3",
          "form": "만들어먹게?",
          "original_form": "만들어먹게?",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005540.1.4",
          "form": "스카 가까이에 배떡 있더라구?!",
          "original_form": "스카 가까이에 배떡 있더라구?!",
          "speaker_id": "1",
          "time": "20210822 07:04"
        },
        {
          "id": "MDRW2100005540.1.5",
          "form": "거기가 유명하댕",
          "original_form": "거기가 유명하댕",
          "speaker_id": "1",
          "time": "20210822 07:04"
        },
        {
          "id": "MDRW2100005540.1.6",
          "form": "배떡?",
          "original_form": "배떡?",
          "speaker_id": "2",
          "time": "20210822 07:21"
        },
        {
          "id": "MDRW2100005540.1.7",
          "form": "첨들어본당",
          "original_form": "첨들어본당",
          "speaker_id": "2",
          "time": "20210822 07:27"
        },
        {
          "id": "MDRW2100005540.1.8",
          "form": "맛나겠고만...",
          "original_form": "맛나겠고만...",
          "speaker_id": "2",
          "time": "20210822 07:27"
        },
        {
          "id": "MDRW2100005540.1.9",
          "form": "떡볶이 먹기로 했오?",
          "original_form": "떡볶이 먹기로 했오?",
          "speaker_id": "2",
          "time": "20210822 09:40"
        },
        {
          "id": "MDRW2100005540.1.10",
          "form": "엄마한테 얘기해봣나..ㅋㅋㅋ",
          "original_form": "엄마한테 얘기해봣나..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:40"
        },
        {
          "id": "MDRW2100005540.1.11",
          "form": "결국 얘기못하곸ㅋㅋ",
          "original_form": "결국 얘기못하곸ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100005540.1.12",
          "form": "포장 갈지안갈지도 모르게써 생각보다 덥네..?",
          "original_form": "포장 갈지안갈지도 모르게써 생각보다 덥네..?",
          "speaker_id": "1",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100005540.1.13",
          "form": "그르게 좀 쌀쌀한 날씨면",
          "original_form": "그르게 좀 쌀쌀한 날씨면",
          "speaker_id": "2",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100005540.1.14",
          "form": "떡볶이 먹기 딱 좋을텐디",
          "original_form": "떡볶이 먹기 딱 좋을텐디",
          "speaker_id": "2",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100005540.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100005540.1.16",
          "form": "훔",
          "original_form": "훔",
          "speaker_id": "2",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100005540.1.17",
          "form": "담에 머거도 되공..",
          "original_form": "담에 머거도 되공..",
          "speaker_id": "2",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100005540.1.18",
          "form": "웅 같이먹으면 마싯게머글텐데 또 혼자먹는거면 훔...",
          "original_form": "웅 같이먹으면 마싯게머글텐데 또 혼자먹는거면 훔...",
          "speaker_id": "1",
          "time": "20210822 14:26"
        },
        {
          "id": "MDRW2100005540.1.19",
          "form": "또 몰라 갑자기 쁼받아서 사러나갈수도..?!",
          "original_form": "또 몰라 갑자기 쁼받아서 사러나갈수도..?!",
          "speaker_id": "1",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100005540.1.20",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:20"
        },
        {
          "id": "MDRW2100005540.1.21",
          "form": "맛평가 부탁..",
          "original_form": "맛평가 부탁..",
          "speaker_id": "2",
          "time": "20210822 15:20"
        },
        {
          "id": "MDRW2100005540.1.22",
          "form": "어엉 드릉드릉하는데 배가고파섴ㅋㅋ",
          "original_form": "어엉 드릉드릉하는데 배가고파섴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100005540.1.23",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:44"
        },
        {
          "id": "MDRW2100005540.1.24",
          "form": "드릉?",
          "original_form": "드릉?",
          "speaker_id": "2",
          "time": "20210822 18:44"
        },
        {
          "id": "MDRW2100005540.1.25",
          "form": "결국 안먹구...가져왔던 닭가슴살볼이랑",
          "original_form": "결국 안먹구...가져왔던 닭가슴살볼이랑",
          "speaker_id": "1",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100005540.1.26",
          "form": "펴늬점에서 삼각김밥 하나...",
          "original_form": "펴늬점에서 삼각김밥 하나...",
          "speaker_id": "1",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100005540.1.27",
          "form": "도르륵",
          "original_form": "도르륵",
          "speaker_id": "1",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100005540.1.28",
          "form": "집에 맛있는 새우튀김 있눈뒈 ㅎㅎ",
          "original_form": "집에 맛있는 새우튀김 있눈뒈 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100005540.1.29",
          "form": "머것따",
          "original_form": "머것따",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100005540.1.30",
          "form": "마싯서",
          "original_form": "마싯서",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100005540.1.31",
          "form": "입천장이 까지는거같아",
          "original_form": "입천장이 까지는거같아",
          "speaker_id": "1",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100005540.1.32",
          "form": "새우튀김 먹어성?",
          "original_form": "새우튀김 먹어성?",
          "speaker_id": "2",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100005540.1.33",
          "form": "새우튀김 모양도 완전 예뻣음",
          "original_form": "새우튀김 모양도 완전 예뻣음",
          "speaker_id": "2",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100005540.1.34",
          "form": "파는 것 같았다구",
          "original_form": "파는 것 같았다구",
          "speaker_id": "2",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100005540.1.35",
          "form": "엉 사이즈도 엄청크고",
          "original_form": "엉 사이즈도 엄청크고",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100005540.1.36",
          "form": "그림같이생겼던뎉ㅋㅋ",
          "original_form": "그림같이생겼던뎉ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100005540.1.37",
          "form": "맞아 동금동글하고",
          "original_form": "맞아 동금동글하고",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100005540.1.38",
          "form": "기여웟음",
          "original_form": "기여웟음",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100005540.1.39",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100005540.1.40",
          "form": "내일 스벅 갈랭?",
          "original_form": "내일 스벅 갈랭?",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100005540.1.41",
          "form": "서벅 먹고싶다",
          "original_form": "서벅 먹고싶다",
          "speaker_id": "2",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100005540.1.42",
          "form": "스벅 뭐먹고시픈데!!",
          "original_form": "스벅 뭐먹고시픈데!!",
          "speaker_id": "1",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100005540.1.43",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100005540.1.44",
          "form": "낼 아침에 땡기는 무언가가 있을 것 같아..",
          "original_form": "낼 아침에 땡기는 무언가가 있을 것 같아..",
          "speaker_id": "2",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100005540.1.45",
          "form": "아마 초코..",
          "original_form": "아마 초코..",
          "speaker_id": "2",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100005540.1.46",
          "form": "ㅋㅋㅋㅋ그럼 낼 정하쟈..!.!!",
          "original_form": "ㅋㅋㅋㅋ그럼 낼 정하쟈..!.!!",
          "speaker_id": "1",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100005540.1.47",
          "form": "녱엥",
          "original_form": "녱엥",
          "speaker_id": "2",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100005540.1.48",
          "form": "꿈에서 떡볶이머거야지",
          "original_form": "꿈에서 떡볶이머거야지",
          "speaker_id": "1",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100005540.1.49",
          "form": "ㅋㄱㄱㅋㅋㅋ낼 점심으로 사머겅!",
          "original_form": "ㅋㄱㄱㅋㅋㅋ낼 점심으로 사머겅!",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100005540.1.50",
          "form": "난 꿈에서 도미노 피자 머글거임",
          "original_form": "난 꿈에서 도미노 피자 머글거임",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100005540.1.51",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ도미노피자 맛있더나..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ도미노피자 맛있더나..",
          "speaker_id": "1",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100005540.1.52",
          "form": "대신 오늘 다른피자가왔네?!",
          "original_form": "대신 오늘 다른피자가왔네?!",
          "speaker_id": "1",
          "time": "20210823 13:41"
        },
        {
          "id": "MDRW2100005540.1.53",
          "form": "넘조아",
          "original_form": "넘조아",
          "speaker_id": "2",
          "time": "20210823 13:44"
        },
        {
          "id": "MDRW2100005540.1.54",
          "form": "바른먹거리 풀무원",
          "original_form": "바른먹거리 풀무원",
          "speaker_id": "2",
          "time": "20210823 13:44"
        },
        {
          "id": "MDRW2100005540.1.55",
          "form": "쿠큐",
          "original_form": "쿠큐",
          "speaker_id": "2",
          "time": "20210823 13:44"
        },
        {
          "id": "MDRW2100005540.1.56",
          "form": "먹을게 짱많아",
          "original_form": "먹을게 짱많아",
          "speaker_id": "2",
          "time": "20210823 13:44"
        },
        {
          "id": "MDRW2100005540.1.57",
          "form": "아임리얼 먹고싶따",
          "original_form": "아임리얼 먹고싶따",
          "speaker_id": "1",
          "time": "20210823 14:42"
        },
        {
          "id": "MDRW2100005540.1.58",
          "form": "집가서 한병해야지",
          "original_form": "집가서 한병해야지",
          "speaker_id": "1",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100005540.1.59",
          "form": "ㅋㅋㅋㅋ나눈 시카고 핏짜..",
          "original_form": "ㅋㅋㅋㅋ나눈 시카고 핏짜..",
          "speaker_id": "2",
          "time": "20210823 17:06"
        },
        {
          "id": "MDRW2100005540.1.60",
          "form": "맛있을 것 같다",
          "original_form": "맛있을 것 같다",
          "speaker_id": "2",
          "time": "20210823 17:06"
        },
        {
          "id": "MDRW2100005540.1.61",
          "form": "둑흔둑흔 언제먹을거야",
          "original_form": "둑흔둑흔 언제먹을거야",
          "speaker_id": "1",
          "time": "20210823 17:28"
        },
        {
          "id": "MDRW2100005540.1.62",
          "form": "주말에??",
          "original_form": "주말에??",
          "speaker_id": "2",
          "time": "20210823 18:02"
        },
        {
          "id": "MDRW2100005540.1.63",
          "form": "가족 다 있을때",
          "original_form": "가족 다 있을때",
          "speaker_id": "2",
          "time": "20210823 18:02"
        },
        {
          "id": "MDRW2100005540.1.64",
          "form": "조아조아 시카고피자 맛이 아주 궁금..",
          "original_form": "조아조아 시카고피자 맛이 아주 궁금..",
          "speaker_id": "1",
          "time": "20210823 19:15"
        },
        {
          "id": "MDRW2100005540.1.65",
          "form": "치즈폭포 핏짜맛",
          "original_form": "치즈폭포 핏짜맛",
          "speaker_id": "2",
          "time": "20210825 08:33"
        },
        {
          "id": "MDRW2100005540.1.66",
          "form": "버블티 먹고싶다",
          "original_form": "버블티 먹고싶다",
          "speaker_id": "2",
          "time": "20210825 08:33"
        },
        {
          "id": "MDRW2100005540.1.67",
          "form": "스벅도 버블티 만들어주면 좋을텐데",
          "original_form": "스벅도 버블티 만들어주면 좋을텐데",
          "speaker_id": "2",
          "time": "20210825 08:33"
        },
        {
          "id": "MDRW2100005540.1.68",
          "form": "시그니쳐아이스 초콜릿에 펄 추가해서 먹으먄",
