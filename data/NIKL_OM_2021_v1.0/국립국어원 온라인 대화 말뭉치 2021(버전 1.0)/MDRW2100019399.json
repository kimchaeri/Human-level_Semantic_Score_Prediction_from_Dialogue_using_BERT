      "utterance": [
        {
          "id": "MDRW2100019399.1.1",
          "form": "사이드쇼 감사해요 찾아봐야겠어요!",
          "original_form": "사이드쇼 감사해요 찾아봐야겠어요!",
          "speaker_id": "2",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100019399.1.2",
          "form": "떡볶이 말고 카페나 이런데도 좋아하시나요?",
          "original_form": "떡볶이 말고 카페나 이런데도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100019399.1.3",
          "form": "네 강추해요 감튀도 필수입니다",
          "original_form": "네 강추해요 감튀도 필수입니다",
          "speaker_id": "1",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100019399.1.4",
          "form": "디저트를 엄청 좋아해요 ㅋㅋㅋ",
          "original_form": "디저트를 엄청 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100019399.1.5",
          "form": "감튀! 감튀도 기억할게요",
          "original_form": "감튀! 감튀도 기억할게요",
          "speaker_id": "2",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100019399.1.6",
          "form": "저도 디저트 귀신인데 ㅋㅋㅋ 뭘 제일 좋아하시나요",
          "original_form": "저도 디저트 귀신인데 ㅋㅋㅋ 뭘 제일 좋아하시나요",
          "speaker_id": "2",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100019399.1.7",
          "form": "저는 단거는 사실 다 좋아해서 ㅋㅋㅋ",
          "original_form": "저는 단거는 사실 다 좋아해서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:47"
        },
        {
          "id": "MDRW2100019399.1.8",
          "form": "빵순이라 빵 좋아해요",
          "original_form": "빵순이라 빵 좋아해요",
          "speaker_id": "1",
          "time": "20210827 18:47"
        },
        {
          "id": "MDRW2100019399.1.9",
          "form": "헉 저도 빵순이에욬ㅋㅋㅋ",
          "original_form": "헉 저도 빵순이에욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:47"
        },
        {
          "id": "MDRW2100019399.1.10",
          "form": "요즘 요기요에서 파바 이벤트 하시는거 아시나요",
          "original_form": "요즘 요기요에서 파바 이벤트 하시는거 아시나요",
          "speaker_id": "2",
          "time": "20210827 18:47"
        },
        {
          "id": "MDRW2100019399.1.11",
          "form": "알져알져 먹는거를 좋아해서요ㅋㅋㅋㅋ",
          "original_form": "알져알져 먹는거를 좋아해서요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:47"
        },
        {
          "id": "MDRW2100019399.1.12",
          "form": "파바는 요새 먹고 싶은게 없어서 안먹은지 좀 됐어요 ㅠㅠ",
          "original_form": "파바는 요새 먹고 싶은게 없어서 안먹은지 좀 됐어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 18:47"
        },
        {
          "id": "MDRW2100019399.1.13",
          "form": "아 저두요 ㅠㅠ 이벤트 한다고 해서 혹 했는데 살게 없더라구요",
          "original_form": "아 저두요 ㅠㅠ 이벤트 한다고 해서 혹 했는데 살게 없더라구요",
          "speaker_id": "2",
          "time": "20210827 18:48"
        },
        {
          "id": "MDRW2100019399.1.14",
          "form": "요즘은 개인 디저트 카페도 너무 많고 해서",
          "original_form": "요즘은 개인 디저트 카페도 너무 많고 해서",
          "speaker_id": "2",
          "time": "20210827 18:48"
        },
        {
          "id": "MDRW2100019399.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 18:48"
        },
        {
          "id": "MDRW2100019399.1.16",
          "form": "개인가게가 제일 좋은거같아요",
          "original_form": "개인가게가 제일 좋은거같아요",
          "speaker_id": "1",
          "time": "20210827 18:48"
        },
        {
          "id": "MDRW2100019399.1.17",
          "form": "좋아하는 단골 가게 있으세요? 전 집 주변엔 없어서 단골이 될수가 없어욬ㅋㅋ",
          "original_form": "좋아하는 단골 가게 있으세요? 전 집 주변엔 없어서 단골이 될수가 없어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:48"
        },
        {
          "id": "MDRW2100019399.1.18",
          "form": "흠 저는..",
          "original_form": "흠 저는..",
          "speaker_id": "1",
          "time": "20210827 18:48"
        },
        {
          "id": "MDRW2100019399.1.19",
          "form": "아이디어스 아세요?",
          "original_form": "아이디어스 아세요?",
          "speaker_id": "1",
          "time": "20210827 18:49"
        },
        {
          "id": "MDRW2100019399.1.20",
          "form": "네! 알아요!",
          "original_form": "네! 알아요!",
          "speaker_id": "2",
          "time": "20210827 18:49"
        },
        {
          "id": "MDRW2100019399.1.21",
          "form": "최근에 할인해서 구매했는데",
          "original_form": "최근에 할인해서 구매했는데",
          "speaker_id": "1",
          "time": "20210827 18:49"
        },
        {
          "id": "MDRW2100019399.1.22",
          "form": "오늘 먹고 기절했어여 크림브륄레 뚱카롱이요 ㅠㅠ",
          "original_form": "오늘 먹고 기절했어여 크림브륄레 뚱카롱이요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 18:49"
        },
        {
          "id": "MDRW2100019399.1.23",
          "form": "헐 이름부터 맛있을 거 같아요......",
          "original_form": "헐 이름부터 맛있을 거 같아요......",
          "speaker_id": "2",
          "time": "20210827 18:49"
        },
        {
          "id": "MDRW2100019399.1.24",
          "form": "저 어제 크림브륄레 티 마셨는뎈ㅋㅋㅋ",
          "original_form": "저 어제 크림브륄레 티 마셨는뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:49"
        },
        {
          "id": "MDRW2100019399.1.25",
          "form": "헐 그런게있나요?",
          "original_form": "헐 그런게있나요?",
          "speaker_id": "1",
          "time": "20210827 18:50"
        },
        {
          "id": "MDRW2100019399.1.26",
          "form": "진짜 살찌기 좋은 세상이네요",
