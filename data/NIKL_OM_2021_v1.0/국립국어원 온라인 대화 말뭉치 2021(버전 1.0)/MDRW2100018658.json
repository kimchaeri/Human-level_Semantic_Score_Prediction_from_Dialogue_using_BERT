      "utterance": [
        {
          "id": "MDRW2100018658.1.1",
          "form": "저는 오늘 저녁에 회먹을거에요 후후",
          "original_form": "저는 오늘 저녁에 회먹을거에요 후후",
          "speaker_id": "1",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100018658.1.2",
          "form": "부럽죠?",
          "original_form": "부럽죠?",
          "speaker_id": "1",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100018658.1.3",
          "form": "비 오는데 회를??",
          "original_form": "비 오는데 회를??",
          "speaker_id": "2",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100018658.1.4",
          "form": "무슨 횝니까",
          "original_form": "무슨 횝니까",
          "speaker_id": "2",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100018658.1.5",
          "form": "비오면 회먹으면안됩니까,, 기생충약 잘먹으면됩니다",
          "original_form": "비오면 회먹으면안됩니까,, 기생충약 잘먹으면됩니다",
          "speaker_id": "1",
          "time": "20210827 10:46"
        },
        {
          "id": "MDRW2100018658.1.6",
          "form": "참치먹을지 광어먹을지~~",
          "original_form": "참치먹을지 광어먹을지~~",
          "speaker_id": "1",
          "time": "20210827 10:46"
        },
        {
          "id": "MDRW2100018658.1.7",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:57"
        },
        {
          "id": "MDRW2100018658.1.8",
          "form": "비오는 날은 비린내가 더 심하잖아요?",
          "original_form": "비오는 날은 비린내가 더 심하잖아요?",
          "speaker_id": "2",
          "time": "20210827 10:57"
        },
        {
          "id": "MDRW2100018658.1.9",
          "form": "회킬러에게는 그런거없습니다",
          "original_form": "회킬러에게는 그런거없습니다",
          "speaker_id": "1",
          "time": "20210827 10:57"
        },
        {
          "id": "MDRW2100018658.1.10",
          "form": "참치는 별로니까 광어가시죠 광어는 맛있음",
          "original_form": "참치는 별로니까 광어가시죠 광어는 맛있음",
          "speaker_id": "2",
          "time": "20210827 10:58"
        },
        {
          "id": "MDRW2100018658.1.11",
          "form": "참치맛있는데 ㅠ",
          "original_form": "참치맛있는데 ㅠ",
          "speaker_id": "1",
          "time": "20210827 10:58"
        },
        {
          "id": "MDRW2100018658.1.12",
          "form": "저는 물고기들에게 차별두지않아요",
          "original_form": "저는 물고기들에게 차별두지않아요",
          "speaker_id": "1",
          "time": "20210827 10:58"
        },
        {
          "id": "MDRW2100018658.1.13",
          "form": "오늘 보드카 먹을예정 ㅎ",
          "original_form": "오늘 보드카 먹을예정 ㅎ",
          "speaker_id": "1",
          "time": "20210827 10:58"
        },
        {
          "id": "MDRW2100018658.1.14",
          "form": "술도 잘 드시나봐요?",
          "original_form": "술도 잘 드시나봐요?",
          "speaker_id": "2",
          "time": "20210827 10:58"
        },
        {
          "id": "MDRW2100018658.1.15",
          "form": "뭐드세요???",
          "original_form": "뭐드세요???",
          "speaker_id": "2",
          "time": "20210827 10:58"
        },
        {
          "id": "MDRW2100018658.1.16",
          "form": "거의 진로먹는데 요즘 와인엄청맛잇드라구여",
          "original_form": "거의 진로먹는데 요즘 와인엄청맛잇드라구여",
          "speaker_id": "1",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100018658.1.17",
          "form": "오 와인 저도",
          "original_form": "오 와인 저도",
          "speaker_id": "2",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100018658.1.18",
          "form": "가성비 와인 찾아마시는데",
          "original_form": "가성비 와인 찾아마시는데",
          "speaker_id": "2",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100018658.1.19",
          "form": "비비노 깔았죠?",
          "original_form": "비비노 깔았죠?",
          "speaker_id": "2",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100018658.1.20",
          "form": "저 와알못이라 그냥 추천받아서먹습니다",
          "original_form": "저 와알못이라 그냥 추천받아서먹습니다",
          "speaker_id": "1",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100018658.1.21",
          "form": "비비노가뭔가요",
          "original_form": "비비노가뭔가요",
          "speaker_id": "1",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100018658.1.22",
          "form": "가게에서요?",
          "original_form": "가게에서요?",
          "speaker_id": "2",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100018658.1.23",
          "form": "아 비비노를 모르시다니",
          "original_form": "아 비비노를 모르시다니",
          "speaker_id": "2",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100018658.1.24",
          "form": "신세대 맞으세요?",
          "original_form": "신세대 맞으세요?",
          "speaker_id": "2",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100018658.1.25",
          "form": "넹 가게에서",
          "original_form": "넹 가게에서",
          "speaker_id": "1",
          "time": "20210827 11:00"
        },
        {
          "id": "MDRW2100018658.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:00"
        },
        {
          "id": "MDRW2100018658.1.27",
          "form": "모른다고 너무하시네ㅜ",
          "original_form": "모른다고 너무하시네ㅜ",
          "speaker_id": "1",
          "time": "20210827 11:00"
        },
        {
          "id": "MDRW2100018658.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:00"
        },
        {
          "id": "MDRW2100018658.1.29",
          "form": "원래 가끔씩 이렇게 딜을 맞아야",
          "original_form": "원래 가끔씩 이렇게 딜을 맞아야",
          "speaker_id": "2",
          "time": "20210827 11:00"
        },
        {
          "id": "MDRW2100018658.1.30",
          "form": "찾아보고 발전하죠",
          "original_form": "찾아보고 발전하죠",
          "speaker_id": "2",
          "time": "20210827 11:00"
        },
        {
          "id": "MDRW2100018658.1.31",
          "form": "비비노 까세요",
          "original_form": "비비노 까세요",
          "speaker_id": "2",
          "time": "20210827 11:00"
        },
        {
          "id": "MDRW2100018658.1.32",
          "form": "vivino",
          "original_form": "vivino",
          "speaker_id": "2",
          "time": "20210827 11:00"
        },
        {
          "id": "MDRW2100018658.1.33",
          "form": "영어만 있긴한데",
          "original_form": "영어만 있긴한데",
          "speaker_id": "2",
          "time": "20210827 11:01"
        },
