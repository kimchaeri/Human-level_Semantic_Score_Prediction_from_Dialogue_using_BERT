      "utterance": [
        {
          "id": "MDRW2100022474.1.1",
          "form": "햄스터 키워봤어요?",
          "original_form": "햄스터 키워봤어요?",
          "speaker_id": "2",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100022474.1.2",
          "form": "name3는 사람을 엄청좋아하거든요",
          "original_form": "&name3&는 사람을 엄청좋아하거든요",
          "speaker_id": "1",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100022474.1.3",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100022474.1.4",
          "form": "키워보셨어요?",
          "original_form": "키워보셨어요?",
          "speaker_id": "1",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100022474.1.5",
          "form": "네 저 키워봤는데",
          "original_form": "네 저 키워봤는데",
          "speaker_id": "2",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100022474.1.6",
          "form": "수컷 한마리 암컷 한마리요",
          "original_form": "수컷 한마리 암컷 한마리요",
          "speaker_id": "2",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100022474.1.7",
          "form": "번식엄청한다던데",
          "original_form": "번식엄청한다던데",
          "speaker_id": "1",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100022474.1.8",
          "form": "세상에 그 둘밖에 없어서 그런지 눈 맞기도 쉽더라구요",
          "original_form": "세상에 그 둘밖에 없어서 그런지 눈 맞기도 쉽더라구요",
          "speaker_id": "2",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100022474.1.9",
          "form": "ㅋㅋㅋㅋ곧바로 번식 하셨어요 그 두 분..",
          "original_form": "ㅋㅋㅋㅋ곧바로 번식 하셨어요 그 두 분..",
          "speaker_id": "2",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100022474.1.10",
          "form": "헉.. 빠르네요",
          "original_form": "헉.. 빠르네요",
          "speaker_id": "1",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100022474.1.11",
          "form": "근데 햄스터 특성이 원래 그렇다는데 그땐 잘몰라서",
          "original_form": "근데 햄스터 특성이 원래 그렇다는데 그땐 잘몰라서",
          "speaker_id": "2",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100022474.1.12",
          "form": "아기 낳고 그냥 한집에 놔뒀거든요?",
          "original_form": "아기 낳고 그냥 한집에 놔뒀거든요?",
          "speaker_id": "2",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100022474.1.13",
          "form": "아기햄스터 잡아먹었어요..",
          "original_form": "아기햄스터 잡아먹었어요..",
          "speaker_id": "2",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100022474.1.14",
          "form": "먹는다면서요",
          "original_form": "먹는다면서요",
          "speaker_id": "1",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022474.1.15",
          "form": "호러.",
          "original_form": "호러.",
          "speaker_id": "1",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022474.1.16",
          "form": "ㅠㅠㅠㅠㅠㅠㅠ왜 먹는거에요?",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠ왜 먹는거에요?",
          "speaker_id": "2",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022474.1.17",
          "form": "진짜 그때 초등학생 때라 충격먹었어요",
          "original_form": "진짜 그때 초등학생 때라 충격먹었어요",
          "speaker_id": "2",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022474.1.18",
          "form": "그거",
          "original_form": "그거",
          "speaker_id": "1",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022474.1.19",
          "form": "스트레스때문에",
          "original_form": "스트레스때문에",
          "speaker_id": "1",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022474.1.20",
          "form": "아 양육스트레스...",
          "original_form": "아 양육스트레스...",
          "speaker_id": "2",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022474.1.21",
          "form": "자기말고는 적으로본대요",
          "original_form": "자기말고는 적으로본대요",
          "speaker_id": "1",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022474.1.22",
          "form": "그래놓고 왜 아기 낳았대요",
          "original_form": "그래놓고 왜 아기 낳았대요",
          "speaker_id": "2",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022474.1.23",
          "form": "이상한 습성이야 정말",
          "original_form": "이상한 습성이야 정말",
          "speaker_id": "2",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022474.1.24",
          "form": "아니요 양육아니고 출산 후에",
          "original_form": "아니요 양육아니고 출산 후에",
          "speaker_id": "1",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022474.1.25",
          "form": "스트레스받아서",
          "original_form": "스트레스받아서",
          "speaker_id": "1",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022474.1.26",
          "form": "ㅠㅠㅠ 분리 해주지 못한 주인 탓이죠 뭐 ㅜㅜ",
          "original_form": "ㅠㅠㅠ 분리 해주지 못한 주인 탓이죠 뭐 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 14:42"
        },
        {
          "id": "MDRW2100022474.1.27",
          "form": "인지못한대요",
          "original_form": "인지못한대요",
          "speaker_id": "1",
          "time": "20210829 14:42"
        },
        {
          "id": "MDRW2100022474.1.28",
          "form": "지자식인걸",
          "original_form": "지자식인걸",
          "speaker_id": "1",
          "time": "20210829 14:42"
        },
        {
          "id": "MDRW2100022474.1.29",
          "form": "되게 다양하게",
          "original_form": "되게 다양하게",
