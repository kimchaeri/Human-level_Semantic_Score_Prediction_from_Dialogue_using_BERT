      "utterance": [
        {
          "id": "MDRW2100033446.1.1",
          "form": "아이들이 저보는데 만화  본다고 리모컨 빼서가요",
          "original_form": "아이들이 저보는데 만화  본다고 리모컨 빼서가요",
          "speaker_id": "2",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100033446.1.2",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100033446.1.3",
          "form": "참나 밤에 그나마 전 볼수있는데",
          "original_form": "참나 밤에 그나마 전 볼수있는데",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100033446.1.4",
          "form": "그냥 티비는 제꺼 아닌걸로 ㅎㅎㅎ",
          "original_form": "그냥 티비는 제꺼 아닌걸로 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100033446.1.5",
          "form": "애들 재워서 못보고",
          "original_form": "애들 재워서 못보고",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100033446.1.6",
          "form": "속상하시겠어요",
          "original_form": "속상하시겠어요",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100033446.1.7",
          "form": "그렇죠 ㅋㅋ",
          "original_form": "그렇죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100033446.1.8",
          "form": "그래도 크면 제꺼돼요",
          "original_form": "그래도 크면 제꺼돼요",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100033446.1.9",
          "form": "그러니 폰으로 웨이브, 티빙, 넷플 다 끊어서 보죠 ㅋㅋㅋ",
          "original_form": "그러니 폰으로 웨이브, 티빙, 넷플 다 끊어서 보죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100033446.1.10",
          "form": "ㅋㅋ애들도 유튜브 게임 요런거 만이 하게돼요",
          "original_form": "ㅋㅋ애들도 유튜브 게임 요런거 만이 하게돼요",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100033446.1.11",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100033446.1.12",
          "form": "저희애들도 .. 이미 유튜브, 게임해요 ㅎㅎ",
          "original_form": "저희애들도 .. 이미 유튜브, 게임해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:43"
        },
        {
          "id": "MDRW2100033446.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 22:43"
        },
        {
          "id": "MDRW2100033446.1.14",
          "form": "확실히 저희 어릴때랑은 다른거 같애요",
          "original_form": "확실히 저희 어릴때랑은 다른거 같애요",
          "speaker_id": "1",
          "time": "20210823 22:43"
        },
        {
          "id": "MDRW2100033446.1.15",
          "form": "요샌 태어난순간 유튜브 본데요",
          "original_form": "요샌 태어난순간 유튜브 본데요",
          "speaker_id": "2",
          "time": "20210823 22:43"
        },
        {
          "id": "MDRW2100033446.1.16",
          "form": "유튜브로 장난감 알고 게임배우고 ㅋㅋㅋ",
          "original_form": "유튜브로 장난감 알고 게임배우고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:43"
        },
        {
          "id": "MDRW2100033446.1.17",
          "form": "다르죠",
          "original_form": "다르죠",
          "speaker_id": "2",
          "time": "20210823 22:43"
        },
        {
          "id": "MDRW2100033446.1.18",
          "form": "저희딸은 넷플 애니 접수요",
          "original_form": "저희딸은 넷플 애니 접수요",
          "speaker_id": "2",
          "time": "20210823 22:43"
        },
        {
          "id": "MDRW2100033446.1.19",
          "form": "전 그때 못봐요 ㅋㅋ",
          "original_form": "전 그때 못봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:44"
        },
        {
          "id": "MDRW2100033446.1.20",
          "form": "하이큐 목소리형태 췌장을 먹고싶어",
          "original_form": "하이큐 목소리형태 췌장을 먹고싶어",
          "speaker_id": "2",
          "time": "20210823 22:44"
        },
        {
          "id": "MDRW2100033446.1.21",
          "form": "아 ㅠㅠㅠ 넷플도  빼앗기다뇨 ㅠㅠㅠ",
          "original_form": "아 ㅠㅠㅠ 넷플도  빼앗기다뇨 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 22:44"
        },
        {
          "id": "MDRW2100033446.1.22",
          "form": "뭐이런거 보더라구요",
          "original_form": "뭐이런거 보더라구요",
          "speaker_id": "2",
          "time": "20210823 22:44"
        },
        {
          "id": "MDRW2100033446.1.23",
          "form": "오면 뺏기죠",
          "original_form": "오면 뺏기죠",
          "speaker_id": "2",
          "time": "20210823 22:44"
        },
        {
          "id": "MDRW2100033446.1.24",
          "form": "그래도 제시간이 더있긴해요 크니꺼",
          "original_form": "그래도 제시간이 더있긴해요 크니꺼",
          "speaker_id": "2",
          "time": "20210823 22:44"
        },
        {
          "id": "MDRW2100033446.1.25",
          "form": "아들은 죠죠본다고",
          "original_form": "아들은 죠죠본다고",
          "speaker_id": "2",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100033446.1.26",
          "form": "저 밥먹을때 뺏어요",
          "original_form": "저 밥먹을때 뺏어요",
          "speaker_id": "2",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100033446.1.27",
          "form": "참나 밥먹을때 봐야하는데 ㅜㅜ",
          "original_form": "참나 밥먹을때 봐야하는데 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 22:46"
        }
      ]
    }
