      "utterance": [
        {
          "id": "MDRW2100042426.1.1",
          "form": "놀러가고싶으신 장소 있으세요?",
          "original_form": "놀러가고싶으신 장소 있으세요?",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042426.1.2",
          "form": "저 제주도 가고 싶어요 수학여행으로만 가봤거든요",
          "original_form": "저 제주도 가고 싶어요 수학여행으로만 가봤거든요",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042426.1.3",
          "form": "name1님은용?",
          "original_form": "&name1&님은용?",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042426.1.4",
          "form": "저도 제주도요ㅠ",
          "original_form": "저도 제주도요ㅠ",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042426.1.5",
          "form": "호주도 가보고싶어요",
          "original_form": "호주도 가보고싶어요",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042426.1.6",
          "form": "여행가본적 있으세요? 저는 여행경험이 없어요!",
          "original_form": "여행가본적 있으세요? 저는 여행경험이 없어요!",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042426.1.7",
          "form": "제주도는 두번 가봤는데 호주는....ㅎㅎ",
          "original_form": "제주도는 두번 가봤는데 호주는....ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042426.1.8",
          "form": "두번간것도 한번은 4살때간거라 기억이 흐릿해요",
          "original_form": "두번간것도 한번은 4살때간거라 기억이 흐릿해요",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042426.1.9",
          "form": "상상만해도 귀여워요ㅋㅋㅋㅋ 4살이라니",
          "original_form": "상상만해도 귀여워요ㅋㅋㅋㅋ 4살이라니",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042426.1.10",
          "form": "다른 한번은 언제 가셨어요?",
          "original_form": "다른 한번은 언제 가셨어요?",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042426.1.11",
          "form": "4년전?이었던것 같아요 그래도 비교적최근!",
          "original_form": "4년전?이었던것 같아요 그래도 비교적최근!",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042426.1.12",
          "form": "오오 어땠어요?",
          "original_form": "오오 어땠어요?",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042426.1.13",
          "form": "해산물 좋아하시면 시장 꼭 가세요... 저 그냥 맨날딱새우먹음.....",
          "original_form": "해산물 좋아하시면 시장 꼭 가세요... 저 그냥 맨날딱새우먹음.....",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042426.1.14",
          "form": "말고기도 첨먹어봤는데 짱 맛있었어요",
          "original_form": "말고기도 첨먹어봤는데 짱 맛있었어요",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042426.1.15",
          "form": "말고기! 저 본적도 없어요 ㅋㅋㅋㅋ",
          "original_form": "말고기! 저 본적도 없어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042426.1.16",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042426.1.17",
          "form": "어떤맛이에요?",
          "original_form": "어떤맛이에요?",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042426.1.18",
          "form": "너무오래돼서 기억이 잘안나기는한데...",
          "original_form": "너무오래돼서 기억이 잘안나기는한데...",
          "speaker_id": "1",
          "time": "20210907 08:22"
        },
        {
          "id": "MDRW2100042426.1.19",
          "form": "하나도 안질기고 맛있었다는것밖엔...ㅋㅋ",
          "original_form": "하나도 안질기고 맛있었다는것밖엔...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:22"
        },
        {
          "id": "MDRW2100042426.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 근육이 도드라져서 조금 질길거라고 생각했는데 아닌가봐요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 근육이 도드라져서 조금 질길거라고 생각했는데 아닌가봐요",
          "speaker_id": "2",
          "time": "20210907 10:53"
        },
        {
          "id": "MDRW2100042426.1.21",
          "form": "저도 그렇게 생각했는데 되게 의외였어요",
          "original_form": "저도 그렇게 생각했는데 되게 의외였어요",
          "speaker_id": "1",
          "time": "20210907 11:54"
        }
      ]
