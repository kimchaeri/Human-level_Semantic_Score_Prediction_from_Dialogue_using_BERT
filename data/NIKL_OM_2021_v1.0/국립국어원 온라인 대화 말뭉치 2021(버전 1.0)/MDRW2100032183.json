      "utterance": [
        {
          "id": "MDRW2100032183.1.1",
          "form": "name2은 커피 드시나요?",
          "original_form": "&name2&은 커피 드시나요?",
          "speaker_id": "1",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100032183.1.2",
          "form": "네 아주 많이요 ㅋㅋㅋㅋ",
          "original_form": "네 아주 많이요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032183.1.3",
          "form": "하루네잔?",
          "original_form": "하루네잔?",
          "speaker_id": "2",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032183.1.4",
          "form": "와 진짜 많이 드시네요 ㅋㅋㅋ 저도 좋아하긴 하는데",
          "original_form": "와 진짜 많이 드시네요 ㅋㅋㅋ 저도 좋아하긴 하는데",
          "speaker_id": "1",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032183.1.5",
          "form": "집에서도 내려드시나요?",
          "original_form": "집에서도 내려드시나요?",
          "speaker_id": "1",
          "time": "20210905 13:53"
        },
        {
          "id": "MDRW2100032183.1.6",
          "form": "원두사서 갈아서 내려마시다가",
          "original_form": "원두사서 갈아서 내려마시다가",
          "speaker_id": "2",
          "time": "20210905 13:53"
        },
        {
          "id": "MDRW2100032183.1.7",
          "form": "귀찮고 여름되서",
          "original_form": "귀찮고 여름되서",
          "speaker_id": "2",
          "time": "20210905 13:53"
        },
        {
          "id": "MDRW2100032183.1.8",
          "form": "머신으로 먹다가",
          "original_form": "머신으로 먹다가",
          "speaker_id": "2",
          "time": "20210905 13:53"
        },
        {
          "id": "MDRW2100032183.1.9",
          "form": "그냥 아이스커피 타먹다가",
          "original_form": "그냥 아이스커피 타먹다가",
          "speaker_id": "2",
          "time": "20210905 13:53"
        },
        {
          "id": "MDRW2100032183.1.10",
          "form": "요즘엔 콜드브루에 빠져있어요",
          "original_form": "요즘엔 콜드브루에 빠져있어요",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032183.1.11",
          "form": "ㅎㅎ 다양하게 드시네요",
          "original_form": "ㅎㅎ 다양하게 드시네요",
          "speaker_id": "1",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032183.1.12",
          "form": "커피값이 만만찮게 들겠어요",
          "original_form": "커피값이 만만찮게 들겠어요",
          "speaker_id": "1",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032183.1.13",
          "form": "동생이 사놔서",
          "original_form": "동생이 사놔서",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032183.1.14",
          "form": "저는 그냥 먹기만",
          "original_form": "저는 그냥 먹기만",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032183.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032183.1.16",
          "form": "오 꿀이네요 ㅋㅋㅋ",
          "original_form": "오 꿀이네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100032183.1.17",
          "form": "원두 선호하시는거 있나요?",
          "original_form": "원두 선호하시는거 있나요?",
          "speaker_id": "1",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100032183.1.18",
          "form": "저는 우선",
          "original_form": "저는 우선",
          "speaker_id": "2",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100032183.1.19",
          "form": "신맛이 적어야해요",
          "original_form": "신맛이 적어야해요",
          "speaker_id": "2",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100032183.1.20",
          "form": "신맛 강하면 싫더라구요",
          "original_form": "신맛 강하면 싫더라구요",
          "speaker_id": "2",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100032183.1.21",
          "form": "아 묵직한거",
          "original_form": "아 묵직한거",
          "speaker_id": "1",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100032183.1.22",
          "form": "예가체프 싫어하시겠어여 ㅋㅋ",
          "original_form": "예가체프 싫어하시겠어여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:56"
        },
        {
          "id": "MDRW2100032183.1.23",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032183.1.24",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032183.1.25",
          "form": "예전엔 그냥 주는대로 먹었는데",
          "original_form": "예전엔 그냥 주는대로 먹었는데",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032183.1.26",
          "form": "먹다보면 호불호가 생기죠 ㅎㅎ",
          "original_form": "먹다보면 호불호가 생기죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100032183.1.27",
          "form": "여친하고 데이트하다가",
          "original_form": "여친하고 데이트하다가",
          "speaker_id": "2",
          "time": "20210905 14:09"
        },
        {
          "id": "MDRW2100032183.1.28",
          "form": "커피를 알게된",
          "original_form": "커피를 알게된",
          "speaker_id": "2",
          "time": "20210905 14:09"
        },
        {
          "id": "MDRW2100032183.1.29",
          "form": "아하 ㅋㅋ 전 요즘 핸드드립 세트 사려고 보는 중인데 너무 귀찮네요 ㅋㅋ",
          "original_form": "아하 ㅋㅋ 전 요즘 핸드드립 세트 사려고 보는 중인데 너무 귀찮네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:23"
        },
        {
          "id": "MDRW2100032183.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ 저희집도 그거 세트있는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저희집도 그거 세트있는데",
          "speaker_id": "2",
          "time": "20210905 15:43"
        },
        {
          "id": "MDRW2100032183.1.31",
          "form": "잘 쓰시나요? ㅋㅋ",
          "original_form": "잘 쓰시나요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032183.1.32",
          "form": "한참 쓰다가 최근엔 잘 안써요ㅋㅋㅋ",
          "original_form": "한참 쓰다가 최근엔 잘 안써요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:53"
        },
        {
          "id": "MDRW2100032183.1.33",
          "form": "그쵸 그게 나중엔 안 써서 참 ㅋㅋㅋ 저도 두번째 사는 거라 고민중이에요",
          "original_form": "그쵸 그게 나중엔 안 써서 참 ㅋㅋㅋ 저도 두번째 사는 거라 고민중이에요",
          "speaker_id": "1",
          "time": "20210905 15:54"
        },
        {
          "id": "MDRW2100032183.1.34",
          "form": "실컷쓰다가 귀찮아지니까  ㅋㅋ",
          "original_form": "실컷쓰다가 귀찮아지니까  ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:55"
        }
      ]
