        },
        {
          "id": "MDRW2100045880.1.2",
          "form": "아침드셨나요?",
          "original_form": "아침드셨나요?",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045880.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045880.1.4",
          "form": "아니요 안 먹었어요 님은요?",
          "original_form": "아니요 안 먹었어요 님은요?",
          "speaker_id": "2",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045880.1.5",
          "form": "저도 아직 안먹었어요",
          "original_form": "저도 아직 안먹었어요",
          "speaker_id": "1",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045880.1.6",
          "form": "원래 아침 안드시나요?",
          "original_form": "원래 아침 안드시나요?",
          "speaker_id": "1",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045880.1.7",
          "form": "네 잘 챙기지 않고 점심 먹어요",
          "original_form": "네 잘 챙기지 않고 점심 먹어요",
          "speaker_id": "2",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100045880.1.8",
          "form": "점심은 뭐 드실건가요?",
          "original_form": "점심은 뭐 드실건가요?",
          "speaker_id": "2",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100045880.1.9",
          "form": "아직 안정했어요 ㅋㅋㅋ",
          "original_form": "아직 안정했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045880.1.10",
          "form": "점심메뉴 정하셨나요?",
          "original_form": "점심메뉴 정하셨나요?",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045880.1.11",
          "form": "저도 아직요",
          "original_form": "저도 아직요",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045880.1.12",
          "form": "샌드위치나 그냥 라면 먹을까싶어요",
          "original_form": "샌드위치나 그냥 라면 먹을까싶어요",
          "speaker_id": "2",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100045880.1.13",
          "form": "샌드위치 무난하죠",
          "original_form": "샌드위치 무난하죠",
          "speaker_id": "1",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100045880.1.14",
          "form": "네 간단하게 먹을수 있어요",
          "original_form": "네 간단하게 먹을수 있어요",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045880.1.15",
          "form": "좋아하는 음식 있나요?",
          "original_form": "좋아하는 음식 있나요?",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045880.1.16",
          "form": "만들어 드실건가요?",
          "original_form": "만들어 드실건가요?",
          "speaker_id": "1",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045880.1.17",
          "form": "아니요 편의점 가서 사먹을까싶어요",
          "original_form": "아니요 편의점 가서 사먹을까싶어요",
          "speaker_id": "2",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045880.1.18",
          "form": "네 저 초밥 좋아해요",
          "original_form": "네 저 초밥 좋아해요",
          "speaker_id": "1",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045880.1.19",
          "form": "저도 초밥 좋아해요 어떤 초밥 좋아하세요?",
          "original_form": "저도 초밥 좋아해요 어떤 초밥 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100045880.1.20",
          "form": "저 단새우 초밥이요!",
          "original_form": "저 단새우 초밥이요!",
          "speaker_id": "1",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100045880.1.21",
          "form": "저도 새우초밥이요 참치나 광어도 좋아해요",
          "original_form": "저도 새우초밥이요 참치나 광어도 좋아해요",
          "speaker_id": "2",
          "time": "20210908 08:40"
        },
        {
          "id": "MDRW2100045880.1.22",
          "form": "참치랑 광어 맛있죠",
          "original_form": "참치랑 광어 맛있죠",
          "speaker_id": "1",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100045880.1.23",
          "form": "해산물 좋아하세요?",
          "original_form": "해산물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 08:42"
        },
        {
          "id": "MDRW2100045880.1.24",
          "form": "아니요 딱히 좋아하는건 아니에요 새우나 낙지 오징어정도요",
          "original_form": "아니요 딱히 좋아하는건 아니에요 새우나 낙지 오징어정도요",
