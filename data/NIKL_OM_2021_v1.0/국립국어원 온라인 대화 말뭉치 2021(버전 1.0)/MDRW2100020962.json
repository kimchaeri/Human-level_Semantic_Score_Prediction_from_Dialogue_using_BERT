      "utterance": [
        {
          "id": "MDRW2100020962.1.1",
          "form": "여행 가고 싶어 죽겠어요!!ㅜㅜ",
          "original_form": "여행 가고 싶어 죽겠어요!!ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100020962.1.2",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210828 14:34"
        },
        {
          "id": "MDRW2100020962.1.3",
          "form": "코로나땜에 ㅜㅜㅜ",
          "original_form": "코로나땜에 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:34"
        },
        {
          "id": "MDRW2100020962.1.4",
          "form": "마지막 여행 어디가셨어요?",
          "original_form": "마지막 여행 어디가셨어요?",
          "speaker_id": "1",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020962.1.5",
          "form": "기억이 잘 안나지만 제주도였던 것 같아요 ㅎㅎ",
          "original_form": "기억이 잘 안나지만 제주도였던 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100020962.1.6",
          "form": "그때가 아마 코로나 이전!",
          "original_form": "그때가 아마 코로나 이전!",
          "speaker_id": "2",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100020962.1.7",
          "form": "제주도 좋죠",
          "original_form": "제주도 좋죠",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020962.1.8",
          "form": "얼마나 다녀오셨어요?",
          "original_form": "얼마나 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020962.1.9",
          "form": "2박3일 아니면 3박4일이었던 것 같아요",
          "original_form": "2박3일 아니면 3박4일이었던 것 같아요",
          "speaker_id": "2",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100020962.1.10",
          "form": "제주도가 생각보다 넓어서 동부/서부인가? 남부/북부 나눠서 여행한다네요?",
          "original_form": "제주도가 생각보다 넓어서 동부/서부인가? 남부/북부 나눠서 여행한다네요?",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020962.1.11",
          "form": "그쵸 간김에 여러군데 다 보고 와야죠",
          "original_form": "그쵸 간김에 여러군데 다 보고 와야죠",
          "speaker_id": "1",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020962.1.12",
          "form": "ㅋㅋㅋ다 볼 수가 없더라구요",
          "original_form": "ㅋㅋㅋ다 볼 수가 없더라구요",
          "speaker_id": "2",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100020962.1.13",
          "form": "그래서 다음에 제주도 가면 나머지 한쪽도 보려구요",
          "original_form": "그래서 다음에 제주도 가면 나머지 한쪽도 보려구요",
          "speaker_id": "2",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100020962.1.14",
          "form": "ㅎㅎ 코로나가 얼른 끝나야될텐데요ㅜ",
          "original_form": "ㅎㅎ 코로나가 얼른 끝나야될텐데요ㅜ",
          "speaker_id": "1",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100020962.1.15",
          "form": "10년 전에 갔을 때랑, 5년 전에 갔을 때랑 최근에 갈때랑",
          "original_form": "10년 전에 갔을 때랑, 5년 전에 갔을 때랑 최근에 갈때랑",
          "speaker_id": "2",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100020962.1.16",
          "form": "저도 끝나면 여행가고싶어요",
          "original_form": "저도 끝나면 여행가고싶어요",
          "speaker_id": "1",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100020962.1.17",
          "form": "제주도는 항상 새롭더라구요",
          "original_form": "제주도는 항상 새롭더라구요",
          "speaker_id": "2",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100020962.1.18",
          "form": "시설 많이 짓고 관광 개발해서 그런가?",
          "original_form": "시설 많이 짓고 관광 개발해서 그런가?",
          "speaker_id": "2",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100020962.1.19",
          "form": "맞아요 갈때마다",
          "original_form": "맞아요 갈때마다",
          "speaker_id": "1",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100020962.1.20",
          "form": "달라져요",
          "original_form": "달라져요",
          "speaker_id": "1",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100020962.1.21",
          "form": "그래서 계속갈만한거같아요",
          "original_form": "그래서 계속갈만한거같아요",
          "speaker_id": "1",
          "time": "20210828 14:49"
        }
      ]
