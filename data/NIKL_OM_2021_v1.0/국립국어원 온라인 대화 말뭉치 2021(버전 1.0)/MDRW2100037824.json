      "utterance": [
        {
          "id": "MDRW2100037824.1.1",
          "form": "오늘 아침에 날씨 너무 춥더라구요 ㅜㅜ",
          "original_form": "오늘 아침에 날씨 너무 춥더라구요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100037824.1.2",
          "form": "그쵸 어제 비가와서 그런가",
          "original_form": "그쵸 어제 비가와서 그런가",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100037824.1.3",
          "form": "오늘 좀 따뜻하게 입고왔어요 그래서 ㅋㅋ",
          "original_form": "오늘 좀 따뜻하게 입고왔어요 그래서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100037824.1.4",
          "form": "요즘같은 날씨에 감기걸리기 쉬울듯해요",
          "original_form": "요즘같은 날씨에 감기걸리기 쉬울듯해요",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100037824.1.5",
          "form": "맞아요 감기 조심해야될 것 같아요..",
          "original_form": "맞아요 감기 조심해야될 것 같아요..",
          "speaker_id": "1",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100037824.1.6",
          "form": "오늘도 원래 비온다고 했는데 비는 안오고 쌀쌀하네요",
          "original_form": "오늘도 원래 비온다고 했는데 비는 안오고 쌀쌀하네요",
          "speaker_id": "1",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100037824.1.7",
          "form": "그쵸 그래서 우산 챙겨왔는데",
          "original_form": "그쵸 그래서 우산 챙겨왔는데",
          "speaker_id": "2",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100037824.1.8",
          "form": "무겁기만 하구 비는 안 오네요 ㅋㅋ",
          "original_form": "무겁기만 하구 비는 안 오네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100037824.1.9",
          "form": "그러니까요ㅋㅋ 또 다음주부터 비온다는데",
          "original_form": "그러니까요ㅋㅋ 또 다음주부터 비온다는데",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037824.1.10",
          "form": "우산 필수로 챙겨야겠어요...",
          "original_form": "우산 필수로 챙겨야겠어요...",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037824.1.11",
          "form": "여름 다 지나서 ㅋㅋㅋ 무슨 일이래요ㅠㅠ",
          "original_form": "여름 다 지나서 ㅋㅋㅋ 무슨 일이래요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100037824.1.12",
          "form": "이제는 비 오면 써늘하네요",
          "original_form": "이제는 비 오면 써늘하네요",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100037824.1.13",
          "form": "그러게요 ..ㅠㅠ 진짜 가을날씨예요 ..",
          "original_form": "그러게요 ..ㅠㅠ 진짜 가을날씨예요 ..",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037824.1.14",
          "form": "근데 또 낮엔 더워서 옷입기 지인짜 애매하더라구요",
          "original_form": "근데 또 낮엔 더워서 옷입기 지인짜 애매하더라구요",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037824.1.15",
          "form": "그냥 긴팔셔츠 위주로 입고더니긴 하지만요",
          "original_form": "그냥 긴팔셔츠 위주로 입고더니긴 하지만요",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100037824.1.16",
          "form": "참 요즘에 날씨가 옷입기 참 애매해요..",
          "original_form": "참 요즘에 날씨가 옷입기 참 애매해요..",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100037824.1.17",
          "form": "저는 원피스도 좋아하는데 비오면 바람불고 해서",
          "original_form": "저는 원피스도 좋아하는데 비오면 바람불고 해서",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037824.1.18",
          "form": "원피스는 잘 못입더라구요",
          "original_form": "원피스는 잘 못입더라구요",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037824.1.19",
          "form": "그러니까 선택의 폭이 확 줄어요 ㅋㅋ",
          "original_form": "그러니까 선택의 폭이 확 줄어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037824.1.20",
          "form": "오 맞아요... 진짜 잘못입으면 하루종일 잡고있어야하는 ...",
          "original_form": "오 맞아요... 진짜 잘못입으면 하루종일 잡고있어야하는 ...",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100037824.1.21",
          "form": "반팔입으려면 겉옷도 챙겨야하는 날씨네요 ..",
          "original_form": "반팔입으려면 겉옷도 챙겨야하는 날씨네요 ..",
          "speaker_id": "1",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100037824.1.22",
          "form": "ㅋㅋㅋ그니까요 진짜 까다롭죠",
          "original_form": "ㅋㅋㅋ그니까요 진짜 까다롭죠",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100037824.1.23",
          "form": "요맘때쯤이 항상 옷 입기 번거로웠던 더 같아요",
          "original_form": "요맘때쯤이 항상 옷 입기 번거로웠던 더 같아요",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100037824.1.24",
          "form": "얼른 쫌만 더 선선해졌으면 좋겠어요 ㅋㅋ",
          "original_form": "얼른 쫌만 더 선선해졌으면 좋겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100037824.1.25",
          "form": "그러게요ㅋㅋㅋ 딱 외투입을 수 있게요!",
          "original_form": "그러게요ㅋㅋㅋ 딱 외투입을 수 있게요!",
          "speaker_id": "1",
          "time": "20210901 22:55"
        }
      ]
    }
