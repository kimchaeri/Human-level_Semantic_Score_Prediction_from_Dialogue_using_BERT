        {
          "id": "MDRW2100046193.1.1",
          "form": "점심은 뭐드세요?",
          "original_form": "점심은 뭐드세요?",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046193.1.2",
          "form": "구내식당이요",
          "original_form": "구내식당이요",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046193.1.3",
          "form": "오늘의 메뉴는?",
          "original_form": "오늘의 메뉴는?",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046193.1.4",
          "form": "오늘은 안동찜닭이래요 ㅎㅎ",
          "original_form": "오늘은 안동찜닭이래요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046193.1.5",
          "form": "오 ㅎ 찜닭좋아하는데",
          "original_form": "오 ㅎ 찜닭좋아하는데",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046193.1.6",
          "form": "메뉴정하셯머요?",
          "original_form": "메뉴정하셯머요?",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046193.1.7",
          "form": "맛나겠어요ㅎ",
          "original_form": "맛나겠어요ㅎ",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046193.1.8",
          "form": "전아직이요",
          "original_form": "전아직이요",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046193.1.9",
          "form": "기대되요 ㅋ",
          "original_form": "기대되요 ㅋ",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046193.1.10",
          "form": "메뉴정하는것도 일이예요",
          "original_form": "메뉴정하는것도 일이예요",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046193.1.11",
          "form": "글쵸",
          "original_form": "글쵸",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046193.1.12",
          "form": "쭈꾸미나 명태조림 고민중ㅋ",
          "original_form": "쭈꾸미나 명태조림 고민중ㅋ",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046193.1.13",
          "form": "저는 그래서 구내식당이 좋아요",
          "original_form": "저는 그래서 구내식당이 좋아요",
