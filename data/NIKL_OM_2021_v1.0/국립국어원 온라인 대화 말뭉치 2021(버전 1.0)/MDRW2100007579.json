      "utterance": [
        {
          "id": "MDRW2100007579.1.1",
          "form": "아 그렇겠네요",
          "original_form": "아 그렇겠네요",
          "speaker_id": "2",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007579.1.2",
          "form": "생각보다 까다롭더라고요",
          "original_form": "생각보다 까다롭더라고요",
          "speaker_id": "2",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007579.1.3",
          "form": "애기가졌을 때 보건소에 신청하려고하니",
          "original_form": "애기가졌을 때 보건소에 신청하려고하니",
          "speaker_id": "2",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007579.1.4",
          "form": "저는 지원 해당안되요 ㅎㅎ",
          "original_form": "저는 지원 해당안되요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007579.1.5",
          "form": "이건뭐 주겠다고 해놓은 정책인가",
          "original_form": "이건뭐 주겠다고 해놓은 정책인가",
          "speaker_id": "2",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007579.1.6",
          "form": "주는척만하는정책인가 싶었어요",
          "original_form": "주는척만하는정책인가 싶었어요",
          "speaker_id": "2",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007579.1.7",
          "form": "잘 사는것도 아닌데 애매한ㅠ",
          "original_form": "잘 사는것도 아닌데 애매한ㅠ",
          "speaker_id": "2",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007579.1.8",
          "form": "제가 다 내고 쓰고 있어요",
          "original_form": "제가 다 내고 쓰고 있어요",
          "speaker_id": "1",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007579.1.9",
          "form": "혹시 얼마정도내요?",
          "original_form": "혹시 얼마정도내요?",
          "speaker_id": "2",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007579.1.10",
          "form": "한달에",
          "original_form": "한달에",
          "speaker_id": "2",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007579.1.11",
          "form": "그정도 부르려면 얼마정도내나 싶어서요",
          "original_form": "그정도 부르려면 얼마정도내나 싶어서요",
          "speaker_id": "2",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007579.1.12",
          "form": "그래도 밖에 업체보다 싼거같아요",
          "original_form": "그래도 밖에 업체보다 싼거같아요",
          "speaker_id": "1",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007579.1.13",
          "form": "여긴 시간당이예요",
          "original_form": "여긴 시간당이예요",
          "speaker_id": "1",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007579.1.14",
          "form": "한시간에 9800원대예요",
          "original_form": "한시간에 9800원대예요",
          "speaker_id": "1",
          "time": "20210822 03:31"
        },
        {
          "id": "MDRW2100007579.1.15",
          "form": "아 그렇구나 제가 도시에서 시골로 이사와서 좀 불편한거같아요",
          "original_form": "아 그렇구나 제가 도시에서 시골로 이사와서 좀 불편한거같아요",
          "speaker_id": "2",
          "time": "20210822 03:31"
        },
        {
          "id": "MDRW2100007579.1.16",
          "form": "교통비도 줘야되고ㅠ",
          "original_form": "교통비도 줘야되고ㅠ",
          "speaker_id": "2",
          "time": "20210822 03:31"
        },
        {
          "id": "MDRW2100007579.1.17",
          "form": "남편 직장때문에 집을 옮겼거든요",
          "original_form": "남편 직장때문에 집을 옮겼거든요",
          "speaker_id": "2",
          "time": "20210822 03:31"
        },
        {
          "id": "MDRW2100007579.1.18",
          "form": "남편은 직급있고 좋은데라",
          "original_form": "남편은 직급있고 좋은데라",
          "speaker_id": "2",
          "time": "20210822 03:32"
        },
        {
          "id": "MDRW2100007579.1.19",
          "form": "또 이건 정부에서 관리하다보니 돌봄선생님들 정기적으로 교육도 시키고 나름 관리하더라구요",
          "original_form": "또 이건 정부에서 관리하다보니 돌봄선생님들 정기적으로 교육도 시키고 나름 관리하더라구요",
          "speaker_id": "1",
          "time": "20210822 03:32"
        },
        {
          "id": "MDRW2100007579.1.20",
          "form": "7년뒤에는 더 좋아졌길 바래요",
          "original_form": "7년뒤에는 더 좋아졌길 바래요",
          "speaker_id": "2",
          "time": "20210822 03:32"
        },
        {
          "id": "MDRW2100007579.1.21",
          "form": "아 시골이시면 많은 불편함이 있으시겠어요",
          "original_form": "아 시골이시면 많은 불편함이 있으시겠어요",
          "speaker_id": "1",
          "time": "20210822 03:32"
        },
        {
          "id": "MDRW2100007579.1.22",
          "form": "네 둘다 차가 있어서 괜찮은데",
          "original_form": "네 둘다 차가 있어서 괜찮은데",
          "speaker_id": "2",
          "time": "20210822 03:32"
        },
        {
          "id": "MDRW2100007579.1.23",
          "form": "제가 이직하게되면 출퇴근시간이 길어진다는게",
          "original_form": "제가 이직하게되면 출퇴근시간이 길어진다는게",
          "speaker_id": "2",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007579.1.24",
          "form": "지금은 차로3분거리거든요ㅋㅋ",
          "original_form": "지금은 차로3분거리거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007579.1.25",
          "form": "이직하면 도시로나가게되서 1시간거리인데 출퇴근시간에는 왕복3시간..",
          "original_form": "이직하면 도시로나가게되서 1시간거리인데 출퇴근시간에는 왕복3시간..",
          "speaker_id": "2",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007579.1.26",
          "form": "에고... 오가다가 지치시겠어요",
          "original_form": "에고... 오가다가 지치시겠어요",
          "speaker_id": "1",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007579.1.27",
          "form": "어마어마하죠 3분에서 3시간은",
          "original_form": "어마어마하죠 3분에서 3시간은",
          "speaker_id": "2",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007579.1.28",
          "form": "쉬운결정은 아니네요",
          "original_form": "쉬운결정은 아니네요",
          "speaker_id": "1",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007579.1.29",
          "form": "지금회사에서 좋은점은 5시퇴근이죠",
          "original_form": "지금회사에서 좋은점은 5시퇴근이죠",
          "speaker_id": "2",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007579.1.30",
          "form": "차이가 많아서",
          "original_form": "차이가 많아서",
          "speaker_id": "1",
          "time": "20210822 03:34"
        },
        {
          "id": "MDRW2100007579.1.31",
          "form": "거리도가까운데다가",
          "original_form": "거리도가까운데다가",
          "speaker_id": "2",
          "time": "20210822 03:34"
        },
        {
          "id": "MDRW2100007579.1.32",
          "form": "유류비가 덜 든다?",
          "original_form": "유류비가 덜 든다?",
          "speaker_id": "2",
          "time": "20210822 03:34"
        },
        {
          "id": "MDRW2100007579.1.33",
          "form": "이정도!",
          "original_form": "이정도!",
          "speaker_id": "2",
          "time": "20210822 03:34"
        },
        {
          "id": "MDRW2100007579.1.34",
          "form": "교통체증없다",
          "original_form": "교통체증없다",
          "speaker_id": "1",
          "time": "20210822 03:34"
        },
        {
          "id": "MDRW2100007579.1.35",
          "form": "도 포함 ㅎㅎ되겠네요",
          "original_form": "도 포함 ㅎㅎ되겠네요",
          "speaker_id": "1",
          "time": "20210822 03:34"
        },
        {
          "id": "MDRW2100007579.1.36",
          "form": "맞아요 시골이라서 \r\n근데 군에서 정책을 잘해놔서 도로옆주차나 개구리주차같은게 전혀없어요",
          "original_form": "맞아요 시골이라서 \r\n근데 군에서 정책을 잘해놔서 도로옆주차나 개구리주차같은게 전혀없어요",
          "speaker_id": "2",
          "time": "20210822 03:35"
        },
        {
          "id": "MDRW2100007579.1.37",
          "form": "도시에비해서 차도없는데 저런질서도 잘되있어서",
          "original_form": "도시에비해서 차도없는데 저런질서도 잘되있어서",
          "speaker_id": "2",
          "time": "20210822 03:35"
        },
        {
          "id": "MDRW2100007579.1.38",
          "form": "도시보다 육아관련된 돈 주는건 더 있는거같고",
          "original_form": "도시보다 육아관련된 돈 주는건 더 있는거같고",
          "speaker_id": "2",
          "time": "20210822 03:35"
        },
        {
          "id": "MDRW2100007579.1.39",
          "form": "혹시 시댁에 전화하시나요?",
          "original_form": "혹시 시댁에 전화하시나요?",
          "speaker_id": "2",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007579.1.40",
          "form": "애어릴땐 자연과 프리하게 키우시고",
          "original_form": "애어릴땐 자연과 프리하게 키우시고",
          "speaker_id": "1",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007579.1.41",
          "form": "그런거원하는집이 좀 있는것같아요!",
          "original_form": "그런거원하는집이 좀 있는것같아요!",
          "speaker_id": "2",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007579.1.42",
          "form": "차후엔 델고나오셔요 ㅎㅎ",
          "original_form": "차후엔 델고나오셔요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007579.1.43",
          "form": "잘안해요 ㅋㅋ",
          "original_form": "잘안해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007579.1.44",
          "form": "네 저도 애기 중학생부터는 도시로나오려고요",
          "original_form": "네 저도 애기 중학생부터는 도시로나오려고요",
          "speaker_id": "2",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007579.1.45",
          "form": "저는 애기가 시댁으로가니 자동적으로 매일보는데",
          "original_form": "저는 애기가 시댁으로가니 자동적으로 매일보는데",
          "speaker_id": "2",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007579.1.46",
          "form": "주말마다 가는게 쉽지않더라고요ㅡㅠ",
          "original_form": "주말마다 가는게 쉽지않더라고요ㅡㅠ",
          "speaker_id": "2",
          "time": "20210822 03:36"
        },
        {
          "id": "MDRW2100007579.1.47",
          "form": "처음엔 바라시던데 안하니까",
          "original_form": "처음엔 바라시던데 안하니까",
          "speaker_id": "1",
          "time": "20210822 03:37"
        }
      ]
    }
