      "utterance": [
        {
          "id": "MDRW2100015285.1.1",
          "form": "제출 빠릿하시네요",
          "original_form": "제출 빠릿하시네요",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015285.1.2",
          "form": "군대 돌아왔어요",
          "original_form": "군대 돌아왔어요",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015285.1.3",
          "form": "군대 얘기 할말 있았어요?",
          "original_form": "군대 얘기 할말 있았어요?",
          "speaker_id": "2",
          "time": "20210828 12:01"
        },
        {
          "id": "MDRW2100015285.1.4",
          "form": "아니요?",
          "original_form": "아니요?",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100015285.1.5",
          "form": "음 .. 군대이야기했는ㄷㅔ",
          "original_form": "음 .. 군대이야기했는ㄷㅔ",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100015285.1.6",
          "form": "군대 이야기로 돌아왔네요",
          "original_form": "군대 이야기로 돌아왔네요",
          "speaker_id": "1",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100015285.1.7",
          "form": "디피 보셨나요",
          "original_form": "디피 보셨나요",
          "speaker_id": "1",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100015285.1.8",
          "form": "아니용 ㅋㅋㅋㅋ봤어요?",
          "original_form": "아니용 ㅋㅋㅋㅋ봤어요?",
          "speaker_id": "2",
          "time": "20210903 09:24"
        },
        {
          "id": "MDRW2100015285.1.9",
          "form": "틀었다가 제스탈 아니라서 껐는데",
          "original_form": "틀었다가 제스탈 아니라서 껐는데",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100015285.1.10",
          "form": "회사사람들은 다 밤새서 봤대요",
          "original_form": "회사사람들은 다 밤새서 봤대요",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100015285.1.11",
          "form": "남자분들 ㅋㅋ",
          "original_form": "남자분들 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100015285.1.12",
          "form": "나두 군대얘기 뭐 재밌겠나 싶어서 안보려고했는데",
          "original_form": "나두 군대얘기 뭐 재밌겠나 싶어서 안보려고했는데",
          "speaker_id": "2",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100015285.1.13",
          "form": "반응이 너무 좋아서 한번 보려고용",
          "original_form": "반응이 너무 좋아서 한번 보려고용",
          "speaker_id": "2",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100015285.1.14",
          "form": "그쵸 반응이 좋더라고요",
          "original_form": "그쵸 반응이 좋더라고요",
          "speaker_id": "1",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100015285.1.15",
          "form": "저는 껐어요 키키",
          "original_form": "저는 껐어요 키키",
          "speaker_id": "1",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100015285.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100015285.1.17",
          "form": "말랑말랑한거 좋아하시쟈나여",
          "original_form": "말랑말랑한거 좋아하시쟈나여",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100015285.1.18",
          "form": "마자여 역시 절 잘 아시네여",
          "original_form": "마자여 역시 절 잘 아시네여",
          "speaker_id": "1",
          "time": "20210903 14:49"
        },
        {
          "id": "MDRW2100015285.1.19",
          "form": "또 뭐보고싶으세요",
          "original_form": "또 뭐보고싶으세요",
          "speaker_id": "1",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100015285.1.20",
          "form": "흠 새로 개봉한 마블",
          "original_form": "흠 새로 개봉한 마블",
          "speaker_id": "2",
          "time": "20210903 15:10"
        },
        {
          "id": "MDRW2100015285.1.21",
          "form": "샹치와 텐링즈",
          "original_form": "샹치와 텐링즈",
          "speaker_id": "2",
          "time": "20210903 15:10"
        },
        {
          "id": "MDRW2100015285.1.22",
          "form": "인데 군인 얘기아니니까 나중에 할게요",
          "original_form": "인데 군인 얘기아니니까 나중에 할게요",
          "speaker_id": "2",
          "time": "20210903 15:10"
        },
        {
          "id": "MDRW2100015285.1.23",
          "form": "아 나도 샹치",
          "original_form": "아 나도 샹치",
          "speaker_id": "1",
          "time": "20210903 15:12"
        },
        {
          "id": "MDRW2100015285.1.24",
          "form": "군대 탈영병 이야기더라규요 디피",
          "original_form": "군대 탈영병 이야기더라규요 디피",
          "speaker_id": "1",
          "time": "20210903 15:12"
        },
        {
          "id": "MDRW2100015285.1.25",
          "form": "좀 현실고증ㅇ이 잘돼서 험악해요",
          "original_form": "좀 현실고증ㅇ이 잘돼서 험악해요",
          "speaker_id": "1",
          "time": "20210903 15:12"
        },
        {
          "id": "MDRW2100015285.1.26",
          "form": "ㅋㅋㅋㅋ아 탈영병 얘기라니 뭔가 마음아프기도 하네여",
          "original_form": "ㅋㅋㅋㅋ아 탈영병 얘기라니 뭔가 마음아프기도 하네여",
