      "utterance": [
        {
          "id": "MDRW2100040782.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210905 06:54"
        },
        {
          "id": "MDRW2100040782.1.2",
          "form": "네 안녕하세요 요즘 많이 시원해졌어요",
          "original_form": "네 안녕하세요 요즘 많이 시원해졌어요",
          "speaker_id": "2",
          "time": "20210905 06:54"
        },
        {
          "id": "MDRW2100040782.1.3",
          "form": "네 정말 가을날씨예요",
          "original_form": "네 정말 가을날씨예요",
          "speaker_id": "1",
          "time": "20210905 06:55"
        },
        {
          "id": "MDRW2100040782.1.4",
          "form": "하늘도 파랗고",
          "original_form": "하늘도 파랗고",
          "speaker_id": "1",
          "time": "20210905 06:55"
        },
        {
          "id": "MDRW2100040782.1.5",
          "form": "네 태풍은 안 오면 좋겠어요",
          "original_form": "네 태풍은 안 오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210905 06:55"
        },
        {
          "id": "MDRW2100040782.1.6",
          "form": "태풍 소식이 있었나요?",
          "original_form": "태풍 소식이 있었나요?",
          "speaker_id": "1",
          "time": "20210905 06:56"
        },
        {
          "id": "MDRW2100040782.1.7",
          "form": "얼마 전에 온다고 했는데 잘 지나간 거 같아요",
          "original_form": "얼마 전에 온다고 했는데 잘 지나간 거 같아요",
          "speaker_id": "2",
          "time": "20210905 06:56"
        },
        {
          "id": "MDRW2100040782.1.8",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210905 06:57"
        },
        {
          "id": "MDRW2100040782.1.9",
          "form": "태풍 온대서 긴장했는데",
          "original_form": "태풍 온대서 긴장했는데",
          "speaker_id": "1",
          "time": "20210905 06:57"
        },
        {
          "id": "MDRW2100040782.1.10",
          "form": "별일 없이 지나가서 정말 다행이에요",
          "original_form": "별일 없이 지나가서 정말 다행이에요",
          "speaker_id": "2",
          "time": "20210905 06:57"
        },
        {
          "id": "MDRW2100040782.1.11",
          "form": "생각보단 빠르게 별 피해없이 지나갔어요",
          "original_form": "생각보단 빠르게 별 피해없이 지나갔어요",
          "speaker_id": "1",
          "time": "20210905 06:57"
        },
        {
          "id": "MDRW2100040782.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 06:57"
        },
        {
          "id": "MDRW2100040782.1.13",
          "form": "예전에 태풍 매미 혹시 기억하세요?",
          "original_form": "예전에 태풍 매미 혹시 기억하세요?",
          "speaker_id": "2",
          "time": "20210905 06:58"
        },
        {
          "id": "MDRW2100040782.1.14",
          "form": "요 며칠은 비도 안오고 날이 맑아서 좋더라고요",
          "original_form": "요 며칠은 비도 안오고 날이 맑아서 좋더라고요",
          "speaker_id": "1",
          "time": "20210905 06:58"
        },
        {
          "id": "MDRW2100040782.1.15",
          "form": "네 매미 기억하죠!",
          "original_form": "네 매미 기억하죠!",
          "speaker_id": "1",
          "time": "20210905 06:58"
        },
        {
          "id": "MDRW2100040782.1.16",
          "form": "날씨가 좋아졌어요",
          "original_form": "날씨가 좋아졌어요",
          "speaker_id": "2",
          "time": "20210905 06:58"
        },
        {
          "id": "MDRW2100040782.1.17",
          "form": "그때 좀 피해를 봐서요",
          "original_form": "그때 좀 피해를 봐서요",
          "speaker_id": "2",
          "time": "20210905 06:58"
        },
        {
          "id": "MDRW2100040782.1.18",
          "form": "아 그때 태풍 피해를 입으셨구나ㅠ",
          "original_form": "아 그때 태풍 피해를 입으셨구나ㅠ",
          "speaker_id": "1",
          "time": "20210905 06:58"
        },
        {
          "id": "MDRW2100040782.1.19",
          "form": "저희 동네는 비가 많이 와요",
          "original_form": "저희 동네는 비가 많이 와요",
