      "utterance": [
        {
          "id": "MDRW2100035668.1.1",
          "form": "오랜만에",
          "original_form": "오랜만에",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035668.1.2",
          "form": "모스버거가 먹고싶네요ㅋㅋ",
          "original_form": "모스버거가 먹고싶네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035668.1.3",
          "form": "모스버거를?",
          "original_form": "모스버거를?",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035668.1.4",
          "form": "그죠 ㅋㅋ 전 거기 라이스버거도 좋아하구,, 치즈버거도 조 ㅎ아하구,,",
          "original_form": "그죠 ㅋㅋ 전 거기 라이스버거도 좋아하구,, 치즈버거도 조 ㅎ아하구,,",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035668.1.5",
          "form": "버거가 와퍼보다 작잖아요",
          "original_form": "버거가 와퍼보다 작잖아요",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035668.1.6",
          "form": "ㅋㅋ 네 전 그냥 기본버거 먹었던 거 같아요",
          "original_form": "ㅋㅋ 네 전 그냥 기본버거 먹었던 거 같아요",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035668.1.7",
          "form": "거기 토마토소스가 매력적이에요",
          "original_form": "거기 토마토소스가 매력적이에요",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035668.1.8",
          "form": "제가 와퍼세트를 다 못먹는데 거기선 세트다먹어도 살짝 배가 사알짝 부르다 싶은?",
          "original_form": "제가 와퍼세트를 다 못먹는데 거기선 세트다먹어도 살짝 배가 사알짝 부르다 싶은?",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035668.1.9",
          "form": "치즈버거에 들어가는 소스면 저도 좋아해요",
          "original_form": "치즈버거에 들어가는 소스면 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035668.1.10",
          "form": "그쵸 되게 맛있어요",
          "original_form": "그쵸 되게 맛있어요",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035668.1.11",
          "form": "저 원래 토마토소스 안좋아하는데",
          "original_form": "저 원래 토마토소스 안좋아하는데",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035668.1.12",
          "form": "거기껀 먹게되더라구여",
          "original_form": "거기껀 먹게되더라구여",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035668.1.13",
          "form": "고기가 좀 많은 소스 아닌가요? 맛잇었어요",
          "original_form": "고기가 좀 많은 소스 아닌가요? 맛잇었어요",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035668.1.14",
          "form": "치즈버거에 치츠추가해서 먹으면 진짜 맛있는데",
          "original_form": "치즈버거에 치츠추가해서 먹으면 진짜 맛있는데",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035668.1.15",
          "form": "ㅋㅋㅋㅋㅋ아 맛있겠네요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ아 맛있겠네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100035668.1.16",
          "form": "먹고싶어요",
          "original_form": "먹고싶어요",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100035668.1.17",
          "form": "고속버스터미날에 칼국수 집도 있잖아요",
          "original_form": "고속버스터미날에 칼국수 집도 있잖아요",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035668.1.18",
          "form": "전주 칼국수집 분점이요",
          "original_form": "전주 칼국수집 분점이요",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035668.1.19",
          "form": "전 거기도 좋아해요",
          "original_form": "전 거기도 좋아해요",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035668.1.20",
          "form": "고터 모스버거애길 하시길래 혹시 여기도 아시나해서,,",
          "original_form": "고터 모스버거애길 하시길래 혹시 여기도 아시나해서,,",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035668.1.21",
          "form": "거기 베태랑인가 그러지 않아요?",
          "original_form": "거기 베태랑인가 그러지 않아요?",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035668.1.22",
          "form": "저도 거기서 먹어봤오요ㅋㅋㅋㅋㅋ",
          "original_form": "저도 거기서 먹어봤오요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035668.1.23",
          "form": "네 베테랑이요! ㅋㅋㅋㅋㅋ 거기 칼국수 좋아해요",
          "original_form": "네 베테랑이요! ㅋㅋㅋㅋㅋ 거기 칼국수 좋아해요",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035668.1.24",
          "form": "2인이상이 가야 만두도 시킬수 있어서 혼자가면 되게 아쉬워요",
          "original_form": "2인이상이 가야 만두도 시킬수 있어서 혼자가면 되게 아쉬워요",
          "speaker_id": "1",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035668.1.25",
          "form": "ㅋㅋㅋ맞아요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ맞아요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035668.1.26",
          "form": "거기 맛있죠",
          "original_form": "거기 맛있죠",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035668.1.27",
          "form": "전 거게 비빔도 좋아해요",
          "original_form": "전 거게 비빔도 좋아해요",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035668.1.28",
          "form": "ㅋㅋ 비빔이요??",
          "original_form": "ㅋㅋ 비빔이요??",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035668.1.29",
          "form": "쫄면인가요?",
          "original_form": "쫄면인가요?",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035668.1.30",
          "form": "칼국수 면인데 좀 덜매운 빨간 양념에 말아줘요 맛있어요",
          "original_form": "칼국수 면인데 좀 덜매운 빨간 양념에 말아줘요 맛있어요",
          "speaker_id": "1",
          "time": "20210829 22:26"
        }
      ]
