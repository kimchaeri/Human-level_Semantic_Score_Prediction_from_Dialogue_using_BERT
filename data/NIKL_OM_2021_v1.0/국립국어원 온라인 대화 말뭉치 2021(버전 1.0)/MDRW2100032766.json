      "utterance": [
        {
          "id": "MDRW2100032766.1.1",
          "form": "안녕하세여!",
          "original_form": "안녕하세여!",
          "speaker_id": "1",
          "time": "20210905 23:15"
        },
        {
          "id": "MDRW2100032766.1.2",
          "form": "안녕하세요 ~",
          "original_form": "안녕하세요 ~",
          "speaker_id": "2",
          "time": "20210905 23:16"
        },
        {
          "id": "MDRW2100032766.1.3",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 23:16"
        },
        {
          "id": "MDRW2100032766.1.4",
          "form": "네넹 좋아합니다 ! 님은요?",
          "original_form": "네넹 좋아합니다 ! 님은요?",
          "speaker_id": "2",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100032766.1.5",
          "form": "네 저도 좋아해여!",
          "original_form": "네 저도 좋아해여!",
          "speaker_id": "1",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100032766.1.6",
          "form": "어떤 여행지가 제일 좋았나여?",
          "original_form": "어떤 여행지가 제일 좋았나여?",
          "speaker_id": "1",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100032766.1.7",
          "form": "음 저는 남해요! 님은요?",
          "original_form": "음 저는 남해요! 님은요?",
          "speaker_id": "2",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100032766.1.8",
          "form": "음 저는 제주도요 ㅎㅎ",
          "original_form": "음 저는 제주도요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100032766.1.9",
          "form": "남해 어디 가셨어요?",
          "original_form": "남해 어디 가셨어요?",
          "speaker_id": "1",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100032766.1.10",
          "form": "남해 유명한곳은 다가본거같아용 ㅎㅎ",
          "original_form": "남해 유명한곳은 다가본거같아용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100032766.1.11",
          "form": "이름이 잘기억안나는뎈ㅋㅋ",
          "original_form": "이름이 잘기억안나는뎈ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100032766.1.12",
          "form": "다랭이마을이었나 거기도 예쁘고",
          "original_form": "다랭이마을이었나 거기도 예쁘고",
          "speaker_id": "2",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100032766.1.13",
          "form": "아하 전 아직 남해 한번도 못가봤어요 ㅋㅋ",
          "original_form": "아하 전 아직 남해 한번도 못가봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100032766.1.14",
          "form": "온통 바다에 둘러쌓인느낌 ㅠㅠ",
          "original_form": "온통 바다에 둘러쌓인느낌 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100032766.1.15",
          "form": "남해에 예쁜 숙소도 많은 것 같더라구요",
          "original_form": "남해에 예쁜 숙소도 많은 것 같더라구요",
          "speaker_id": "1",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100032766.1.16",
          "form": "한번 가보고 싶네여",
          "original_form": "한번 가보고 싶네여",
          "speaker_id": "1",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100032766.1.17",
          "form": "기회되면 꼭 가보셔용",
          "original_form": "기회되면 꼭 가보셔용",
          "speaker_id": "2",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032766.1.18",
          "form": "제주도도 넘 좋죠 ㅎㅎ",
          "original_form": "제주도도 넘 좋죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032766.1.19",
          "form": "내년에 함 가려구요",
          "original_form": "내년에 함 가려구요",
          "speaker_id": "2",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032766.1.20",
          "form": "제주도 고민고민하다가 6월에 갔는데",
          "original_form": "제주도 고민고민하다가 6월에 갔는데",
          "speaker_id": "1",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032766.1.21",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032766.1.22",
          "form": "오랜만에 가니까 더 좋더라구요!",
          "original_form": "오랜만에 가니까 더 좋더라구요!",
