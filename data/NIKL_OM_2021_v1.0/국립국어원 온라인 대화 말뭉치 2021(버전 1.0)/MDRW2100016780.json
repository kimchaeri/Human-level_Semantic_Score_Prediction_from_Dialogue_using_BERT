        },
        {
          "id": "MDRW2100016780.1.8",
          "form": "정신차려",
          "original_form": "정신차려",
          "speaker_id": "2",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016780.1.9",
          "form": "자유를 찾아 떠나는선배를 찾아 떠날게",
