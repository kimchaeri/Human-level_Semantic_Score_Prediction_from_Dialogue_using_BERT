      "utterance": [
        {
          "id": "MDRW2100024173.1.1",
          "form": "반려동물 키우시나요? 아이디가 강쥐짱이시길래",
          "original_form": "반려동물 키우시나요? 아이디가 강쥐짱이시길래",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024173.1.2",
          "form": "아닐수도 있을거 같긴 한데 여쭤봐요 ㅋㅋ",
          "original_form": "아닐수도 있을거 같긴 한데 여쭤봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024173.1.3",
          "form": "강아지 키워요ㅋㅋㅋ",
          "original_form": "강아지 키워요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024173.1.4",
          "form": "주택살아서 시골멍뭉이",
          "original_form": "주택살아서 시골멍뭉이",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024173.1.5",
          "form": "와 귀엽겠다 저는 멍뭉이 없네여...",
          "original_form": "와 귀엽겠다 저는 멍뭉이 없네여...",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024173.1.6",
          "form": "귀여워요ㅋㅋ진도믹스ㅋㅋ인데 뭔가 똥강아지에 가까운느낌?ㅋㅋ",
          "original_form": "귀여워요ㅋㅋ진도믹스ㅋㅋ인데 뭔가 똥강아지에 가까운느낌?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024173.1.7",
          "form": "시골멍뭉이 귀엽겠다",
          "original_form": "시골멍뭉이 귀엽겠다",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024173.1.8",
          "form": "악 귀여울거 같아요",
          "original_form": "악 귀여울거 같아요",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024173.1.9",
          "form": "귀가 쫑긋하지 않아요ㅋㅋㅋㅋ",
          "original_form": "귀가 쫑긋하지 않아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024173.1.10",
          "form": "요즘 흰둥이가 많던데 우리name3는 갈색강쥐ㅋㅋ",
          "original_form": "요즘 흰둥이가 많던데 우리&name3&는 갈색강쥐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024173.1.11",
          "form": "이름은 이name3예요ㅋㅋ제가 이씨라서 ㅎㅎㅎ",
          "original_form": "이름은 이&name3&예요ㅋㅋ제가 이씨라서 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024173.1.12",
          "form": "자랑 좀 많이 해주세요",
          "original_form": "자랑 좀 많이 해주세요",
          "speaker_id": "1",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024173.1.13",
          "form": "ㅋㅋㅋㅋ 악",
          "original_form": "ㅋㅋㅋㅋ 악",
          "speaker_id": "1",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024173.1.14",
          "form": "name3",
          "original_form": "&name3&",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100024173.1.15",
          "form": "이름만 들어도 귀여울거 같아요 원래 믹스멍멍이들이 귀엽자나여",
          "original_form": "이름만 들어도 귀여울거 같아요 원래 믹스멍멍이들이 귀엽자나여",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100024173.1.16",
          "form": "name3는 아직 2살이 안되었어요ㅋㅋ",
          "original_form": "&name3&는 아직 2살이 안되었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024173.1.17",
          "form": "강아지는 2살이 넘어야 성견이되어서ㅋㅋㅋ지금도 큰데 더 자랄듯ㅋㅋ",
          "original_form": "강아지는 2살이 넘어야 성견이되어서ㅋㅋㅋ지금도 큰데 더 자랄듯ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024173.1.18",
          "form": "근데 커서 더 왕귀여워요ㅋㅋ",
          "original_form": "근데 커서 더 왕귀여워요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024173.1.19",
          "form": "ㅠㅠ 왕크니까 왕귀엽겠다",
          "original_form": "ㅠㅠ 왕크니까 왕귀엽겠다",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024173.1.20",
