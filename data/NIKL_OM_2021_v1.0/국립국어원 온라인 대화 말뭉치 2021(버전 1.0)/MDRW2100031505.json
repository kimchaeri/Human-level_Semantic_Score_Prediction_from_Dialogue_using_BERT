      "utterance": [
        {
          "id": "MDRW2100031505.1.1",
          "form": "꽤 잘만든 드라마라 그러든데",
          "original_form": "꽤 잘만든 드라마라 그러든데",
          "speaker_id": "2",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100031505.1.2",
          "form": "그렇군요.",
          "original_form": "그렇군요.",
          "speaker_id": "2",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100031505.1.3",
          "form": "함 봐야지봐야지 하면서도",
          "original_form": "함 봐야지봐야지 하면서도",
          "speaker_id": "2",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100031505.1.4",
          "form": "드라마는 선뜻 시작하기 어렵더라구요",
          "original_form": "드라마는 선뜻 시작하기 어렵더라구요",
          "speaker_id": "2",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100031505.1.5",
          "form": "호흡이 길어서 그런가",
          "original_form": "호흡이 길어서 그런가",
          "speaker_id": "2",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100031505.1.6",
          "form": "아 맞아요 영화보다 드라마보면 너무 길어서 힘들더라고요",
          "original_form": "아 맞아요 영화보다 드라마보면 너무 길어서 힘들더라고요",
          "speaker_id": "1",
          "time": "20210904 22:16"
        },
        {
          "id": "MDRW2100031505.1.7",
          "form": "진짜요. 그렇게 손 못댄 드라마가 한트럭이에요",
          "original_form": "진짜요. 그렇게 손 못댄 드라마가 한트럭이에요",
          "speaker_id": "2",
          "time": "20210904 22:19"
        },
        {
          "id": "MDRW2100031505.1.8",
          "form": "시그널, 도깨비 다들 엄청 회자되는 작품이지만",
          "original_form": "시그널, 도깨비 다들 엄청 회자되는 작품이지만",
          "speaker_id": "2",
          "time": "20210904 22:19"
        },
        {
          "id": "MDRW2100031505.1.9",
          "form": "막상 시작하려니 부담되서...",
          "original_form": "막상 시작하려니 부담되서...",
          "speaker_id": "2",
          "time": "20210904 22:19"
        },
        {
          "id": "MDRW2100031505.1.10",
          "form": "저는 그래서 보고싶은거는 2 3배속해서 보는 능력을 길렀어요...ㅎ",
          "original_form": "저는 그래서 보고싶은거는 2 3배속해서 보는 능력을 길렀어요...ㅎ",
          "speaker_id": "1",
          "time": "20210904 22:21"
        },
        {
          "id": "MDRW2100031505.1.11",
          "form": "아, 그게 되요?",
          "original_form": "아, 그게 되요?",
          "speaker_id": "2",
          "time": "20210904 22:23"
        },
        {
          "id": "MDRW2100031505.1.12",
          "form": "티빙에 그런 기능이 있나요?",
          "original_form": "티빙에 그런 기능이 있나요?",
          "speaker_id": "2",
          "time": "20210904 22:23"
        },
        {
          "id": "MDRW2100031505.1.13",
          "form": "크롬 자체 프로그램으로 배속 할 수있는 프로그램이 있어서",
          "original_form": "크롬 자체 프로그램으로 배속 할 수있는 프로그램이 있어서",
          "speaker_id": "1",
          "time": "20210904 22:25"
        },
        {
          "id": "MDRW2100031505.1.14",
          "form": "그거 써서 배속해서 보고있어요",
          "original_form": "그거 써서 배속해서 보고있어요",
          "speaker_id": "1",
          "time": "20210904 22:25"
        },
        {
          "id": "MDRW2100031505.1.15",
          "form": "아하... 전 주로 아이패드로 봐서...",
          "original_form": "아하... 전 주로 아이패드로 봐서...",
          "speaker_id": "2",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100031505.1.16",
          "form": "아이패드로도 할수있는지 찾아봐야겠네요",
          "original_form": "아이패드로도 할수있는지 찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100031505.1.17",
          "form": "예전에는 드라마 볼때 연출도 보고 하면서 봤는데",
          "original_form": "예전에는 드라마 볼때 연출도 보고 하면서 봤는데",
          "speaker_id": "1",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100031505.1.18",
          "form": "이젠 일단 길이도 버거워서 스토리만 보기도 바쁘네요ㅠ",
          "original_form": "이젠 일단 길이도 버거워서 스토리만 보기도 바쁘네요ㅠ",
          "speaker_id": "1",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100031505.1.19",
          "form": "하... 맞아요. 나름 덕질?도 하면서 봤었던것도 같은데",
          "original_form": "하... 맞아요. 나름 덕질?도 하면서 봤었던것도 같은데",
          "speaker_id": "2",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100031505.1.20",
          "form": "이젠 보는거 자체가 버겁죠...",
          "original_form": "이젠 보는거 자체가 버겁죠...",
          "speaker_id": "2",
          "time": "20210904 22:31"
        },
        {
          "id": "MDRW2100031505.1.21",
          "form": "일단 스토리 이해하기도 버거워요...너무 길어서ㅠㅠ",
          "original_form": "일단 스토리 이해하기도 버거워요...너무 길어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100031505.1.22",
          "form": "하, 예전엔 좀만 복잡하게 꼬아도 엄마가 저게 뭔소리냐 묻는게 이해안갔는데",
          "original_form": "하, 예전엔 좀만 복잡하게 꼬아도 엄마가 저게 뭔소리냐 묻는게 이해안갔는데",
          "speaker_id": "2",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100031505.1.23",
          "form": "이젠 알것도 같아요 ㅠㅠ",
          "original_form": "이젠 알것도 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100031505.1.24",
          "form": "영화보다가 드라마보다가 다시 영화 보면",
          "original_form": "영화보다가 드라마보다가 다시 영화 보면",
          "speaker_id": "1",
          "time": "20210904 22:36"
        },
        {
          "id": "MDRW2100031505.1.25",
          "form": "영화감독들이 위대해보이더라고요",
          "original_form": "영화감독들이 위대해보이더라고요",
          "speaker_id": "1",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100031505.1.26",
          "form": "드라마는 이것저것 다 설명해서 이렇게 되는거다 이러는데",
          "original_form": "드라마는 이것저것 다 설명해서 이렇게 되는거다 이러는데",
          "speaker_id": "1",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100031505.1.27",
          "form": "영화는 2시간만에 그렇게 만드는게...",
          "original_form": "영화는 2시간만에 그렇게 만드는게...",
          "speaker_id": "1",
          "time": "20210904 22:37"
        }
      ]
