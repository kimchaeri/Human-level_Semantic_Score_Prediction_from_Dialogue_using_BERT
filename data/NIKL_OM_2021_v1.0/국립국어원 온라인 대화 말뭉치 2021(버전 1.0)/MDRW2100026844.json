      "utterance": [
        {
          "id": "MDRW2100026844.1.1",
          "form": "전 라면에 계란 꼭 넣어요",
          "original_form": "전 라면에 계란 꼭 넣어요",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026844.1.2",
          "form": "근데 노른자를 절대 풀지는 않습니다",
          "original_form": "근데 노른자를 절대 풀지는 않습니다",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026844.1.3",
          "form": "헉 저두요",
          "original_form": "헉 저두요",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026844.1.4",
          "form": "살짝 반숙으로 먹으면 더 맛있더라구요",
          "original_form": "살짝 반숙으로 먹으면 더 맛있더라구요",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026844.1.5",
          "form": "맞아요, 노른자를 건드릴때는 그릇으로 옮긴 다음입니다!",
          "original_form": "맞아요, 노른자를 건드릴때는 그릇으로 옮긴 다음입니다!",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026844.1.6",
          "form": "아 라면 먹고싶네요ㅎㅎ",
          "original_form": "아 라면 먹고싶네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026844.1.7",
          "form": "배달음식은 자주 드시나요?",
          "original_form": "배달음식은 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026844.1.8",
          "form": "음... 되도록 안시키려고 해요.",
          "original_form": "음... 되도록 안시키려고 해요.",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026844.1.9",
          "form": "쓰레기도 무시무시하고 돈도 많이 들고",
          "original_form": "쓰레기도 무시무시하고 돈도 많이 들고",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026844.1.10",
          "form": "의존도를 낮춰야겠다고 생각합니다.",
          "original_form": "의존도를 낮춰야겠다고 생각합니다.",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026844.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100026844.1.12",
          "form": "배달 한번 시키면 플라스틱이 어마어마하더라구요",
          "original_form": "배달 한번 시키면 플라스틱이 어마어마하더라구요",
          "speaker_id": "1",
          "time": "20210902 16:59"
        },
        {
          "id": "MDRW2100026844.1.13",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 16:59"
        },
        {
          "id": "MDRW2100026844.1.14",
          "form": "네, 아까 먹었어요",
          "original_form": "네, 아까 먹었어요",
          "speaker_id": "2",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100026844.1.15",
          "form": "그래서 지금 또 출출해졌네요",
          "original_form": "그래서 지금 또 출출해졌네요",
          "speaker_id": "2",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100026844.1.16",
          "form": "어떤거 드셨나요?",
          "original_form": "어떤거 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 19:33"
        },
        {
          "id": "MDRW2100026844.1.17",
          "form": "콩불이요",
          "original_form": "콩불이요",
          "speaker_id": "2",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100026844.1.18",
          "form": "제가 만들었답니다. 야매로 ㅋㅋ",
          "original_form": "제가 만들었답니다. 야매로 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100026844.1.19",
          "form": "오 요리 잘하시나봐요!!!",
          "original_form": "오 요리 잘하시나봐요!!!",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100026844.1.20",
          "form": "콩불 어려울거 같은데...",
          "original_form": "콩불 어려울거 같은데...",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100026844.1.21",
          "form": "음..  시판양념장의 도움을 받고 있습니다",
          "original_form": "음..  시판양념장의 도움을 받고 있습니다",
          "speaker_id": "2",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100026844.1.22",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100026844.1.23",
          "form": "양념만 있으면 걍 고기랑 콩나물만 넣고 볶으면 돼요 ㅎㅎ",
          "original_form": "양념만 있으면 걍 고기랑 콩나물만 넣고 볶으면 돼요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100026844.1.24",
          "form": "오 요리랑 거리가 멀어서 해본적 없는데 나중에 사서 해봐야겠어요",
          "original_form": "오 요리랑 거리가 멀어서 해본적 없는데 나중에 사서 해봐야겠어요",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100026844.1.25",
          "form": "콩불 자주 드시나요?",
          "original_form": "콩불 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100026844.1.26",
          "form": "음, 자주라고 하긴 애매한데",
          "original_form": "음, 자주라고 하긴 애매한데",
          "speaker_id": "2",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100026844.1.27",
          "form": "한달에 한두번? 정도 해요",
          "original_form": "한달에 한두번? 정도 해요",
          "speaker_id": "2",
          "time": "20210902 20:45"
        }
      ]
    }
