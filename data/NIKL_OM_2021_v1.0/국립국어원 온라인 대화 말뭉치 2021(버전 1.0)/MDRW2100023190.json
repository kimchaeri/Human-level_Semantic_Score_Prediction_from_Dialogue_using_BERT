        },
        {
          "id": "MDRW2100023190.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023190.1.4",
          "form": "저녁에 어떤 메뉴로 드셨어요?",
          "original_form": "저녁에 어떤 메뉴로 드셨어요?",
          "speaker_id": "1",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023190.1.5",
          "form": "김치찜이요!",
          "original_form": "김치찜이요!",
          "speaker_id": "2",
          "time": "20210829 23:25"
        },
        {
          "id": "MDRW2100023190.1.6",
          "form": "김치찜 맛있죠\n밥 한 공기 금방 먹게 해주는 메뉴네요",
          "original_form": "김치찜 맛있죠\n밥 한 공기 금방 먹게 해주는 메뉴네요",
          "speaker_id": "1",
          "time": "20210829 23:27"
        },
        {
          "id": "MDRW2100023190.1.7",
          "form": "두공기먹었어요ㅋㅋㅋㅋ",
          "original_form": "두공기먹었어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100023190.1.8",
          "form": "한공기로 끝날수가 없었군요ㅋㅋㅋ",
          "original_form": "한공기로 끝날수가 없었군요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:30"
        },
        {
          "id": "MDRW2100023190.1.9",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:36"
        },
        {
          "id": "MDRW2100023190.1.10",
          "form": "삼겹살이랑 김치 같이 돌돌 말아 넣어 찜하면 맛있는데 먹고 싶네요",
          "original_form": "삼겹살이랑 김치 같이 돌돌 말아 넣어 찜하면 맛있는데 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210829 23:38"
        },
        {
          "id": "MDRW2100023190.1.11",
          "form": "헐 저도그거좋아하는데",
          "original_form": "헐 저도그거좋아하는데",
          "speaker_id": "2",
          "time": "20210830 00:04"
        },
        {
          "id": "MDRW2100023190.1.12",
          "form": "그쵸!! 그거 맛있죠ㅎㅎ",
          "original_form": "그쵸!! 그거 맛있죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:20"
        },
        {
          "id": "MDRW2100023190.1.13",
          "form": "자주해먹어요ㅋㅋ",
          "original_form": "자주해먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100023190.1.14",
          "form": "김치가 진짜마싯으면 어후",
          "original_form": "김치가 진짜마싯으면 어후",
          "speaker_id": "2",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023190.1.15",
          "form": "상상만으로 침고이죠ㅎㅎ",
          "original_form": "상상만으로 침고이죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023190.1.16",
          "form": "김치들어가는 요리들은 김치가 맛있으면 맛이 보장되는거같아요",
          "original_form": "김치들어가는 요리들은 김치가 맛있으면 맛이 보장되는거같아요",
          "speaker_id": "1",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023190.1.17",
          "form": "맞아요ㅋㅋ 왠만하면 평타치는듯해요",
          "original_form": "맞아요ㅋㅋ 왠만하면 평타치는듯해요",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
