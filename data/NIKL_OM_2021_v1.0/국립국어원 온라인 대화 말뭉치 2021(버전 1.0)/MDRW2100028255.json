      "utterance": [
        {
          "id": "MDRW2100028255.1.1",
          "form": "영화 드라마인가요?!",
          "original_form": "영화 드라마인가요?!",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100028255.1.2",
          "form": "앗 넵넵 경극에 관한 영화예요",
          "original_form": "앗 넵넵 경극에 관한 영화예요",
          "speaker_id": "2",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100028255.1.3",
          "form": "등장인물 이름이 샤오루라서 여쭤봤어요ㅎㅎ",
          "original_form": "등장인물 이름이 샤오루라서 여쭤봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100028255.1.4",
          "form": "제가 좋아하는 연예인 별명이네용 ㅎㅎ",
          "original_form": "제가 좋아하는 연예인 별명이네용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100028255.1.5",
          "form": "중국어를 알아보시다닛",
          "original_form": "중국어를 알아보시다닛",
          "speaker_id": "1",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100028255.1.6",
          "form": "앗 누구 좋아하세요?",
          "original_form": "앗 누구 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100028255.1.7",
          "form": "전엑소인데 루한이요",
          "original_form": "전엑소인데 루한이요",
          "speaker_id": "1",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028255.1.8",
          "form": "엑소 노래 가끔 들어요! 혹시 추천 해주실 수 있나요ㅎㅎ",
          "original_form": "엑소 노래 가끔 들어요! 혹시 추천 해주실 수 있나요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028255.1.9",
          "form": "요즘 엑소 노래는 잘 몰라용 ㅎㅎㅎ",
          "original_form": "요즘 엑소 노래는 잘 몰라용 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028255.1.10",
          "form": "좋아하는 연예인 있으신가요?",
          "original_form": "좋아하는 연예인 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028255.1.11",
          "form": "저는 샤이니 좋아해요!",
          "original_form": "저는 샤이니 좋아해요!",
          "speaker_id": "2",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100028255.1.12",
          "form": "이번에 키 신곡 나와서 요새는 그거 듣고 있어요ㅎㅎ",
          "original_form": "이번에 키 신곡 나와서 요새는 그거 듣고 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100028255.1.13",
          "form": "팬이신건가요?! 전 종현님 솔로곡 좋게 들었었는데 ㅠ",
          "original_form": "팬이신건가요?! 전 종현님 솔로곡 좋게 들었었는데 ㅠ",
          "speaker_id": "1",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100028255.1.14",
          "form": "팬... 까지는 아닌데 그냥 좋아하는 연예인으로 찜꽁해두고 있어요ㅎㅎ",
          "original_form": "팬... 까지는 아닌데 그냥 좋아하는 연예인으로 찜꽁해두고 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028255.1.15",
          "form": "앗 종현... 종현 그립죠ㅜㅜ 그래도 넷이서 잘 활동하고 있답니당",
          "original_form": "앗 종현... 종현 그립죠ㅜㅜ 그래도 넷이서 잘 활동하고 있답니당",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028255.1.16",
          "form": "민호 넘 잘생겨써요",
          "original_form": "민호 넘 잘생겨써요",
          "speaker_id": "1",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100028255.1.17",
          "form": "루한이랑 같이 축구할때도 있었는데",
          "original_form": "루한이랑 같이 축구할때도 있었는데",
          "speaker_id": "1",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100028255.1.18",
          "form": "그립네요",
          "original_form": "그립네요",
          "speaker_id": "1",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100028255.1.19",
          "form": "앗 저 민호 제일 좋아해요ㅋㅋㅋ 축구 좋아하는 민호",
          "original_form": "앗 저 민호 제일 좋아해요ㅋㅋㅋ 축구 좋아하는 민호",
          "speaker_id": "2",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100028255.1.20",
          "form": "유튜브에 키세키라는 노래 부른 거 있는데 엄청 좋아요! 한번 들어보세요ㅎㅎ",
          "original_form": "유튜브에 키세키라는 노래 부른 거 있는데 엄청 좋아요! 한번 들어보세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100028255.1.21",
          "form": "님도 사슴상 좋아하나바요 ㅋㅋㅋ",
          "original_form": "님도 사슴상 좋아하나바요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:03"
        },
        {
          "id": "MDRW2100028255.1.22",
          "form": "네 맞아요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 맞아요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100028255.1.23",
          "form": "윤아도 좋아한다는 ㅋㅋㅋㅋㅋ",
          "original_form": "윤아도 좋아한다는 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100028255.1.24",
          "form": "헉ㅋㅋㅋㅋㅋ 와 진짜 다 겹쳐서 넘 신기해요",
          "original_form": "헉ㅋㅋㅋㅋㅋ 와 진짜 다 겹쳐서 넘 신기해요",
          "speaker_id": "2",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100028255.1.25",
          "form": "역시 사슴상 국룰",
          "original_form": "역시 사슴상 국룰",
          "speaker_id": "1",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100028255.1.26",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100028255.1.27",
          "form": "준호는 투피엠인가요??? 박보검 착하게 생겨서 좋아해요ㅎㅎㅎ",
          "original_form": "준호는 투피엠인가요??? 박보검 착하게 생겨서 좋아해요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:07"
        }
      ]
