      "utterance": [
        {
          "id": "MDRW2100015088.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015088.1.2",
          "form": "안녕하세요!! 넵 혼자는 안가고 친구들이랑 가는 걸 좋아해요!!",
          "original_form": "안녕하세요!! 넵 혼자는 안가고 친구들이랑 가는 걸 좋아해요!!",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015088.1.3",
          "form": "혼자 여행도 다니시나요?",
          "original_form": "혼자 여행도 다니시나요?",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015088.1.4",
          "form": "저도 혼자는 다녀본적 없어요! 국내여행지 어디가 좋으셨어요?",
          "original_form": "저도 혼자는 다녀본적 없어요! 국내여행지 어디가 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015088.1.5",
          "form": "저는 여수요!!!",
          "original_form": "저는 여수요!!!",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015088.1.6",
          "form": "아 전주도 좋았어요",
          "original_form": "아 전주도 좋았어요",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015088.1.7",
          "form": "게장 드셨나요?ㅎㅎ",
          "original_form": "게장 드셨나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015088.1.8",
          "form": "예스예스",
          "original_form": "예스예스",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015088.1.9",
          "form": "저 간장게장 맛있는거 첨 먹어봤어요",
          "original_form": "저 간장게장 맛있는거 첨 먹어봤어요",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015088.1.10",
          "form": "여행 어디가 제일 좋으셨어요?",
          "original_form": "여행 어디가 제일 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015088.1.11",
          "form": "저 제주도요! 가본적 있으세요?",
          "original_form": "저 제주도요! 가본적 있으세요?",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015088.1.12",
          "form": "네네네!!",
          "original_form": "네네네!!",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015088.1.13",
          "form": "오 올레길 가보신건가요?",
          "original_form": "오 올레길 가보신건가요?",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015088.1.14",
          "form": "아뇨 그냥 관광지 명소 위주로 돌아다녔어요! 제주도 어디 가보셨어요?",
          "original_form": "아뇨 그냥 관광지 명소 위주로 돌아다녔어요! 제주도 어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015088.1.15",
          "form": "저는 식도락으로만..다녀서..",
          "original_form": "저는 식도락으로만..다녀서..",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015088.1.16",
          "form": "그 먹은것밖에...",
          "original_form": "그 먹은것밖에...",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015088.1.17",
          "form": "돈까스랑..면이랑..",
          "original_form": "돈까스랑..면이랑..",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015088.1.18",
          "form": "어우 최고의 투어...",
          "original_form": "어우 최고의 투어...",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015088.1.19",
          "form": "해외여행지는 다녀오셨나요?",
          "original_form": "해외여행지는 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015088.1.20",
          "form": "엇 네네네",
          "original_form": "엇 네네네",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015088.1.21",
          "form": "전 벹남이 가장 좋았어요",
          "original_form": "전 벹남이 가장 좋았어요",
