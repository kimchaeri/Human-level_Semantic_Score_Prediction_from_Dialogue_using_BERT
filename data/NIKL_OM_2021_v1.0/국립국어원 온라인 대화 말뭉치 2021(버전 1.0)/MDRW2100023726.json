      "utterance": [
        {
          "id": "MDRW2100023726.1.1",
          "form": "왜헤어졋냐구여?ㅋㅋㅋㅋ",
          "original_form": "왜헤어졋냐구여?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:56"
        },
        {
          "id": "MDRW2100023726.1.2",
          "form": "오래 사귀기도했고 뭐랄까 ㅋㅋㅋ 서로 누가먼저헤어지자고해도 아무렇지도않을정도까지 가서 ㅋㅋㅋ",
          "original_form": "오래 사귀기도했고 뭐랄까 ㅋㅋㅋ 서로 누가먼저헤어지자고해도 아무렇지도않을정도까지 가서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:00"
        },
        {
          "id": "MDRW2100023726.1.3",
          "form": "아... 지쳐서 서로 끝난거구나",
          "original_form": "아... 지쳐서 서로 끝난거구나",
          "speaker_id": "2",
          "time": "20210830 16:02"
        },
        {
          "id": "MDRW2100023726.1.4",
          "form": "얼마나 만났어요??",
          "original_form": "얼마나 만났어요??",
          "speaker_id": "2",
          "time": "20210830 16:02"
        },
        {
          "id": "MDRW2100023726.1.5",
          "form": "3년 10개월인가?",
          "original_form": "3년 10개월인가?",
          "speaker_id": "1",
          "time": "20210830 16:02"
        },
        {
          "id": "MDRW2100023726.1.6",
          "form": "4년만났어요 ㅋㅋㅋ",
          "original_form": "4년만났어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:02"
        },
        {
          "id": "MDRW2100023726.1.7",
          "form": "와ㅋㅋㅋㅋㅋㅋ",
          "original_form": "와ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:08"
        },
        {
          "id": "MDRW2100023726.1.8",
          "form": "저랑 큰 차이 안났네요?",
          "original_form": "저랑 큰 차이 안났네요?",
          "speaker_id": "2",
          "time": "20210830 16:08"
        },
        {
          "id": "MDRW2100023726.1.9",
          "form": "언제부터 싸워썽요?",
          "original_form": "언제부터 싸워썽요?",
          "speaker_id": "2",
          "time": "20210830 16:09"
        },
        {
          "id": "MDRW2100023726.1.10",
          "form": "3년 넘어가니까 진짜 별것도 아닌걸로 싸우게되던데",
          "original_form": "3년 넘어가니까 진짜 별것도 아닌걸로 싸우게되던데",
          "speaker_id": "2",
          "time": "20210830 16:09"
        },
        {
          "id": "MDRW2100023726.1.11",
          "form": "그전엔 한번도 안 싸운 것 같거든요?",
          "original_form": "그전엔 한번도 안 싸운 것 같거든요?",
          "speaker_id": "2",
          "time": "20210830 16:09"
        },
        {
          "id": "MDRW2100023726.1.12",
          "form": "싸웟다기보다는 말투 이런거? 연락빈도수라던가,,",
          "original_form": "싸웟다기보다는 말투 이런거? 연락빈도수라던가,,",
          "speaker_id": "1",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100023726.1.13",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "2",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100023726.1.14",
          "form": "말투야 뭐 초기랑 많이 바뀐다 그러지만",
          "original_form": "말투야 뭐 초기랑 많이 바뀐다 그러지만",
          "speaker_id": "2",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100023726.1.15",
          "form": "빈도수는 참..",
          "original_form": "빈도수는 참..",
          "speaker_id": "2",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100023726.1.16",
          "form": "그때도 일하셨나요?",
          "original_form": "그때도 일하셨나요?",
          "speaker_id": "2",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100023726.1.17",
          "form": "걔 만나면서 저는 취업했고 걔는 쭉 백,수하다가 군대감",
          "original_form": "걔 만나면서 저는 취업했고 걔는 쭉 백,수하다가 군대감",
          "speaker_id": "1",
          "time": "20210830 16:26"
        },
        {
          "id": "MDRW2100023726.1.18",
          "form": "전 만나고 몇달뒤에 취업했었는데",
          "original_form": "전 만나고 몇달뒤에 취업했었는데",
          "speaker_id": "2",
          "time": "20210830 16:27"
        },
        {
          "id": "MDRW2100023726.1.19",
          "form": "이게 짬이 차다보니 사원때보다 대리가 바쁘고",
          "original_form": "이게 짬이 차다보니 사원때보다 대리가 바쁘고",
          "speaker_id": "2",
          "time": "20210830 16:27"
        },
        {
          "id": "MDRW2100023726.1.20",
          "form": "어쩔수없이 바쁠때가 있는데",
          "original_form": "어쩔수없이 바쁠때가 있는데",
          "speaker_id": "2",
          "time": "20210830 16:27"
        },
        {
          "id": "MDRW2100023726.1.21",
          "form": "얘도 백조여서 그런지",
          "original_form": "얘도 백조여서 그런지",
          "speaker_id": "2",
          "time": "20210830 16:27"
        },
        {
          "id": "MDRW2100023726.1.22",
          "form": "못참더라구요",
          "original_form": "못참더라구요",
          "speaker_id": "2",
          "time": "20210830 16:28"
        },
        {
          "id": "MDRW2100023726.1.23",
          "form": "답장 왜 안하냐...",
          "original_form": "답장 왜 안하냐...",
          "speaker_id": "2",
          "time": "20210830 16:28"
        },
        {
          "id": "MDRW2100023726.1.24",
          "form": "갑자기 일 터져서 수습하고 왔는데 그런 문자 와있으면 참",
          "original_form": "갑자기 일 터져서 수습하고 왔는데 그런 문자 와있으면 참",
          "speaker_id": "2",
          "time": "20210830 16:28"
        },
        {
          "id": "MDRW2100023726.1.25",
          "form": "이해는 하지만 어렵더라구요 이해를 해주던가 비슷한 사람을 만나는게 이래선가 싶음 지금 생각해보면",
          "original_form": "이해는 하지만 어렵더라구요 이해를 해주던가 비슷한 사람을 만나는게 이래선가 싶음 지금 생각해보면",
          "speaker_id": "2",
          "time": "20210830 16:29"
        },
        {
          "id": "MDRW2100023726.1.26",
          "form": "사람은 다 이기적이잖아요",
          "original_form": "사람은 다 이기적이잖아요",
          "speaker_id": "2",
          "time": "20210830 16:29"
        },
        {
          "id": "MDRW2100023726.1.27",
          "form": "아 일할때 답장왜안하냐고요,,? 진짜 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 일할때 답장왜안하냐고요,,? 진짜 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:30"
        },
        {
          "id": "MDRW2100023726.1.28",
          "form": "장난똥때리네 ㅋㅋㅋㅋ",
          "original_form": "장난똥때리네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:30"
        },
        {
          "id": "MDRW2100023726.1.29",
          "form": "ㅋㅋ퇴근하고도 톡 못한적 있겠죠 뭐",
          "original_form": "ㅋㅋ퇴근하고도 톡 못한적 있겠죠 뭐",
          "speaker_id": "2",
          "time": "20210830 16:30"
        },
        {
          "id": "MDRW2100023726.1.30",
          "form": "그럼 또 화내고 삐지고",
          "original_form": "그럼 또 화내고 삐지고",
          "speaker_id": "2",
          "time": "20210830 16:30"
        },
        {
          "id": "MDRW2100023726.1.31",
          "form": "그만큼 의지하나 싶기도 한데 제가 그땐 그릇이 작아서ㅎㅎ",
          "original_form": "그만큼 의지하나 싶기도 한데 제가 그땐 그릇이 작아서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 16:31"
        },
        {
          "id": "MDRW2100023726.1.32",
          "form": "그때는 양념그릇이엿다면 지금은 냉면그릇인가요?",
          "original_form": "그때는 양념그릇이엿다면 지금은 냉면그릇인가요?",
          "speaker_id": "1",
          "time": "20210830 16:32"
        },
        {
          "id": "MDRW2100023726.1.33",
          "form": "와 부장님?",
          "original_form": "와 부장님?",
          "speaker_id": "2",
          "time": "20210830 16:32"
        },
        {
          "id": "MDRW2100023726.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:33"
        },
        {
          "id": "MDRW2100023726.1.35",
          "form": "갑자기그러니까 냉면먹고싶네요 만두랑",
          "original_form": "갑자기그러니까 냉면먹고싶네요 만두랑",
          "speaker_id": "1",
          "time": "20210830 16:33"
        },
        {
          "id": "MDRW2100023726.1.36",
