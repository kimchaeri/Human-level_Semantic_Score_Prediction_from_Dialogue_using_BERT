      "utterance": [
        {
          "id": "MDRW2100039832.1.1",
          "form": "게임 요즘 뭐하고싶은거있어?",
          "original_form": "게임 요즘 뭐하고싶은거있어?",
          "speaker_id": "1",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039832.1.2",
          "form": "포켓몬이랑 젤다?",
          "original_form": "포켓몬이랑 젤다?",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039832.1.3",
          "form": "포켓몬 신작 중에 뭐할것같아?",
          "original_form": "포켓몬 신작 중에 뭐할것같아?",
          "speaker_id": "1",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039832.1.4",
          "form": "음.. 4세대 리메이크 할거같아",
          "original_form": "음.. 4세대 리메이크 할거같아",
          "speaker_id": "2",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039832.1.5",
          "form": "그럼 아르세우스는 안 해?",
          "original_form": "그럼 아르세우스는 안 해?",
          "speaker_id": "1",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039832.1.6",
          "form": "신작 발매되면 한번 영상이나.. 후기 보고? 고민중!",
          "original_form": "신작 발매되면 한번 영상이나.. 후기 보고? 고민중!",
          "speaker_id": "2",
          "time": "20210903 21:58"
        },
        {
          "id": "MDRW2100039832.1.7",
          "form": "그럼 젤다는 야숨? 아님 무쌍?",
          "original_form": "그럼 젤다는 야숨? 아님 무쌍?",
          "speaker_id": "1",
          "time": "20210903 21:59"
        },
        {
          "id": "MDRW2100039832.1.8",
          "form": "야숨! 야생의 숨결?이었나 그거 재밌어보이더라",
          "original_form": "야숨! 야생의 숨결?이었나 그거 재밌어보이더라",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100039832.1.9",
          "form": "젤다 얼른 할 수 있었으면 좋겠다\n난 하고있는데 맨날 낙사해ㅎㅎ",
          "original_form": "젤다 얼른 할 수 있었으면 좋겠다\n난 하고있는데 맨날 낙사해ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039832.1.10",
          "form": "ㅋㅋㅋㅋ그래도 재밌으니까 계속 하는거지?",
          "original_form": "ㅋㅋㅋㅋ그래도 재밌으니까 계속 하는거지?",
          "speaker_id": "2",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100039832.1.11",
          "form": "재밌긴해! 근데 초보인 나는 맨날 게임오버 봐\n같이 이 험난함을 경험했으면 좋겠다",
          "original_form": "재밌긴해! 근데 초보인 나는 맨날 게임오버 봐\n같이 이 험난함을 경험했으면 좋겠다",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100039832.1.12",
          "form": "험난함ㅋㅋ 웃기다. 그렇게 어려워?",
          "original_form": "험난함ㅋㅋ 웃기다. 그렇게 어려워?",
          "speaker_id": "2",
          "time": "20210904 11:22"
        },
        {
          "id": "MDRW2100039832.1.13",
          "form": "왜 떨어져서 죽는지 이해 못함 용사면 노오력으로 극복해야지",
          "original_form": "왜 떨어져서 죽는지 이해 못함 용사면 노오력으로 극복해야지",
          "speaker_id": "1",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100039832.1.14",
          "form": "용사는 체력이 부족해 빨리 사당 찾아서 하트 올려줘",
          "original_form": "용사는 체력이 부족해 빨리 사당 찾아서 하트 올려줘",
          "speaker_id": "2",
          "time": "20210904 11:46"
        },
        {
          "id": "MDRW2100039832.1.15",
          "form": "지금 초보존이라 그런가 체력 아직도 3개라구ㅠ",
          "original_form": "지금 초보존이라 그런가 체력 아직도 3개라구ㅠ",
          "speaker_id": "1",
          "time": "20210904 11:49"
        },
        {
          "id": "MDRW2100039832.1.16",
          "form": "당신은 할수있다",
          "original_form": "당신은 할수있다",
          "speaker_id": "2",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100039832.1.17",
          "form": "링크를 잘 키울수있다",
          "original_form": "링크를 잘 키울수있다",
          "speaker_id": "2",
          "time": "20210904 12:14"
        }
      ]
