      "utterance": [
        {
          "id": "MDRW2100045521.1.1",
          "form": "지킬때 딕션 딱딱 꽂히는거",
          "original_form": "지킬때 딕션 딱딱 꽂히는거",
          "speaker_id": "2",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045521.1.2",
          "form": "아 저게 그리고",
          "original_form": "아 저게 그리고",
          "speaker_id": "1",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045521.1.3",
          "form": "짐승소리로 슬쩍 뭉개는거 ㄹㅇ..",
          "original_form": "짐승소리로 슬쩍 뭉개는거 ㄹㅇ..",
          "speaker_id": "2",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045521.1.4",
          "form": "하이드가 왼쪽손을",
          "original_form": "하이드가 왼쪽손을",
          "speaker_id": "1",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045521.1.5",
          "form": "저러는 이유는",
          "original_form": "저러는 이유는",
          "speaker_id": "1",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045521.1.6",
          "form": "저기에 약물을 주입해서 하이드가 생겨서...",
          "original_form": "저기에 약물을 주입해서 하이드가 생겨서...",
          "speaker_id": "1",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045521.1.7",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045521.1.8",
          "form": "주사를 왼찰에 맞앗나여?",
          "original_form": "주사를 왼찰에 맞앗나여?",
          "speaker_id": "2",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045521.1.9",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045521.1.10",
          "form": "저거",
          "original_form": "저거",
          "speaker_id": "2",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100045521.1.11",
          "form": "저 연출",
          "original_form": "저 연출",
          "speaker_id": "2",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100045521.1.12",
          "form": "나는나!! 너는너!",
          "original_form": "나는나!! 너는너!",
          "speaker_id": "1",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100045521.1.13",
          "form": "지금은 바꼈지여?",
          "original_form": "지금은 바꼈지여?",
          "speaker_id": "2",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100045521.1.14",
          "form": "똑같나?",
          "original_form": "똑같나?",
          "speaker_id": "2",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100045521.1.15",
          "form": "아니여\r\n같이여",
          "original_form": "아니여\r\n같이여",
          "speaker_id": "1",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100045521.1.16",
          "form": "저걸 우리나라에서",
          "original_form": "저걸 우리나라에서",
          "speaker_id": "1",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100045521.1.17",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100045521.1.18",
          "form": "더극적으로 만들어버려서",
          "original_form": "더극적으로 만들어버려서",
          "speaker_id": "1",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100045521.1.19",
          "form": "저러고",
          "original_form": "저러고",
          "speaker_id": "2",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100045521.1.20",
          "form": "방송을 끝냇다고?",
          "original_form": "방송을 끝냇다고?",
          "speaker_id": "2",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100045521.1.21",
          "form": "?????",
          "original_form": "?????",
          "speaker_id": "2",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100045521.1.22",
