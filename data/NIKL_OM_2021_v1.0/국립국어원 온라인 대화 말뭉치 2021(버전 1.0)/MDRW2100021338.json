        {
          "id": "MDRW2100021338.1.1",
          "form": "스포츠조아하시나요",
          "original_form": "스포츠조아하시나요",
          "speaker_id": "1",
          "time": "20210828 17:47"
        },
        {
          "id": "MDRW2100021338.1.2",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100021338.1.3",
          "form": "보시는 경기 있나요?",
          "original_form": "보시는 경기 있나요?",
          "speaker_id": "2",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100021338.1.4",
          "form": "해외축구요!",
          "original_form": "해외축구요!",
          "speaker_id": "1",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100021338.1.5",
          "form": "손흥민 경기 보세요?",
          "original_form": "손흥민 경기 보세요?",
          "speaker_id": "2",
          "time": "20210828 18:03"
        },
        {
          "id": "MDRW2100021338.1.6",
          "form": "오 맞아요토트넘 좋아해요",
          "original_form": "오 맞아요토트넘 좋아해요",
          "speaker_id": "1",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021338.1.7",
          "form": "ㅋㅋ다들 토트넘 경기 보더라구요",
          "original_form": "ㅋㅋ다들 토트넘 경기 보더라구요",
          "speaker_id": "2",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021338.1.8",
          "form": "어떤스포츠 좋아하세요?",
          "original_form": "어떤스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100021338.1.9",
          "form": "올림픽보고 배구에 관심생겨서요",
          "original_form": "올림픽보고 배구에 관심생겨서요",
          "speaker_id": "2",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021338.1.10",
          "form": "리그도 하고 있더라구요",
          "original_form": "리그도 하고 있더라구요",
          "speaker_id": "2",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021338.1.11",
          "form": "아 여자배구 개막했나요?",
          "original_form": "아 여자배구 개막했나요?",
          "speaker_id": "1",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100021338.1.12",
          "form": "네 이번주에요",
          "original_form": "네 이번주에요",
          "speaker_id": "2",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100021338.1.13",
          "form": "오 한번 챙겨봐야겠어요",
          "original_form": "오 한번 챙겨봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100021338.1.14",
          "form": "재밌더라구요. 어느 팀을 응원할지는 못 정했는데ㅋㅋㅋ",
          "original_form": "재밌더라구요. 어느 팀을 응원할지는 못 정했는데ㅋㅋㅋ",
