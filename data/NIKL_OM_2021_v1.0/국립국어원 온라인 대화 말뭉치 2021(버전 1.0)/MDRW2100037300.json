      "utterance": [
        {
          "id": "MDRW2100037300.1.1",
          "form": "빡돌아서",
          "original_form": "빡돌아서",
          "speaker_id": "2",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100037300.1.2",
          "form": "안가리고 태워버리면",
          "original_form": "안가리고 태워버리면",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037300.1.3",
          "form": "어쩌려고",
          "original_form": "어쩌려고",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037300.1.4",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037300.1.5",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037300.1.6",
          "form": "김소하가 주인공 완전 개무시하는데",
          "original_form": "김소하가 주인공 완전 개무시하는데",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037300.1.7",
          "form": "고작 이야기꾼주제에?",
          "original_form": "고작 이야기꾼주제에?",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037300.1.8",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037300.1.9",
          "form": "나같았으면 내가 아쉽지 니가 아쉽냐??",
          "original_form": "나같았으면 내가 아쉽지 니가 아쉽냐??",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037300.1.10",
          "form": "아니 애초에",
          "original_form": "아니 애초에",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037300.1.11",
          "form": "하고 때려쳤음",
          "original_form": "하고 때려쳤음",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037300.1.12",
          "form": "달의 신이 가진 능력이",
          "original_form": "달의 신이 가진 능력이",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037300.1.13",
          "form": "설화 읽기랑 태우기밖에",
          "original_form": "설화 읽기랑 태우기밖에",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037300.1.14",
          "form": "없단말야?",
          "original_form": "없단말야?",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037300.1.15",
          "form": "그게 무슨 신이야",
          "original_form": "그게 무슨 신이야",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037300.1.16",
          "form": "그거랑 설화계 문 여닫기래",
          "original_form": "그거랑 설화계 문 여닫기래",
          "speaker_id": "1",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037300.1.17",
          "form": "구천에 감금하기",
          "original_form": "구천에 감금하기",
          "speaker_id": "1",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037300.1.18",
          "form": "걔한텐 그거 왜 안했대",
          "original_form": "걔한텐 그거 왜 안했대",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037300.1.19",
          "form": "그 능력은 아직 각성을 못했대?",
          "original_form": "그 능력은 아직 각성을 못했대?",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037300.1.20",
          "form": "여주가?",
          "original_form": "여주가?",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037300.1.21",
          "form": "그거는",
          "original_form": "그거는",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100037300.1.22",
          "form": "신이돼야 할 수 있고",
          "original_form": "신이돼야 할 수 있고",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100037300.1.23",
          "form": "여주는 자격자상태라 안되는듯?",
          "original_form": "여주는 자격자상태라 안되는듯?",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100037300.1.24",
          "form": "아니 그럼 진짜",
          "original_form": "아니 그럼 진짜",
          "speaker_id": "2",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100037300.1.25",
          "form": "뭘 어떡하라고",
          "original_form": "뭘 어떡하라고",
          "speaker_id": "2",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100037300.1.26",
          "form": "능력을 갈라 준거지",
          "original_form": "능력을 갈라 준거지",
          "speaker_id": "2",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100037300.1.27",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100037300.1.28",
          "form": "대뜸 능력 준 것도 환장하는데",
          "original_form": "대뜸 능력 준 것도 환장하는데",
          "speaker_id": "1",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100037300.1.29",
          "form": "ㅇㅇ나같으면",
          "original_form": "ㅇㅇ나같으면",
          "speaker_id": "2",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100037300.1.30",
          "form": "걔가 갑자기 친구 죽인 시점에서",
          "original_form": "걔가 갑자기 친구 죽인 시점에서",
          "speaker_id": "2",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100037300.1.31",
          "form": "때려친다",
          "original_form": "때려친다",
          "speaker_id": "2",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100037300.1.32",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100037300.1.33",
          "form": "환장하는거",
          "original_form": "환장하는거",
          "speaker_id": "1",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100037300.1.34",
          "form": "이상태에서",
          "original_form": "이상태에서",
          "speaker_id": "1",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100037300.1.35",
          "form": "악우가 신되면",
          "original_form": "악우가 신되면",
          "speaker_id": "1",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100037300.1.36",
          "form": "여주랑 계약한 사자+여주 본인까지 싹 죽는대",
          "original_form": "여주랑 계약한 사자+여주 본인까지 싹 죽는대",
          "speaker_id": "1",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100037300.1.37",
          "form": "아니 이건",
          "original_form": "아니 이건",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100037300.1.38",
          "form": "불공정 계약이야;;;;;",
          "original_form": "불공정 계약이야;;;;;",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100037300.1.39",
          "form": "노조에 고발감인데",
          "original_form": "노조에 고발감인데",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100037300.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100037300.1.41",
          "form": "오 원신",
          "original_form": "오 원신",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037300.1.42",
          "form": "수메르는 인도풍인가?",
          "original_form": "수메르는 인도풍인가?",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037300.1.43",
          "form": "꽃의 신 이름이 쿠사나리 데비 임",
          "original_form": "꽃의 신 이름이 쿠사나리 데비 임",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037300.1.44",
          "form": "나 스트레스 받아서",
          "original_form": "나 스트레스 받아서",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100037300.1.45",
          "form": "밥을 먹어야겟어",
          "original_form": "밥을 먹어야겟어",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100037300.1.46",
          "form": "아닠 그래 ㅋㅋㅋㅋㅋ",
          "original_form": "아닠 그래 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037300.1.47",
          "form": "와 나 리월 상자 900개 열었어",
          "original_form": "와 나 리월 상자 900개 열었어",
