      "utterance": [
        {
          "id": "MDRW2100002776.1.1",
          "form": "눈이 침침한데 뭘 먹어야할까요?",
          "original_form": "눈이 침침한데 뭘 먹어야할까요?",
          "speaker_id": "1",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002776.1.2",
          "form": "루테인 들은거요",
          "original_form": "루테인 들은거요",
          "speaker_id": "2",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002776.1.3",
          "form": "아로나민 아이 엄청 효과 좋은데 추천해드려요",
          "original_form": "아로나민 아이 엄청 효과 좋은데 추천해드려요",
          "speaker_id": "2",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002776.1.4",
          "form": "아로나민에서 눈영양제도 나와요?",
          "original_form": "아로나민에서 눈영양제도 나와요?",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002776.1.5",
          "form": "네 완전 좋은데 부산 약국에는 이제 한군데만 넣는데요",
          "original_form": "네 완전 좋은데 부산 약국에는 이제 한군데만 넣는데요",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002776.1.6",
          "form": "루테인을 먹어야하나 고민중이였는데 함 알아봐야겠네요",
          "original_form": "루테인을 먹어야하나 고민중이였는데 함 알아봐야겠네요",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002776.1.7",
          "form": "나이먹으니 건강신경써야겠더라구요ㅠ",
          "original_form": "나이먹으니 건강신경써야겠더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002776.1.8",
          "form": "지인들한테 추천했는데 다들 좋데요 다른거랑 비교해서도",
          "original_form": "지인들한테 추천했는데 다들 좋데요 다른거랑 비교해서도",
          "speaker_id": "2",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002776.1.9",
          "form": "가격 비싸겠어요 효과도좋으니ㅎ",
          "original_form": "가격 비싸겠어요 효과도좋으니ㅎ",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002776.1.10",
          "form": "이게 약국마다 가격이 틀려요",
          "original_form": "이게 약국마다 가격이 틀려요",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002776.1.11",
          "form": "한통에 25,000~30,000",
          "original_form": "한통에 25,000~30,000",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002776.1.12",
          "form": "비싸네요ㅎ 다른영양제 또드시는거 있으세요?",
          "original_form": "비싸네요ㅎ 다른영양제 또드시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002776.1.13",
          "form": "칼슘제요 우유랑 두유도 먹는데 골밀도 위해서 먹고있어요 ㅎㅎ",
          "original_form": "칼슘제요 우유랑 두유도 먹는데 골밀도 위해서 먹고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002776.1.14",
          "form": "건강 잘챙기고 계시네요 ㅎ",
          "original_form": "건강 잘챙기고 계시네요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002776.1.15",
          "form": "전귀찮아서 못먹겠던데",
          "original_form": "전귀찮아서 못먹겠던데",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002776.1.16",
          "form": "먹었다안먹었다 해요 ㅋ",
          "original_form": "먹었다안먹었다 해요 ㅋ",
          "speaker_id": "2",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002776.1.17",
          "form": "비타민이랑 유산균이라도 챙겨먹어야겠어요",
          "original_form": "비타민이랑 유산균이라도 챙겨먹어야겠어요",
          "speaker_id": "1",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002776.1.18",
          "form": "콜라겐도 드셔요",
          "original_form": "콜라겐도 드셔요",
