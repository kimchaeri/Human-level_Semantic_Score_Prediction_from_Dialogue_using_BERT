      "utterance": [
        {
          "id": "MDRW2100024064.1.1",
          "form": "저 이번주에 자동차극장 가보려구여",
          "original_form": "저 이번주에 자동차극장 가보려구여",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024064.1.2",
          "form": "오어 전에 가보셨어요?",
          "original_form": "오어 전에 가보셨어요?",
          "speaker_id": "2",
          "time": "20210830 22:25"
        },
        {
          "id": "MDRW2100024064.1.3",
          "form": "뭐 상영해요?",
          "original_form": "뭐 상영해요?",
          "speaker_id": "2",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100024064.1.4",
          "form": "예~~~전에 갔는데",
          "original_form": "예~~~전에 갔는데",
          "speaker_id": "1",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100024064.1.5",
          "form": "그때 봤던영화 2를 이번주에 개봉하는거 있죠 ㅋㅋ",
          "original_form": "그때 봤던영화 2를 이번주에 개봉하는거 있죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100024064.1.6",
          "form": "마블영화 개봉하는거 보고싶어서요",
          "original_form": "마블영화 개봉하는거 보고싶어서요",
          "speaker_id": "1",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100024064.1.7",
          "form": "오오 그 샹치 텐링즈 어쩌고오",
          "original_form": "오오 그 샹치 텐링즈 어쩌고오",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024064.1.8",
          "form": "네그거요",
          "original_form": "네그거요",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024064.1.9",
          "form": "아시네요 ㅋㅋ 근데요",
          "original_form": "아시네요 ㅋㅋ 근데요",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024064.1.10",
          "form": "자동차극장 인기폭발이라",
          "original_form": "자동차극장 인기폭발이라",
          "speaker_id": "1",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024064.1.11",
          "form": "3시간전부터 대기해야된대요",
          "original_form": "3시간전부터 대기해야된대요",
          "speaker_id": "1",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024064.1.12",
          "form": "아 진짜요....못가겠당...",
          "original_form": "아 진짜요....못가겠당...",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024064.1.13",
          "form": "자동차극장은 제약이 너무 많아요",
          "original_form": "자동차극장은 제약이 너무 많아요",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024064.1.14",
          "form": "그냥 극장 가는게 낫겠네요 ㅎㅎ...",
          "original_form": "그냥 극장 가는게 낫겠네요 ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100024064.1.15",
          "form": "네.... 그냥 갈까 싶기도 해요",
          "original_form": "네.... 그냥 갈까 싶기도 해요",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100024064.1.16",
          "form": "오붓하게 있기 좋으니깐ㅎㅎㅎ",
          "original_form": "오붓하게 있기 좋으니깐ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100024064.1.17",
          "form": "아 좋겠어요 주말데이트 ",
          "original_form": "아 좋겠어요 주말데이트 {emoji:♡}",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024064.1.18",
          "form": "ㅋㅋㅋ 담주에 하실거면서 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 담주에 하실거면서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100024064.1.19",
          "form": "백신맞고 몸조리 잘하셔요",
          "original_form": "백신맞고 몸조리 잘하셔요",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100024064.1.20",
          "form": "넹 감사합니당 ㅋㅋㅋ여친분도 잘 맞으기길 ㅋㅋㅋㅋ",
          "original_form": "넹 감사합니당 ㅋㅋㅋ여친분도 잘 맞으기길 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100024064.1.21",
          "form": "여친분 후기 듣는게 더 빠르긴 하겠지만",
          "original_form": "여친분 후기 듣는게 더 빠르긴 하겠지만",
          "speaker_id": "2",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100024064.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100024064.1.23",
          "form": "근데 사람마다 다 달라서",
          "original_form": "근데 사람마다 다 달라서",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024064.1.24",
          "form": "설마 백신후기도 비슷한건 아니겠죠",
          "original_form": "설마 백신후기도 비슷한건 아니겠죠",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024064.1.25",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024064.1.26",
          "form": "둘다 비슷하게 많이 안아팠으면 좋겠네요ㅋㅋㅋ",
          "original_form": "둘다 비슷하게 많이 안아팠으면 좋겠네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:12"
        },
        {
          "id": "MDRW2100024064.1.27",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210831 09:39"
        },
        {
          "id": "MDRW2100024064.1.28",
          "form": "여친언니는 고등학교라 미뤄지는거 없이 맞았는데",
          "original_form": "여친언니는 고등학교라 미뤄지는거 없이 맞았는데",
          "speaker_id": "1",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100024064.1.29",
          "form": "좀 아프고 끝났다던데",
          "original_form": "좀 아프고 끝났다던데",
          "speaker_id": "1",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100024064.1.30",
          "form": "오 좀 희망적이네요ㅋㅋㅋ",
          "original_form": "오 좀 희망적이네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:59"
        },
        {
          "id": "MDRW2100024064.1.31",
          "form": "요새 자꾸 무서운 기사만 봐서 부정회로 돌렸는데",
          "original_form": "요새 자꾸 무서운 기사만 봐서 부정회로 돌렸는데",
          "speaker_id": "2",
          "time": "20210831 09:59"
        },
        {
          "id": "MDRW2100024064.1.32",
          "form": "좋은기사는 안나와요",
          "original_form": "좋은기사는 안나와요",
          "speaker_id": "1",
          "time": "20210831 11:58"
        },
        {
          "id": "MDRW2100024064.1.33",
          "form": "죄다 공포감조성 기사만...",
          "original_form": "죄다 공포감조성 기사만...",
          "speaker_id": "1",
          "time": "20210831 11:58"
        },
        {
          "id": "MDRW2100024064.1.34",
          "form": "그래놓고 기자도 맞았을거면서",
          "original_form": "그래놓고 기자도 맞았을거면서",
          "speaker_id": "1",
          "time": "20210831 11:58"
        },
        {
          "id": "MDRW2100024064.1.35",
          "form": "ㅠ아니 오늘 학교 학부모중에 백신맞은 후에 뇌경색 오신 분 계셔서",
          "original_form": "ㅠ아니 오늘 학교 학부모중에 백신맞은 후에 뇌경색 오신 분 계셔서",
          "speaker_id": "2",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100024064.1.36",
          "form": "진짜 겁나네요",
          "original_form": "진짜 겁나네요",
          "speaker_id": "2",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100024064.1.37",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210831 12:32"
        },
        {
          "id": "MDRW2100024064.1.38",
          "form": "정말요?",
          "original_form": "정말요?",
          "speaker_id": "1",
          "time": "20210831 12:32"
        },
        {
          "id": "MDRW2100024064.1.39",
          "form": "넹 ㅠ주변에서 이런 일이 일어나네요 ㅠ",
          "original_form": "넹 ㅠ주변에서 이런 일이 일어나네요 ㅠ",
          "speaker_id": "2",
          "time": "20210831 16:00"
        },
        {
          "id": "MDRW2100024064.1.40",
          "form": "무섭게시리...",
          "original_form": "무섭게시리...",
          "speaker_id": "1",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100024064.1.41",
          "form": "저도 무섭네요",
          "original_form": "저도 무섭네요",
          "speaker_id": "1",
