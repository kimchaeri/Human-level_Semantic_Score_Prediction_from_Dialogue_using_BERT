      "utterance": [
        {
          "id": "MDRW2100009029.1.1",
          "form": "오 보고있지",
          "original_form": "오 보고있지",
          "speaker_id": "2",
          "time": "20210822 20:35"
        },
        {
          "id": "MDRW2100009029.1.2",
          "form": "왓챠 은근재밋는거많아ㅜ",
          "original_form": "왓챠 은근재밋는거많아ㅜ",
          "speaker_id": "2",
          "time": "20210822 20:36"
        },
        {
          "id": "MDRW2100009029.1.3",
          "form": "와이이우먼킬 재밋더라",
          "original_form": "와이이우먼킬 재밋더라",
          "speaker_id": "2",
          "time": "20210822 20:36"
        },
        {
          "id": "MDRW2100009029.1.4",
          "form": "그건 한달에 얼마랬죵 ㅋㅋ",
          "original_form": "그건 한달에 얼마랬죵 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:56"
        },
        {
          "id": "MDRW2100009029.1.5",
          "form": "이건  나도 다른사람 걸쳐서하는거라 돈안냄",
          "original_form": "이건  나도 다른사람 걸쳐서하는거라 돈안냄",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009029.1.6",
          "form": "열심히봐야하는디 시간이 없네 ㅋㅋ 놀고싶어라",
          "original_form": "열심히봐야하는디 시간이 없네 ㅋㅋ 놀고싶어라",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009029.1.7",
          "form": "다큐나 독립애니가 많아 넷플보다",
          "original_form": "다큐나 독립애니가 많아 넷플보다",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009029.1.8",
          "form": "근데 좀 무섭기도한게",
          "original_form": "근데 좀 무섭기도한게",
          "speaker_id": "1",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009029.1.9",
          "form": "하루종일 보고있음ㅜ",
          "original_form": "하루종일 보고있음ㅜ",
          "speaker_id": "1",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009029.1.10",
          "form": "아그건또 무섭네",
          "original_form": "아그건또 무섭네",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009029.1.11",
          "form": "영화나 유투브가 인생의 주가 되어버렸어 ㅋㅋ",
          "original_form": "영화나 유투브가 인생의 주가 되어버렸어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009029.1.12",
          "form": "웨이브 한달무료였는데",
          "original_form": "웨이브 한달무료였는데",
          "speaker_id": "1",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100009029.1.13",
          "form": "소시민마인드폭발해서",
          "original_form": "소시민마인드폭발해서",
          "speaker_id": "1",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009029.1.14",
          "form": "뽕 뽑겠다고",
          "original_form": "뽕 뽑겠다고",
          "speaker_id": "1",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009029.1.15",
          "form": "진짜하루종일본듯",
          "original_form": "진짜하루종일본듯",
          "speaker_id": "1",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009029.1.16",
          "form": "한달을ㅋㅋㅋㅋ",
          "original_form": "한달을ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100009029.1.17",
          "form": "대단하다 나는 힘들어서 못볼거같은디",
          "original_form": "대단하다 나는 힘들어서 못볼거같은디",
          "speaker_id": "2",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100009029.1.18",
          "form": "눈은괜찮음?",
          "original_form": "눈은괜찮음?",
          "speaker_id": "2",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100009029.1.19",
          "form": "병은 안났음 다행히 ㅋㅋ",
          "original_form": "병은 안났음 다행히 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009029.1.20",
          "form": "뻑뻑한정도",
          "original_form": "뻑뻑한정도",
          "speaker_id": "1",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009029.1.21",
          "form": "눈떨림?",
          "original_form": "눈떨림?",
          "speaker_id": "1",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009029.1.22",
          "form": "ㅋㅋ 눈에 눈물넣으면서 봐랑ㅋㅋㅋ",
          "original_form": "ㅋㅋ 눈에 눈물넣으면서 봐랑ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:50"
        },
        {
          "id": "MDRW2100009029.1.23",
          "form": "영어자막없이 보능가?",
          "original_form": "영어자막없이 보능가?",
          "speaker_id": "2",
          "time": "20210822 21:50"
        },
        {
          "id": "MDRW2100009029.1.24",
          "form": "노놉 그냥 자막 알아서나옴",
          "original_form": "노놉 그냥 자막 알아서나옴",
          "speaker_id": "1",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100009029.1.25",
          "form": "뭔가한달지나고나니 차라리 맘편했음 ㅋㅋ",
          "original_form": "뭔가한달지나고나니 차라리 맘편했음 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100009029.1.26",
          "form": "ㅇㅇ ㅋㅋ 폭주하는 우리",
          "original_form": "ㅇㅇ ㅋㅋ 폭주하는 우리",
          "speaker_id": "2",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100009029.1.27",
          "form": "영화는 단편이니까 좋아",
          "original_form": "영화는 단편이니까 좋아",
          "speaker_id": "2",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100009029.1.28",
          "form": "드라마 넘 나오기 힘들어",
          "original_form": "드라마 넘 나오기 힘들어",
          "speaker_id": "2",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100009029.1.29",
          "form": "뭔가 공짜다 뽕뽑자란이유로",
          "original_form": "뭔가 공짜다 뽕뽑자란이유로",
          "speaker_id": "1",
          "time": "20210822 21:58"
        },
        {
          "id": "MDRW2100009029.1.30",
          "form": "더 혹사한 느낌이었음",
          "original_form": "더 혹사한 느낌이었음",
          "speaker_id": "1",
          "time": "20210822 21:58"
        },
        {
          "id": "MDRW2100009029.1.31",
          "form": "무소유가 될지어다 ㅋㅋ",
          "original_form": "무소유가 될지어다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:58"
        },
        {
          "id": "MDRW2100009029.1.32",
          "form": "ㅋㅋㅋ 그래 해봤으니 됐당",
          "original_form": "ㅋㅋㅋ 그래 해봤으니 됐당",
          "speaker_id": "2",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009029.1.33",
          "form": "이번에 아마존프라임들어왔는데 회원가입시 일주일무료래서",
          "original_form": "이번에 아마존프라임들어왔는데 회원가입시 일주일무료래서",
          "speaker_id": "2",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009029.1.34",
          "form": "여유있는날 하려고 기다리고있는중",
          "original_form": "여유있는날 하려고 기다리고있는중",
          "speaker_id": "2",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009029.1.35",
          "form": "오 그건 또 외국자본이 들어왔군요",
          "original_form": "오 그건 또 외국자본이 들어왔군요",
          "speaker_id": "1",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009029.1.36",
          "form": "넷플같은 위력을 발휘할것인가",
          "original_form": "넷플같은 위력을 발휘할것인가",
          "speaker_id": "1",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009029.1.37",
          "form": "ㅋㅋ 재택근무가 많아지면 늘어날텐데말이여  넷플에 킹덤보고싶다",
          "original_form": "ㅋㅋ 재택근무가 많아지면 늘어날텐데말이여  넷플에 킹덤보고싶다",
          "speaker_id": "2",
          "time": "20210822 22:20"
        }
      ]
    }
