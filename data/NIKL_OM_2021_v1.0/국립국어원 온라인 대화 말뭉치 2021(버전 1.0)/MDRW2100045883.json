      "utterance": [
        {
          "id": "MDRW2100045883.1.1",
          "form": "네이버멤버십데이때 구매하시죠?",
          "original_form": "네이버멤버십데이때 구매하시죠?",
          "speaker_id": "2",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100045883.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100045883.1.3",
          "form": "저도 그 이야기하려했어요ㅋㅋㅋㅋ",
          "original_form": "저도 그 이야기하려했어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100045883.1.4",
          "form": "아 너무 웃기다ㅋㅋㅋㄲㅋㅋ",
          "original_form": "아 너무 웃기다ㅋㅋㅋㄲㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100045883.1.5",
          "form": "온라인 폐지도 줍습니다ㅠㅠ",
          "original_form": "온라인 폐지도 줍습니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100045883.1.6",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100045883.1.7",
          "form": "그게뭐에요?",
          "original_form": "그게뭐에요?",
          "speaker_id": "2",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100045883.1.8",
          "form": "저는왜이렇게모르는게많죠?",
          "original_form": "저는왜이렇게모르는게많죠?",
          "speaker_id": "2",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100045883.1.9",
          "form": "그 쇼핑라이브같은 거 보고 댓글 달면",
          "original_form": "그 쇼핑라이브같은 거 보고 댓글 달면",
          "speaker_id": "1",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100045883.1.10",
          "form": "150원 정도 주는데",
          "original_form": "150원 정도 주는데",
          "speaker_id": "1",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100045883.1.11",
          "form": "아아아그걸",
          "original_form": "아아아그걸",
          "speaker_id": "2",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045883.1.12",
          "form": "온라인폐지라고하는군요 ㅋㅋ",
          "original_form": "온라인폐지라고하는군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045883.1.13",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045883.1.14",
          "form": "조금씩 모아서ㅋㅋㅋ",
          "original_form": "조금씩 모아서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045883.1.15",
          "form": "진짜..포인트의노예",
          "original_form": "진짜..포인트의노예",
          "speaker_id": "2",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045883.1.16",
          "form": "완전 푼돈이지만 모으면...",
          "original_form": "완전 푼돈이지만 모으면...",
          "speaker_id": "1",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045883.1.17",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045883.1.18",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045883.1.19",
          "form": "열심히모아서 물건사야죠",
          "original_form": "열심히모아서 물건사야죠",
          "speaker_id": "2",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045883.1.20",
          "form": "저도 힘든데 물욕이 너무 많아서",
          "original_form": "저도 힘든데 물욕이 너무 많아서",
          "speaker_id": "1",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045883.1.21",
          "form": "이렇게라도 모아야...",
          "original_form": "이렇게라도 모아야...",
          "speaker_id": "1",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045883.1.22",
          "form": "저도진짜 물욕미치겠어요",
          "original_form": "저도진짜 물욕미치겠어요",
          "speaker_id": "2",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045883.1.23",
          "form": "저도요 진짜",
          "original_form": "저도요 진짜",
          "speaker_id": "1",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100045883.1.24",
          "form": "감정이허하면그렇다는데",
          "original_form": "감정이허하면그렇다는데",
          "speaker_id": "2",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100045883.1.25",
          "form": "진짜그런건지",
          "original_form": "진짜그런건지",
          "speaker_id": "2",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100045883.1.26",
          "form": "물욕이 정말...",
          "original_form": "물욕이 정말...",
          "speaker_id": "1",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100045883.1.27",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100045883.1.28",
          "form": "아니 부츠가 있으면",
          "original_form": "아니 부츠가 있으면",
          "speaker_id": "1",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100045883.1.29",
          "form": "앵클부츠도 사고싶고",
          "original_form": "앵클부츠도 사고싶고",
          "speaker_id": "1",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100045883.1.30",
          "form": "니하이도 사고싶고",
          "original_form": "니하이도 사고싶고",
          "speaker_id": "1",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100045883.1.31",
          "form": "그죠?",
          "original_form": "그죠?",
          "speaker_id": "1",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100045883.1.32",
          "form": "그냥 다 갖고싶어요",
          "original_form": "그냥 다 갖고싶어요",
          "speaker_id": "1",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100045883.1.33",
          "form": "네 ㅋㅋㅋㅋ 다 다른거잖아요그건",
          "original_form": "네 ㅋㅋㅋㅋ 다 다른거잖아요그건",
          "speaker_id": "2",
