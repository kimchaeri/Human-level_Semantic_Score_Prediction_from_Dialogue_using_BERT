      "utterance": [
        {
          "id": "MDRW2100019863.1.1",
          "form": "제출가능해서 바꿨습니다 넷플릭스 보시나요?",
          "original_form": "제출가능해서 바꿨습니다 넷플릭스 보시나요?",
          "speaker_id": "1",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019863.1.2",
          "form": "네네ㅋ 넷플 얘기 아까 했었는데...!",
          "original_form": "네네ㅋ 넷플 얘기 아까 했었는데...!",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019863.1.3",
          "form": "예전에 보다가 지금은 안봐요ㅎ",
          "original_form": "예전에 보다가 지금은 안봐요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019863.1.4",
          "form": "지금 다른분들이랑 섞여서 헷갈려요",
          "original_form": "지금 다른분들이랑 섞여서 헷갈려요",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019863.1.5",
          "form": "똑같은걸 선택했나보네요ㅋㅋㅋㅋ 스포츠 피하려다가",
          "original_form": "똑같은걸 선택했나보네요ㅋㅋㅋㅋ 스포츠 피하려다가",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019863.1.6",
          "form": "ㅋㅋㅋㅋ영화이야기할까요...ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ영화이야기할까요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019863.1.7",
          "form": "아님 보시는 예능프로있어요?",
          "original_form": "아님 보시는 예능프로있어요?",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019863.1.8",
          "form": "무한도전 다시보고있어요",
          "original_form": "무한도전 다시보고있어요",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019863.1.9",
          "form": "무한도전은 다시봐도 재밌죠ㅋㅋ",
          "original_form": "무한도전은 다시봐도 재밌죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019863.1.10",
          "form": "옛날에 봤던걸 대부분 잊어먹어서",
          "original_form": "옛날에 봤던걸 대부분 잊어먹어서",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019863.1.11",
          "form": "너무 재밌더라고요",
          "original_form": "너무 재밌더라고요",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019863.1.12",
          "form": "전 요즘 그냥 보이는대로 재방하면 보는데 요즘에는 올림픽선수들이 게스트로 많이 나오더라구요ㅋ",
          "original_form": "전 요즘 그냥 보이는대로 재방하면 보는데 요즘에는 올림픽선수들이 게스트로 많이 나오더라구요ㅋ",
          "speaker_id": "2",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019863.1.13",
          "form": "영화는 스파이더맨 기대중입니다",
          "original_form": "영화는 스파이더맨 기대중입니다",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019863.1.14",
          "form": "스파이더맨 언제가 개봉이죠?ㅋㅋ",
          "original_form": "스파이더맨 언제가 개봉이죠?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019863.1.15",
          "form": "12월에 개봉입니다",
          "original_form": "12월에 개봉입니다",
          "speaker_id": "1",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019863.1.16",
          "form": "오...! 그 때쯤 되면 코로나도 좀 사그라들겠죠ㅜ?",
          "original_form": "오...! 그 때쯤 되면 코로나도 좀 사그라들겠죠ㅜ?",
          "speaker_id": "2",
          "time": "20210827 22:39"
        },
        {
          "id": "MDRW2100019863.1.17",
          "form": "영화는 극장에서 봐야 재밌는데 ㅜ",
          "original_form": "영화는 극장에서 봐야 재밌는데 ㅜ",
          "speaker_id": "2",
          "time": "20210827 22:39"
        },
        {
          "id": "MDRW2100019863.1.18",
          "form": "그러게요... 제일 최근에 극장에서 본게 1917이예요",
          "original_form": "그러게요... 제일 최근에 극장에서 본게 1917이예요",
          "speaker_id": "1",
          "time": "20210827 22:39"
        },
        {
          "id": "MDRW2100019863.1.19",
          "form": "전 언제 봤는지 기억도 안나요...ㅋㅋㅋㅋㅋ",
          "original_form": "전 언제 봤는지 기억도 안나요...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019863.1.20",
          "form": "집에서 구매해서 보기만하고...",
          "original_form": "집에서 구매해서 보기만하고...",
          "speaker_id": "2",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019863.1.21",
          "form": "마지막으로 본 게...크루엘라?",
          "original_form": "마지막으로 본 게...크루엘라?",
          "speaker_id": "2",
          "time": "20210827 22:41"
        }
      ]
    }
