      "utterance": [
        {
          "id": "MDRW2100041981.1.1",
          "form": "교직원 뭔가 준공무원급같아요",
          "original_form": "교직원 뭔가 준공무원급같아요",
          "speaker_id": "1",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100041981.1.2",
          "form": "와,, 그러면 좀 일 일찍 끝나는 날에는",
          "original_form": "와,, 그러면 좀 일 일찍 끝나는 날에는",
          "speaker_id": "2",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100041981.1.3",
          "form": "보통 9시부터 5시 근무 맞죠?!",
          "original_form": "보통 9시부터 5시 근무 맞죠?!",
          "speaker_id": "2",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100041981.1.4",
          "form": "부업 할 수도 있고..... 너무 좋을 것 같은데요....?",
          "original_form": "부업 할 수도 있고..... 너무 좋을 것 같은데요....?",
          "speaker_id": "2",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100041981.1.5",
          "form": "5시반~6시쯤 끝나요!",
          "original_form": "5시반~6시쯤 끝나요!",
          "speaker_id": "1",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100041981.1.6",
          "form": "근데 사업하나맡으면 야근도많이하기도해요ㅠ",
          "original_form": "근데 사업하나맡으면 야근도많이하기도해요ㅠ",
          "speaker_id": "1",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100041981.1.7",
          "form": "부서바이부서",
          "original_form": "부서바이부서",
          "speaker_id": "1",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100041981.1.8",
          "form": "헉 그렇군요 진짜...",
          "original_form": "헉 그렇군요 진짜...",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.9",
          "form": "하 뭐든 쉬운 게 없습니다ㅠ",
          "original_form": "하 뭐든 쉬운 게 없습니다ㅠ",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.10",
          "form": "들디님 어학성적 만들어놓으시면",
          "original_form": "들디님 어학성적 만들어놓으시면",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.11",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.12",
          "form": "국제교류처같은곳은",
          "original_form": "국제교류처같은곳은",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.13",
          "form": "가기좋을거에요",
          "original_form": "가기좋을거에요",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.14",
          "form": "우...우와",
          "original_form": "우...우와",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.15",
          "form": "전공이",
          "original_form": "전공이",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.16",
          "form": "이대 국교 지금 계약직",
          "original_form": "이대 국교 지금 계약직",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.17",
          "form": "뽑더라고요",
          "original_form": "뽑더라고요",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.18",
          "form": "무관해도요?!",
          "original_form": "무관해도요?!",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.19",
          "form": "네네 8ㅅ8",
          "original_form": "네네 8ㅅ8",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.20",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.21",
          "form": "헉 헉",
          "original_form": "헉 헉",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.22",
          "form": "영어나 중국어하나만",
          "original_form": "영어나 중국어하나만",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.23",
          "form": "우와 그렇군요,, 이렇게 좋은 정보를 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "우와 그렇군요,, 이렇게 좋은 정보를 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.24",
          "form": "네네네네,,,",
          "original_form": "네네네네,,,",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.25",
          "form": "잘해도 엄청좋아해요",
          "original_form": "잘해도 엄청좋아해요",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041981.1.26",
          "form": "그럴거같네요,,, 저는 중국어는 이제",
          "original_form": "그럴거같네요,,, 저는 중국어는 이제",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041981.1.27",
          "form": "가나다 배우는 수준인데",
          "original_form": "가나다 배우는 수준인데",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041981.1.28",
          "form": "저희국교남 영어프리토킹되는거 빼고 별거없는,,,",
          "original_form": "저희국교남 영어프리토킹되는거 빼고 별거없는,,,",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041981.1.29",
          "form": "허허....",
          "original_form": "허허....",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041981.1.30",
          "form": "와,,,",
          "original_form": "와,,,",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041981.1.31",
          "form": "저는 프리토킹이 안되는데 (?)",
          "original_form": "저는 프리토킹이 안되는데 (?)",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041981.1.32",
          "form": "ㅋㄱㄱㅋㄱㄱㅋㄱㄱ저도요^-^,,,",
          "original_form": "ㅋㄱㄱㅋㄱㄱㅋㄱㄱ저도요^-^,,,",
