      "utterance": [
        {
          "id": "MDRW2100026353.1.1",
          "form": "다 못먹고 남김 양이 많아서 ㅋ",
          "original_form": "다 못먹고 남김 양이 많아서 ㅋ",
          "speaker_id": "2",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100026353.1.2",
          "form": "으음ㅜㅜ",
          "original_form": "으음ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100026353.1.3",
          "form": "치킨도 시킨거예요?",
          "original_form": "치킨도 시킨거예요?",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100026353.1.4",
          "form": "그게 세트메뉴야",
          "original_form": "그게 세트메뉴야",
          "speaker_id": "2",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100026353.1.5",
          "form": "떡볶이랑 치킨 같이와 ㅋ",
          "original_form": "떡볶이랑 치킨 같이와 ㅋ",
          "speaker_id": "2",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100026353.1.6",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100026353.1.7",
          "form": "다드셨어여?ㅇㅅㅇ",
          "original_form": "다드셨어여?ㅇㅅㅇ",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100026353.1.8",
          "form": "언니 오늘도 고생많으셨어요",
          "original_form": "언니 오늘도 고생많으셨어요",
          "speaker_id": "1",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100026353.1.9",
          "form": "웅 name1두~~~",
          "original_form": "웅 &name1&두~~~",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100026353.1.10",
          "form": "고생많았어!!!!",
          "original_form": "고생많았어!!!!",
          "speaker_id": "2",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100026353.1.11",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100026353.1.12",
          "form": "탕비실도 언니가 다 챙겨야되고",
          "original_form": "탕비실도 언니가 다 챙겨야되고",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100026353.1.13",
          "form": "외근도 언니가",
          "original_form": "외근도 언니가",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100026353.1.14",
          "form": "챙길게 많으실텐데ㅜㅜ",
          "original_form": "챙길게 많으실텐데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100026353.1.15",
          "form": "탕비실에 커피랑 과자를 항상 채워두는데",
          "original_form": "탕비실에 커피랑 과자를 항상 채워두는데",
          "speaker_id": "2",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100026353.1.16",
          "form": "건강 꼭 잘챙기시고요ㅜ",
          "original_form": "건강 꼭 잘챙기시고요ㅜ",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100026353.1.17",
          "form": "커피먹고 흘리는것도 다반사고",
          "original_form": "커피먹고 흘리는것도 다반사고",
          "speaker_id": "2",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100026353.1.18",
          "form": "흘리면 닦아야하는데 안닦아",
          "original_form": "흘리면 닦아야하는데 안닦아",
          "speaker_id": "2",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100026353.1.19",
          "form": "ㅜㅜ왜 안닦아요?",
          "original_form": "ㅜㅜ왜 안닦아요?",
          "speaker_id": "1",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100026353.1.20",
          "form": "언니가 안치우면 더럽게 살았던건가",
          "original_form": "언니가 안치우면 더럽게 살았던건가",
          "speaker_id": "1",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100026353.1.21",
          "form": "나도 모르겠어 ~",
          "original_form": "나도 모르겠어 ~",
          "speaker_id": "2",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100026353.1.22",
          "form": "커피를 냉장고에도 흘리고 서랍장에도 흘리고",
          "original_form": "커피를 냉장고에도 흘리고 서랍장에도 흘리고",
          "speaker_id": "2",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100026353.1.23",
          "form": "ㅜㅜ그럼 언니 처음 입사할때부터 더러웠어요?",
          "original_form": "ㅜㅜ그럼 언니 처음 입사할때부터 더러웠어요?",
          "speaker_id": "1",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100026353.1.24",
          "form": "어후",
          "original_form": "어후",
          "speaker_id": "1",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100026353.1.25",
          "form": "싱크대도 정수기도",
          "original_form": "싱크대도 정수기도",
          "speaker_id": "2",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100026353.1.26",
          "form": "바닥에도",
          "original_form": "바닥에도",
          "speaker_id": "2",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100026353.1.27",
          "form": "다흘려",
          "original_form": "다흘려",
          "speaker_id": "2",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100026353.1.28",
          "form": "입사할때보다 더 심해지는듯 ㅋ",
          "original_form": "입사할때보다 더 심해지는듯 ㅋ",
          "speaker_id": "2",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100026353.1.29",
          "form": "으헉ㅜㅜ",
          "original_form": "으헉ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100026353.1.30",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100026353.1.31",
          "form": "언니가 다 챙겨주니까 더 그런듯ㅜ",
          "original_form": "언니가 다 챙겨주니까 더 그런듯ㅜ",
          "speaker_id": "1",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100026353.1.32",
          "form": "응 입사때보다도 일도 더 힘들어지는데",
          "original_form": "응 입사때보다도 일도 더 힘들어지는데",
          "speaker_id": "2",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100026353.1.33",
          "form": "3배는 더 힘들어졋어",
          "original_form": "3배는 더 힘들어졋어",
          "speaker_id": "2",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100026353.1.34",
          "form": "허..ㅜㅜ",
          "original_form": "허..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100026353.1.35",
          "form": "3배..",
          "original_form": "3배..",
          "speaker_id": "1",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100026353.1.36",
          "form": "언니가 잘하는만큼 나머지도 으쌰으쌰해야되는데",
          "original_form": "언니가 잘하는만큼 나머지도 으쌰으쌰해야되는데",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100026353.1.37",
          "form": "오히려 일을 안하니ㅜㅜ",
          "original_form": "오히려 일을 안하니ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100026353.1.38",
          "form": "과자도 먹고 그냥 그자리에 버림",
          "original_form": "과자도 먹고 그냥 그자리에 버림",
          "speaker_id": "2",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100026353.1.39",
          "form": "ㅋㅋㅋㅋㅋ진짜 ....욕이나와",
          "original_form": "ㅋㅋㅋㅋㅋ진짜 ....욕이나와",
          "speaker_id": "2",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100026353.1.40",
          "form": "헐?",
          "original_form": "헐?",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100026353.1.41",
          "form": "언니 치워보라고해도",
          "original_form": "언니 치워보라고해도",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100026353.1.42",
          "form": "그러는거예요?",
          "original_form": "그러는거예요?",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100026353.1.43",
          "form": "진짜 답이없다",
          "original_form": "진짜 답이없다",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100026353.1.44",
          "form": "누가 그런건지 내 앞에서 그런게 아니라서",
          "original_form": "누가 그런건지 내 앞에서 그런게 아니라서",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100026353.1.45",
          "form": "그냥 내가 치워..",
          "original_form": "그냥 내가 치워..",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100026353.1.46",
          "form": "얼음도 나만 채우고 아무도 안채움 ㅋ빼먹고 물을 안얼려놔 ㅋ",
          "original_form": "얼음도 나만 채우고 아무도 안채움 ㅋ빼먹고 물을 안얼려놔 ㅋ",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100026353.1.47",
          "form": "빈 트레이만 쏙 넣음",
          "original_form": "빈 트레이만 쏙 넣음",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100026353.1.48",
          "form": "아이고..",
          "original_form": "아이고..",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100026353.1.49",
          "form": "진짜욕이 안나올수가 없다",
          "original_form": "진짜욕이 안나올수가 없다",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100026353.1.50",
          "form": "화장실도 마찬가지 ㅋ",
          "original_form": "화장실도 마찬가지 ㅋ",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100026353.1.51",
          "form": "이런 기본적인것들도 제대로 안하고",
          "original_form": "이런 기본적인것들도 제대로 안하고",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100026353.1.52",
          "form": "아이고ㅜ",
          "original_form": "아이고ㅜ",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100026353.1.53",
          "form": "그래도 중간중간 스트레스 풀어야지 안그러면 번아웃돼~",
          "original_form": "그래도 중간중간 스트레스 풀어야지 안그러면 번아웃돼~",
          "speaker_id": "2",
          "time": "20210905 22:33"
        }
      ]
    }
