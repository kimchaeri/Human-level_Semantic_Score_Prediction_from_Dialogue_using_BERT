      "utterance": [
        {
          "id": "MDRW2100000329.1.1",
          "form": "주식 진짜 많이 빠짐",
          "original_form": "주식 진짜 많이 빠짐",
          "speaker_id": "1",
          "time": "20210818 15:40"
        },
        {
          "id": "MDRW2100000329.1.2",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210818 15:40"
        },
        {
          "id": "MDRW2100000329.1.3",
          "form": "몇살까지 살려고",
          "original_form": "몇살까지 살려고",
          "speaker_id": "2",
          "time": "20210818 15:40"
        },
        {
          "id": "MDRW2100000329.1.4",
          "form": "급여차이는 갈수록 줄어든대",
          "original_form": "급여차이는 갈수록 줄어든대",
          "speaker_id": "1",
          "time": "20210818 15:40"
        },
        {
          "id": "MDRW2100000329.1.5",
          "form": "무슨말이지?",
          "original_form": "무슨말이지?",
          "speaker_id": "2",
          "time": "20210818 15:40"
        },
        {
          "id": "MDRW2100000329.1.6",
          "form": "그런데 자산차이는 계속 늘어난대",
          "original_form": "그런데 자산차이는 계속 늘어난대",
          "speaker_id": "1",
          "time": "20210818 15:40"
        },
        {
          "id": "MDRW2100000329.1.7",
          "form": "빈부격차가",
          "original_form": "빈부격차가",
          "speaker_id": "1",
          "time": "20210818 15:40"
        },
        {
          "id": "MDRW2100000329.1.8",
          "form": "급여가 평준화 된다고?",
          "original_form": "급여가 평준화 된다고?",
          "speaker_id": "2",
          "time": "20210818 15:40"
        },
        {
          "id": "MDRW2100000329.1.9",
          "form": "말이 되나",
          "original_form": "말이 되나",
          "speaker_id": "2",
          "time": "20210818 15:41"
        },
        {
          "id": "MDRW2100000329.1.10",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210818 15:41"
        },
        {
          "id": "MDRW2100000329.1.11",
          "form": "급여차이는 줄어들고있어",
          "original_form": "급여차이는 줄어들고있어",
          "speaker_id": "1",
          "time": "20210818 15:41"
        },
        {
          "id": "MDRW2100000329.1.12",
          "form": "통계가그래",
          "original_form": "통계가그래",
          "speaker_id": "1",
          "time": "20210818 15:41"
        },
        {
          "id": "MDRW2100000329.1.13",
          "form": "난 5년 동결",
          "original_form": "난 5년 동결",
          "speaker_id": "2",
          "time": "20210818 15:41"
        },
        {
          "id": "MDRW2100000329.1.14",
          "form": "그런데 자산의 가치상승이 급여상승보다 더빠른데",
          "original_form": "그런데 자산의 가치상승이 급여상승보다 더빠른데",
          "speaker_id": "1",
          "time": "20210818 15:41"
        },
        {
          "id": "MDRW2100000329.1.15",
          "form": "공장 시급제들 5년 전보다 더 줄어들고",
          "original_form": "공장 시급제들 5년 전보다 더 줄어들고",
          "speaker_id": "2",
          "time": "20210818 15:41"
        },
        {
          "id": "MDRW2100000329.1.16",
          "form": "그래서 자산으로 빈부격차가 발생핫대",
          "original_form": "그래서 자산으로 빈부격차가 발생핫대",
          "speaker_id": "1",
          "time": "20210818 15:41"
        },
        {
          "id": "MDRW2100000329.1.17",
          "form": "그러니 코인에 다 몰리지",
          "original_form": "그러니 코인에 다 몰리지",
          "speaker_id": "2",
          "time": "20210818 15:41"
        },
        {
          "id": "MDRW2100000329.1.18",
          "form": "내 친구 4억 넣었다",
          "original_form": "내 친구 4억 넣었다",
          "speaker_id": "2",
          "time": "20210818 15:42"
        },
        {
          "id": "MDRW2100000329.1.19",
          "form": "왕",
          "original_form": "왕",
          "speaker_id": "1",
          "time": "20210818 15:42"
        },
        {
          "id": "MDRW2100000329.1.20",
