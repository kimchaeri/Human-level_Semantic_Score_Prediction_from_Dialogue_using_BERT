      "utterance": [
        {
          "id": "MDRW2100003182.1.1",
          "form": "자만추를 선호하시나요?",
          "original_form": "자만추를 선호하시나요?",
          "speaker_id": "1",
          "time": "20210820 20:12"
        },
        {
          "id": "MDRW2100003182.1.2",
          "form": "앗 저는 지금은 유부이고",
          "original_form": "앗 저는 지금은 유부이고",
          "speaker_id": "2",
          "time": "20210820 22:04"
        },
        {
          "id": "MDRW2100003182.1.3",
          "form": "자만추를 선호했어요",
          "original_form": "자만추를 선호했어요",
          "speaker_id": "2",
          "time": "20210820 22:05"
        },
        {
          "id": "MDRW2100003182.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210820 22:05"
        },
        {
          "id": "MDRW2100003182.1.5",
          "form": "아~ 제 연애는 한번빼곤...  다 소개팅 통해서 였어요",
          "original_form": "아~ 제 연애는 한번빼곤...  다 소개팅 통해서 였어요",
          "speaker_id": "1",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003182.1.6",
          "form": "전 미혼이거든요",
          "original_form": "전 미혼이거든요",
          "speaker_id": "1",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003182.1.7",
          "form": "결혼하시니 어때요?ㅋ",
          "original_form": "결혼하시니 어때요?ㅋ",
          "speaker_id": "1",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003182.1.8",
          "form": "오! 저는 소개팅으로는\r\n잘된적이 없던거 같아요ㅋㅋ",
          "original_form": "오! 저는 소개팅으로는\r\n잘된적이 없던거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003182.1.9",
          "form": "결혼하니....연애가 그립네요? ㅋㅋㅋ",
          "original_form": "결혼하니....연애가 그립네요? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003182.1.10",
          "form": "ㅋㅋㅋ 결혼 몇년차신가요?",
          "original_form": "ㅋㅋㅋ 결혼 몇년차신가요?",
          "speaker_id": "1",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003182.1.11",
          "form": "저는 7년차입니당:)",
          "original_form": "저는 7년차입니당:)",
          "speaker_id": "2",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003182.1.12",
          "form": "name1님은 지금 연애중이세요?",
          "original_form": "&name1&님은 지금 연애중이세요?",
          "speaker_id": "2",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003182.1.13",
          "form": "네 여친있어요 ㅋㅋ",
          "original_form": "네 여친있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003182.1.14",
          "form": "7년차요?",
          "original_form": "7년차요?",
          "speaker_id": "1",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003182.1.15",
          "form": "그럼 아이도 있으시겠네요",
          "original_form": "그럼 아이도 있으시겠네요",
          "speaker_id": "1",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003182.1.16",
          "form": "연애하신지 얼마나 되셨어요?",
          "original_form": "연애하신지 얼마나 되셨어요?",
          "speaker_id": "2",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003182.1.17",
          "form": "저는 두아이의 엄마입니다ㅋㅋ",
          "original_form": "저는 두아이의 엄마입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003182.1.18",
          "form": "저 5년차 ㅋㅋ 결혼해야죠..ㅋㅋ",
          "original_form": "저 5년차 ㅋㅋ 결혼해야죠..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003182.1.19",
          "form": "두아이 엄마시라구요? 와우",
          "original_form": "두아이 엄마시라구요? 와우",
          "speaker_id": "1",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003182.1.20",
          "form": "그래서 답장이 늦으셨구나",
          "original_form": "그래서 답장이 늦으셨구나",
          "speaker_id": "1",
          "time": "20210820 22:16"
        },
        {
          "id": "MDRW2100003182.1.21",
          "form": "와 연애 5년이라니!!!!!",
          "original_form": "와 연애 5년이라니!!!!!",
          "speaker_id": "2",
          "time": "20210820 22:16"
        },
        {
          "id": "MDRW2100003182.1.22",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:16"
        },
        {
          "id": "MDRW2100003182.1.23",
          "form": "저는 그렇게 긴 연애를\r\n해본적이 없어서 ㅋㅋ",
          "original_form": "저는 그렇게 긴 연애를\r\n해본적이 없어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:16"
        },
        {
          "id": "MDRW2100003182.1.24",
          "form": "신기해요:)",
          "original_form": "신기해요:)",
          "speaker_id": "2",
          "time": "20210820 22:16"
        },
        {
          "id": "MDRW2100003182.1.25",
          "form": "몇년 연애하고 결혼하셨어요?",
          "original_form": "몇년 연애하고 결혼하셨어요?",
          "speaker_id": "1",
          "time": "20210820 22:16"
        },
        {
          "id": "MDRW2100003182.1.26",
          "form": "저는 1년 조금 안되서 결혼했어요",
          "original_form": "저는 1년 조금 안되서 결혼했어요",
          "speaker_id": "2",
          "time": "20210820 22:17"
        },
        {
          "id": "MDRW2100003182.1.27",
          "form": "와우..ㅈ",
          "original_form": "와우..ㅈ",
          "speaker_id": "1",
          "time": "20210820 22:19"
        },
        {
          "id": "MDRW2100003182.1.28",
          "form": "1년도 안되서 하시다니",
          "original_form": "1년도 안되서 하시다니",
          "speaker_id": "1",
          "time": "20210820 22:19"
        },
        {
          "id": "MDRW2100003182.1.29",
          "form": "진짜 좋아하셨나보다",
          "original_form": "진짜 좋아하셨나보다",
          "speaker_id": "1",
          "time": "20210820 22:19"
        },
        {
          "id": "MDRW2100003182.1.30",
          "form": "아 연애는 짧지만",
          "original_form": "아 연애는 짧지만",
          "speaker_id": "2",
          "time": "20210820 22:20"
        },
        {
          "id": "MDRW2100003182.1.31",
          "form": "대학동기라 알고 지낸 시간도 길고ㅋㅋ",
          "original_form": "대학동기라 알고 지낸 시간도 길고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:20"
        },
        {
          "id": "MDRW2100003182.1.32",
          "form": "대학때 썸타다 군대 갔었어서요ㅋㅋ",
          "original_form": "대학때 썸타다 군대 갔었어서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:20"
        },
        {
          "id": "MDRW2100003182.1.33",
          "form": "이후에 돌고돌아 만나서 결혼까지\r\n하게 되었네요ㅋㅋ",
          "original_form": "이후에 돌고돌아 만나서 결혼까지\r\n하게 되었네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:20"
        },
        {
          "id": "MDRW2100003182.1.34",
          "form": "드라마군요",
          "original_form": "드라마군요",
          "speaker_id": "1",
          "time": "20210820 22:21"
        },
        {
          "id": "MDRW2100003182.1.35",
          "form": "연애때가 그리우세요?ㅋㅋㅋ",
          "original_form": "연애때가 그리우세요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:21"
        },
        {
          "id": "MDRW2100003182.1.36",
          "form": "그럼요ㅋㅋㅋ 연애때 그립죠",
          "original_form": "그럼요ㅋㅋㅋ 연애때 그립죠",
          "speaker_id": "2",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100003182.1.37",
          "form": "그럼 7년차시니까...",
          "original_form": "그럼 7년차시니까...",
          "speaker_id": "1",
          "time": "20210820 22:24"
        },
        {
          "id": "MDRW2100003182.1.38",
          "form": "결혼준비할때",
          "original_form": "결혼준비할때",
          "speaker_id": "1",
          "time": "20210820 22:24"
        },
        {
          "id": "MDRW2100003182.1.39",
          "form": "안싸우고 잘하는법 알려주세요",
          "original_form": "안싸우고 잘하는법 알려주세요",
          "speaker_id": "1",
          "time": "20210820 22:24"
        },
        {
          "id": "MDRW2100003182.1.40",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:25"
        },
        {
          "id": "MDRW2100003182.1.41",
          "form": "피터지게 싸웠어서ㅜㅜ",
          "original_form": "피터지게 싸웠어서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 22:28"
        },
        {
          "id": "MDRW2100003182.1.42",
          "form": "저도 좀 듣고싶네여ㅋㅋ",
          "original_form": "저도 좀 듣고싶네여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:28"
        },
        {
          "id": "MDRW2100003182.1.43",
          "form": "안싸우고 하셨던 분들 썰",
          "original_form": "안싸우고 하셨던 분들 썰",
          "speaker_id": "2",
          "time": "20210820 22:28"
        },
        {
          "id": "MDRW2100003182.1.44",
          "form": "ㅋㅋㅋㅋ 피터지게요????",
          "original_form": "ㅋㅋㅋㅋ 피터지게요????",
          "speaker_id": "1",
          "time": "20210820 22:30"
        },
        {
          "id": "MDRW2100003182.1.45",
          "form": "뭘로 그렇게 싸우나요?",
          "original_form": "뭘로 그렇게 싸우나요?",
          "speaker_id": "1",
          "time": "20210820 22:30"
        },
        {
          "id": "MDRW2100003182.1.46",
          "form": "아직 경험해보지 못해서",
          "original_form": "아직 경험해보지 못해서",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003182.1.47",
          "form": "미리 듣고프네요",
          "original_form": "미리 듣고프네요",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003182.1.48",
          "form": "지금은 추억이 되셨을테니",
          "original_form": "지금은 추억이 되셨을테니",
          "speaker_id": "1",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003182.1.49",
          "form": "자 어서 썰을 풀어주세요",
          "original_form": "자 어서 썰을 풀어주세요",
          "speaker_id": "1",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003182.1.50",
          "form": "썰이라... \r\n미담이 없어서 결혼을\r\n앞 둔 커플에게 들려드릴\r\n이야기가 없어요ㅠㅠㅋ",
          "original_form": "썰이라... \r\n미담이 없어서 결혼을\r\n앞 둔 커플에게 들려드릴\r\n이야기가 없어요ㅠㅠㅋ",
          "speaker_id": "2",
