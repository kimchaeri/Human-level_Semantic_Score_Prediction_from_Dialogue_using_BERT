      "utterance": [
        {
          "id": "MDRW2100028943.1.1",
          "form": "오늘 뭐 먹었어?",
          "original_form": "오늘 뭐 먹었어?",
          "speaker_id": "1",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100028943.1.2",
          "form": "뭔가 시켜먹었던거 같은데... 뭐먹엇더라",
          "original_form": "뭔가 시켜먹었던거 같은데... 뭐먹엇더라",
          "speaker_id": "2",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028943.1.3",
          "form": "나는 연어초밥이랑 새우튀김 먹었다",
          "original_form": "나는 연어초밥이랑 새우튀김 먹었다",
          "speaker_id": "1",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028943.1.4",
          "form": "헐 맛있었겠다",
          "original_form": "헐 맛있었겠다",
          "speaker_id": "2",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028943.1.5",
          "form": "연어 소중해... 어디 나가서 먹은거야?",
          "original_form": "연어 소중해... 어디 나가서 먹은거야?",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028943.1.6",
          "form": "그쵸? 넘 좋았어 다 내가 좋아하는거라",
          "original_form": "그쵸? 넘 좋았어 다 내가 좋아하는거라",
          "speaker_id": "1",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028943.1.7",
          "form": "아니 사서 먹음!",
          "original_form": "아니 사서 먹음!",
          "speaker_id": "1",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028943.1.8",
          "form": "사온거?",
          "original_form": "사온거?",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028943.1.9",
          "form": "응!!!사서 먹었어",
          "original_form": "응!!!사서 먹었어",
          "speaker_id": "1",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100028943.1.10",
          "form": "너는 배달 어떤거 시켰어?",
          "original_form": "너는 배달 어떤거 시켰어?",
          "speaker_id": "1",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028943.1.11",
          "form": "생각났다. 영양밥이랑 고기!",
          "original_form": "생각났다. 영양밥이랑 고기!",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028943.1.12",
          "form": "요샌 영양밥 집도 배달을 하더라고. 밥에 연근도 들어가있었어",
          "original_form": "요샌 영양밥 집도 배달을 하더라고. 밥에 연근도 들어가있었어",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028943.1.13",
          "form": "영양밥 맛있어???",
          "original_form": "영양밥 맛있어???",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028943.1.14",
          "form": "맛있어~ 버섯 들어간 건 별론데",
          "original_form": "맛있어~ 버섯 들어간 건 별론데",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028943.1.15",
          "form": "난 별로 안 좋아하는데 역시 어른 입맛 같아",
          "original_form": "난 별로 안 좋아하는데 역시 어른 입맛 같아",
          "speaker_id": "1",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028943.1.16",
          "form": "콩, 전복, 연근 같은건 좋더라고",
          "original_form": "콩, 전복, 연근 같은건 좋더라고",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028943.1.17",
          "form": "어른입맛이래서 지금 웃고있어",
          "original_form": "어른입맛이래서 지금 웃고있어",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028943.1.18",
          "form": "약간 호불호가 있잖아",
          "original_form": "약간 호불호가 있잖아",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028943.1.19",
          "form": "건강해지는 맛은 다 호불호갈리는듯",
          "original_form": "건강해지는 맛은 다 호불호갈리는듯",
          "speaker_id": "2",
          "time": "20210902 23:09"
        }
      ]
