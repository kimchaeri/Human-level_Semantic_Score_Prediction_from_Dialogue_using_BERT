          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028193.1.15",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028193.1.16",
          "form": "소각로같은건가?",
          "original_form": "소각로같은건가?",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028193.1.17",
          "form": "뭐 먹는데도 과부하 걸리는",
          "original_form": "뭐 먹는데도 과부하 걸리는",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028193.1.18",
          "form": "골골몸",
          "original_form": "골골몸",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028193.1.19",
