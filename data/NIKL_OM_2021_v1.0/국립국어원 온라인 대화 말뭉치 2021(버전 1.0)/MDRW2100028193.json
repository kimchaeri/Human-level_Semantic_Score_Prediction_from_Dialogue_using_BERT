          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028193.1.23",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028193.1.24",
          "form": "부채살 크게 썰어서",
          "original_form": "부채살 크게 썰어서",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028193.1.25",
          "form": "감자가득이랑",
          "original_form": "감자가득이랑",
