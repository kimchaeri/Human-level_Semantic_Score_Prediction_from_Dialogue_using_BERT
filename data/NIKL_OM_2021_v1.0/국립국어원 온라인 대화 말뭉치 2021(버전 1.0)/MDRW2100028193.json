          "id": "MDRW2100028193.1.33",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028193.1.34",
          "form": "소고기는 언제나 맛잇다고요",
          "original_form": "소고기는 언제나 맛잇다고요",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028193.1.35",
          "form": "당신 소고기 배척해?",
          "original_form": "당신 소고기 배척해?",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028193.1.36",
          "form": "하지만 카래는 돼지고기가국룰",
          "original_form": "하지만 카래는 돼지고기가국룰",
          "speaker_id": "2",
          "time": "20210902 21:01"
