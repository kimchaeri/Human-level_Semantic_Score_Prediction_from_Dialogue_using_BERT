          "original_form": "부채살",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028193.1.27",
          "form": "양파도많이넣고",
          "original_form": "양파도많이넣고",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028193.1.28",
          "form": "단백질 함량이 높아 운동할 때 먹으면 좋은 부위",
          "original_form": "단백질 함량이 높아 운동할 때 먹으면 좋은 부위",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028193.1.29",
          "form": "를 드셨네요",
          "original_form": "를 드셨네요",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028193.1.30",
          "form": "소고기도 카레에넣으니까맛있어요",
          "original_form": "소고기도 카레에넣으니까맛있어요",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028193.1.31",
          "form": "몸키우세요?",
          "original_form": "몸키우세요?",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028193.1.32",
          "form": "담에 먹어보세요",
          "original_form": "담에 먹어보세요",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
