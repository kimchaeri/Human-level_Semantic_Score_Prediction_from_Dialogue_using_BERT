      "utterance": [
        {
          "id": "MDRW2100038879.1.1",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100038879.1.2",
          "form": "겉은 코팅하고 속은 바삭하게",
          "original_form": "겉은 코팅하고 속은 바삭하게",
          "speaker_id": "1",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100038879.1.3",
          "form": "너무 뜨거우면 다같이 눅눅해지지 않아요?",
          "original_form": "너무 뜨거우면 다같이 눅눅해지지 않아요?",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100038879.1.4",
          "form": "식은건 질기고",
          "original_form": "식은건 질기고",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100038879.1.5",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100038879.1.6",
          "form": "이연복 셰프가게는 안 그럴것 같아 가보고 싶은데",
          "original_form": "이연복 셰프가게는 안 그럴것 같아 가보고 싶은데",
          "speaker_id": "1",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100038879.1.7",
          "form": "아직 못가봤네요",
          "original_form": "아직 못가봤네요",
          "speaker_id": "1",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100038879.1.8",
          "form": "은근히 다들 만들긴 하지만 잘만드는곳은 찾기 힘든거 같아요",
          "original_form": "은근히 다들 만들긴 하지만 잘만드는곳은 찾기 힘든거 같아요",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100038879.1.9",
          "form": "맞아요ㅜㅜㅜ",
          "original_form": "맞아요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100038879.1.10",
          "form": "근데 또 찍어만 먹으면 튀김옷이 너무 강렬해서 아쉬울 때가 있더라구요ㅎ",
          "original_form": "근데 또 찍어만 먹으면 튀김옷이 너무 강렬해서 아쉬울 때가 있더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100038879.1.11",
          "form": "그래서 살짝 담가 먹기ㅅㅇ",
          "original_form": "그래서 살짝 담가 먹기ㅅㅇ",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100038879.1.12",
          "form": "혼자 먹을 때만 할수있는 사도죠ㅎ",
          "original_form": "혼자 먹을 때만 할수있는 사도죠ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100038879.1.13",
          "form": "방식이 샤브샤브 먹는것 같아요ㅎ",
          "original_form": "방식이 샤브샤브 먹는것 같아요ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100038879.1.14",
          "form": "ㅋㅋㅋ그러게요 샤브샤브먹는거 같아요ㅋㅋ",
          "original_form": "ㅋㅋㅋ그러게요 샤브샤브먹는거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100038879.1.15",
          "form": "아 탕수육도 집에서 에어프라이어기로 만들수 있으려나요?",
          "original_form": "아 탕수육도 집에서 에어프라이어기로 만들수 있으려나요?",
          "speaker_id": "2",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100038879.1.16",
          "form": "탕수육 레토르트 사서 먹은 분들이",
          "original_form": "탕수육 레토르트 사서 먹은 분들이",
          "speaker_id": "1",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100038879.1.17",
          "form": "대만족이라던데요!",
          "original_form": "대만족이라던데요!",
          "speaker_id": "1",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100038879.1.18",
          "form": "에어프라이기에 하니 파는것처럼 된다구",
          "original_form": "에어프라이기에 하니 파는것처럼 된다구",
          "speaker_id": "1",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100038879.1.19",
          "form": "오 정말요??",
          "original_form": "오 정말요??",
          "speaker_id": "2",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100038879.1.20",
          "form": "저는 집에서 에어프라이어기에 만두넣으니까",
          "original_form": "저는 집에서 에어프라이어기에 만두넣으니까",
          "speaker_id": "2",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100038879.1.21",
          "form": "데워지긴 하던데 노릇노릇해지진 않더라고요ㅠ",
          "original_form": "데워지긴 하던데 노릇노릇해지진 않더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100038879.1.22",
          "form": "아 안 노릇해져요;;?",
          "original_form": "아 안 노릇해져요;;?",
          "speaker_id": "1",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100038879.1.23",
          "form": "김말이같은 건 노릇노릇 되던데",
          "original_form": "김말이같은 건 노릇노릇 되던데",
          "speaker_id": "1",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100038879.1.24",
          "form": "저는 표면이 갈색이 되는걸 바랬는데",
          "original_form": "저는 표면이 갈색이 되는걸 바랬는데",
          "speaker_id": "2",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038879.1.25",
          "form": "온도나 시간조절 하면 되지 않을까요?",
          "original_form": "온도나 시간조절 하면 되지 않을까요?",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038879.1.26",
          "form": "계속 하얀 밀가루 색에 표면에 습기가 좀 빠지는 느낌이더라고요ㅠ",
          "original_form": "계속 하얀 밀가루 색에 표면에 습기가 좀 빠지는 느낌이더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038879.1.27",
          "form": "아ㅜㅜ;;",
          "original_form": "아ㅜㅜ;;",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038879.1.28",
          "form": "근데 고기는 확실히 에어프라이어기가 낫더라고요",
          "original_form": "근데 고기는 확실히 에어프라이어기가 낫더라고요",
          "speaker_id": "2",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100038879.1.29",
          "form": "에프로 뭐뭐  해보셨나요",
          "original_form": "에프로 뭐뭐  해보셨나요",
          "speaker_id": "1",
          "time": "20210902 22:45"
        }
      ]
