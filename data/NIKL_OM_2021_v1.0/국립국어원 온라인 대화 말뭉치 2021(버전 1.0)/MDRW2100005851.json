      "utterance": [
        {
          "id": "MDRW2100005851.1.1",
          "form": "텀블러 애용하시죠???",
          "original_form": "텀블러 애용하시죠???",
          "speaker_id": "1",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100005851.1.2",
          "form": "그럼요 지금도 텀블어에 마시고있음",
          "original_form": "그럼요 지금도 텀블어에 마시고있음",
          "speaker_id": "2",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100005851.1.3",
          "form": "나도 텀블러에 커피 마시는 중",
          "original_form": "나도 텀블러에 커피 마시는 중",
          "speaker_id": "1",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100005851.1.4",
          "form": "이직하면서 하나 샀어요",
          "original_form": "이직하면서 하나 샀어요",
          "speaker_id": "1",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100005851.1.5",
          "form": "얼음 오래가서 좋아요",
          "original_form": "얼음 오래가서 좋아요",
          "speaker_id": "1",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100005851.1.6",
          "form": "카페갈때 텀블러 들고가?",
          "original_form": "카페갈때 텀블러 들고가?",
          "speaker_id": "2",
          "time": "20210907 14:16"
        },
        {
          "id": "MDRW2100005851.1.7",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210907 14:16"
        },
        {
          "id": "MDRW2100005851.1.8",
          "form": "귀찮아서 못해먹겠어요",
          "original_form": "귀찮아서 못해먹겠어요",
          "speaker_id": "1",
          "time": "20210907 14:16"
        },
        {
          "id": "MDRW2100005851.1.9",
          "form": "대신 자리에 앉았다오면 매장용컵에 달라고 합니다",
          "original_form": "대신 자리에 앉았다오면 매장용컵에 달라고 합니다",
          "speaker_id": "1",
          "time": "20210907 14:17"
        },
        {
          "id": "MDRW2100005851.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ나는 스벅 가기로 약속했다! 그러면 챙겨가는듯",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ나는 스벅 가기로 약속했다! 그러면 챙겨가는듯",
          "speaker_id": "2",
          "time": "20210907 14:17"
        },
        {
          "id": "MDRW2100005851.1.11",
          "form": "근데 이게 환경생각해서라기보단 한푼이라도 아끼려고가 더 큰거같음",
          "original_form": "근데 이게 환경생각해서라기보단 한푼이라도 아끼려고가 더 큰거같음",
          "speaker_id": "2",
          "time": "20210907 14:18"
        },
        {
          "id": "MDRW2100005851.1.12",
          "form": "저희 팀에 에코걸에 있어요",
          "original_form": "저희 팀에 에코걸에 있어요",
          "speaker_id": "1",
          "time": "20210907 14:18"
        },
        {
          "id": "MDRW2100005851.1.13",
          "form": "카페 가자고 하면 대문짝만한 머그 들고가요",
          "original_form": "카페 가자고 하면 대문짝만한 머그 들고가요",
          "speaker_id": "1",
          "time": "20210907 14:18"
        },
        {
          "id": "MDRW2100005851.1.14",
          "form": "결국 윈윈같지",
          "original_form": "결국 윈윈같지",
          "speaker_id": "2",
          "time": "20210907 14:18"
        },
        {
          "id": "MDRW2100005851.1.15",
          "form": "와....",
          "original_form": "와....",
          "speaker_id": "2",
          "time": "20210907 14:18"
        },
        {
          "id": "MDRW2100005851.1.16",
          "form": "그러라고 할인해주죠",
          "original_form": "그러라고 할인해주죠",
          "speaker_id": "1",
          "time": "20210907 14:18"
        },
        {
          "id": "MDRW2100005851.1.17",
          "form": "그분은 진짜 에코걸이네",
          "original_form": "그분은 진짜 에코걸이네",
          "speaker_id": "2",
          "time": "20210907 14:18"
        },
        {
          "id": "MDRW2100005851.1.18",
          "form": "이디야커피는 300원 할인해줍니댜",
          "original_form": "이디야커피는 300원 할인해줍니댜",
          "speaker_id": "1",
          "time": "20210907 14:18"
        },
        {
          "id": "MDRW2100005851.1.19",
          "form": "꿀팁 감사합니다",
          "original_form": "꿀팁 감사합니다",
          "speaker_id": "2",
          "time": "20210907 14:18"
        },
        {
          "id": "MDRW2100005851.1.20",
          "form": "근데 요샌 테이크아웃을 많이하니까",
          "original_form": "근데 요샌 테이크아웃을 많이하니까",
          "speaker_id": "2",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100005851.1.21",
          "form": "그런 테이크아웃 전문점도 좀 할인해줬으면 좋겠음",
          "original_form": "그런 테이크아웃 전문점도 좀 할인해줬으면 좋겠음",
          "speaker_id": "2",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100005851.1.22",
          "form": "코로나라소 일회용 컵만 주는 매장도 있어서 아쉬유ㅠ용",
          "original_form": "코로나라소 일회용 컵만 주는 매장도 있어서 아쉬유ㅠ용",
          "speaker_id": "1",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100005851.1.23",
          "form": "일회용컵은 집에서 생활용품 담을 때 쓰기도 함",
          "original_form": "일회용컵은 집에서 생활용품 담을 때 쓰기도 함",
          "speaker_id": "1",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100005851.1.24",
          "form": "스타벅스는 다회용 컵 사용하면 별 추가 적립이던가???",
          "original_form": "스타벅스는 다회용 컵 사용하면 별 추가 적립이던가???",
          "speaker_id": "1",
          "time": "20210907 14:22"
        },
        {
          "id": "MDRW2100005851.1.25",
          "form": "요즘은 컵 씻으라고 셀프바에 세척할 수 있는 장치 설치하더라고",
          "original_form": "요즘은 컵 씻으라고 셀프바에 세척할 수 있는 장치 설치하더라고",
          "speaker_id": "1",
          "time": "20210907 14:22"
        },
        {
          "id": "MDRW2100005851.1.26",
          "form": "나 할때 300원할인이었는데 바꼈나?",
          "original_form": "나 할때 300원할인이었는데 바꼈나?",
          "speaker_id": "2",
          "time": "20210907 14:22"
        },
        {
          "id": "MDRW2100005851.1.27",
          "form": "헐 좋다 그런거좋네",
          "original_form": "헐 좋다 그런거좋네",
          "speaker_id": "2",
          "time": "20210907 14:23"
        },
        {
          "id": "MDRW2100005851.1.28",
          "form": "맨날 컵한번 물로 헹궈주세요 하면서 나 진상아닌가 검열 했는데ㅠ",
          "original_form": "맨날 컵한번 물로 헹궈주세요 하면서 나 진상아닌가 검열 했는데ㅠ",
          "speaker_id": "2",
          "time": "20210907 14:23"
        },
        {
          "id": "MDRW2100005851.1.29",
          "form": "카페에서 텀블러 많이 제작하는데",
          "original_form": "카페에서 텀블러 많이 제작하는데",
          "speaker_id": "1",
          "time": "20210907 14:24"
        },
        {
          "id": "MDRW2100005851.1.30",
          "form": "시즌마다 나오니까 오히려 환경파괴 같기도 함",
          "original_form": "시즌마다 나오니까 오히려 환경파괴 같기도 함",
          "speaker_id": "1",
          "time": "20210907 14:24"
        },
        {
          "id": "MDRW2100005851.1.31",
          "form": "맞어 그래서 안사게됨",
          "original_form": "맞어 그래서 안사게됨",
          "speaker_id": "2",
          "time": "20210907 14:24"
        },
        {
          "id": "MDRW2100005851.1.32",
          "form": "좋은 정책일지 다시 한 번 고려해야할듯",
          "original_form": "좋은 정책일지 다시 한 번 고려해야할듯",
          "speaker_id": "1",
          "time": "20210907 14:24"
        },
        {
          "id": "MDRW2100005851.1.33",
          "form": "그리고 아 나 생각난거있음",
          "original_form": "그리고 아 나 생각난거있음",
          "speaker_id": "2",
          "time": "20210907 14:24"
        },
        {
          "id": "MDRW2100005851.1.34",
          "form": "커피용량을 톨 라지 그란데 이런거말고",
          "original_form": "커피용량을 톨 라지 그란데 이런거말고",
          "speaker_id": "2",
          "time": "20210907 14:25"
        },
        {
          "id": "MDRW2100005851.1.35",
          "form": "몇리터인지 표기해야한다고 생각해..",
          "original_form": "몇리터인지 표기해야한다고 생각해..",
          "speaker_id": "2",
          "time": "20210907 14:25"
        },
        {
          "id": "MDRW2100005851.1.36",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "1",
          "time": "20210907 14:25"
        },
        {
          "id": "MDRW2100005851.1.37",
          "form": "온즈 단위도 헷갈림",
          "original_form": "온즈 단위도 헷갈림",
          "speaker_id": "1",
          "time": "20210907 14:25"
        },
        {
          "id": "MDRW2100005851.1.38",
          "form": "가끔 너무 작은 텀블러 가져오시는 고객님들른 좀 곤란하더라고",
          "original_form": "가끔 너무 작은 텀블러 가져오시는 고객님들른 좀 곤란하더라고",
          "speaker_id": "1",
          "time": "20210907 14:25"
        },
        {
          "id": "MDRW2100005851.1.39",
          "form": "그런 봄봄같은 카페도 텀블러 챙기고 싶은데",
          "original_form": "그런 봄봄같은 카페도 텀블러 챙기고 싶은데",
          "speaker_id": "2",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100005851.1.40",
          "form": "몇리터인지 몰라서 못가져감...",
          "original_form": "몇리터인지 몰라서 못가져감...",
          "speaker_id": "2",
          "time": "20210907 14:26"
        }
      ]
    }
