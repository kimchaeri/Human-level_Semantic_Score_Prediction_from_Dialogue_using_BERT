      "utterance": [
        {
          "id": "MDRW2100008024.1.1",
          "form": "학교 사람 만나요?",
          "original_form": "학교 사람 만나요?",
          "speaker_id": "2",
          "time": "20210823 08:39"
        },
        {
          "id": "MDRW2100008024.1.2",
          "form": "아 저는 전공이 다 비슷한 계열로 취업해서 자주 만나요 학교시절 언제가 제일 재밌었나요?",
          "original_form": "아 저는 전공이 다 비슷한 계열로 취업해서 자주 만나요 학교시절 언제가 제일 재밌었나요?",
          "speaker_id": "1",
          "time": "20210823 08:41"
        },
        {
          "id": "MDRW2100008024.1.3",
          "form": "초등학교요",
          "original_form": "초등학교요",
          "speaker_id": "2",
          "time": "20210823 08:41"
        },
        {
          "id": "MDRW2100008024.1.4",
          "form": "뭐 하고 노셨어요?",
          "original_form": "뭐 하고 노셨어요?",
          "speaker_id": "1",
          "time": "20210823 08:42"
        },
        {
          "id": "MDRW2100008024.1.5",
          "form": "그냥 막 뛰어다녓죠",
          "original_form": "그냥 막 뛰어다녓죠",
          "speaker_id": "2",
          "time": "20210823 08:44"
        },
        {
          "id": "MDRW2100008024.1.6",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 08:44"
        },
        {
          "id": "MDRW2100008024.1.7",
          "form": "ㅋㅋ 동네 친구 많았나보네요",
          "original_form": "ㅋㅋ 동네 친구 많았나보네요",
          "speaker_id": "1",
          "time": "20210823 08:50"
        },
        {
          "id": "MDRW2100008024.1.8",
          "form": "네 또래거 많아서",
          "original_form": "네 또래거 많아서",
          "speaker_id": "2",
          "time": "20210823 08:50"
        },
        {
          "id": "MDRW2100008024.1.9",
          "form": "재밌었겠어요 학교는 가까웠나요?",
          "original_form": "재밌었겠어요 학교는 가까웠나요?",
          "speaker_id": "1",
          "time": "20210823 08:52"
        },
        {
          "id": "MDRW2100008024.1.10",
          "form": "네 근처라 좋았어요",
          "original_form": "네 근처라 좋았어요",
          "speaker_id": "2",
          "time": "20210823 08:53"
        },
        {
          "id": "MDRW2100008024.1.11",
          "form": "공부스트레스같은건 없었나요?",
          "original_form": "공부스트레스같은건 없었나요?",
          "speaker_id": "1",
          "time": "20210823 08:57"
        },
        {
          "id": "MDRW2100008024.1.12",
          "form": "그당시는 아무생각없이그냥",
          "original_form": "그당시는 아무생각없이그냥",
          "speaker_id": "2",
          "time": "20210823 09:38"
        },
        {
          "id": "MDRW2100008024.1.13",
          "form": "그러게요 무슨과목 좋아하셨어요?",
          "original_form": "그러게요 무슨과목 좋아하셨어요?",
          "speaker_id": "1",
          "time": "20210823 11:10"
        },
