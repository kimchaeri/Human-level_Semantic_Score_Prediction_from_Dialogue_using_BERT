      "utterance": [
        {
          "id": "MDRW2100010557.1.1",
          "form": "안녕하세요! 반려동물 키우시나용?",
          "original_form": "안녕하세요! 반려동물 키우시나용?",
          "speaker_id": "1",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100010557.1.2",
          "form": "저는 좋아하는데 키울사정은 안돼서 안키우고있어요ㅎㅎ",
          "original_form": "저는 좋아하는데 키울사정은 안돼서 안키우고있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100010557.1.3",
          "form": "토끼를 키우고있어요",
          "original_form": "토끼를 키우고있어요",
          "speaker_id": "2",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100010557.1.4",
          "form": "지금도 막 저녁거리로 간식 챙겨줬어요",
          "original_form": "지금도 막 저녁거리로 간식 챙겨줬어요",
          "speaker_id": "2",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010557.1.5",
          "form": "뭐 키우고 싶으세요?",
          "original_form": "뭐 키우고 싶으세요?",
          "speaker_id": "2",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010557.1.6",
          "form": "옹 토끼 키우시는구나!!ㅎㅎ 귀여울것같아요",
          "original_form": "옹 토끼 키우시는구나!!ㅎㅎ 귀여울것같아요",
          "speaker_id": "1",
          "time": "20210823 19:38"
        },
        {
          "id": "MDRW2100010557.1.7",
          "form": "저는 고양이나 강아지요ㅎㅎ",
          "original_form": "저는 고양이나 강아지요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 19:38"
        },
        {
          "id": "MDRW2100010557.1.8",
          "form": "토끼 이름 물어봐도될까요?",
          "original_form": "토끼 이름 물어봐도될까요?",
          "speaker_id": "1",
          "time": "20210823 19:38"
        },
        {
          "id": "MDRW2100010557.1.9",
          "form": "name3 이에요 외자로 ㅎㅎ",
          "original_form": "&name3& 이에요 외자로 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:39"
        },
        {
          "id": "MDRW2100010557.1.10",
          "form": "키우고싶은 드림묘나 견종은 있으신가요ㅡ",
          "original_form": "키우고싶은 드림묘나 견종은 있으신가요ㅡ",
          "speaker_id": "2",
          "time": "20210823 19:40"
        },
        {
          "id": "MDRW2100010557.1.11",
          "form": "이름도 너무 예쁘네요ㅎㅎㅎ",
          "original_form": "이름도 너무 예쁘네요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010557.1.12",
          "form": "사실 키우고싶은 견종은 사모예드인데",
          "original_form": "사실 키우고싶은 견종은 사모예드인데",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010557.1.13",
          "form": "대형견 너무 좋죠 ㅠㅠ",
          "original_form": "대형견 너무 좋죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010557.1.14",
          "form": "그쵸ㅎㅎㅎ",
          "original_form": "그쵸ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010557.1.15",
          "form": "뭔가 포옥 안아보고싶은 그런 느낌",
          "original_form": "뭔가 포옥 안아보고싶은 그런 느낌",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010557.1.16",
          "form": "ㅋㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010557.1.17",
          "form": "토끼도 산책하나요?",
          "original_form": "토끼도 산책하나요?",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010557.1.18",
          "form": "아 토끼는 집에서만 산책시켜요",
          "original_form": "아 토끼는 집에서만 산책시켜요",
          "speaker_id": "2",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010557.1.19",
          "form": "초식동물이다보니 모르는 환경에가면",
          "original_form": "초식동물이다보니 모르는 환경에가면",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010557.1.20",
          "form": "무서워해서 ㅎㅎㅎ;;,",
          "original_form": "무서워해서 ㅎㅎㅎ;;,",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010557.1.21",
          "form": "자면 케이지 안에 들어가고 평소에는 집 안에서 돌아다녀요",
          "original_form": "자면 케이지 안에 들어가고 평소에는 집 안에서 돌아다녀요",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010557.1.22",
          "form": "지금도 옆에서 돌아다니는데...",
          "original_form": "지금도 옆에서 돌아다니는데...",
          "speaker_id": "2",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010557.1.23",
          "form": "잠깐 눈 떼면 전선을 갉고 있네요ㅋㅋㅠㅠㅠ",
          "original_form": "잠깐 눈 떼면 전선을 갉고 있네요ㅋㅋㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010557.1.24",
          "form": "오 처음알게 된 사실이네요!! 집에서 산책시키는거!",
          "original_form": "오 처음알게 된 사실이네요!! 집에서 산책시키는거!",
          "speaker_id": "1",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010557.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ전선을 갉고있다니",
          "original_form": "ㅋㅋㅋㅋㅋㅋ전선을 갉고있다니",
          "speaker_id": "1",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010557.1.26",
          "form": "아 예전에 인터넷에서 봣는데",
          "original_form": "아 예전에 인터넷에서 봣는데",
          "speaker_id": "1",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010557.1.27",
          "form": "하네스 있는 분들은",
          "original_form": "하네스 있는 분들은",
          "speaker_id": "2",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010557.1.28",
          "form": "토끼가 앞머리 갉아먹었다고ㅠㅜㅜ",
          "original_form": "토끼가 앞머리 갉아먹었다고ㅠㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010557.1.29",
          "form": "울타리 있는 폐쇄된 풀밭에서 산책 시킨다 하더라구요",
          "original_form": "울타리 있는 폐쇄된 풀밭에서 산책 시킨다 하더라구요",
          "speaker_id": "2",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010557.1.30",
          "form": "넼ㅋㅋㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010557.1.31",
          "form": "옹 하네스 있으면 그럴수있군용",
          "original_form": "옹 하네스 있으면 그럴수있군용",
          "speaker_id": "1",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010557.1.32",
          "form": "ㅋㅋㄱㄱㅋㅋ",
          "original_form": "ㅋㅋㄱㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010557.1.33",
          "form": "그냥 머리도 자주 먹어요...ㅋㅋ",
          "original_form": "그냥 머리도 자주 먹어요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010557.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋ그래도 귀여워서 용서되지않나요,,",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그래도 귀여워서 용서되지않나요,,",
          "speaker_id": "1",
          "time": "20210823 19:47"
        },
        {
          "id": "MDRW2100010557.1.35",
