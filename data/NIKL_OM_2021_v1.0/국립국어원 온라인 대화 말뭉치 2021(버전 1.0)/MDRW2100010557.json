        {
          "id": "MDRW2100010557.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ진짜 그럴수밖에 없을것같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ진짜 그럴수밖에 없을것같아요",
          "speaker_id": "1",
