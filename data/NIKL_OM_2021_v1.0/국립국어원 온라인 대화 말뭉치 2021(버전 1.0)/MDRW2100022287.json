      "utterance": [
        {
          "id": "MDRW2100022287.1.1",
          "form": "점심 아직 안드셨죠?",
          "original_form": "점심 아직 안드셨죠?",
          "speaker_id": "2",
          "time": "20210829 12:07"
        },
        {
          "id": "MDRW2100022287.1.2",
          "form": "네 아직요",
          "original_form": "네 아직요",
          "speaker_id": "1",
          "time": "20210829 12:07"
        },
        {
          "id": "MDRW2100022287.1.3",
          "form": "언제 드실거에요",
          "original_form": "언제 드실거에요",
          "speaker_id": "2",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022287.1.4",
          "form": "음 1시쯤에?",
          "original_form": "음 1시쯤에?",
          "speaker_id": "1",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022287.1.5",
          "form": "그럼 저 님하고 같이 먹을래요",
          "original_form": "그럼 저 님하고 같이 먹을래요",
          "speaker_id": "2",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022287.1.6",
          "form": "제가 참을게요",
          "original_form": "제가 참을게요",
          "speaker_id": "2",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022287.1.7",
          "form": "어제먹은게 소화가 안되서요",
          "original_form": "어제먹은게 소화가 안되서요",
          "speaker_id": "1",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022287.1.8",
          "form": "ㅠㅠ 님이 좋아요",
          "original_form": "ㅠㅠ 님이 좋아요",
          "speaker_id": "2",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022287.1.9",
          "form": "헐 안됩니다 뽈찜 드신다면서..",
          "original_form": "헐 안됩니다 뽈찜 드신다면서..",
          "speaker_id": "1",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022287.1.10",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "1",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022287.1.11",
          "form": "참치뽈찜은....",
          "original_form": "참치뽈찜은....",
          "speaker_id": "2",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022287.1.12",
          "form": "더 기다려줘야죠",
          "original_form": "더 기다려줘야죠",
          "speaker_id": "2",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022287.1.13",
          "form": "ㅋㅋ 아 기다림이 기시겠어요",
          "original_form": "ㅋㅋ 아 기다림이 기시겠어요",
          "speaker_id": "1",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022287.1.14",
          "form": "숙성시킨다 생각하고 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "숙성시킨다 생각하고 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022287.1.15",
          "form": "기다리몈돼요",
          "original_form": "기다리몈돼요",
          "speaker_id": "2",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022287.1.16",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022287.1.17",
          "form": "그럼 뭐 좋아하세요",
          "original_form": "그럼 뭐 좋아하세요",
          "speaker_id": "1",
          "time": "20210829 12:09"
        },
        {
          "id": "MDRW2100022287.1.18",
          "form": "음식 중에서요?",
          "original_form": "음식 중에서요?",
          "speaker_id": "2",
          "time": "20210829 12:09"
        },
