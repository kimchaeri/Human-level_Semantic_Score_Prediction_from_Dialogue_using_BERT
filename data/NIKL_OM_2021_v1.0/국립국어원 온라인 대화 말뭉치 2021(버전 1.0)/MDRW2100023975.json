      "utterance": [
        {
          "id": "MDRW2100023975.1.1",
          "form": "오늘 점심 때 뭐 드셨었어요?",
          "original_form": "오늘 점심 때 뭐 드셨었어요?",
          "speaker_id": "1",
          "time": "20210830 20:49"
        },
        {
          "id": "MDRW2100023975.1.2",
          "form": "오늘은~",
          "original_form": "오늘은~",
          "speaker_id": "2",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023975.1.3",
          "form": "된장찌개에다가 밥 말아먹었어요~",
          "original_form": "된장찌개에다가 밥 말아먹었어요~",
          "speaker_id": "2",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023975.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023975.1.5",
          "form": "저는 집에서 참치회덮밥 만들어 먹었네요 ㅎㅎ",
          "original_form": "저는 집에서 참치회덮밥 만들어 먹었네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023975.1.6",
          "form": "된장찌개는 직접 끓이신거에요?",
          "original_form": "된장찌개는 직접 끓이신거에요?",
          "speaker_id": "1",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023975.1.7",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100023975.1.8",
          "form": "부모님이랑 살고 있어서",
          "original_form": "부모님이랑 살고 있어서",
          "speaker_id": "2",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100023975.1.9",
          "form": "부모님이 끓여주셨어요!",
          "original_form": "부모님이 끓여주셨어요!",
          "speaker_id": "2",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100023975.1.10",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100023975.1.11",
          "form": "참치회덮밥",
          "original_form": "참치회덮밥",
          "speaker_id": "2",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100023975.1.12",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100023975.1.13",
          "form": "참치 좋아하세요?",
          "original_form": "참치 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 20:59"
        },
        {
          "id": "MDRW2100023975.1.14",
          "form": "네네 ㅎㅎ 근데 참치회덮밥에 넣는 참치는",
          "original_form": "네네 ㅎㅎ 근데 참치회덮밥에 넣는 참치는",
          "speaker_id": "1",
          "time": "20210830 21:00"
        },
        {
          "id": "MDRW2100023975.1.15",
          "form": "고급어종이 아니라서 맛은 좀 덜해요 ㅜㅜ",
          "original_form": "고급어종이 아니라서 맛은 좀 덜해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 21:00"
        },
        {
          "id": "MDRW2100023975.1.16",
          "form": "그렇구나ㅏ",
          "original_form": "그렇구나ㅏ",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023975.1.17",
          "form": "참치회덮밥에",
          "original_form": "참치회덮밥에",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023975.1.18",
          "form": "채소는 뭐뭐 넣으세요??",
          "original_form": "채소는 뭐뭐 넣으세요??",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023975.1.19",
          "form": "양배추랑 상추 깻잎 당근 등등 넣네요",
          "original_form": "양배추랑 상추 깻잎 당근 등등 넣네요",
          "speaker_id": "1",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100023975.1.20",
          "form": "전엔 치커리도 넣어봤는데 이건 맛이 너무 강해서 안어울리더라구요",
          "original_form": "전엔 치커리도 넣어봤는데 이건 맛이 너무 강해서 안어울리더라구요",
          "speaker_id": "1",
          "time": "20210830 21:12"
        },
        {
          "id": "MDRW2100023975.1.21",
          "form": "오 치커리...",
          "original_form": "오 치커리...",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023975.1.22",
          "form": "다음에 만들어보려구",
          "original_form": "다음에 만들어보려구",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023975.1.23",
          "form": "물어봤어요~ ㅎㅎ",
          "original_form": "물어봤어요~ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023975.1.24",
          "form": "어머~ 치커리는 넣지마세요 ㅠㅠ",
          "original_form": "어머~ 치커리는 넣지마세요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100023975.1.25",
          "form": "그리고 채소 넣으실 땐 최대한 얇게 채썰기 해요",
          "original_form": "그리고 채소 넣으실 땐 최대한 얇게 채썰기 해요",
          "speaker_id": "1",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100023975.1.26",
          "form": "아하ㅏㅏ",
          "original_form": "아하ㅏㅏ",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100023975.1.27",
          "form": "ㅋㅋㅋ 알려주셔서 감사해요~",
          "original_form": "ㅋㅋㅋ 알려주셔서 감사해요~",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100023975.1.28",
          "form": "다음에 꼭 해먹어 볼게요!!",
          "original_form": "다음에 꼭 해먹어 볼게요!!",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100023975.1.29",
          "form": "네~ 참고로 참치는 인터넷에 큐브 참치 이런거 검색하면",
          "original_form": "네~ 참고로 참치는 인터넷에 큐브 참치 이런거 검색하면",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100023975.1.30",
          "form": "주사위모양으로 조각낸거 팔아요~",
          "original_form": "주사위모양으로 조각낸거 팔아요~",
          "speaker_id": "1",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100023975.1.31",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100023975.1.32",
          "form": "그런것도 있구나",
          "original_form": "그런것도 있구나",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100023975.1.33",
          "form": "큐브 참치!",
          "original_form": "큐브 참치!",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100023975.1.34",
          "form": "좋아요~",
          "original_form": "좋아요~",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100023975.1.35",
          "form": "아니면 참치 말고 다른거 넣어도 돼요",
          "original_form": "아니면 참치 말고 다른거 넣어도 돼요",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100023975.1.36",
          "form": "연어나 불고기나 대패삼겹 같은거요",
          "original_form": "연어나 불고기나 대패삼겹 같은거요",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100023975.1.37",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100023975.1.38",
          "form": "저는 연어를 제일 좋아해요!",
          "original_form": "저는 연어를 제일 좋아해요!",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100023975.1.39",
          "form": "연어 넣어야겠다",
          "original_form": "연어 넣어야겠다",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100023975.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋ ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100023975.1.41",
          "form": "연어ㅋㅋ 거기에다가 그럼 아보카도랑 삶은계란도 같이 넣어봐요",
          "original_form": "연어ㅋㅋ 거기에다가 그럼 아보카도랑 삶은계란도 같이 넣어봐요",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100023975.1.42",
          "form": "그럼 더욱 맛있을 거 같아요",
          "original_form": "그럼 더욱 맛있을 거 같아요",
          "speaker_id": "1",
          "time": "20210830 23:41"
        }
      ]
