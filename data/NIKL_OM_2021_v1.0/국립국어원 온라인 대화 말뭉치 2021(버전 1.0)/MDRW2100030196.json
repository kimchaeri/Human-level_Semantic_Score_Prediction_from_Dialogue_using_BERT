      "utterance": [
        {
          "id": "MDRW2100030196.1.1",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030196.1.2",
          "form": "비타민d도 꼬박꼬박",
          "original_form": "비타민d도 꼬박꼬박",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030196.1.3",
          "form": "챙겨먹어",
          "original_form": "챙겨먹어",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030196.1.4",
          "form": "홍삼도 많이 먹어야돼고",
          "original_form": "홍삼도 많이 먹어야돼고",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030196.1.5",
          "form": "비타민d도 먹어야되고",
          "original_form": "비타민d도 먹어야되고",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030196.1.6",
          "form": "요즘에는 세상이 좋아진거같아",
          "original_form": "요즘에는 세상이 좋아진거같아",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030196.1.7",
          "form": "비타민d는 어디에 좋은지 알어?",
          "original_form": "비타민d는 어디에 좋은지 알어?",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030196.1.8",
          "form": "햇빛을 안쬐도 건강할수있잖아",
          "original_form": "햇빛을 안쬐도 건강할수있잖아",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.9",
          "form": "그건아닌가?",
          "original_form": "그건아닌가?",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.10",
          "form": "내가알겠너?",
          "original_form": "내가알겠너?",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.11",
          "form": "알려줘",
          "original_form": "알려줘",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.12",
          "form": "햇빛은 비타민 c 아닐까?",
          "original_form": "햇빛은 비타민 c 아닐까?",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.13",
          "form": "그런건 중요하지안아",
          "original_form": "그런건 중요하지안아",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.14",
          "form": "비타민D는 뼈랑 관련잇어",
          "original_form": "비타민D는 뼈랑 관련잇어",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.15",
          "form": "아무튼 알려줘",
          "original_form": "아무튼 알려줘",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.16",
          "form": "그렇구나!!",
          "original_form": "그렇구나!!",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.17",
          "form": "암도 예방해준대",
          "original_form": "암도 예방해준대",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.18",
          "form": "운동하는거보다",
          "original_form": "운동하는거보다",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.19",
          "form": "비타민d먹는데",
          "original_form": "비타민d먹는데",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.20",
          "form": "먹는게",
          "original_form": "먹는게",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.21",
          "form": "좋을거같아",
          "original_form": "좋을거같아",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.22",
          "form": "그정돈 아냐 ㅎㅎ",
          "original_form": "그정돈 아냐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.23",
          "form": "암도 예방해준다니 ㄷㄷ",
          "original_form": "암도 예방해준다니 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.24",
          "form": "비타민디도 많이먹으면 오히려",
          "original_form": "비타민디도 많이먹으면 오히려",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.25",
          "form": "그럴수있지 ㅎㅎ",
          "original_form": "그럴수있지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.26",
          "form": "몸에 나쁠 수 잇어.",
          "original_form": "몸에 나쁠 수 잇어.",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030196.1.27",
          "form": "요즘에 건강 관리 하려고",
          "original_form": "요즘에 건강 관리 하려고",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030196.1.28",
          "form": "물도 많이 먹고 돼지감자차도 마시고있어",
          "original_form": "물도 많이 먹고 돼지감자차도 마시고있어",
