      "utterance": [
        {
          "id": "MDRW2100013077.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100013077.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 12:31"
        },
        {
          "id": "MDRW2100013077.1.3",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "1",
          "time": "20210825 12:31"
        },
        {
          "id": "MDRW2100013077.1.4",
          "form": "전혀요ㅋㅋㅋ",
          "original_form": "전혀요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:38"
        },
        {
          "id": "MDRW2100013077.1.5",
          "form": "학생인줄 알았네요",
          "original_form": "학생인줄 알았네요",
          "speaker_id": "1",
          "time": "20210825 12:39"
        },
        {
          "id": "MDRW2100013077.1.6",
          "form": "저는 졸업한지 8년 됐습니다",
          "original_form": "저는 졸업한지 8년 됐습니다",
          "speaker_id": "1",
          "time": "20210825 12:40"
        },
        {
          "id": "MDRW2100013077.1.7",
          "form": "대학교 졸업",
          "original_form": "대학교 졸업",
          "speaker_id": "1",
          "time": "20210825 12:40"
        },
        {
          "id": "MDRW2100013077.1.8",
          "form": "ㅋㅋㅋ저도 꽤오래됐습니다",
          "original_form": "ㅋㅋㅋ저도 꽤오래됐습니다",
          "speaker_id": "2",
          "time": "20210825 12:48"
        },
        {
          "id": "MDRW2100013077.1.9",
          "form": "한참이죠",
          "original_form": "한참이죠",
          "speaker_id": "2",
          "time": "20210825 12:48"
        },
        {
          "id": "MDRW2100013077.1.10",
          "form": "그때로 돌아가고싶어요",
          "original_form": "그때로 돌아가고싶어요",
          "speaker_id": "1",
          "time": "20210825 12:51"
        },
        {
          "id": "MDRW2100013077.1.11",
          "form": "대학시절이 너무 그리워요",
          "original_form": "대학시절이 너무 그리워요",
          "speaker_id": "1",
          "time": "20210825 12:51"
        },
        {
          "id": "MDRW2100013077.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 12:59"
        },
        {
          "id": "MDRW2100013077.1.13",
          "form": "지나고보니 아쉽네요",
          "original_form": "지나고보니 아쉽네요",
          "speaker_id": "2",
          "time": "20210825 12:59"
        },
        {
          "id": "MDRW2100013077.1.14",
          "form": "전공이 어떻게 되세요?",
          "original_form": "전공이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210825 13:07"
        },
        {
          "id": "MDRW2100013077.1.15",
          "form": "저 경영이요",
          "original_form": "저 경영이요",
          "speaker_id": "2",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013077.1.16",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013077.1.17",
          "form": "저는 전자전기에요",
          "original_form": "저는 전자전기에요",
          "speaker_id": "1",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013077.1.18",
          "form": "지금 생각해보면 컴공을 하면 좋았을것 같은데...",
          "original_form": "지금 생각해보면 컴공을 하면 좋았을것 같은데...",
          "speaker_id": "1",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013077.1.19",
          "form": "아쉬워요",
          "original_form": "아쉬워요",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100013077.1.20",
          "form": "전공 만족하세요?",
          "original_form": "전공 만족하세요?",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100013077.1.21",
          "form": "저는 그닥ㅠ",
          "original_form": "저는 그닥ㅠ",
          "speaker_id": "2",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100013077.1.22",
          "form": "글도 공대는",
          "original_form": "글도 공대는",
          "speaker_id": "2",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100013077.1.23",
          "form": "취업잘되지않나요",
          "original_form": "취업잘되지않나요",
          "speaker_id": "2",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100013077.1.24",
          "form": "취업은 잘되서 좋죠",
          "original_form": "취업은 잘되서 좋죠",
          "speaker_id": "1",
          "time": "20210825 13:28"
        }
      ]
    }
