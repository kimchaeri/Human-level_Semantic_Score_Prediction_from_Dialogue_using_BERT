      "utterance": [
        {
          "id": "MDRW2100026419.1.1",
          "form": "서울도 물가는 비싼편이지만 대학로가면 가성비 있게 먹을 수 있는거 같아요",
          "original_form": "서울도 물가는 비싼편이지만 대학로가면 가성비 있게 먹을 수 있는거 같아요",
          "speaker_id": "2",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026419.1.2",
          "form": "좋아하는 책 분야 있어요?",
          "original_form": "좋아하는 책 분야 있어요?",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026419.1.3",
          "form": "소설쪽 좋아해요",
          "original_form": "소설쪽 좋아해요",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026419.1.4",
          "form": "어떤쪽 좋아하세요?",
          "original_form": "어떤쪽 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026419.1.5",
          "form": "저도 소설이요!",
          "original_form": "저도 소설이요!",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026419.1.6",
          "form": "저는 판타지쪽 좋아해요",
          "original_form": "저는 판타지쪽 좋아해요",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026419.1.7",
          "form": "판타지쪽은 중학생때 자주 읽었는데",
          "original_form": "판타지쪽은 중학생때 자주 읽었는데",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026419.1.8",
          "form": "요즘은 어떤게 유명한가요?",
          "original_form": "요즘은 어떤게 유명한가요?",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026419.1.9",
          "form": "저도 예전옛날에 본게 다라...해리포터 아시나요?",
          "original_form": "저도 예전옛날에 본게 다라...해리포터 아시나요?",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026419.1.10",
          "form": "해리포터 알죠!",
          "original_form": "해리포터 알죠!",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026419.1.11",
          "form": "신간 나오는거 기다렸다가 서점가서 사오고했던 책이였어요",
          "original_form": "신간 나오는거 기다렸다가 서점가서 사오고했던 책이였어요",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026419.1.12",
          "form": "해리포터 너무 재미있어요!!",
          "original_form": "해리포터 너무 재미있어요!!",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026419.1.13",
          "form": "그때가 제일 판타지 책이 이것저것 많이 나온거 같아요",
          "original_form": "그때가 제일 판타지 책이 이것저것 많이 나온거 같아요",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026419.1.14",
          "form": "영화도 재밌었지만 책도 다른재미가 있어서 좋았어요",
          "original_form": "영화도 재밌었지만 책도 다른재미가 있어서 좋았어요",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026419.1.15",
          "form": "판타지가 전국적으로 유행이였죠",
          "original_form": "판타지가 전국적으로 유행이였죠",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026419.1.16",
          "form": "다들 몰려가서 책사구...",
          "original_form": "다들 몰려가서 책사구...",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026419.1.17",
          "form": "또 다른 판타지 뭐 보셨어요?",
          "original_form": "또 다른 판타지 뭐 보셨어요?",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026419.1.18",
          "form": "저는 그 제목이 기억안나는데",
          "original_form": "저는 그 제목이 기억안나는데",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026419.1.19",
          "form": "게임세계에서 일어나는것도 있었구요",
          "original_form": "게임세계에서 일어나는것도 있었구요",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026419.1.20",
          "form": "나니아연대기도 봤어요",
          "original_form": "나니아연대기도 봤어요",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026419.1.21",
          "form": "헐 저도요",
          "original_form": "헐 저도요",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026419.1.22",
          "form": "나니아 연대기는 진짜 몇번을 읽었는지",
          "original_form": "나니아 연대기는 진짜 몇번을 읽었는지",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026419.1.23",
          "form": "지금은 세세하게 기억이 안나지만",
          "original_form": "지금은 세세하게 기억이 안나지만",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026419.1.24",
          "form": "세계관이 좋았어요",
          "original_form": "세계관이 좋았어요",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026419.1.25",
          "form": "혹시 타라 덩컨이나 고양이 판타지 소설도 아세요?",
          "original_form": "혹시 타라 덩컨이나 고양이 판타지 소설도 아세요?",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026419.1.26",
          "form": "고양이 판타지 소설은 제목이 기억이 안나네요...그땐 참 재미있었는데",
          "original_form": "고양이 판타지 소설은 제목이 기억이 안나네요...그땐 참 재미있었는데",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026419.1.27",
          "form": "앗 그건 모르겠어요",
          "original_form": "앗 그건 모르겠어요",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026419.1.28",
          "form": "생각하다보니까 정말 그때가 판타지 붐이었나봐요",
          "original_form": "생각하다보니까 정말 그때가 판타지 붐이었나봐요",
          "speaker_id": "2",
          "time": "20210901 23:53"
        }
      ]
