        {
          "id": "MDRW2100018293.1.1",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018293.1.2",
          "form": "내일 비가또온대요?!",
          "original_form": "내일 비가또온대요?!",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018293.1.3",
          "form": "넵 전국적으로 많이 온다더라고요ㅜㅠ",
          "original_form": "넵 전국적으로 많이 온다더라고요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018293.1.4",
          "form": "또르르",
          "original_form": "또르르",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018293.1.5",
          "form": "사실 비는좋아하긴해요",
          "original_form": "사실 비는좋아하긴해요",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018293.1.6",
          "form": "틀렸음 좋겠지만... 김치전이나 라면 괜찮을듯싶고요",
          "original_form": "틀렸음 좋겠지만... 김치전이나 라면 괜찮을듯싶고요",
          "speaker_id": "2",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018293.1.7",
          "form": "파전",
          "original_form": "파전",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018293.1.8",
          "form": "저도 비오는거 좋아해요 라면 ㅎㅎ",
          "original_form": "저도 비오는거 좋아해요 라면 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018293.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018293.1.10",
          "form": "라면뭐좋아하세요?",
          "original_form": "라면뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018293.1.11",
          "form": "전 진매 좋아요",
          "original_form": "전 진매 좋아요",
          "speaker_id": "2",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018293.1.12",
          "form": "전",
          "original_form": "전",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018293.1.13",
          "form": "열라면파입니다",
          "original_form": "열라면파입니다",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018293.1.14",
          "form": "열라면 순두부 해드셔보셨어요???",
          "original_form": "열라면 순두부 해드셔보셨어요???",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018293.1.15",
          "form": "한번도요",
          "original_form": "한번도요",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018293.1.16",
          "form": "그거마시나여?",
          "original_form": "그거마시나여?",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018293.1.17",
          "form": "저도 말만 들었어요",
          "original_form": "저도 말만 들었어요",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018293.1.18",
          "form": "라면인데",
          "original_form": "라면인데",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018293.1.19",
          "form": "순두부를",
          "original_form": "순두부를",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018293.1.20",
          "form": "넣는거죠???",
          "original_form": "넣는거죠???",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018293.1.21",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018293.1.22",
          "form": "스프는 다넣고 순두부 반만?",
          "original_form": "스프는 다넣고 순두부 반만?",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018293.1.23",
          "form": "좋네요ㅋㅋㅋ",
          "original_form": "좋네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018293.1.24",
          "form": "맛집좋아하세요?",
