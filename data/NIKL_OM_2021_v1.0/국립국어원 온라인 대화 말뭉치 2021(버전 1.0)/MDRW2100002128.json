      "utterance": [
        {
          "id": "MDRW2100002128.1.1",
          "form": "동호회 하는거 있어?",
          "original_form": "동호회 하는거 있어?",
          "speaker_id": "1",
          "time": "20210819 23:49"
        },
        {
          "id": "MDRW2100002128.1.2",
          "form": "스터디도 동호회랑 비슷한가?",
          "original_form": "스터디도 동호회랑 비슷한가?",
          "speaker_id": "2",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002128.1.3",
          "form": "스터디도 공부동호회라고 할수있지않을까?",
          "original_form": "스터디도 공부동호회라고 할수있지않을까?",
          "speaker_id": "1",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002128.1.4",
          "form": "그럼 토익 동호회 하고있어ㅋㅋㅋ",
          "original_form": "그럼 토익 동호회 하고있어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002128.1.5",
          "form": "토익공부하는구나 열심히하네",
          "original_form": "토익공부하는구나 열심히하네",
          "speaker_id": "1",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002128.1.6",
          "form": "동호회 사람들도 진짜 다들 열심히라서",
          "original_form": "동호회 사람들도 진짜 다들 열심히라서",
          "speaker_id": "2",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002128.1.7",
          "form": "뭔가 더 모티베이션 되는 느낌이야",
          "original_form": "뭔가 더 모티베이션 되는 느낌이야",
          "speaker_id": "2",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002128.1.8",
          "form": "다행이네 멋지다",
          "original_form": "다행이네 멋지다",
          "speaker_id": "1",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100002128.1.9",
          "form": "몇명이나 모여서 해?",
          "original_form": "몇명이나 모여서 해?",
          "speaker_id": "1",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100002128.1.10",
          "form": "나 포함 다섯명.",
          "original_form": "나 포함 다섯명.",
          "speaker_id": "2",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002128.1.11",
          "form": "두명은 직장 있는데도 우리중에 제일 열심히 하더라",
          "original_form": "두명은 직장 있는데도 우리중에 제일 열심히 하더라",
          "speaker_id": "2",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002128.1.12",
          "form": "직장 이직하려고 하는건가? 힘들겠네 ㅠㅠ",
          "original_form": "직장 이직하려고 하는건가? 힘들겠네 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 00:10"
        },
        {
          "id": "MDRW2100002128.1.13",
          "form": "그런것 같아. 그런거 보면 나도 열심히 해야겠다는 생각이 들어",
          "original_form": "그런것 같아. 그런거 보면 나도 열심히 해야겠다는 생각이 들어",
          "speaker_id": "2",
          "time": "20210820 00:12"
        },
        {
          "id": "MDRW2100002128.1.14",
          "form": "그러게 열심히 사는게 쉽지않은데ㅠㅠ 대단해",
          "original_form": "그러게 열심히 사는게 쉽지않은데ㅠㅠ 대단해",
          "speaker_id": "1",
          "time": "20210820 00:16"
        },
        {
          "id": "MDRW2100002128.1.15",
          "form": "이런게 동호회의 순기능 아닐까?",
          "original_form": "이런게 동호회의 순기능 아닐까?",
          "speaker_id": "2",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002128.1.16",
          "form": "동호회 하고있는거 있어?",
          "original_form": "동호회 하고있는거 있어?",
          "speaker_id": "2",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002128.1.17",
          "form": "나는 원래 배드민턴 동호회 했는데 코로나때매 멈춘지 1년은 된거같아 ㅠㅠ",
          "original_form": "나는 원래 배드민턴 동호회 했는데 코로나때매 멈춘지 1년은 된거같아 ㅠㅠ",
