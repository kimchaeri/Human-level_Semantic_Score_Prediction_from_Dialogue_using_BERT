      "utterance": [
        {
          "id": "MDRW2100045437.1.1",
          "form": "나는 거의 늘 소화가 안돼서",
          "original_form": "나는 거의 늘 소화가 안돼서",
          "speaker_id": "2",
          "time": "20210908 02:31"
        },
        {
          "id": "MDRW2100045437.1.2",
          "form": "이게....늙는다는것인가....?",
          "original_form": "이게....늙는다는것인가....?",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045437.1.3",
          "form": "소화기관이 퇴화해간다는것....?",
          "original_form": "소화기관이 퇴화해간다는것....?",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045437.1.4",
          "form": "아닠",
          "original_form": "아닠",
          "speaker_id": "1",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045437.1.5",
          "form": "나이든 인간들은 이러고 어떻게 삶을 사는것이지....?",
          "original_form": "나이든 인간들은 이러고 어떻게 삶을 사는것이지....?",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045437.1.6",
          "form": "하고",
          "original_form": "하고",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045437.1.7",
          "form": "걍 선천적으로 소화가 잘 안될수도...",
          "original_form": "걍 선천적으로 소화가 잘 안될수도...",
          "speaker_id": "1",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045437.1.8",
          "form": "영생을 빼앗긴 인외처럼",
          "original_form": "영생을 빼앗긴 인외처럼",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045437.1.9",
          "form": "생각하고잇엇는데",
          "original_form": "생각하고잇엇는데",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045437.1.10",
          "form": "약먹으니까 잘되더라구",
          "original_form": "약먹으니까 잘되더라구",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045437.1.11",
          "form": "내가 장이 좀 안좋긴 했어",
          "original_form": "내가 장이 좀 안좋긴 했어",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045437.1.12",
          "form": "옛날부러",
          "original_form": "옛날부러",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045437.1.13",
          "form": "터",
          "original_form": "터",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045437.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 역시 현대의학이 최고다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 역시 현대의학이 최고다",
          "speaker_id": "1",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045437.1.15",
          "form": "ㅇㅇ.....근데 진짜",
          "original_form": "ㅇㅇ.....근데 진짜",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045437.1.16",
          "form": "내가 역류성 식도염",
          "original_form": "내가 역류성 식도염",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045437.1.17",
          "form": "대충 그런거 있긴 하겠지 했는데",
          "original_form": "대충 그런거 있긴 하겠지 했는데",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045437.1.18",
          "form": "그게 소화불량을 일으킬줄은",
          "original_form": "그게 소화불량을 일으킬줄은",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045437.1.19",
          "form": "생각도 못했어",
          "original_form": "생각도 못했어",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045437.1.20",
          "form": "위는 깨끗하대?",
          "original_form": "위는 깨끗하대?",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045437.1.21",
          "form": "ㅇㅇ다행히",
          "original_form": "ㅇㅇ다행히",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045437.1.22",
          "form": "옛날에 있던 궤양은",
          "original_form": "옛날에 있던 궤양은",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045437.1.23",
          "form": "없어졋나봐",
          "original_form": "없어졋나봐",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045437.1.24",
          "form": "다행이네",
          "original_form": "다행이네",
          "speaker_id": "1",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045437.1.25",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045437.1.26",
          "form": "난 위 점막에 약간 초기단계? 있대서",
          "original_form": "난 위 점막에 약간 초기단계? 있대서",
          "speaker_id": "1",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100045437.1.27",
          "form": "겁먹고",
          "original_form": "겁먹고",
          "speaker_id": "1",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100045437.1.28",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100045437.1.29",
          "form": "헉 어케하죠??? 했는데",
          "original_form": "헉 어케하죠??? 했는데",
          "speaker_id": "1",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100045437.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ의사들은",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ의사들은",
          "speaker_id": "2",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100045437.1.31",
          "form": "말을 무섭게 해",
          "original_form": "말을 무섭게 해",
          "speaker_id": "2",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100045437.1.32",
          "form": "ㅇㅇ ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100045437.1.33",
          "form": "근데 되게 심드렁한 얼굴로",
          "original_form": "근데 되게 심드렁한 얼굴로",
          "speaker_id": "1",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100045437.1.34",
          "form": "현대인은 이정도 다 있어요... 없는 사람 찾는게 더 힘들어요...",
          "original_form": "현대인은 이정도 다 있어요... 없는 사람 찾는게 더 힘들어요...",
          "speaker_id": "1",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100045437.1.35",
          "form": "하길래 안심함",
          "original_form": "하길래 안심함",
          "speaker_id": "1",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100045437.1.36",
          "form": "난 궁금한게 잇어",
          "original_form": "난 궁금한게 잇어",
          "speaker_id": "2",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100045437.1.37",
          "form": "현대인들이 어느정도 달고 사는 가벼운 질병 있잖아",
          "original_form": "현대인들이 어느정도 달고 사는 가벼운 질병 있잖아",
          "speaker_id": "2",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045437.1.38",
          "form": "그럼 옛날사람들은 그런거 없었을까?",
          "original_form": "그럼 옛날사람들은 그런거 없었을까?",
          "speaker_id": "2",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045437.1.39",
          "form": "왜 현대인들은 그정도는 다 잇다고 할까",
          "original_form": "왜 현대인들은 그정도는 다 잇다고 할까",
          "speaker_id": "2",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045437.1.40",
          "form": "이런 말 입밖으러 꺼내면",
          "original_form": "이런 말 입밖으러 꺼내면",
          "speaker_id": "2",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045437.1.41",
          "form": "엄마가 또 쓰잘떼기없는 생각한다고 등짝 때리니까",
          "original_form": "엄마가 또 쓰잘떼기없는 생각한다고 등짝 때리니까",
          "speaker_id": "2",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045437.1.42",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아닠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아닠",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045437.1.43",
          "form": "음 근데",
          "original_form": "음 근데",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045437.1.44",
          "form": "일단 옛날 사람은 없었을거갘눔",
          "original_form": "일단 옛날 사람은 없었을거갘눔",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045437.1.45",
          "form": "같음",
          "original_form": "같음",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045437.1.46",
