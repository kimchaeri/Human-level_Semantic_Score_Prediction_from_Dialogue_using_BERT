      "utterance": [
        {
          "id": "MDRW2100002960.1.1",
          "form": "나는 괜찮았어",
          "original_form": "나는 괜찮았어",
          "speaker_id": "1",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002960.1.2",
          "form": "음식 가리지 않고 잘먹는 편이라 영국음식이라고 맛없거나 하지는 않았어",
          "original_form": "음식 가리지 않고 잘먹는 편이라 영국음식이라고 맛없거나 하지는 않았어",
          "speaker_id": "1",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002960.1.3",
          "form": "여행가면 먹거리도 정말 중요하지",
          "original_form": "여행가면 먹거리도 정말 중요하지",
          "speaker_id": "1",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002960.1.4",
          "form": "영국음식 빵에 생선들어가있다고 막 ㅋㅋㅋ",
          "original_form": "영국음식 빵에 생선들어가있다고 막 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002960.1.5",
          "form": "나는 외화보면 영국 발음 특이해서 매력있더라고",
          "original_form": "나는 외화보면 영국 발음 특이해서 매력있더라고",
          "speaker_id": "2",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002960.1.6",
          "form": "응 나도 영국발음은 너무 좋더라",
          "original_form": "응 나도 영국발음은 너무 좋더라",
          "speaker_id": "1",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002960.1.7",
          "form": "이유는 모르겠는데",
          "original_form": "이유는 모르겠는데",
          "speaker_id": "1",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002960.1.8",
          "form": "이번달 말에 휴가 가는데",
          "original_form": "이번달 말에 휴가 가는데",
          "speaker_id": "1",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002960.1.9",
          "form": "휴가는 다녀왔어?",
          "original_form": "휴가는 다녀왔어?",
          "speaker_id": "1",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002960.1.10",
          "form": "발음이 물툭하면서 고급져 ㅎ",
          "original_form": "발음이 물툭하면서 고급져 ㅎ",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002960.1.11",
          "form": "휴가가 늦네",
          "original_form": "휴가가 늦네",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002960.1.12",
          "form": "우린 진작 집에서 홈캉스라고 하나? ㅋㅋㅋ",
          "original_form": "우린 진작 집에서 홈캉스라고 하나? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002960.1.13",
          "form": "어디로 가?",
          "original_form": "어디로 가?",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002960.1.14",
          "form": "회사에서 휴양시설 신청한게 당첨되서",
          "original_form": "회사에서 휴양시설 신청한게 당첨되서",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002960.1.15",
          "form": "부산 해운대로 갈 예정이야",
          "original_form": "부산 해운대로 갈 예정이야",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002960.1.16",
          "form": "복지가 좋은 회사네",
          "original_form": "복지가 좋은 회사네",
          "speaker_id": "2",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002960.1.17",
          "form": "선선해서 여행하기좋겠다 사람많은 시즌보다는",
          "original_form": "선선해서 여행하기좋겠다 사람많은 시즌보다는",
          "speaker_id": "2",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002960.1.18",
          "form": "응 사람붐비는것보다는 여유있고 한가한게 좋아서",
          "original_form": "응 사람붐비는것보다는 여유있고 한가한게 좋아서",
          "speaker_id": "1",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002960.1.19",
          "form": "요새 코로나때문에 집에서 휴가 보냈나보네",
          "original_form": "요새 코로나때문에 집에서 휴가 보냈나보네",
          "speaker_id": "1",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002960.1.20",
          "form": "우리는 8월 초였어",
          "original_form": "우리는 8월 초였어",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002960.1.21",
          "form": "내가 더운것도ㅜ싫어하고",
          "original_form": "내가 더운것도ㅜ싫어하고",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002960.1.22",
          "form": "사람 많은건 더 싫어하고",
          "original_form": "사람 많은건 더 싫어하고",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002960.1.23",
          "form": "맞아 코로나가 한몫했지",
          "original_form": "맞아 코로나가 한몫했지",
          "speaker_id": "2",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002960.1.24",
          "form": "지금은 여행다니기 좋을것 같아",
          "original_form": "지금은 여행다니기 좋을것 같아",
          "speaker_id": "2",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002960.1.25",
          "form": "응 많이 선선해진것 같아",
          "original_form": "응 많이 선선해진것 같아",
          "speaker_id": "1",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100002960.1.26",
          "form": "여전히 코로나 기승이지만",
          "original_form": "여전히 코로나 기승이지만",
          "speaker_id": "1",
          "time": "20210820 17:35"
        },
        {
          "id": "MDRW2100002960.1.27",
          "form": "돌아다니지 맣고",
          "original_form": "돌아다니지 맣고",
          "speaker_id": "1",
          "time": "20210820 17:35"
        },
        {
          "id": "MDRW2100002960.1.28",
          "form": "날씨가 너무 좋으니 드라이브만 해도 좋을것 같어",
          "original_form": "날씨가 너무 좋으니 드라이브만 해도 좋을것 같어",
          "speaker_id": "2",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100002960.1.29",
          "form": "힐링되게 ㅎ",
          "original_form": "힐링되게 ㅎ",
          "speaker_id": "2",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100002960.1.30",
          "form": "그것도 좋겠다",
          "original_form": "그것도 좋겠다",
          "speaker_id": "1",
          "time": "20210820 17:42"
        },
        {
          "id": "MDRW2100002960.1.31",
          "form": "드라이브도 좋지",
          "original_form": "드라이브도 좋지",
          "speaker_id": "1",
          "time": "20210820 17:42"
        },
        {
          "id": "MDRW2100002960.1.32",
          "form": "주말 잘 보내고 있어?",
          "original_form": "주말 잘 보내고 있어?",
          "speaker_id": "2",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100002960.1.33",
          "form": "난 좀 바빴어",
          "original_form": "난 좀 바빴어",
          "speaker_id": "2",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100002960.1.34",
          "form": "길냥이를 갑자기 입양하게 되서 준비하고 적응하느라 ㅠ",
          "original_form": "길냥이를 갑자기 입양하게 되서 준비하고 적응하느라 ㅠ",
          "speaker_id": "2",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100002960.1.35",
          "form": "주말인데 와이프랑 어디 놀러안가?",
          "original_form": "주말인데 와이프랑 어디 놀러안가?",
          "speaker_id": "2",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100002960.1.36",
          "form": "와이프 컨디션이 안좋아서",
          "original_form": "와이프 컨디션이 안좋아서",
          "speaker_id": "1",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100002960.1.37",
          "form": "집에서 보내고 있어",
          "original_form": "집에서 보내고 있어",
          "speaker_id": "1",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100002960.1.38",
          "form": "고양이 키우는거야?",
          "original_form": "고양이 키우는거야?",
          "speaker_id": "1",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100002960.1.39",
          "form": "아 저런 집에서 잘 케어해줘",
          "original_form": "아 저런 집에서 잘 케어해줘",
          "speaker_id": "2",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100002960.1.40",
          "form": "주말은 남편이 맛난거 해줘도 되는거 알지 ㅎ",
          "original_form": "주말은 남편이 맛난거 해줘도 되는거 알지 ㅎ",
          "speaker_id": "2",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100002960.1.41",
          "form": "응 식구가 하나 늘었어 ㅎ",
          "original_form": "응 식구가 하나 늘었어 ㅎ",
          "speaker_id": "2",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100002960.1.42",
          "form": "새ㄲㅣ고양이야?",
          "original_form": "새ㄲㅣ고양이야?",
          "speaker_id": "1",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100002960.1.43",
          "form": "응 태어난지 두달 됐나봐",
          "original_form": "응 태어난지 두달 됐나봐",
          "speaker_id": "2",
          "time": "20210822 11:07"
        },
        {
          "id": "MDRW2100002960.1.44",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210822 11:07"
        },
        {
          "id": "MDRW2100002960.1.45",
          "form": "좋은일했다",
          "original_form": "좋은일했다",
          "speaker_id": "1",
          "time": "20210822 11:08"
        },
        {
          "id": "MDRW2100002960.1.46",
          "form": "이제 얘때문에 외출도 마음대로 못하겠다 ㅋㅋㅋ",
          "original_form": "이제 얘때문에 외출도 마음대로 못하겠다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:08"
        },
        {
          "id": "MDRW2100002960.1.47",
          "form": "고양이는 독립성이 강해서 괜찮지 않을까",
          "original_form": "고양이는 독립성이 강해서 괜찮지 않을까",
          "speaker_id": "1",
          "time": "20210822 12:29"
        }
      ]
    }
