      "utterance": [
        {
          "id": "MDRW2100038070.1.1",
          "form": "name1님 머하구계시나요 친구랑 사업준비 ㅋ",
          "original_form": "&name1&님 머하구계시나요 친구랑 사업준비 ㅋ",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038070.1.2",
          "form": "저는 실업급여 받았어요 어제",
          "original_form": "저는 실업급여 받았어요 어제",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038070.1.3",
          "form": "실업자예요 ㅠ",
          "original_form": "실업자예요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038070.1.4",
          "form": "실업자니까 새일을 꾸려야쥬",
          "original_form": "실업자니까 새일을 꾸려야쥬",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038070.1.5",
          "form": "일하기 싫어요",
          "original_form": "일하기 싫어요",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038070.1.6",
          "form": "저도 마찬가지예요",
          "original_form": "저도 마찬가지예요",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038070.1.7",
          "form": "일 잘 못하고 인간관계 스트레스요",
          "original_form": "일 잘 못하고 인간관계 스트레스요",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038070.1.8",
          "form": "name1님 일잘하시자나요",
          "original_form": "&name1&님 일잘하시자나요",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038070.1.9",
          "form": "돈 많이주면 할꺼봐유",
          "original_form": "돈 많이주면 할꺼봐유",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038070.1.10",
          "form": "ㅋㅋ 오우 그런건 나도알려주쇼 ㅋㅋ",
          "original_form": "ㅋㅋ 오우 그런건 나도알려주쇼 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038070.1.11",
          "form": "등이는 재계약 안됨 다른일 할거임?",
          "original_form": "등이는 재계약 안됨 다른일 할거임?",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100038070.1.12",
          "form": "일안함 어짜농 머라두해야제ㅋㅋ",
          "original_form": "일안함 어짜농 머라두해야제ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100038070.1.13",
          "form": "이직한다면 가장 중요한 조건은?",
          "original_form": "이직한다면 가장 중요한 조건은?",
