      "utterance": [
        {
          "id": "MDRW2100002963.1.1",
          "form": "체온계 사야된다며?",
          "original_form": "체온계 사야된다며?",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002963.1.2",
          "form": "근데 아까 애들이 회 다 먹어서 배고프다 ㅠㅠ",
          "original_form": "근데 아까 애들이 회 다 먹어서 배고프다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 19:06"
        },
        {
          "id": "MDRW2100002963.1.3",
          "form": "저번에 치료받은 무릎은 좀 괜찮아?",
          "original_form": "저번에 치료받은 무릎은 좀 괜찮아?",
          "speaker_id": "1",
          "time": "20210820 23:16"
        },
        {
          "id": "MDRW2100002963.1.4",
          "form": "좋지~ 최고지",
          "original_form": "좋지~ 최고지",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100002963.1.5",
          "form": "집에 1kg 미용아령 있잖아. 그걸로 운동할까?",
          "original_form": "집에 1kg 미용아령 있잖아. 그걸로 운동할까?",
          "speaker_id": "2",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100002963.1.6",
          "form": "ㅇㅇ 있는 것 좀 활용해",
          "original_form": "ㅇㅇ 있는 것 좀 활용해",
          "speaker_id": "1",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100002963.1.7",
          "form": "결혼 하기 전엔 수시로 했는데 지금은 ㅠㅠ",
          "original_form": "결혼 하기 전엔 수시로 했는데 지금은 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100002963.1.8",
          "form": "지금부터라도 하면 돼",
          "original_form": "지금부터라도 하면 돼",
          "speaker_id": "1",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100002963.1.9",
          "form": "아령 들었다놨다 하는 것도 애들 있으면 신경쓰여 ㅎㅎ",
          "original_form": "아령 들었다놨다 하는 것도 애들 있으면 신경쓰여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100002963.1.10",
          "form": "ㅎㅎ 애들이 맨날 엄마 곁을 맴도니",
          "original_form": "ㅎㅎ 애들이 맨날 엄마 곁을 맴도니",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100002963.1.11",
          "form": "아령 사서 운동하는 게 그래도 효과는 좋더라",
          "original_form": "아령 사서 운동하는 게 그래도 효과는 좋더라",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100002963.1.12",
          "form": "맨손으로 하는 것보다는 확실히 낫지",
          "original_form": "맨손으로 하는 것보다는 확실히 낫지",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100002963.1.13",
          "form": "아령 너무 오래 돼서 새 걸로 하나 알아봐야겠다",
          "original_form": "아령 너무 오래 돼서 새 걸로 하나 알아봐야겠다",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100002963.1.14",
          "form": "ㅇㅇ 그랴요 다 녹슬었더라",
          "original_form": "ㅇㅇ 그랴요 다 녹슬었더라",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100002963.1.15",
          "form": "ㅎㅎ 그럼 아령 하나 산다",
          "original_form": "ㅎㅎ 그럼 아령 하나 산다",
          "speaker_id": "2",
