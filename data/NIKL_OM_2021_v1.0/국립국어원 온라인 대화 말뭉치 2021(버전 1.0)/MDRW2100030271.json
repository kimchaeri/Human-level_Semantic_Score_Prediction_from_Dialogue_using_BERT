      },
      "utterance": [
        {
          "id": "MDRW2100030271.1.1",
          "form": "나두 긍금하다 ㅎㅎ",
          "original_form": "나두 긍금하다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030271.1.2",
          "form": "아직 취업해본저깅 없어서",
          "original_form": "아직 취업해본저깅 없어서",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030271.1.3",
          "form": "걱정되기두해,,",
          "original_form": "걱정되기두해,,",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030271.1.4",
          "form": "나도 없어..",
          "original_form": "나도 없어..",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030271.1.5",
          "form": "맞아 너무 궁금하구",
          "original_form": "맞아 너무 궁금하구",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030271.1.6",
          "form": "ㅜㅠ",
          "original_form": "ㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030271.1.7",
          "form": "상사한테 혼나면 슬프겠지..",
          "original_form": "상사한테 혼나면 슬프겠지..",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030271.1.8",
          "form": "하지만 회사에 다니면",
          "original_form": "하지만 회사에 다니면",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030271.1.9",
          "form": "돈을 벌수 있잖어 ㅋㅋ",
          "original_form": "돈을 벌수 있잖어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030271.1.10",
          "form": "그 전까지는 윗사람이 있다는 개념이없잔아",
          "original_form": "그 전까지는 윗사람이 있다는 개념이없잔아",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030271.1.11",
          "form": "그렇지만",
          "original_form": "그렇지만",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030271.1.12",
          "form": "그래서 힘내서 다니는거겟지..",
          "original_form": "그래서 힘내서 다니는거겟지..",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030271.1.13",
          "form": "처음으로 윗사람이 생기는거니까..",
          "original_form": "처음으로 윗사람이 생기는거니까..",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030271.1.14",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030271.1.15",
          "form": "선생님한테 혼나는거랑은 다르지 ㅎㅎ",
          "original_form": "선생님한테 혼나는거랑은 다르지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030271.1.16",
          "form": "선생님들은 윗사람이라해도 다 친절하잖아",
          "original_form": "선생님들은 윗사람이라해도 다 친절하잖아",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030271.1.17",
          "form": "맞아..",
          "original_form": "맞아..",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030271.1.18",
          "form": "그래도 요즘엔 직원들끼리 위계질서 없는 회사두",
          "original_form": "그래도 요즘엔 직원들끼리 위계질서 없는 회사두",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030271.1.19",
          "form": "잇긴 잇대 ㅎㅎ",
          "original_form": "잇긴 잇대 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030271.1.20",
          "form": "오 ㅎㅎ 진짜일지모르겠지만",
          "original_form": "오 ㅎㅎ 진짜일지모르겠지만",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030271.1.21",
          "form": "그런 회사가 많아지면 좋겟지",
          "original_form": "그런 회사가 많아지면 좋겟지",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030271.1.22",
          "form": "맞아 ㅎㅎ",
          "original_form": "맞아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030271.1.23",
          "form": "그래두 난 이왕이면",
          "original_form": "그래두 난 이왕이면",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030271.1.24",
          "form": "돈 많이 주는 회사에",
          "original_form": "돈 많이 주는 회사에",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030271.1.25",
          "form": "다니구 싶어 ㅋ",
          "original_form": "다니구 싶어 ㅋ",
          "speaker_id": "2",
          "time": "20210903 23:25"
        }
      ]
