      "utterance": [
        {
          "id": "MDRW2100039761.1.1",
          "form": "name2님은 동물 좋아하세요??",
          "original_form": "&name2&님은 동물 좋아하세요??",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039761.1.2",
          "form": "너무좋아해요!",
          "original_form": "너무좋아해요!",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039761.1.3",
          "form": "저도 진짜 좋아하는데 기르는건 여건이 안돼서ㅠㅠ",
          "original_form": "저도 진짜 좋아하는데 기르는건 여건이 안돼서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039761.1.4",
          "form": "고양이파세요 강아지파세요????",
          "original_form": "고양이파세요 강아지파세요????",
          "speaker_id": "1",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039761.1.5",
          "form": "저는... 햄스터파예요ㅋㅋㅋㅋ",
          "original_form": "저는... 햄스터파예요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100039761.1.6",
          "form": "저원래 강아지파였는데",
          "original_form": "저원래 강아지파였는데",
          "speaker_id": "2",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100039761.1.7",
          "form": "회사 고양이들에 홀랑빠져서 고양이파가됐어요",
          "original_form": "회사 고양이들에 홀랑빠져서 고양이파가됐어요",
          "speaker_id": "2",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100039761.1.8",
          "form": "햄스터 너무 깜찍하죠!",
          "original_form": "햄스터 너무 깜찍하죠!",
          "speaker_id": "2",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100039761.1.9",
          "form": "ㅠㅠ햄스터들 그 멍때릴때 얼굴이 넘 귀엽다고 생각해요ㅜㅜ",
          "original_form": "ㅠㅠ햄스터들 그 멍때릴때 얼굴이 넘 귀엽다고 생각해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100039761.1.10",
          "form": "헉 근데 회사고양이면 회사 근처 길냥이들인건가요???",
          "original_form": "헉 근데 회사고양이면 회사 근처 길냥이들인건가요???",
          "speaker_id": "1",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039761.1.11",
          "form": "그래도 사람 잘 따르나봐요!",
          "original_form": "그래도 사람 잘 따르나봐요!",
          "speaker_id": "1",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039761.1.12",
          "form": "친구햄스터 며칠 돌봐줬었는데 너무귀엽더라고요ㅠ",
          "original_form": "친구햄스터 며칠 돌봐줬었는데 너무귀엽더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039761.1.13",
          "form": "회사내에서 살고있는 반스트릿이 되어버린 애옹들이에요",
          "original_form": "회사내에서 살고있는 반스트릿이 되어버린 애옹들이에요",
          "speaker_id": "2",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039761.1.14",
          "form": "우와우와 회사안에서 산다니 넘 신기해요",
          "original_form": "우와우와 회사안에서 산다니 넘 신기해요",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039761.1.15",
          "form": "어,,,,",
          "original_form": "어,,,,",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039761.1.16",
          "form": "제가회사가 학교였어서ㅎㅎ",
          "original_form": "제가회사가 학교였어서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039761.1.17",
          "form": "아 아예안에서 사는건 아니군욬ㅋㅋㅋㅋ",
          "original_form": "아 아예안에서 사는건 아니군욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039761.1.18",
          "form": "아...아...!!!!",
          "original_form": "아...아...!!!!",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039761.1.19",
          "form": "ㅋㅋㅋㅋㅋ네학생들이 잘돌봐줘서",
          "original_form": "ㅋㅋㅋㅋㅋ네학생들이 잘돌봐줘서",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039761.1.20",
          "form": "애옹들이 애교가많았어요",
          "original_form": "애옹들이 애교가많았어요",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039761.1.21",
          "form": "ㅋㅋㅋㅋ아 그래서 반스트릿이라고 하셨구나ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ아 그래서 반스트릿이라고 하셨구나ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039761.1.22",
          "form": "혹시 이름도 있었나요???",
          "original_form": "혹시 이름도 있었나요???",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039761.1.23",
          "form": "냥이들이요!",
          "original_form": "냥이들이요!",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039761.1.24",
          "form": "앗 대학생이라고하셔서 ㅋㅋㅋ말못할것같아요",
          "original_form": "앗 대학생이라고하셔서 ㅋㅋㅋ말못할것같아요",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039761.1.25",
          "form": "나름 스타애옹이라서",
          "original_form": "나름 스타애옹이라서",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039761.1.26",
          "form": "인스타에치면 나온답니다",
          "original_form": "인스타에치면 나온답니다",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039761.1.27",
          "form": "앗아 넵ㅋㅋㅋㅋㅋ 이해햇어요",
          "original_form": "앗아 넵ㅋㅋㅋㅋㅋ 이해햇어요",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039761.1.28",
          "form": "교내에 동물보호동아리 있으면 근처 유기동물들 사진도 올리고 해서",
          "original_form": "교내에 동물보호동아리 있으면 근처 유기동물들 사진도 올리고 해서",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039761.1.29",
          "form": "스타냥이되는 것 같아요ㅋㅋㅋㅋ 저희 학교도 비슷하답니다",
          "original_form": "스타냥이되는 것 같아요ㅋㅋㅋㅋ 저희 학교도 비슷하답니다",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039761.1.30",
          "form": "요즘 학교들마다 마스코트애옹들이 있더라고요ㅠ",
          "original_form": "요즘 학교들마다 마스코트애옹들이 있더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039761.1.31",
          "form": "쏘큩깜찍",
          "original_form": "쏘큩깜찍",
          "speaker_id": "2",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039761.1.32",
          "form": "맞아요ㅋㅋㅋㅋ 마스코트 냥이들ㅎㅅㅎ",
          "original_form": "맞아요ㅋㅋㅋㅋ 마스코트 냥이들ㅎㅅㅎ",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039761.1.33",
          "form": "대학가라 고양이 유기가 쉬워서 학교 근처에 냥이가 많은가 싶기도 해요",
          "original_form": "대학가라 고양이 유기가 쉬워서 학교 근처에 냥이가 많은가 싶기도 해요",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039761.1.34",
          "form": "저 자취할때도 유기묘 구조해서 입양보낸적 있거든요ㅠㅠ",
          "original_form": "저 자취할때도 유기묘 구조해서 입양보낸적 있거든요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 21:31"
        }
      ]
