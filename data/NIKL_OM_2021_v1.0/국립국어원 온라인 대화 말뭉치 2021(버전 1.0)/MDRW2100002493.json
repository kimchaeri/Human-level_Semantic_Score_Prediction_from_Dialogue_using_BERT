      "utterance": [
        {
          "id": "MDRW2100002493.1.1",
          "form": "잡지를 즐겨보시나요?",
          "original_form": "잡지를 즐겨보시나요?",
          "speaker_id": "1",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002493.1.2",
          "form": "음 아니요",
          "original_form": "음 아니요",
          "speaker_id": "2",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002493.1.3",
          "form": "안본지 오래된듯해요",
          "original_form": "안본지 오래된듯해요",
          "speaker_id": "2",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002493.1.4",
          "form": "지원님은 자주보세요?",
          "original_form": "지원님은 자주보세요?",
          "speaker_id": "2",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002493.1.5",
          "form": "저는 미용실같은 곳 가면 보는 거 같아요",
          "original_form": "저는 미용실같은 곳 가면 보는 거 같아요",
          "speaker_id": "1",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002493.1.6",
          "form": "저도 미용실에서 보는거아니면 잘 안보게되네요",
          "original_form": "저도 미용실에서 보는거아니면 잘 안보게되네요",
          "speaker_id": "2",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002493.1.7",
          "form": "평소에는 안보지만 시간 떼우기는 좋은?ㅎ",
          "original_form": "평소에는 안보지만 시간 떼우기는 좋은?ㅎ",
          "speaker_id": "1",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002493.1.8",
          "form": "이상하게 미용실만가면",
          "original_form": "이상하게 미용실만가면",
          "speaker_id": "2",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002493.1.9",
          "form": "재밌죠",
          "original_form": "재밌죠",
          "speaker_id": "1",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002493.1.10",
          "form": "그렇게 재밌게 느껴져요ㅋㅋㅋ",
          "original_form": "그렇게 재밌게 느껴져요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002493.1.11",
          "form": "치과나 미용실 이런데가면ㅋㅋ",
          "original_form": "치과나 미용실 이런데가면ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002493.1.12",
          "form": "평소에는 보지도 않는데",
          "original_form": "평소에는 보지도 않는데",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002493.1.13",
          "form": "맞아요ㅋㅋㅋ 그냥 시간때우기로",
          "original_form": "맞아요ㅋㅋㅋ 그냥 시간때우기로",
          "speaker_id": "2",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002493.1.14",
          "form": "광고도 재밋어서",
          "original_form": "광고도 재밋어서",
          "speaker_id": "2",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002493.1.15",
          "form": "그런 곳 가면 재밌더라구요ㅋㅋ",
          "original_form": "그런 곳 가면 재밌더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002493.1.16",
          "form": "맞아요ㅋㅋㄲ",
          "original_form": "맞아요ㅋㅋㄲ",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002493.1.17",
          "form": "자세히읽게돼요ㅋㅋㅋ",
          "original_form": "자세히읽게돼요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002493.1.18",
          "form": "그리고 홈쇼핑카달로그 이런것도",
          "original_form": "그리고 홈쇼핑카달로그 이런것도",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002493.1.19",
          "form": "있으면 보고",
          "original_form": "있으면 보고",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002493.1.20",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002493.1.21",
          "form": "평소엔 신경도 안쓰는뎅",
          "original_form": "평소엔 신경도 안쓰는뎅",
          "speaker_id": "2",
          "time": "20210820 12:53"
        },
        {
          "id": "MDRW2100002493.1.22",
          "form": "할게없으니까 집중하게돼서ㅋㅋ",
          "original_form": "할게없으니까 집중하게돼서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:53"
        },
        {
          "id": "MDRW2100002493.1.23",
          "form": "그 순간만큼은 진짜 진심이되서 ㅋㅋ",
          "original_form": "그 순간만큼은 진짜 진심이되서 ㅋㅋ",
