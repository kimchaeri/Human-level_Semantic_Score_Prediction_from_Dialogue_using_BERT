      "utterance": [
        {
          "id": "MDRW2100022169.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100022169.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210829 10:37"
        },
        {
          "id": "MDRW2100022169.1.3",
          "form": "좋은 아침입니다",
          "original_form": "좋은 아침입니다",
          "speaker_id": "1",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022169.1.4",
          "form": "네 좋은아침입니다ㅋ",
          "original_form": "네 좋은아침입니다ㅋ",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022169.1.5",
          "form": "요즘 티비 뭐 보는거있으세요?",
          "original_form": "요즘 티비 뭐 보는거있으세요?",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022169.1.6",
          "form": "음 저는 슬기로운의사생활 보는데 힐링되요 ㅎㅎ",
          "original_form": "음 저는 슬기로운의사생활 보는데 힐링되요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022169.1.7",
          "form": "다른건 안봅니다 ㅎㅎ",
          "original_form": "다른건 안봅니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022169.1.8",
          "form": "장르 어떤거 좋아하세요?",
          "original_form": "장르 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022169.1.9",
          "form": "맞아요ㅋ저도 슬기로운의사생활",
          "original_form": "맞아요ㅋ저도 슬기로운의사생활",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022169.1.10",
          "form": "너무좋아해요ㅋ",
          "original_form": "너무좋아해요ㅋ",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022169.1.11",
          "form": "감동입니다",
          "original_form": "감동입니다",
          "speaker_id": "1",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100022169.1.12",
          "form": "이번에 도재학 선생 관련내용은 맘이 아팠어요",
          "original_form": "이번에 도재학 선생 관련내용은 맘이 아팠어요",
          "speaker_id": "1",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100022169.1.13",
          "form": "저도 얼마나 울었는지몰라요ㅜㅠ슬펐어요",
          "original_form": "저도 얼마나 울었는지몰라요ㅜㅠ슬펐어요",
          "speaker_id": "2",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100022169.1.14",
          "form": "결국 잘될겁니다",
          "original_form": "결국 잘될겁니다",
          "speaker_id": "1",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100022169.1.15",
          "form": "도재학 화이팅!",
          "original_form": "도재학 화이팅!",
          "speaker_id": "1",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100022169.1.16",
          "form": "아\n그리고",
          "original_form": "아\n그리고",
          "speaker_id": "1",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100022169.1.17",
          "form": "익순이 이야기 너무 궁금해요",
          "original_form": "익순이 이야기 너무 궁금해요",
          "speaker_id": "1",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100022169.1.18",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100022169.1.19",
          "form": "익순이도 잘되기를",
          "original_form": "익순이도 잘되기를",
          "speaker_id": "2",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100022169.1.20",
          "form": "추추 좋아하십니까",
          "original_form": "추추 좋아하십니까",
          "speaker_id": "1",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100022169.1.21",
          "form": "해피엔딩일것같지만ㅋ",
          "original_form": "해피엔딩일것같지만ㅋ",
          "speaker_id": "2",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100022169.1.22",
          "form": "진짜 요즘엔",
          "original_form": "진짜 요즘엔",
          "speaker_id": "2",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100022169.1.23",
          "form": "추추를 넘 응원하게되네요",
          "original_form": "추추를 넘 응원하게되네요",
          "speaker_id": "2",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100022169.1.24",
          "form": "추추 넘 귀염ㅋ",
          "original_form": "추추 넘 귀염ㅋ",
          "speaker_id": "2",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100022169.1.25",
          "form": "짱 귀염",
          "original_form": "짱 귀염",
          "speaker_id": "1",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100022169.1.26",
          "form": "마지막 누가 다쳤을까요?",
          "original_form": "마지막 누가 다쳤을까요?",
          "speaker_id": "1",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100022169.1.27",
          "form": "익준이인것같아요.ㅜㅜ",
          "original_form": "익준이인것같아요.ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100022169.1.28",
          "form": "안돼,~~",
          "original_form": "안돼,~~",
          "speaker_id": "1",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100022169.1.29",
          "form": "두 의사 모두 아니지 않을까요?",
          "original_form": "두 의사 모두 아니지 않을까요?",
          "speaker_id": "1",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100022169.1.30",
          "form": "ㅜㅠ그러길요ㅋㅋ",
          "original_form": "ㅜㅠ그러길요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:55"
        }
      ]
    }
