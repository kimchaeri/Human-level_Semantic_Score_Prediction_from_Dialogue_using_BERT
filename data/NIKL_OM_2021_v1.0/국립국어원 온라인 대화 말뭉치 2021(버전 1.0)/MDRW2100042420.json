      "utterance": [
        {
          "id": "MDRW2100042420.1.1",
          "form": "그람유..",
          "original_form": "그람유..",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042420.1.2",
          "form": "낼 아침입니당",
          "original_form": "낼 아침입니당",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042420.1.3",
          "form": "오 아침부터 배에 기름칠 제대로하시네여",
          "original_form": "오 아침부터 배에 기름칠 제대로하시네여",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042420.1.4",
          "form": "배우신분",
          "original_form": "배우신분",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042420.1.5",
          "form": "당연하쥬 약간 볶음밥도 시도해보려고",
          "original_form": "당연하쥬 약간 볶음밥도 시도해보려고",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042420.1.6",
          "form": "생각중인데 너무 투머츼할까요?",
          "original_form": "생각중인데 너무 투머츼할까요?",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042420.1.7",
          "form": "아니요 전혀요???",
          "original_form": "아니요 전혀요???",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042420.1.8",
          "form": "완전 맛있겠는데요??",
          "original_form": "완전 맛있겠는데요??",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042420.1.9",
          "form": "완전 허허허허허",
          "original_form": "완전 허허허허허",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042420.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ낼 뭐 드실계획이세요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ낼 뭐 드실계획이세요!",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042420.1.11",
          "form": "전오늘 편의점 김밥과 아아랑 그릭요거트를 먹었답니다",
          "original_form": "전오늘 편의점 김밥과 아아랑 그릭요거트를 먹었답니다",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100042420.1.12",
          "form": "후식으로 크레이프케익도요!!",
          "original_form": "후식으로 크레이프케익도요!!",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100042420.1.13",
          "form": "헐~~ 전 반금 와플대학 먹었습니다!",
          "original_form": "헐~~ 전 반금 와플대학 먹었습니다!",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100042420.1.14",
          "form": "헐 와플대학 안먹은지 오래됐어요",
          "original_form": "헐 와플대학 안먹은지 오래됐어요",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100042420.1.15",
          "form": "저도 거의 처음 먹는거 같아요!",
          "original_form": "저도 거의 처음 먹는거 같아요!",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100042420.1.16",
          "form": "와플 좋아하세요?",
          "original_form": "와플 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100042420.1.17",
          "form": "네 디저트 좋아해요",
          "original_form": "네 디저트 좋아해요",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100042420.1.18",
          "form": "와플도좋구 크로플도좋아요!!",
          "original_form": "와플도좋구 크로플도좋아요!!",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100042420.1.19",
          "form": "전 요즘 완전 크로플에 뻐졌엉ㅅ",
          "original_form": "전 요즘 완전 크로플에 뻐졌엉ㅅ",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100042420.1.20",
          "form": "크로플 너무 맛있지않나요?",
          "original_form": "크로플 너무 맛있지않나요?",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100042420.1.21",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100042420.1.22",
          "form": "전 개인적으로 크로플이더좋아요",
          "original_form": "전 개인적으로 크로플이더좋아요",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100042420.1.23",
          "form": "저도요 완전 바삭하구 넘 맛있어요 ㅠ",
          "original_form": "저도요 완전 바삭하구 넘 맛있어요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100042420.1.24",
          "form": "뉴가 처음에 만들 생각을 했을가요",
          "original_form": "뉴가 처음에 만들 생각을 했을가요",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100042420.1.25",
          "form": "그러게요 진짜ㅋㅋ",
