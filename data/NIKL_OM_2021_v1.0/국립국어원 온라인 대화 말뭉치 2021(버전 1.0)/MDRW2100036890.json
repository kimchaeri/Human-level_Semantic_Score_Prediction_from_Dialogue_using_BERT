      "utterance": [
        {
          "id": "MDRW2100036890.1.1",
          "form": "아고 바쁘다ㅜ머이래바쁘노",
          "original_form": "아고 바쁘다ㅜ머이래바쁘노",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100036890.1.2",
          "form": "빵사러가나?ㅡ",
          "original_form": "빵사러가나?ㅡ",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100036890.1.3",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100036890.1.4",
          "form": "빵사러가려구ㅋ",
          "original_form": "빵사러가려구ㅋ",
          "speaker_id": "1",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100036890.1.5",
          "form": "오~~~~ 득템하러가는구만ㅇ",
          "original_form": "오~~~~ 득템하러가는구만ㅇ",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100036890.1.6",
          "form": "얍얍ㅋㅋ",
          "original_form": "얍얍ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100036890.1.7",
          "form": "애들은다갔지? 자유시간이네ㅋ",
          "original_form": "애들은다갔지? 자유시간이네ㅋ",
          "speaker_id": "2",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100036890.1.8",
          "form": "지금 차안이라ㅋ히",
          "original_form": "지금 차안이라ㅋ히",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100036890.1.9",
          "form": "응응 자유시간이다ㅋㅋ너무좋네ㅋㅋ",
          "original_form": "응응 자유시간이다ㅋㅋ너무좋네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100036890.1.10",
          "form": "부럽다 자유시간ㅠㅠ",
          "original_form": "부럽다 자유시간ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100036890.1.11",
          "form": "빵사고는 뭐하려고?ㅋㅋ",
          "original_form": "빵사고는 뭐하려고?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100036890.1.12",
          "form": "내년쯤 좋지 않겠나 너도",
          "original_form": "내년쯤 좋지 않겠나 너도",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100036890.1.13",
          "form": "빵 사고 집에 와서 집안일 해야지",
          "original_form": "빵 사고 집에 와서 집안일 해야지",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100036890.1.14",
          "form": "아직 아침도 못 먹었다",
          "original_form": "아직 아침도 못 먹었다",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100036890.1.15",
          "form": "아침 먹었어?",
          "original_form": "아침 먹었어?",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100036890.1.16",
          "form": "아직ㅠㅠ못먹었어 ㅋㅋ배고프당",
          "original_form": "아직ㅠㅠ못먹었어 ㅋㅋ배고프당",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100036890.1.17",
          "form": "어제산모카빵 머그려고",
          "original_form": "어제산모카빵 머그려고",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100036890.1.18",
          "form": "모카빵 맛있지 ㅋㅋ좋더ㅋ",
          "original_form": "모카빵 맛있지 ㅋㅋ좋더ㅋ",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100036890.1.19",
          "form": "나도 오늘 모카빵두사야겠다ㅋ",
          "original_form": "나도 오늘 모카빵두사야겠다ㅋ",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100036890.1.20",
          "form": "생각만ㅋㅋ하고 움직이진않음",
          "original_form": "생각만ㅋㅋ하고 움직이진않음",
          "speaker_id": "2",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100036890.1.21",
          "form": "응응ㅋㅋ배가 덜고픈가베ㅋㅋ",
          "original_form": "응응ㅋㅋ배가 덜고픈가베ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100036890.1.22",
          "form": "아 웃겨ㅋ",
          "original_form": "아 웃겨ㅋ",
          "speaker_id": "1",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100036890.1.23",
          "form": "그런갑다ㅋㅋㅋㅋ귀차니즘이 배고픔을. 이기다니",
          "original_form": "그런갑다ㅋㅋㅋㅋ귀차니즘이 배고픔을. 이기다니",
