      "utterance": [
        {
          "id": "MDRW2100042323.1.1",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042323.1.2",
          "form": "어떤 스타일의 여행을 즐기시나요?",
          "original_form": "어떤 스타일의 여행을 즐기시나요?",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042323.1.3",
          "form": "호캉스가 좋은데 아직 유럽 못가봐서 거긴 꼭\n가려고요",
          "original_form": "호캉스가 좋은데 아직 유럽 못가봐서 거긴 꼭\n가려고요",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042323.1.4",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042323.1.5",
          "form": "호캉스는 사실 동남아쪽이 가성비가 좋죠ㅎㅎ",
          "original_form": "호캉스는 사실 동남아쪽이 가성비가 좋죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042323.1.6",
          "form": "그니까요ㅋㅋ 근데 요즘 상황이 태국도 베트남도 난리라",
          "original_form": "그니까요ㅋㅋ 근데 요즘 상황이 태국도 베트남도 난리라",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042323.1.7",
          "form": "네 동남아쪽 코로나 상황이ㅠ",
          "original_form": "네 동남아쪽 코로나 상황이ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042323.1.8",
          "form": "근데 사실 이건 해외 대부분 해당이라",
          "original_form": "근데 사실 이건 해외 대부분 해당이라",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042323.1.9",
          "form": "당분간 해외여행은 힘들듯해요ㅜ",
          "original_form": "당분간 해외여행은 힘들듯해요ㅜ",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042323.1.10",
          "form": "맞아요ㅜ 한국이 가장 안전하고 제일 깨끗하고요",
          "original_form": "맞아요ㅜ 한국이 가장 안전하고 제일 깨끗하고요",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042323.1.11",
          "form": "네 그래서 저는 코로나 좀 잠잠해지면 국내여행을 좀 다녀볼까해요",
          "original_form": "네 그래서 저는 코로나 좀 잠잠해지면 국내여행을 좀 다녀볼까해요",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042323.1.12",
          "form": "어디 가보셨어요?",
          "original_form": "어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042323.1.13",
          "form": "해외여행이요?",
          "original_form": "해외여행이요?",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042323.1.14",
          "form": "국내요",
          "original_form": "국내요",
          "speaker_id": "2",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042323.1.15",
          "form": "아 국내는 강원도 전라도 쪽 자주 다녔어요",
          "original_form": "아 국내는 강원도 전라도 쪽 자주 다녔어요",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042323.1.16",
          "form": "어 저도 전주랑 강릉 가봤어요",
          "original_form": "어 저도 전주랑 강릉 가봤어요",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042323.1.17",
          "form": "전주 강릉 다 좋죠~",
          "original_form": "전주 강릉 다 좋죠~",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042323.1.18",
          "form": "다음에 여수가고 싶어요",
          "original_form": "다음에 여수가고 싶어요",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042323.1.19",
          "form": "여수밤바다",
          "original_form": "여수밤바다",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042323.1.20",
          "form": "노래도 있잖아요",
          "original_form": "노래도 있잖아요",
          "speaker_id": "1",
          "time": "20210906 21:50"
        }
      ]
