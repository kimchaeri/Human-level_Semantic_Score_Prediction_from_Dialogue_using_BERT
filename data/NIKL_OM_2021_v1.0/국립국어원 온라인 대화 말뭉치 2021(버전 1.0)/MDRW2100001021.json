      "utterance": [
        {
          "id": "MDRW2100001021.1.1",
          "form": "티비는 자주보시나요?",
          "original_form": "티비는 자주보시나요?",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100001021.1.2",
          "form": "티비 자주 못보지만 예능좋아해요",
          "original_form": "티비 자주 못보지만 예능좋아해요",
          "speaker_id": "1",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100001021.1.3",
          "form": "저도요ㅎㅎ 펜트하우스는 안보시나요?ㅎ",
          "original_form": "저도요ㅎㅎ 펜트하우스는 안보시나요?ㅎ",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100001021.1.4",
          "form": "1편만 봤었어요ㅎ",
          "original_form": "1편만 봤었어요ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100001021.1.5",
          "form": "펜트하우스 재밌나요?ㅎ",
          "original_form": "펜트하우스 재밌나요?ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100001021.1.6",
          "form": "네~  재밌었는데 갑자기 지난주부터 코미디느낌?인더라구요",
          "original_form": "네~  재밌었는데 갑자기 지난주부터 코미디느낌?인더라구요",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100001021.1.7",
          "form": "그래도의리로 ㅋㅋ봅니다",
          "original_form": "그래도의리로 ㅋㅋ봅니다",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100001021.1.8",
          "form": "다른 추천 드라마 있으신가요?ㅎ",
          "original_form": "다른 추천 드라마 있으신가요?ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100001021.1.9",
          "form": "드라마는 오케이 광자매요",
          "original_form": "드라마는 오케이 광자매요",
          "speaker_id": "2",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100001021.1.10",
          "form": "요것도 이름만 들어봤어요ㅅ",
          "original_form": "요것도 이름만 들어봤어요ㅅ",
          "speaker_id": "1",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100001021.1.11",
          "form": "앗 그럼 예능은 뭘로 보시나요?ㅎㅎ",
          "original_form": "앗 그럼 예능은 뭘로 보시나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100001021.1.12",
          "form": "거의 챙겨보지는 않지만 대탈출 좋아해요ㅎ",
          "original_form": "거의 챙겨보지는 않지만 대탈출 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100001021.1.13",
          "form": "앗 저도 대탈출 좋아해요",
          "original_form": "앗 저도 대탈출 좋아해요",
          "speaker_id": "2",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100001021.1.14",
          "form": "이번시즌은 근데 못챙겨봤네여ㅜ",
          "original_form": "이번시즌은 근데 못챙겨봤네여ㅜ",
          "speaker_id": "2",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100001021.1.15",
          "form": "저번주했던 좀비 재밌었어요",
          "original_form": "저번주했던 좀비 재밌었어요",
          "speaker_id": "1",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100001021.1.16",
          "form": "앗 다시보기로 한번봐야겠네요",
          "original_form": "앗 다시보기로 한번봐야겠네요",
