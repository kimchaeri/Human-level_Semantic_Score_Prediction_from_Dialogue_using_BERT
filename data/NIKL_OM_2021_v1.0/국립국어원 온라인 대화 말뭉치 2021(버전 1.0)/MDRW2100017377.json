      "utterance": [
        {
          "id": "MDRW2100017377.1.1",
          "form": "하늘만 계속 시커멓네 ㅋㅋ",
          "original_form": "하늘만 계속 시커멓네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017377.1.2",
          "form": "금방이라도 쏟아질 것 같은데 비는 왜 안오는지",
          "original_form": "금방이라도 쏟아질 것 같은데 비는 왜 안오는지",
          "speaker_id": "1",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017377.1.3",
          "form": "훌.. 더 오를 습도가 없는데도 계속 오르네",
          "original_form": "훌.. 더 오를 습도가 없는데도 계속 오르네",
          "speaker_id": "2",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017377.1.4",
          "form": "집안 습도 86프로",
          "original_form": "집안 습도 86프로",
          "speaker_id": "1",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017377.1.5",
          "form": "며칠 전이랑 비슷하네",
          "original_form": "며칠 전이랑 비슷하네",
          "speaker_id": "2",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017377.1.6",
          "form": "집안에 사용할 수건이 없어",
          "original_form": "집안에 사용할 수건이 없어",
          "speaker_id": "1",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017377.1.7",
          "form": "너무 안마르면 건조기 돌려야지 뭐",
          "original_form": "너무 안마르면 건조기 돌려야지 뭐",
          "speaker_id": "2",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017377.1.8",
          "form": "ㅠㅠ 빨래가 진짜 너무 안말라",
          "original_form": "ㅠㅠ 빨래가 진짜 너무 안말라",
          "speaker_id": "1",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017377.1.9",
          "form": "가을이라 건조해지나 했더니 습도 폭탄이네",
          "original_form": "가을이라 건조해지나 했더니 습도 폭탄이네",
          "speaker_id": "2",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017377.1.10",
          "form": "이 정도 습도면 저녁에 또 보일러 돌리게 생김",
          "original_form": "이 정도 습도면 저녁에 또 보일러 돌리게 생김",
          "speaker_id": "1",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017377.1.11",
          "form": "바람 불어서 시원한데 이불은 축축 ㅠ",
          "original_form": "바람 불어서 시원한데 이불은 축축 ㅠ",
          "speaker_id": "2",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017377.1.12",
          "form": "이 정도 습도는 물먹는 하마도 소용없어",
          "original_form": "이 정도 습도는 물먹는 하마도 소용없어",
          "speaker_id": "1",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017377.1.13",
          "form": "물먹는 하마는 무슨.. 우리가 물 속에 살고 있구만 ㅋ",
          "original_form": "물먹는 하마는 무슨.. 우리가 물 속에 살고 있구만 ㅋ",
          "speaker_id": "2",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017377.1.14",
          "form": "너무 습해서 방금 에어컨 틀었어",
          "original_form": "너무 습해서 방금 에어컨 틀었어",
