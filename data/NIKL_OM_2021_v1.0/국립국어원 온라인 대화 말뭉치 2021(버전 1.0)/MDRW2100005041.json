      "utterance": [
        {
          "id": "MDRW2100005041.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 20:08"
        },
        {
          "id": "MDRW2100005041.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210821 20:11"
        },
        {
          "id": "MDRW2100005041.1.3",
          "form": "오늘 비가와서 많이 습하네요",
          "original_form": "오늘 비가와서 많이 습하네요",
          "speaker_id": "2",
          "time": "20210821 20:11"
        },
        {
          "id": "MDRW2100005041.1.4",
          "form": "빨래에 냄새 나네요",
          "original_form": "빨래에 냄새 나네요",
          "speaker_id": "1",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100005041.1.5",
          "form": "정말 비오면 빨래가 문제예요",
          "original_form": "정말 비오면 빨래가 문제예요",
          "speaker_id": "1",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100005041.1.6",
          "form": "아 그거 너무 싫어요",
          "original_form": "아 그거 너무 싫어요",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005041.1.7",
          "form": "제습기 빵빵하게 돌려야해요",
          "original_form": "제습기 빵빵하게 돌려야해요",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005041.1.8",
          "form": "전 비오는데 멀리 갔다왔는데 운전할때 앞이 안보이더라구요",
          "original_form": "전 비오는데 멀리 갔다왔는데 운전할때 앞이 안보이더라구요",
          "speaker_id": "1",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005041.1.9",
          "form": "네 제습기 틀어야죠",
          "original_form": "네 제습기 틀어야죠",
          "speaker_id": "1",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005041.1.10",
          "form": "그정도로 많이왔었나요?",
          "original_form": "그정도로 많이왔었나요?",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005041.1.11",
          "form": "요즘 날씨가 동남아 같아요 스콜처럼",
          "original_form": "요즘 날씨가 동남아 같아요 스콜처럼",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005041.1.12",
          "form": "네 앞이 안보일 정도였어요",
          "original_form": "네 앞이 안보일 정도였어요",
          "speaker_id": "1",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005041.1.13",
          "form": "네 정말 넘 습해요",
          "original_form": "네 정말 넘 습해요",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005041.1.14",
          "form": "저 곱슬인데 습도 높고 심한날 머리가 파마 한거 같이돼요",
          "original_form": "저 곱슬인데 습도 높고 심한날 머리가 파마 한거 같이돼요",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005041.1.15",
          "form": "올 겨울만이라도 덜 추우면 좋겠어요",
          "original_form": "올 겨울만이라도 덜 추우면 좋겠어요",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005041.1.16",
          "form": "그래도 여름 다가서 시원해서 좋아요 ㅎㅎ",
          "original_form": "그래도 여름 다가서 시원해서 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100005041.1.17",
          "form": "저두 곱슬이라서 장마철 정말 싫어해요 ㅜ",
          "original_form": "저두 곱슬이라서 장마철 정말 싫어해요 ㅜ",
          "speaker_id": "2",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100005041.1.18",
          "form": "그래서 여름보다 겨울이 더 좋더라구요!",
          "original_form": "그래서 여름보다 겨울이 더 좋더라구요!",
          "speaker_id": "2",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100005041.1.19",
          "form": "그죠 진짜 머리가 부시시해요",
          "original_form": "그죠 진짜 머리가 부시시해요",
          "speaker_id": "1",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100005041.1.20",
          "form": "날씨가 확실히 시원해졌죠 ㅎㅎ",
          "original_form": "날씨가 확실히 시원해졌죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100005041.1.21",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100005041.1.22",
          "form": "네 밤엔 춥더라구요",
          "original_form": "네 밤엔 춥더라구요",
          "speaker_id": "1",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100005041.1.23",
          "form": "감기 걸리기 딱좋은 계절이예요",
          "original_form": "감기 걸리기 딱좋은 계절이예요",
          "speaker_id": "1",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100005041.1.24",
          "form": "ㅋㅋㅋ 새벽에 넘 시원해서 좋더라구요",
          "original_form": "ㅋㅋㅋ 새벽에 넘 시원해서 좋더라구요",
          "speaker_id": "2",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100005041.1.25",
          "form": "생각해보니 가을은 추석이 있어 별로네요 ㅋㅋ",
          "original_form": "생각해보니 가을은 추석이 있어 별로네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100005041.1.26",
          "form": "바람이 많이불어요",
          "original_form": "바람이 많이불어요",
          "speaker_id": "2",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100005041.1.27",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100005041.1.28",
          "form": "그래서 더 시원해요 ㅎㅎ",
          "original_form": "그래서 더 시원해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100005041.1.29",
          "form": "장마라는데 낼도 비가 오나 모르겠어요",
          "original_form": "장마라는데 낼도 비가 오나 모르겠어요",
          "speaker_id": "1",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100005041.1.30",
          "form": "비가 오면 신발이 젖어서 찝찝해요",
          "original_form": "비가 오면 신발이 젖어서 찝찝해요",
          "speaker_id": "1",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100005041.1.31",
          "form": "당분간 비가 쭉 오는거같더라구요",
          "original_form": "당분간 비가 쭉 오는거같더라구요",
          "speaker_id": "2",
          "time": "20210821 20:17"
        }
      ]
    }
