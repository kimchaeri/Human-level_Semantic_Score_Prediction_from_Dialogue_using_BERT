      "utterance": [
        {
          "id": "MDRW2100032700.1.1",
          "form": "걔 동생이 엄마한테 그 언니 부담스럽게 왜 그래 나라도 엄청 부담스럽겠다",
          "original_form": "걔 동생이 엄마한테 그 언니 부담스럽게 왜 그래 나라도 엄청 부담스럽겠다",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100032700.1.2",
          "form": "해서 잘 빠져나왔다고 하더라고요",
          "original_form": "해서 잘 빠져나왔다고 하더라고요",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032700.1.3",
          "form": "시누이가 괜찮네요",
          "original_form": "시누이가 괜찮네요",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032700.1.4",
          "form": "제 생각엔",
          "original_form": "제 생각엔",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032700.1.5",
          "form": "시누이라뇨",
          "original_form": "시누이라뇨",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032700.1.6",
          "form": "조만간 남친이 또 그 얘기를 꺼낼 거 같아요",
          "original_form": "조만간 남친이 또 그 얘기를 꺼낼 거 같아요",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032700.1.7",
          "form": "시누됴",
          "original_form": "시누됴",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032700.1.8",
          "form": "시누죠",
          "original_form": "시누죠",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032700.1.9",
          "form": "네 기회 있으면 늘 던져요",
          "original_form": "네 기회 있으면 늘 던져요",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032700.1.10",
          "form": "어멋",
          "original_form": "어멋",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032700.1.11",
          "form": "부모님이 맨날 제 얘기만 한다면사",
          "original_form": "부모님이 맨날 제 얘기만 한다면사",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032700.1.12",
          "form": "100일 안에 결혼하시겠네요",
          "original_form": "100일 안에 결혼하시겠네요",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032700.1.13",
          "form": "며칠 사귀셨죠?",
          "original_form": "며칠 사귀셨죠?",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032700.1.14",
          "form": "무슨 소리세요",
          "original_form": "무슨 소리세요",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032700.1.15",
          "form": "저 아직 학생인데",
          "original_form": "저 아직 학생인데",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032700.1.16",
          "form": "64일이요",
          "original_form": "64일이요",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032700.1.17",
          "form": "오래 사귀시긴 했네요",
          "original_form": "오래 사귀시긴 했네요",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032700.1.18",
          "form": "결혼 얘기 나올 때죠",
          "original_form": "결혼 얘기 나올 때죠",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032700.1.19",
          "form": "오래요??",
          "original_form": "오래요??",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032700.1.20",
          "form": "학생이 뭐 어때서요",
          "original_form": "학생이 뭐 어때서요",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032700.1.21",
          "form": "꼴랑 64일이요?",
          "original_form": "꼴랑 64일이요?",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032700.1.22",
          "form": "학생은 가난하자나요",
          "original_form": "학생은 가난하자나요",
          "speaker_id": "2",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032700.1.23",
          "form": "원룸 신혼집 해도 되죠",
          "original_form": "원룸 신혼집 해도 되죠",
          "speaker_id": "1",
          "time": "20210905 22:33"
        }
