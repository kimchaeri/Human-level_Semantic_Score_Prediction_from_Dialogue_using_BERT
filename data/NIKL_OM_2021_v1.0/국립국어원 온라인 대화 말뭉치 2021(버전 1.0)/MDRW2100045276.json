      "utterance": [
        {
          "id": "MDRW2100045276.1.1",
          "form": "거긴뭐야?",
          "original_form": "거긴뭐야?",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045276.1.2",
          "form": "곤트란쉐리에모름?ㅆ",
          "original_form": "곤트란쉐리에모름?ㅆ",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045276.1.3",
          "form": "빵집",
          "original_form": "빵집",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045276.1.4",
          "form": "몰라ㅋㅋ어디있는거야?",
          "original_form": "몰라ㅋㅋ어디있는거야?",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045276.1.5",
          "form": "그 삼산에 무슨 테라스 있잖아",
          "original_form": "그 삼산에 무슨 테라스 있잖아",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045276.1.6",
          "form": "가든테라스..?",
          "original_form": "가든테라스..?",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045276.1.7",
          "form": "응응 거기에있어?",
          "original_form": "응응 거기에있어?",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045276.1.8",
          "form": "거기1층에 있었는데....",
          "original_form": "거기1층에 있었는데....",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045276.1.9",
          "form": "사라짐...",
          "original_form": "사라짐...",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045276.1.10",
          "form": "아 이런",
          "original_form": "아 이런",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045276.1.11",
          "form": "거기가 그렇게 존맛이라던데...",
          "original_form": "거기가 그렇게 존맛이라던데...",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045276.1.12",
          "form": "왜사라짐...",
          "original_form": "왜사라짐...",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045276.1.13",
          "form": "윤정이가 그 쪽에서 일했었거든",
          "original_form": "윤정이가 그 쪽에서 일했었거든",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045276.1.14",
          "form": "진짜맛있다그랬어",
          "original_form": "진짜맛있다그랬어",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045276.1.15",
          "form": "거기 위치가 좀 그렇다",
          "original_form": "거기 위치가 좀 그렇다",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045276.1.16",
          "form": "나도 먹고싶었던 빵집이 사라짐 옛날에 사라진듯",
          "original_form": "나도 먹고싶었던 빵집이 사라짐 옛날에 사라진듯",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045276.1.17",
          "form": "어딘데 ㅋㅋㅋㅋㅋ",
          "original_form": "어딘데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045276.1.18",
          "form": "무슨빵집인데",
          "original_form": "무슨빵집인데",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045276.1.19",
          "form": "대학병원 아마데우스ㅋㅋ",
          "original_form": "대학병원 아마데우스ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045276.1.20",
          "form": "엥 예술관에있는거아니여?",
          "original_form": "엥 예술관에있는거아니여?",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045276.1.21",
          "form": "그거없어짐?",
          "original_form": "그거없어짐?",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045276.1.22",
          "form": "없어짐...ㅜㅜㅜㅜ찾아봤는데 울산에 안나와ㅜㅜ",
          "original_form": "없어짐...ㅜㅜㅜㅜ찾아봤는데 울산에 안나와ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045276.1.23",
          "form": "코로나전에분명갔어는데",
          "original_form": "코로나전에분명갔어는데",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045276.1.24",
          "form": "내가찾아봤는데 안나오더라ㅠㅣ",
          "original_form": "내가찾아봤는데 안나오더라ㅠㅣ",
