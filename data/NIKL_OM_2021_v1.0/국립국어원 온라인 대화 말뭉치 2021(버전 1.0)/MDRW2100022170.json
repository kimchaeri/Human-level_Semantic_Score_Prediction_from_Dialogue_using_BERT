          "form": "의경?",
          "original_form": "의경?",
          "speaker_id": "2",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100022170.1.4",
          "form": "다녀왓어요",
          "original_form": "다녀왓어요",
          "speaker_id": "2",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100022170.1.5",
          "form": "아하 좋은곳 다녀왓네요",
          "original_form": "아하 좋은곳 다녀왓네요",
          "speaker_id": "1",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100022170.1.6",
          "form": "좋앗다고그랫어요",
          "original_form": "좋앗다고그랫어요",
          "speaker_id": "2",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100022170.1.7",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100022170.1.8",
          "form": "남자 군필 vs 미필",
          "original_form": "남자 군필 vs 미필",
          "speaker_id": "1",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100022170.1.9",
          "form": "닥 군필",
          "original_form": "닥 군필",
          "speaker_id": "2",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100022170.1.10",
          "form": "다녀오셧나여",
          "original_form": "다녀오셧나여",
          "speaker_id": "2",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100022170.1.11",
          "form": "저는 다녀왓죠 오지에서 군생활했습니다",
          "original_form": "저는 다녀왓죠 오지에서 군생활했습니다",
          "speaker_id": "1",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100022170.1.12",
          "form": "간첩도 잡고했습니다",
          "original_form": "간첩도 잡고했습니다",
          "speaker_id": "1",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100022170.1.13",
          "form": "우와ㅠㅠ",
          "original_form": "우와ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022170.1.14",
          "form": "머 특수부대이런거에여?",
          "original_form": "머 특수부대이런거에여?",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022170.1.15",
          "form": "지오피",
          "original_form": "지오피",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022170.1.16",
          "form": "이렁거",
          "original_form": "이렁거",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022170.1.17",
          "form": "네 특수부대요",
          "original_form": "네 특수부대요",
          "speaker_id": "1",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022170.1.18",
          "form": "낙하산타고",
          "original_form": "낙하산타고",
          "speaker_id": "1",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022170.1.19",
          "form": "토끼잡고",
          "original_form": "토끼잡고",
          "speaker_id": "1",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022170.1.20",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022170.1.21",
          "form": "특수부대머에여",
          "original_form": "특수부대머에여",
          "speaker_id": "2",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022170.1.22",
          "form": "강철부대에 나온곳?",
          "original_form": "강철부대에 나온곳?",
          "speaker_id": "2",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022170.1.23",
          "form": "박군알죠?",
          "original_form": "박군알죠?",
          "speaker_id": "1",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022170.1.24",
          "form": "특전사",
          "original_form": "특전사",
          "speaker_id": "2",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022170.1.25",
          "form": "네 그거요 ㅋㅋ",
