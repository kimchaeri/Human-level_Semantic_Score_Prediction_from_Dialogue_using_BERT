      "utterance": [
        {
          "id": "MDRW2100017951.1.1",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017951.1.2",
          "form": "갈비요!",
          "original_form": "갈비요!",
          "speaker_id": "2",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100017951.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100017951.1.4",
          "form": "오 저도 갈비 좋아해요",
          "original_form": "오 저도 갈비 좋아해요",
          "speaker_id": "1",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017951.1.5",
          "form": "저는 양념갈비 좋아해요",
          "original_form": "저는 양념갈비 좋아해요",
          "speaker_id": "1",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017951.1.6",
          "form": "오 통했네요",
          "original_form": "오 통했네요",
          "speaker_id": "2",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017951.1.7",
          "form": "저는 간장이요!",
          "original_form": "저는 간장이요!",
          "speaker_id": "2",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017951.1.8",
          "form": "ㅋㅋㅋ 그거만 있으면 무한대로 흡입가능하죠",
          "original_form": "ㅋㅋㅋ 그거만 있으면 무한대로 흡입가능하죠",
          "speaker_id": "1",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017951.1.9",
          "form": "저도 간장이요 ㅋㅋㅋ달달하고 맛있어요",
          "original_form": "저도 간장이요 ㅋㅋㅋ달달하고 맛있어요",
          "speaker_id": "1",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017951.1.10",
          "form": "맞아요ㅋㅋ 진짜 많이 먹고싶을때는 무한리필 가능한 곳 가요",
          "original_form": "맞아요ㅋㅋ 진짜 많이 먹고싶을때는 무한리필 가능한 곳 가요",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017951.1.11",
          "form": "양념도 너무 맛있어요",
          "original_form": "양념도 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017951.1.12",
          "form": "오 저두요 ㅋㅋㅋㅋ 무한리필도 좋더라구요",
          "original_form": "오 저두요 ㅋㅋㅋㅋ 무한리필도 좋더라구요",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017951.1.13",
          "form": "고기는 전..배가 안부르더라구요",
          "original_form": "고기는 전..배가 안부르더라구요",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017951.1.14",
          "form": "앗! 저도..ㅋㅋㅋ",
          "original_form": "앗! 저도..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017951.1.15",
          "form": "넣어도 넣어도 끝이 없는 기분이에요",
          "original_form": "넣어도 넣어도 끝이 없는 기분이에요",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017951.1.16",
          "form": "계속 먹고싶어요ㅋㅋㄲ",
          "original_form": "계속 먹고싶어요ㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017951.1.17",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017951.1.18",
          "form": "쌈무에 먹어도 맛있어요 드셔보셨어요?",
          "original_form": "쌈무에 먹어도 맛있어요 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017951.1.19",
          "form": "네! 역시 먹는걸 즐기시는 분이시네요",
          "original_form": "네! 역시 먹는걸 즐기시는 분이시네요",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017951.1.20",
          "form": "먹는 낙으로 살죠 ㅋㅋㅋ",
          "original_form": "먹는 낙으로 살죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017951.1.21",
          "form": "약간 질리나? 싶을때 먹음 그리 입맛이 다시 돌더라구요",
          "original_form": "약간 질리나? 싶을때 먹음 그리 입맛이 다시 돌더라구요",
          "speaker_id": "2",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100017951.1.22",
          "form": "맞아요 달달해서 질릴 수 있는데 쌈무가 잡아줘요 ㅋㅋㅋ",
          "original_form": "맞아요 달달해서 질릴 수 있는데 쌈무가 잡아줘요 ㅋㅋㅋ",
          "speaker_id": "1",
