        },
        {
          "id": "MDRW2100031314.1.2",
          "form": "나이 차이가 있나요",
          "original_form": "나이 차이가 있나요",
          "speaker_id": "1",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100031314.1.3",
          "form": "다섯 살요",
          "original_form": "다섯 살요",
          "speaker_id": "2",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100031314.1.4",
          "form": "사이가 좋나요",
          "original_form": "사이가 좋나요",
          "speaker_id": "1",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100031314.1.5",
          "form": "어릴 땐 안 그러더니 커서 사이가 좋네요",
          "original_form": "어릴 땐 안 그러더니 커서 사이가 좋네요",
          "speaker_id": "2",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100031314.1.6",
          "form": "그런 거 같아요 딸은 크면 친구라잖아요",
          "original_form": "그런 거 같아요 딸은 크면 친구라잖아요",
          "speaker_id": "1",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100031314.1.7",
          "form": "맞아요 것도 좋은 친구",
          "original_form": "맞아요 것도 좋은 친구",
          "speaker_id": "2",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100031314.1.8",
          "form": "딸과 같이 사시나요",
          "original_form": "딸과 같이 사시나요",
          "speaker_id": "1",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100031314.1.9",
          "form": "내일 같이 살고 있습니다",
          "original_form": "내일 같이 살고 있습니다",
          "speaker_id": "2",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100031314.1.10",
          "form": "자주 싸우지는 않으시나요",
          "original_form": "자주 싸우지는 않으시나요",
          "speaker_id": "1",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100031314.1.11",
          "form": "싸우는게 일입니다",
          "original_form": "싸우는게 일입니다",
          "speaker_id": "2",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100031314.1.12",
          "form": "웃기네요 재밌네요",
          "original_form": "웃기네요 재밌네요",
          "speaker_id": "1",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100031314.1.13",
          "form": "덕분에 사는 재미가 있습니다",
          "original_form": "덕분에 사는 재미가 있습니다",
