      "utterance": [
        {
          "id": "MDRW2100034861.1.1",
          "form": "피부 관리 받아보셨나요",
          "original_form": "피부 관리 받아보셨나요",
          "speaker_id": "2",
          "time": "20210829 06:17"
        },
        {
          "id": "MDRW2100034861.1.2",
          "form": "아뇨 name2님은요?",
          "original_form": "아뇨 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 06:56"
        },
        {
          "id": "MDRW2100034861.1.3",
          "form": "저두요 기미때문에 고민중이예요",
          "original_form": "저두요 기미때문에 고민중이예요",
          "speaker_id": "2",
          "time": "20210829 06:57"
        },
        {
          "id": "MDRW2100034861.1.4",
          "form": "운동은 따로 하시나요?",
          "original_form": "운동은 따로 하시나요?",
          "speaker_id": "1",
          "time": "20210829 09:08"
        },
        {
          "id": "MDRW2100034861.1.5",
          "form": "음 필라테스 해요",
          "original_form": "음 필라테스 해요",
          "speaker_id": "2",
          "time": "20210829 09:09"
        },
        {
          "id": "MDRW2100034861.1.6",
          "form": "성형열풍에 대해 어찌생각하세요?",
          "original_form": "성형열풍에 대해 어찌생각하세요?",
          "speaker_id": "1",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100034861.1.7",
          "form": "본인 만족이니 주변에서 관여할바는 없다고 생각해요",
          "original_form": "본인 만족이니 주변에서 관여할바는 없다고 생각해요",
          "speaker_id": "2",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100034861.1.8",
          "form": "저는 갑자기 예뻐진 다음 공주 코스프레 하는애들 보면 짜증나요",
          "original_form": "저는 갑자기 예뻐진 다음 공주 코스프레 하는애들 보면 짜증나요",
          "speaker_id": "1",
          "time": "20210829 15:08"
        },
        {
          "id": "MDRW2100034861.1.9",
          "form": "아 그런가요^^ 전 주변에 그런 케이스가 없어서 몰랐어요",
          "original_form": "아 그런가요^^ 전 주변에 그런 케이스가 없어서 몰랐어요",
          "speaker_id": "2",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100034861.1.10",
          "form": "대학때 친구가 그랬어요",
          "original_form": "대학때 친구가 그랬어요",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100034861.1.11",
          "form": "그 한명을 보고나니 그뒤에 비슷한 유형보면 알아봐요 ㅜ",
          "original_form": "그 한명을 보고나니 그뒤에 비슷한 유형보면 알아봐요 ㅜ",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100034861.1.12",
          "form": "아 그렇군요 눈에 띄게 예뻐졌나요?",
          "original_form": "아 그렇군요 눈에 띄게 예뻐졌나요?",
          "speaker_id": "2",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100034861.1.13",
          "form": "넹 그런편이에요",
          "original_form": "넹 그런편이에요",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100034861.1.14",
          "form": "예전 얼굴로는 할 수 없는 이상한 공주목소리에 공주행동을 해서 꼴불견이었어요",
          "original_form": "예전 얼굴로는 할 수 없는 이상한 공주목소리에 공주행동을 해서 꼴불견이었어요",
          "speaker_id": "1",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100034861.1.15",
          "form": "ㅋㅋㅋ 그래도 예뻐지는거 부럽긴 해요..",
          "original_form": "ㅋㅋㅋ 그래도 예뻐지는거 부럽긴 해요..",
