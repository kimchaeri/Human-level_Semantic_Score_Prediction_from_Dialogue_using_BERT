      "utterance": [
        {
          "id": "MDRW2100022202.1.1",
          "form": "주말에 비소식 있었는데",
          "original_form": "주말에 비소식 있었는데",
          "speaker_id": "2",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022202.1.2",
          "form": "어제 날씨 좋지않았어요?",
          "original_form": "어제 날씨 좋지않았어요?",
          "speaker_id": "2",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022202.1.3",
          "form": "네 비 안오고 맑네요",
          "original_form": "네 비 안오고 맑네요",
          "speaker_id": "1",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022202.1.4",
          "form": "오늘도요",
          "original_form": "오늘도요",
          "speaker_id": "1",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022202.1.5",
          "form": "이제 진짜 가을이 오나 싶어요",
          "original_form": "이제 진짜 가을이 오나 싶어요",
          "speaker_id": "2",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022202.1.6",
          "form": "그렇지만 에어컨은 포기 못하고..",
          "original_form": "그렇지만 에어컨은 포기 못하고..",
          "speaker_id": "2",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022202.1.7",
          "form": "아 습도 때문이라도 잠깐씩 켜게 되더라고요",
          "original_form": "아 습도 때문이라도 잠깐씩 켜게 되더라고요",
          "speaker_id": "1",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022202.1.8",
          "form": "근데 또 오래 틀면 추워요",
          "original_form": "근데 또 오래 틀면 추워요",
          "speaker_id": "1",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022202.1.9",
          "form": "더위 많이 안 타는 편인가봐요~",
          "original_form": "더위 많이 안 타는 편인가봐요~",
          "speaker_id": "2",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022202.1.10",
          "form": "저는 심해서 겨울에도 반팔입어요ㅜ",
          "original_form": "저는 심해서 겨울에도 반팔입어요ㅜ",
          "speaker_id": "2",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022202.1.11",
          "form": "아 전 추위도 많이 타서ㅜ",
          "original_form": "아 전 추위도 많이 타서ㅜ",
          "speaker_id": "1",
          "time": "20210829 11:00"
        },
        {
          "id": "MDRW2100022202.1.12",
          "form": "요즘 날씨에서",
          "original_form": "요즘 날씨에서",
          "speaker_id": "2",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022202.1.13",
          "form": "갑자기 확 쌀쌀해지잖아요",
          "original_form": "갑자기 확 쌀쌀해지잖아요",
          "speaker_id": "2",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022202.1.14",
          "form": "외투 들고 다니셔야겠어요",
          "original_form": "외투 들고 다니셔야겠어요",
          "speaker_id": "2",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022202.1.15",
          "form": "네네 안그래도 가디건 챙겨다녀요",
          "original_form": "네네 안그래도 가디건 챙겨다녀요",
          "speaker_id": "1",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100022202.1.16",
          "form": "그럼 요즘은 긴팔 입으시나요??",
          "original_form": "그럼 요즘은 긴팔 입으시나요??",
          "speaker_id": "2",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100022202.1.17",
          "form": "7부 정도요ㅎㅎ",
          "original_form": "7부 정도요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022202.1.18",
          "form": "더위도 타는 편이라 긴팔은ㅠ",
          "original_form": "더위도 타는 편이라 긴팔은ㅠ",
          "speaker_id": "1",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022202.1.19",
          "form": "전 긴팔옷 하나도 없어요ㅋㅋㅋㅋ",
          "original_form": "전 긴팔옷 하나도 없어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022202.1.20",
          "form": "다 반팔",
          "original_form": "다 반팔",
          "speaker_id": "2",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022202.1.21",
          "form": "근데 친구들이 하도 보기만하도 춥다그래서",
          "original_form": "근데 친구들이 하도 보기만하도 춥다그래서",
          "speaker_id": "2",
          "time": "20210829 11:09"
        },
        {
          "id": "MDRW2100022202.1.22",
          "form": "오 정말 열이 많으신가보다",
          "original_form": "오 정말 열이 많으신가보다",
          "speaker_id": "1",
          "time": "20210829 11:09"
        },
        {
          "id": "MDRW2100022202.1.23",
          "form": "얇은 가디건류 하나로 겨울 버텨요ㅋㅋ",
          "original_form": "얇은 가디건류 하나로 겨울 버텨요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:09"
        },
        {
          "id": "MDRW2100022202.1.24",
          "form": "겨울에도 그럼 반팔에 외투 걸치시는 거예요?",
          "original_form": "겨울에도 그럼 반팔에 외투 걸치시는 거예요?",
          "speaker_id": "1",
          "time": "20210829 11:09"
        },
        {
          "id": "MDRW2100022202.1.25",
          "form": "네ㅋㅋㅋ그 흔한 롱패딩도 없어요",
          "original_form": "네ㅋㅋㅋ그 흔한 롱패딩도 없어요",
          "speaker_id": "2",
          "time": "20210829 11:09"
        }
      ]
    }
  ]
