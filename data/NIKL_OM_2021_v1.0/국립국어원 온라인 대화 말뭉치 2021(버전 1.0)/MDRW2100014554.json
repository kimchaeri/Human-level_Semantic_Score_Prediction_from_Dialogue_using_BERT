      "utterance": [
        {
          "id": "MDRW2100014554.1.1",
          "form": "요즘 재택하세요?",
          "original_form": "요즘 재택하세요?",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014554.1.2",
          "form": "저는 병원에서 근무라ㅜ 재택 꿈같은 이야기입니다 흑흑",
          "original_form": "저는 병원에서 근무라ㅜ 재택 꿈같은 이야기입니다 흑흑",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014554.1.3",
          "form": "재택근무하고싶어요",
          "original_form": "재택근무하고싶어요",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014554.1.4",
          "form": "name2 님 고생하시는구나 ㅠㅠㅠㅠ",
          "original_form": "&name2& 님 고생하시는구나 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014554.1.5",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014554.1.6",
          "form": "그래도 근무환경이 좋은편이라 고생은아니여요",
          "original_form": "그래도 근무환경이 좋은편이라 고생은아니여요",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014554.1.7",
          "form": "진짜 주변에보면 고생하시는분들은 따로 있지요ㅜㅜ",
          "original_form": "진짜 주변에보면 고생하시는분들은 따로 있지요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014554.1.8",
          "form": "다행이에요!",
          "original_form": "다행이에요!",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014554.1.9",
          "form": "재택중이세요?",
          "original_form": "재택중이세요?",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014554.1.10",
          "form": "의료진들이 너무 고생이셔서",
          "original_form": "의료진들이 너무 고생이셔서",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014554.1.11",
          "form": "네 저는 재택이요!",
          "original_form": "네 저는 재택이요!",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014554.1.12",
          "form": "우와ㅜㅜ",
          "original_form": "우와ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014554.1.13",
          "form": "이제 재택이 익숙해져서 다시 못 돌아갈 것 같아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이제 재택이 익숙해져서 다시 못 돌아갈 것 같아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014554.1.14",
          "form": "크ㅜㅜㅜㅜ 부러워요 ㅎㅎ",
          "original_form": "크ㅜㅜㅜㅜ 부러워요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014554.1.15",
          "form": "회식도 없어지고",
          "original_form": "회식도 없어지고",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014554.1.16",
          "form": "회의도 줄고요!",
          "original_form": "회의도 줄고요!",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014554.1.17",
          "form": "코시국 유일한 최고 장점이 회식....",
          "original_form": "코시국 유일한 최고 장점이 회식....",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014554.1.18",
          "form": "크",
          "original_form": "크",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014554.1.19",
          "form": "진짜 진짜 진짜 좋아요",
          "original_form": "진짜 진짜 진짜 좋아요",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014554.1.20",
          "form": "회식 정말 싫었어요ㅜㅜ",
          "original_form": "회식 정말 싫었어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014554.1.21",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014554.1.22",
          "form": "어우 회식은 잘 없어졌어요 정말",
          "original_form": "어우 회식은 잘 없어졌어요 정말",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014554.1.23",
          "form": "부활해도 안 갈 거예요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "부활해도 안 갈 거예요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014554.1.24",
          "form": "요즘은 업무 스트레스 없으세요?",
          "original_form": "요즘은 업무 스트레스 없으세요?",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014554.1.25",
          "form": "일이 많아져서 스트레스는 있는데..",
          "original_form": "일이 많아져서 스트레스는 있는데..",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014554.1.26",
          "form": "그래도 이 시국에 일하는 게 다행인것 같아서",
          "original_form": "그래도 이 시국에 일하는 게 다행인것 같아서",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014554.1.27",
          "form": "꾹꾹참고 있어요",
          "original_form": "꾹꾹참고 있어요",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014554.1.28",
          "form": "ㅜㅜ그렇긴하지만",
          "original_form": "ㅜㅜ그렇긴하지만",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014554.1.29",
          "form": "참는게능사는아닌데",
          "original_form": "참는게능사는아닌데",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014554.1.30",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014554.1.31",
          "form": "꾹꾹참는다는게 넘 안타깝네요",
          "original_form": "꾹꾹참는다는게 넘 안타깝네요",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014554.1.32",
          "form": "힘내세요",
          "original_form": "힘내세요",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014554.1.33",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014554.1.34",
          "form": "네! 그래서 퇴근하고 사는 삶에 충실하고자ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네! 그래서 퇴근하고 사는 삶에 충실하고자ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014554.1.35",
          "form": "이것저것 배우려고 하고 있어요",
          "original_form": "이것저것 배우려고 하고 있어요",
          "speaker_id": "1",
