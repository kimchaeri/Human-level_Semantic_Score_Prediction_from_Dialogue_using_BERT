      "utterance": [
        {
          "id": "MDRW2100024657.1.1",
          "form": "둘째날에  일어나서  뭐먹었냐",
          "original_form": "둘째날에  일어나서  뭐먹었냐",
          "speaker_id": "2",
          "time": "20210831 16:44"
        },
        {
          "id": "MDRW2100024657.1.2",
          "form": "전날에 편의점에서 사 온 일본 컵라면 먹었던 것 같아",
          "original_form": "전날에 편의점에서 사 온 일본 컵라면 먹었던 것 같아",
          "speaker_id": "1",
          "time": "20210831 16:44"
        },
        {
          "id": "MDRW2100024657.1.3",
          "form": "일본여행한 목적 중 하나가",
          "original_form": "일본여행한 목적 중 하나가",
          "speaker_id": "1",
          "time": "20210831 16:44"
        },
        {
          "id": "MDRW2100024657.1.4",
          "form": "일본 컵라면이었어ㅋㅋㅋ",
          "original_form": "일본 컵라면이었어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:44"
        },
        {
          "id": "MDRW2100024657.1.5",
          "form": "ㅋ  ㅋ ㅋㅋㅋ참나 컵라며ㆍ 먹으러 여행갔냐",
          "original_form": "ㅋ  ㅋ ㅋㅋㅋ참나 컵라며ㆍ 먹으러 여행갔냐",
          "speaker_id": "2",
          "time": "20210831 16:45"
        },
        {
          "id": "MDRW2100024657.1.6",
          "form": "그래서 먹어보니 어때",
          "original_form": "그래서 먹어보니 어때",
          "speaker_id": "2",
          "time": "20210831 16:45"
        },
        {
          "id": "MDRW2100024657.1.7",
          "form": "한국 컵라면보다 종류가 다양해서 재밌게 먹었어",
          "original_form": "한국 컵라면보다 종류가 다양해서 재밌게 먹었어",
          "speaker_id": "1",
          "time": "20210831 16:45"
        },
        {
          "id": "MDRW2100024657.1.8",
          "form": "카레컵라면 맛있다",
          "original_form": "카레컵라면 맛있다",
          "speaker_id": "1",
          "time": "20210831 16:46"
        },
        {
          "id": "MDRW2100024657.1.9",
          "form": "다음에 나랑도 가자",
          "original_form": "다음에 나랑도 가자",
          "speaker_id": "2",
          "time": "20210831 16:46"
        },
        {
          "id": "MDRW2100024657.1.10",
          "form": "좋아 너랑 같이 하는 여행 대환영이야",
          "original_form": "좋아 너랑 같이 하는 여행 대환영이야",
          "speaker_id": "1",
          "time": "20210831 16:46"
        },
        {
          "id": "MDRW2100024657.1.11",
          "form": "아 컵라면중에",
          "original_form": "아 컵라면중에",
          "speaker_id": "1",
          "time": "20210831 16:46"
        },
        {
          "id": "MDRW2100024657.1.12",
          "form": "응 실패도 있었니?",
          "original_form": "응 실패도 있었니?",
          "speaker_id": "2",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100024657.1.13",
          "form": "파란뚜껑으로 된 튀김맛 컵라면도 있는데 진짜 제일 맛있었어",
          "original_form": "파란뚜껑으로 된 튀김맛 컵라면도 있는데 진짜 제일 맛있었어",
          "speaker_id": "1",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100024657.1.14",
          "form": "엥 설명 으로는 진짜   이상한대",
          "original_form": "엥 설명 으로는 진짜   이상한대",
          "speaker_id": "2",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100024657.1.15",
          "form": "컵라면먹고 뭐했어?",
          "original_form": "컵라면먹고 뭐했어?",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100024657.1.16",
          "form": "나는 실패한 음식이 따로 없었는데 친구는 오코노미야끼 겨자맛이 별로라더라",
          "original_form": "나는 실패한 음식이 따로 없었는데 친구는 오코노미야끼 겨자맛이 별로라더라",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100024657.1.17",
          "form": "컵라면 먹고 오사카 시장 엄청 돌아다녔어",
          "original_form": "컵라면 먹고 오사카 시장 엄청 돌아다녔어",
          "speaker_id": "1",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100024657.1.18",
          "form": "실패한음식이 없다니 나도 기대된다",
          "original_form": "실패한음식이 없다니 나도 기대된다",
          "speaker_id": "2",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100024657.1.19",
          "form": "짠맛이 걱정이긴해",
          "original_form": "짠맛이 걱정이긴해",
          "speaker_id": "2",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100024657.1.20",
          "form": "오사카시장에 뭐 다른게 있띠",
          "original_form": "오사카시장에 뭐 다른게 있띠",
          "speaker_id": "2",
          "time": "20210831 16:50"
        },
        {
          "id": "MDRW2100024657.1.21",
          "form": "관광지다 보니까 기념품가게가 엄청 많더라",
          "original_form": "관광지다 보니까 기념품가게가 엄청 많더라",
          "speaker_id": "1",
          "time": "20210831 16:50"
        },
        {
          "id": "MDRW2100024657.1.22",
          "form": "쓸데없는데 예쁜 물건이 많았어",
          "original_form": "쓸데없는데 예쁜 물건이 많았어",
          "speaker_id": "1",
          "time": "20210831 16:50"
        },
        {
          "id": "MDRW2100024657.1.23",
          "form": "한국에 잘 안 파는 게 있었던듯",
          "original_form": "한국에 잘 안 파는 게 있었던듯",
          "speaker_id": "1",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100024657.1.24",
          "form": "쓸데없지만 예쁜물건 1위",
          "original_form": "쓸데없지만 예쁜물건 1위",
          "speaker_id": "2",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100024657.1.25",
          "form": "젓가락 받침대",
          "original_form": "젓가락 받침대",
          "speaker_id": "2",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100024657.1.26",
          "form": "어때",
          "original_form": "어때",
          "speaker_id": "2",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100024657.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ어 맞아 수저받침대도 팔더라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ어 맞아 수저받침대도 팔더라",
          "speaker_id": "1",
          "time": "20210831 16:52"
        }
      ]
