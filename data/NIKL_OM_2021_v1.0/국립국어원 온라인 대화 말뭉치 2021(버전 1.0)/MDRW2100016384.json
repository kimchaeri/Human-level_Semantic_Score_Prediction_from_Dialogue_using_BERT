      "utterance": [
        {
          "id": "MDRW2100016384.1.1",
          "form": "고양이 키우고 싶다!",
          "original_form": "고양이 키우고 싶다!",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100016384.1.2",
          "form": "고양이 좋아하세요",
          "original_form": "고양이 좋아하세요",
          "speaker_id": "1",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100016384.1.3",
          "form": "네ㅜㅜ너무귀여워요",
          "original_form": "네ㅜㅜ너무귀여워요",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100016384.1.4",
          "form": "저도 고양이좋아해요",
          "original_form": "저도 고양이좋아해요",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100016384.1.5",
          "form": "키우고 계신가요?",
          "original_form": "키우고 계신가요?",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100016384.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100016384.1.7",
          "form": "헉 어떤 냥이 키우시나요?",
          "original_form": "헉 어떤 냥이 키우시나요?",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100016384.1.8",
          "form": "저는 렉돌이요",
          "original_form": "저는 렉돌이요",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100016384.1.9",
          "form": "ㅜㅜ 너무 귀엽겠어요",
          "original_form": "ㅜㅜ 너무 귀엽겠어요",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100016384.1.10",
          "form": "제 보물이에요",
          "original_form": "제 보물이에요",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100016384.1.11",
          "form": "돈 많이들지않나요?",
          "original_form": "돈 많이들지않나요?",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100016384.1.12",
          "form": "제가 덜먹으면 되요ㄲㅋㄱ",
          "original_form": "제가 덜먹으면 되요ㄲㅋㄱ",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100016384.1.13",
          "form": "앗...",
          "original_form": "앗...",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100016384.1.14",
          "form": "찐사랑이네요",
          "original_form": "찐사랑이네요",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100016384.1.15",
          "form": "그정도책임감은있어야 한다고 생각해요",
          "original_form": "그정도책임감은있어야 한다고 생각해요",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100016384.1.16",
          "form": "반려동물을 기른다면",
          "original_form": "반려동물을 기른다면",
          "speaker_id": "1",
