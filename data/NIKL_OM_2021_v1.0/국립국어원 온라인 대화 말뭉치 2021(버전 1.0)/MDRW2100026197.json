      "utterance": [
        {
          "id": "MDRW2100026197.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100026197.1.2",
          "form": "네~저희 햄스터  키워요~",
          "original_form": "네~저희 햄스터  키워요~",
          "speaker_id": "2",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100026197.1.3",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100026197.1.4",
          "form": "오 저는 키울 능력이 없다 생각해서 못키우고 있어요",
          "original_form": "오 저는 키울 능력이 없다 생각해서 못키우고 있어요",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026197.1.5",
          "form": "저 하나 건사하기가 쉽지 않거든요ㅋㅋ",
          "original_form": "저 하나 건사하기가 쉽지 않거든요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026197.1.6",
          "form": "저도 그랬어요ㅋㅋ",
          "original_form": "저도 그랬어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026197.1.7",
          "form": "햄스터 키우기 은근 까다롭다고 들었는데 어떤가요?",
          "original_form": "햄스터 키우기 은근 까다롭다고 들었는데 어떤가요?",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026197.1.8",
          "form": "저 어렸을 때도 햄스터 키웠었거든요~",
          "original_form": "저 어렸을 때도 햄스터 키웠었거든요~",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026197.1.9",
          "form": "그래서 크게 까다로운건 모르겠어요~",
          "original_form": "그래서 크게 까다로운건 모르겠어요~",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026197.1.10",
          "form": "아 그렇군요 햄스터 번식력은 남다르고 탈출도 잘한다고 들어서요",
          "original_form": "아 그렇군요 햄스터 번식력은 남다르고 탈출도 잘한다고 들어서요",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026197.1.11",
          "form": "아 햄스터는 한 마리씩 따로 키워야 해요~",
          "original_form": "아 햄스터는 한 마리씩 따로 키워야 해요~",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026197.1.12",
          "form": "탈출은 늘 시도 합니다ㅋㅋㅋ",
          "original_form": "탈출은 늘 시도 합니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026197.1.13",
          "form": "그걸 찾을 수있어요?\r\n그걸 감당 못할 것 같아요",
          "original_form": "그걸 찾을 수있어요?\r\n그걸 감당 못할 것 같아요",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100026197.1.14",
          "form": "오래 키우면 이름 부르면 나와요ㅋㅋ",
          "original_form": "오래 키우면 이름 부르면 나와요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026197.1.15",
          "form": "생각보다 영리한 아이랍니다~",
          "original_form": "생각보다 영리한 아이랍니다~",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026197.1.16",
          "form": "오 진짜요? 그런 이야기는 처음 들었어요",
          "original_form": "오 진짜요? 그런 이야기는 처음 들었어요",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026197.1.17",
          "form": "탈출을 잘하는 것도 똑똑해서 그런가보네요",
          "original_form": "탈출을 잘하는 것도 똑똑해서 그런가보네요",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026197.1.18",
          "form": "왠지 키워보고 싶네요",
          "original_form": "왠지 키워보고 싶네요",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026197.1.19",
          "form": "생각보다 냄새도 안 나고 키우기 괜찮아요~",
          "original_form": "생각보다 냄새도 안 나고 키우기 괜찮아요~",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026197.1.20",
          "form": "그리고 귀여워요ㅋㅋㅋ",
          "original_form": "그리고 귀여워요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026197.1.21",
          "form": "오 말도 알아듣는다는 것에서부터 끌리네요ㅋㅋ",
          "original_form": "오 말도 알아듣는다는 것에서부터 끌리네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:55"
        }
