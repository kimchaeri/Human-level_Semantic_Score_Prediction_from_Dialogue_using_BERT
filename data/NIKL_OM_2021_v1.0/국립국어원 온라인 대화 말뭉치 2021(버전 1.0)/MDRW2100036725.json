        {
          "id": "MDRW2100036725.1.1",
          "form": "내가 사주기 싫어서 할아버지한테 전화했음",
          "original_form": "내가 사주기 싫어서 할아버지한테 전화했음",
          "speaker_id": "2",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100036725.1.2",
          "form": "가격 저렴하네 ㅋㅋ",
          "original_form": "가격 저렴하네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100036725.1.3",
          "form": "울고 울고 있으니까",
          "original_form": "울고 울고 있으니까",
          "speaker_id": "2",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100036725.1.4",
          "form": "아아아 ㅋ나도 한번씩 그러는데 ㅋㅋ",
          "original_form": "아아아 ㅋ나도 한번씩 그러는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100036725.1.5",
          "form": "할아버지가 돈 줄 테니까 빨리 사 주라고",
          "original_form": "할아버지가 돈 줄 테니까 빨리 사 주라고",
          "speaker_id": "2",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100036725.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100036725.1.7",
          "form": "나는 몇십만원짜리 그 전동차얘기하는줄",
          "original_form": "나는 몇십만원짜리 그 전동차얘기하는줄",
          "speaker_id": "1",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100036725.1.8",
          "form": "웃겨 ㅋㅋㅋ할아버지가 사주신거네 ㅋㅋㅋ",
          "original_form": "웃겨 ㅋㅋㅋ할아버지가 사주신거네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100036725.1.9",
          "form": "잘됐다이 ㅋㅋㅋㅋㅋ",
          "original_form": "잘됐다이 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100036725.1.10",
          "form": "전에 하트 토이갔다가",
          "original_form": "전에 하트 토이갔다가",
          "speaker_id": "2",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100036725.1.11",
          "form": "우리도 요즘에 띠띠뽀붕붕차로 둘이 싸운다",
          "original_form": "우리도 요즘에 띠띠뽀붕붕차로 둘이 싸운다",
          "speaker_id": "1",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100036725.1.12",
          "form": "타요 지붕카 타고 안 내려서",
          "original_form": "타요 지붕카 타고 안 내려서",
          "speaker_id": "2",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100036725.1.13",
          "form": "웅웅 하트토이 ㅋㅋ",
          "original_form": "웅웅 하트토이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100036725.1.14",
          "form": "사장님 난감 ㅋㅋㅋㅋㅋ",
          "original_form": "사장님 난감 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100036725.1.15",
          "form": "할비가 이것도 달라고 ㅋㅋㅋㅋ",
          "original_form": "할비가 이것도 달라고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100036725.1.16",
          "form": "너무 비싸서 안된다카고 질질 끌고나왔음 ㅋㅋㅋ",
          "original_form": "너무 비싸서 안된다카고 질질 끌고나왔음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100036725.1.17",
          "form": "나도 양심은 있다캄서 ㅋㅋㅋ",
          "original_form": "나도 양심은 있다캄서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100036725.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아 웃겨 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아 웃겨 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100036725.1.19",
          "form": "우리할아버지는 애들한테 뭐 안주신다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "우리할아버지는 애들한테 뭐 안주신다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100036725.1.20",
          "form": "우린 친 할비가 잘 안 사주신다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "우린 친 할비가 잘 안 사주신다ㅋㅋㅋㅋㅋㅋ",
