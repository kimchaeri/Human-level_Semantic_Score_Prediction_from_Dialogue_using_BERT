      "utterance": [
        {
          "id": "MDRW2100021721.1.1",
          "form": "요즘같을때 유튜브없었음 어떻게 살았겠나 싶어요",
          "original_form": "요즘같을때 유튜브없었음 어떻게 살았겠나 싶어요",
          "speaker_id": "1",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021721.1.2",
          "form": "그쵸 ㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021721.1.3",
          "form": "전 출퇴근 시간도 길어서 꼭 봐야해요",
          "original_form": "전 출퇴근 시간도 길어서 꼭 봐야해요",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021721.1.4",
          "form": "ㅋㅋ출근길의 비타민이죠!",
          "original_form": "ㅋㅋ출근길의 비타민이죠!",
          "speaker_id": "1",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100021721.1.5",
          "form": "ㅋㅋㅋ구독하시는 분 있으세요??",
          "original_form": "ㅋㅋㅋ구독하시는 분 있으세요??",
          "speaker_id": "2",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021721.1.6",
          "form": "저 베이킹하고 홈트요! ㅎㅎ",
          "original_form": "저 베이킹하고 홈트요! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021721.1.7",
          "form": "저는 게임방송 쪼끔 봐서 그거랑 클래식 채널 몇 개랑 홈트 봐요!",
          "original_form": "저는 게임방송 쪼끔 봐서 그거랑 클래식 채널 몇 개랑 홈트 봐요!",
          "speaker_id": "2",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021721.1.8",
          "form": "홈트 뭐 보세요? 전 강하나님 다노언니 봐요 ㅋㅋ",
          "original_form": "홈트 뭐 보세요? 전 강하나님 다노언니 봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021721.1.9",
          "form": "아 저 다노 봤었어요! 지금은 빅씨스꺼 봐요 뷰가 좋아요",
          "original_form": "아 저 다노 봤었어요! 지금은 빅씨스꺼 봐요 뷰가 좋아요",
          "speaker_id": "1",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021721.1.10",
          "form": "동작 반복도 없고요",
          "original_form": "동작 반복도 없고요",
          "speaker_id": "1",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021721.1.11",
          "form": "앗 기억해놨다가 저도 봐야겠어요 ㅋㅋㅋㅋ",
          "original_form": "앗 기억해놨다가 저도 봐야겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021721.1.12",
          "form": "넵ㅋㅋ 스튜디오가 해외에 고층건물이라 아주 시원해요",
          "original_form": "넵ㅋㅋ 스튜디오가 해외에 고층건물이라 아주 시원해요",
          "speaker_id": "1",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021721.1.13",
          "form": "베이킹은 어떤 거 보세요??",
          "original_form": "베이킹은 어떤 거 보세요??",
          "speaker_id": "2",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021721.1.14",
          "form": "그때그때 만들고싶은 과자 레시피 영상중 가장 쉬운거요ㅋㅋ",
          "original_form": "그때그때 만들고싶은 과자 레시피 영상중 가장 쉬운거요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021721.1.15",
          "form": "ㅋㅋㅋㅋ 전 베이킹은 해 본 적 없어서 신기해요",
          "original_form": "ㅋㅋㅋㅋ 전 베이킹은 해 본 적 없어서 신기해요",
          "speaker_id": "2",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021721.1.16",
          "form": "가끔 추천영상 뜨면 봐요",
          "original_form": "가끔 추천영상 뜨면 봐요",
          "speaker_id": "2",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021721.1.17",
          "form": "진짜 시간가는줄 모르고보게 돼요",
          "original_form": "진짜 시간가는줄 모르고보게 돼요",
          "speaker_id": "1",
          "time": "20210828 22:28"
        }
      ]
