      "utterance": [
        {
          "id": "MDRW2100039189.1.1",
          "form": "가족은 어떻게 되세요?",
          "original_form": "가족은 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210903 13:17"
        },
        {
          "id": "MDRW2100039189.1.2",
          "form": "아이1명 있어요",
          "original_form": "아이1명 있어요",
          "speaker_id": "2",
          "time": "20210903 13:28"
        },
        {
          "id": "MDRW2100039189.1.3",
          "form": "앗 강아지도",
          "original_form": "앗 강아지도",
          "speaker_id": "2",
          "time": "20210903 13:28"
        },
        {
          "id": "MDRW2100039189.1.4",
          "form": "어머 애기!",
          "original_form": "어머 애기!",
          "speaker_id": "1",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100039189.1.5",
          "form": "몇살이에요?",
          "original_form": "몇살이에요?",
          "speaker_id": "1",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100039189.1.6",
          "form": "초3이여ㅋ",
          "original_form": "초3이여ㅋ",
          "speaker_id": "2",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100039189.1.7",
          "form": "이번주에",
          "original_form": "이번주에",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039189.1.8",
          "form": "학교 개학했나요?",
          "original_form": "학교 개학했나요?",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039189.1.9",
          "form": "개학인데 온라인수업해요ㅋ",
          "original_form": "개학인데 온라인수업해요ㅋ",
          "speaker_id": "2",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039189.1.10",
          "form": "등교 아예 안 해요?",
          "original_form": "등교 아예 안 해요?",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039189.1.11",
          "form": "네...ㅠㅠ",
          "original_form": "네...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 12:01"
        },
        {
          "id": "MDRW2100039189.1.12",
          "form": "다음주부턴 며칠 가는것 같아요",
          "original_form": "다음주부턴 며칠 가는것 같아요",
          "speaker_id": "2",
          "time": "20210904 12:01"
        },
        {
          "id": "MDRW2100039189.1.13",
          "form": "계신 지역 확진자 계속 나와요?",
          "original_form": "계신 지역 확진자 계속 나와요?",
          "speaker_id": "1",
          "time": "20210904 12:27"
        },
        {
          "id": "MDRW2100039189.1.14",
          "form": "아이가 집에 있으면 더 할일 많아지잖아요ㅜ",
          "original_form": "아이가 집에 있으면 더 할일 많아지잖아요ㅜ",
          "speaker_id": "1",
          "time": "20210904 12:27"
        },
        {
          "id": "MDRW2100039189.1.15",
          "form": "육아가 최고봉인것같아요ㅎ",
          "original_form": "육아가 최고봉인것같아요ㅎ",
          "speaker_id": "2",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100039189.1.16",
          "form": "그래도 초3이면 애기보다는",
          "original_form": "그래도 초3이면 애기보다는",
          "speaker_id": "1",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100039189.1.17",
          "form": "손이 덜 가죠?",
          "original_form": "손이 덜 가죠?",
          "speaker_id": "1",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100039189.1.18",
          "form": "네 애기보다는 쉬운것 같아요",
          "original_form": "네 애기보다는 쉬운것 같아요",
          "speaker_id": "2",
          "time": "20210904 17:43"
        }
