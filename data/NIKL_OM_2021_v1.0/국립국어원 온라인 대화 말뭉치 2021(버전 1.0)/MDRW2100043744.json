      "utterance": [
        {
          "id": "MDRW2100043744.1.1",
          "form": "얼른 역시즌 세일로 장만하시져.",
          "original_form": "얼른 역시즌 세일로 장만하시져.",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043744.1.2",
          "form": "진짜 계절바뀔때마다 생각하는 거지만",
          "original_form": "진짜 계절바뀔때마다 생각하는 거지만",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043744.1.3",
          "form": "작년에는 무슨 옷을 입고다녔던걸까요..? 입을 옷이 없어요",
          "original_form": "작년에는 무슨 옷을 입고다녔던걸까요..? 입을 옷이 없어요",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043744.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아 너무 공감합니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아 너무 공감합니다",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043744.1.5",
          "form": "진짜 다 버릴옷밖에 없어요",
          "original_form": "진짜 다 버릴옷밖에 없어요",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043744.1.6",
          "form": "이 무슨....",
          "original_form": "이 무슨....",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043744.1.7",
          "form": "전 그래서 맨날 남자옷가게 가서 튼튼한 옷 삽니다ㅠ",
          "original_form": "전 그래서 맨날 남자옷가게 가서 튼튼한 옷 삽니다ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043744.1.8",
          "form": "진짜 .. 요즘 거의 공용으로사거나 그러잖아요",
          "original_form": "진짜 .. 요즘 거의 공용으로사거나 그러잖아요",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043744.1.9",
          "form": "자켓에 안주머니가 없어요...! 겉옷 주머니는 왜 가짜로 만든거죠..?",
          "original_form": "자켓에 안주머니가 없어요...! 겉옷 주머니는 왜 가짜로 만든거죠..?",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043744.1.10",
          "form": "맞아요..!!!!!! 큰 공감",
          "original_form": "맞아요..!!!!!! 큰 공감",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043744.1.11",
          "form": "도대체 옷이 옷다워야말이죠",
          "original_form": "도대체 옷이 옷다워야말이죠",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043744.1.12",
          "form": "정말루요.. ㅠㅠ 요즘 옷들이 너무 가격에 비해서 제값을 못하는 옷이 참 많아요",
          "original_form": "정말루요.. ㅠㅠ 요즘 옷들이 너무 가격에 비해서 제값을 못하는 옷이 참 많아요",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043744.1.13",
          "form": "잘 살펴보고 사야듀ㅣ겠더라구요..!",
          "original_form": "잘 살펴보고 사야듀ㅣ겠더라구요..!",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043744.1.14",
          "form": "그니까요ㅜㅜ 너무 얇고 잘 튿어지고....",
          "original_form": "그니까요ㅜㅜ 너무 얇고 잘 튿어지고....",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043744.1.15",
          "form": "조금 비싸도 괜찮은걸로 사야해요 진짜",
          "original_form": "조금 비싸도 괜찮은걸로 사야해요 진짜",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043744.1.16",
          "form": "좀 비싼거사서 오래입는게 이득인 것 같아요",
          "original_form": "좀 비싼거사서 오래입는게 이득인 것 같아요",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043744.1.17",
          "form": "옷은 주로 어떤 스타일로 입으세요?",
          "original_form": "옷은 주로 어떤 스타일로 입으세요?",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043744.1.18",
          "form": "저 완전 스트릿입니다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 완전 스트릿입니다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043744.1.19",
          "form": "편한걸 추구해욧",
          "original_form": "편한걸 추구해욧",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043744.1.20",
          "form": "저도 스트릿 좋아해요 ㅋㅋ",
          "original_form": "저도 스트릿 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043744.1.21",
          "form": "와이드팬츠 조거팬츠 너무 좋아요..",
          "original_form": "와이드팬츠 조거팬츠 너무 좋아요..",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043744.1.22",
          "form": "크으으 저랑 넘 잘맞으시네요",
          "original_form": "크으으 저랑 넘 잘맞으시네요",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043744.1.23",
          "form": "저도 와이드팬츠 러버에요",
          "original_form": "저도 와이드팬츠 러버에요",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043744.1.24",
          "form": "맞아요 진짜 편하구 좋아요 ㅋㅋ 딱 가을에 맨투맨에 와이드팬츠 조합 ..!",
          "original_form": "맞아요 진짜 편하구 좋아요 ㅋㅋ 딱 가을에 맨투맨에 와이드팬츠 조합 ..!",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043744.1.25",
          "form": "와 정말이지 배우신분이굼요",
          "original_form": "와 정말이지 배우신분이굼요",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043744.1.26",
          "form": "제가 딱 그렇게 입습니다",
          "original_form": "제가 딱 그렇게 입습니다",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043744.1.27",
          "form": "그쵸.. 스트릿패션 아주 굿입니다",
          "original_form": "그쵸.. 스트릿패션 아주 굿입니다",
