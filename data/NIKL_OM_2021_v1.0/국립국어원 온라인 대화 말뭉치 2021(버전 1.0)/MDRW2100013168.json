      "utterance": [
        {
          "id": "MDRW2100013168.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013168.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013168.1.3",
          "form": "좋아하는 맛집 알려주실수 있나요?",
          "original_form": "좋아하는 맛집 알려주실수 있나요?",
          "speaker_id": "2",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013168.1.4",
          "form": "맛집이요?",
          "original_form": "맛집이요?",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013168.1.5",
          "form": "어떤종류요?",
          "original_form": "어떤종류요?",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013168.1.6",
          "form": "와.. 정말 많이 알고계신가봐요",
          "original_form": "와.. 정말 많이 알고계신가봐요",
          "speaker_id": "2",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013168.1.7",
          "form": "음 양식이나 일식? 맛있는곳 있을까요?",
          "original_form": "음 양식이나 일식? 맛있는곳 있을까요?",
          "speaker_id": "2",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100013168.1.8",
          "form": "아니면 정말 최애 맛집! 여기저기 다 소개해준 찐 맛집! 이런곳도 좋아요!",
          "original_form": "아니면 정말 최애 맛집! 여기저기 다 소개해준 찐 맛집! 이런곳도 좋아요!",
          "speaker_id": "2",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100013168.1.9",
          "form": "ㅋㅋㅋㅋ 근데 지역이랑",
          "original_form": "ㅋㅋㅋㅋ 근데 지역이랑",
          "speaker_id": "1",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100013168.1.10",
          "form": "너무광범위해서요ㅋㅋㅋㅋ",
          "original_form": "너무광범위해서요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100013168.1.11",
          "form": "어디 근처에 사세요?",
          "original_form": "어디 근처에 사세요?",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100013168.1.12",
          "form": "서울쪽 아시면 서울로 추천해주세요!",
          "original_form": "서울쪽 아시면 서울로 추천해주세요!",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100013168.1.13",
          "form": "제일 무난하니까",
          "original_form": "제일 무난하니까",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013168.1.14",
          "form": "사실 여긴",
          "original_form": "사실 여긴",
          "speaker_id": "1",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013168.1.15",
          "form": "저도안가봤는데 가보고싶은데인데요",
          "original_form": "저도안가봤는데 가보고싶은데인데요",
          "speaker_id": "1",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013168.1.16",
          "form": "몽탄이 그렇게 맛있대요ㅋㅋㅋ",
          "original_form": "몽탄이 그렇게 맛있대요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013168.1.17",
          "form": "몽탄이요? 어떤걸 파는곳인가요?",
          "original_form": "몽탄이요? 어떤걸 파는곳인가요?",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013168.1.18",
          "form": "우대갈비라고",
          "original_form": "우대갈비라고",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013168.1.19",
          "form": "고깃집인데",
          "original_form": "고깃집인데",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013168.1.20",
          "form": "웨이팅이 장난아니래요ㅋㅋㅋ 그리고비싸요",
          "original_form": "웨이팅이 장난아니래요ㅋㅋㅋ 그리고비싸요",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013168.1.21",
          "form": "어엄청",
          "original_form": "어엄청",
          "speaker_id": "1",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013168.1.22",
          "form": "유명해섴ㅋㅋ ㅠㅠ",
          "original_form": "유명해섴ㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013168.1.23",
          "form": "오.....",
          "original_form": "오.....",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013168.1.24",
          "form": "저는 고깃집 금돼지식당 맛있다고만 들었는데",
          "original_form": "저는 고깃집 금돼지식당 맛있다고만 들었는데",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013168.1.25",
          "form": "몽탄이란곳도 있군요",
          "original_form": "몽탄이란곳도 있군요",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013168.1.26",
          "form": "거기도ㄱㅏ보고싶어요ㅋㅋㅋ",
          "original_form": "거기도ㄱㅏ보고싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013168.1.27",
          "form": "참 맛집엔",
          "original_form": "참 맛집엔",
          "speaker_id": "1",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013168.1.28",
          "form": "사람이 많은거같아요",
          "original_form": "사람이 많은거같아요",
          "speaker_id": "1",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013168.1.29",
          "form": "어디서들 그렇게오시는지 웨이팅이",
          "original_form": "어디서들 그렇게오시는지 웨이팅이",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013168.1.30",
          "form": "맞아요.... 근데 맛집이라고 유명해도 다 맛있는건 아니더라구요...ㅠ",
          "original_form": "맞아요.... 근데 맛집이라고 유명해도 다 맛있는건 아니더라구요...ㅠ",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013168.1.31",
          "form": "소금델리? 거기도 유명해서 가봤는데 제 스타일은 아니었어요...",
          "original_form": "소금델리? 거기도 유명해서 가봤는데 제 스타일은 아니었어요...",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013168.1.32",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100013168.1.33",
          "form": "방어 좋아하세요?",
          "original_form": "방어 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013168.1.34",
          "form": "연남 바다회사랑 가보고싶어요ㅋㅋㅋ",
          "original_form": "연남 바다회사랑 가보고싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013168.1.35",
          "form": "방어도 좋죠...",
          "original_form": "방어도 좋죠...",
          "speaker_id": "2",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013168.1.36",
          "form": "거기도 유명한곳인가봐요",
          "original_form": "거기도 유명한곳인가봐요",
          "speaker_id": "2",
          "time": "20210825 14:48"
        }
      ]
