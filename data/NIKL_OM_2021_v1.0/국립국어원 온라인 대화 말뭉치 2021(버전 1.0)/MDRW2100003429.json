      "utterance": [
        {
          "id": "MDRW2100003429.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003429.1.2",
          "form": "이상형이어떻게되세요?",
          "original_form": "이상형이어떻게되세요?",
          "speaker_id": "1",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003429.1.3",
          "form": "귀엽고 뽀짝한 사람이요!",
          "original_form": "귀엽고 뽀짝한 사람이요!",
          "speaker_id": "2",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003429.1.4",
          "form": "연하를 좋아해요",
          "original_form": "연하를 좋아해요",
          "speaker_id": "2",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003429.1.5",
          "form": "결혼하셨어요?",
          "original_form": "결혼하셨어요?",
          "speaker_id": "2",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003429.1.6",
          "form": "저랑 이상형이 똑같네요",
          "original_form": "저랑 이상형이 똑같네요",
          "speaker_id": "1",
          "time": "20210820 23:00"
        },
        {
          "id": "MDRW2100003429.1.7",
          "form": "아뇨 결혼은 아직이에요",
          "original_form": "아뇨 결혼은 아직이에요",
          "speaker_id": "1",
          "time": "20210820 23:00"
        },
        {
          "id": "MDRW2100003429.1.8",
          "form": "이상형을 아직 못만났네요",
          "original_form": "이상형을 아직 못만났네요",
          "speaker_id": "1",
          "time": "20210820 23:00"
        },
        {
          "id": "MDRW2100003429.1.9",
          "form": "저도 미혼입니다ㅎㅎ",
          "original_form": "저도 미혼입니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 23:00"
        },
        {
          "id": "MDRW2100003429.1.10",
          "form": "소개팅해보셨어요?",
          "original_form": "소개팅해보셨어요?",
          "speaker_id": "1",
          "time": "20210820 23:00"
        },
        {
          "id": "MDRW2100003429.1.11",
          "form": "많이 해봤죠!!",
          "original_form": "많이 해봤죠!!",
          "speaker_id": "2",
          "time": "20210820 23:01"
        },
        {
          "id": "MDRW2100003429.1.12",
          "form": "요근래들어서는 못했어요ㅜㅜ 코로나때문에",
          "original_form": "요근래들어서는 못했어요ㅜㅜ 코로나때문에",
          "speaker_id": "2",
          "time": "20210820 23:01"
        },
        {
          "id": "MDRW2100003429.1.13",
          "form": "아 소개팅 좋아하시나봐요",
          "original_form": "아 소개팅 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210820 23:01"
        },
        {
          "id": "MDRW2100003429.1.14",
          "form": "코로나 끝나면 짝꿍을 찾을 수 있게 많이 해봐야죠",
          "original_form": "코로나 끝나면 짝꿍을 찾을 수 있게 많이 해봐야죠",
          "speaker_id": "2",
          "time": "20210820 23:02"
        },
        {
          "id": "MDRW2100003429.1.15",
          "form": "근데 잘 성사가 안되시나봐요",
          "original_form": "근데 잘 성사가 안되시나봐요",
          "speaker_id": "1",
          "time": "20210820 23:02"
        },
        {
          "id": "MDRW2100003429.1.16",
          "form": "아뇨 얼마전에 헤어졌거든요!!",
          "original_form": "아뇨 얼마전에 헤어졌거든요!!",
          "speaker_id": "2",
          "time": "20210820 23:02"
        },
        {
          "id": "MDRW2100003429.1.17",
          "form": "문제가 뭔지 파악하는게 중요한거같아요",
          "original_form": "문제가 뭔지 파악하는게 중요한거같아요",
          "speaker_id": "1",
          "time": "20210820 23:02"
        },
        {
          "id": "MDRW2100003429.1.18",
          "form": "아 금방 차이셨어요?",
          "original_form": "아 금방 차이셨어요?",
          "speaker_id": "1",
          "time": "20210820 23:02"
        },
        {
          "id": "MDRW2100003429.1.19",
          "form": "네넹 바람피다 걸렸거든요",
          "original_form": "네넹 바람피다 걸렸거든요",
          "speaker_id": "2",
          "time": "20210820 23:03"
        },
        {
          "id": "MDRW2100003429.1.20",
          "form": "저질이시네요..",
          "original_form": "저질이시네요..",
          "speaker_id": "1",
          "time": "20210820 23:03"
        },
        {
          "id": "MDRW2100003429.1.21",
          "form": "하지만 그럴수있죠뭐",
