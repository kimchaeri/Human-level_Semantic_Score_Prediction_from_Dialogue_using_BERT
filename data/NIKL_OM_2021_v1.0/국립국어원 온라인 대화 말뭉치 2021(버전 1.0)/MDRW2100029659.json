          "original_form": "지들끼리 야 몇살이야",
          "speaker_id": "2",
          "time": "20210903 12:01"
        },
        {
          "id": "MDRW2100029659.1.38",
          "form": "이러고있고ㅋㅋㅋ",
          "original_form": "이러고있고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:01"
        },
        {
          "id": "MDRW2100029659.1.39",
          "form": "아 ㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:01"
        },
        {
          "id": "MDRW2100029659.1.40",
