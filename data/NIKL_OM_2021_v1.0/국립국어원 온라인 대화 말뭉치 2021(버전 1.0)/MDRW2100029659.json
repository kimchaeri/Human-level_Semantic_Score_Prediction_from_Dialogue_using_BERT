        {
          "id": "MDRW2100029659.1.1",
          "form": "게임?",
          "original_form": "게임?",
          "speaker_id": "2",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029659.1.2",
          "form": "친구들하고 뭔겜하심?",
          "original_form": "친구들하고 뭔겜하심?",
          "speaker_id": "1",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029659.1.3",
          "form": "겜 같이 안해도",
          "original_form": "겜 같이 안해도",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029659.1.4",
          "form": "디코는 항상 들어오고",
          "original_form": "디코는 항상 들어오고",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029659.1.5",
          "form": "요새 오랜만에 다 맘맞아서 로아해요",
          "original_form": "요새 오랜만에 다 맘맞아서 로아해요",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029659.1.6",
          "form": "ㅋㅋㅋㅋ겜안해도 디코해요?",
          "original_form": "ㅋㅋㅋㅋ겜안해도 디코해요?",
          "speaker_id": "1",
          "time": "20210903 11:43"
        },
        {
          "id": "MDRW2100029659.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029659.1.8",
          "form": "카톡임",
          "original_form": "카톡임",
          "speaker_id": "2",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029659.1.9",
          "form": "음성카톡",
          "original_form": "음성카톡",
          "speaker_id": "2",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029659.1.10",
          "form": "그럴거면 전화로하지",
          "original_form": "그럴거면 전화로하지",
          "speaker_id": "1",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029659.1.11",
          "form": "아 여러명모이니까 그런가봄",
          "original_form": "아 여러명모이니까 그런가봄",
          "speaker_id": "1",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029659.1.12",
          "form": "전화는 폰 들고있거나 해야되잖아요",
          "original_form": "전화는 폰 들고있거나 해야되잖아요",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029659.1.13",
          "form": "이건 걍 컴키면 되는거니",
          "original_form": "이건 걍 컴키면 되는거니",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029659.1.14",
          "form": "오버워치로 넘어오시죠",
          "original_form": "오버워치로 넘어오시죠",
          "speaker_id": "1",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029659.1.15",
          "form": "옵치 초창기에 했었는데",
          "original_form": "옵치 초창기에 했었는데",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029659.1.16",
          "form": "재미읍써짐",
          "original_form": "재미읍써짐",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029659.1.17",
          "form": "파라 날뛰는거 좋아서 자주했는디",
          "original_form": "파라 날뛰는거 좋아서 자주했는디",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100029659.1.18",
          "form": "맥꾸리랑",
          "original_form": "맥꾸리랑",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100029659.1.19",
          "form": "저는 바스랑 메르시만함ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 바스랑 메르시만함ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100029659.1.20",
          "form": "바스ㅋㅋㅋㅋㅋ",
          "original_form": "바스ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100029659.1.21",
          "form": "저도 바스 좋아함료",
          "original_form": "저도 바스 좋아함료",
          "speaker_id": "2",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100029659.1.22",
          "form": "바스왕잘함",
          "original_form": "바스왕잘함",
          "speaker_id": "1",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100029659.1.23",
          "form": "전 바스로 뒤.치.기함",
          "original_form": "전 바스로 뒤.치.기함",
          "speaker_id": "2",
          "time": "20210903 11:56"
        },
        {
          "id": "MDRW2100029659.1.24",
          "form": "저도숨어서 잘쏨ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도숨어서 잘쏨ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100029659.1.25",
          "form": "기회되면 같이해봐요",
          "original_form": "기회되면 같이해봐요",
          "speaker_id": "1",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100029659.1.26",
          "form": "팟지대결인가",
          "original_form": "팟지대결인가",
          "speaker_id": "2",
          "time": "20210903 11:59"
        },
        {
          "id": "MDRW2100029659.1.27",
          "form": "그럴듯",
          "original_form": "그럴듯",
          "speaker_id": "1",
          "time": "20210903 11:59"
        },
        {
          "id": "MDRW2100029659.1.28",
          "form": "내가이길듯",
          "original_form": "내가이길듯",
          "speaker_id": "1",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100029659.1.29",
          "form": "좋은 자신감입니다",
          "original_form": "좋은 자신감입니다",
          "speaker_id": "2",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100029659.1.30",
          "form": "승리는 곧 자신감",
          "original_form": "승리는 곧 자신감",
          "speaker_id": "1",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100029659.1.31",
          "form": "자신감은 곧 승리",
          "original_form": "자신감은 곧 승리",
          "speaker_id": "1",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100029659.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅌ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100029659.1.33",
          "form": "최근에 그",
          "original_form": "최근에 그",
          "speaker_id": "2",
          "time": "20210903 12:01"
        },
        {
          "id": "MDRW2100029659.1.34",
          "form": "중력쓰는 남자 나와서",
          "original_form": "중력쓰는 남자 나와서",
          "speaker_id": "2",
          "time": "20210903 12:01"
        },
        {
          "id": "MDRW2100029659.1.35",
          "form": "했었는데",
          "original_form": "했었는데",
          "speaker_id": "2",
          "time": "20210903 12:01"
        },
        {
          "id": "MDRW2100029659.1.36",
          "form": "초딩들 너무많아요",
          "original_form": "초딩들 너무많아요",
          "speaker_id": "2",
          "time": "20210903 12:01"
        },
        {
          "id": "MDRW2100029659.1.37",
