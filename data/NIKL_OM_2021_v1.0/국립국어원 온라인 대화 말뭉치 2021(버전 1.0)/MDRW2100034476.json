      "utterance": [
        {
          "id": "MDRW2100034476.1.1",
          "form": "점심 드셨나요",
          "original_form": "점심 드셨나요",
          "speaker_id": "2",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100034476.1.2",
          "form": "파스타 배달 시켜놨어요",
          "original_form": "파스타 배달 시켜놨어요",
          "speaker_id": "1",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100034476.1.3",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100034476.1.4",
          "form": "네 저는 밀키트요 ㅋ",
          "original_form": "네 저는 밀키트요 ㅋ",
          "speaker_id": "2",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100034476.1.5",
          "form": "오 주로 어떤 밀키트 해서 드세요",
          "original_form": "오 주로 어떤 밀키트 해서 드세요",
          "speaker_id": "1",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100034476.1.6",
          "form": "밀키트 자주 안사는데 간만에 쌀국수랑 파스타 먹어봤어요",
          "original_form": "밀키트 자주 안사는데 간만에 쌀국수랑 파스타 먹어봤어요",
          "speaker_id": "2",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100034476.1.7",
          "form": "저도 밀키트가 좋아보여서 주문해도 결국 배달보다 맛없을때가 있길래 ㅋㅋㅋㅋ",
          "original_form": "저도 밀키트가 좋아보여서 주문해도 결국 배달보다 맛없을때가 있길래 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:24"
        },
        {
          "id": "MDRW2100034476.1.8",
          "form": "그냥 시켜먹게 되었어요 ㅠㅠ",
          "original_form": "그냥 시켜먹게 되었어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 13:24"
        },
        {
          "id": "MDRW2100034476.1.9",
          "form": "ㅋㅋㅋ 그런가요 그리고 은근 손도 가긴해요 편하긴 하면서도",
          "original_form": "ㅋㅋㅋ 그런가요 그리고 은근 손도 가긴해요 편하긴 하면서도",
          "speaker_id": "2",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100034476.1.10",
          "form": "맞아요 식재료 준비만 다 해준 상태라ㅋㅋㅋ",
          "original_form": "맞아요 식재료 준비만 다 해준 상태라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100034476.1.11",
          "form": "전 가끔 샤브샤브같은 밀키트 시켜요",
          "original_form": "전 가끔 샤브샤브같은 밀키트 시켜요",
          "speaker_id": "1",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100034476.1.12",
          "form": "밀키트하면 샤브샤브 더라구요 대표 메뉴 ㅎㅎ",
          "original_form": "밀키트하면 샤브샤브 더라구요 대표 메뉴 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100034476.1.13",
          "form": "네 이거야 말로 밀키트 없었으면 준비과정이 너무 끔찍하게 귀찮은 ㅋㅋㅋ",
          "original_form": "네 이거야 말로 밀키트 없었으면 준비과정이 너무 끔찍하게 귀찮은 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100034476.1.14",
          "form": "국수까지 끓여먹으면 완벽하죠",
          "original_form": "국수까지 끓여먹으면 완벽하죠",
          "speaker_id": "2",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100034476.1.15",
          "form": "크 맞아요 최고로 완벽한 음식",
          "original_form": "크 맞아요 최고로 완벽한 음식",
          "speaker_id": "1",
          "time": "20210828 13:49"
        }
      ]
