      "utterance": [
        {
          "id": "MDRW2100020304.1.1",
          "form": "일기예보가 너무 안맞아요",
          "original_form": "일기예보가 너무 안맞아요",
          "speaker_id": "1",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020304.1.2",
          "form": "저 오늘 밖을 안나가서 ㅋㅋㅋㅋㅋㅋ 오늘 비가 온다고했다고 안왔나요?? 아 어제인가 이제 ㅋㅋㅋㅋ",
          "original_form": "저 오늘 밖을 안나가서 ㅋㅋㅋㅋㅋㅋ 오늘 비가 온다고했다고 안왔나요?? 아 어제인가 이제 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020304.1.3",
          "form": "오늘 비온다고해서 재택했거든요",
          "original_form": "오늘 비온다고해서 재택했거든요",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020304.1.4",
          "form": "온다고 해서 운동 안가면 비가 안오고",
          "original_form": "온다고 해서 운동 안가면 비가 안오고",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020304.1.5",
          "form": "안온다고 해서 준비해서 운동하러 나가면 비가옵니다 ,,",
          "original_form": "안온다고 해서 준비해서 운동하러 나가면 비가옵니다 ,,",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020304.1.6",
          "form": "앗.. 이런 ㅋㅋㅋㅋㅋ",
          "original_form": "앗.. 이런 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020304.1.7",
          "form": "운동은 어떤거 하세요?!?!",
          "original_form": "운동은 어떤거 하세요?!?!",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020304.1.8",
          "form": "공원가서 뛰고걷고 반복합니다 !",
          "original_form": "공원가서 뛰고걷고 반복합니다 !",
          "speaker_id": "1",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020304.1.9",
          "form": "런데이까지는 아니고 ,,",
          "original_form": "런데이까지는 아니고 ,,",
          "speaker_id": "1",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020304.1.10",
          "form": "저 러닝 좀 해보고싶은데 마스크끼고 못뛰겠더라구여",
          "original_form": "저 러닝 좀 해보고싶은데 마스크끼고 못뛰겠더라구여",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020304.1.11",
          "form": "진짜 숨막혀서",
          "original_form": "진짜 숨막혀서",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020304.1.12",
          "form": "죽는줄알았어여",
          "original_form": "죽는줄알았어여",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020304.1.13",
          "form": "그쵸?!! 마스크끼니까 더 힘들어요 진짜 숨이 헉헉",
          "original_form": "그쵸?!! 마스크끼니까 더 힘들어요 진짜 숨이 헉헉",
          "speaker_id": "1",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020304.1.14",
          "form": "미세먼지!!! 코로나!!!!",
          "original_form": "미세먼지!!! 코로나!!!!",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020304.1.15",
          "form": "정말 ㅠㅠㅠ",
          "original_form": "정말 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020304.1.16",
          "form": "언제 예전으로 돌아갈수있을까요",
          "original_form": "언제 예전으로 돌아갈수있을까요",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020304.1.17",
          "form": "올해 더위보니까 진짜 무섭더라구요",
          "original_form": "올해 더위보니까 진짜 무섭더라구요",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020304.1.18",
          "form": "아 올해 더위 끔찍했어요",
          "original_form": "아 올해 더위 끔찍했어요",
          "speaker_id": "1",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020304.1.19",
          "form": "지구 온도 1도 올라간다는것도 실감되고",
          "original_form": "지구 온도 1도 올라간다는것도 실감되고",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020304.1.20",
          "form": "땀을 그렇게 흘리는 편이 아니었는데, 마스크밑으로땀이 흐르더라고요",
          "original_form": "땀을 그렇게 흘리는 편이 아니었는데, 마스크밑으로땀이 흐르더라고요",
          "speaker_id": "1",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020304.1.21",
          "form": "근데 또 더워서 에어컨을 안틀수가 없더라고여ㅠㅜ",
          "original_form": "근데 또 더워서 에어컨을 안틀수가 없더라고여ㅠㅜ",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020304.1.22",
          "form": "어 맞아요 ,, 그래서 27~28도로 맞춰놨었어요",
          "original_form": "어 맞아요 ,, 그래서 27~28도로 맞춰놨었어요",
          "speaker_id": "1",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020304.1.23",
          "form": "지구야...미안해...흑 ..ㅋㅋㅋㅋ",
          "original_form": "지구야...미안해...흑 ..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020304.1.24",
          "form": "근데 또 요즘은",
          "original_form": "근데 또 요즘은",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020304.1.25",
          "form": "엄청 선선하네요",
          "original_form": "엄청 선선하네요",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020304.1.26",
          "form": "자다가 추워서 깼어요",
          "original_form": "자다가 추워서 깼어요",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020304.1.27",
          "form": "아직 여름이불이거든요",
          "original_form": "아직 여름이불이거든요",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020304.1.28",
          "form": "헉 맞아요 요즘엔 이불이 있어야겠더라고요",
          "original_form": "헉 맞아요 요즘엔 이불이 있어야겠더라고요",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020304.1.29",
          "form": "이러다 가을 일주일 지나고 금방 겨울오겠죠 ?",
          "original_form": "이러다 가을 일주일 지나고 금방 겨울오겠죠 ?",
