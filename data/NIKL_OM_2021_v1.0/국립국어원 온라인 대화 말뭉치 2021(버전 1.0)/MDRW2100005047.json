      "utterance": [
        {
          "id": "MDRW2100005047.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100005047.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005047.1.3",
          "form": "요새 사고싶은 물건 있으세요?",
          "original_form": "요새 사고싶은 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005047.1.4",
          "form": "글쎄요ㅋ 가방 하나 사고 싶네요",
          "original_form": "글쎄요ㅋ 가방 하나 사고 싶네요",
          "speaker_id": "2",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005047.1.5",
          "form": "역시 가방이 다들사고싶나봐요",
          "original_form": "역시 가방이 다들사고싶나봐요",
          "speaker_id": "1",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005047.1.6",
          "form": "어떤거 사고 싶으세요?",
          "original_form": "어떤거 사고 싶으세요?",
          "speaker_id": "2",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005047.1.7",
          "form": "전 냉장고요 ㅋㅋ",
          "original_form": "전 냉장고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005047.1.8",
          "form": "아 그래요?  냉장고 좋은건 엄청 비싸던데",
          "original_form": "아 그래요?  냉장고 좋은건 엄청 비싸던데",
          "speaker_id": "2",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005047.1.9",
          "form": "네 그래서 못사고 망설여요",
          "original_form": "네 그래서 못사고 망설여요",
          "speaker_id": "1",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005047.1.10",
          "form": "전자제품은 큰맘 먹고 사야 되지 싶네요",
          "original_form": "전자제품은 큰맘 먹고 사야 되지 싶네요",
          "speaker_id": "2",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005047.1.11",
          "form": "네 지금 그래서 고민만 해요",
          "original_form": "네 지금 그래서 고민만 해요",
          "speaker_id": "1",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005047.1.12",
          "form": "가방은 어디 제품사고싶으새요?",
          "original_form": "가방은 어디 제품사고싶으새요?",
          "speaker_id": "1",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005047.1.13",
          "form": "세요?",
          "original_form": "세요?",
          "speaker_id": "1",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005047.1.14",
          "form": "전 명품보다는 중저가 실용적인거 사고 싶어요",
          "original_form": "전 명품보다는 중저가 실용적인거 사고 싶어요",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005047.1.15",
          "form": "맞아요 저도 그래요",
          "original_form": "맞아요 저도 그래요",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005047.1.16",
          "form": "가방 있지만 그래도 많으면 좋으니깐",
          "original_form": "가방 있지만 그래도 많으면 좋으니깐",
          "speaker_id": "2",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005047.1.17",
          "form": "맞아요 많아야 해요",
          "original_form": "맞아요 많아야 해요",
          "speaker_id": "1",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005047.1.18",
          "form": "저 요새 캉골 가격도 좋고 편해서 하고 다녀요",
          "original_form": "저 요새 캉골 가격도 좋고 편해서 하고 다녀요",
          "speaker_id": "1",
          "time": "20210821 20:30"
        }
      ]
