      "utterance": [
        {
          "id": "MDRW2100017057.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100017057.1.2",
          "form": "오늘 모처럼 비가 내리지 않네요",
          "original_form": "오늘 모처럼 비가 내리지 않네요",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017057.1.3",
          "form": "네 가을 오나 싶었는데 아직은 여름인것 같아요",
          "original_form": "네 가을 오나 싶었는데 아직은 여름인것 같아요",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017057.1.4",
          "form": "네 아직은 낮엔 좀 더워요",
          "original_form": "네 아직은 낮엔 좀 더워요",
          "speaker_id": "1",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100017057.1.5",
          "form": "그래도 바람은 많이 선선해졌어요",
          "original_form": "그래도 바람은 많이 선선해졌어요",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017057.1.6",
          "form": "오랜만에 햇볕 쨍쨍해서 기분도 좋네요",
          "original_form": "오랜만에 햇볕 쨍쨍해서 기분도 좋네요",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017057.1.7",
          "form": "네 내일부터 또 비온다니",
          "original_form": "네 내일부터 또 비온다니",
          "speaker_id": "1",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100017057.1.8",
          "form": "오늘 햇볕을 실컷 누려야겠어요",
          "original_form": "오늘 햇볕을 실컷 누려야겠어요",
          "speaker_id": "1",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100017057.1.9",
          "form": "찰나의 맑음이네요 ㅜㅜ",
          "original_form": "찰나의 맑음이네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100017057.1.10",
          "form": "네 그러게요",
          "original_form": "네 그러게요",
          "speaker_id": "1",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100017057.1.11",
          "form": "잠깐 나왔는데 공기는 약간 축축해요",
          "original_form": "잠깐 나왔는데 공기는 약간 축축해요",
          "speaker_id": "1",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100017057.1.12",
          "form": "저희 동네는 오늘은 습도는 별로 안 높은 것 같아요",
          "original_form": "저희 동네는 오늘은 습도는 별로 안 높은 것 같아요",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017057.1.13",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "1",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100017057.1.14",
          "form": "해 쨍한 맑은 날 좋아해서 넘 좋아요 ㅋㅋ",
          "original_form": "해 쨍한 맑은 날 좋아해서 넘 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017057.1.15",
          "form": "ㅎㅎ오늘의 날씨를 누리셔야겠네요!",
          "original_form": "ㅎㅎ오늘의 날씨를 누리셔야겠네요!",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017057.1.16",
          "form": "네 화분도 해 잘 드는 곳으로 옮겨두고요 ㅋㅋ",
          "original_form": "네 화분도 해 잘 드는 곳으로 옮겨두고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017057.1.17",
          "form": "오 식물도 오랜만에 햇살을 보겠네요",
          "original_form": "오 식물도 오랜만에 햇살을 보겠네요",
          "speaker_id": "1",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017057.1.18",
          "form": "네 잘 컸으면 좋겠어요 광합성 많이 하구 ㅋㅋ",
          "original_form": "네 잘 컸으면 좋겠어요 광합성 많이 하구 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017057.1.19",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017057.1.20",
          "form": "해를 많이 봐야 광합성도 잘할텐데",
          "original_form": "해를 많이 봐야 광합성도 잘할텐데",
          "speaker_id": "1",
          "time": "20210826 17:24"
        }
