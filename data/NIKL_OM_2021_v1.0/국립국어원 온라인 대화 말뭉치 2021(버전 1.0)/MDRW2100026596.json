      "utterance": [
        {
          "id": "MDRW2100026596.1.1",
          "form": "좋아하진 않는데 걷기는 하려구 햄",
          "original_form": "좋아하진 않는데 걷기는 하려구 햄",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026596.1.2",
          "form": "걷기도 너무 좋은 운동이지!",
          "original_form": "걷기도 너무 좋은 운동이지!",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026596.1.3",
          "form": "나도 산책 자주 하는 편이야!ㅎㅎ",
          "original_form": "나도 산책 자주 하는 편이야!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026596.1.4",
          "form": "어디로 산책해? 나는 주변에 공원이 있어!",
          "original_form": "어디로 산책해? 나는 주변에 공원이 있어!",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026596.1.5",
          "form": "나는 집앞에 산책로가 있어서 거기로 다니고 있어!",
          "original_form": "나는 집앞에 산책로가 있어서 거기로 다니고 있어!",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026596.1.6",
          "form": "이번주엔 날씨가 좋지 않아서 많이 못나가긴 했어ㅜ",
          "original_form": "이번주엔 날씨가 좋지 않아서 많이 못나가긴 했어ㅜ",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026596.1.7",
          "form": "아맞아 비가 많이 와서ㅠㅠ",
          "original_form": "아맞아 비가 많이 와서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026596.1.8",
          "form": "산책로 좋다~ 걷기가 제일 몸에 부담이 안 간대",
          "original_form": "산책로 좋다~ 걷기가 제일 몸에 부담이 안 간대",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026596.1.9",
          "form": "그래서 좋은 것 같아!",
          "original_form": "그래서 좋은 것 같아!",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026596.1.10",
          "form": "맞아! 초보자도 쉽게 할수있고ㅎㅎ",
          "original_form": "맞아! 초보자도 쉽게 할수있고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026596.1.11",
          "form": "걷기 말고 건강을 위해 하는거 있어??",
          "original_form": "걷기 말고 건강을 위해 하는거 있어??",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026596.1.12",
          "form": "나 스트레칭! 전신!",
          "original_form": "나 스트레칭! 전신!",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026596.1.13",
          "form": "책 사서 그거 따라해 ㅎㅎ",
          "original_form": "책 사서 그거 따라해 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026596.1.14",
          "form": "오호! 효과가 좋은것 같아??",
          "original_form": "오호! 효과가 좋은것 같아??",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026596.1.15",
          "form": "응응! 방구 잘나오고 몸이 풀려!!",
          "original_form": "응응! 방구 잘나오고 몸이 풀려!!",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026596.1.16",
          "form": "추천추천",
          "original_form": "추천추천",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026596.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 너무 효과가 맘에 든다!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 너무 효과가 맘에 든다!",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026596.1.18",
          "form": "계속 앉아있으면 배에 자꾸 가스차는데",
          "original_form": "계속 앉아있으면 배에 자꾸 가스차는데",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026596.1.19",
          "form": "나도 해봐야겠다!",
          "original_form": "나도 해봐야겠다!",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026596.1.20",
          "form": "응응 진짜 추천이얌",
          "original_form": "응응 진짜 추천이얌",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026596.1.21",
          "form": "스트레칭은 맨손으로 그냥 아무곳에서나 할 수 있어서 좋아",
          "original_form": "스트레칭은 맨손으로 그냥 아무곳에서나 할 수 있어서 좋아",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026596.1.22",
          "form": "검색해도 방법 많이 나오공",
          "original_form": "검색해도 방법 많이 나오공",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026596.1.23",
          "form": "당장 내일부터 해봐야겠다! ٩( ᐛ )و",
          "original_form": "당장 내일부터 해봐야겠다! ٩( ᐛ )و",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026596.1.24",
          "form": "진짜 추천이야... 나 끈기 0인데 스트레칭 매일해",
          "original_form": "진짜 추천이야... 나 끈기 0인데 스트레칭 매일해",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026596.1.25",
          "form": "짱 쉬워 효과도 좋아",
          "original_form": "짱 쉬워 효과도 좋아",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026596.1.26",
          "form": "나 근데 좀 그런거 있엌ㅋㅋ",
          "original_form": "나 근데 좀 그런거 있엌ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100026596.1.27",
          "form": "운동화도 매장 언니가 신은거 사",
          "original_form": "운동화도 매장 언니가 신은거 사",
