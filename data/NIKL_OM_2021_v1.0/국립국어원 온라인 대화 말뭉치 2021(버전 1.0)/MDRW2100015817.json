      "utterance": [
        {
          "id": "MDRW2100015817.1.1",
          "form": "치킨좋아하세요?",
          "original_form": "치킨좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015817.1.2",
          "form": "네 치킨 좋아요 ㅋㅋㅋ",
          "original_form": "네 치킨 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015817.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015817.1.4",
          "form": "저도 치킨좋아해요! 어디브랜드 선호하세요?",
          "original_form": "저도 치킨좋아해요! 어디브랜드 선호하세요?",
          "speaker_id": "1",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015817.1.5",
          "form": "저는 뿌링클이요",
          "original_form": "저는 뿌링클이요",
          "speaker_id": "2",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015817.1.6",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015817.1.7",
          "form": "저는 교촌치킨 좋아해요! 드셔보셨나요?",
          "original_form": "저는 교촌치킨 좋아해요! 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015817.1.8",
          "form": "네네넹 당연하죠!!!",
          "original_form": "네네넹 당연하죠!!!",
          "speaker_id": "2",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015817.1.9",
          "form": "예전에는 교촌이 맵다고 느꼈는데 요즘은",
          "original_form": "예전에는 교촌이 맵다고 느꼈는데 요즘은",
          "speaker_id": "2",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015817.1.10",
          "form": "허니콤보만 먹어서",
          "original_form": "허니콤보만 먹어서",
          "speaker_id": "2",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015817.1.11",
          "form": "이미지가 확 바뀌었어요",
          "original_form": "이미지가 확 바뀌었어요",
          "speaker_id": "2",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015817.1.12",
          "form": "허콤 맛있죠ㅠ 내일 점심 뭐 드시고싶으세요?",
          "original_form": "허콤 맛있죠ㅠ 내일 점심 뭐 드시고싶으세요?",
          "speaker_id": "1",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015817.1.13",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015817.1.14",
          "form": "저는 허니콤보...요 하지만 점심은 그럴 수 없으니",
          "original_form": "저는 허니콤보...요 하지만 점심은 그럴 수 없으니",
          "speaker_id": "2",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015817.1.15",
          "form": "음....",
          "original_form": "음....",
          "speaker_id": "2",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015817.1.16",
          "form": "빅맥 세트 먹고 싶네요!!!!ㅋㅋㅋㅋ",
          "original_form": "빅맥 세트 먹고 싶네요!!!!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015817.1.17",
          "form": "아미면 뜨끈한 돼지국밥이요",
          "original_form": "아미면 뜨끈한 돼지국밥이요",
          "speaker_id": "2",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015817.1.18",
          "form": "국밥 맛있겠다ㅠㅠ 좋아하는 카페 브랜드 있으세요?",
          "original_form": "국밥 맛있겠다ㅠㅠ 좋아하는 카페 브랜드 있으세요?",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015817.1.19",
          "form": "아 저 이디야 집앞에 있어서 자주가요",
          "original_form": "아 저 이디야 집앞에 있어서 자주가요",
          "speaker_id": "2",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015817.1.20",
          "form": "뭔가 이디야는 사장님이 직접 많이 나오시고",
          "original_form": "뭔가 이디야는 사장님이 직접 많이 나오시고",
          "speaker_id": "2",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015817.1.21",
          "form": "다들 친절한 느낌???",
          "original_form": "다들 친절한 느낌???",
          "speaker_id": "2",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015817.1.22",
          "form": "친근..푸근???",
          "original_form": "친근..푸근???",
          "speaker_id": "2",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015817.1.23",
          "form": "그래서 좋아합니다 ㅎㅎ",
          "original_form": "그래서 좋아합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015817.1.24",
          "form": "디저트는 투썸이용!",
          "original_form": "디저트는 투썸이용!",
          "speaker_id": "2",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015817.1.25",
          "form": "저도 이디야 자주가요! 좋아하는 음료 있으세요?",
          "original_form": "저도 이디야 자주가요! 좋아하는 음료 있으세요?",
          "speaker_id": "1",
          "time": "20210826 01:39"
        },
        {
          "id": "MDRW2100015817.1.26",
          "form": "아아를 가장 많이 마시는데 꿀복숭아 플랫치노 맛있어요 ㅎㅎ",
          "original_form": "아아를 가장 많이 마시는데 꿀복숭아 플랫치노 맛있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 01:41"
        },
        {
          "id": "MDRW2100015817.1.27",
          "form": "꿀복숭아가 작은데 말랑 해서 재미있는 느낌이어서 좋아합니다",
          "original_form": "꿀복숭아가 작은데 말랑 해서 재미있는 느낌이어서 좋아합니다",
          "speaker_id": "2",
          "time": "20210826 01:41"
        },
        {
          "id": "MDRW2100015817.1.28",
          "form": "전 연유라떼 좋아해요!",
          "original_form": "전 연유라떼 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 01:42"
        }
      ]
    }
