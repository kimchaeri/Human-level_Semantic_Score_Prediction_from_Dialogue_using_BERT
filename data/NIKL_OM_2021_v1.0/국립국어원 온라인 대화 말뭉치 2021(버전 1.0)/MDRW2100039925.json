      "utterance": [
        {
          "id": "MDRW2100039925.1.1",
          "form": "모히또 이드라마는 뭐지",
          "original_form": "모히또 이드라마는 뭐지",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100039925.1.2",
          "form": "몰디브에서 모히토 한잔ㅋ",
          "original_form": "몰디브에서 모히토 한잔ㅋ",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039925.1.3",
          "form": "마이너스 상속자인 재벌3세 남주",
          "original_form": "마이너스 상속자인 재벌3세 남주",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039925.1.4",
          "form": "ㅋㅋ 재벌이 안나오면 얘기가 안돔",
          "original_form": "ㅋㅋ 재벌이 안나오면 얘기가 안돔",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039925.1.5",
          "form": "그러게 나두 그생각했다",
          "original_form": "그러게 나두 그생각했다",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100039925.1.6",
          "form": "재벌은ㅈ누구예요?",
          "original_form": "재벌은ㅈ누구예요?",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100039925.1.7",
          "form": "악착같이 돈을 모아도 늘 돈이 없는 여주의 만남",
          "original_form": "악착같이 돈을 모아도 늘 돈이 없는 여주의 만남",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100039925.1.8",
          "form": "꼭 여주가 가난함",
          "original_form": "꼭 여주가 가난함",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039925.1.9",
          "form": "남주는 성준이다",
          "original_form": "남주는 성준이다",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039925.1.10",
          "form": "여주는 누군가요?",
          "original_form": "여주는 누군가요?",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039925.1.11",
          "form": "드라마 공식인가보다 여주가 가난한게",
          "original_form": "드라마 공식인가보다 여주가 가난한게",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039925.1.12",
          "form": "꼭 그렇죠?  나중에 둘이 잘됨?",
          "original_form": "꼭 그렇죠?  나중에 둘이 잘됨?",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100039925.1.13",
          "form": "여주는 임지연인데 손현주의 간이역에 나온 배우임",
          "original_form": "여주는 임지연인데 손현주의 간이역에 나온 배우임",
          "speaker_id": "2",
          "time": "20210903 22:54"
        }
