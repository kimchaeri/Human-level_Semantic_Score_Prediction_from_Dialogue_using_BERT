      "utterance": [
        {
          "id": "MDRW2100031307.1.1",
          "form": "좋아하는 음식이뭐야?",
          "original_form": "좋아하는 음식이뭐야?",
          "speaker_id": "1",
          "time": "20210904 20:17"
        },
        {
          "id": "MDRW2100031307.1.2",
          "form": "떡볶이요! 좋아하는음식 뭐에요?",
          "original_form": "떡볶이요! 좋아하는음식 뭐에요?",
          "speaker_id": "2",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100031307.1.3",
          "form": "저는 피자요ㅋㅋㅋ",
          "original_form": "저는 피자요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100031307.1.4",
          "form": "떡볶이ㅜㅜ맛있죠",
          "original_form": "떡볶이ㅜㅜ맛있죠",
          "speaker_id": "1",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100031307.1.5",
          "form": "맞아요 피자도 맛있죠  어떤 피자 젤좋아하세요?",
          "original_form": "맞아요 피자도 맛있죠  어떤 피자 젤좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100031307.1.6",
          "form": "포테이토요!",
          "original_form": "포테이토요!",
          "speaker_id": "1",
          "time": "20210904 20:26"
        },
        {
          "id": "MDRW2100031307.1.7",
          "form": "거기다가",
          "original_form": "거기다가",
          "speaker_id": "1",
          "time": "20210904 20:26"
        },
        {
          "id": "MDRW2100031307.1.8",
          "form": "갈릭딥핑소스찍으면",
          "original_form": "갈릭딥핑소스찍으면",
          "speaker_id": "1",
          "time": "20210904 20:26"
        },
        {
          "id": "MDRW2100031307.1.9",
          "form": "크",
          "original_form": "크",
          "speaker_id": "1",
          "time": "20210904 20:26"
        },
        {
          "id": "MDRW2100031307.1.10",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 20:26"
        },
        {
          "id": "MDRW2100031307.1.11",
          "form": "짱 맛있겠어요~",
          "original_form": "짱 맛있겠어요~",
          "speaker_id": "2",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100031307.1.12",
          "form": "저는 먹는걸 워낙좋아해서",
          "original_form": "저는 먹는걸 워낙좋아해서",
          "speaker_id": "1",
          "time": "20210904 20:29"
        },
        {
          "id": "MDRW2100031307.1.13",
          "form": "ㅋㅋㅋㅋ사실 다 좋아해요",
          "original_form": "ㅋㅋㅋㅋ사실 다 좋아해요",
          "speaker_id": "1",
          "time": "20210904 20:29"
        },
        {
          "id": "MDRW2100031307.1.14",
          "form": "배달음식류는",
          "original_form": "배달음식류는",
          "speaker_id": "1",
          "time": "20210904 20:29"
        },
        {
          "id": "MDRW2100031307.1.15",
          "form": "저두 배달음식들은 다 좋아해요ㅎㅎ",
          "original_form": "저두 배달음식들은 다 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:29"
        },
        {
          "id": "MDRW2100031307.1.16",
          "form": "배달 자주 시켜요?",
          "original_form": "배달 자주 시켜요?",
          "speaker_id": "1",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100031307.1.17",
          "form": "저는 자취해서 자주 먹어요",
          "original_form": "저는 자취해서 자주 먹어요",
          "speaker_id": "1",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031307.1.18",
          "form": "전 가족들이랑 살아서 엄마가 해주시는 집밥 먹고 가끔 시켜먹어요~",
          "original_form": "전 가족들이랑 살아서 엄마가 해주시는 집밥 먹고 가끔 시켜먹어요~",
          "speaker_id": "2",
          "time": "20210904 21:03"
        },
        {
          "id": "MDRW2100031307.1.19",
          "form": "오 ㅜㅜ엄마밥 부러워요",
          "original_form": "오 ㅜㅜ엄마밥 부러워요",
          "speaker_id": "1",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100031307.1.20",
          "form": "젤 좋아하는 후식 이나 디져트 있으세요?",
          "original_form": "젤 좋아하는 후식 이나 디져트 있으세요?",
          "speaker_id": "2",
          "time": "20210904 21:42"
        },
        {
          "id": "MDRW2100031307.1.21",
          "form": "음 저는 마카롱?",
          "original_form": "음 저는 마카롱?",
          "speaker_id": "1",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100031307.1.22",
          "form": "자주는 아닌데 종종 막 땡겨서 먹어요",
          "original_form": "자주는 아닌데 종종 막 땡겨서 먹어요",
          "speaker_id": "1",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100031307.1.23",
          "form": "마카롱 맛있죠!",
          "original_form": "마카롱 맛있죠!",
          "speaker_id": "2",
          "time": "20210904 22:07"
        }
      ]
