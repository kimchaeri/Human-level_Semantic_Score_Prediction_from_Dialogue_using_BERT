      "utterance": [
        {
          "id": "MDRW2100029223.1.1",
          "form": "최근 여행가신곳이 어디에요",
          "original_form": "최근 여행가신곳이 어디에요",
          "speaker_id": "1",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029223.1.2",
          "form": "1년쯤 되긴 했는데 속초다녀왔어요",
          "original_form": "1년쯤 되긴 했는데 속초다녀왔어요",
          "speaker_id": "2",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029223.1.3",
          "form": "물회가 맛있었어요",
          "original_form": "물회가 맛있었어요",
          "speaker_id": "2",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029223.1.4",
          "form": "속초면 등산하셨어요?",
          "original_form": "속초면 등산하셨어요?",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029223.1.5",
          "form": "어떤물회에요?  생선인가요?",
          "original_form": "어떤물회에요?  생선인가요?",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029223.1.6",
          "form": "속초는 자연경관이 좋겠어요",
          "original_form": "속초는 자연경관이 좋겠어요",
          "speaker_id": "1",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029223.1.7",
          "form": "사실 출장이긴했는데 그 김에 좀 주변 둘러보고왔어요",
          "original_form": "사실 출장이긴했는데 그 김에 좀 주변 둘러보고왔어요",
          "speaker_id": "2",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029223.1.8",
          "form": "물회집 뷰가 좋아서 바다가 한 눈에 보이더라구요",
          "original_form": "물회집 뷰가 좋아서 바다가 한 눈에 보이더라구요",
          "speaker_id": "2",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029223.1.9",
          "form": "여행지로 운전하고 가셨나요?",
          "original_form": "여행지로 운전하고 가셨나요?",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029223.1.10",
          "form": "속초는 물이 맑나요?",
          "original_form": "속초는 물이 맑나요?",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029223.1.11",
          "form": "넵 제가 한 건 아니지만 자동차로 갔어요!",
          "original_form": "넵 제가 한 건 아니지만 자동차로 갔어요!",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029223.1.12",
          "form": "동해바다라 물이 맑더라구요",
          "original_form": "동해바다라 물이 맑더라구요",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029223.1.13",
          "form": "바다 좋지요",
          "original_form": "바다 좋지요",
          "speaker_id": "1",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029223.1.14",
          "form": "빨간 등대도 유명하더라구요",
          "original_form": "빨간 등대도 유명하더라구요",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029223.1.15",
          "form": "속초 관광명소는 어디에요?",
          "original_form": "속초 관광명소는 어디에요?",
          "speaker_id": "1",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029223.1.16",
          "form": "속초시장?",
          "original_form": "속초시장?",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029223.1.17",
          "form": "바닷가에 있어 예쁘겠어요",
          "original_form": "바닷가에 있어 예쁘겠어요",
          "speaker_id": "1",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029223.1.18",
          "form": "속초시장 만석닭강정 많이 사가더라구요",
          "original_form": "속초시장 만석닭강정 많이 사가더라구요",
          "speaker_id": "2",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029223.1.19",
          "form": "재래시장에서 기념품도 사셨나요?",
          "original_form": "재래시장에서 기념품도 사셨나요?",
          "speaker_id": "1",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029223.1.20",
          "form": "아 들어봤어요",
          "original_form": "아 들어봤어요",
          "speaker_id": "1",
          "time": "20210903 00:33"
        }
      ]
