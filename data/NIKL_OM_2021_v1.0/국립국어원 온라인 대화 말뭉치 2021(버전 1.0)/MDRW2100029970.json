      "utterance": [
        {
          "id": "MDRW2100029970.1.1",
          "form": "너 커뮤 뛴 적 있니",
          "original_form": "너 커뮤 뛴 적 있니",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029970.1.2",
          "form": "살면서 해본건 커뮤밖에 없단말야",
          "original_form": "살면서 해본건 커뮤밖에 없단말야",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029970.1.3",
          "form": "아 그게그건가",
          "original_form": "아 그게그건가",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029970.1.4",
          "form": "자캐커뮤도 온라인커뮤니티,,,,그렇겠네 여태까지 그냥 오타쿠들의 음습한 취미생활로만 생각했다",
          "original_form": "자캐커뮤도 온라인커뮤니티,,,,그렇겠네 여태까지 그냥 오타쿠들의 음습한 취미생활로만 생각했다",
          "speaker_id": "1",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029970.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029970.1.6",
          "form": "나도 최근에 친구가 하자 해서",
          "original_form": "나도 최근에 친구가 하자 해서",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029970.1.7",
          "form": "커뮤 뛰어봤지...",
          "original_form": "커뮤 뛰어봤지...",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029970.1.8",
          "form": "어땠어",
          "original_form": "어땠어",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029970.1.9",
          "form": "잘했어?",
          "original_form": "잘했어?",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029970.1.10",
          "form": "동결안났어?",
          "original_form": "동결안났어?",
          "speaker_id": "1",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029970.1.11",
          "form": "나?",
          "original_form": "나?",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029970.1.12",
          "form": "당연히 탈주햇지...",
          "original_form": "당연히 탈주햇지...",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029970.1.13",
          "form": "ㅋㅋㅋㅋㅋ잘 안맞았납지?",
          "original_form": "ㅋㅋㅋㅋㅋ잘 안맞았납지?",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029970.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ 응....",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 응....",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029970.1.15",
          "form": "너무 힘들어",
          "original_form": "너무 힘들어",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029970.1.16",
          "form": "나랑 커뮤는 잘 안맞나봐",
          "original_form": "나랑 커뮤는 잘 안맞나봐",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029970.1.17",
          "form": "그럴 수 있지...",
          "original_form": "그럴 수 있지...",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029970.1.18",
          "form": "하나부터 끝까지 다 기간제라 기간 안에 해야하잖어",
          "original_form": "하나부터 끝까지 다 기간제라 기간 안에 해야하잖어",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029970.1.19",
          "form": "커뮤가면 기력딸리는게 당연하지...",
          "original_form": "커뮤가면 기력딸리는게 당연하지...",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029970.1.20",
          "form": "너는 어때?",
          "original_form": "너는 어때?",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029970.1.21",
          "form": "하나 개장 기다리는데는 있는데",
          "original_form": "하나 개장 기다리는데는 있는데",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029970.1.22",
          "form": "망할것 같은 기분이 들어",
          "original_form": "망할것 같은 기분이 들어",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029970.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 으악",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 으악",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029970.1.24",
          "form": "안돼 망하면...",
          "original_form": "안돼 망하면...",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029970.1.25",
          "form": "인원수 모잘라서 커뮤 개장 미뤄졌거든...",
          "original_form": "인원수 모잘라서 커뮤 개장 미뤄졌거든...",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029970.1.26",
          "form": "오....",
          "original_form": "오....",
          "speaker_id": "2",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100029970.1.27",
          "form": "미뤄지면 ㅁ엎어질 수도 잇잖아",
          "original_form": "미뤄지면 ㅁ엎어질 수도 잇잖아",
          "speaker_id": "2",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100029970.1.28",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
