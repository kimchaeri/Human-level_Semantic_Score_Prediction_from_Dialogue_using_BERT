      "utterance": [
        {
          "id": "MDRW2100030880.1.1",
          "form": "새우 머리튀김인데 ㅋㅋ메뉴에",
          "original_form": "새우 머리튀김인데 ㅋㅋ메뉴에",
          "speaker_id": "2",
          "time": "20210904 13:54"
        },
        {
          "id": "MDRW2100030880.1.2",
          "form": "마빡튀김이라고 명기?해놨습니다",
          "original_form": "마빡튀김이라고 명기?해놨습니다",
          "speaker_id": "2",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030880.1.3",
          "form": "아ㅋㅋ 새우머리튀김 맛있죠!!",
          "original_form": "아ㅋㅋ 새우머리튀김 맛있죠!!",
          "speaker_id": "1",
          "time": "20210904 14:29"
        },
        {
          "id": "MDRW2100030880.1.4",
          "form": "몸통 말고 머리만 파나요ㅋㅋ",
          "original_form": "몸통 말고 머리만 파나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100030880.1.5",
          "form": "처가에 와서 답장이 늦었너요 ㅠ 죄송",
          "original_form": "처가에 와서 답장이 늦었너요 ㅠ 죄송",
          "speaker_id": "2",
          "time": "20210905 06:45"
        },
        {
          "id": "MDRW2100030880.1.6",
          "form": "머리만 튀겨 주시더라구요",
          "original_form": "머리만 튀겨 주시더라구요",
          "speaker_id": "2",
          "time": "20210905 06:45"
        },
        {
          "id": "MDRW2100030880.1.7",
          "form": "호불호 있는것 같아용",
          "original_form": "호불호 있는것 같아용",
          "speaker_id": "2",
          "time": "20210905 06:45"
        },
        {
          "id": "MDRW2100030880.1.8",
          "form": "어...저는 새우머리도 좋아하지만",
          "original_form": "어...저는 새우머리도 좋아하지만",
          "speaker_id": "1",
          "time": "20210905 10:16"
        },
        {
          "id": "MDRW2100030880.1.9",
          "form": "몸통이 없는건 용서가 안되는데요",
          "original_form": "몸통이 없는건 용서가 안되는데요",
          "speaker_id": "1",
          "time": "20210905 10:17"
        },
        {
          "id": "MDRW2100030880.1.10",
          "form": "ㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210905 19:08"
        },
        {
          "id": "MDRW2100030880.1.11",
          "form": "몸통은 없어유",
          "original_form": "몸통은 없어유",
          "speaker_id": "2",
          "time": "20210905 19:08"
        },
        {
          "id": "MDRW2100030880.1.12",
          "form": "황금 처가집 다녀왔습니다",
          "original_form": "황금 처가집 다녀왔습니다",
          "speaker_id": "2",
          "time": "20210905 19:08"
        },
        {
          "id": "MDRW2100030880.1.13",
          "form": "방금",
          "original_form": "방금",
          "speaker_id": "2",
          "time": "20210905 19:09"
        },
        {
          "id": "MDRW2100030880.1.14",
          "form": "오늘 즐거운 식사 많이 하셨나요",
          "original_form": "오늘 즐거운 식사 많이 하셨나요",
          "speaker_id": "2",
          "time": "20210905 19:11"
        },
        {
          "id": "MDRW2100030880.1.15",
          "form": "오늘은 출근하는고로ㅜ 적당히 떼웠습니다",
          "original_form": "오늘은 출근하는고로ㅜ 적당히 떼웠습니다",
          "speaker_id": "1",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100030880.1.16",
          "form": "님은 오늘도 name3님이 솜씨를 발휘했나요!",
          "original_form": "님은 오늘도 &name3&님이 솜씨를 발휘했나요!",
          "speaker_id": "1",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100030880.1.17",
          "form": "아 출근하셨구나",
          "original_form": "아 출근하셨구나",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100030880.1.18",
          "form": "오늘 저녁은 홍 라면입니다",
          "original_form": "오늘 저녁은 홍 라면입니다",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100030880.1.19",
          "form": "저녁을 못 먹었어요",
          "original_form": "저녁을 못 먹었어요",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100030880.1.20",
          "form": "왜 못 먹었나요ㅜ",
          "original_form": "왜 못 먹었나요ㅜ",
          "speaker_id": "1",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100030880.1.21",
          "form": "처가집 다녀와서 피곤하셨나봐요ㅜㅇㅜ",
          "original_form": "처가집 다녀와서 피곤하셨나봐요ㅜㅇㅜ",
          "speaker_id": "1",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100030880.1.22",
          "form": "둘다 자기 할거한다고 ㅋㅋ",
          "original_form": "둘다 자기 할거한다고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100030880.1.23",
          "form": "저는 머리 깍고",
          "original_form": "저는 머리 깍고",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100030880.1.24",
          "form": "와이프는 네일했서요 ㅋㅋ",
          "original_form": "와이프는 네일했서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100030880.1.25",
          "form": "그래서 피곤한듯 ㅋㅋ",
          "original_form": "그래서 피곤한듯 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:07"
        },
        {
          "id": "MDRW2100030880.1.26",
          "form": "오늘 일 많으셨습니까",
          "original_form": "오늘 일 많으셨습니까",
          "speaker_id": "2",
          "time": "20210905 21:07"
        },
        {
          "id": "MDRW2100030880.1.27",
          "form": "마감때문에 가신거세요?",
          "original_form": "마감때문에 가신거세요?",
          "speaker_id": "2",
          "time": "20210905 21:07"
        },
        {
          "id": "MDRW2100030880.1.28",
          "form": "네ㅎㅎ 월요일에 신문 나오니",
          "original_form": "네ㅎㅎ 월요일에 신문 나오니",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100030880.1.29",
          "form": "조금 엇박자로 일합니다",
          "original_form": "조금 엇박자로 일합니다",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100030880.1.30",
          "form": "머리는 커트만 하신 건가요?",
          "original_form": "머리는 커트만 하신 건가요?",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100030880.1.31",
          "form": "아 고생하셨습니다",
          "original_form": "아 고생하셨습니다",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100030880.1.32",
          "form": "뽁았는데여",
          "original_form": "뽁았는데여",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100030880.1.33",
          "form": "아줌마같습니다",
          "original_form": "아줌마같습니다",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100030880.1.34",
          "form": "제가 약간 아줌마 상인데",
          "original_form": "제가 약간 아줌마 상인데",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100030880.1.35",
          "form": "자아성찰좀 하고올게요",
          "original_form": "자아성찰좀 하고올게요",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100030880.1.36",
          "form": "앜",
          "original_form": "앜",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100030880.1.37",
          "form": "악!",
          "original_form": "악!",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100030880.1.38",
          "form": "아ㅋㅋㅋ 사진 보내기 기능 언제 되남요ㅋㅋ",
          "original_form": "아ㅋㅋㅋ 사진 보내기 기능 언제 되남요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100030880.1.39",
          "form": "제가 다이어트 시켜드리는건가요?",
          "original_form": "제가 다이어트 시켜드리는건가요?",
          "speaker_id": "2",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100030880.1.40",
          "form": "입맛 안드로메다인데",
          "original_form": "입맛 안드로메다인데",
          "speaker_id": "2",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100030880.1.41",
          "form": "보시면 ㅋㅋㅋㅋ",
          "original_form": "보시면 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100030880.1.42",
          "form": "사진 보내기",
          "original_form": "사진 보내기",
          "speaker_id": "2",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100030880.1.43",
          "form": "된대요?",
          "original_form": "된대요?",
          "speaker_id": "2",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100030880.1.44",
          "form": "아 진짜 됐으마 좋것는데",
          "original_form": "아 진짜 됐으마 좋것는데",
          "speaker_id": "2",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100030880.1.45",
          "form": "안해줄드싶어요",
          "original_form": "안해줄드싶어요",
          "speaker_id": "2",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100030880.1.46",
          "form": "식사는 하셨서용?",
          "original_form": "식사는 하셨서용?",
          "speaker_id": "2",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100030880.1.47",
          "form": "지금 귀가중이요",
          "original_form": "지금 귀가중이요",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100030880.1.48",
          "form": "에너지바 먹음서 일해서ㅎㅇ",
          "original_form": "에너지바 먹음서 일해서ㅎㅇ",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100030880.1.49",
          "form": "이제 집가서 포식할 거예요!!",
          "original_form": "이제 집가서 포식할 거예요!!",
          "speaker_id": "1",
          "time": "20210905 21:24"
        }
      ]
