        },
        {
          "id": "MDRW2100014998.1.18",
          "form": "name1님은 구먀하고싶은거 있으세요?",
          "original_form": "&name1&님은 구먀하고싶은거 있으세요?",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100014998.1.19",
          "form": "ㅋㅋㅋㅋ괜히 한번 찜목록 보고왔네용",
          "original_form": "ㅋㅋㅋㅋ괜히 한번 찜목록 보고왔네용",
