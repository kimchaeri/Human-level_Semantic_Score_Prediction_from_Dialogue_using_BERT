      "utterance": [
        {
          "id": "MDRW2100014998.1.1",
          "form": "요즘 사고싶으신거 있으세요?",
          "original_form": "요즘 사고싶으신거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014998.1.2",
          "form": "저 요즘...음...스탭퍼 사고싶더라고요",
          "original_form": "저 요즘...음...스탭퍼 사고싶더라고요",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014998.1.3",
          "form": "헬스 나가기 귀찮아서",
          "original_form": "헬스 나가기 귀찮아서",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014998.1.4",
          "form": "근데 손잡이도 있는걸로요",
          "original_form": "근데 손잡이도 있는걸로요",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014998.1.5",
          "form": "앜ㅋㅋㅋㅋ맞아요!! 그거있으면 왠지 홈트도 잘 할 수 있을것같고...",
          "original_form": "앜ㅋㅋㅋㅋ맞아요!! 그거있으면 왠지 홈트도 잘 할 수 있을것같고...",
          "speaker_id": "1",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100014998.1.6",
          "form": "요즘은 워킹패드라고 런닝머신의 축소판?같이 나왔더라구요",
          "original_form": "요즘은 워킹패드라고 런닝머신의 축소판?같이 나왔더라구요",
          "speaker_id": "1",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100014998.1.7",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100014998.1.8",
          "form": "헐 아아아 그 속도 5까지만 되는 그거",
          "original_form": "헐 아아아 그 속도 5까지만 되는 그거",
          "speaker_id": "2",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100014998.1.9",
          "form": "맞아요 그것도 사고싶었어요",
          "original_form": "맞아요 그것도 사고싶었어요",
          "speaker_id": "2",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100014998.1.10",
          "form": "저번에 운동하려고 아령 사려했다가 귀차니즘으로 안샀었거든요",
          "original_form": "저번에 운동하려고 아령 사려했다가 귀차니즘으로 안샀었거든요",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100014998.1.11",
          "form": "근데 아령에서 발암물질들이 나왔다대요?",
          "original_form": "근데 아령에서 발암물질들이 나왔다대요?",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100014998.1.12",
          "form": "조심하세요",
          "original_form": "조심하세요",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100014998.1.13",
          "form": "헉 아령에서 발암물질이라니",
          "original_form": "헉 아령에서 발암물질이라니",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100014998.1.14",
          "form": "아령살생각은 없지만",
          "original_form": "아령살생각은 없지만",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100014998.1.15",
          "form": "왠지 더 안살것같군용ㅎㅎ",
          "original_form": "왠지 더 안살것같군용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100014998.1.16",
          "form": "저도요 갑자기",
          "original_form": "저도요 갑자기",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100014998.1.17",
          "form": "살생각 없어젺어욬ㅋㅋㅋ",
          "original_form": "살생각 없어젺어욬ㅋㅋㅋ",
