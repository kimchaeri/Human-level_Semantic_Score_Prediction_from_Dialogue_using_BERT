      "utterance": [
        {
          "id": "MDRW2100035414.1.1",
          "form": "유투브 보시나요?",
          "original_form": "유투브 보시나요?",
          "speaker_id": "1",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100035414.1.2",
          "form": "잘 안봐요! 많이 보세요?",
          "original_form": "잘 안봐요! 많이 보세요?",
          "speaker_id": "2",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100035414.1.3",
          "form": "음 저는 요리나 운동채널은 한번씩 봥ㅅ",
          "original_form": "음 저는 요리나 운동채널은 한번씩 봥ㅅ",
          "speaker_id": "1",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100035414.1.4",
          "form": "봐요",
          "original_form": "봐요",
          "speaker_id": "1",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100035414.1.5",
          "form": "아 저는 동물이나 뜨개 관련 가끔 봐요 ㅋㅋ",
          "original_form": "아 저는 동물이나 뜨개 관련 가끔 봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100035414.1.6",
          "form": "오 그러시는군요^^ 그럼 영화/드라마 중 뭐 좋아하시나요",
          "original_form": "오 그러시는군요^^ 그럼 영화/드라마 중 뭐 좋아하시나요",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100035414.1.7",
          "form": "영화만 쭉 보다가 요즘엔 드라마만 봐요 ㅋㅋㅋ",
          "original_form": "영화만 쭉 보다가 요즘엔 드라마만 봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100035414.1.8",
          "form": "어떤 드라마 보시나요?",
          "original_form": "어떤 드라마 보시나요?",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100035414.1.9",
          "form": "요즘 오월의 청춘 보는중이에요!",
          "original_form": "요즘 오월의 청춘 보는중이에요!",
          "speaker_id": "2",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100035414.1.10",
          "form": "꺅...저 완전 팬이예요ㅠㅠㅠ",
          "original_form": "꺅...저 완전 팬이예요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100035414.1.11",
          "form": "지금 11회까지 봤어요... ㅜㅜ 너무슬퍼요",
          "original_form": "지금 11회까지 봤어요... ㅜㅜ 너무슬퍼요",
          "speaker_id": "2",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100035414.1.12",
          "form": "저 올해 최애 드라마예요..앞으로도 그럴것 같고..",
          "original_form": "저 올해 최애 드라마예요..앞으로도 그럴것 같고..",
          "speaker_id": "1",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100035414.1.13",
          "form": "아픈 과거라도 자꾸 얘기를 하고 꺼냐야 잊지 않을거같아요 ㅜ",
          "original_form": "아픈 과거라도 자꾸 얘기를 하고 꺼냐야 잊지 않을거같아요 ㅜ",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100035414.1.14",
          "form": "맞아요 드라마로 다뤄주니 넘 좋더라구요",
