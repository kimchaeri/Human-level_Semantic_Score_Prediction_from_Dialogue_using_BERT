      "utterance": [
        {
          "id": "MDRW2100007763.1.1",
          "form": "언니 이제 잘한데이~",
          "original_form": "언니 이제 잘한데이~",
          "speaker_id": "2",
          "time": "20210822 10:18"
        },
        {
          "id": "MDRW2100007763.1.2",
          "form": "노력하고 있다 ㅋㅋㅋㅋㄱㄱㅋ",
          "original_form": "노력하고 있다 ㅋㅋㅋㅋㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007763.1.3",
          "form": "name3 밥 잘먹나??",
          "original_form": "&name3& 밥 잘먹나??",
          "speaker_id": "1",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007763.1.4",
          "form": "우리애들은 지금 아빠한테 밥먹으란 소리",
          "original_form": "우리애들은 지금 아빠한테 밥먹으란 소리",
          "speaker_id": "1",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007763.1.5",
          "form": "백번 들으면서 밥 먹고 있네",
          "original_form": "백번 들으면서 밥 먹고 있네",
          "speaker_id": "1",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007763.1.6",
          "form": "집에선 잘 안먹어서 아침도 안줘요~",
          "original_form": "집에선 잘 안먹어서 아침도 안줘요~",
          "speaker_id": "2",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007763.1.7",
          "form": "name4이는 뭐 식욕없고 ㅋㅋ",
          "original_form": "&name4&이는 뭐 식욕없고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007763.1.8",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007763.1.9",
          "form": "니 답다",
          "original_form": "니 답다",
          "speaker_id": "1",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007763.1.10",
          "form": "할매한테만 오면 밥 두그릇 ㅋㅋ",
          "original_form": "할매한테만 오면 밥 두그릇 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007763.1.11",
          "form": "그래 우리집도 식욕이 없다",
          "original_form": "그래 우리집도 식욕이 없다",
          "speaker_id": "1",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007763.1.12",
          "form": "할매집 음식이 입에 맞나 보네",
          "original_form": "할매집 음식이 입에 맞나 보네",
          "speaker_id": "1",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007763.1.13",
          "form": "아침 초코우유한잔하고 미소로 ㅋㅋ",
          "original_form": "아침 초코우유한잔하고 미소로 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007763.1.14",
          "form": "니가 한게 맛 없나보다",
          "original_form": "니가 한게 맛 없나보다",
          "speaker_id": "1",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007763.1.15",
          "form": "ㅋㅋㅋㄱ 그래도 미소가믄",
          "original_form": "ㅋㅋㅋㄱ 그래도 미소가믄",
          "speaker_id": "1",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007763.1.16",
          "form": "든든하게먹고 오니",
          "original_form": "든든하게먹고 오니",
          "speaker_id": "1",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007763.1.17",
          "form": "걱정없다",
          "original_form": "걱정없다",
          "speaker_id": "1",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007763.1.18",
          "form": "그쵸~name5이가 걱정이여요~",
          "original_form": "그쵸~&name5&이가 걱정이여요~",
          "speaker_id": "2",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007763.1.19",
          "form": "난 결국 유치원에 점심 너무 맛없다고 얘기했다",
          "original_form": "난 결국 유치원에 점심 너무 맛없다고 얘기했다",
          "speaker_id": "1",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007763.1.20",
          "form": "급식도 올캐 먹는동 우짠동",
          "original_form": "급식도 올캐 먹는동 우짠동",
          "speaker_id": "2",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007763.1.21",
          "form": "학교 급식은 반찬이 너무 잘 나오던데",
          "original_form": "학교 급식은 반찬이 너무 잘 나오던데",
          "speaker_id": "1",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007763.1.22",
          "form": "미소도 근데 예전같진 않아서~",
          "original_form": "미소도 근데 예전같진 않아서~",
          "speaker_id": "2",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007763.1.23",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007763.1.24",
          "form": "왜그래 예전같지않노",
          "original_form": "왜그래 예전같지않노",
          "speaker_id": "1",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007763.1.25",
          "form": "name5이는 늦게 먹어서",
          "original_form": "&name5&이는 늦게 먹어서",
          "speaker_id": "2",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007763.1.26",
          "form": "시간 맞춰먹는게 힘든가봐여~",
          "original_form": "시간 맞춰먹는게 힘든가봐여~",
          "speaker_id": "2",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007763.1.27",
          "form": "아 그래 시간이 있으니",
          "original_form": "아 그래 시간이 있으니",
          "speaker_id": "1",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007763.1.28",
          "form": "맨날 김치에 비벼먹었다고..",
          "original_form": "맨날 김치에 비벼먹었다고..",
          "speaker_id": "2",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007763.1.29",
          "form": "애들이 맞춰먹기 힘들지",
          "original_form": "애들이 맞춰먹기 힘들지",
          "speaker_id": "1",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007763.1.30",
          "form": "name6이는 맛있데요??",
          "original_form": "&name6&이는 맛있데요??",
          "speaker_id": "2",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007763.1.31",
          "form": "아 진짜?? ㅠㅠ 우째",
          "original_form": "아 진짜?? ㅠㅠ 우째",
          "speaker_id": "1",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007763.1.32",
          "form": "엉 name6이는 학교밥 엄청 맛있다고 하대",
          "original_form": "엉 &name6&이는 학교밥 엄청 맛있다고 하대",
          "speaker_id": "1",
          "time": "20210822 10:23"
        },
        {
          "id": "MDRW2100007763.1.33",
          "form": "유치원은 드럽게 맛없어서 맨날 남아서 혼자먹고 했거든",
          "original_form": "유치원은 드럽게 맛없어서 맨날 남아서 혼자먹고 했거든",
          "speaker_id": "1",
          "time": "20210822 10:23"
        },
        {
          "id": "MDRW2100007763.1.34",
          "form": "학교에서는 시간안에 다 먹는다고 하더라고",
          "original_form": "학교에서는 시간안에 다 먹는다고 하더라고",
          "speaker_id": "1",
          "time": "20210822 10:23"
        },
        {
          "id": "MDRW2100007763.1.35",
          "form": "ㅋㅋㅋㅋㅋ아욱겨라~",
          "original_form": "ㅋㅋㅋㅋㅋ아욱겨라~",
          "speaker_id": "2",
          "time": "20210822 10:23"
        },
        {
          "id": "MDRW2100007763.1.36",
          "form": "반찬남겨도 뭐라하는사람이 없으니",
          "original_form": "반찬남겨도 뭐라하는사람이 없으니",
          "speaker_id": "1",
          "time": "20210822 10:23"
        }
      ]
