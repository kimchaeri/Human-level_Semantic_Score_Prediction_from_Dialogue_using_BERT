      "utterance": [
        {
          "id": "MDRW2100018677.1.1",
          "form": "쇼핑 자주 하세요?",
          "original_form": "쇼핑 자주 하세요?",
          "speaker_id": "2",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100018677.1.2",
          "form": "음 애들도 있고 하루에 하나씩 살게생겨요",
          "original_form": "음 애들도 있고 하루에 하나씩 살게생겨요",
          "speaker_id": "1",
          "time": "20210827 10:35"
        },
        {
          "id": "MDRW2100018677.1.3",
          "form": "자주사요 ㅎㅎ",
          "original_form": "자주사요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 10:35"
        },
        {
          "id": "MDRW2100018677.1.4",
          "form": "오늘도 모기약도 사고 ㅎㅎ",
          "original_form": "오늘도 모기약도 사고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100018677.1.5",
          "form": "아 애들이 있으면",
          "original_form": "아 애들이 있으면",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018677.1.6",
          "form": "그럴 수 있죠",
          "original_form": "그럴 수 있죠",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018677.1.7",
          "form": "요새 여름에 없던 모기가 갑자기 비오고",
          "original_form": "요새 여름에 없던 모기가 갑자기 비오고",
          "speaker_id": "1",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018677.1.8",
          "form": "나타났네요",
          "original_form": "나타났네요",
          "speaker_id": "1",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018677.1.9",
          "form": "쇼핑 자주 하세요?",
          "original_form": "쇼핑 자주 하세요?",
          "speaker_id": "1",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018677.1.10",
          "form": "어 맞다 여기도",
          "original_form": "어 맞다 여기도",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018677.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018677.1.12",
          "form": "갑자기 한두마리가 ㅎㅎ",
          "original_form": "갑자기 한두마리가 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018677.1.13",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018677.1.14",
          "form": "저는 가끔 11번가나",
          "original_form": "저는 가끔 11번가나",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018677.1.15",
          "form": "지마켓 가서",
          "original_form": "지마켓 가서",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018677.1.16",
          "form": "모기장도 꺼냈어요",
          "original_form": "모기장도 꺼냈어요",
          "speaker_id": "1",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018677.1.17",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018677.1.18",
          "form": "베스트 보고 특가다 싶으면 사요",
          "original_form": "베스트 보고 특가다 싶으면 사요",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018677.1.19",
          "form": "저도 그러는데 ㅋㅋ",
          "original_form": "저도 그러는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018677.1.20",
          "form": "막 필요해서 사는거보단 쌀때다 싶으면 사는듯해요",
          "original_form": "막 필요해서 사는거보단 쌀때다 싶으면 사는듯해요",
          "speaker_id": "2",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018677.1.21",
          "form": "보면 안살수 없어요 ㅋㅋ",
          "original_form": "보면 안살수 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018677.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018677.1.23",
          "form": "그때 안사면 안될듯해요 ㅋㅋ",
          "original_form": "그때 안사면 안될듯해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018677.1.24",
          "form": "저 위메프 12시 ㅋㅋ달리기도 했었는데",
          "original_form": "저 위메프 12시 ㅋㅋ달리기도 했었는데",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018677.1.25",
          "form": "요새 안했네요",
          "original_form": "요새 안했네요",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018677.1.26",
          "form": "쿠팡은 와우하니 로켓 무배고 빨리와서",
          "original_form": "쿠팡은 와우하니 로켓 무배고 빨리와서",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018677.1.27",
