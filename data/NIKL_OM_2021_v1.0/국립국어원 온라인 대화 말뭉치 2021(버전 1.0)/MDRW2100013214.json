          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013214.1.6",
          "form": "냉장고에 넣어두면대지?",
          "original_form": "냉장고에 넣어두면대지?",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013214.1.7",
          "form": "옹옹",
          "original_form": "옹옹",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013214.1.8",
          "form": "집가서",
          "original_form": "집가서",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013214.1.9",
          "form": "얼른 먹어야집",
          "original_form": "얼른 먹어야집",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013214.1.10",
          "form": "나도나도",
          "original_form": "나도나도",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013214.1.11",
          "form": "빨리먹고싶우",
          "original_form": "빨리먹고싶우",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013214.1.12",
          "form": "한번도 안먹어봐서",
          "original_form": "한번도 안먹어봐서",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013214.1.13",
          "form": "기대돼",
          "original_form": "기대돼",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013214.1.14",
          "form": "ㅋㅋㅋ마저",
          "original_form": "ㅋㅋㅋ마저",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013214.1.15",
          "form": "첨시켜보는거자나",
          "original_form": "첨시켜보는거자나",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013214.1.16",
          "form": "유가사탕 맛일까?",
          "original_form": "유가사탕 맛일까?",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013214.1.17",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013214.1.18",
          "form": "비슷하거나 아예다르거나",
          "original_form": "비슷하거나 아예다르거나",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013214.1.19",
          "form": "그렁거같은데",
          "original_form": "그렁거같은데",
          "speaker_id": "2",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013214.1.20",
          "form": "몇개잇을까?",
          "original_form": "몇개잇을까?",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013214.1.21",
          "form": "너무 적으면 실망이여",
          "original_form": "너무 적으면 실망이여",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013214.1.22",
          "form": "마자 적당해야돼",
          "original_form": "마자 적당해야돼",
          "speaker_id": "2",
          "time": "20210825 13:34"
        },
