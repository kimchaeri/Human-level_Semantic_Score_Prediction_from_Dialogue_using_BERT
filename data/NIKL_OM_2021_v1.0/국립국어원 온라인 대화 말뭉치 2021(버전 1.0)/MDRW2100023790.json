      "utterance": [
        {
          "id": "MDRW2100023790.1.1",
          "form": "단걸 좋아하는 편이야?",
          "original_form": "단걸 좋아하는 편이야?",
          "speaker_id": "2",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100023790.1.2",
          "form": "단 거 좋지",
          "original_form": "단 거 좋지",
          "speaker_id": "1",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100023790.1.3",
          "form": "나중에 당뇨 생기는거 아니야??;;",
          "original_form": "나중에 당뇨 생기는거 아니야??;;",
          "speaker_id": "2",
          "time": "20210830 17:38"
        },
        {
          "id": "MDRW2100023790.1.4",
          "form": "아이 적당히 먹지",
          "original_form": "아이 적당히 먹지",
          "speaker_id": "1",
          "time": "20210830 17:38"
        },
        {
          "id": "MDRW2100023790.1.5",
          "form": "그럼 다행이고",
          "original_form": "그럼 다행이고",
          "speaker_id": "2",
          "time": "20210830 17:38"
        },
        {
          "id": "MDRW2100023790.1.6",
          "form": "무슨 어플 먹고 싶은데?",
          "original_form": "무슨 어플 먹고 싶은데?",
          "speaker_id": "1",
          "time": "20210830 17:38"
        },
        {
          "id": "MDRW2100023790.1.7",
          "form": "와플 종류 많잖아",
          "original_form": "와플 종류 많잖아",
          "speaker_id": "2",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100023790.1.8",
          "form": "그러니까 안에 토핑 뭐 넣어 먹고 싶냐고",
          "original_form": "그러니까 안에 토핑 뭐 넣어 먹고 싶냐고",
          "speaker_id": "1",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100023790.1.9",
          "form": "토핑 뭐 있는데?",
          "original_form": "토핑 뭐 있는데?",
          "speaker_id": "2",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100023790.1.10",
          "form": "그냥 보통 대중적인 거",
          "original_form": "그냥 보통 대중적인 거",
          "speaker_id": "1",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100023790.1.11",
          "form": "대중적인게 뭔데?",
          "original_form": "대중적인게 뭔데?",
          "speaker_id": "2",
          "time": "20210830 17:40"
        },
        {
          "id": "MDRW2100023790.1.12",
          "form": "생크림 들어가는 경우도 있고 누텔라 바르는 경우도 있고 과일 넣는 경우도 있고 다 집어넣는 경우도 있어",
          "original_form": "생크림 들어가는 경우도 있고 누텔라 바르는 경우도 있고 과일 넣는 경우도 있고 다 집어넣는 경우도 있어",
          "speaker_id": "1",
          "time": "20210830 17:40"
        },
        {
          "id": "MDRW2100023790.1.13",
          "form": "생크림 좋아해 과일이랑",
          "original_form": "생크림 좋아해 과일이랑",
          "speaker_id": "2",
          "time": "20210830 17:40"
        },
        {
          "id": "MDRW2100023790.1.14",
          "form": "예를 들면 생크림에 딸기 딸기쨈 생딸기 토핑",
          "original_form": "예를 들면 생크림에 딸기 딸기쨈 생딸기 토핑",
          "speaker_id": "1",
          "time": "20210830 17:40"
        },
        {
          "id": "MDRW2100023790.1.15",
          "form": "오 맛있겠다",
          "original_form": "오 맛있겠다",
          "speaker_id": "2",
          "time": "20210830 17:41"
        },
        {
          "id": "MDRW2100023790.1.16",
          "form": "와플 토핑엔 여러 조합이 있어",
          "original_form": "와플 토핑엔 여러 조합이 있어",
          "speaker_id": "1",
          "time": "20210830 17:41"
        }
