      "utterance": [
        {
          "id": "MDRW2100026760.1.1",
          "form": "식음료겠죠? ㅎㅎ",
          "original_form": "식음료겠죠? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026760.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026760.1.3",
          "form": "저 가자미조림 먹었어요",
          "original_form": "저 가자미조림 먹었어요",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026760.1.4",
          "form": "와 직접 요리하신거예요?",
          "original_form": "와 직접 요리하신거예요?",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026760.1.5",
          "form": "네 해먹었어요!!",
          "original_form": "네 해먹었어요!!",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026760.1.6",
          "form": "생각보다 간단하게 돼요",
          "original_form": "생각보다 간단하게 돼요",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026760.1.7",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026760.1.8",
          "form": "그냥 웬만한건 해먹는거 같아요 ㅎㅎ 헬로님은 뭐 드셨어요??",
          "original_form": "그냥 웬만한건 해먹는거 같아요 ㅎㅎ 헬로님은 뭐 드셨어요??",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026760.1.9",
          "form": "저는 파스타요",
          "original_form": "저는 파스타요",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026760.1.10",
          "form": "맛있었겠어요",
          "original_form": "맛있었겠어요",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026760.1.11",
          "form": "토마토 오일 크림??",
          "original_form": "토마토 오일 크림??",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026760.1.12",
          "form": "로제요 ㅎㅎ",
          "original_form": "로제요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026760.1.13",
          "form": "시판소스 샀어요",
          "original_form": "시판소스 샀어요",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026760.1.14",
          "form": "ㅋㅋㅋ 빗나갔네여",
          "original_form": "ㅋㅋㅋ 빗나갔네여",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026760.1.15",
          "form": "아숩",
          "original_form": "아숩",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026760.1.16",
          "form": "맛있으셨겠다",
          "original_form": "맛있으셨겠다",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026760.1.17",
          "form": "어디꺼로 해드셨어요?",
          "original_form": "어디꺼로 해드셨어요?",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026760.1.18",
          "form": "요즘 시판소스 넘 잘나와요",
          "original_form": "요즘 시판소스 넘 잘나와요",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026760.1.19",
          "form": "백설이요",
          "original_form": "백설이요",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026760.1.20",
          "form": "맞아요 그냥 소스만 넣어도 맛있어서 좋아요 간단하고",
          "original_form": "맞아요 그냥 소스만 넣어도 맛있어서 좋아요 간단하고",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026760.1.21",
          "form": "사실 똥손이라 요리 잼병인데",
          "original_form": "사실 똥손이라 요리 잼병인데",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026760.1.22",
          "form": "백설 맛있죠 전 청정원 로제소스도 좋아해요 맛있어요",
          "original_form": "백설 맛있죠 전 청정원 로제소스도 좋아해요 맛있어요",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026760.1.23",
          "form": "파스타는 소스의 힘을 빌릴수있어서 ㅎㅎ",
          "original_form": "파스타는 소스의 힘을 빌릴수있어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026760.1.24",
          "form": "오이 벌써 이일이네옄ㅋㅋㅋ",
          "original_form": "오이 벌써 이일이네옄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026760.1.25",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026760.1.26",
          "form": "그러네요 ㅋㅋ",
          "original_form": "그러네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026760.1.27",
          "form": "맞아요 소스의 힘을 빌리면 다 돼요!!!",
          "original_form": "맞아요 소스의 힘을 빌리면 다 돼요!!!",
