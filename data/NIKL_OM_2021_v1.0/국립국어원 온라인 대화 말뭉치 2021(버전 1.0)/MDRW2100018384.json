        {
          "id": "MDRW2100018384.1.1",
          "form": "name1님은",
          "original_form": "&name1&님은",
          "speaker_id": "2",
          "time": "20210827 01:01"
        },
        {
          "id": "MDRW2100018384.1.2",
          "form": "알바는 뭐 해본거 있으세용?",
          "original_form": "알바는 뭐 해본거 있으세용?",
          "speaker_id": "2",
          "time": "20210827 01:01"
        },
        {
          "id": "MDRW2100018384.1.3",
          "form": "음 저는 알바를 많이 안해봤어요",
          "original_form": "음 저는 알바를 많이 안해봤어요",
          "speaker_id": "1",
          "time": "20210827 01:16"
        },
        {
          "id": "MDRW2100018384.1.4",
          "form": "횟집 홀알바랑",
          "original_form": "횟집 홀알바랑",
          "speaker_id": "1",
          "time": "20210827 01:16"
        },
        {
          "id": "MDRW2100018384.1.5",
          "form": "최근에 도서관 근로 했었어요",
          "original_form": "최근에 도서관 근로 했었어요",
          "speaker_id": "1",
          "time": "20210827 01:16"
        },
        {
          "id": "MDRW2100018384.1.6",
          "form": "헉 근데 횟집 홀 알바는 왠지 힘들거같은데요?",
          "original_form": "헉 근데 횟집 홀 알바는 왠지 힘들거같은데요?",
          "speaker_id": "2",
          "time": "20210827 01:24"
        },
        {
          "id": "MDRW2100018384.1.7",
          "form": "도서관 근로좋죠ㅎㅎ 저도 도서관 근로 해봤었어요",
          "original_form": "도서관 근로좋죠ㅎㅎ 저도 도서관 근로 해봤었어요",
          "speaker_id": "2",
          "time": "20210827 01:24"
        },
        {
          "id": "MDRW2100018384.1.8",
          "form": "name1님 대구 시청에서 매 분기별로 대구시청인턴 뽑는건 아세요?",
          "original_form": "&name1&님 대구 시청에서 매 분기별로 대구시청인턴 뽑는건 아세요?",
          "speaker_id": "2",
          "time": "20210827 01:24"
        },
        {
          "id": "MDRW2100018384.1.9",
          "form": "공공데이터 청년인턴 말씀하시는건가요?",
          "original_form": "공공데이터 청년인턴 말씀하시는건가요?",
          "speaker_id": "1",
          "time": "20210827 01:25"
        },
        {
          "id": "MDRW2100018384.1.10",
          "form": "앗 그런것도 있나요?",
          "original_form": "앗 그런것도 있나요?",
          "speaker_id": "2",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018384.1.11",
          "form": "그거 말고 대구 시청에서 분기별로 뽑는거있어요",
          "original_form": "그거 말고 대구 시청에서 분기별로 뽑는거있어요",
          "speaker_id": "2",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018384.1.12",
          "form": "방학때랑 학기중에요",
          "original_form": "방학때랑 학기중에요",
          "speaker_id": "2",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018384.1.13",
          "form": "인턴뽑는게 이거 아닌가요?",
          "original_form": "인턴뽑는게 이거 아닌가요?",
          "speaker_id": "1",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018384.1.14",
          "form": "어떤건지 궁금해요",
          "original_form": "어떤건지 궁금해요",
          "speaker_id": "1",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018384.1.15",
          "form": "그 대구시청 블로그 가시면 매번 공고 글 올라오는데요",
          "original_form": "그 대구시청 블로그 가시면 매번 공고 글 올라오는데요",
          "speaker_id": "2",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018384.1.16",
          "form": "대구에 공공기관들 중에 하나 선택해서",
          "original_form": "대구에 공공기관들 중에 하나 선택해서",
          "speaker_id": "2",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018384.1.17",
          "form": "가셔서 인턴 할 수 있어요!",
          "original_form": "가셔서 인턴 할 수 있어요!",
          "speaker_id": "2",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018384.1.18",
          "form": "제때는 대부분 도서관이였는데 요즘은 기관들이 진짜 다양하게 있더라구용!",
          "original_form": "제때는 대부분 도서관이였는데 요즘은 기관들이 진짜 다양하게 있더라구용!",
          "speaker_id": "2",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018384.1.19",
          "form": "아하 제가 말한 청년인턴 맞는거같네여",
          "original_form": "아하 제가 말한 청년인턴 맞는거같네여",
          "speaker_id": "1",
          "time": "20210827 01:50"
        },
        {
          "id": "MDRW2100018384.1.20",
          "form": "이번에 열린게 공공데이터 관련 기관이라",
          "original_form": "이번에 열린게 공공데이터 관련 기관이라",
          "speaker_id": "1",
          "time": "20210827 01:50"
        },
        {
          "id": "MDRW2100018384.1.21",
          "form": "근데 학기중에 할 수 있을지 몰라서",
          "original_form": "근데 학기중에 할 수 있을지 몰라서",
          "speaker_id": "1",
          "time": "20210827 01:50"
        },
        {
          "id": "MDRW2100018384.1.22",
          "form": "공고만보고 그냥 신청안했거든요",
          "original_form": "공고만보고 그냥 신청안했거든요",
          "speaker_id": "1",
          "time": "20210827 01:50"
        },
        {
          "id": "MDRW2100018384.1.23",
          "form": "학기중에도 가능할 정도인가요?",
          "original_form": "학기중에도 가능할 정도인가요?",
          "speaker_id": "1",
          "time": "20210827 01:51"
        },
        {
          "id": "MDRW2100018384.1.24",
          "form": "아하~~~ 이번엔 공공데이터로 바꼈나보네요!",
          "original_form": "아하~~~ 이번엔 공공데이터로 바꼈나보네요!",
          "speaker_id": "2",
          "time": "20210827 01:51"
        },
        {
          "id": "MDRW2100018384.1.25",
          "form": "사실 제가 최신 소식은 잘 알지못해요... 이미 졸업한 화석이라 ㅋㅋㅋㅋ",
          "original_form": "사실 제가 최신 소식은 잘 알지못해요... 이미 졸업한 화석이라 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:51"
        },
        {
          "id": "MDRW2100018384.1.26",
          "form": "전 도서관에서 했는데 일이 그렇게 힘들진 않았어요",
          "original_form": "전 도서관에서 했는데 일이 그렇게 힘들진 않았어요",
          "speaker_id": "2",
          "time": "20210827 01:51"
        },
        {
          "id": "MDRW2100018384.1.27",
          "form": "근데 학기중엔 수업 들어야해서 못하지않을까요?",
          "original_form": "근데 학기중엔 수업 들어야해서 못하지않을까요?",
          "speaker_id": "2",
          "time": "20210827 01:52"
        },
        {
          "id": "MDRW2100018384.1.28",
          "form": "사실 학기중인 교외근로 붙기를 원했는데",
          "original_form": "사실 학기중인 교외근로 붙기를 원했는데",
          "speaker_id": "1",
          "time": "20210827 01:52"
        },
        {
          "id": "MDRW2100018384.1.29",
          "form": "이번엔 떨어진거같아요",
          "original_form": "이번엔 떨어진거같아요",
          "speaker_id": "1",
          "time": "20210827 01:52"
        },
        {
          "id": "MDRW2100018384.1.30",
          "form": "학교에 예산도 부족해서",
          "original_form": "학교에 예산도 부족해서",
          "speaker_id": "1",
          "time": "20210827 01:53"
        },
        {
          "id": "MDRW2100018384.1.31",
          "form": "일자리가 400개 중에 300개가 사라져서 ㅋㅋ..",
          "original_form": "일자리가 400개 중에 300개가 사라져서 ㅋㅋ..",
          "speaker_id": "1",
          "time": "20210827 01:53"
        },
        {
          "id": "MDRW2100018384.1.32",
          "form": "헐????",
          "original_form": "헐????",
          "speaker_id": "2",
          "time": "20210827 01:56"
        },
        {
          "id": "MDRW2100018384.1.33",
          "form": "일자리가 원래 400개였는데 300개나 없어졌다구요???헐 왜요??ㅠㅠㅠㅠ",
          "original_form": "일자리가 원래 400개였는데 300개나 없어졌다구요???헐 왜요??ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018384.1.34",
          "form": "방학중에",
          "original_form": "방학중에",
          "speaker_id": "1",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018384.1.35",
          "form": "어떻게 반의반토막이 날수있죠...?",
          "original_form": "어떻게 반의반토막이 날수있죠...?",
          "speaker_id": "2",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018384.1.36",
          "form": "코로나 특별예산이랍시고",
          "original_form": "코로나 특별예산이랍시고",
          "speaker_id": "1",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018384.1.37",
          "form": "추가모집 두번이나했는데",
          "original_form": "추가모집 두번이나했는데",
          "speaker_id": "1",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018384.1.38",
          "form": "아 헐....ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 헐....ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018384.1.39",
          "form": "추가모집이 아니라",
          "original_form": "추가모집이 아니라",
          "speaker_id": "2",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018384.1.40",
          "form": "너무 막써버려서",
          "original_form": "너무 막써버려서",
          "speaker_id": "1",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018384.1.41",
          "form": "학기중에 해야하는걸",
          "original_form": "학기중에 해야하는걸",
          "speaker_id": "2",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018384.1.42",
          "form": "2학기 생각을 안했나봐요 ㅋㅋㅋ",
          "original_form": "2학기 생각을 안했나봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018384.1.43",
          "form": "댕겨쓴거네요;;",
          "original_form": "댕겨쓴거네요;;",
          "speaker_id": "2",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018384.1.44",
          "form": "욕 엄청 먹던데",
          "original_form": "욕 엄청 먹던데",
          "speaker_id": "1",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018384.1.45",
          "form": "무슨 카드 돌려먹기도 아니고",
          "original_form": "무슨 카드 돌려먹기도 아니고",
          "speaker_id": "2",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018384.1.46",
          "form": "너무 생각없네요 진짜로",
          "original_form": "너무 생각없네요 진짜로",
          "speaker_id": "2",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018384.1.47",
          "form": "그러게요 참 답답한게",
          "original_form": "그러게요 참 답답한게",
          "speaker_id": "1",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018384.1.48",
          "form": "교내근로는 그대론데",
          "original_form": "교내근로는 그대론데",
          "speaker_id": "1",
          "time": "20210827 01:58"
        },
        {
          "id": "MDRW2100018384.1.49",
          "form": "교외근로는 아예 없는 수준이라 ㅋㅋ",
          "original_form": "교외근로는 아예 없는 수준이라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:58"
        },
        {
          "id": "MDRW2100018384.1.50",
          "form": "근데 교외근로가 돈 훨 세잖아요ㅠ....",
          "original_form": "근데 교외근로가 돈 훨 세잖아요ㅠ....",
          "speaker_id": "2",
          "time": "20210827 01:58"
        },
        {
          "id": "MDRW2100018384.1.51",
          "form": "진짜 400개를 100개로 만들다니",
          "original_form": "진짜 400개를 100개로 만들다니",
          "speaker_id": "2",
          "time": "20210827 01:58"
        },
        {
          "id": "MDRW2100018384.1.52",
          "form": "타노스도 아니고 ㅋㅋ...",
          "original_form": "타노스도 아니고 ㅋㅋ...",
          "speaker_id": "2",
          "time": "20210827 01:58"
        },
        {
          "id": "MDRW2100018384.1.53",
          "form": "좀 충격이긴했죠",
          "original_form": "좀 충격이긴했죠",
          "speaker_id": "1",
          "time": "20210827 01:58"
        },
        {
          "id": "MDRW2100018384.1.54",
          "form": "제가 1학기부터 방학때까지",
          "original_form": "제가 1학기부터 방학때까지",
          "speaker_id": "1",
          "time": "20210827 01:58"
        },
        {
          "id": "MDRW2100018384.1.55",
          "form": "교외도서관근로했는데",
          "original_form": "교외도서관근로했는데",
          "speaker_id": "1",
          "time": "20210827 01:58"
        },
        {
          "id": "MDRW2100018384.1.56",
          "form": "관련지역 도서관 근로모집은",
          "original_form": "관련지역 도서관 근로모집은",
          "speaker_id": "1",
          "time": "20210827 01:59"
        },
        {
          "id": "MDRW2100018384.1.57",
          "form": "아예 다사라졌더라구여",
          "original_form": "아예 다사라졌더라구여",
          "speaker_id": "1",
          "time": "20210827 01:59"
        },
        {
          "id": "MDRW2100018384.1.58",
          "form": "아이고....ㅜㅜ",
          "original_form": "아이고....ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 02:00"
        },
        {
          "id": "MDRW2100018384.1.59",
          "form": "아직 교외근로 발표 안났나요?",
          "original_form": "아직 교외근로 발표 안났나요?",
          "speaker_id": "2",
          "time": "20210827 02:00"
