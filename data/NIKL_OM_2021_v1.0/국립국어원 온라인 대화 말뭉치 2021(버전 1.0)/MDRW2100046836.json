        {
          "id": "MDRW2100046836.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211105 13:00"
        },
        {
          "id": "MDRW2100046836.1.2",
          "form": "네 안녕하세용",
          "original_form": "네 안녕하세용",
          "speaker_id": "2",
          "time": "20211105 13:21"
        },
        {
          "id": "MDRW2100046836.1.3",
          "form": "최근에 재밌게 보신 콘텐츠가 있으신가요?",
          "original_form": "최근에 재밌게 보신 콘텐츠가 있으신가요?",
          "speaker_id": "2",
          "time": "20211105 13:21"
        },
        {
          "id": "MDRW2100046836.1.4",
          "form": "콘텐츠라면 넷플릭스에서 본것도 되는건가요??",
          "original_form": "콘텐츠라면 넷플릭스에서 본것도 되는건가요??",
          "speaker_id": "1",
          "time": "20211105 13:21"
        },
        {
          "id": "MDRW2100046836.1.5",
          "form": "네 그럼요 ㅎ",
          "original_form": "네 그럼요 ㅎ",
          "speaker_id": "2",
          "time": "20211105 13:21"
        },
        {
          "id": "MDRW2100046836.1.6",
          "form": "금쪽같은 내새끼랑 스카이캐슬 재밌게 보고있습니다 ㅎㅎ",
          "original_form": "금쪽같은 내새끼랑 스카이캐슬 재밌게 보고있습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211105 13:22"
        },
        {
          "id": "MDRW2100046836.1.7",
          "form": "아 스카이캐슬 지금 보시는군요!",
          "original_form": "아 스카이캐슬 지금 보시는군요!",
          "speaker_id": "2",
          "time": "20211105 13:22"
        },
        {
          "id": "MDRW2100046836.1.8",
          "form": "어디까지 보셨나요 ㅋㅋㅋㅋㅋ",
          "original_form": "어디까지 보셨나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 13:23"
        },
        {
          "id": "MDRW2100046836.1.9",
          "form": "전에 한번 봤고 또보는중인데 11화보고있습니다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전에 한번 봤고 또보는중인데 11화보고있습니다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211105 13:23"
        },
        {
          "id": "MDRW2100046836.1.10",
          "form": "아아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 다시 정주행 하시는거구나 ㅋㅋㅋㅋ 재밌죠 ㅎㄹ",
          "original_form": "아아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 다시 정주행 하시는거구나 ㅋㅋㅋㅋ 재밌죠 ㅎㄹ",
          "speaker_id": "2",
          "time": "20211105 13:23"
        },
        {
          "id": "MDRW2100046836.1.11",
          "form": "마지막 화만 아니었으면..",
          "original_form": "마지막 화만 아니었으면{emoji:…}{emoji:…}..",
          "speaker_id": "2",
          "time": "20211105 13:24"
        },
        {
          "id": "MDRW2100046836.1.12",
          "form": "정말 명작으로 남았을텐데 ㅜ",
          "original_form": "정말 명작으로 남았을텐데 ㅜ",
          "speaker_id": "2",
          "time": "20211105 13:24"
        },
        {
          "id": "MDRW2100046836.1.13",
          "form": "스카이캐슬에서 최애 캐릭터는",
          "original_form": "스카이캐슬에서 최애 캐릭터는",
          "speaker_id": "2",
          "time": "20211105 13:25"
        },
        {
          "id": "MDRW2100046836.1.14",
          "form": "누군가용",
          "original_form": "누군가용",
          "speaker_id": "2",
          "time": "20211105 13:25"
        },
        {
          "id": "MDRW2100046836.1.15",
          "form": "우주요 우주우주",
          "original_form": "우주요 우주우주",
          "speaker_id": "1",
          "time": "20211105 13:25"
        },
        {
          "id": "MDRW2100046836.1.16",
          "form": "아아 ㅋㅋㅋㅋㅋ 우주",
          "original_form": "아아 ㅋㅋㅋㅋㅋ 우주",
          "speaker_id": "2",
          "time": "20211105 13:25"
        },
        {
          "id": "MDRW2100046836.1.17",
          "form": "혹시 sf9 남팬이신가요?",
          "original_form": "혹시 sf9 남팬이신가요?",
          "speaker_id": "2",
          "time": "20211105 13:26"
