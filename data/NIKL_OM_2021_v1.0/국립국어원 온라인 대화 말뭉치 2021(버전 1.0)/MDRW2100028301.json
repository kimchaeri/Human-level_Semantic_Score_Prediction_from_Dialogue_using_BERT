          "time": "20210903 07:22"
        },
        {
          "id": "MDRW2100028301.1.11",
          "form": "엄 ㅜ있어요",
          "original_form": "엄 ㅜ있어요",
          "speaker_id": "1",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100028301.1.12",
          "form": "빡센 일인가 보군요ㅜㅜㅡ",
