        },
        {
          "id": "MDRW2100011970.1.2",
          "form": "지중해 날씨 진짜 경험해보고 싶다",
          "original_form": "지중해 날씨 진짜 경험해보고 싶다",
          "speaker_id": "2",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011970.1.3",
          "form": "근데 파리는 안가고 싶어",
          "original_form": "근데 파리는 안가고 싶어",
          "speaker_id": "2",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011970.1.4",
          "form": "쥐가 그렇게 많다더라",
          "original_form": "쥐가 그렇게 많다더라",
          "speaker_id": "2",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011970.1.5",
          "form": "프랑스??",
          "original_form": "프랑스??",
          "speaker_id": "1",
          "time": "20210824 17:57"
        },
        {
          "id": "MDRW2100011970.1.6",
          "form": "오마이갓...",
          "original_form": "오마이갓...",
          "speaker_id": "1",
          "time": "20210824 17:57"
        },
        {
          "id": "MDRW2100011970.1.7",
          "form": "그래..유럽에 피리부는사나이 동화가 괜히있는게 아니겠지",
          "original_form": "그래..유럽에 피리부는사나이 동화가 괜히있는게 아니겠지",
          "speaker_id": "1",
          "time": "20210824 17:57"
        },
        {
          "id": "MDRW2100011970.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄴㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄴㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011970.1.9",
          "form": "노숙자도 너무 많아서",
          "original_form": "노숙자도 너무 많아서",
          "speaker_id": "2",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011970.1.10",
          "form": "가게들 앞에 쇠 봉같은거 밖아둔대",
          "original_form": "가게들 앞에 쇠 봉같은거 밖아둔대",
          "speaker_id": "2",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011970.1.11",
          "form": "에펠탑이나 그런거 한번 보고 싶기는 한데",
          "original_form": "에펠탑이나 그런거 한번 보고 싶기는 한데",
          "speaker_id": "2",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011970.1.12",
          "form": "소매치기도 진짜많잖아",
          "original_form": "소매치기도 진짜많잖아",
          "speaker_id": "1",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011970.1.13",
          "form": "참..장단점이 뚜렷하네 ㅎㅎ",
          "original_form": "참..장단점이 뚜렷하네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011970.1.14",
          "form": "그 집시들도 많아서",
          "original_form": "그 집시들도 많아서",
          "speaker_id": "2",
          "time": "20210824 18:00"
        },
        {
          "id": "MDRW2100011970.1.15",
          "form": "애기 던지고 얼떨결에 애 받는 순간 몸 다 훑고 털어간대 ㄷㄷ",
          "original_form": "애기 던지고 얼떨결에 애 받는 순간 몸 다 훑고 털어간대 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011970.1.16",
          "form": "헐...너무하다",
          "original_form": "헐...너무하다",
          "speaker_id": "1",
          "time": "20210824 18:03"
        },
        {
          "id": "MDRW2100011970.1.17",
          "form": "사람 선의을 악용하는구만",
          "original_form": "사람 선의을 악용하는구만",
          "speaker_id": "1",
          "time": "20210824 18:03"
        },
        {
          "id": "MDRW2100011970.1.18",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100011970.1.19",
          "form": "24년에 파리올림픽때도",
          "original_form": "24년에 파리올림픽때도",
          "speaker_id": "1",
          "time": "20210824 18:56"
        },
        {
          "id": "MDRW2100011970.1.20",
          "form": "소매치기 장난아니겠다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "소매치기 장난아니겠다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:56"
        },
        {
          "id": "MDRW2100011970.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100011970.1.22",
          "form": "소매치기 하면",
          "original_form": "소매치기 하면",
          "speaker_id": "2",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100011970.1.23",
          "form": "이탈리아가 최고지",
          "original_form": "이탈리아가 최고지",
          "speaker_id": "2",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100011970.1.24",
          "form": "이탈리아도 가기 싫더라 그래서",
          "original_form": "이탈리아도 가기 싫더라 그래서",
          "speaker_id": "2",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100011970.1.25",
          "form": "나도 관광지로 너무 유명한데는 별로 안가고싶음..",
          "original_form": "나도 관광지로 너무 유명한데는 별로 안가고싶음..",
          "speaker_id": "1",
          "time": "20210824 18:59"
        },
        {
          "id": "MDRW2100011970.1.26",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210824 18:59"
        },
        {
          "id": "MDRW2100011970.1.27",
          "form": "좀 여유롭게 경치구경하고",
          "original_form": "좀 여유롭게 경치구경하고",
          "speaker_id": "2",
          "time": "20210824 19:00"
        },
        {
          "id": "MDRW2100011970.1.28",
          "form": "스위스 이런곳 걷고 그러고 싶어",
          "original_form": "스위스 이런곳 걷고 그러고 싶어",
          "speaker_id": "2",
          "time": "20210824 19:00"
        },
        {
          "id": "MDRW2100011970.1.29",
          "form": "우리 여행도 다녀야하고 동물 입양도 해야하고 좋은집도 사야하고 40살전에 은퇴도 해야하고ㅋㅋㅋㅋㅋㅋ",
          "original_form": "우리 여행도 다녀야하고 동물 입양도 해야하고 좋은집도 사야하고 40살전에 은퇴도 해야하고ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100011970.1.30",
          "form": "진짜 열심히살자..ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 열심히살자..ㅋㅋㅋㅋㅋ",
