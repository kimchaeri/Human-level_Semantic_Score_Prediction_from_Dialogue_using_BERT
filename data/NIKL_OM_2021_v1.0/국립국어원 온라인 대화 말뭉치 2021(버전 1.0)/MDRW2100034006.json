      "utterance": [
        {
          "id": "MDRW2100034006.1.1",
          "form": "어...아뇨?",
          "original_form": "어...아뇨?",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100034006.1.2",
          "form": "처음들어봐요",
          "original_form": "처음들어봐요",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100034006.1.3",
          "form": "아 최근 개봉영화구나",
          "original_form": "아 최근 개봉영화구나",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100034006.1.4",
          "form": "디즈니 작품 멋졌어요",
          "original_form": "디즈니 작품 멋졌어요",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100034006.1.5",
          "form": "재밌나요??",
          "original_form": "재밌나요??",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100034006.1.6",
          "form": "아 아뇨 올해초 개봉했었어요",
          "original_form": "아 아뇨 올해초 개봉했었어요",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100034006.1.7",
          "form": "요즘 ㅠㅠㅋㅋㅋㅋ 코로나땜에 영화관을 안간지 오래돼서ㅠ",
          "original_form": "요즘 ㅠㅠㅋㅋㅋㅋ 코로나땜에 영화관을 안간지 오래돼서ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100034006.1.8",
          "form": "믿고 보는 디즈니작이죠",
          "original_form": "믿고 보는 디즈니작이죠",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100034006.1.9",
          "form": "최신 영화들 안찾아본지도 좀 됐어욬ㅋㅋㅋㅋ",
          "original_form": "최신 영화들 안찾아본지도 좀 됐어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100034006.1.10",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100034006.1.11",
          "form": "봐야겠다",
          "original_form": "봐야겠다",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100034006.1.12",
          "form": "전 디즈니 마지막이 영화관가서 보고온",
          "original_form": "전 디즈니 마지막이 영화관가서 보고온",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100034006.1.13",
          "form": "겨울왕국2였어요...",
          "original_form": "겨울왕국2였어요...",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100034006.1.14",
          "form": "저는 헌혈하고 영화티켓 받아서가요",
          "original_form": "저는 헌혈하고 영화티켓 받아서가요",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100034006.1.15",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100034006.1.16",
          "form": "아 저 결왕국2못봤어요ㅜ",
          "original_form": "아 저 결왕국2못봤어요ㅜ",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100034006.1.17",
          "form": "앗 못보셨구나",
          "original_form": "앗 못보셨구나",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100034006.1.18",
          "form": "전 딱 개봉한 당일에 영화관갔었는데",
          "original_form": "전 딱 개봉한 당일에 영화관갔었는데",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100034006.1.19",
          "form": "네ㅜㅜ 요새 사람이 없어서 티켓 두장씩줘요",
          "original_form": "네ㅜㅜ 요새 사람이 없어서 티켓 두장씩줘요",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100034006.1.20",
          "form": "바로 뒷자리에 외국인 셋이 앉았는데",
          "original_form": "바로 뒷자리에 외국인 셋이 앉았는데",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100034006.1.21",
          "form": "자꾸...",
          "original_form": "자꾸...",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100034006.1.22",
          "form": "저는 알라딘도 못봤어요",
          "original_form": "저는 알라딘도 못봤어요",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100034006.1.23",
          "form": "방청객처럼 떠들면서 영화봐서",
          "original_form": "방청객처럼 떠들면서 영화봐서",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100034006.1.24",
          "form": "고통받았던 기억...",
          "original_form": "고통받았던 기억...",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100034006.1.25",
          "form": "헐 알라딘",
          "original_form": "헐 알라딘",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100034006.1.26",
          "form": "시간나실때 보셔요 볼만해요",
          "original_form": "시간나실때 보셔요 볼만해요",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100034006.1.27",
          "form": "11월에 디즈니 플러스 국내서비스시작하믄 봐야죠",
          "original_form": "11월에 디즈니 플러스 국내서비스시작하믄 봐야죠",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100034006.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저도...소울..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저도...소울..",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100034006.1.29",
          "form": "다들 알라딘 노래부를때 혼자 간첩된 기분",
          "original_form": "다들 알라딘 노래부를때 혼자 간첩된 기분",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100034006.1.30",
          "form": "나중에 봐봐야겠어요",
          "original_form": "나중에 봐봐야겠어요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100034006.1.31",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100034006.1.32",
          "form": "다들 불러 자꾸 나만몰라",
          "original_form": "다들 불러 자꾸 나만몰라",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100034006.1.33",
          "form": "그치만 알라딘 옛날꺼 사람버젼(?)으로 새로만든거라",
          "original_form": "그치만 알라딘 옛날꺼 사람버젼(?)으로 새로만든거라",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100034006.1.34",
          "form": "노래는 이미 있던거잖아욬ㅋㅋㅋㅋㅋ",
          "original_form": "노래는 이미 있던거잖아욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100034006.1.35",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100034006.1.36",
          "form": "근데 사람들이 계속 옆에서 부르진 않았어요ㅜㅜㅜㅜ",
          "original_form": "근데 사람들이 계속 옆에서 부르진 않았어요ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100034006.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그건 그래욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그건 그래욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100034006.1.38",
          "form": "영화보고 자꾸불러",
          "original_form": "영화보고 자꾸불러",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100034006.1.39",
          "form": "저는 책읽을시간없을때 유투브에 책리뷰봤었어요",
          "original_form": "저는 책읽을시간없을때 유투브에 책리뷰봤었어요",
          "speaker_id": "1",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100034006.1.40",
          "form": "설민석씨의 머 정의란무엇인가 이런거요ㅋㅋㅋ",
          "original_form": "설민석씨의 머 정의란무엇인가 이런거요ㅋㅋㅋ",
          "speaker_id": "1",
