      "utterance": [
        {
          "id": "MDRW2100013275.1.1",
          "form": "쇼핑이어가자",
          "original_form": "쇼핑이어가자",
          "speaker_id": "1",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013275.1.2",
          "form": "방금 택배 왔는데",
          "original_form": "방금 택배 왔는데",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013275.1.3",
          "form": "또 캠핑 장비 왔음",
          "original_form": "또 캠핑 장비 왔음",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013275.1.4",
          "form": "우리 신랑 돈 없다 하면서 뭘 자꾸 사나 몰라",
          "original_form": "우리 신랑 돈 없다 하면서 뭘 자꾸 사나 몰라",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013275.1.5",
          "form": "돈많겠지ㅋㅋ",
          "original_form": "돈많겠지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013275.1.6",
          "form": "돈이 어디 있지??",
          "original_form": "돈이 어디 있지??",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013275.1.7",
          "form": "그건 남편에게 물어봐라ㅋㅋㅋ",
          "original_form": "그건 남편에게 물어봐라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013275.1.8",
          "form": "나도 돈좀있었으면",
          "original_form": "나도 돈좀있었으면",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013275.1.9",
          "form": "ㅋㅋㅋㅋㅋㄱㅋ 수분크림도 사야되고",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋ 수분크림도 사야되고",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013275.1.10",
          "form": "마사지팩도 사고싶고",
          "original_form": "마사지팩도 사고싶고",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013275.1.11",
          "form": "퇴근하면서",
          "original_form": "퇴근하면서",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013275.1.12",
          "form": "리틀제이니 들려서 애들 바지 3개 사옴",
          "original_form": "리틀제이니 들려서 애들 바지 3개 사옴",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013275.1.13",
          "form": "나도 마사지하곱",
          "original_form": "나도 마사지하곱",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100013275.1.14",
          "form": "아까니봤다",
          "original_form": "아까니봤다",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100013275.1.15",
          "form": "affiliation",
          "original_form": "&affiliation&",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100013275.1.16",
          "form": "육출이군 ㅎㅎ",
          "original_form": "육출이군 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100013275.1.17",
          "form": "내옷도 사고 싶네",
          "original_form": "내옷도 사고 싶네",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100013275.1.18",
          "form": "오늘 오케이마트 세일한다고 문자왔던데",
          "original_form": "오늘 오케이마트 세일한다고 문자왔던데",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100013275.1.19",
          "form": "오케이?",
          "original_form": "오케이?",
          "speaker_id": "1",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100013275.1.20",
          "form": "자연드림만갔다왔다",
          "original_form": "자연드림만갔다왔다",
          "speaker_id": "1",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100013275.1.21",
          "form": "아까 오케이 마트 세일 한다던데",
          "original_form": "아까 오케이 마트 세일 한다던데",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100013275.1.22",
          "form": "자연드림 가서 뭐 사왔슈??",
          "original_form": "자연드림 가서 뭐 사왔슈??",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100013275.1.23",
          "form": "계란 사야 되는데 깜빡했네",
          "original_form": "계란 사야 되는데 깜빡했네",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100013275.1.24",
          "form": "계란없드라ㅡㅡ",
          "original_form": "계란없드라ㅡㅡ",
          "speaker_id": "1",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100013275.1.25",
          "form": "일찍가야됨ㅡㅡ",
          "original_form": "일찍가야됨ㅡㅡ",
          "speaker_id": "1",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100013275.1.26",
          "form": "아 계란 사기 힘들다 정말",
          "original_form": "아 계란 사기 힘들다 정말",
          "speaker_id": "2",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100013275.1.27",
          "form": "계란이\r\n이래 귀하다니...",
          "original_form": "계란이\r\n이래 귀하다니...",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100013275.1.28",
          "form": "자연드림 수산물 행사 끝났는가",
          "original_form": "자연드림 수산물 행사 끝났는가",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100013275.1.29",
          "form": "고등어 사놔야되는데",
          "original_form": "고등어 사놔야되는데",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100013275.1.30",
          "form": "대패행사하드라",
          "original_form": "대패행사하드라",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100013275.1.31",
          "form": "깊은물도 주는거같은데",
          "original_form": "깊은물도 주는거같은데",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100013275.1.32",
          "form": "대패??ㅅ",
          "original_form": "대패??ㅅ",
          "speaker_id": "2",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100013275.1.33",
          "form": "자연드림에??",
          "original_form": "자연드림에??",
          "speaker_id": "2",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100013275.1.34",
          "form": "자연드림도 대패 팔아요??",
          "original_form": "자연드림도 대패 팔아요??",
          "speaker_id": "2",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100013275.1.35",
          "form": "오늘보니 판다하더라",
          "original_form": "오늘보니 판다하더라",
          "speaker_id": "1",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100013275.1.36",
          "form": "삼계탕하느것도있고",
          "original_form": "삼계탕하느것도있고",
          "speaker_id": "1",
          "time": "20210825 19:31"
        }
      ]
