      "utterance": [
        {
          "id": "MDRW2100039306.1.1",
          "form": "난 식욕폭발했어",
          "original_form": "난 식욕폭발했어",
          "speaker_id": "2",
          "time": "20210903 16:00"
        },
        {
          "id": "MDRW2100039306.1.2",
          "form": "밤식빵 순식간에 먹었어 ㅠㅠ",
          "original_form": "밤식빵 순식간에 먹었어 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 16:00"
        },
        {
          "id": "MDRW2100039306.1.3",
          "form": "그러니까 저녁 없다고 내 빵 줬지",
          "original_form": "그러니까 저녁 없다고 내 빵 줬지",
          "speaker_id": "1",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100039306.1.4",
          "form": "미안해 갑자기 그랬어 나 빵순이 아니잖아",
          "original_form": "미안해 갑자기 그랬어 나 빵순이 아니잖아",
          "speaker_id": "2",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100039306.1.5",
          "form": "그니까 이상해 앞으론 남겨놔 식사할 분량은",
          "original_form": "그니까 이상해 앞으론 남겨놔 식사할 분량은",
          "speaker_id": "1",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100039306.1.6",
          "form": "반성합니다 ㅋㅋ 그나저나 내일 아침 뭐 먹지?",
          "original_form": "반성합니다 ㅋㅋ 그나저나 내일 아침 뭐 먹지?",
          "speaker_id": "2",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100039306.1.7",
          "form": "반찬 남은 거 먹도록 해 그리고 먹을 거 사러 가자",
          "original_form": "반찬 남은 거 먹도록 해 그리고 먹을 거 사러 가자",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039306.1.8",
          "form": "뭐 살거야 오늘??",
          "original_form": "뭐 살거야 오늘??",
          "speaker_id": "2",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100039306.1.9",
          "form": "잘 모르겠어.분식이 좀 땡기긴하는데",
          "original_form": "잘 모르겠어.분식이 좀 땡기긴하는데",
          "speaker_id": "1",
          "time": "20210904 11:24"
        },
        {
          "id": "MDRW2100039306.1.10",
          "form": "난 김밥 떡볶이 순대 다 먹고 싶어",
          "original_form": "난 김밥 떡볶이 순대 다 먹고 싶어",
          "speaker_id": "2",
          "time": "20210904 11:24"
        },
        {
          "id": "MDRW2100039306.1.11",
          "form": "그것도 좋은 것 같아",
          "original_form": "그것도 좋은 것 같아",
          "speaker_id": "1",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100039306.1.12",
          "form": "그런데 어디서 사 먹어? 나운 or 대명",
          "original_form": "그런데 어디서 사 먹어? 나운 or 대명",
          "speaker_id": "2",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100039306.1.13",
          "form": "대명동이 좋은 듯",
          "original_form": "대명동이 좋은 듯",
          "speaker_id": "1",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100039306.1.14",
          "form": "새로운 곳 도전? 근데 간하고 염통만 먹고 싶은데",
          "original_form": "새로운 곳 도전? 근데 간하고 염통만 먹고 싶은데",
          "speaker_id": "2",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100039306.1.15",
          "form": "염통 맞나? 부위 좀 알려줘",
          "original_form": "염통 맞나? 부위 좀 알려줘",
          "speaker_id": "2",
          "time": "20210904 11:39"
        },
        {
          "id": "MDRW2100039306.1.16",
          "form": "오늘 양과점 가는 거 기대된다. 새로운 데 가서",
          "original_form": "오늘 양과점 가는 거 기대된다. 새로운 데 가서",
          "speaker_id": "2",
          "time": "20210908 09:25"
        }
      ]
