      "utterance": [
        {
          "id": "MDRW2100033518.1.1",
          "form": "저희동네엔 강아지 빵집이 있어요",
          "original_form": "저희동네엔 강아지 빵집이 있어요",
          "speaker_id": "1",
          "time": "20210824 10:34"
        },
        {
          "id": "MDRW2100033518.1.2",
          "form": "오 맞아요 비슷한거 본거같아요 강아지들 간식파는ㅎㅎ",
          "original_form": "오 맞아요 비슷한거 본거같아요 강아지들 간식파는ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 10:35"
        },
        {
          "id": "MDRW2100033518.1.3",
          "form": "가게이름이 개 빵 집 이에요",
          "original_form": "가게이름이 개 빵 집 이에요",
          "speaker_id": "1",
          "time": "20210824 10:35"
        },
        {
          "id": "MDRW2100033518.1.4",
          "form": "왜 나쁜말이라하지",
          "original_form": "왜 나쁜말이라하지",
          "speaker_id": "1",
          "time": "20210824 10:35"
        },
        {
          "id": "MDRW2100033518.1.5",
          "form": "아ㅋㅋㅋ재밌네요ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ재밌네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:36"
        },
        {
          "id": "MDRW2100033518.1.6",
          "form": "애들 좋은거 먹더라구요ㅋㅋ",
          "original_form": "애들 좋은거 먹더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:39"
        },
        {
          "id": "MDRW2100033518.1.7",
          "form": "가족같으니 좋은거 유기농 많이 신경쓰시더리구요",
          "original_form": "가족같으니 좋은거 유기농 많이 신경쓰시더리구요",
          "speaker_id": "2",
          "time": "20210824 10:39"
        },
        {
          "id": "MDRW2100033518.1.8",
          "form": "코로나에도 그집은 유지하고있더라구요 다른데 망했는데",
          "original_form": "코로나에도 그집은 유지하고있더라구요 다른데 망했는데",
          "speaker_id": "1",
          "time": "20210824 10:41"
        },
        {
          "id": "MDRW2100033518.1.9",
          "form": "주변에도 반려동물한텐 돈을 안아끼는걸 보니 이해가 가네요ㅎㅎ",
          "original_form": "주변에도 반려동물한텐 돈을 안아끼는걸 보니 이해가 가네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 10:42"
        },
        {
          "id": "MDRW2100033518.1.10",
          "form": "네 저두 제 아이 생기면 좋은거만 주고싶을거같아요",
          "original_form": "네 저두 제 아이 생기면 좋은거만 주고싶을거같아요",
          "speaker_id": "1",
          "time": "20210824 10:44"
        },
        {
          "id": "MDRW2100033518.1.11",
          "form": "저도 그렇긴한데ㅎㅎ 그날이 언제 올지ㅎㅎ",
          "original_form": "저도 그렇긴한데ㅎㅎ 그날이 언제 올지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 10:45"
        },
        {
          "id": "MDRW2100033518.1.12",
          "form": "요새 강아지 먹방유툽도 있어요",
          "original_form": "요새 강아지 먹방유툽도 있어요",
          "speaker_id": "1",
          "time": "20210824 10:45"
        },
        {
          "id": "MDRW2100033518.1.13",
          "form": "오 신기하네요 강아지먹방이라니ㅎㅎ",
          "original_form": "오 신기하네요 강아지먹방이라니ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 10:45"
        },
        {
          "id": "MDRW2100033518.1.14",
          "form": "소리듣는거에요",
          "original_form": "소리듣는거에요",
          "speaker_id": "1",
          "time": "20210824 10:46"
        },
        {
          "id": "MDRW2100033518.1.15",
          "form": "냠냠",
          "original_form": "냠냠",
          "speaker_id": "1",
          "time": "20210824 10:46"
        },
        {
          "id": "MDRW2100033518.1.16",
          "form": "재밌네요ㅎㅎ 사람들 신기해라ㅋㅋ",
          "original_form": "재밌네요ㅎㅎ 사람들 신기해라ㅋㅋ",
