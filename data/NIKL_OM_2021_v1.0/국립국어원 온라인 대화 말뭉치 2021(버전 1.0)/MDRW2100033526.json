      "utterance": [
        {
          "id": "MDRW2100033526.1.1",
          "form": "쇼핑하는거 좋아하세요?",
          "original_form": "쇼핑하는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 11:15"
        },
        {
          "id": "MDRW2100033526.1.2",
          "form": "구경하는거 사는거 다 좋아요",
          "original_form": "구경하는거 사는거 다 좋아요",
          "speaker_id": "2",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100033526.1.3",
          "form": "돈쓰는거 짜릿해ㅜ",
          "original_form": "돈쓰는거 짜릿해ㅜ",
          "speaker_id": "2",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100033526.1.4",
          "form": "최근에 뭐 사셨어요?",
          "original_form": "최근에 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100033526.1.5",
          "form": "내돈이지만요 ㅋㅋ",
          "original_form": "내돈이지만요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100033526.1.6",
          "form": "그쵸.. 우리가 이러려고 돈 벌잖아요 ㅋㅋㅋ",
          "original_form": "그쵸.. 우리가 이러려고 돈 벌잖아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100033526.1.7",
          "form": "추천하고 싶은 꿀템 있으세요?",
          "original_form": "추천하고 싶은 꿀템 있으세요?",
          "speaker_id": "2",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100033526.1.8",
          "form": "음 신일 선풍기 샀어요 ㅋㅋ",
          "original_form": "음 신일 선풍기 샀어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100033526.1.9",
          "form": "최근에 실내화 샀어요 발 안아픈걸로요 ㅎ",
          "original_form": "최근에 실내화 샀어요 발 안아픈걸로요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100033526.1.10",
          "form": "오 어디껀가요",
          "original_form": "오 어디껀가요",
          "speaker_id": "2",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100033526.1.11",
          "form": "발 안아픈거 좋네요",
          "original_form": "발 안아픈거 좋네요",
          "speaker_id": "2",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100033526.1.12",
          "form": "신미사라고 검색하시면 통쿱 슬러퍼가 나오는데 이게 발바닥이 안아프고 좋더라고요",
          "original_form": "신미사라고 검색하시면 통쿱 슬러퍼가 나오는데 이게 발바닥이 안아프고 좋더라고요",
          "speaker_id": "1",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100033526.1.13",
          "form": "오 꿀정보 감사합니다",
          "original_form": "오 꿀정보 감사합니다",
          "speaker_id": "2",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100033526.1.14",
          "form": "필요했어요",
          "original_form": "필요했어요",
          "speaker_id": "2",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100033526.1.15",
          "form": "저는 아쿠아슈즈 얼마전에 샀어요",
          "original_form": "저는 아쿠아슈즈 얼마전에 샀어요",
          "speaker_id": "2",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100033526.1.16",
          "form": "이야기했던가요? ㅎㅎ",
          "original_form": "이야기했던가요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100033526.1.17",
          "form": "아뇨~ 어디껄로 사셨어요?",
          "original_form": "아뇨~ 어디껄로 사셨어요?",
          "speaker_id": "1",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100033526.1.18",
          "form": "벨롭이란 브랜드에요",
          "original_form": "벨롭이란 브랜드에요",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100033526.1.19",
          "form": "근데좀 사고나서 미묘하네요 ㅎㅎ",
          "original_form": "근데좀 사고나서 미묘하네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100033526.1.20",
          "form": "방금 찾아보고 왔는데 엄청 예쁘네요^^",
          "original_form": "방금 찾아보고 왔는데 엄청 예쁘네요^^",
          "speaker_id": "1",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100033526.1.21",
          "form": "예쁘긴한데",
          "original_form": "예쁘긴한데",
          "speaker_id": "2",
          "time": "20210824 18:26"
        },
        {
          "id": "MDRW2100033526.1.22",
