      "utterance": [
        {
          "id": "MDRW2100031117.1.1",
          "form": "온라인 피티 아시나요",
          "original_form": "온라인 피티 아시나요",
          "speaker_id": "1",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100031117.1.2",
          "form": "들어는본거같아요!",
          "original_form": "들어는본거같아요!",
          "speaker_id": "2",
          "time": "20210904 17:09"
        },
        {
          "id": "MDRW2100031117.1.3",
          "form": "어떤거? 들어보셨나요",
          "original_form": "어떤거? 들어보셨나요",
          "speaker_id": "1",
          "time": "20210904 18:41"
        },
        {
          "id": "MDRW2100031117.1.4",
          "form": "아 그런게 있다고 들어만 봤다구요 ㅋㅋㅋㅋ 잘몰라요",
          "original_form": "아 그런게 있다고 들어만 봤다구요 ㅋㅋㅋㅋ 잘몰라요",
          "speaker_id": "2",
          "time": "20210904 19:05"
        },
        {
          "id": "MDRW2100031117.1.5",
          "form": "그러시군요 서비스가 다양한것 같아요",
          "original_form": "그러시군요 서비스가 다양한것 같아요",
          "speaker_id": "1",
          "time": "20210905 06:26"
        },
        {
          "id": "MDRW2100031117.1.6",
          "form": "어떤게있나요?",
          "original_form": "어떤게있나요?",
          "speaker_id": "2",
          "time": "20210905 09:45"
        },
        {
          "id": "MDRW2100031117.1.7",
          "form": "삼성에서 호텔급 피트니스 영상을 유료로 제공한다는 기사를 봤어요",
          "original_form": "삼성에서 호텔급 피트니스 영상을 유료로 제공한다는 기사를 봤어요",
          "speaker_id": "1",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100031117.1.8",
          "form": "오오 실제로 피티받는것과는 괴리가 있긴 할거같은데요!",
          "original_form": "오오 실제로 피티받는것과는 괴리가 있긴 할거같은데요!",
          "speaker_id": "2",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100031117.1.9",
          "form": "그렇죠 식단관리나 운동 커리큘럼 등을 제공해주는듯하더라구요",
          "original_form": "그렇죠 식단관리나 운동 커리큘럼 등을 제공해주는듯하더라구요",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100031117.1.10",
          "form": "오 일대일로 해주는건가요?",
          "original_form": "오 일대일로 해주는건가요?",
          "speaker_id": "2",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100031117.1.11",
          "form": "네 제가 검색한건 월 만원 정도로 나오더라구요",
          "original_form": "네 제가 검색한건 월 만원 정도로 나오더라구요",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100031117.1.12",
          "form": "오 일대일인데 가능한가격인가요?! ㅋㅋㅋ 오신기하네요",
          "original_form": "오 일대일인데 가능한가격인가요?! ㅋㅋㅋ 오신기하네요",
          "speaker_id": "2",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100031117.1.13",
          "form": "비대면이라 그러겠죠? 다른 업체는 더 비싼 가격도 봤어요 케어되는 부분이 다르겠죠?",
          "original_form": "비대면이라 그러겠죠? 다른 업체는 더 비싼 가격도 봤어요 케어되는 부분이 다르겠죠?",
          "speaker_id": "1",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100031117.1.14",
          "form": "관심있으세요?",
          "original_form": "관심있으세요?",
          "speaker_id": "2",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100031117.1.15",
          "form": "와이프가 관심있어서 같이 찾아봤어요",
          "original_form": "와이프가 관심있어서 같이 찾아봤어요",
          "speaker_id": "1",
          "time": "20210905 14:00"
        }
      ]
