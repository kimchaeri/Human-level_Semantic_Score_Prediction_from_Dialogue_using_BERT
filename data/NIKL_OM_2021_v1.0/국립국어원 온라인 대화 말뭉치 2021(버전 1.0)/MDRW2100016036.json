      "utterance": [
        {
          "id": "MDRW2100016036.1.1",
          "form": "날씨좋네",
          "original_form": "날씨좋네",
          "speaker_id": "2",
          "time": "20210826 08:30"
        },
        {
          "id": "MDRW2100016036.1.2",
          "form": "오늘계획은?",
          "original_form": "오늘계획은?",
          "speaker_id": "2",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016036.1.3",
          "form": "신랑 머리자르고 난 뿌염을 좀 허구요~",
          "original_form": "신랑 머리자르고 난 뿌염을 좀 허구요~",
          "speaker_id": "1",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016036.1.4",
          "form": "은행가서 name3 통장 만들어주려구요^^",
          "original_form": "은행가서 &name3& 통장 만들어주려구요^^",
          "speaker_id": "1",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016036.1.5",
          "form": "오랜만에 해 쨍쨍 ㅋㅋㅋㅋ",
          "original_form": "오랜만에 해 쨍쨍 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016036.1.6",
          "form": "바쁘네 쉬어도",
          "original_form": "바쁘네 쉬어도",
          "speaker_id": "2",
          "time": "20210826 08:33"
        },
        {
          "id": "MDRW2100016036.1.7",
          "form": "신랑도휴무가?",
          "original_form": "신랑도휴무가?",
          "speaker_id": "2",
          "time": "20210826 08:33"
        },
        {
          "id": "MDRW2100016036.1.8",
          "form": "오늘 완전가을인강?",
          "original_form": "오늘 완전가을인강?",
          "speaker_id": "2",
          "time": "20210826 08:33"
        },
        {
          "id": "MDRW2100016036.1.9",
          "form": "난 쓰봉들고 출근한다",
          "original_form": "난 쓰봉들고 출근한다",
          "speaker_id": "2",
          "time": "20210826 08:34"
        },
        {
          "id": "MDRW2100016036.1.10",
          "form": "신랑 어깨 수술해가 요번주 쭉 쉽니더 ㅋㅋㅋㅋㅋ",
          "original_form": "신랑 어깨 수술해가 요번주 쭉 쉽니더 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:39"
        },
        {
          "id": "MDRW2100016036.1.11",
          "form": "쓰봉 ㅋㅋㅋ",
          "original_form": "쓰봉 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:39"
        },
        {
          "id": "MDRW2100016036.1.12",
          "form": "움직이니 더버요ㅠ",
          "original_form": "움직이니 더버요ㅠ",
          "speaker_id": "1",
          "time": "20210826 08:39"
        },
        {
          "id": "MDRW2100016036.1.13",
          "form": "덥긴하다",
          "original_form": "덥긴하다",
          "speaker_id": "2",
          "time": "20210826 08:52"
        },
        {
          "id": "MDRW2100016036.1.14",
          "form": "비오고 나면 좀 시원해질꺼라 생각했는데..",
          "original_form": "비오고 나면 좀 시원해질꺼라 생각했는데..",
          "speaker_id": "1",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016036.1.15",
          "form": "해 뜨니 늠 더워요~",
          "original_form": "해 뜨니 늠 더워요~",
          "speaker_id": "1",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016036.1.16",
          "form": "빨리 가을이 왔음 ㅋㅋㅋㅋ",
          "original_form": "빨리 가을이 왔음 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016036.1.17",
          "form": "오늘 미먼없이 쨍하니 좋으다",
          "original_form": "오늘 미먼없이 쨍하니 좋으다",
          "speaker_id": "2",
          "time": "20210826 09:07"
        },
        {
          "id": "MDRW2100016036.1.18",
          "form": "차 타고 다니기 좋은 날씨",
          "original_form": "차 타고 다니기 좋은 날씨",
          "speaker_id": "1",
          "time": "20210826 09:31"
        },
        {
          "id": "MDRW2100016036.1.19",
          "form": "걸으면 넘나 더워",
          "original_form": "걸으면 넘나 더워",
          "speaker_id": "1",
          "time": "20210826 09:32"
        },
        {
          "id": "MDRW2100016036.1.20",
          "form": "언니 마이크라니까 엄청 편해요",
          "original_form": "언니 마이크라니까 엄청 편해요",
          "speaker_id": "1",
          "time": "20210826 09:32"
        },
        {
          "id": "MDRW2100016036.1.21",
          "form": "그렇구만",
          "original_form": "그렇구만",
          "speaker_id": "2",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100016036.1.22",
          "form": "차 타고 다니기 좋은 날씨 맞다",
          "original_form": "차 타고 다니기 좋은 날씨 맞다",
          "speaker_id": "2",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100016036.1.23",
          "form": "날씨 좋은디 점심 뭐 먹었어용??",
          "original_form": "날씨 좋은디 점심 뭐 먹었어용??",
          "speaker_id": "1",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100016036.1.24",
          "form": "우린 볼인보고 모다 근처에서 먹었는데..마음속에.비내림..넘나 맛이 없네요 ㅋㅋㅋ",
          "original_form": "우린 볼인보고 모다 근처에서 먹었는데..마음속에.비내림..넘나 맛이 없네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100016036.1.25",
          "form": "난 신마녀떡볶잉",
          "original_form": "난 신마녀떡볶잉",
          "speaker_id": "2",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100016036.1.26",
          "form": "떡볶이 맛있겠구만요~",
          "original_form": "떡볶이 맛있겠구만요~",
          "speaker_id": "1",
          "time": "20210826 13:18"
        },
        {
          "id": "MDRW2100016036.1.27",
          "form": "이제 퇴근한거라요?,",
          "original_form": "이제 퇴근한거라요?,",
          "speaker_id": "1",
          "time": "20210826 13:18"
        },
        {
          "id": "MDRW2100016036.1.28",
          "form": "주사맞으러왔다",
          "original_form": "주사맞으러왔다",
          "speaker_id": "2",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100016036.1.29",
          "form": "2치백신",
          "original_form": "2치백신",
          "speaker_id": "2",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100016036.1.30",
          "form": "오~언니 난 낼 1차 맞으러 가요~",
          "original_form": "오~언니 난 낼 1차 맞으러 가요~",
