      "utterance": [
        {
          "id": "MDRW2100025811.1.1",
          "form": "엉엉 어느나라거야?",
          "original_form": "엉엉 어느나라거야?",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100025811.1.2",
          "form": "우리나",
          "original_form": "우리나",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100025811.1.3",
          "form": "라 ㅋㅋㅋㅋ이제훈이랑 신혜선나와",
          "original_form": "라 ㅋㅋㅋㅋ이제훈이랑 신혜선나와",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100025811.1.4",
          "form": "아! 들어본거같애",
          "original_form": "아! 들어본거같애",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100025811.1.5",
          "form": "지금 대충 틀어놓고 보고 있는데 ㅋㅋㅋ웃기다",
          "original_form": "지금 대충 틀어놓고 보고 있는데 ㅋㅋㅋ웃기다",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025811.1.6",
          "form": "코믹영화여?ㅋㅋㅋㅋㅋ",
          "original_form": "코믹영화여?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025811.1.7",
          "form": "응 약간 한국판 인디아나존스?ㅋㅋ이제훈이 그런역임",
          "original_form": "응 약간 한국판 인디아나존스?ㅋㅋ이제훈이 그런역임",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025811.1.8",
          "form": "아잌ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 어느 플랫폼에서 봐?",
          "original_form": "아잌ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 어느 플랫폼에서 봐?",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025811.1.9",
          "form": "나 티빙에서 ㅋㅋㅋ티빙 이용해?",
          "original_form": "나 티빙에서 ㅋㅋㅋ티빙 이용해?",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025811.1.10",
          "form": "아 나 티빙 결제 끊었어ㅠㅠㅠㅠㅠ",
          "original_form": "아 나 티빙 결제 끊었어ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025811.1.11",
          "form": "그만 뒀다고?",
          "original_form": "그만 뒀다고?",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025811.1.12",
          "form": "엉엉 나머지 다 보는데 티빙만 안봐ㅠㅠ",
          "original_form": "엉엉 나머지 다 보는데 티빙만 안봐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025811.1.13",
          "form": "나도 연장은 안했어 볼만한게 없더라 넷플이나 푹에도 있지 않을까?",
          "original_form": "나도 연장은 안했어 볼만한게 없더라 넷플이나 푹에도 있지 않을까?",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025811.1.14",
          "form": "오 쫌따 넷플서 찾아봐야겠다 언니 넷플봐?",
          "original_form": "오 쫌따 넷플서 찾아봐야겠다 언니 넷플봐?",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025811.1.15",
          "form": "난 넷플은 안보는데 푹은 티빙이랑 영화같은건 겹치더라구",
          "original_form": "난 넷플은 안보는데 푹은 티빙이랑 영화같은건 겹치더라구",
          "speaker_id": "1",
          "time": "20210901 17:14"
        }
      ]
