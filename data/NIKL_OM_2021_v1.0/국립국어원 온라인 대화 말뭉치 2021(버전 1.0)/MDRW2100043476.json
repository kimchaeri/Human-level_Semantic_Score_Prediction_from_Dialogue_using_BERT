      "utterance": [
        {
          "id": "MDRW2100043476.1.1",
          "form": "생오렌지 이천원이면 살수있어요",
          "original_form": "생오렌지 이천원이면 살수있어요",
          "speaker_id": "1",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043476.1.2",
          "form": "ㅎ 식당서먹으면 비싼데요",
          "original_form": "ㅎ 식당서먹으면 비싼데요",
          "speaker_id": "2",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043476.1.3",
          "form": "그래서 얼마에요?",
          "original_form": "그래서 얼마에요?",
          "speaker_id": "1",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043476.1.4",
          "form": "구천원",
          "original_form": "구천원",
          "speaker_id": "2",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043476.1.5",
          "form": ";;;; 걍 사이다 마실게요",
          "original_form": ";;;; 걍 사이다 마실게요",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043476.1.6",
          "form": "네 비싸요",
          "original_form": "네 비싸요",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043476.1.7",
          "form": "버터핑거 팬케익 가셧어요",
          "original_form": "버터핑거 팬케익 가셧어요",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043476.1.8",
          "form": "와플 프렌치토스트",
          "original_form": "와플 프렌치토스트",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043476.1.9",
          "form": "골라야해요",
          "original_form": "골라야해요",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043476.1.10",
          "form": "그냥 두개 시킬까요?",
          "original_form": "그냥 두개 시킬까요?",
          "speaker_id": "1",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043476.1.11",
          "form": "플렉스 하시는편 ?",
          "original_form": "플렉스 하시는편 ?",
          "speaker_id": "2",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043476.1.12",
          "form": "근데 음식남기는건 싫어해요ㅠ",
          "original_form": "근데 음식남기는건 싫어해요ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043476.1.13",
          "form": "여기가 에이드 8천 9천 입니다",
          "original_form": "여기가 에이드 8천 9천 입니다",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043476.1.14",
          "form": "어휴 거긴 음식값도 비싸구 다비싸네여",
          "original_form": "어휴 거긴 음식값도 비싸구 다비싸네여",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043476.1.15",
          "form": "그죠... 와플 길거리 올 고급와플",
          "original_form": "그죠... 와플 길거리 올 고급와플",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043476.1.16",
          "form": "선택 ㄱ",
          "original_form": "선택 ㄱ",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043476.1.17",
          "form": "아 와플은 고급",
          "original_form": "아 와플은 고급",
