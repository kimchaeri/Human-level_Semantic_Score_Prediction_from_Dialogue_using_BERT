      "utterance": [
        {
          "id": "MDRW2100038967.1.1",
          "form": "낼메뉴 라볶이결정ㅎㅎ",
          "original_form": "낼메뉴 라볶이결정ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100038967.1.2",
          "form": "라볶이도 좋죠",
          "original_form": "라볶이도 좋죠",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100038967.1.3",
          "form": "아까 생활의달인봤는데 라볶이달인나오더라고요",
          "original_form": "아까 생활의달인봤는데 라볶이달인나오더라고요",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100038967.1.4",
          "form": "레시피를봐도 도저히따라할수없는 스케일",
          "original_form": "레시피를봐도 도저히따라할수없는 스케일",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100038967.1.5",
          "form": "그래서 달인 아닐까요",
          "original_form": "그래서 달인 아닐까요",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100038967.1.6",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100038967.1.7",
          "form": "달인들 보면 한가지 음식에",
          "original_form": "달인들 보면 한가지 음식에",
          "speaker_id": "2",
          "time": "20210903 14:58"
        },
        {
          "id": "MDRW2100038967.1.8",
          "form": "재료를 수십개 넣죠",
          "original_form": "재료를 수십개 넣죠",
          "speaker_id": "2",
          "time": "20210903 14:59"
        },
        {
          "id": "MDRW2100038967.1.9",
          "form": "맞아요 진짜대단",
          "original_form": "맞아요 진짜대단",
          "speaker_id": "1",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100038967.1.10",
          "form": "조리법도 특이하잖아요",
          "original_form": "조리법도 특이하잖아요",
          "speaker_id": "2",
          "time": "20210903 15:20"
        },
        {
          "id": "MDRW2100038967.1.11",
          "form": "그니까요 가서 먹어야만하는",
          "original_form": "그니까요 가서 먹어야만하는",
          "speaker_id": "1",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100038967.1.12",
          "form": "코로나 나빠요",
          "original_form": "코로나 나빠요",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100038967.1.13",
          "form": "가서 먹어보고 싶은데 말이죠",
          "original_form": "가서 먹어보고 싶은데 말이죠",
          "speaker_id": "2",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100038967.1.14",
          "form": "그니까요ㅜ 괜히 먹고싶은마음만",
          "original_form": "그니까요ㅜ 괜히 먹고싶은마음만",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100038967.1.15",
          "form": "그래서 먹을거나오는건 잘 안봐야지 하는데 자꾸보네요",
          "original_form": "그래서 먹을거나오는건 잘 안봐야지 하는데 자꾸보네요",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100038967.1.16",
          "form": "맛집 탐방 프로 식객으로 가있고 싶어요",
          "original_form": "맛집 탐방 프로 식객으로 가있고 싶어요",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100038967.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100038967.1.18",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100038967.1.19",
          "form": "돈받고 먹고싶다",
          "original_form": "돈받고 먹고싶다",
          "speaker_id": "1",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100038967.1.20",
          "form": "그러면 1석2조겠군요",
          "original_form": "그러면 1석2조겠군요",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
