      "utterance": [
        {
          "id": "MDRW2100015554.1.1",
          "form": "좋아하는 음료가 먼가요",
          "original_form": "좋아하는 음료가 먼가요",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015554.1.2",
          "form": "저는 맥콜이요",
          "original_form": "저는 맥콜이요",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015554.1.3",
          "form": "name1은?",
          "original_form": "&name1&은?",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015554.1.4",
          "form": "전 믹스커피 좋아합니다",
          "original_form": "전 믹스커피 좋아합니다",
          "speaker_id": "1",
          "time": "20210826 07:09"
        },
        {
          "id": "MDRW2100015554.1.5",
          "form": "오? 색다르시군요",
          "original_form": "오? 색다르시군요",
          "speaker_id": "2",
          "time": "20210826 08:44"
        },
        {
          "id": "MDRW2100015554.1.6",
          "form": "맥콜도 옛날 제품아닌가요",
          "original_form": "맥콜도 옛날 제품아닌가요",
          "speaker_id": "1",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100015554.1.7",
          "form": "ㅋㅋㅋ맥콜 맛있어요",
          "original_form": "ㅋㅋㅋ맥콜 맛있어요",
          "speaker_id": "2",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100015554.1.8",
          "form": "울 아들이 맥콜좋아해요",
          "original_form": "울 아들이 맥콜좋아해요",
          "speaker_id": "1",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100015554.1.9",
          "form": "지금도 먹고 있어요ㅋㅈㅋ",
          "original_form": "지금도 먹고 있어요ㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100015554.1.10",
          "form": "저도 먹어보니 맛이 좋더라고요",
          "original_form": "저도 먹어보니 맛이 좋더라고요",
          "speaker_id": "1",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100015554.1.11",
          "form": "맛있죠ㅋㅋ음식은 뭐 좋아하세요",
          "original_form": "맛있죠ㅋㅋ음식은 뭐 좋아하세요",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100015554.1.12",
          "form": "비지찌개를 엄청 좋아해요",
          "original_form": "비지찌개를 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210827 06:47"
        },
        {
          "id": "MDRW2100015554.1.13",
          "form": "콩비지 맛있죠 ㅎㅎ",
          "original_form": "콩비지 맛있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 09:37"
        },
        {
          "id": "MDRW2100015554.1.14",
          "form": "저희는 저만 좋아해서 자주 먹진 못해요",
          "original_form": "저희는 저만 좋아해서 자주 먹진 못해요",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100015554.1.15",
          "form": "순대국은요?ㅎㅎ",
          "original_form": "순대국은요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100015554.1.16",
          "form": "그냥 먹긴해요",
          "original_form": "그냥 먹긴해요",
          "speaker_id": "1",
          "time": "20210828 09:02"
        },
        {
          "id": "MDRW2100015554.1.17",
          "form": "맛있는데",
          "original_form": "맛있는데",
          "speaker_id": "2",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100015554.1.18",
          "form": "전 솔직히 맛있는지는 모르겠어요",
