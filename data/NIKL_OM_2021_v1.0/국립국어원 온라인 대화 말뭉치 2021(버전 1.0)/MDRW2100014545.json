      "utterance": [
        {
          "id": "MDRW2100014545.1.1",
          "form": "좋아하는 가수는 누구에요?",
          "original_form": "좋아하는 가수는 누구에요?",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014545.1.2",
          "form": "연예인은?",
          "original_form": "연예인은?",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014545.1.3",
          "form": "저 가수는   아이유 요 ㅎㅎ",
          "original_form": "저 가수는   아이유 요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014545.1.4",
          "form": "아이유 좋죠ㅎㅎ",
          "original_form": "아이유 좋죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014545.1.5",
          "form": "노래도 잘하고 이쁘고ㅎ",
          "original_form": "노래도 잘하고 이쁘고ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014545.1.6",
          "form": "저번에 온앤오프에서 나오던데요ㅎㅎ 티아라 지연이랑 같이ㅎ",
          "original_form": "저번에 온앤오프에서 나오던데요ㅎㅎ 티아라 지연이랑 같이ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014545.1.7",
          "form": "팔색조  매력같아요 ㅎㅎ",
          "original_form": "팔색조  매력같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014545.1.8",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014545.1.9",
          "form": "티아라 왕따사껀이 생각나는건뭐죠ㅎㄹㄹ",
          "original_form": "티아라 왕따사껀이 생각나는건뭐죠ㅎㄹㄹ",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014545.1.10",
          "form": "우리가 모르는세계에 모르는 사건들이 참 많죠 \r\n왕따 당하면 진짜 힘들거 같아요",
          "original_form": "우리가 모르는세계에 모르는 사건들이 참 많죠 \r\n왕따 당하면 진짜 힘들거 같아요",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014545.1.11",
          "form": "맞아요ㅠ 진짜 왕따당하면ㅠㅍ",
          "original_form": "맞아요ㅠ 진짜 왕따당하면ㅠㅍ",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014545.1.12",
          "form": "우리가 모르는일이지만... 그래도 소문까지난거보면",
          "original_form": "우리가 모르는일이지만... 그래도 소문까지난거보면",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014545.1.13",
          "form": "엄청 심했을꺼에요",
          "original_form": "엄청 심했을꺼에요",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014545.1.14",
          "form": "연애인  직업자체가 우울증도 많다는데",
          "original_form": "연애인  직업자체가 우울증도 많다는데",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014545.1.15",
          "form": "왕따까지 당하면 멘탈 진짜 힘들거 같아요",
          "original_form": "왕따까지 당하면 멘탈 진짜 힘들거 같아요",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014545.1.16",
          "form": "맞아요ㅎ",
          "original_form": "맞아요ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014545.1.17",
          "form": "근데 저도 잘 모르지만 우울증많을것같긴해요",
          "original_form": "근데 저도 잘 모르지만 우울증많을것같긴해요",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014545.1.18",
          "form": "공인이라 못하는것도 만ㅍ고",
          "original_form": "공인이라 못하는것도 만ㅍ고",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014545.1.19",
          "form": "그리고 인기가 떨어진다 생각하면 ..ㅠㅠ",
          "original_form": "그리고 인기가 떨어진다 생각하면 ..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014545.1.20",
          "form": "누구한테 선뜻 말못하자나요",
          "original_form": "누구한테 선뜻 말못하자나요",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014545.1.21",
          "form": "종현이 그랬어서  \r\n저는 샤이니 팬도 아니였는데",
          "original_form": "종현이 그랬어서  \r\n저는 샤이니 팬도 아니였는데",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014545.1.22",
          "form": "온유가  종현이 작곡한  곡 부르는데 눈물이 다 나더라구요",
          "original_form": "온유가  종현이 작곡한  곡 부르는데 눈물이 다 나더라구요",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014545.1.23",
          "form": "맞아요ㅠㅍ",
          "original_form": "맞아요ㅠㅍ",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014545.1.24",
          "form": "근데 돈을많이 버는데도",
          "original_form": "근데 돈을많이 버는데도",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014545.1.25",
          "form": "그게 치료가 안되나봐요ㅠ",
          "original_form": "그게 치료가 안되나봐요ㅠ",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014545.1.26",
          "form": "상담도 받아볼수도 있을꺼고 주위사람들에게 알릴수도 있을껀데...",
          "original_form": "상담도 받아볼수도 있을꺼고 주위사람들에게 알릴수도 있을껀데...",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014545.1.27",
          "form": "그래서 마음의 병 ㅜㅜ",
          "original_form": "그래서 마음의 병 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014545.1.28",
          "form": "선뜻 말못하는것도 병이죠ㅍㅍ",
          "original_form": "선뜻 말못하는것도 병이죠ㅍㅍ",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014545.1.29",
          "form": "근데 진짜 공감하는게",
          "original_form": "근데 진짜 공감하는게",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014545.1.30",
          "form": "내 찐친아니면 나의 고민을 털어놓을 상대가 많이 없죠ㅍ",
          "original_form": "내 찐친아니면 나의 고민을 털어놓을 상대가 많이 없죠ㅍ",
          "speaker_id": "2",
          "time": "20210825 19:51"
        }
      ]
