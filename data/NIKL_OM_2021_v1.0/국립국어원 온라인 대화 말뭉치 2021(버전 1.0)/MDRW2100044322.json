      "utterance": [
        {
          "id": "MDRW2100044322.1.1",
          "form": "네 제일 좋아하는 맛이예요",
          "original_form": "네 제일 좋아하는 맛이예요",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044322.1.2",
          "form": "아몬드봉봉이랑",
          "original_form": "아몬드봉봉이랑",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044322.1.3",
          "form": "아봉 !",
          "original_form": "아봉 !",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044322.1.4",
          "form": "민초드세요?",
          "original_form": "민초드세요?",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044322.1.5",
          "form": "네 민초 좋아합니다",
          "original_form": "네 민초 좋아합니다",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044322.1.6",
          "form": "ㅋㅋㅋ 체리쥬빌레는용",
          "original_form": "ㅋㅋㅋ 체리쥬빌레는용",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044322.1.7",
          "form": "전에 트리플민초 나왔을때 좋았어요",
          "original_form": "전에 트리플민초 나왔을때 좋았어요",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044322.1.8",
          "form": "체리쥬빌레도 사랑이죠",
          "original_form": "체리쥬빌레도 사랑이죠",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044322.1.9",
          "form": "오오 ㅋㅋㅋ 잘맞네요",
          "original_form": "오오 ㅋㅋㅋ 잘맞네요",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044322.1.10",
          "form": "전 피스타치오도 조아하요",
          "original_form": "전 피스타치오도 조아하요",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044322.1.11",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044322.1.12",
          "form": "꺅 배라 털고싶네요",
          "original_form": "꺅 배라 털고싶네요",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044322.1.13",
          "form": "메뉴 얘기하다보니",
          "original_form": "메뉴 얘기하다보니",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044322.1.14",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044322.1.15",
          "form": "ㅋㅋㅋ 그죵",
          "original_form": "ㅋㅋㅋ 그죵",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044322.1.16",
          "form": "아이스크림 또 머조아하세여",
          "original_form": "아이스크림 또 머조아하세여",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044322.1.17",
          "form": "소프트 아이스크림도 좋아해요",
          "original_form": "소프트 아이스크림도 좋아해요",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044322.1.18",
          "form": "앗 맥날거도 맛잇죠",
          "original_form": "앗 맥날거도 맛잇죠",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044322.1.19",
          "form": "폴바셋!",
          "original_form": "폴바셋!",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044322.1.20",
          "form": "아 폴바셋 좋죠",
          "original_form": "아 폴바셋 좋죠",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044322.1.21",
          "form": "우유맛 진햐",
          "original_form": "우유맛 진햐",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044322.1.22",
          "form": "먹으러가고싶어요",
          "original_form": "먹으러가고싶어요",
          "speaker_id": "1",
