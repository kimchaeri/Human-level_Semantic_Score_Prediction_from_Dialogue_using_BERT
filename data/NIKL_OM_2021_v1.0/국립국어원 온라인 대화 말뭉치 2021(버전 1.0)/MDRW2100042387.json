      "utterance": [
        {
          "id": "MDRW2100042387.1.1",
          "form": "삼양라면 매운맛이 있나요 ?",
          "original_form": "삼양라면 매운맛이 있나요 ?",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042387.1.2",
          "form": "네 오래안됐을걸요!! 나온지ㅋㅋ",
          "original_form": "네 오래안됐을걸요!! 나온지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042387.1.3",
          "form": "많이 맵나요?",
          "original_form": "많이 맵나요?",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042387.1.4",
          "form": "어 저는 조금 맵더라고요",
          "original_form": "어 저는 조금 맵더라고요",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042387.1.5",
          "form": "매운거잘드세요?",
          "original_form": "매운거잘드세요?",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042387.1.6",
          "form": "그렇게 잘 먹진않아요 ㅎㅎ",
          "original_form": "그렇게 잘 먹진않아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042387.1.7",
          "form": "계란넣어서 한번 드셔보세요!",
          "original_form": "계란넣어서 한번 드셔보세요!",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042387.1.8",
          "form": "계란넣으면 좀 덜 맵죠?",
          "original_form": "계란넣으면 좀 덜 맵죠?",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042387.1.9",
          "form": "네ㅋㅋ계란덕에",
          "original_form": "네ㅋㅋ계란덕에",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042387.1.10",
          "form": "밥도먹었어요?",
          "original_form": "밥도먹었어요?",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042387.1.11",
          "form": "아 안먹었어요ㅠ",
          "original_form": "아 안먹었어요ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042387.1.12",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042387.1.13",
          "form": "배가좀부르더라고요ㅋㅋㅋ",
          "original_form": "배가좀부르더라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042387.1.14",
          "form": "계란을 넣으면 좀 더 든든하죠",
          "original_form": "계란을 넣으면 좀 더 든든하죠",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042387.1.15",
          "form": "맞아요 계란덕분인듯ㅋㅋ",
          "original_form": "맞아요 계란덕분인듯ㅋㅋ",
