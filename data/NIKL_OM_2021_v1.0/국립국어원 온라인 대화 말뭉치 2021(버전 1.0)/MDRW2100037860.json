      "utterance": [
        {
          "id": "MDRW2100037860.1.1",
          "form": "그럼 혼자 사시면서 키우시는건가요?",
          "original_form": "그럼 혼자 사시면서 키우시는건가요?",
          "speaker_id": "1",
          "time": "20210901 19:54"
        },
        {
          "id": "MDRW2100037860.1.2",
          "form": "전 사실 알레르기있는거 몰랐을땐 독립할까 생각도했어요 ㅋㅋㅋ",
          "original_form": "전 사실 알레르기있는거 몰랐을땐 독립할까 생각도했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100037860.1.3",
          "form": "네네 ㅎㅎ 어떻게 알레르기 알게 되셨나요 ㅜㅜ",
          "original_form": "네네 ㅎㅎ 어떻게 알레르기 알게 되셨나요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100037860.1.4",
          "form": "아는 언니가 자취를 시작했는데",
          "original_form": "아는 언니가 자취를 시작했는데",
          "speaker_id": "1",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100037860.1.5",
          "form": "그 언니 룸메가 고양이를 길렀어요",
          "original_form": "그 언니 룸메가 고양이를 길렀어요",
          "speaker_id": "1",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100037860.1.6",
          "form": "러블로!",
          "original_form": "러블로!",
          "speaker_id": "1",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100037860.1.7",
          "form": "잘 만지지도 못하시겠네요? ㅠㅠ",
          "original_form": "잘 만지지도 못하시겠네요? ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100037860.1.8",
          "form": "그래서 저는 그때 처음 고양이를 진짜 가까이에서 만났거든요",
          "original_form": "그래서 저는 그때 처음 고양이를 진짜 가까이에서 만났거든요",
          "speaker_id": "1",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100037860.1.9",
          "form": "근데 걔가 와 진짜오래전인데 이름도기억나요",
          "original_form": "근데 걔가 와 진짜오래전인데 이름도기억나요",
          "speaker_id": "1",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100037860.1.10",
          "form": "루이비통에서 따와서 name3였어요",
          "original_form": "루이비통에서 따와서 &name3&였어요",
          "speaker_id": "1",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100037860.1.11",
          "form": "완전 개냥이였는데",
          "original_form": "완전 개냥이였는데",
          "speaker_id": "1",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100037860.1.12",
          "form": "저희 다 처음보는 사람인데 와서 한번씩 무릎에 누워주고",
          "original_form": "저희 다 처음보는 사람인데 와서 한번씩 무릎에 누워주고",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100037860.1.13",
          "form": "서비스로 안겨주고 가더라고요",
          "original_form": "서비스로 안겨주고 가더라고요",
          "speaker_id": "1",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100037860.1.14",
          "form": "ㅠㅠ 접대냥인가요",
          "original_form": "ㅠㅠ 접대냥인가요",
          "speaker_id": "2",
          "time": "20210901 20:16"
        },
        {
          "id": "MDRW2100037860.1.15",
          "form": "넘나 사람을 좋아하나봐요",
          "original_form": "넘나 사람을 좋아하나봐요",
          "speaker_id": "2",
          "time": "20210901 20:16"
        },
        {
          "id": "MDRW2100037860.1.16",
          "form": "지금 생각해보니 요즘말로 접대냥이었네요",
          "original_form": "지금 생각해보니 요즘말로 접대냥이었네요",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037860.1.17",
          "form": "근데 저한텐 안오는거에요ㅜㅜ",
          "original_form": "근데 저한텐 안오는거에요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037860.1.18",
          "form": "그언니가 안겨줘도 액체처럼 슥 빠져나가버림",
          "original_form": "그언니가 안겨줘도 액체처럼 슥 빠져나가버림",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037860.1.19",
          "form": "생각해보니그래도 발톱으로 안할퀸거보면 진짜착한애였네요",
          "original_form": "생각해보니그래도 발톱으로 안할퀸거보면 진짜착한애였네요",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037860.1.20",
          "form": "ㅠㅠ 맴도 몰라주고",
          "original_form": "ㅠㅠ 맴도 몰라주고",
          "speaker_id": "2",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100037860.1.21",
          "form": "지금 울집애들은 제가 먹는 꽃게 같이 먹고있네요 ㅋㅋ",
          "original_form": "지금 울집애들은 제가 먹는 꽃게 같이 먹고있네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100037860.1.22",
          "form": "아이고 포식하네요 ㅋㅋ",
          "original_form": "아이고 포식하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100037860.1.23",
          "form": "지금 꽃게 철인데",
          "original_form": "지금 꽃게 철인데",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100037860.1.24",
          "form": "근데 그name3이네 놀러가서 한시간정도 수다를 떠는데 제가 갑자기 목소리가 안나오더라고요",
          "original_form": "근데 그&name3&이네 놀러가서 한시간정도 수다를 떠는데 제가 갑자기 목소리가 안나오더라고요",
          "speaker_id": "1",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037860.1.25",
          "form": "홈플러스에 살아있는걸 파는 ㄷㄷ",
          "original_form": "홈플러스에 살아있는걸 파는 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037860.1.26",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037860.1.27",
          "form": "심각했네요",
          "original_form": "심각했네요",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037860.1.28",
          "form": "목이쉰건가 생각했어요 그냥 ㅋㅋ",
          "original_form": "목이쉰건가 생각했어요 그냥 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037860.1.29",
          "form": "목쉰사람 목소리ㅋㅋ",
          "original_form": "목쉰사람 목소리ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037860.1.30",
          "form": "너무수다 떨어서",
          "original_form": "너무수다 떨어서",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037860.1.31",
          "form": "눈이가렵고 팔도 가려웠는데 좀지나니까",
          "original_form": "눈이가렵고 팔도 가려웠는데 좀지나니까",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037860.1.32",
          "form": "그니까요 ㄷㄷ 고양이 알레르기라고 생각못할듯",
          "original_form": "그니까요 ㄷㄷ 고양이 알레르기라고 생각못할듯",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100037860.1.33",
          "form": "그건 그냥 또고양이털이 많아서 그런갑다 ㅋㅋ",
          "original_form": "그건 그냥 또고양이털이 많아서 그런갑다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100037860.1.34",
          "form": "고양이 사는집에 처음가봐서요 ㅋㅋㅋ",
          "original_form": "고양이 사는집에 처음가봐서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100037860.1.35",
          "form": "그럴만두 해요 ㅋㅋㅋㅋ",
          "original_form": "그럴만두 해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100037860.1.36",
          "form": "저도 4년전 처음 고양이 만져봤어요",
