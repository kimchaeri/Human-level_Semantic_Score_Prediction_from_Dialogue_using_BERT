      "utterance": [
        {
          "id": "MDRW2100045419.1.1",
          "form": "아뇨 요즘 노래를 듣는 것만 들어서요",
          "original_form": "아뇨 요즘 노래를 듣는 것만 들어서요",
          "speaker_id": "2",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045419.1.2",
          "form": "악뮤 노래 전쟁터인가 좋아요",
          "original_form": "악뮤 노래 전쟁터인가 좋아요",
          "speaker_id": "1",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045419.1.3",
          "form": "그 수현 목소리가 너무 좋아서",
          "original_form": "그 수현 목소리가 너무 좋아서",
          "speaker_id": "1",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045419.1.4",
          "form": "수현 목소리가 좋긴 하죠",
          "original_form": "수현 목소리가 좋긴 하죠",
          "speaker_id": "2",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045419.1.5",
          "form": "한번 들어봐야겠어요",
          "original_form": "한번 들어봐야겠어요",
          "speaker_id": "2",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045419.1.6",
          "form": "네 꼭 들어주세요 ㅠ 가왕이면 낙하도 ㅡ",
          "original_form": "네 꼭 들어주세요 ㅠ 가왕이면 낙하도 ㅡ",
          "speaker_id": "1",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045419.1.7",
          "form": "name2님은 주로 뭐 들으세요?",
          "original_form": "&name2&님은 주로 뭐 들으세요?",
          "speaker_id": "1",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045419.1.8",
          "form": "저는 팝송 주로 들어요",
          "original_form": "저는 팝송 주로 들어요",
          "speaker_id": "2",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045419.1.9",
          "form": "어떤 노래 들으세요?",
          "original_form": "어떤 노래 들으세요?",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045419.1.10",
          "form": "유튜브로 직키픽님거? 막 들어요",
          "original_form": "유튜브로 직키픽님거? 막 들어요",
          "speaker_id": "2",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045419.1.11",
          "form": "아 저도 그분꺼 봐요 !",
          "original_form": "아 저도 그분꺼 봐요 !",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045419.1.12",
          "form": "그중에서 제목이 지옥에도 비는 내리고 였나",
          "original_form": "그중에서 제목이 지옥에도 비는 내리고 였나",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045419.1.13",
          "form": "앜ㅋㅋㅋ 그거 컨셉이 진짜 좋았어요",
          "original_form": "앜ㅋㅋㅋ 그거 컨셉이 진짜 좋았어요",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045419.1.14",
          "form": "그게 노래가 귀에 착찻 붙더라구요",
          "original_form": "그게 노래가 귀에 착찻 붙더라구요",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045419.1.15",
          "form": "아 저랑 노래 취향 좀 맞으시는거 같은데",
          "original_form": "아 저랑 노래 취향 좀 맞으시는거 같은데",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045419.1.16",
          "form": "추천 좀 해주세요",
          "original_form": "추천 좀 해주세요",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045419.1.17",
          "form": "노래 제목이 쥬시인가 ?? 그거 자신감 차올라서 좋아요",
          "original_form": "노래 제목이 쥬시인가 ?? 그거 자신감 차올라서 좋아요",
          "speaker_id": "2",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045419.1.18",
          "form": "오 그거 한번 들어볼게요 !",
          "original_form": "오 그거 한번 들어볼게요 !",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045419.1.19",
          "form": "추천 감사합니다",
          "original_form": "추천 감사합니다",
