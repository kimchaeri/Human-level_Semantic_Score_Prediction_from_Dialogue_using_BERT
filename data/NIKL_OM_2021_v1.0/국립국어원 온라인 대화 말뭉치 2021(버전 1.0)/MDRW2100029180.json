      "utterance": [
        {
          "id": "MDRW2100029180.1.1",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "1",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100029180.1.2",
          "form": "넵 저녁은 서브웨이 먹었어요",
          "original_form": "넵 저녁은 서브웨이 먹었어요",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029180.1.3",
          "form": "아 좋지요  샌드위치죠?",
          "original_form": "아 좋지요  샌드위치죠?",
          "speaker_id": "1",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029180.1.4",
          "form": "무슨맛드셨어요?",
          "original_form": "무슨맛드셨어요?",
          "speaker_id": "1",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029180.1.5",
          "form": "넵 다이어트하느라 토핑은 이것저것 못 넣지만 맛있게 먹었어요",
          "original_form": "넵 다이어트하느라 토핑은 이것저것 못 넣지만 맛있게 먹었어요",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029180.1.6",
          "form": "로티셰리 치킨이었나? 그럴거에요",
          "original_form": "로티셰리 치킨이었나? 그럴거에요",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029180.1.7",
          "form": "네 저는 전복죽 먹었어요",
          "original_form": "네 저는 전복죽 먹었어요",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029180.1.8",
          "form": "치킨좋죠^^",
          "original_form": "치킨좋죠^^",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029180.1.9",
          "form": "전복죽! 맛있겠어요",
          "original_form": "전복죽! 맛있겠어요",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029180.1.10",
          "form": "직접 끓이셨나요?",
          "original_form": "직접 끓이셨나요?",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029180.1.11",
          "form": "서브웨이 샐러드도 드셔보셨나요?",
          "original_form": "서브웨이 샐러드도 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029180.1.12",
          "form": "어머니께서 요리해주셨어요",
          "original_form": "어머니께서 요리해주셨어요",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029180.1.13",
          "form": "샐러드는 잘 안먹게 되는 것 같아요",
          "original_form": "샐러드는 잘 안먹게 되는 것 같아요",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029180.1.14",
          "form": "저녁으로 양이 부족하지않으셔요?",
          "original_form": "저녁으로 양이 부족하지않으셔요?",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029180.1.15",
          "form": "샌드위치는 간식같아요",
          "original_form": "샌드위치는 간식같아요",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029180.1.16",
          "form": "다이어트하려면 은근히 영양소 든든하게 챙겨야해서",
          "original_form": "다이어트하려면 은근히 영양소 든든하게 챙겨야해서",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029180.1.17",
          "form": "채소 좋죠!",
          "original_form": "채소 좋죠!",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029180.1.18",
          "form": "풀떼기로는 기력이 없더라구요",
          "original_form": "풀떼기로는 기력이 없더라구요",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029180.1.19",
          "form": "고기를 넣어야 맛있지요",
          "original_form": "고기를 넣어야 맛있지요",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029180.1.20",
          "form": "과일은 좋아하시나요?",
          "original_form": "과일은 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210903 00:09"
        }
      ]
