      "utterance": [
        {
          "id": "MDRW2100030288.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100030288.1.2",
          "form": "저녁 먹었어요 name2님도 드셨어요??",
          "original_form": "저녁 먹었어요 &name2&님도 드셨어요??",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100030288.1.3",
          "form": "네! 저 열심히 아침점심저녁",
          "original_form": "네! 저 열심히 아침점심저녁",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100030288.1.4",
          "form": "식단 열심히 해놓고 과자 한박스 먹었네요",
          "original_form": "식단 열심히 해놓고 과자 한박스 먹었네요",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100030288.1.5",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100030288.1.6",
          "form": "ㅜㅜ 과자 한박스밖에 안되니까 괜찮을거에요",
          "original_form": "ㅜㅜ 과자 한박스밖에 안되니까 괜찮을거에요",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100030288.1.7",
          "form": "저는 오늘 마라낙지젓을 먹어봤어요",
          "original_form": "저는 오늘 마라낙지젓을 먹어봤어요",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100030288.1.8",
          "form": "근데 입맛에 아니었어욬ㅋㅋㅋㅋ",
          "original_form": "근데 입맛에 아니었어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100030288.1.9",
          "form": "ㅋㅋㅋㅋㅋ오 마라낙지젓",
          "original_form": "ㅋㅋㅋㅋㅋ오 마라낙지젓",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100030288.1.10",
          "form": "완전 특이해요!",
          "original_form": "완전 특이해요!",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100030288.1.11",
          "form": "어떠셨어요?",
          "original_form": "어떠셨어요?",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100030288.1.12",
          "form": "제가 산건 아니랔ㅋㅋㅋ",
          "original_form": "제가 산건 아니랔ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100030288.1.13",
          "form": "마라맛 진짜 강해요",
          "original_form": "마라맛 진짜 강해요",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100030288.1.14",
          "form": "마라 좋아하시면 추천해요",
          "original_form": "마라 좋아하시면 추천해요",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100030288.1.15",
          "form": "마라는 좋아하는데 뭔가 상상이 안가요!",
          "original_form": "마라는 좋아하는데 뭔가 상상이 안가요!",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100030288.1.16",
          "form": "저도요 이런 제품도 나오다니",
          "original_form": "저도요 이런 제품도 나오다니",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100030288.1.17",
          "form": "마라가 진짜 인기가 있는거 같아요",
          "original_form": "마라가 진짜 인기가 있는거 같아요",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100030288.1.18",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100030288.1.19",
          "form": "마라샹궈랑 마라탕중에 어던거 좋어하새요?",
          "original_form": "마라샹궈랑 마라탕중에 어던거 좋어하새요?",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100030288.1.20",
          "form": "저는 마라샹궈는 안먹어봤어요",
          "original_form": "저는 마라샹궈는 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100030288.1.21",
          "form": "마라탕은 잘한다고 하는곳에서 먹은건 맛있었는데",
          "original_form": "마라탕은 잘한다고 하는곳에서 먹은건 맛있었는데",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100030288.1.22",
          "form": "아닌곳에서는 좀 아니다 싶었어요 ㅋㅋㅋㅋ",
          "original_form": "아닌곳에서는 좀 아니다 싶었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100030288.1.23",
          "form": "맞아요 약간 그런게 있죠!",
          "original_form": "맞아요 약간 그런게 있죠!",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100030288.1.24",
          "form": "저는 샹궈를 더 좋아해요!",
          "original_form": "저는 샹궈를 더 좋아해요!",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100030288.1.25",
          "form": "그러시군요!! 저도 먹어보고 싶어요",
          "original_form": "그러시군요!! 저도 먹어보고 싶어요",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100030288.1.26",
          "form": "제가 중국소스를 하나샀는데 그게 제입맛에는 마라보다 낫더라고요",
          "original_form": "제가 중국소스를 하나샀는데 그게 제입맛에는 마라보다 낫더라고요",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100030288.1.27",
          "form": "무슨 이름이었는지 라오깐마였나??",
          "original_form": "무슨 이름이었는지 라오깐마였나??",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100030288.1.28",
          "form": "오 라오깐마요? 저는 아직 안먹어봤어요!",
          "original_form": "오 라오깐마요? 저는 아직 안먹어봤어요!",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100030288.1.29",
          "form": "아니 저 완전",
          "original_form": "아니 저 완전",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100030288.1.30",
          "form": "어묵 얼마나 좋아하냐면",
          "original_form": "어묵 얼마나 좋아하냐면",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100030288.1.31",
          "form": "길가눈데 어묵집이 잇는거애요",
          "original_form": "길가눈데 어묵집이 잇는거애요",
          "speaker_id": "2",
          "time": "20210906 22:47"
        }
      ]
