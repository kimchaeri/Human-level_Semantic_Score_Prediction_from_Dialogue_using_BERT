      "utterance": [
        {
          "id": "MDRW2100004565.1.1",
          "form": "호준이가 설치부가 자기 구박했다고 차승원한테 바로 애기한다 귀업다",
          "original_form": "호준이가 설치부가 자기 구박했다고 차승원한테 바로 애기한다 귀업다",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004565.1.2",
          "form": "호준이 은근 귀엽네ㅋ",
          "original_form": "호준이 은근 귀엽네ㅋ",
          "speaker_id": "1",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004565.1.3",
          "form": "엉 아직 얘기다 애기",
          "original_form": "엉 아직 얘기다 애기",
          "speaker_id": "2",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004565.1.4",
          "form": "애기라도 저때 30대 아니가?",
          "original_form": "애기라도 저때 30대 아니가?",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004565.1.5",
          "form": "유해진이랑 남주혁이 하는 아재개그 한개도 재미없는데",
          "original_form": "유해진이랑 남주혁이 하는 아재개그 한개도 재미없는데",
          "speaker_id": "2",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004565.1.6",
          "form": "유해진이 아재개그 엄청 하지",
          "original_form": "유해진이 아재개그 엄청 하지",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004565.1.7",
          "form": "30대지 ㅋㅋ40대가 보기에는 아직 얘기 ㅋㅋ",
          "original_form": "30대지 ㅋㅋ40대가 보기에는 아직 얘기 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004565.1.8",
          "form": "지금 호준이도 40대쯤 됐지",
          "original_form": "지금 호준이도 40대쯤 됐지",
          "speaker_id": "1",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004565.1.9",
          "form": "아니 지금 아직 30대 초반",
          "original_form": "아니 지금 아직 30대 초반",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004565.1.10",
          "form": "호준이가?  아닐껄?나이 좀 있다",
          "original_form": "호준이가?  아닐껄?나이 좀 있다",
          "speaker_id": "1",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004565.1.11",
          "form": "올해 죽굴도편에서 얘기하더라 자기 이제 30대 초반이라고",
          "original_form": "올해 죽굴도편에서 얘기하더라 자기 이제 30대 초반이라고",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004565.1.12",
          "form": "그렇네 37",
          "original_form": "그렇네 37",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004565.1.13",
          "form": "84년생이네. 한창 나오때가 30 대 초",
          "original_form": "84년생이네. 한창 나오때가 30 대 초",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004565.1.14",
          "form": "인상이 착해보여서 어려보인다",
          "original_form": "인상이 착해보여서 어려보인다",
