      "utterance": [
        {
          "id": "MDRW2100029976.1.1",
          "form": "미용에 관심 있으신가요?",
          "original_form": "미용에 관심 있으신가요?",
          "speaker_id": "2",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100029976.1.2",
          "form": "화장은 하긴합니당ㅋㅋㅋ",
          "original_form": "화장은 하긴합니당ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100029976.1.3",
          "form": "근데요즘 마스크써서 화장도 잘 안하네요 ㅋㅋㅋ",
          "original_form": "근데요즘 마스크써서 화장도 잘 안하네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100029976.1.4",
          "form": "에이 마스크쓰는데 굳이 화장할필요 있나요ㅎㅎ",
          "original_form": "에이 마스크쓰는데 굳이 화장할필요 있나요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100029976.1.5",
          "form": "클렌징도 번거롭고 피부에도 안좋잖아요",
          "original_form": "클렌징도 번거롭고 피부에도 안좋잖아요",
          "speaker_id": "2",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100029976.1.6",
          "form": "맞아요~~ 마스크쓰니까 참 편하네요~~",
          "original_form": "맞아요~~ 마스크쓰니까 참 편하네요~~",
          "speaker_id": "1",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029976.1.7",
          "form": "운동은 좀 하시나여?",
          "original_form": "운동은 좀 하시나여?",
          "speaker_id": "1",
          "time": "20210903 17:46"
        },
        {
          "id": "MDRW2100029976.1.8",
          "form": "저는 운동 하나도 안했어요ㅠㅎㅎ",
          "original_form": "저는 운동 하나도 안했어요ㅠㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100029976.1.9",
          "form": "아 저도 안해요 돈주고 하라해도 못할듯요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 저도 안해요 돈주고 하라해도 못할듯요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100029976.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ돈받고 하면 할거같습니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ돈받고 하면 할거같습니다",
          "speaker_id": "2",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100029976.1.11",
          "form": "악 ㅋㅋㅋㅋㅋㅋ 저도...돈준다면 막상 할수도...ㅋㅋㅋ",
          "original_form": "악 ㅋㅋㅋㅋㅋㅋ 저도...돈준다면 막상 할수도...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100029976.1.12",
          "form": "운동은 해야한다더라구요 시간이지날수록 체감이 확확된다구..",
          "original_form": "운동은 해야한다더라구요 시간이지날수록 체감이 확확된다구..",
          "speaker_id": "2",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100029976.1.13",
          "form": "그건 맞는거 같아요 저도 하루하루 체력떨어지는거 느끼고 있답니다",
          "original_form": "그건 맞는거 같아요 저도 하루하루 체력떨어지는거 느끼고 있답니다",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100029976.1.14",
          "form": "유산소운동이라도 좀 해야하는디",
          "original_form": "유산소운동이라도 좀 해야하는디",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100029976.1.15",
          "form": "계단오르기가 힘드네요",
          "original_form": "계단오르기가 힘드네요",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100029976.1.16",
          "form": "엄청 창피한데 어제 팔굽혀펴기 3개하고 지금 온몸이 쑤셔요ㅋㅋㅋ",
          "original_form": "엄청 창피한데 어제 팔굽혀펴기 3개하고 지금 온몸이 쑤셔요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100029976.1.17",
          "form": "악ㅋㅋㅋㅋㅋ 그래도 운동하려고 하시나보네요 저는 작심삼일 그 자체라 ㅎㅎ",
          "original_form": "악ㅋㅋㅋㅋㅋ 그래도 운동하려고 하시나보네요 저는 작심삼일 그 자체라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100029976.1.18",
          "form": "작심삼일 10번만하면 한달이래요ㅎㅎ",
          "original_form": "작심삼일 10번만하면 한달이래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 18:03"
        },
        {
