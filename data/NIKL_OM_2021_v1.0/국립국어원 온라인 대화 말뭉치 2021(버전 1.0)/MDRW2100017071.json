      "utterance": [
        {
          "id": "MDRW2100017071.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017071.1.2",
          "form": "최근에 구매하신 물건이 있나요?",
          "original_form": "최근에 구매하신 물건이 있나요?",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017071.1.3",
          "form": "저는 최근에 향수 샀습니다 ㅎㅎ",
          "original_form": "저는 최근에 향수 샀습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017071.1.4",
          "form": "좋아하는 향수 있으세요?",
          "original_form": "좋아하는 향수 있으세요?",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017071.1.5",
          "form": "오 향수 좋죠",
          "original_form": "오 향수 좋죠",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017071.1.6",
          "form": "저는 여름엔 시트러스 계열 향수 썼는데",
          "original_form": "저는 여름엔 시트러스 계열 향수 썼는데",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017071.1.7",
          "form": "이제 쌀쌀해지니 새로 하나 구입해야하나 싶어요",
          "original_form": "이제 쌀쌀해지니 새로 하나 구입해야하나 싶어요",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017071.1.8",
          "form": "오~ 상큼한 향! 쌀쌀해지면 어떤향 쓰세요?",
          "original_form": "오~ 상큼한 향! 쌀쌀해지면 어떤향 쓰세요?",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017071.1.9",
          "form": "추천해주실만한 것 있나요?",
          "original_form": "추천해주실만한 것 있나요?",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017071.1.10",
          "form": "저도 꿀팁좀 얻어가려구 ㅋㅋㅋ",
          "original_form": "저도 꿀팁좀 얻어가려구 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017071.1.11",
          "form": "저는 사실 계절에 상관없이 그때 좋은거 써요 ㅎㄹㄹ",
          "original_form": "저는 사실 계절에 상관없이 그때 좋은거 써요 ㅎㄹㄹ",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017071.1.12",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "1",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017071.1.13",
          "form": "저도 사실 그때그때 ㅎㅎ",
          "original_form": "저도 사실 그때그때 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017071.1.14",
          "form": "매장에 가서 시향해보고 사기도 하고",
          "original_form": "매장에 가서 시향해보고 사기도 하고",
          "speaker_id": "1",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017071.1.15",
          "form": "그렇군요 ㅎㅎㅎ",
          "original_form": "그렇군요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017071.1.16",
          "form": "저는 이번에 불가리 아메시스트 샀어요!",
          "original_form": "저는 이번에 불가리 아메시스트 샀어요!",
          "speaker_id": "2",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017071.1.17",
          "form": "오호 들어본 적 있어요",
          "original_form": "오호 들어본 적 있어요",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017071.1.18",
          "form": "만족하시나요?",
          "original_form": "만족하시나요?",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017071.1.19",
          "form": "넹ㅋㅋㅋ 원래 사용하던거라~ 뭔가 향이 단정하고 무난해요 ㅎㅎ",
          "original_form": "넹ㅋㅋㅋ 원래 사용하던거라~ 뭔가 향이 단정하고 무난해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017071.1.20",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017071.1.21",
          "form": "처음에 선물받아서 썼었는데 그땐 별로였거든요",
          "original_form": "처음에 선물받아서 썼었는데 그땐 별로였거든요",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017071.1.22",
          "form": "찾아보니 스테디템인거 같더라구요.",
          "original_form": "찾아보니 스테디템인거 같더라구요.",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017071.1.23",
          "form": "그 사이에 상품에 대한 느낌이 달라지셨나봐요",
          "original_form": "그 사이에 상품에 대한 느낌이 달라지셨나봐요",
