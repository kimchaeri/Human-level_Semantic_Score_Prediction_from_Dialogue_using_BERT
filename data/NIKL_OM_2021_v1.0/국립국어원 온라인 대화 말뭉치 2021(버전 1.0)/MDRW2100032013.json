      "utterance": [
        {
          "id": "MDRW2100032013.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210905 10:52"
        },
        {
          "id": "MDRW2100032013.1.2",
          "form": "제주 라이프 좋은가요~?:)",
          "original_form": "제주 라이프 좋은가요~?:)",
          "speaker_id": "1",
          "time": "20210905 10:53"
        },
        {
          "id": "MDRW2100032013.1.3",
          "form": "좋은점도 있고 별로인 것도 있어요",
          "original_form": "좋은점도 있고 별로인 것도 있어요",
          "speaker_id": "2",
          "time": "20210905 11:15"
        },
        {
          "id": "MDRW2100032013.1.4",
          "form": "하루하루 여행다니는 마음이예여",
          "original_form": "하루하루 여행다니는 마음이예여",
          "speaker_id": "2",
          "time": "20210905 11:15"
        },
        {
          "id": "MDRW2100032013.1.5",
          "form": "ㅎㅎ어떤점이 좋으세요?",
          "original_form": "ㅎㅎ어떤점이 좋으세요?",
          "speaker_id": "1",
          "time": "20210905 11:36"
        },
        {
          "id": "MDRW2100032013.1.6",
          "form": "해변과 오름 모두 버스타고 원할때",
          "original_form": "해변과 오름 모두 버스타고 원할때",
          "speaker_id": "2",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100032013.1.7",
          "form": "갈수있는 거요",
          "original_form": "갈수있는 거요",
          "speaker_id": "2",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100032013.1.8",
          "form": "멋지네요~",
          "original_form": "멋지네요~",
          "speaker_id": "1",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032013.1.9",
          "form": "휴직하고싶어요 ㅋㅋ",
          "original_form": "휴직하고싶어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032013.1.10",
          "form": "제주도 와보셨나요",
          "original_form": "제주도 와보셨나요",
          "speaker_id": "2",
          "time": "20210905 16:45"
        },
        {
          "id": "MDRW2100032013.1.11",
          "form": "성인되고는 안가봤어요",
          "original_form": "성인되고는 안가봤어요",
          "speaker_id": "1",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100032013.1.12",
          "form": "그러셨군요",
          "original_form": "그러셨군요",
          "speaker_id": "2",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100032013.1.13",
          "form": "해외여행은 어떠세요",
          "original_form": "해외여행은 어떠세요",
          "speaker_id": "2",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100032013.1.14",
          "form": "완전 그리워하고이씀니다",
          "original_form": "완전 그리워하고이씀니다",
          "speaker_id": "1",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100032013.1.15",
          "form": "코로나 풀리면 어디 가고 싶으세요",
          "original_form": "코로나 풀리면 어디 가고 싶으세요",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032013.1.16",
          "form": "발리요!",
          "original_form": "발리요!",
          "speaker_id": "1",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100032013.1.17",
          "form": "그렇게이쁘다던데",
          "original_form": "그렇게이쁘다던데",
          "speaker_id": "1",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100032013.1.18",
          "form": "우와 저는 쿠알라룸프르요",
          "original_form": "우와 저는 쿠알라룸프르요",
          "speaker_id": "2",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100032013.1.19",
          "form": "어떤걸 보고싶으세요?",
          "original_form": "어떤걸 보고싶으세요?",
          "speaker_id": "1",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100032013.1.20",
          "form": "반딪불이 투어 가고싶어요",
          "original_form": "반딪불이 투어 가고싶어요",
          "speaker_id": "2",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100032013.1.21",
          "form": "우와",
          "original_form": "우와",
