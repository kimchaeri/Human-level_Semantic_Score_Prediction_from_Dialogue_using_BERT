      "utterance": [
        {
          "id": "MDRW2100003440.1.1",
          "form": "왜 넘 맛있는뎅",
          "original_form": "왜 넘 맛있는뎅",
          "speaker_id": "2",
          "time": "20210820 23:09"
        },
        {
          "id": "MDRW2100003440.1.2",
          "form": "플라스틱에 뜨거운 음식 배달 안좋다",
          "original_form": "플라스틱에 뜨거운 음식 배달 안좋다",
          "speaker_id": "1",
          "time": "20210820 23:09"
        },
        {
          "id": "MDRW2100003440.1.3",
          "form": "집에서 만들어 먹어라",
          "original_form": "집에서 만들어 먹어라",
          "speaker_id": "1",
          "time": "20210820 23:10"
        },
        {
          "id": "MDRW2100003440.1.4",
          "form": "집에서 만들어먹으면 이런 맛 안나지요",
          "original_form": "집에서 만들어먹으면 이런 맛 안나지요",
          "speaker_id": "2",
          "time": "20210820 23:10"
        },
        {
          "id": "MDRW2100003440.1.5",
          "form": "한번씩만 먹는건데 어때~",
          "original_form": "한번씩만 먹는건데 어때~",
          "speaker_id": "2",
          "time": "20210820 23:10"
        },
        {
          "id": "MDRW2100003440.1.6",
          "form": "야식은 좋지않다",
          "original_form": "야식은 좋지않다",
          "speaker_id": "1",
          "time": "20210820 23:10"
        },
        {
          "id": "MDRW2100003440.1.7",
          "form": "낮에 먹던지",
          "original_form": "낮에 먹던지",
          "speaker_id": "1",
          "time": "20210820 23:10"
        },
        {
          "id": "MDRW2100003440.1.8",
          "form": "알았당 ㅋㅋㅋ",
          "original_form": "알았당 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 23:10"
        },
        {
          "id": "MDRW2100003440.1.9",
          "form": "그래 살찌니까",
          "original_form": "그래 살찌니까",
          "speaker_id": "1",
          "time": "20210820 23:10"
        },
        {
          "id": "MDRW2100003440.1.10",
          "form": "ㅋㅋ 건강도 챙겨야지",
          "original_form": "ㅋㅋ 건강도 챙겨야지",
          "speaker_id": "1",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003440.1.11",
          "form": "ㅠㅠ맛난건 왜 다 몸에 안좋은지",
          "original_form": "ㅠㅠ맛난건 왜 다 몸에 안좋은지",
          "speaker_id": "2",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003440.1.12",
          "form": "몸에 좋은 음식도 많다",
          "original_form": "몸에 좋은 음식도 많다",
          "speaker_id": "1",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003440.1.13",
          "form": "별로 없는듯..",
          "original_form": "별로 없는듯..",
          "speaker_id": "2",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003440.1.14",
          "form": "아니다 많다",
          "original_form": "아니다 많다",
          "speaker_id": "1",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003440.1.15",
          "form": "거짓말~",
          "original_form": "거짓말~",
          "speaker_id": "2",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003440.1.16",
          "form": "아니다 채소들 맛나잖아",
          "original_form": "아니다 채소들 맛나잖아",
          "speaker_id": "1",
          "time": "20210820 23:12"
        },
        {
          "id": "MDRW2100003440.1.17",
          "form": "아닌것 같은뎅..ㅋㅋ",
          "original_form": "아닌것 같은뎅..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 23:12"
        },
        {
          "id": "MDRW2100003440.1.18",
          "form": "내일은 뭐 해먹을까 주말인데~ 맛난거 해먹자",
          "original_form": "내일은 뭐 해먹을까 주말인데~ 맛난거 해먹자",
