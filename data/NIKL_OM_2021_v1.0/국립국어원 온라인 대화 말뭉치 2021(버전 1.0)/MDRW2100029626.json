      "utterance": [
        {
          "id": "MDRW2100029626.1.1",
          "form": "철위에 호박얼굴 놓으면 골렘됙",
          "original_form": "철위에 호박얼굴 놓으면 골렘됙",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029626.1.2",
          "form": "얼음덩이 위에 놓으면",
          "original_form": "얼음덩이 위에 놓으면",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029626.1.3",
          "form": "그럼 공격하나요",
          "original_form": "그럼 공격하나요",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029626.1.4",
          "form": "움직이는 눈사람돠요",
          "original_form": "움직이는 눈사람돠요",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029626.1.5",
          "form": "창조주를 공격하나요",
          "original_form": "창조주를 공격하나요",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029626.1.6",
          "form": "철골렘은",
          "original_form": "철골렘은",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029626.1.7",
          "form": "머리가 나빠서",
          "original_form": "머리가 나빠서",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029626.1.8",
          "form": "눈사람은 울라프네요",
          "original_form": "눈사람은 울라프네요",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029626.1.9",
          "form": "제가 주민공격하거나",
          "original_form": "제가 주민공격하거나",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029626.1.10",
          "form": "지를 공격하면 죽을때까지",
          "original_form": "지를 공격하면 죽을때까지",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029626.1.11",
          "form": "때려요",
          "original_form": "때려요",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029626.1.12",
          "form": "올라프죠 ㅋㅋㅋ",
          "original_form": "올라프죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029626.1.13",
          "form": "무섭군요",
          "original_form": "무섭군요",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029626.1.14",
          "form": "대신 만들어놓으면",
          "original_form": "대신 만들어놓으면",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029626.1.15",
          "form": "울라프군요 귀용",
          "original_form": "울라프군요 귀용",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029626.1.16",
          "form": "밤에 괴물 나타날때",
          "original_form": "밤에 괴물 나타날때",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029626.1.17",
          "form": "든든한가요",
          "original_form": "든든한가요",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029626.1.18",
          "form": "우선공격해줘요",
          "original_form": "우선공격해줘요",
          "speaker_id": "2",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029626.1.19",
          "form": "쓸모있네요",
          "original_form": "쓸모있네요",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029626.1.20",
          "form": "눈사람은 하등쓸모없어요",
          "original_form": "눈사람은 하등쓸모없어요",
          "speaker_id": "2",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029626.1.21",
          "form": "하나 키우겠읍니다",
          "original_form": "하나 키우겠읍니다",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029626.1.22",
          "form": "철골렘은든든",
          "original_form": "철골렘은든든",
          "speaker_id": "2",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029626.1.23",
          "form": "말만 많죠눈사람",
          "original_form": "말만 많죠눈사람",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029626.1.24",
          "form": "눈뿌리고 다녀요",
          "original_form": "눈뿌리고 다녀요",
          "speaker_id": "2",
          "time": "20210903 11:02"
        }
      ]
