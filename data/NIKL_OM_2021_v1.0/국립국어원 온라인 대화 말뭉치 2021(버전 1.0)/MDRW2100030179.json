      "utterance": [
        {
          "id": "MDRW2100030179.1.1",
          "form": "가을에 입고픈 패션은?",
          "original_form": "가을에 입고픈 패션은?",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030179.1.2",
          "form": "시크 도도한 가을여자 패션 이요",
          "original_form": "시크 도도한 가을여자 패션 이요",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030179.1.3",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030179.1.4",
          "form": "저도요 ㅠㅠ",
          "original_form": "저도요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030179.1.5",
          "form": "그러려고 단발했는데 시골소녀 되었어요",
          "original_form": "그러려고 단발했는데 시골소녀 되었어요",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030179.1.6",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030179.1.7",
          "form": "어렵네요ㅠㅠㅠㅠㅠㅠ",
          "original_form": "어렵네요ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030179.1.8",
          "form": "근데 미용실에서 많이 말리지 않았나요??",
          "original_form": "근데 미용실에서 많이 말리지 않았나요??",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030179.1.9",
          "form": "저희원장님은 그냥 바로 해줘요 ㅋㅋㅋ",
          "original_form": "저희원장님은 그냥 바로 해줘요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030179.1.10",
          "form": "ㅋㅋㅋㅋㅋ쿨하시당",
          "original_form": "ㅋㅋㅋㅋㅋ쿨하시당",
          "speaker_id": "2",
          "time": "20210904 22:22"
        },
        {
          "id": "MDRW2100030179.1.11",
          "form": "이틀 정도 지나니 이제 맘에드네요 조금씩 ㅋㅋㅋ",
          "original_form": "이틀 정도 지나니 이제 맘에드네요 조금씩 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100030179.1.12",
          "form": "어머 정말 별로 안되셨네요!!",
          "original_form": "어머 정말 별로 안되셨네요!!",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100030179.1.13",
          "form": "시간 되돌리고 싶으세요?ㅋㅋ",
          "original_form": "시간 되돌리고 싶으세요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100030179.1.14",
          "form": "구쵸ㅠㅠ 딱 마지막 더운날 더워서 확 잘랐는데",
          "original_form": "구쵸ㅠㅠ 딱 마지막 더운날 더워서 확 잘랐는데",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100030179.1.15",
          "form": "아니요 ㅋㅋㅋ 이렇게 안 잘렸음 계속 정신 못차렸을거에요",
          "original_form": "아니요 ㅋㅋㅋ 이렇게 안 잘렸음 계속 정신 못차렸을거에요",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100030179.1.16",
          "form": "단발병에 허덕이며 ㅋㅋㅋㅋ",
          "original_form": "단발병에 허덕이며 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100030179.1.17",
          "form": "이젠 알았으니 기를거에요 ㅋㅋㅋㅋ",
          "original_form": "이젠 알았으니 기를거에요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100030179.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100030179.1.19",
          "form": "거지존에서 또 자르시면 안되여,,",
          "original_form": "거지존에서 또 자르시면 안되여,,",
          "speaker_id": "2",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100030179.1.20",
          "form": "제가 그랬거든요 ㅋㅋㅋ",
          "original_form": "제가 그랬거든요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100030179.1.21",
          "form": "아 그러게요 ㅋㅋㅋㅋ",
          "original_form": "아 그러게요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100030179.1.22",
          "form": "기다려보겠습니다 우선은 너무 짧아요ㅠㅠ",
          "original_form": "기다려보겠습니다 우선은 너무 짧아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100030179.1.23",
          "form": "ㅠㅜ 원래 긴머리셨어요?",
          "original_form": "ㅠㅜ 원래 긴머리셨어요?",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100030179.1.24",
          "form": "어느정도 이셨어요?",
          "original_form": "어느정도 이셨어요?",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100030179.1.25",
          "form": "긴머리는 어깨밑으로요ㅠㅠ",
          "original_form": "긴머리는 어깨밑으로요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100030179.1.26",
          "form": "어머,,",
          "original_form": "어머,,",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100030179.1.27",
          "form": "주변반응 괜찮나요..?",
          "original_form": "주변반응 괜찮나요..?",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100030179.1.28",
          "form": "많이 잘렸어요 ㅋㅋㅋ그래도 단발병 치유",
          "original_form": "많이 잘렸어요 ㅋㅋㅋ그래도 단발병 치유",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100030179.1.29",
          "form": "네네 괜찮아요",
          "original_form": "네네 괜찮아요",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100030179.1.30",
          "form": "우와 다행이네요!ㅎㅎ",
          "original_form": "우와 다행이네요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100030179.1.31",
          "form": "단발병 한번 걸리면 끝이 없죠",
          "original_form": "단발병 한번 걸리면 끝이 없죠",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100030179.1.32",
          "form": "마치 아이패드병 처럼,,ㅎㅎ",
          "original_form": "마치 아이패드병 처럼,,ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100030179.1.33",
          "form": "맞아요 애플워치병도 있잖아요",
          "original_form": "맞아요 애플워치병도 있잖아요",
          "speaker_id": "1",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100030179.1.34",
          "form": "사야 끝나는 병",
          "original_form": "사야 끝나는 병",
          "speaker_id": "1",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100030179.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100030179.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋ마자요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ마자요",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100030179.1.37",
          "form": "저도 애플워치 병 걸려서",
          "original_form": "저도 애플워치 병 걸려서",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100030179.1.38",
          "form": "줄질까지하고 끝을 봤습니다 ㅋㅋㅋㅋ",
          "original_form": "줄질까지하고 끝을 봤습니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100030179.1.39",
          "form": "아정말요 ㅋㅋ이제 치유되셨나요",
          "original_form": "아정말요 ㅋㅋ이제 치유되셨나요",
          "speaker_id": "1",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100030179.1.40",
          "form": "전 갤럭시핏으로 싸게 치료했어요 ㅋㅋㅋ",
          "original_form": "전 갤럭시핏으로 싸게 치료했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100030179.1.41",
          "form": "치유되었습니다 ㅋㅋ",
          "original_form": "치유되었습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:02"
        },
        {
          "id": "MDRW2100030179.1.42",
          "form": "갤럭시 핏,, 괜찮나요??",
          "original_form": "갤럭시 핏,, 괜찮나요??",
          "speaker_id": "2",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100030179.1.43",
          "form": "네 저는 너무 만족해요",
          "original_form": "네 저는 너무 만족해요",
          "speaker_id": "1",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100030179.1.44",
          "form": "일단 느낌 보고 워치사려고요",
          "original_form": "일단 느낌 보고 워치사려고요",
          "speaker_id": "1",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100030179.1.45",
          "form": "갤럭시 워치요? 아니면 애플?",
          "original_form": "갤럭시 워치요? 아니면 애플?",
          "speaker_id": "2",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100030179.1.46",
          "form": "우와 되게 잘 쓰시나봐요!!",
          "original_form": "우와 되게 잘 쓰시나봐요!!",
          "speaker_id": "2",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100030179.1.47",
          "form": "그것도 고민이에요 지금 애플 쓰다가 갤럭시 폰 넘어왔는데",
          "original_form": "그것도 고민이에요 지금 애플 쓰다가 갤럭시 폰 넘어왔는데",
          "speaker_id": "1",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100030179.1.48",
          "form": "저는 약간 애플워치 무쓸모,,ㅋㅎㅎㅠ",
          "original_form": "저는 약간 애플워치 무쓸모,,ㅋㅎㅎㅠ",
          "speaker_id": "2",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100030179.1.49",
          "form": "폰을 뭐 할지 아직 고민중이라",
          "original_form": "폰을 뭐 할지 아직 고민중이라",
          "speaker_id": "1",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100030179.1.50",
          "form": "폰 정해지면 워치도 같이 ㅋㅋ",
          "original_form": "폰 정해지면 워치도 같이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100030179.1.51",
          "form": "왜요ㅠㅠ",
          "original_form": "왜요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100030179.1.52",
          "form": "오 지금 폰이 뭐세요?",
          "original_form": "오 지금 폰이 뭐세요?",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100030179.1.53",
          "form": "워치 몉이에요?",
          "original_form": "워치 몉이에요?",
          "speaker_id": "1",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100030179.1.54",
          "form": "저 갤럭시에용",
          "original_form": "저 갤럭시에용",
          "speaker_id": "1",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100030179.1.55",
          "form": "에스텐",
          "original_form": "에스텐",
          "speaker_id": "1",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100030179.1.56",
          "form": "아 저는 워치 se 셀룰러예요",
          "original_form": "아 저는 워치 se 셀룰러예요",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100030179.1.57",
          "form": "아 저도 그거 보고 있었는데",
          "original_form": "아 저도 그거 보고 있었는데",
          "speaker_id": "1",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100030179.1.58",
          "form": "인스타에서 친구가 슬쩍 하고 있는데 너무 이쁘고 멋있고 시크해보이더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "인스타에서 친구가 슬쩍 하고 있는데 너무 이쁘고 멋있고 시크해보이더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100030179.1.59",
          "form": "앜ㅋㅋㅋㅋ그렁가요",
          "original_form": "앜ㅋㅋㅋㅋ그렁가요",
          "speaker_id": "1",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100030179.1.60",
          "form": "저도 이거 끼고 간지난다는 이야기 많이 들었어요 ㅋㅋㅋㅋ",
          "original_form": "저도 이거 끼고 간지난다는 이야기 많이 들었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:07"
        },
