      "utterance": [
        {
          "id": "MDRW2100020880.1.1",
          "form": "안녕하세요 점심드셨나요",
          "original_form": "안녕하세요 점심드셨나요",
          "speaker_id": "1",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100020880.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100020880.1.3",
          "form": "안녕하세요!\r\n네 저는 오늘 출근해가지구ㅠ\r\n회사에서 밥먹었어요ㅠ",
          "original_form": "안녕하세요!\r\n네 저는 오늘 출근해가지구ㅠ\r\n회사에서 밥먹었어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020880.1.4",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020880.1.5",
          "form": "헉 주말출근",
          "original_form": "헉 주말출근",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020880.1.6",
          "form": "저는 방금 라면 끓여먹었어요",
          "original_form": "저는 방금 라면 끓여먹었어요",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020880.1.7",
          "form": "회사 식당 밥이 잘 나오시나요?",
          "original_form": "회사 식당 밥이 잘 나오시나요?",
          "speaker_id": "1",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100020880.1.8",
          "form": "그냥 그렇죠ㅠㅠ",
          "original_form": "그냥 그렇죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100020880.1.9",
          "form": "라면 맛있겠네요ㅠ",
          "original_form": "라면 맛있겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100020880.1.10",
          "form": "저녁이라도 맛난거 먹어야겠어요",
          "original_form": "저녁이라도 맛난거 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100020880.1.11",
          "form": "구내식당은 가격만 괜찮은것같아요",
          "original_form": "구내식당은 가격만 괜찮은것같아요",
          "speaker_id": "1",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100020880.1.12",
          "form": "저녁은 뭐드실생각이세요?",
          "original_form": "저녁은 뭐드실생각이세요?",
          "speaker_id": "1",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100020880.1.13",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100020880.1.14",
          "form": "저녁은 로제떡볶이 아니면",
          "original_form": "저녁은 로제떡볶이 아니면",
          "speaker_id": "2",
          "time": "20210828 14:04"
        },
        {
          "id": "MDRW2100020880.1.15",
          "form": "꿔바로우에 중국냉면 먹고싶어요",
          "original_form": "꿔바로우에 중국냉면 먹고싶어요",
          "speaker_id": "2",
          "time": "20210828 14:04"
        },
        {
          "id": "MDRW2100020880.1.16",
          "form": "저 중국냉면 안먹어봤어요",
          "original_form": "저 중국냉면 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100020880.1.17",
          "form": "궁금한미지의세계",
          "original_form": "궁금한미지의세계",
          "speaker_id": "1",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100020880.1.18",
          "form": "아 진짜요? 중국냉면 맛있어요!",
          "original_form": "아 진짜요? 중국냉면 맛있어요!",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020880.1.19",
          "form": "냉면처럼 새콤달콤한데",
          "original_form": "냉면처럼 새콤달콤한데",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020880.1.20",
          "form": "땅콩소스 맛도  나고 그래요",
          "original_form": "땅콩소스 맛도  나고 그래요",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020880.1.21",
          "form": "면은 냉면면 아니구 그냥 밀가루면이구요",
          "original_form": "면은 냉면면 아니구 그냥 밀가루면이구요",
          "speaker_id": "2",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100020880.1.22",
          "form": "중국집 냉우동 드셔보셨어요?! 비슷하려나?!!",
          "original_form": "중국집 냉우동 드셔보셨어요?! 비슷하려나?!!",
          "speaker_id": "1",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100020880.1.23",
          "form": "아니요 못먹어봤어요",
          "original_form": "아니요 못먹어봤어요",
          "speaker_id": "2",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020880.1.24",
          "form": "냉우동은 우동 국물맛인가요?",
          "original_form": "냉우동은 우동 국물맛인가요?",
          "speaker_id": "2",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020880.1.25",
          "form": "아뇨 ! 면은 얇은 짜장면 면인데 국물이 약간",
          "original_form": "아뇨 ! 면은 얇은 짜장면 면인데 국물이 약간",
          "speaker_id": "1",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020880.1.26",
          "form": "쫄면 비냉 이런느낌?!!",
          "original_form": "쫄면 비냉 이런느낌?!!",
          "speaker_id": "1",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020880.1.27",
          "form": "냉우동은 매콤한거군요?",
          "original_form": "냉우동은 매콤한거군요?",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020880.1.28",
          "form": "중국냉면은 물냉 느낌이에요",
          "original_form": "중국냉면은 물냉 느낌이에요",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020880.1.29",
          "form": "네 중국집 냉우동은 매콤하더라고요! 이름을 잘못지은 느낌",
          "original_form": "네 중국집 냉우동은 매콤하더라고요! 이름을 잘못지은 느낌",
          "speaker_id": "1",
          "time": "20210828 14:13"
        }
      ]
    }
