        },
        {
          "id": "MDRW2100045148.1.2",
          "form": "맞아 봤구나",
          "original_form": "맞아 봤구나",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045148.1.3",
          "form": "보다안보다",
          "original_form": "보다안보다",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045148.1.4",
          "form": "그게 뭐야",
          "original_form": "그게 뭐야",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045148.1.5",
          "form": "다불륜이고",
          "original_form": "다불륜이고",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045148.1.6",
          "form": "원래 막장 작가꺼",
          "original_form": "원래 막장 작가꺼",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045148.1.7",
          "form": "그렇긴하지",
          "original_form": "그렇긴하지",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045148.1.8",
          "form": "벌서 시즌2가 종영되고",
          "original_form": "벌서 시즌2가 종영되고",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045148.1.9",
          "form": "시즌3도 나올려나",
          "original_form": "시즌3도 나올려나",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045148.1.10",
          "form": "모르지",
          "original_form": "모르지",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045148.1.11",
          "form": "내생각에 나올등",
          "original_form": "내생각에 나올등",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045148.1.12",
          "form": "그럴수도 있지 펜트도 시즌3이잖아",
          "original_form": "그럴수도 있지 펜트도 시즌3이잖아",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045148.1.13",
          "form": "엉 근데 길게하면 재미없다",
          "original_form": "엉 근데 길게하면 재미없다",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045148.1.14",
          "form": "이야기전개가 흐지부지",
          "original_form": "이야기전개가 흐지부지",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045148.1.15",
          "form": "결혼작사 이혼작곡은 시청률이 좋다",
          "original_form": "결혼작사 이혼작곡은 시청률이 좋다",
