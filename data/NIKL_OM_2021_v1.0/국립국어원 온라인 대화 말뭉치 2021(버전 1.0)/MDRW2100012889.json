        },
        {
          "id": "MDRW2100012889.1.11",
          "form": "코로나때문에",
          "original_form": "코로나때문에",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100012889.1.12",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210827 10:20"
        },
        {
          "id": "MDRW2100012889.1.13",
          "form": "보고 싶은마음이드제여",
          "original_form": "보고 싶은마음이드제여",
          "speaker_id": "1",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100012889.1.14",
