      "utterance": [
        {
          "id": "MDRW2100036884.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 14:29"
        },
        {
          "id": "MDRW2100036884.1.2",
          "form": "넹 안녕하세요~",
          "original_form": "넹 안녕하세요~",
          "speaker_id": "2",
          "time": "20210831 14:32"
        },
        {
          "id": "MDRW2100036884.1.3",
          "form": "점심 드셨나요~",
          "original_form": "점심 드셨나요~",
          "speaker_id": "1",
          "time": "20210831 14:33"
        },
        {
          "id": "MDRW2100036884.1.4",
          "form": "아니용..ㅠ 귀찮아서 있다보니 또 까먹었네요..",
          "original_form": "아니용..ㅠ 귀찮아서 있다보니 또 까먹었네요..",
          "speaker_id": "2",
          "time": "20210831 14:37"
        },
        {
          "id": "MDRW2100036884.1.5",
          "form": "드셨나요??",
          "original_form": "드셨나요??",
          "speaker_id": "2",
          "time": "20210831 14:37"
        },
        {
          "id": "MDRW2100036884.1.6",
          "form": "빵 먹었어요!",
          "original_form": "빵 먹었어요!",
          "speaker_id": "1",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036884.1.7",
          "form": "배 안고프신가요ㅠㅠ 간식이라도 드시죠ㅠㅠ",
          "original_form": "배 안고프신가요ㅠㅠ 간식이라도 드시죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036884.1.8",
          "form": "습관이 되다보니 이런가보ㅓ요ㅠㅠ 이래서 몸 다 망가졌는데 규칙적으로 챙겨먹는게 쉽지않네용ㅜㅜ",
          "original_form": "습관이 되다보니 이런가보ㅓ요ㅠㅠ 이래서 몸 다 망가졌는데 규칙적으로 챙겨먹는게 쉽지않네용ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036884.1.9",
          "form": "끼니 불규칙하게 먹는게 몸에 영향이 꽤 가더라구요ㅠㅠ",
          "original_form": "끼니 불규칙하게 먹는게 몸에 영향이 꽤 가더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036884.1.10",
          "form": "저도 그런 적 있는데 병원에서 크게 혼나고 요즘은 신경 써서 먹고 있어요ㅠ",
          "original_form": "저도 그런 적 있는데 병원에서 크게 혼나고 요즘은 신경 써서 먹고 있어요ㅠ",
          "speaker_id": "1",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036884.1.11",
          "form": "그러니까요ㅠㅠ 규칙적인 식습관 가져야하는데..",
          "original_form": "그러니까요ㅠㅠ 규칙적인 식습관 가져야하는데..",
          "speaker_id": "2",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100036884.1.12",
          "form": "고민하다가 지지고라이스 시켰어요ㅋㅋㅋ",
          "original_form": "고민하다가 지지고라이스 시켰어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100036884.1.13",
          "form": "헛 면은 같이 안 시키셨나요 ㅎㅎ",
          "original_form": "헛 면은 같이 안 시키셨나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100036884.1.14",
          "form": "넹ㅠㅠ 혼자라서 하나만 시켰어요ㅋㅋㅋ",
          "original_form": "넹ㅠㅠ 혼자라서 하나만 시켰어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100036884.1.15",
          "form": "혼자 밥 먹을 때 그게 참 안좋아요ㅠㅠ\n여러가지 다 맛을 못 보는게 ㅎㅎ",
          "original_form": "혼자 밥 먹을 때 그게 참 안좋아요ㅠㅠ\n여러가지 다 맛을 못 보는게 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 15:03"
        },
        {
          "id": "MDRW2100036884.1.16",
          "form": "4시간 뒤에 꼭 저녁도 드셔야해요!",
          "original_form": "4시간 뒤에 꼭 저녁도 드셔야해요!",
          "speaker_id": "1",
          "time": "20210831 15:04"
        },
        {
          "id": "MDRW2100036884.1.17",
          "form": "맞아요ㅠㅠ 이제 슬슬 저녁 먹어야겠네용ㅋㅋㅋ",
          "original_form": "맞아요ㅠㅠ 이제 슬슬 저녁 먹어야겠네용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:43"
        },
        {
          "id": "MDRW2100036884.1.18",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210831 18:43"
        },
        {
          "id": "MDRW2100036884.1.19",
          "form": "네! 비 온다고해서 파전 부쳤는데 비가 안 오네요 ㅎㅎ",
          "original_form": "네! 비 온다고해서 파전 부쳤는데 비가 안 오네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 20:51"
        }
      ]
