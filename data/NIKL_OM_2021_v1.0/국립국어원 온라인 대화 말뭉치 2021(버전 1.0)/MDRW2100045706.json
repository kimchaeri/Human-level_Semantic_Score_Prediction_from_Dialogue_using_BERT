      "utterance": [
        {
          "id": "MDRW2100045706.1.1",
          "form": "좋아하는 음식 있으신가요?",
          "original_form": "좋아하는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210908 05:46"
        },
        {
          "id": "MDRW2100045706.1.2",
          "form": "저는 헤산물 요리를 좋아합니다 name1님은요?",
          "original_form": "저는 헤산물 요리를 좋아합니다 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 05:46"
        },
        {
          "id": "MDRW2100045706.1.3",
          "form": "해산물 맛있죠ㅠㅠㅠ",
          "original_form": "해산물 맛있죠ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:46"
        },
        {
          "id": "MDRW2100045706.1.4",
          "form": "저는 떡볶이 좋아합니다",
          "original_form": "저는 떡볶이 좋아합니다",
          "speaker_id": "1",
          "time": "20210908 05:47"
        },
        {
          "id": "MDRW2100045706.1.5",
          "form": "저도 떡볶이 좋아해요",
          "original_form": "저도 떡볶이 좋아해요",
          "speaker_id": "2",
          "time": "20210908 05:48"
        },
        {
          "id": "MDRW2100045706.1.6",
          "form": "근데 탄수화물 줄이느라 자주는 못먹네요",
          "original_form": "근데 탄수화물 줄이느라 자주는 못먹네요",
          "speaker_id": "2",
          "time": "20210908 05:49"
        },
        {
          "id": "MDRW2100045706.1.7",
          "form": "아 맞아요ㅠㅠ 그거 탄수화물 폭탄이죠",
          "original_form": "아 맞아요ㅠㅠ 그거 탄수화물 폭탄이죠",
          "speaker_id": "1",
          "time": "20210908 05:49"
        },
        {
          "id": "MDRW2100045706.1.8",
          "form": "그래도 맛있으니 악마의 음식이에요",
          "original_form": "그래도 맛있으니 악마의 음식이에요",
          "speaker_id": "1",
          "time": "20210908 05:49"
        },
        {
          "id": "MDRW2100045706.1.9",
          "form": "자주드시니요?",
          "original_form": "자주드시니요?",
          "speaker_id": "2",
          "time": "20210908 05:49"
        },
        {
          "id": "MDRW2100045706.1.10",
          "form": "매운 게 땡길 때마다 먹어요",
          "original_form": "매운 게 땡길 때마다 먹어요",
          "speaker_id": "1",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045706.1.11",
          "form": "저는 나이가 드니까 너무 매우면 속이 안좋아서 못먹게 되더라구요",
          "original_form": "저는 나이가 드니까 너무 매우면 속이 안좋아서 못먹게 되더라구요",
          "speaker_id": "2",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100045706.1.12",
          "form": "그쵸ㅠㅠㅠ 속이 쓰려가지고...",
          "original_form": "그쵸ㅠㅠㅠ 속이 쓰려가지고...",
          "speaker_id": "1",
          "time": "20210908 05:53"
        },
        {
          "id": "MDRW2100045706.1.13",
          "form": "네.. 원래 매운거 잘먹었는데 이제 안먹다 보니 맵찔이 됐어요.",
          "original_form": "네.. 원래 매운거 잘먹었는데 이제 안먹다 보니 맵찔이 됐어요.",
          "speaker_id": "2",
          "time": "20210908 05:54"
        },
        {
          "id": "MDRW2100045706.1.14",
          "form": "매운거 잘드시나요?",
          "original_form": "매운거 잘드시나요?",
          "speaker_id": "2",
          "time": "20210908 05:55"
        },
        {
          "id": "MDRW2100045706.1.15",
          "form": "저도 맵찔이에요ㅜㅜ",
          "original_form": "저도 맵찔이에요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 05:56"
        },
        {
          "id": "MDRW2100045706.1.16",
          "form": "신라면도 힘들더라구요",
          "original_form": "신라면도 힘들더라구요",
          "speaker_id": "1",
          "time": "20210908 05:56"
        },
        {
          "id": "MDRW2100045706.1.17",
          "form": "앗 저보다 심하시네요 저는 열라면 먹으면 땀뻘뻘인데 ㅎㅎ",
          "original_form": "앗 저보다 심하시네요 저는 열라면 먹으면 땀뻘뻘인데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 05:56"
        },
        {
          "id": "MDRW2100045706.1.18",
          "form": "열라면은 시도도 못해봤죠ㅋㅋㅋ",
          "original_form": "열라면은 시도도 못해봤죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:56"
        }
      ]
