      "utterance": [
        {
          "id": "MDRW2100006078.1.1",
          "form": "회 좋아하시나요",
          "original_form": "회 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100006078.1.2",
          "form": "날것을 못 먹어서요 ㅋㅋ",
          "original_form": "날것을 못 먹어서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100006078.1.3",
          "form": "무슨회 좋아하시나요?",
          "original_form": "무슨회 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100006078.1.4",
          "form": "연어킬러에요",
          "original_form": "연어킬러에요",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100006078.1.5",
          "form": "전생에 연어 잡아먹는 바다사자였을듯ㅇᆢ",
          "original_form": "전생에 연어 잡아먹는 바다사자였을듯ㅇᆢ",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100006078.1.6",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100006078.1.7",
          "form": "연어 좋아하시는 분 많더라구요 ㅋㅋ",
          "original_form": "연어 좋아하시는 분 많더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100006078.1.8",
          "form": "좋아하시는 음식 다른건 없으신가요",
          "original_form": "좋아하시는 음식 다른건 없으신가요",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006078.1.9",
          "form": "피자 햄버거 고기",
          "original_form": "피자 햄버거 고기",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100006078.1.10",
          "form": "다 좋아합니다...",
          "original_form": "다 좋아합니다...",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100006078.1.11",
          "form": "햄버거! 저도 햄버거 좋아해요",
          "original_form": "햄버거! 저도 햄버거 좋아해요",
          "speaker_id": "2",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100006078.1.12",
          "form": "좋아하는 버거 있으신가요",
          "original_form": "좋아하는 버거 있으신가요",
          "speaker_id": "2",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100006078.1.13",
          "form": "버거킹이 최애긴한데 ...",
          "original_form": "버거킹이 최애긴한데 ...",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006078.1.14",
          "form": "집 근처에 없네요ㅠ",
          "original_form": "집 근처에 없네요ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006078.1.15",
          "form": "저도 버거킹 좋아하는데 집근처에 없어요ㅜㅜ",
          "original_form": "저도 버거킹 좋아하는데 집근처에 없어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006078.1.16",
          "form": "어디갈때나 사먹어요",
          "original_form": "어디갈때나 사먹어요",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006078.1.17",
          "form": "버거킹 버거 뭐 좋아하시나요",
          "original_form": "버거킹 버거 뭐 좋아하시나요",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006078.1.18",
          "form": "와퍼가 가장 무난하죠?",
          "original_form": "와퍼가 가장 무난하죠?",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100006078.1.19",
          "form": "갈스가 맛있긴한데 비싸요 ㅠ",
          "original_form": "갈스가 맛있긴한데 비싸요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100006078.1.20",
          "form": "전 불고기와퍼가 맛있더라고요 ㅋㅋ 기네스와퍼도 맛있었어요",
          "original_form": "전 불고기와퍼가 맛있더라고요 ㅋㅋ 기네스와퍼도 맛있었어요",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006078.1.21",
          "form": "갈스는 뭐예요? 갈릭?",
          "original_form": "갈스는 뭐예요? 갈릭?",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006078.1.22",
          "form": "네 갈릭스테이크요",
          "original_form": "네 갈릭스테이크요",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006078.1.23",
          "form": "아 ㅋㅋ 그건안먹어봤네요 ㅋㅋ",
          "original_form": "아 ㅋㅋ 그건안먹어봤네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100006078.1.24",
          "form": "다음에 드셔보세요 !",
          "original_form": "다음에 드셔보세요 !",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100006078.1.25",
          "form": "버거킹은 롱치킨버거? 그거말고는 대부분맛나더라구요 ㅋㄱ",
          "original_form": "버거킹은 롱치킨버거? 그거말고는 대부분맛나더라구요 ㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100006078.1.26",
          "form": "네네 먹어봐야겠어요",
          "original_form": "네네 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210907 20:56"
        }
      ]
    }
  ]
