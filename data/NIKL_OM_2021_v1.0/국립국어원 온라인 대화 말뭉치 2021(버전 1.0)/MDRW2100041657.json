      "utterance": [
        {
          "id": "MDRW2100041657.1.1",
          "form": "아 만사귀찮노",
          "original_form": "아 만사귀찮노",
          "speaker_id": "2",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100041657.1.2",
          "form": "설거지도 해야되는데",
          "original_form": "설거지도 해야되는데",
          "speaker_id": "2",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100041657.1.3",
          "form": "난도 밥 먹은거 그대로 냅두고 있다 ㅎㅎ",
          "original_form": "난도 밥 먹은거 그대로 냅두고 있다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100041657.1.4",
          "form": "둘찌 재우나",
          "original_form": "둘찌 재우나",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100041657.1.5",
          "form": "잔당 ㅋㅋㅋㅋ 자유시간 좋다",
          "original_form": "잔당 ㅋㅋㅋㅋ 자유시간 좋다",
          "speaker_id": "1",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100041657.1.6",
          "form": "난다시 병원으로 가는길",
          "original_form": "난다시 병원으로 가는길",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100041657.1.7",
          "form": "열은??",
          "original_form": "열은??",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100041657.1.8",
          "form": "37.3",
          "original_form": "37.3",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100041657.1.9",
          "form": "가서 또 좀 앉아있어야지",
          "original_form": "가서 또 좀 앉아있어야지",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100041657.1.10",
          "form": "아니 근데 진짜 열이 나는거면. 안맞아야되는거 아냐?",
          "original_form": "아니 근데 진짜 열이 나는거면. 안맞아야되는거 아냐?",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100041657.1.11",
          "form": "37.5이하는 괜찮은데",
          "original_form": "37.5이하는 괜찮은데",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041657.1.12",
          "form": "37.5네",
          "original_form": "37.5네",
          "speaker_id": "2",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100041657.1.13",
          "form": "자는거니",
          "original_form": "자는거니",
          "speaker_id": "2",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100041657.1.14",
          "form": "그정도면... 안맞는게 안낮나.ㅠ",
          "original_form": "그정도면... 안맞는게 안낮나.ㅠ",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100041657.1.15",
          "form": "몸에 문제가 있으니 열이 나는거겠지. 왜 열이 난데 근데?",
          "original_form": "몸에 문제가 있으니 열이 나는거겠지. 왜 열이 난데 근데?",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100041657.1.16",
          "form": "몸살인가",
          "original_form": "몸살인가",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100041657.1.17",
          "form": "나 건강한데ㅋ",
          "original_form": "나 건강한데ㅋ",
          "speaker_id": "2",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100041657.1.18",
          "form": "근데 마스크쓰니 콧바람이 따숩긴해",
          "original_form": "근데 마스크쓰니 콧바람이 따숩긴해",
          "speaker_id": "2",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100041657.1.19",
          "form": "열나니까그래",
          "original_form": "열나니까그래",
          "speaker_id": "1",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100041657.1.20",
          "form": "진찰해봤어?? 목이나 그런데는?",
          "original_form": "진찰해봤어?? 목이나 그런데는?",
          "speaker_id": "1",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041657.1.21",
          "form": "침삼킬때는 안아푸닝",
          "original_form": "침삼킬때는 안아푸닝",
          "speaker_id": "2",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041657.1.22",
          "form": "어딘가 염증은 있겠지 미열 나는거니",
          "original_form": "어딘가 염증은 있겠지 미열 나는거니",
          "speaker_id": "2",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041657.1.23",
          "form": "그 피부땜시. 그런갑다ㅠㅠ",
          "original_form": "그 피부땜시. 그런갑다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041657.1.24",
          "form": "아직도 병원에있어???",
          "original_form": "아직도 병원에있어???",
          "speaker_id": "1",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041657.1.25",
          "form": "방금 집왔으ㅋㅋ",
          "original_form": "방금 집왔으ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:00"
        },
        {
          "id": "MDRW2100041657.1.26",
          "form": "간호사도 계속 봐주다가 안맞으시는게 나을 것같다고ㅋㄱㅋㄱ",
          "original_form": "간호사도 계속 봐주다가 안맞으시는게 나을 것같다고ㅋㄱㅋㄱ",
          "speaker_id": "2",
          "time": "20210906 15:00"
        },
        {
          "id": "MDRW2100041657.1.27",
          "form": "어제 캠핑땜시 몸살났나 비맞아서 그런가",
          "original_form": "어제 캠핑땜시 몸살났나 비맞아서 그런가",
          "speaker_id": "2",
          "time": "20210906 15:01"
        },
        {
          "id": "MDRW2100041657.1.28",
          "form": "그니까 내가봐도 안맞는게 ㅠ",
          "original_form": "그니까 내가봐도 안맞는게 ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041657.1.29",
          "form": "그래 쉬어야지 집에와서 누워있으니 머리 아픈거 긋노 ㅋㅋ\n기분탓이제 ㅋㅋ",
          "original_form": "그래 쉬어야지 집에와서 누워있으니 머리 아픈거 긋노 ㅋㅋ\n기분탓이제 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041657.1.30",
          "form": "아니 열나서 ㅠ",
          "original_form": "아니 열나서 ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041657.1.31",
          "form": "근거아냐??/ㅠㅠㅠ",
          "original_form": "근거아냐??/ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 15:18"
        },
        {
          "id": "MDRW2100041657.1.32",
          "form": "그런거가 ㅠㅠㅠ",
          "original_form": "그런거가 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 15:19"
        }
