        {
          "id": "MDRW2100021324.1.1",
          "form": "운동좋아하시나요",
          "original_form": "운동좋아하시나요",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100021324.1.2",
          "form": "홈트랑 등산이요..",
          "original_form": "홈트랑 등산이요..",
          "speaker_id": "1",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021324.1.3",
          "form": "뭐하세요?",
          "original_form": "뭐하세요?",
          "speaker_id": "1",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021324.1.4",
          "form": "홈트 해보려구요!",
          "original_form": "홈트 해보려구요!",
          "speaker_id": "2",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100021324.1.5",
          "form": "오.. 어떤? 요가 같은거요?",
          "original_form": "오.. 어떤? 요가 같은거요?",
          "speaker_id": "1",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100021324.1.6",
          "form": "아뇨 철봉이랑 푸시업이요!",
          "original_form": "아뇨 철봉이랑 푸시업이요!",
          "speaker_id": "2",
          "time": "20210828 17:47"
        },
        {
          "id": "MDRW2100021324.1.7",
          "form": "철봉있어요?",
          "original_form": "철봉있어요?",
          "speaker_id": "1",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100021324.1.8",
          "form": "문틈철봉 샀습니다!",
          "original_form": "문틈철봉 샀습니다!",
          "speaker_id": "2",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100021324.1.9",
          "form": "그거 피스 안박음 사고 많더라구요..",
          "original_form": "그거 피스 안박음 사고 많더라구요..",
          "speaker_id": "1",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100021324.1.10",
          "form": "저도 풀업해요.. ㅎㅎㅎ",
          "original_form": "저도 풀업해요.. ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100021324.1.11",
          "form": "오맞아요 좀 무서워요ㅠㅠ ㅋㅋ",
          "original_form": "오맞아요 좀 무서워요ㅠㅠ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100021324.1.12",
          "form": "그게 처음하면 반동을 많이줘서 가끔 풀리더라구요..ㅋㅋㅋ 유튭에 보면 사고 모음집도 있어요",
          "original_form": "그게 처음하면 반동을 많이줘서 가끔 풀리더라구요..ㅋㅋㅋ 유튭에 보면 사고 모음집도 있어요",
          "speaker_id": "1",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100021324.1.13",
          "form": "아우 생각만해도 아파요",
          "original_form": "아우 생각만해도 아파요",
          "speaker_id": "2",
          "time": "20210828 17:53"
        },
        {
