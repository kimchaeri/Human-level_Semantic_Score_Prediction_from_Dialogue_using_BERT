      "utterance": [
        {
          "id": "MDRW2100007759.1.1",
          "form": "오늘 날씨가 쯘하고 좋아 졌어요",
          "original_form": "오늘 날씨가 쯘하고 좋아 졌어요",
          "speaker_id": "2",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007759.1.2",
          "form": "쩐",
          "original_form": "쩐",
          "speaker_id": "2",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007759.1.3",
          "form": "이러다 갑자기 비가 오는 때가 잦아서",
          "original_form": "이러다 갑자기 비가 오는 때가 잦아서",
          "speaker_id": "1",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007759.1.4",
          "form": "짠",
          "original_form": "짠",
          "speaker_id": "2",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007759.1.5",
          "form": "그렇죠 예상이 안돼요",
          "original_form": "그렇죠 예상이 안돼요",
          "speaker_id": "2",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007759.1.6",
          "form": "요즘엔 너무 갑자기 비가 오더라고요",
          "original_form": "요즘엔 너무 갑자기 비가 오더라고요",
          "speaker_id": "1",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007759.1.7",
          "form": "날씨 좋아서 빨래중요",
          "original_form": "날씨 좋아서 빨래중요",
          "speaker_id": "2",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007759.1.8",
          "form": "오 아침부터 부지런하게 지내고 계시네요",
          "original_form": "오 아침부터 부지런하게 지내고 계시네요",
          "speaker_id": "1",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007759.1.9",
          "form": "그래서 잠깐 피했다 다시 볼일봐요",
          "original_form": "그래서 잠깐 피했다 다시 볼일봐요",
          "speaker_id": "2",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007759.1.10",
          "form": "빨래가 산더미요",
          "original_form": "빨래가 산더미요",
          "speaker_id": "2",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007759.1.11",
          "form": "이제 무더위는 끝나가나봐요",
          "original_form": "이제 무더위는 끝나가나봐요",
          "speaker_id": "1",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007759.1.12",
          "form": "비때문에",
          "original_form": "비때문에",
          "speaker_id": "2",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007759.1.13",
          "form": "네 지금도 선선하죠",
          "original_form": "네 지금도 선선하죠",
          "speaker_id": "2",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007759.1.14",
          "form": "가을이 오려는지 바람이 제법 선선하네요",
          "original_form": "가을이 오려는지 바람이 제법 선선하네요",
          "speaker_id": "1",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007759.1.15",
          "form": "여름엔 언제 가을어나 해도 금방와요",
          "original_form": "여름엔 언제 가을어나 해도 금방와요",
          "speaker_id": "2",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007759.1.16",
          "form": "이제 겨울옷 사고 있을듯해요",
          "original_form": "이제 겨울옷 사고 있을듯해요",
          "speaker_id": "2",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007759.1.17",
          "form": "지금 계곡가면 비오려나 고민되네요",
          "original_form": "지금 계곡가면 비오려나 고민되네요",
          "speaker_id": "2",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007759.1.18",
          "form": "네 곧 쌀쌀해지고 겨울옷 꺼내야할 듯",
          "original_form": "네 곧 쌀쌀해지고 겨울옷 꺼내야할 듯",
          "speaker_id": "1",
          "time": "20210822 10:18"
        },
        {
          "id": "MDRW2100007759.1.19",
          "form": "전 롱패딩 하나 사야겠어요",
          "original_form": "전 롱패딩 하나 사야겠어요",
          "speaker_id": "2",
          "time": "20210822 10:18"
        },
        {
          "id": "MDRW2100007759.1.20",
          "form": "추위 싫어서",
          "original_form": "추위 싫어서",
          "speaker_id": "2",
          "time": "20210822 10:18"
        },
        {
          "id": "MDRW2100007759.1.21",
          "form": "겨울엔 롱패딩이죠!",
          "original_form": "겨울엔 롱패딩이죠!",
          "speaker_id": "1",
          "time": "20210822 10:18"
        },
        {
          "id": "MDRW2100007759.1.22",
          "form": "맞아요 짧은거 노노요",
          "original_form": "맞아요 짧은거 노노요",
          "speaker_id": "2",
          "time": "20210822 10:18"
        },
        {
          "id": "MDRW2100007759.1.23",
          "form": "저도 재작년엔가 한파 왔을때 롱패딩 샀는데",
          "original_form": "저도 재작년엔가 한파 왔을때 롱패딩 샀는데",
          "speaker_id": "1",
          "time": "20210822 10:18"
        },
        {
          "id": "MDRW2100007759.1.24",
          "form": "엉덩이 시립니다",
          "original_form": "엉덩이 시립니다",
          "speaker_id": "2",
          "time": "20210822 10:18"
        },
        {
          "id": "MDRW2100007759.1.25",
          "form": "한번 입기 시작하니",
          "original_form": "한번 입기 시작하니",
          "speaker_id": "1",
          "time": "20210822 10:18"
        },
        {
          "id": "MDRW2100007759.1.26",
          "form": "네 와 추웠죠",
          "original_form": "네 와 추웠죠",
          "speaker_id": "2",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007759.1.27",
          "form": "짧은 건 못입겠더라고요",
          "original_form": "짧은 건 못입겠더라고요",
          "speaker_id": "1",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007759.1.28",
          "form": "올해는 어떨지 모르겠네요",
          "original_form": "올해는 어떨지 모르겠네요",
          "speaker_id": "2",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007759.1.29",
          "form": "다리도 시리고",
          "original_form": "다리도 시리고",
          "speaker_id": "1",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007759.1.30",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007759.1.31",
          "form": "다리가 맞아요 넘 추워요",
          "original_form": "다리가 맞아요 넘 추워요",
          "speaker_id": "2",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007759.1.32",
          "form": "여름이 더우면 겨울이 춥다던데",
          "original_form": "여름이 더우면 겨울이 춥다던데",
          "speaker_id": "1",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007759.1.33",
          "form": "올해 한 2주 폭염이었으니",
          "original_form": "올해 한 2주 폭염이었으니",
          "speaker_id": "1",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007759.1.34",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007759.1.35",
          "form": "2주 겨울인가요?",
          "original_form": "2주 겨울인가요?",
          "speaker_id": "2",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007759.1.36",
          "form": "겨울도 짧고 굵게 엄청 추우려나...",
          "original_form": "겨울도 짧고 굵게 엄청 추우려나...",
          "speaker_id": "1",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007759.1.37",
          "form": "것도 무섭네요",
          "original_form": "것도 무섭네요",
          "speaker_id": "2",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007759.1.38",
          "form": "눈이 좀 안오면 좋겠어요",
          "original_form": "눈이 좀 안오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007759.1.39",
          "form": "이젠 낭만이 사라져서 질척되고 교통이 마비라",
          "original_form": "이젠 낭만이 사라져서 질척되고 교통이 마비라",
          "speaker_id": "2",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007759.1.40",
          "form": "힘들어서요",
          "original_form": "힘들어서요",
          "speaker_id": "2",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007759.1.41",
          "form": "요새 안오긴해요",
          "original_form": "요새 안오긴해요",
          "speaker_id": "2",
          "time": "20210822 10:20"
        }
      ]
    }
  ]
