      "utterance": [
        {
          "id": "MDRW2100034017.1.1",
          "form": "소뼈가루일 거예요",
          "original_form": "소뼈가루일 거예요",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100034017.1.2",
          "form": "뭐라했는데 기억도 안나요",
          "original_form": "뭐라했는데 기억도 안나요",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100034017.1.3",
          "form": "아!!! 그걸 단단하게 만드는 거군요",
          "original_form": "아!!! 그걸 단단하게 만드는 거군요",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100034017.1.4",
          "form": "그래서 결론은 관리잘해서 안하는게 좋아요",
          "original_form": "그래서 결론은 관리잘해서 안하는게 좋아요",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100034017.1.5",
          "form": "지금은 이제 괜찮으신가요",
          "original_form": "지금은 이제 괜찮으신가요",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100034017.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100034017.1.7",
          "form": "다행이네요..",
          "original_form": "다행이네요..",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100034017.1.8",
          "form": "혹시 운동 같은 건 하고 계시는 거 있으세요?",
          "original_form": "혹시 운동 같은 건 하고 계시는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100034017.1.9",
          "form": "걷기요",
          "original_form": "걷기요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100034017.1.10",
          "form": "요새 다시좀 걸어요",
          "original_form": "요새 다시좀 걸어요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100034017.1.11",
          "form": "뭐하세요 ?운동",
          "original_form": "뭐하세요 ?운동",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100034017.1.12",
          "form": "저는 원래 안하는데 요새 체중이 불어서 ㅠㅠ 홈트 하려고요..",
          "original_form": "저는 원래 안하는데 요새 체중이 불어서 ㅠㅠ 홈트 하려고요..",
          "speaker_id": "1",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100034017.1.13",
          "form": "저도요 체중이 불러서",
          "original_form": "저도요 체중이 불러서",
          "speaker_id": "2",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100034017.1.14",
          "form": "불어서",
          "original_form": "불어서",
          "speaker_id": "2",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100034017.1.15",
          "form": "홈트가 한 5분하면 하기 싫어요 ㅋㅋ",
          "original_form": "홈트가 한 5분하면 하기 싫어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100034017.1.16",
          "form": "인내심이 필요해요ㅜㅜ",
          "original_form": "인내심이 필요해요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100034017.1.17",
          "form": "다시 시작 하려니 막막해요",
          "original_form": "다시 시작 하려니 막막해요",
          "speaker_id": "2",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100034017.1.18",
          "form": "오 그래도 하셨었나봐요!!!",
          "original_form": "오 그래도 하셨었나봐요!!!",
          "speaker_id": "1",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100034017.1.19",
          "form": "영상보시고 하시나요?",
          "original_form": "영상보시고 하시나요?",
          "speaker_id": "1",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100034017.1.20",
          "form": "네 영상보고 했었어요",
          "original_form": "네 영상보고 했었어요",
          "speaker_id": "2",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100034017.1.21",
          "form": "안하니 ㅋㅋ",
          "original_form": "안하니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100034017.1.22",
          "form": "바로 요요가 ㅋㅋ",
          "original_form": "바로 요요가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100034017.1.23",
          "form": "20킬로 뺐다가 다시 쪘어요 ㅜㅜ",
          "original_form": "20킬로 뺐다가 다시 쪘어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100034017.1.24",
          "form": "헐.. 저랑 너무 비슷하세요 저는 17키로 ㅋㅋㅋ",
          "original_form": "헐.. 저랑 너무 비슷하세요 저는 17키로 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100034017.1.25",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100034017.1.26",
          "form": "동지네요",
          "original_form": "동지네요",
          "speaker_id": "2",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100034017.1.27",
          "form": "한참 빼고서 더 안 찔거 같았거든요 몇년 가길래..",
          "original_form": "한참 빼고서 더 안 찔거 같았거든요 몇년 가길래..",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100034017.1.28",
          "form": "방심했더니만 ㅋㅋㅋ",
          "original_form": "방심했더니만 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100034017.1.29",
          "form": "전 바로 쪘어요 ㅋㅋ",
          "original_form": "전 바로 쪘어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100034017.1.30",
          "form": "아 몇년 부러워요",
          "original_form": "아 몇년 부러워요",
          "speaker_id": "2",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100034017.1.31",
          "form": "1년이라도 갔음 좋았을텐데 전 2달 ㅜㅜ",
          "original_form": "1년이라도 갔음 좋았을텐데 전 2달 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 13:30"
        },
        {
          "id": "MDRW2100034017.1.32",
          "form": "아 정말요?? 몸에 무리 엄청 왔을거 같은데 ㅠㅠㅠ",
          "original_form": "아 정말요?? 몸에 무리 엄청 왔을거 같은데 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 13:30"
        },
        {
          "id": "MDRW2100034017.1.33",
          "form": "네 탈모가 막 왔어요",
          "original_form": "네 탈모가 막 왔어요",
          "speaker_id": "2",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100034017.1.34",
          "form": "아 그쵸.. 저도 한때 엄청 빠지더라고요 그거 회복되는데 꽤 걸리지 않던가요",
          "original_form": "아 그쵸.. 저도 한때 엄청 빠지더라고요 그거 회복되는데 꽤 걸리지 않던가요",
          "speaker_id": "1",
          "time": "20210826 13:32"
        },
        {
          "id": "MDRW2100034017.1.35",
          "form": "아직도 회복못했어요 ㅜㅠㅠ",
          "original_form": "아직도 회복못했어요 ㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 13:32"
        },
        {
          "id": "MDRW2100034017.1.36",
          "form": "이대로 살아야 하나봐요 ㅋㅋ",
          "original_form": "이대로 살아야 하나봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:32"
        },
        {
          "id": "MDRW2100034017.1.37",
          "form": "아 살보다 탈모가 더 싫어요",
          "original_form": "아 살보다 탈모가 더 싫어요",
          "speaker_id": "2",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100034017.1.38",
          "form": "살을 뺄땐 식단을 너무 제한하지 말아야겠어요",
          "original_form": "살을 뺄땐 식단을 너무 제한하지 말아야겠어요",
          "speaker_id": "2",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100034017.1.39",
          "form": "헐 ㅠㅠㅠㅠㅠ 진짜 부작용은 역시 오래가네요..",
          "original_form": "헐 ㅠㅠㅠㅠㅠ 진짜 부작용은 역시 오래가네요..",
          "speaker_id": "1",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100034017.1.40",
          "form": "꼭 드시고 하세요 ㅎㅎ",
          "original_form": "꼭 드시고 하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100034017.1.41",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100034017.1.42",
          "form": "맞아요 잘 챙겨먹어야돼요 저는 다행히 숱은 돌아왔어요..",
          "original_form": "맞아요 잘 챙겨먹어야돼요 저는 다행히 숱은 돌아왔어요..",
          "speaker_id": "1",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100034017.1.43",
          "form": "너무 좋겠다 부라워요",
          "original_form": "너무 좋겠다 부라워요",
          "speaker_id": "2",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100034017.1.44",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "2",
          "time": "20210826 13:34"
        }
      ]
