      "utterance": [
        {
          "id": "MDRW2100027380.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100027380.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100027380.1.3",
          "form": "재밌게 읽으신 책 있나요?",
          "original_form": "재밌게 읽으신 책 있나요?",
          "speaker_id": "1",
          "time": "20210902 12:05"
        },
        {
          "id": "MDRW2100027380.1.4",
          "form": "저는 최근 주식관련책읽었어요ㅎㅎ\r\n재밌게읽으신책있나요?",
          "original_form": "저는 최근 주식관련책읽었어요ㅎㅎ\r\n재밌게읽으신책있나요?",
          "speaker_id": "2",
          "time": "20210902 12:05"
        },
        {
          "id": "MDRW2100027380.1.5",
          "form": "저는 심리에 관련된 책을 읽었어요",
          "original_form": "저는 심리에 관련된 책을 읽었어요",
          "speaker_id": "1",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100027380.1.6",
          "form": "그쪽에 관심이많으신가요?",
          "original_form": "그쪽에 관심이많으신가요?",
          "speaker_id": "2",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100027380.1.7",
          "form": "그냥 심심풀이로 읽고 있어요",
          "original_form": "그냥 심심풀이로 읽고 있어요",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027380.1.8",
          "form": "가끔 도움이 되기도 하구요",
          "original_form": "가끔 도움이 되기도 하구요",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027380.1.9",
          "form": "도움많이될거같아요 인간관계에있어서도..",
          "original_form": "도움많이될거같아요 인간관계에있어서도..",
          "speaker_id": "2",
          "time": "20210902 12:11"
        },
        {
          "id": "MDRW2100027380.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 12:12"
        },
        {
          "id": "MDRW2100027380.1.11",
          "form": "써먹을데가 꼭 생겨요",
          "original_form": "써먹을데가 꼭 생겨요",
          "speaker_id": "1",
          "time": "20210902 12:12"
        },
        {
          "id": "MDRW2100027380.1.12",
          "form": "책많이읽음좋긴좋죠..ㅎㅎ\r\n도서관이용많이하셔요?",
          "original_form": "책많이읽음좋긴좋죠..ㅎㅎ\r\n도서관이용많이하셔요?",
          "speaker_id": "2",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027380.1.13",
          "form": "네 자주 가는 편입니다",
          "original_form": "네 자주 가는 편입니다",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100027380.1.14",
          "form": "오 멋지시네요 책안읽는사람도많은데..",
          "original_form": "오 멋지시네요 책안읽는사람도많은데..",
          "speaker_id": "2",
          "time": "20210902 12:30"
        },
        {
          "id": "MDRW2100027380.1.15",
          "form": "가서 구경만 하고 오기도 해요",
          "original_form": "가서 구경만 하고 오기도 해요",
          "speaker_id": "1",
          "time": "20210902 12:31"
        },
        {
          "id": "MDRW2100027380.1.16",
          "form": "매번 신간 코너가 가득차 있어서ㅎㅎ",
          "original_form": "매번 신간 코너가 가득차 있어서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:31"
        },
        {
          "id": "MDRW2100027380.1.17",
          "form": "근데 꼭 읽고싶은 신간은 또 없더라구요 ㅋㅋㅋ인기가많아서 디ㅡ들빌려가서ㅜㅜ",
          "original_form": "근데 꼭 읽고싶은 신간은 또 없더라구요 ㅋㅋㅋ인기가많아서 디ㅡ들빌려가서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 12:32"
        },
        {
          "id": "MDRW2100027380.1.18",
          "form": "맞아요 신간 경쟁 치열하죠",
          "original_form": "맞아요 신간 경쟁 치열하죠",
          "speaker_id": "1",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100027380.1.19",
          "form": "네 신간 신청한사람 먼저 가져가잖아요 \r\n그 찬스 이용하지않으면 진짜 한참뒤에나볼수있는..ㅎㅎ",
          "original_form": "네 신간 신청한사람 먼저 가져가잖아요 \r\n그 찬스 이용하지않으면 진짜 한참뒤에나볼수있는..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100027380.1.20",
          "form": "신청해본적 있으세요?",
          "original_form": "신청해본적 있으세요?",
          "speaker_id": "1",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100027380.1.21",
          "form": "아니요ㅎㅎ  도서관알바할때 이렇게 빌리는게좋겠구나 생각만했었어요ㅎㅎ",
          "original_form": "아니요ㅎㅎ  도서관알바할때 이렇게 빌리는게좋겠구나 생각만했었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:00"
        },
        {
          "id": "MDRW2100027380.1.22",
          "form": "와 도서관 알바라니 멋있어요",
          "original_form": "와 도서관 알바라니 멋있어요",
          "speaker_id": "1",
          "time": "20210902 13:01"
        },
        {
          "id": "MDRW2100027380.1.23",
          "form": "안멋있구그냥노가다였어오...ㅋㅋㄲㅋㅋㅋ흑흑",
          "original_form": "안멋있구그냥노가다였어오...ㅋㅋㄲㅋㅋㅋ흑흑",
          "speaker_id": "2",
          "time": "20210902 13:02"
        },
        {
          "id": "MDRW2100027380.1.24",
          "form": "ㅠ 많이 힘드셨겠어요",
          "original_form": "ㅠ 많이 힘드셨겠어요",
          "speaker_id": "1",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100027380.1.25",
          "form": "네그래도책제자리꽂는그게재밌어서 버틸수있었어용 ㅎㅎ",
          "original_form": "네그래도책제자리꽂는그게재밌어서 버틸수있었어용 ㅎㅎ",
