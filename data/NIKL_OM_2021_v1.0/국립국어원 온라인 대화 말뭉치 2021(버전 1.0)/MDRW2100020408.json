      "utterance": [
        {
          "id": "MDRW2100020408.1.1",
          "form": "올림픽 보셨나요?",
          "original_form": "올림픽 보셨나요?",
          "speaker_id": "1",
          "time": "20210828 02:58"
        },
        {
          "id": "MDRW2100020408.1.2",
          "form": "네 재밌게봤어요",
          "original_form": "네 재밌게봤어요",
          "speaker_id": "2",
          "time": "20210828 02:58"
        },
        {
          "id": "MDRW2100020408.1.3",
          "form": "오 어떤 종목 보셨어요",
          "original_form": "오 어떤 종목 보셨어요",
          "speaker_id": "1",
          "time": "20210828 02:58"
        },
        {
          "id": "MDRW2100020408.1.4",
          "form": "배구하고 양궁하고요",
          "original_form": "배구하고 양궁하고요",
          "speaker_id": "2",
          "time": "20210828 02:59"
        },
        {
          "id": "MDRW2100020408.1.5",
          "form": "유명한거 위주로 보셨네요",
          "original_form": "유명한거 위주로 보셨네요",
          "speaker_id": "1",
          "time": "20210828 02:59"
        },
        {
          "id": "MDRW2100020408.1.6",
          "form": "생각나는게 유명한거네요 ㅎㅎ 클라이밍두봤어요",
          "original_form": "생각나는게 유명한거네요 ㅎㅎ 클라이밍두봤어요",
          "speaker_id": "2",
          "time": "20210828 03:00"
        },
        {
          "id": "MDRW2100020408.1.7",
          "form": "오 이번에 저희나라 영재 선수 4등한거 보셨나요?",
          "original_form": "오 이번에 저희나라 영재 선수 4등한거 보셨나요?",
          "speaker_id": "1",
          "time": "20210828 03:00"
        },
        {
          "id": "MDRW2100020408.1.8",
          "form": "애가 어린데 잘 오르더라구요",
          "original_form": "애가 어린데 잘 오르더라구요",
          "speaker_id": "1",
          "time": "20210828 03:00"
        },
        {
          "id": "MDRW2100020408.1.9",
          "form": "네 대단하더라구요",
          "original_form": "네 대단하더라구요",
          "speaker_id": "2",
          "time": "20210828 03:01"
        },
        {
          "id": "MDRW2100020408.1.10",
          "form": "멋졌어요",
          "original_form": "멋졌어요",
          "speaker_id": "2",
          "time": "20210828 03:01"
        },
        {
          "id": "MDRW2100020408.1.11",
          "form": "ㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210828 03:01"
        },
        {
          "id": "MDRW2100020408.1.12",
          "form": "내년 베이징동계올림픽도 기대되요",
          "original_form": "내년 베이징동계올림픽도 기대되요",
          "speaker_id": "2",
          "time": "20210828 03:01"
        },
        {
          "id": "MDRW2100020408.1.13",
          "form": "이번 올림픽은 특히",
          "original_form": "이번 올림픽은 특히",
          "speaker_id": "1",
          "time": "20210828 03:01"
        },
        {
          "id": "MDRW2100020408.1.14",
          "form": "뭔가 몰랐던 대단한 사람들? 그런 사람들이 마구 나온거같아요",
          "original_form": "뭔가 몰랐던 대단한 사람들? 그런 사람들이 마구 나온거같아요",
          "speaker_id": "1",
          "time": "20210828 03:02"
        },
        {
          "id": "MDRW2100020408.1.15",
          "form": "어린선수가많았죠",
          "original_form": "어린선수가많았죠",
          "speaker_id": "2",
          "time": "20210828 03:02"
        },
        {
          "id": "MDRW2100020408.1.16",
          "form": "맞아요 베이징올림픽도 얼마 안남았네요",
          "original_form": "맞아요 베이징올림픽도 얼마 안남았네요",
          "speaker_id": "1",
          "time": "20210828 03:02"
        },
        {
          "id": "MDRW2100020408.1.17",
          "form": "대견해요",
          "original_form": "대견해요",
          "speaker_id": "2",
          "time": "20210828 03:02"
        },
        {
          "id": "MDRW2100020408.1.18",
          "form": "아 맞아요 ㅋㅋ 어린 선수들이 많아서",
          "original_form": "아 맞아요 ㅋㅋ 어린 선수들이 많아서",
          "speaker_id": "1",
          "time": "20210828 03:02"
        },
        {
          "id": "MDRW2100020408.1.19",
          "form": "파리올림픽도 기대해볼만할거같아요 ㅎㅎ",
          "original_form": "파리올림픽도 기대해볼만할거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 03:02"
        },
        {
          "id": "MDRW2100020408.1.20",
          "form": "무관중이라서 연습했던 환경대로 하는게 유리했던점도 있대요",
          "original_form": "무관중이라서 연습했던 환경대로 하는게 유리했던점도 있대요",
          "speaker_id": "2",
          "time": "20210828 03:03"
        },
        {
          "id": "MDRW2100020408.1.21",
          "form": "아~~~",
          "original_form": "아~~~",
          "speaker_id": "1",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020408.1.22",
          "form": "그랴서 어린 선수들이",
          "original_form": "그랴서 어린 선수들이",
          "speaker_id": "1",
          "time": "20210828 03:04"
        },
        {
          "id": "MDRW2100020408.1.23",
          "form": "상대적으로 좀 덜 부담감 느꼈을 수 있겠네요!",
          "original_form": "상대적으로 좀 덜 부담감 느꼈을 수 있겠네요!",
          "speaker_id": "1",
          "time": "20210828 03:05"
        },
        {
          "id": "MDRW2100020408.1.24",
          "form": "함성이나 고함 이런게 없으니 ㅎㅎ",
          "original_form": "함성이나 고함 이런게 없으니 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 03:05"
        },
        {
          "id": "MDRW2100020408.1.25",
          "form": "네 카더란데 무게있는 의견같아요",
          "original_form": "네 카더란데 무게있는 의견같아요",
          "speaker_id": "2",
          "time": "20210828 03:05"
        },
        {
          "id": "MDRW2100020408.1.26",
          "form": "사람마다 다르겠죠 ㅎㅎ",
          "original_form": "사람마다 다르겠죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 03:05"
        }
      ]
    }
