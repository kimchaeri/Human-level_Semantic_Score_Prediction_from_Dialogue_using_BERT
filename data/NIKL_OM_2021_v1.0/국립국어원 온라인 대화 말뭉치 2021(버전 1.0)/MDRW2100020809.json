      "utterance": [
        {
          "id": "MDRW2100020809.1.1",
          "form": "크리스탈라이트는뭐임?",
          "original_form": "크리스탈라이트는뭐임?",
          "speaker_id": "2",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100020809.1.2",
          "form": "탄산음ㅇ?",
          "original_form": "탄산음ㅇ?",
          "speaker_id": "2",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100020809.1.3",
          "form": "ㄴㄴㄴ 여러가지 맛이 있는 가루임",
          "original_form": "ㄴㄴㄴ 여러가지 맛이 있는 가루임",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020809.1.4",
          "form": "탄산수나 물에 타먹는 거",
          "original_form": "탄산수나 물에 타먹는 거",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020809.1.5",
          "form": "아이스티 가루 같은 거임",
          "original_form": "아이스티 가루 같은 거임",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020809.1.6",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020809.1.7",
          "form": "뭔지알겠음",
          "original_form": "뭔지알겠음",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020809.1.8",
          "form": "그게당이적음?",
          "original_form": "그게당이적음?",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020809.1.9",
          "form": "무가당인데 아스파탐이 들어간대 찾아보고 알았음 왜이리 단가 했네",
          "original_form": "무가당인데 아스파탐이 들어간대 찾아보고 알았음 왜이리 단가 했네",
          "speaker_id": "1",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100020809.1.10",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100020809.1.11",
          "form": "그거탄산있는거야?",
          "original_form": "그거탄산있는거야?",
          "speaker_id": "2",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100020809.1.12",
          "form": "아 ㄴㄴㄴㄴ",
          "original_form": "아 ㄴㄴㄴㄴ",
          "speaker_id": "1",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100020809.1.13",
          "form": "그냥이온음료같음?",
          "original_form": "그냥이온음료같음?",
          "speaker_id": "2",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100020809.1.14",
          "form": "이온 음료보다는",
          "original_form": "이온 음료보다는",
          "speaker_id": "1",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100020809.1.15",
          "form": "엉ㅇㅇ",
          "original_form": "엉ㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100020809.1.16",
          "form": "그냥 여러 맛 중에 골라서 먹는거라",
          "original_form": "그냥 여러 맛 중에 골라서 먹는거라",
          "speaker_id": "1",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100020809.1.17",
          "form": "다양한 맛의 아이스티 먹는 기분임",
          "original_form": "다양한 맛의 아이스티 먹는 기분임",
          "speaker_id": "1",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100020809.1.18",
          "form": "아 괜찮겠다",
          "original_form": "아 괜찮겠다",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100020809.1.19",
          "form": "너도당줄이려고",
          "original_form": "너도당줄이려고",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100020809.1.20",
          "form": "제로사이다이런거",
          "original_form": "제로사이다이런거",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100020809.1.21",
          "form": "마셨는데",
          "original_form": "마셨는데",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100020809.1.22",
          "form": "이런것도괜찮을즛",
          "original_form": "이런것도괜찮을즛",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100020809.1.23",
          "form": "아 나돜ㅋㅋㅋㅋㅋㅋㅋ 제로 콜라 이런거",
          "original_form": "아 나돜ㅋㅋㅋㅋㅋㅋㅋ 제로 콜라 이런거",
          "speaker_id": "1",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100020809.1.24",
          "form": "아 근데 아스파탐이 닜어서 많이 먹으면 안좋댜",
          "original_form": "아 근데 아스파탐이 닜어서 많이 먹으면 안좋댜",
          "speaker_id": "1",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100020809.1.25",
          "form": "하루에 한 두포 정도만 먹으래",
          "original_form": "하루에 한 두포 정도만 먹으래",
          "speaker_id": "1",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100020809.1.26",
          "form": "아스파탐도몸에안좋아?",
          "original_form": "아스파탐도몸에안좋아?",
          "speaker_id": "2",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100020809.1.27",
          "form": "당이아니라괜찮은줄",
          "original_form": "당이아니라괜찮은줄",
          "speaker_id": "2",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100020809.1.28",
          "form": "그게 설탕의 200배 단맛을 내는데",
          "original_form": "그게 설탕의 200배 단맛을 내는데",
          "speaker_id": "1",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100020809.1.29",
          "form": "200배?",
          "original_form": "200배?",
          "speaker_id": "2",
          "time": "20210828 15:30"
        },
        {
          "id": "MDRW2100020809.1.30",
