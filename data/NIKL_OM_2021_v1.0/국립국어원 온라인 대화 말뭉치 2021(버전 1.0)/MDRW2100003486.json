      "utterance": [
        {
          "id": "MDRW2100003486.1.1",
          "form": "울 강아지들 뭐하는데 지금",
          "original_form": "울 강아지들 뭐하는데 지금",
          "speaker_id": "2",
          "time": "20210820 23:39"
        },
        {
          "id": "MDRW2100003486.1.2",
          "form": "다 디비자는데?",
          "original_form": "다 디비자는데?",
          "speaker_id": "1",
          "time": "20210820 23:40"
        },
        {
          "id": "MDRW2100003486.1.3",
          "form": "깨워서 산책좀갔다온나",
          "original_form": "깨워서 산책좀갔다온나",
          "speaker_id": "1",
          "time": "20210820 23:40"
        },
        {
          "id": "MDRW2100003486.1.4",
          "form": "애들 살쪄가지고 비만이다",
          "original_form": "애들 살쪄가지고 비만이다",
          "speaker_id": "1",
          "time": "20210820 23:40"
        },
        {
          "id": "MDRW2100003486.1.5",
          "form": "아까전에 산책 다 갔다왔다",
          "original_form": "아까전에 산책 다 갔다왔다",
          "speaker_id": "2",
          "time": "20210820 23:40"
        },
        {
          "id": "MDRW2100003486.1.6",
          "form": "그리고 애들 양치좀 시켜라",
          "original_form": "그리고 애들 양치좀 시켜라",
          "speaker_id": "1",
          "time": "20210820 23:40"
        },
        {
          "id": "MDRW2100003486.1.7",
          "form": "피곤한가보다",
          "original_form": "피곤한가보다",
          "speaker_id": "2",
          "time": "20210820 23:40"
        },
        {
          "id": "MDRW2100003486.1.8",
          "form": "이빨에 썩어서  피가난다",
          "original_form": "이빨에 썩어서  피가난다",
          "speaker_id": "1",
          "time": "20210820 23:40"
        },
        {
          "id": "MDRW2100003486.1.9",
          "form": "양치 시킨다 내 혼자로 역부족해서 그렇지",
          "original_form": "양치 시킨다 내 혼자로 역부족해서 그렇지",
          "speaker_id": "2",
          "time": "20210820 23:40"
        },
        {
          "id": "MDRW2100003486.1.10",
          "form": "아니거든 ㅡㅡ 니도 좀 시켜줘라",
          "original_form": "아니거든 ㅡㅡ 니도 좀 시켜줘라",
          "speaker_id": "2",
          "time": "20210820 23:40"
        },
        {
          "id": "MDRW2100003486.1.11",
          "form": "나중에가면 강냉이 우수수",
          "original_form": "나중에가면 강냉이 우수수",
          "speaker_id": "1",
          "time": "20210820 23:40"
        },
        {
          "id": "MDRW2100003486.1.12",
          "form": "떨어진다",
          "original_form": "떨어진다",
          "speaker_id": "1",
          "time": "20210820 23:40"
        },
        {
          "id": "MDRW2100003486.1.13",
          "form": "강아지 인형인줄 아나 잘 관리해줘야지",
          "original_form": "강아지 인형인줄 아나 잘 관리해줘야지",
          "speaker_id": "2",
          "time": "20210820 23:40"
        },
        {
          "id": "MDRW2100003486.1.14",
          "form": "책임감이 없누",
          "original_form": "책임감이 없누",
          "speaker_id": "2",
          "time": "20210820 23:40"
        },
        {
          "id": "MDRW2100003486.1.15",
          "form": "그럼 애들 잇몸으로 밥 먹겟지",
          "original_form": "그럼 애들 잇몸으로 밥 먹겟지",
          "speaker_id": "1",
          "time": "20210820 23:41"
        },
        {
          "id": "MDRW2100003486.1.16",
          "form": "넘슬프군",
          "original_form": "넘슬프군",
          "speaker_id": "1",
          "time": "20210820 23:41"
        },
        {
          "id": "MDRW2100003486.1.17",
          "form": "안된다 그럼",
          "original_form": "안된다 그럼",
          "speaker_id": "2",
          "time": "20210820 23:41"
        },
        {
          "id": "MDRW2100003486.1.18",
          "form": "오래오래 이빨 튼튼해야하는디",
          "original_form": "오래오래 이빨 튼튼해야하는디",
          "speaker_id": "2",
          "time": "20210820 23:41"
        },
        {
          "id": "MDRW2100003486.1.19",
          "form": "많이슬프군",
          "original_form": "많이슬프군",
          "speaker_id": "1",
          "time": "20210820 23:41"
        },
        {
          "id": "MDRW2100003486.1.20",
          "form": "니가 좀 해줘라 울 강지들 오래살게",
          "original_form": "니가 좀 해줘라 울 강지들 오래살게",
          "speaker_id": "2",
          "time": "20210820 23:41"
        },
        {
          "id": "MDRW2100003486.1.21",
          "form": "그럼 강아지 사료 내가 사옴 이빨 건강해지는거",
          "original_form": "그럼 강아지 사료 내가 사옴 이빨 건강해지는거",
          "speaker_id": "1",
          "time": "20210820 23:41"
        }
