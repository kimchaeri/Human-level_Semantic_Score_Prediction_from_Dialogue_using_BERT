      "utterance": [
        {
          "id": "MDRW2100004134.1.1",
          "form": "주로 어떤데에서 쇼핑하세요?",
          "original_form": "주로 어떤데에서 쇼핑하세요?",
          "speaker_id": "1",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004134.1.2",
          "form": "전 인터넷이요",
          "original_form": "전 인터넷이요",
          "speaker_id": "2",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004134.1.3",
          "form": "쇼핑할땐 사이즈를 잘 봐야해요",
          "original_form": "쇼핑할땐 사이즈를 잘 봐야해요",
          "speaker_id": "2",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004134.1.4",
          "form": "어떤 쇼셜커머스이용하세요?",
          "original_form": "어떤 쇼셜커머스이용하세요?",
          "speaker_id": "1",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004134.1.5",
          "form": "저는 옥션이용하다 요즘은 네이버요",
          "original_form": "저는 옥션이용하다 요즘은 네이버요",
          "speaker_id": "2",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004134.1.6",
          "form": "네이버~ 포인트가 잘쌓여서 좋아요",
          "original_form": "네이버~ 포인트가 잘쌓여서 좋아요",
          "speaker_id": "1",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004134.1.7",
          "form": "그쵸 ㅋㅋ",
          "original_form": "그쵸 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004134.1.8",
          "form": "네이버 포인트가 꽤 쏠쏠하더라고요",
          "original_form": "네이버 포인트가 꽤 쏠쏠하더라고요",
          "speaker_id": "2",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004134.1.9",
          "form": "맞아요~ 어느날보면 쌓여있더라구요 쏠쏠",
          "original_form": "맞아요~ 어느날보면 쌓여있더라구요 쏠쏠",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004134.1.10",
          "form": "그쵸 ㅎㅎ",
          "original_form": "그쵸 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004134.1.11",
          "form": "전 영수증리뷰도 많이해요",
          "original_form": "전 영수증리뷰도 많이해요",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004134.1.12",
          "form": "아 그것도 좋아요 영수증리뷰도 적립되더라구요",
          "original_form": "아 그것도 좋아요 영수증리뷰도 적립되더라구요",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004134.1.13",
          "form": "네 전 그래서 식당가면 영수증 챙기는 습관이 생겼어요",
          "original_form": "네 전 그래서 식당가면 영수증 챙기는 습관이 생겼어요",
          "speaker_id": "2",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004134.1.14",
          "form": "근데 네이버쇼핑은 배송비 붙은게 많긴해요",
          "original_form": "근데 네이버쇼핑은 배송비 붙은게 많긴해요",
          "speaker_id": "2",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004134.1.15",
          "form": "배송이나 빠른거는 역시 쿠팡이에요",
          "original_form": "배송이나 빠른거는 역시 쿠팡이에요",
          "speaker_id": "1",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004134.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004134.1.17",
          "form": "전 그래서 마켓컬리도 종종 이용해요",
          "original_form": "전 그래서 마켓컬리도 종종 이용해요",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004134.1.18",
          "form": "담날 먹을거 없을땐 짱편",
          "original_form": "담날 먹을거 없을땐 짱편",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004134.1.19",
          "form": "마켓컬리는 궁금하긴하네요 저희지역은 새벽배송지역이 아니라",
          "original_form": "마켓컬리는 궁금하긴하네요 저희지역은 새벽배송지역이 아니라",
          "speaker_id": "1",
          "time": "20210821 17:45"
        },
        {
          "id": "MDRW2100004134.1.20",
          "form": "대신 쿠팡 프레쉬이야기해요",
          "original_form": "대신 쿠팡 프레쉬이야기해요",
          "speaker_id": "1",
          "time": "20210821 17:45"
        },
        {
          "id": "MDRW2100004134.1.21",
          "form": "초반엔 마니 주문했는데 가격이 비합리적이라 점점 끊어가는 추세예요",
          "original_form": "초반엔 마니 주문했는데 가격이 비합리적이라 점점 끊어가는 추세예요",
          "speaker_id": "2",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004134.1.22",
          "form": "맞어요 저도요 쿠팡 이용자주했는데 식비가 너무 많이들더라구요",
          "original_form": "맞어요 저도요 쿠팡 이용자주했는데 식비가 너무 많이들더라구요",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004134.1.23",
          "form": "이제는 필요한만큼 사러가요",
          "original_form": "이제는 필요한만큼 사러가요",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004134.1.24",
          "form": "맞아요 버려지는것도 많아서 딱 필요한 만큼만",
          "original_form": "맞아요 버려지는것도 많아서 딱 필요한 만큼만",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004134.1.25",
          "form": "배송비 맞추기위해 막필요없는것도 사게되구 맞아욥!",
          "original_form": "배송비 맞추기위해 막필요없는것도 사게되구 맞아욥!",
          "speaker_id": "1",
          "time": "20210821 18:38"
        }
      ]
    }
