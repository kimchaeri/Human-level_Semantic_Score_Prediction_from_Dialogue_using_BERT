      "utterance": [
        {
          "id": "MDRW2100031960.1.1",
          "form": "크리미하지않아서 좀 별로였어 넌?",
          "original_form": "크리미하지않아서 좀 별로였어 넌?",
          "speaker_id": "2",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100031960.1.2",
          "form": "나도... 너무 조미료맛이 강해",
          "original_form": "나도... 너무 조미료맛이 강해",
          "speaker_id": "1",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100031960.1.3",
          "form": "그치 근데 칼칼한로제 좋아하는사람은 맛있어할듯",
          "original_form": "그치 근데 칼칼한로제 좋아하는사람은 맛있어할듯",
          "speaker_id": "2",
          "time": "20210905 09:47"
        },
        {
          "id": "MDRW2100031960.1.4",
          "form": "나는 태리로제가 훨 좋더라",
          "original_form": "나는 태리로제가 훨 좋더라",
          "speaker_id": "1",
          "time": "20210905 09:47"
        },
        {
          "id": "MDRW2100031960.1.5",
          "form": "거긴 맛난데 배달료가 비싸니 내가 거기 갈일 있을때 사올게",
          "original_form": "거긴 맛난데 배달료가 비싸니 내가 거기 갈일 있을때 사올게",
          "speaker_id": "2",
          "time": "20210905 09:48"
        },
        {
          "id": "MDRW2100031960.1.6",
          "form": "근데 석관동에도 생크림을 넣으면 맛날거같애",
          "original_form": "근데 석관동에도 생크림을 넣으면 맛날거같애",
          "speaker_id": "1",
          "time": "20210905 09:49"
        },
        {
          "id": "MDRW2100031960.1.7",
          "form": "올 제법인데 나도 그리 생각해 \r\n생크림좀 사놓아야겠다",
          "original_form": "올 제법인데 나도 그리 생각해 \r\n생크림좀 사놓아야겠다",
          "speaker_id": "2",
          "time": "20210905 09:49"
        },
        {
          "id": "MDRW2100031960.1.8",
          "form": "신전 로제도 맛있대",
          "original_form": "신전 로제도 맛있대",
          "speaker_id": "1",
          "time": "20210905 09:50"
        },
        {
          "id": "MDRW2100031960.1.9",
          "form": "거기에서도 시켜먹자\r\n순만맛으로",
          "original_form": "거기에서도 시켜먹자\r\n순만맛으로",
          "speaker_id": "2",
          "time": "20210905 09:50"
        },
        {
          "id": "MDRW2100031960.1.10",
          "form": "근데 거긴 후추맛 로제 아닐까ㅋㅋ",
          "original_form": "근데 거긴 후추맛 로제 아닐까ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 09:50"
        },
        {
          "id": "MDRW2100031960.1.11",
          "form": "거기 음식맛의 반이 후추맛이니 그럴지도",
          "original_form": "거기 음식맛의 반이 후추맛이니 그럴지도",
          "speaker_id": "2",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100031960.1.12",
          "form": "어제 납작당면 진짜 맛있드라",
          "original_form": "어제 납작당면 진짜 맛있드라",
          "speaker_id": "1",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100031960.1.13",
          "form": "소스가 좀 넉넉하니 당면 삶아서 넣어먹을수있어 그건 좋네",
          "original_form": "소스가 좀 넉넉하니 당면 삶아서 넣어먹을수있어 그건 좋네",
          "speaker_id": "2",
          "time": "20210905 09:51"
