      "utterance": [
        {
          "id": "MDRW2100018403.1.1",
          "form": "저녁드셨어요!?",
          "original_form": "저녁드셨어요!?",
          "speaker_id": "1",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100018403.1.2",
          "form": "네 저녁먹엇어요 ㅋㅋㅋㅋ",
          "original_form": "네 저녁먹엇어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100018403.1.3",
          "form": "저녁 드셧어요?",
          "original_form": "저녁 드셧어요?",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100018403.1.4",
          "form": "네네ㅋㅋㅋㅋ볶음밥해먹었어요",
          "original_form": "네네ㅋㅋㅋㅋ볶음밥해먹었어요",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100018403.1.5",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100018403.1.6",
          "form": "저는 카레해먹엇어요 ㅋㅋㅋㅋ",
          "original_form": "저는 카레해먹엇어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100018403.1.7",
          "form": "보통 볶음밥에 뭐 넣으세요?",
          "original_form": "보통 볶음밥에 뭐 넣으세요?",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100018403.1.8",
          "form": "파 감자 양파 당근 정도 넣어요 ㅋㅋㅋㅋ 햄있으면 햄넣구요!",
          "original_form": "파 감자 양파 당근 정도 넣어요 ㅋㅋㅋㅋ 햄있으면 햄넣구요!",
          "speaker_id": "1",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100018403.1.9",
          "form": "계란은 안 넣으시나요?",
          "original_form": "계란은 안 넣으시나요?",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100018403.1.10",
          "form": "아 계란은 따로 후라이해서 같이먹는편이에요 ㅋㅋㅋㅋ",
          "original_form": "아 계란은 따로 후라이해서 같이먹는편이에요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100018403.1.11",
          "form": "아 그렇게 먹어도 맛잇죠 ㅎㅎ",
          "original_form": "아 그렇게 먹어도 맛잇죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 20:02"
        },
        {
          "id": "MDRW2100018403.1.12",
          "form": "요리 좋아하세요?",
          "original_form": "요리 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100018403.1.13",
          "form": "네 좋아하는편이에요 ㅋㅋㅋㅋ근데 좀 귀찮음이 커서 자주는 안해요",
          "original_form": "네 좋아하는편이에요 ㅋㅋㅋㅋ근데 좀 귀찮음이 커서 자주는 안해요",
          "speaker_id": "1",
          "time": "20210828 20:04"
        },
        {
          "id": "MDRW2100018403.1.14",
          "form": "제일 자신있는 요리 있으세요?",
          "original_form": "제일 자신있는 요리 있으세요?",
          "speaker_id": "2",
          "time": "20210828 20:07"
        },
        {
          "id": "MDRW2100018403.1.15",
          "form": "아니요 그정도는 아니에요ㅋㅋㅋㅋ",
          "original_form": "아니요 그정도는 아니에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100018403.1.16",
          "form": "그나마 파스타 자주해서 나름 자신잌ㅅ어요 ㅋㅋㅋㅋ",
          "original_form": "그나마 파스타 자주해서 나름 자신잌ㅅ어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:10"
        },
        {
          "id": "MDRW2100018403.1.17",
          "form": "어떤 파스타 주로 드세요?",
          "original_form": "어떤 파스타 주로 드세요?",
          "speaker_id": "2",
          "time": "20210828 20:11"
        },
        {
          "id": "MDRW2100018403.1.18",
          "form": "저는 크림파스타 좋아해요",
          "original_form": "저는 크림파스타 좋아해요",
          "speaker_id": "1",
          "time": "20210828 20:12"
        },
        {
          "id": "MDRW2100018403.1.19",
          "form": "저도 크림파스타 좋아해요!",
          "original_form": "저도 크림파스타 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 20:13"
        },
        {
          "id": "MDRW2100018403.1.20",
          "form": "크림 소스도 만드시나요?",
          "original_form": "크림 소스도 만드시나요?",
          "speaker_id": "2",
          "time": "20210828 20:13"
        },
        {
          "id": "MDRW2100018403.1.21",
          "form": "만든적도있는데 제품사는게 더 편하더라구요 ㅋㅋㅋㅋ",
          "original_form": "만든적도있는데 제품사는게 더 편하더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:14"
        },
        {
          "id": "MDRW2100018403.1.22",
          "form": "ㅋㅋㅋㅋ어느 제품 애용하세요?",
          "original_form": "ㅋㅋㅋㅋ어느 제품 애용하세요?",
          "speaker_id": "2",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100018403.1.23",
          "form": "저 청정원 소스요 ㅋㅋㅋ",
          "original_form": "저 청정원 소스요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100018403.1.24",
          "form": "저도 청정원 소스 써요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 청정원 소스 써요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100018403.1.25",
          "form": "며칠전에도 청정원 로제소스 병뚜껑 낑낑거리며 겨우 열엇네요 ㅋㅋㅋㅋ",
