      "utterance": [
        {
          "id": "MDRW2100035481.1.1",
          "form": "집에서 직접 요리도 하시나요?",
          "original_form": "집에서 직접 요리도 하시나요?",
          "speaker_id": "2",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100035481.1.2",
          "form": "간단한건 해먹어요!",
          "original_form": "간단한건 해먹어요!",
          "speaker_id": "1",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100035481.1.3",
          "form": "특기는 볶음밥입니당ㅋㅋ",
          "original_form": "특기는 볶음밥입니당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100035481.1.4",
          "form": "볶음밥 잘 하신다니 부러워요ㅠ 속재료는 때마다 달라지나요?",
          "original_form": "볶음밥 잘 하신다니 부러워요ㅠ 속재료는 때마다 달라지나요?",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100035481.1.5",
          "form": "있는걸로 해요!!",
          "original_form": "있는걸로 해요!!",
          "speaker_id": "1",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100035481.1.6",
          "form": "그러다 요즘은 냉동 야채로 하고 있어요ㅋㅋ",
          "original_form": "그러다 요즘은 냉동 야채로 하고 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100035481.1.7",
          "form": "저는 예전에 냉동 파로 파기름 내다가 이리 저리 튀어 난리났었어요ㅋㅋㅋ",
          "original_form": "저는 예전에 냉동 파로 파기름 내다가 이리 저리 튀어 난리났었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100035481.1.8",
          "form": "앗ㅋㅋㅋ저도 냉동야채 쓸때 그랬어요ㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋ저도 냉동야채 쓸때 그랬어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:27"
        },
        {
          "id": "MDRW2100035481.1.9",
          "form": "너무 당황해서 불껐었어욬ㅋㅋㅋ",
          "original_form": "너무 당황해서 불껐었어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:27"
        },
        {
          "id": "MDRW2100035481.1.10",
          "form": "키친타올로 흡수시켜도 물기가ㅠㅠㅠ",
          "original_form": "키친타올로 흡수시켜도 물기가ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100035481.1.11",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100035481.1.12",
          "form": "처음 사용할땐 놀랐죠",
          "original_form": "처음 사용할땐 놀랐죠",
          "speaker_id": "1",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100035481.1.13",
          "form": "대처 방안이 있으신가요?",
          "original_form": "대처 방안이 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 20:48"
        },
        {
          "id": "MDRW2100035481.1.14",
          "form": "저는 그냥 넣고 뚜껑 덮고 좀 기다렸다가 볶아요ㅋㅋ",
          "original_form": "저는 그냥 넣고 뚜껑 덮고 좀 기다렸다가 볶아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100035481.1.15",
          "form": "헐헐....왜 지금까지 뚜껑 생각을 못 했을까요...",
          "original_form": "헐헐....왜 지금까지 뚜껑 생각을 못 했을까요...",
          "speaker_id": "2",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100035481.1.16",
          "form": "팔을 내어주고 얻게되었어욥..",
          "original_form": "팔을 내어주고 얻게되었어욥..",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100035481.1.17",
          "form": "화상입지는 않으셨나요ㅠ??",
          "original_form": "화상입지는 않으셨나요ㅠ??",
          "speaker_id": "2",
          "time": "20210829 22:06"
        }
      ]
