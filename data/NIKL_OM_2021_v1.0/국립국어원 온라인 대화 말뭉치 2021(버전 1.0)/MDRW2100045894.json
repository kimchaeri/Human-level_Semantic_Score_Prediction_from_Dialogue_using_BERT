      "utterance": [
        {
          "id": "MDRW2100045894.1.1",
          "form": "품절이란...선택고자에겐 신과 같죠",
          "original_form": "품절이란...선택고자에겐 신과 같죠",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045894.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045894.1.3",
          "form": "그냥 운명으로 받아들이면 편한...",
          "original_form": "그냥 운명으로 받아들이면 편한...",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045894.1.4",
          "form": "전에기사보는데",
          "original_form": "전에기사보는데",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045894.1.5",
          "form": "장바구니 넣기만해도 일단",
          "original_form": "장바구니 넣기만해도 일단",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045894.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045894.1.7",
          "form": "감정적으로 충족되는게 있다고",
          "original_form": "감정적으로 충족되는게 있다고",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045894.1.8",
          "form": "물욕이 충족된대요?",
          "original_form": "물욕이 충족된대요?",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045894.1.9",
          "form": "하더리구요 ㅋㅋ",
          "original_form": "하더리구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045894.1.10",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045894.1.11",
          "form": "맞는 거 같아요",
          "original_form": "맞는 거 같아요",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045894.1.12",
          "form": "완전히는아니겠지만",
          "original_form": "완전히는아니겠지만",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045894.1.13",
          "form": "일단 넣고봐야돼요",
          "original_form": "일단 넣고봐야돼요",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045894.1.14",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045894.1.15",
          "form": "왜냐면 그순간 이미 약간 짜릿해요",
          "original_form": "왜냐면 그순간 이미 약간 짜릿해요",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045894.1.16",
          "form": "일단애매한건 넣어두고",
          "original_form": "일단애매한건 넣어두고",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045894.1.17",
          "form": "품절 임박 기다리는",
          "original_form": "품절 임박 기다리는",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045894.1.18",
          "form": "급한 건 일단 결제하고",
          "original_form": "급한 건 일단 결제하고",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045894.1.19",
          "form": "맞아요ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045894.1.20",
          "form": "5개남으면 사고",
          "original_form": "5개남으면 사고",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045894.1.21",
          "form": "그때도아른거리면",
          "original_form": "그때도아른거리면",
          "speaker_id": "1",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045894.1.22",
          "form": "근데 내가 산 거 품절 안뜨면",
          "original_form": "근데 내가 산 거 품절 안뜨면",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045894.1.23",
          "form": "좀 찜찜해요",
          "original_form": "좀 찜찜해요",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045894.1.24",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045894.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045894.1.26",
          "form": "구린거샀나싶고",
          "original_form": "구린거샀나싶고",
          "speaker_id": "1",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045894.1.27",
          "form": "내 선택이 잘못됐나",
          "original_form": "내 선택이 잘못됐나",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045894.1.28",
          "form": "좀 머뭇머뭇",
          "original_form": "좀 머뭇머뭇",
          "speaker_id": "2",
          "time": "20210908 08:31"
