          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045894.1.30",
          "form": "정말현명한쇼핑",
          "original_form": "정말현명한쇼핑",
          "speaker_id": "1",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045894.1.31",
          "form": "쉽게결제하지않는",
          "original_form": "쉽게결제하지않는",
          "speaker_id": "1",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045894.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045894.1.33",
          "form": "우리 진짜 자제한 거라고요",
          "original_form": "우리 진짜 자제한 거라고요",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045894.1.34",
          "form": "그니까요 결제까지 100번고민",
          "original_form": "그니까요 결제까지 100번고민",
          "speaker_id": "1",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045894.1.35",
          "form": "왜냐면아끼면",
