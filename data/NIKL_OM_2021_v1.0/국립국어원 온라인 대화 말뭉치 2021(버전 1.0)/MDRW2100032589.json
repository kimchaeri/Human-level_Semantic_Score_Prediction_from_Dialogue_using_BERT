      "utterance": [
        {
          "id": "MDRW2100032589.1.1",
          "form": "최애 반찬이 머야",
          "original_form": "최애 반찬이 머야",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100032589.1.2",
          "form": "장 조 림",
          "original_form": "장 조 림",
          "speaker_id": "1",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100032589.1.3",
          "form": "진짜 밥도둑",
          "original_form": "진짜 밥도둑",
          "speaker_id": "1",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100032589.1.4",
          "form": "ㅋㅋㅋㅋ머선 장조림?!",
          "original_form": "ㅋㅋㅋㅋ머선 장조림?!",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100032589.1.5",
          "form": "고기는 당얀히 좋은데 계란이 진짜 짱이여",
          "original_form": "고기는 당얀히 좋은데 계란이 진짜 짱이여",
          "speaker_id": "1",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100032589.1.6",
          "form": "예전에 마약장조림도 엄청먹억는뎈ㅋㅋㅋ",
          "original_form": "예전에 마약장조림도 엄청먹억는뎈ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100032589.1.7",
          "form": "예전에 마약장조림도 엄청먹억는뎈ㅋㅋㅋ",
          "original_form": "예전에 마약장조림도 엄청먹억는뎈ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100032589.1.8",
          "form": "ㅋㅋㅋ마약장조림은 머고",
          "original_form": "ㅋㅋㅋ마약장조림은 머고",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100032589.1.9",
          "form": "계란 맛잇지...밥도둑ㅇㅈ",
          "original_form": "계란 맛잇지...밥도둑ㅇㅈ",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100032589.1.10",
          "form": "맞재 진짜 나는 좀 간장베이스 좋아하나바",
          "original_form": "맞재 진짜 나는 좀 간장베이스 좋아하나바",
          "speaker_id": "1",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100032589.1.11",
          "form": "잔맛탱",
          "original_form": "잔맛탱",
          "speaker_id": "1",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100032589.1.12",
          "form": "ㅋㅋ간장 베이스가 꾸준히 들어가긴 함...",
          "original_form": "ㅋㅋ간장 베이스가 꾸준히 들어가긴 함...",
          "speaker_id": "2",
          "time": "20210905 21:04"
        },
        {
          "id": "MDRW2100032589.1.13",
          "form": "내 최애 반찬은 국인거 같은데",
          "original_form": "내 최애 반찬은 국인거 같은데",
          "speaker_id": "2",
          "time": "20210905 21:04"
        },
        {
          "id": "MDRW2100032589.1.14",
          "form": "오 국은 무슨국조아해",
          "original_form": "오 국은 무슨국조아해",
          "speaker_id": "1",
          "time": "20210905 21:04"
        },
        {
          "id": "MDRW2100032589.1.15",
          "form": "소고기국 오징어국 북어국 매운콩나물국....",
          "original_form": "소고기국 오징어국 북어국 매운콩나물국....",
          "speaker_id": "2",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100032589.1.16",
          "form": "고를 수 없어ㅠ",
          "original_form": "고를 수 없어ㅠ",
          "speaker_id": "2",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100032589.1.17",
          "form": "아 맞아 맛잇는 국 많음 ㅠㅠ",
          "original_form": "아 맞아 맛잇는 국 많음 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100032589.1.18",
          "form": "오징어국 되게 오랭만에듣는다",
          "original_form": "오징어국 되게 오랭만에듣는다",
          "speaker_id": "1",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100032589.1.19",
          "form": "ㅋㅋㅋ오징어국 맛잇다아녀",
          "original_form": "ㅋㅋㅋ오징어국 맛잇다아녀",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100032589.1.20",
          "form": "난 맛있는 국물만 있으면 밥 두그릇 먹어 ㅋㅋㅋ",
          "original_form": "난 맛있는 국물만 있으면 밥 두그릇 먹어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100032589.1.21",
          "form": "ㅇㅇㅇ묘하게 시유ㅓㄴ하고 몬가 맛잇어",
          "original_form": "ㅇㅇㅇ묘하게 시유ㅓㄴ하고 몬가 맛잇어",
          "speaker_id": "1",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100032589.1.22",
          "form": "ㅋㅋㅋㅋㅋ밥도둑이네 국이",
          "original_form": "ㅋㅋㅋㅋㅋ밥도둑이네 국이",
          "speaker_id": "1",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100032589.1.23",
          "form": "ㅇㅇ오징어가 진짜 감칠맛있음ㅋㅋㅋ",
          "original_form": "ㅇㅇ오징어가 진짜 감칠맛있음ㅋㅋㅋ",
