      "utterance": [
        {
          "id": "MDRW2100004303.1.1",
          "form": "일하는거 힘들지? 휴가도 끝나서",
          "original_form": "일하는거 힘들지? 휴가도 끝나서",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004303.1.2",
          "form": "힘듭니다",
          "original_form": "힘듭니다",
          "speaker_id": "2",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004303.1.3",
          "form": "휴가도 끝나고 일하는거 힘들겄어~",
          "original_form": "휴가도 끝나고 일하는거 힘들겄어~",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004303.1.4",
          "form": "나도 곧 끝날 휴가가 두렵네",
          "original_form": "나도 곧 끝날 휴가가 두렵네",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004303.1.5",
          "form": "새로운 프로젝트 기획하는 것도 힘들고요",
          "original_form": "새로운 프로젝트 기획하는 것도 힘들고요",
          "speaker_id": "2",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004303.1.6",
          "form": "아 그치",
          "original_form": "아 그치",
          "speaker_id": "1",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004303.1.7",
          "form": "프로젝트 계획하면서 뭐가 많이 힘들어?",
          "original_form": "프로젝트 계획하면서 뭐가 많이 힘들어?",
          "speaker_id": "1",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004303.1.8",
          "form": "그냥 방향 잡는거랑",
          "original_form": "그냥 방향 잡는거랑",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004303.1.9",
          "form": "관련자료 찾는 것",
          "original_form": "관련자료 찾는 것",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004303.1.10",
          "form": "그리고 이게 잘 실현될 것인가에 대한 불안?",
          "original_form": "그리고 이게 잘 실현될 것인가에 대한 불안?",
          "speaker_id": "2",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004303.1.11",
          "form": "아하 프로젝트 대상자나 동료나 상사같은 사람이 아니라",
          "original_form": "아하 프로젝트 대상자나 동료나 상사같은 사람이 아니라",
          "speaker_id": "1",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004303.1.12",
          "form": "프로젝트 자체가 신경이 쓰이는거구나",
          "original_form": "프로젝트 자체가 신경이 쓰이는거구나",
          "speaker_id": "1",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004303.1.13",
          "form": "그렇습니다",
          "original_form": "그렇습니다",
          "speaker_id": "2",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004303.1.14",
          "form": "언니는 일과 직업에 만족하나요?",
          "original_form": "언니는 일과 직업에 만족하나요?",
          "speaker_id": "2",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004303.1.15",
          "form": "네 뭐 만족은 아니지만",
          "original_form": "네 뭐 만족은 아니지만",
          "speaker_id": "1",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004303.1.16",
          "form": "좀 안정적인 느낌이 있어서 좋네요",
          "original_form": "좀 안정적인 느낌이 있어서 좋네요",
          "speaker_id": "1",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004303.1.17",
          "form": "월급이 꼬박꼬박 나온다는 점이..",
          "original_form": "월급이 꼬박꼬박 나온다는 점이..",
          "speaker_id": "1",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004303.1.18",
          "form": "최근에 지금 직장이 1년에 한번 인센티브를 준다는 뉴스를 들었는데",
          "original_form": "최근에 지금 직장이 1년에 한번 인센티브를 준다는 뉴스를 들었는데",
          "speaker_id": "1",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004303.1.19",
          "form": "왠지 정규직만 해당인듯해서 (왜냐면 내 계약서에선 본 기억이 없음) 빨리 잊으려고 ㅋㅋ",
          "original_form": "왠지 정규직만 해당인듯해서 (왜냐면 내 계약서에선 본 기억이 없음) 빨리 잊으려고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004303.1.20",
          "form": "계약직 짱나~~",
          "original_form": "계약직 짱나~~",
          "speaker_id": "1",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004303.1.21",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004303.1.22",
          "form": "얼른 더 안정적인 근무조건이 되길",
          "original_form": "얼른 더 안정적인 근무조건이 되길",
          "speaker_id": "2",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004303.1.23",
          "form": "응 이직해야..하는데.. 이직하려면 자기계발을 열심히 해야하는데... 하기가... 귀찮다...",
          "original_form": "응 이직해야..하는데.. 이직하려면 자기계발을 열심히 해야하는데... 하기가... 귀찮다...",
          "speaker_id": "1",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004303.1.24",
          "form": "의 상태입니다",
          "original_form": "의 상태입니다",
          "speaker_id": "1",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004303.1.25",
          "form": "이해합니다~",
          "original_form": "이해합니다~",
          "speaker_id": "2",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004303.1.26",
          "form": "월요일부터 해야할 일도 준비해야하는데",
          "original_form": "월요일부터 해야할 일도 준비해야하는데",
          "speaker_id": "2",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004303.1.27",
          "form": "매우 귀찮군요",
          "original_form": "매우 귀찮군요",
          "speaker_id": "2",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004303.1.28",
          "form": "컴퓨터 켜고 계속 앉아있는데",
          "original_form": "컴퓨터 켜고 계속 앉아있는데",
          "speaker_id": "2",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004303.1.29",
          "form": "진척이 없음",
          "original_form": "진척이 없음",
          "speaker_id": "2",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004303.1.30",
          "form": "ㅋㅋㅋ주말에 열심히 해야할 건 휴식 아닌가요",
          "original_form": "ㅋㅋㅋ주말에 열심히 해야할 건 휴식 아닌가요",
          "speaker_id": "1",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004303.1.31",
          "form": "업무에서 벗어난 휴식을 즐기시죠",
          "original_form": "업무에서 벗어난 휴식을 즐기시죠",
          "speaker_id": "1",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004303.1.32",
          "form": "그 뭐야 지난번 인터뷰한거 페이는 정산됐나요",
          "original_form": "그 뭐야 지난번 인터뷰한거 페이는 정산됐나요",
          "speaker_id": "1",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004303.1.33",
          "form": "아 서류는 전송해서 받았단 메일 받았는데",
          "original_form": "아 서류는 전송해서 받았단 메일 받았는데",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004303.1.34",
          "form": "입금은 아직입니다",
          "original_form": "입금은 아직입니다",
          "speaker_id": "2",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004303.1.35",
          "form": "이제 일하러 다시 가봐야겠어요",
          "original_form": "이제 일하러 다시 가봐야겠어요",
          "speaker_id": "2",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004303.1.36",
          "form": "일하기 싫다",
          "original_form": "일하기 싫다",
          "speaker_id": "2",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100004303.1.37",
          "form": "내 일을 잘하고 싶은데",
          "original_form": "내 일을 잘하고 싶은데",
          "speaker_id": "2",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100004303.1.38",
          "form": "하기는 싫다ㅋㅋ",
          "original_form": "하기는 싫다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:36"
        }
      ]
    }
