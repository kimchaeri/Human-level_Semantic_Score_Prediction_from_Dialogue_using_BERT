      "utterance": [
        {
          "id": "MDRW2100019629.1.1",
          "form": "종류?",
          "original_form": "종류?",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100019629.1.2",
          "form": "종류도 그렇고 맛?",
          "original_form": "종류도 그렇고 맛?",
          "speaker_id": "1",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019629.1.3",
          "form": "아이스 아메리카노 좋아해.",
          "original_form": "아이스 아메리카노 좋아해.",
          "speaker_id": "2",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019629.1.4",
          "form": "산미감 있는 게 좋아?",
          "original_form": "산미감 있는 게 좋아?",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019629.1.5",
          "form": "산미감이 머야?",
          "original_form": "산미감이 머야?",
          "speaker_id": "2",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019629.1.6",
          "form": "신맛 나는 거",
          "original_form": "신맛 나는 거",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019629.1.7",
          "form": "아메리카노에서 신맛도 나?",
          "original_form": "아메리카노에서 신맛도 나?",
          "speaker_id": "2",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019629.1.8",
          "form": "약간 신맛 나는 커피들 있어",
          "original_form": "약간 신맛 나는 커피들 있어",
          "speaker_id": "1",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019629.1.9",
          "form": "예를 들면?",
          "original_form": "예를 들면?",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019629.1.10",
          "form": "그냥 마셔보면 카페마다 다르던데?",
          "original_form": "그냥 마셔보면 카페마다 다르던데?",
          "speaker_id": "1",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019629.1.11",
          "form": "난 그렇게 미각이 예민하지 않아서",
          "original_form": "난 그렇게 미각이 예민하지 않아서",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019629.1.12",
          "form": "그냥 크게 상관없구나",
          "original_form": "그냥 크게 상관없구나",
          "speaker_id": "1",
