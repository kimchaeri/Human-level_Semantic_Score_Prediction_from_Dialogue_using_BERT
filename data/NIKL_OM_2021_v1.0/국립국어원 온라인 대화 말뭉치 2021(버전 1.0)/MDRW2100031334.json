      "utterance": [
        {
          "id": "MDRW2100031334.1.1",
          "form": "공표 맥주 드셔보셨어요",
          "original_form": "공표 맥주 드셔보셨어요",
          "speaker_id": "1",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031334.1.2",
          "form": "그게 뭔가요",
          "original_form": "그게 뭔가요",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031334.1.3",
          "form": "곰표에서 나온 디자인",
          "original_form": "곰표에서 나온 디자인",
          "speaker_id": "1",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031334.1.4",
          "form": "아기 곰이야",
          "original_form": "아기 곰이야",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031334.1.5",
          "form": "아니에요 큰 곰이에요",
          "original_form": "아니에요 큰 곰이에요",
          "speaker_id": "1",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031334.1.6",
          "form": "네 고민이네요",
          "original_form": "네 고민이네요",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031334.1.7",
          "form": "고민하시고 드시지 마시고",
          "original_form": "고민하시고 드시지 마시고",
          "speaker_id": "1",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031334.1.8",
          "form": "맛있나요",
          "original_form": "맛있나요",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031334.1.9",
          "form": "달달하고 밀맥이라 괜찮아요",
          "original_form": "달달하고 밀맥이라 괜찮아요",
          "speaker_id": "1",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031334.1.10",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031334.1.11",
          "form": "추천 드릴 테니 한번 드셔 보세요",
          "original_form": "추천 드릴 테니 한번 드셔 보세요",
          "speaker_id": "1",
          "time": "20210904 20:35"
        },
        {
          "id": "MDRW2100031334.1.12",
          "form": "어디서 파나요",
          "original_form": "어디서 파나요",
          "speaker_id": "2",
          "time": "20210904 20:35"
        },
        {
          "id": "MDRW2100031334.1.13",
          "form": "편의점에서 팔아요",
          "original_form": "편의점에서 팔아요",
          "speaker_id": "1",
          "time": "20210904 20:35"
        },
        {
          "id": "MDRW2100031334.1.14",
          "form": "저희 동네는 없던데",
          "original_form": "저희 동네는 없던데",
