      "utterance": [
        {
          "id": "MDRW2100040750.1.1",
          "form": "ㅋㅋㅋㅋ 진짜 너무하다고",
          "original_form": "ㅋㅋㅋㅋ 진짜 너무하다고",
          "speaker_id": "2",
          "time": "20210905 01:45"
        },
        {
          "id": "MDRW2100040750.1.2",
          "form": "생각이 들정도입니닼ㅋㅋㅋ",
          "original_form": "생각이 들정도입니닼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:45"
        },
        {
          "id": "MDRW2100040750.1.3",
          "form": "왜 그러니까요 너무 이상했어요",
          "original_form": "왜 그러니까요 너무 이상했어요",
          "speaker_id": "1",
          "time": "20210905 01:46"
        },
        {
          "id": "MDRW2100040750.1.4",
          "form": "저런 것 엄청 많을 걸요 몰라서 그렇죠",
          "original_form": "저런 것 엄청 많을 걸요 몰라서 그렇죠",
          "speaker_id": "1",
          "time": "20210905 01:46"
        },
        {
          "id": "MDRW2100040750.1.5",
          "form": "블로거까지 동원하는거 보면",
          "original_form": "블로거까지 동원하는거 보면",
          "speaker_id": "2",
          "time": "20210905 01:46"
        },
        {
          "id": "MDRW2100040750.1.6",
          "form": "저거 말고도요",
          "original_form": "저거 말고도요",
          "speaker_id": "1",
          "time": "20210905 01:46"
        },
        {
          "id": "MDRW2100040750.1.7",
          "form": "진짜 이상ㅋㅋㅋㅋ",
          "original_form": "진짜 이상ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:46"
        },
        {
          "id": "MDRW2100040750.1.8",
          "form": "어쩜 저기는 블로그 하는거",
          "original_form": "어쩜 저기는 블로그 하는거",
          "speaker_id": "1",
          "time": "20210905 01:46"
        },
        {
          "id": "MDRW2100040750.1.9",
          "form": "그거도 sns담당자라고 월 이백 주고",
          "original_form": "그거도 sns담당자라고 월 이백 주고",
          "speaker_id": "1",
          "time": "20210905 01:46"
        },
        {
          "id": "MDRW2100040750.1.10",
          "form": "청년 고용해서 또 그거는 청년인건비 지원받을지도요 ㅋㅋㅋㅋ",
          "original_form": "청년 고용해서 또 그거는 청년인건비 지원받을지도요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:46"
        },
        {
          "id": "MDRW2100040750.1.11",
          "form": "저런 블로그 운영이면 저 혼자 열개는 하겠어요 ㅋㅋㅋ",
          "original_form": "저런 블로그 운영이면 저 혼자 열개는 하겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:47"
        },
        {
          "id": "MDRW2100040750.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그러니까요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그러니까요",
          "speaker_id": "2",
          "time": "20210905 01:47"
        },
        {
          "id": "MDRW2100040750.1.13",
          "form": "일주일에 한번 올라올까 말까",
          "original_form": "일주일에 한번 올라올까 말까",
          "speaker_id": "2",
          "time": "20210905 01:47"
        },
        {
          "id": "MDRW2100040750.1.14",
          "form": "시간 어떻게 때우져??",
          "original_form": "시간 어떻게 때우져??",
          "speaker_id": "2",
          "time": "20210905 01:47"
        },
        {
          "id": "MDRW2100040750.1.15",
          "form": "저런 자잘한 센터들이 다 홈페이지 있고 블로그 있고 담당자 있고 다 있더라고요",
          "original_form": "저런 자잘한 센터들이 다 홈페이지 있고 블로그 있고 담당자 있고 다 있더라고요",
          "speaker_id": "1",
          "time": "20210905 01:47"
        },
        {
          "id": "MDRW2100040750.1.16",
          "form": "시간 때우는게 곤욕스러울거 같은뎈ㅋㅋㅋ",
          "original_form": "시간 때우는게 곤욕스러울거 같은뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:48"
        },
        {
          "id": "MDRW2100040750.1.17",
          "form": "그러면서 자기들끼리 무슨 자격증 발급 한다면서 교육하고",
          "original_form": "그러면서 자기들끼리 무슨 자격증 발급 한다면서 교육하고",
          "speaker_id": "1",
          "time": "20210905 01:48"
        },
        {
          "id": "MDRW2100040750.1.18",
          "form": "거기서 자격증 따고 교육 받으면 정말 전문가 될까요 일주일에 3시간 정도 듣고 한 달 정도 들으면 전문가가 되나요",
          "original_form": "거기서 자격증 따고 교육 받으면 정말 전문가 될까요 일주일에 3시간 정도 듣고 한 달 정도 들으면 전문가가 되나요",
          "speaker_id": "1",
          "time": "20210905 01:48"
        },
        {
          "id": "MDRW2100040750.1.19",
          "form": "자격증 남발하던",
          "original_form": "자격증 남발하던",
          "speaker_id": "2",
          "time": "20210905 01:48"
        },
        {
          "id": "MDRW2100040750.1.20",
          "form": "그런 업체가 국가가 됐네요",
          "original_form": "그런 업체가 국가가 됐네요",
          "speaker_id": "2",
          "time": "20210905 01:48"
        },
        {
          "id": "MDRW2100040750.1.21",
          "form": "ㅋㅋㅋ 스케일이 커졌어요",
          "original_form": "ㅋㅋㅋ 스케일이 커졌어요",
          "speaker_id": "2",
          "time": "20210905 01:48"
        },
        {
          "id": "MDRW2100040750.1.22",
          "form": "저런 사람들이 좋아하는게 교육 수료증 이런 거 같아요",
          "original_form": "저런 사람들이 좋아하는게 교육 수료증 이런 거 같아요",
          "speaker_id": "1",
          "time": "20210905 01:48"
        },
        {
          "id": "MDRW2100040750.1.23",
          "form": "확인증ㅋㅋㅋㅋㅋ",
          "original_form": "확인증ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:49"
        },
        {
          "id": "MDRW2100040750.1.24",
          "form": "맨날 차별하지 말자 뭐 학력차별 하지 말자 이러면서",
          "original_form": "맨날 차별하지 말자 뭐 학력차별 하지 말자 이러면서",
          "speaker_id": "1",
          "time": "20210905 01:49"
        },
        {
          "id": "MDRW2100040750.1.25",
          "form": "무슨 증은 그렇게만 들고 좋아하더라고요",
          "original_form": "무슨 증은 그렇게만 들고 좋아하더라고요",
          "speaker_id": "1",
          "time": "20210905 01:49"
        },
        {
          "id": "MDRW2100040750.1.26",
          "form": "ㅋㅋㅋㅋㅋ 돈만 주면 단기간에 만들수 있는거",
          "original_form": "ㅋㅋㅋㅋㅋ 돈만 주면 단기간에 만들수 있는거",
