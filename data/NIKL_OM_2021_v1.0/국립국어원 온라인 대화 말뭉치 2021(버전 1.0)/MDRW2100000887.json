        {
          "id": "MDRW2100000887.1.2",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000887.1.3",
          "form": "나도 머리 해야 되는데",
          "original_form": "나도 머리 해야 되는데",
          "speaker_id": "1",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000887.1.4",
          "form": "구랭 엄마도 머리가 넘 부시시하다",
          "original_form": "구랭 엄마도 머리가 넘 부시시하다",
          "speaker_id": "2",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000887.1.5",
          "form": "그러게 머리가 너무 부시시해서 지저분하다",
          "original_form": "그러게 머리가 너무 부시시해서 지저분하다",
          "speaker_id": "1",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000887.1.6",
          "form": "깔끔하게 컷트 하자~",
          "original_form": "깔끔하게 컷트 하자~",
          "speaker_id": "2",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000887.1.7",
          "form": "언제 갈까",
          "original_form": "언제 갈까",
          "speaker_id": "1",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000887.1.8",
          "form": "토요일에 고고싱",
          "original_form": "토요일에 고고싱",
          "speaker_id": "2",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000887.1.9",
          "form": "우리 항상 가던 곳 가나",
          "original_form": "우리 항상 가던 곳 가나",
          "speaker_id": "1",
          "time": "20210819 20:24"
        },
        {
          "id": "MDRW2100000887.1.10",
          "form": "구랭 거기 가야지 ㅋㅋ",
          "original_form": "구랭 거기 가야지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:24"
        },
        {
          "id": "MDRW2100000887.1.11",
          "form": "나는 파마하고싶옹",
          "original_form": "나는 파마하고싶옹",
          "speaker_id": "2",
          "time": "20210819 20:24"
        },
        {
          "id": "MDRW2100000887.1.12",
          "form": "파마 하면은 관리가 힘든데",
          "original_form": "파마 하면은 관리가 힘든데",
          "speaker_id": "1",
          "time": "20210819 20:24"
        },
