      "utterance": [
        {
          "id": "MDRW2100023338.1.1",
          "form": "여행좋아하시나요",
          "original_form": "여행좋아하시나요",
          "speaker_id": "1",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100023338.1.2",
          "form": "네넹 여행 좋아해요",
          "original_form": "네넹 여행 좋아해요",
          "speaker_id": "2",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023338.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023338.1.4",
          "form": "네 완전좋아해요",
          "original_form": "네 완전좋아해요",
          "speaker_id": "1",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023338.1.5",
          "form": "가장 좋았던 여행지는 어디인가요",
          "original_form": "가장 좋았던 여행지는 어디인가요",
          "speaker_id": "1",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023338.1.6",
          "form": "저는 제주도랑 마카오가 제일 좋았던 것 같아요",
          "original_form": "저는 제주도랑 마카오가 제일 좋았던 것 같아요",
          "speaker_id": "2",
          "time": "20210830 00:28"
        },
        {
          "id": "MDRW2100023338.1.7",
          "form": "어디 좋아하세요?",
          "original_form": "어디 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 00:28"
        },
        {
          "id": "MDRW2100023338.1.8",
          "form": "저도 국내는 제주도를 젤좋아해요",
          "original_form": "저도 국내는 제주도를 젤좋아해요",
          "speaker_id": "1",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023338.1.9",
          "form": "바다를좋아해서",
          "original_form": "바다를좋아해서",
          "speaker_id": "1",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023338.1.10",
          "form": "제주도 여행할때 어디가 가장좋았어요?",
          "original_form": "제주도 여행할때 어디가 가장좋았어요?",
          "speaker_id": "1",
          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100023338.1.11",
          "form": "저는 함덕이랑 월정리바다욬ㅋㅋ 아쿠아리움도 좋았어요",
          "original_form": "저는 함덕이랑 월정리바다욬ㅋㅋ 아쿠아리움도 좋았어요",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023338.1.12",
          "form": "어디 제일 좋아하세요?",
          "original_form": "어디 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023338.1.13",
          "form": "저도 제주도모든바다랑 우도가 좋았어요ㅜ",
          "original_form": "저도 제주도모든바다랑 우도가 좋았어요ㅜ",
          "speaker_id": "1",
          "time": "20210830 00:42"
        },
        {
          "id": "MDRW2100023338.1.14",
          "form": "길이좁고 복잡했는데 바다가 너무이쁘고",
          "original_form": "길이좁고 복잡했는데 바다가 너무이쁘고",
          "speaker_id": "1",
          "time": "20210830 00:42"
        },
        {
          "id": "MDRW2100023338.1.15",
          "form": "우도에서보트탄게 넘재밌더라구여",
          "original_form": "우도에서보트탄게 넘재밌더라구여",
          "speaker_id": "1",
          "time": "20210830 00:42"
        },
        {
          "id": "MDRW2100023338.1.16",
          "form": "맞아요 우도도 넘 좋져",
          "original_form": "맞아요 우도도 넘 좋져",
          "speaker_id": "2",
          "time": "20210830 00:51"
        },
        {
          "id": "MDRW2100023338.1.17",
          "form": "얼른 코로나 끝났으면 좋겠어요ㅠㅠ",
          "original_form": "얼른 코로나 끝났으면 좋겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 00:51"
        },
        {
          "id": "MDRW2100023338.1.18",
          "form": "네ㅜㅜ코로나끝나면 제주도부터 갈래여ㅜ",
          "original_form": "네ㅜㅜ코로나끝나면 제주도부터 갈래여ㅜ",
          "speaker_id": "1",
          "time": "20210830 01:06"
        },
        {
          "id": "MDRW2100023338.1.19",
          "form": "제주도 음식은뭐드셨었어요",
          "original_form": "제주도 음식은뭐드셨었어요",
          "speaker_id": "1",
          "time": "20210830 01:06"
        },
        {
          "id": "MDRW2100023338.1.20",
          "form": "엄청 많이 먹었져....",
          "original_form": "엄청 많이 먹었져....",
          "speaker_id": "2",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100023338.1.21",
          "form": "제주도만 6번 정도 가서 뭐 회두 먹구 고기두 먹구 국수도 먹고 오돌뼈도 먹고...",
          "original_form": "제주도만 6번 정도 가서 뭐 회두 먹구 고기두 먹구 국수도 먹고 오돌뼈도 먹고...",
          "speaker_id": "2",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100023338.1.22",
          "form": "다 먹은 거 같아여..",
          "original_form": "다 먹은 거 같아여..",
          "speaker_id": "2",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100023338.1.23",
          "form": "뭐가 제일 맛있으셨어요?",
          "original_form": "뭐가 제일 맛있으셨어요?",
          "speaker_id": "2",
          "time": "20210830 01:17"
        },
        {
          "id": "MDRW2100023338.1.24",
          "form": "젤맛있었던건 아무래도 흑돼지 ㅎㅎㅎ 고기가 젤맛있었구요",
          "original_form": "젤맛있었던건 아무래도 흑돼지 ㅎㅎㅎ 고기가 젤맛있었구요",
          "speaker_id": "1",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100023338.1.25",
          "form": "소소하게기억나는건 귤하르방만쥬였나 그거 뜨끈할때먹으니 맛있더라구여",
          "original_form": "소소하게기억나는건 귤하르방만쥬였나 그거 뜨끈할때먹으니 맛있더라구여",
          "speaker_id": "1",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100023338.1.26",
          "form": "name2은 뭐가젤맛있었나요",
          "original_form": "&name2&은 뭐가젤맛있었나요",
          "speaker_id": "1",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100023338.1.27",
          "form": "전 빨간집 오돌뼈랑 등갈비요..",
          "original_form": "전 빨간집 오돌뼈랑 등갈비요..",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100023338.1.28",
          "form": "제주에만 있는 체인이래요",
          "original_form": "제주에만 있는 체인이래요",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100023338.1.29",
          "form": "아 빨간집들어봤어요ㅜ 못갔는데 담에는 거기가봐야겠네요",
          "original_form": "아 빨간집들어봤어요ㅜ 못갔는데 담에는 거기가봐야겠네요",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100023338.1.30",
          "form": "상세하게 후기알려주세요",
          "original_form": "상세하게 후기알려주세요",
          "speaker_id": "1",
          "time": "20210830 23:47"
        }
      ]
    }
