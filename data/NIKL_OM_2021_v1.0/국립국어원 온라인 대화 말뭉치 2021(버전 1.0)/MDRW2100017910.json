      "utterance": [
        {
          "id": "MDRW2100017910.1.1",
          "form": "저희집만 그런가요....",
          "original_form": "저희집만 그런가요....",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017910.1.2",
          "form": "무슨 먼지가 이렇게 빨리 쌓이는지?",
          "original_form": "무슨 먼지가 이렇게 빨리 쌓이는지?",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017910.1.3",
          "form": "먼지 진짜 자주 쌓여요",
          "original_form": "먼지 진짜 자주 쌓여요",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017910.1.4",
          "form": "그래서 화이트 인테리어가 유행인가봐요 *^^*",
          "original_form": "그래서 화이트 인테리어가 유행인가봐요 *^^*",
          "speaker_id": "2",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017910.1.5",
          "form": "본의 아니게 집에있던걸 가져오는 바람에",
          "original_form": "본의 아니게 집에있던걸 가져오는 바람에",
          "speaker_id": "1",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017910.1.6",
          "form": "밥솥이 검정, 전자렌지가 빨강인데",
          "original_form": "밥솥이 검정, 전자렌지가 빨강인데",
          "speaker_id": "1",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017910.1.7",
          "form": "하루만 지나도 먼지가 수북...",
          "original_form": "하루만 지나도 먼지가 수북...",
          "speaker_id": "1",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017910.1.8",
          "form": "앗 ㅋㅋㅋ",
          "original_form": "앗 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017910.1.9",
          "form": "먼지 청소는 보통 어떻게 하세요?",
          "original_form": "먼지 청소는 보통 어떻게 하세요?",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017910.1.10",
          "form": "물티슈 너무 써서",
          "original_form": "물티슈 너무 써서",
          "speaker_id": "1",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100017910.1.11",
          "form": "지난주에 무선청소기 샀어요~",
          "original_form": "지난주에 무선청소기 샀어요~",
          "speaker_id": "1",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100017910.1.12",
          "form": "완전 가성비 내리는건 고장나면 답없을거 같아서",
          "original_form": "완전 가성비 내리는건 고장나면 답없을거 같아서",
          "speaker_id": "1",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100017910.1.13",
          "form": "일렉트로룩스꺼로 ㅋㅋ",
          "original_form": "일렉트로룩스꺼로 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100017910.1.14",
          "form": "우리누나도 그거 선물받았던데~",
          "original_form": "우리누나도 그거 선물받았던데~",
          "speaker_id": "2",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100017910.1.15",
          "form": "제 직장도 그거 써요 ㅋㅋㅋㅋ 성능 좋더라구요",
          "original_form": "제 직장도 그거 써요 ㅋㅋㅋㅋ 성능 좋더라구요",
          "speaker_id": "2",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100017910.1.16",
          "form": "디자인도 나쁘지않고 좋아요ㅋㅋ",
          "original_form": "디자인도 나쁘지않고 좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100017910.1.17",
          "form": "쭈구리고 바닥 닦다가 덕분에 청소는 편해진ㅎ",
          "original_form": "쭈구리고 바닥 닦다가 덕분에 청소는 편해진ㅎ",
          "speaker_id": "1",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100017910.1.18",
          "form": "앗 ㅋㅋ 그거 오래 작동하기도 하잖아요",
          "original_form": "앗 ㅋㅋ 그거 오래 작동하기도 하잖아요",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100017910.1.19",
          "form": "그럼 걸1레질은 어떻게 하세요?",
          "original_form": "그럼 걸1레질은 어떻게 하세요?",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100017910.1.20",
          "form": "본가가 근처라 코드제로 청소기 가져와서",
          "original_form": "본가가 근처라 코드제로 청소기 가져와서",
          "speaker_id": "1",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100017910.1.21",
          "form": "물청소키트 썼는데 그만가져가라해서ㅋㅋㅋㅋㅋ",
          "original_form": "물청소키트 썼는데 그만가져가라해서ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100017910.1.22",
          "form": "본가찬스 ㅋㅋㅋㅋ",
          "original_form": "본가찬스 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100017910.1.23",
          "form": "부직포같은거 끼는거 있잖아요 마대 봉긴거...",
          "original_form": "부직포같은거 끼는거 있잖아요 마대 봉긴거...",
          "speaker_id": "1",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100017910.1.24",
          "form": "본가 가까운데 왜 동생이랑 독립했어요?",
          "original_form": "본가 가까운데 왜 동생이랑 독립했어요?",
          "speaker_id": "2",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100017910.1.25",
          "form": "그거써요ㅠ 성에 안차지만..",
          "original_form": "그거써요ㅠ 성에 안차지만..",
          "speaker_id": "1",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100017910.1.26",
          "form": "부직포말고 그 물에 젖어있는 것도 있더라구요 그거 진짜 좋아요",
          "original_form": "부직포말고 그 물에 젖어있는 것도 있더라구요 그거 진짜 좋아요",
          "speaker_id": "2",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100017910.1.27",
          "form": "가족이랑 하루종일 보니까 자꾸 싸우게돼서 반은 쫓겨나고 반은 독립하고ㅋㅋㅋ",
          "original_form": "가족이랑 하루종일 보니까 자꾸 싸우게돼서 반은 쫓겨나고 반은 독립하고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:21"
        }
      ]
