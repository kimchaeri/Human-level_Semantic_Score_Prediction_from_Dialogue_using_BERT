        },
        {
          "id": "MDRW2100032825.1.2",
          "form": "name1이 아이디니깐ㅎㅎ",
          "original_form": "&name1&이 아이디니깐ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100032825.1.3",
          "form": "모더나.. 그거맞고 숨졌다는데ㅠㅠ",
          "original_form": "모더나.. 그거맞고 숨졌다는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 00:28"
        },
        {
          "id": "MDRW2100032825.1.4",
          "form": "네에 3주밖에 안됐뉸데",
          "original_form": "네에 3주밖에 안됐뉸데",
          "speaker_id": "1",
          "time": "20210906 00:29"
        },
        {
          "id": "MDRW2100032825.1.5",
          "form": "아에 내려가신거에요?",
          "original_form": "아에 내려가신거에요?",
          "speaker_id": "2",
          "time": "20210906 00:29"
        },
        {
          "id": "MDRW2100032825.1.6",
          "form": "저 아직 화이자랑모더나중 안정해졌어요",
          "original_form": "저 아직 화이자랑모더나중 안정해졌어요",
          "speaker_id": "1",
          "time": "20210906 00:29"
        },
        {
          "id": "MDRW2100032825.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210906 00:29"
        },
        {
          "id": "MDRW2100032825.1.8",
          "form": "일단 내년 3월까지 살아보구 정하려구요",
          "original_form": "일단 내년 3월까지 살아보구 정하려구요",
          "speaker_id": "1",
          "time": "20210906 00:29"
        },
        {
          "id": "MDRW2100032825.1.9",
          "form": "3월까지는 뭔 이유가있으세요?",
          "original_form": "3월까지는 뭔 이유가있으세요?",
          "speaker_id": "2",
          "time": "20210906 00:30"
        },
        {
          "id": "MDRW2100032825.1.10",
          "form": "화이자가 낫ㅈ다고하더라구요ㅎ",
          "original_form": "화이자가 낫ㅈ다고하더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210906 00:30"
        },
        {
          "id": "MDRW2100032825.1.11",
          "form": "아 집 계약을 8월부터 내년 3월까지 해서요",
          "original_form": "아 집 계약을 8월부터 내년 3월까지 해서요",
          "speaker_id": "1",
          "time": "20210906 00:30"
        },
        {
          "id": "MDRW2100032825.1.12",
          "form": "화이자 기원해봅니다",
          "original_form": "화이자 기원해봅니다",
          "speaker_id": "1",
          "time": "20210906 00:31"
        },
        {
          "id": "MDRW2100032825.1.13",
          "form": "근데 모더나를 왜이렇게.많이 공급하는거지",
          "original_form": "근데 모더나를 왜이렇게.많이 공급하는거지",
          "speaker_id": "1",
          "time": "20210906 00:31"
        },
        {
          "id": "MDRW2100032825.1.14",
          "form": "그니깐요ㅠ",
          "original_form": "그니깐요ㅠ",
          "speaker_id": "2",
          "time": "20210906 00:31"
        },
        {
          "id": "MDRW2100032825.1.15",
          "form": "저도 모더나 10월에 맞아야해요ㅍ",
          "original_form": "저도 모더나 10월에 맞아야해요ㅍ",
          "speaker_id": "2",
          "time": "20210906 00:31"
        },
        {
          "id": "MDRW2100032825.1.16",
          "form": "아 모더나로 정해졌나뇨",
          "original_form": "아 모더나로 정해졌나뇨",
          "speaker_id": "1",
          "time": "20210906 00:31"
        },
        {
          "id": "MDRW2100032825.1.17",
          "form": "아니요ㅎ 정해주던데요ㅎㅎㅎ",
          "original_form": "아니요ㅎ 정해주던데요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 00:31"
        },
        {
          "id": "MDRW2100032825.1.18",
          "form": "나의 의지에 상관없이",
          "original_form": "나의 의지에 상관없이",
          "speaker_id": "2",
          "time": "20210906 00:32"
        },
        {
          "id": "MDRW2100032825.1.19",
          "form": "그럴군요",
          "original_form": "그럴군요",
          "speaker_id": "1",
          "time": "20210906 00:32"
        },
        {
          "id": "MDRW2100032825.1.20",
          "form": "어느지역에 30대여성분이 돌아가셨나요",
          "original_form": "어느지역에 30대여성분이 돌아가셨나요",
          "speaker_id": "1",
          "time": "20210906 00:32"
        },
        {
          "id": "MDRW2100032825.1.21",
          "form": "제주도 20대 대학생이 죽었는데",
          "original_form": "제주도 20대 대학생이 죽었는데",
          "speaker_id": "1",
          "time": "20210906 00:32"
        },
        {
