      "utterance": [
        {
          "id": "MDRW2100021339.1.1",
          "form": "저녁 뭐드실거예요?",
          "original_form": "저녁 뭐드실거예요?",
          "speaker_id": "1",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100021339.1.2",
          "form": "옥수수랑 복숭아 먹으려고요!",
          "original_form": "옥수수랑 복숭아 먹으려고요!",
          "speaker_id": "2",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100021339.1.3",
          "form": "옥수수 좋아하세요?",
          "original_form": "옥수수 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100021339.1.4",
          "form": "넵 맛있어용 ㅎ",
          "original_form": "넵 맛있어용 ㅎ",
          "speaker_id": "2",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100021339.1.5",
          "form": "초당옥수수",
          "original_form": "초당옥수수",
          "speaker_id": "1",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100021339.1.6",
          "form": "드셔보셨나요?",
          "original_form": "드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100021339.1.7",
          "form": "저는 보통 시장에서 사먹어서",
          "original_form": "저는 보통 시장에서 사먹어서",
          "speaker_id": "2",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100021339.1.8",
          "form": "딱 초당옥수수로 사먹은 기억은 없는것같아요",
          "original_form": "딱 초당옥수수로 사먹은 기억은 없는것같아요",
          "speaker_id": "2",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100021339.1.9",
          "form": "초당옥수수좋아하세요?",
          "original_form": "초당옥수수좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100021339.1.10",
          "form": "네 저는 좋아해요",
          "original_form": "네 저는 좋아해요",
          "speaker_id": "1",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100021339.1.11",
          "form": "간식으로 좋아서요",
          "original_form": "간식으로 좋아서요",
          "speaker_id": "1",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100021339.1.12",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100021339.1.13",
          "form": "끼니로 떼우기도 좋고",
          "original_form": "끼니로 떼우기도 좋고",
          "speaker_id": "2",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100021339.1.14",
          "form": "아삭아삭한 식감이 좋아요",
          "original_form": "아삭아삭한 식감이 좋아요",
          "speaker_id": "1",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100021339.1.15",
          "form": "간식삼아먹기도 좋죠",
          "original_form": "간식삼아먹기도 좋죠",
          "speaker_id": "2",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100021339.1.16",
          "form": "앗 초당옥수슈는 아삭아삭하군요",
          "original_form": "앗 초당옥수슈는 아삭아삭하군요",
          "speaker_id": "2",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100021339.1.17",
          "form": "네 달고 아삭거리는 식감이예요",
          "original_form": "네 달고 아삭거리는 식감이예요",
          "speaker_id": "1",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100021339.1.18",
          "form": "오 다음에 택배시켜봐야겠어요!",
          "original_form": "오 다음에 택배시켜봐야겠어요!",
          "speaker_id": "2",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100021339.1.19",
          "form": "공차 초당옥수수음료 드셔보셨어요?",
          "original_form": "공차 초당옥수수음료 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100021339.1.20",
          "form": "아니요 어때요?",
          "original_form": "아니요 어때요?",
          "speaker_id": "1",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100021339.1.21",
          "form": "앗 저도 안먹어봤어요",
          "original_form": "앗 저도 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100021339.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100021339.1.23",
          "form": "너무 궁금했는데 초당옥수수 좋아하신다고 해서 생각났어요",
          "original_form": "너무 궁금했는데 초당옥수수 좋아하신다고 해서 생각났어요",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100021339.1.24",
          "form": "앜ㅋㅋㅋㅋㅋ그러시구나",
          "original_form": "앜ㅋㅋㅋㅋㅋ그러시구나",
          "speaker_id": "1",
          "time": "20210828 17:56"
        },
