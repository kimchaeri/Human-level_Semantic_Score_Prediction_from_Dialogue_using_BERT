      "utterance": [
        {
          "id": "MDRW2100031102.1.1",
          "form": "다른 고양이를 데리고 올 생각은 없어?",
          "original_form": "다른 고양이를 데리고 올 생각은 없어?",
          "speaker_id": "1",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100031102.1.2",
          "form": "한마리 더 키우고 싶긴 한데",
          "original_form": "한마리 더 키우고 싶긴 한데",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031102.1.3",
          "form": "지금은 좀 힘들고",
          "original_form": "지금은 좀 힘들고",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031102.1.4",
          "form": "나중에 ㅋㅋ",
          "original_form": "나중에 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031102.1.5",
          "form": "나중에라니...",
          "original_form": "나중에라니...",
          "speaker_id": "1",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100031102.1.6",
          "form": "name3이 심심할것인데",
          "original_form": "&name3&이 심심할것인데",
          "speaker_id": "1",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100031102.1.7",
          "form": "마저..",
          "original_form": "마저..",
          "speaker_id": "2",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100031102.1.8",
          "form": "특히 집에 사람 다 나가고 혼자 있으면 할거없어서 맨날 잠만 자",
          "original_form": "특히 집에 사람 다 나가고 혼자 있으면 할거없어서 맨날 잠만 자",
          "speaker_id": "2",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100031102.1.9",
          "form": "원래 잠이 많은 친구인걸",
          "original_form": "원래 잠이 많은 친구인걸",
          "speaker_id": "1",
          "time": "20210904 16:53"
        },
        {
          "id": "MDRW2100031102.1.10",
          "form": "파충류는 어때?",
          "original_form": "파충류는 어때?",
          "speaker_id": "1",
          "time": "20210904 16:53"
        },
        {
          "id": "MDRW2100031102.1.11",
          "form": "엑",
          "original_form": "엑",
          "speaker_id": "2",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100031102.1.12",
          "form": "안돼여",
          "original_form": "안돼여",
          "speaker_id": "2",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100031102.1.13",
          "form": "name3이 장난감이 될것",
          "original_form": "&name3&이 장난감이 될것",
          "speaker_id": "2",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100031102.1.14",
          "form": "갸아아악",
          "original_form": "갸아아악",
          "speaker_id": "1",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100031102.1.15",
          "form": "먹는거 아닝데",
          "original_form": "먹는거 아닝데",
          "speaker_id": "1",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100031102.1.16",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100031102.1.17",
          "form": "먹진 않을거고",
          "original_form": "먹진 않을거고",
          "speaker_id": "2",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100031102.1.18",
          "form": "아마 굴리지 않을까..",
          "original_form": "아마 굴리지 않을까..",
          "speaker_id": "2",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100031102.1.19",
          "form": "꺄아아악 그게 더... 더..더해요",
          "original_form": "꺄아아악 그게 더... 더..더해요",
          "speaker_id": "1",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031102.1.20",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031102.1.21",
          "form": "그게 더 자닌하잖아",
          "original_form": "그게 더 자닌하잖아",
          "speaker_id": "1",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031102.1.22",
          "form": "아무튼 파충류는 안되는걸로",
          "original_form": "아무튼 파충류는 안되는걸로",
