      "utterance": [
        {
          "id": "MDRW2100030589.1.1",
          "form": "안녕하셍,",
          "original_form": "안녕하셍,",
          "speaker_id": "1",
          "time": "20210904 09:38"
        },
        {
          "id": "MDRW2100030589.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210904 09:38"
        },
        {
          "id": "MDRW2100030589.1.3",
          "form": "네 가을여행지 좋은데아세요?",
          "original_form": "네 가을여행지 좋은데아세요?",
          "speaker_id": "2",
          "time": "20210904 09:47"
        },
        {
          "id": "MDRW2100030589.1.4",
          "form": "가을은 캠핑가기좋은계절이죠",
          "original_form": "가을은 캠핑가기좋은계절이죠",
          "speaker_id": "1",
          "time": "20210904 09:48"
        },
        {
          "id": "MDRW2100030589.1.5",
          "form": "캠핑장비가 없어서 가기어려워요",
          "original_form": "캠핑장비가 없어서 가기어려워요",
          "speaker_id": "2",
          "time": "20210904 09:48"
        },
        {
          "id": "MDRW2100030589.1.6",
          "form": "아..그러시군요",
          "original_form": "아..그러시군요",
          "speaker_id": "1",
          "time": "20210904 09:49"
        },
        {
          "id": "MDRW2100030589.1.7",
          "form": "요즘은 글랭핑도 잘되있긴하던데",
          "original_form": "요즘은 글랭핑도 잘되있긴하던데",
          "speaker_id": "1",
          "time": "20210904 09:49"
        },
        {
          "id": "MDRW2100030589.1.8",
          "form": "글램핑장 어디 추천해주실곳잇나요?",
          "original_form": "글램핑장 어디 추천해주실곳잇나요?",
          "speaker_id": "2",
          "time": "20210905 00:05"
        },
        {
          "id": "MDRW2100030589.1.9",
          "form": "저도 캠핑을 다녀서 글램핑장은 잘모르네요ㅜㅜ",
          "original_form": "저도 캠핑을 다녀서 글램핑장은 잘모르네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 07:40"
        },
        {
          "id": "MDRW2100030589.1.10",
          "form": "캠핑은 장비가 끝도없죠? 그래도 경치보면 그고생이 사라지나요?",
          "original_form": "캠핑은 장비가 끝도없죠? 그래도 경치보면 그고생이 사라지나요?",
          "speaker_id": "2",
          "time": "20210906 16:20"
        },
        {
          "id": "MDRW2100030589.1.11",
          "form": "그렇죠..다치고 커피한잔하면 피로가 싹 풀려요",
          "original_form": "그렇죠..다치고 커피한잔하면 피로가 싹 풀려요",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100030589.1.12",
          "form": "맞아요 멋진 경치보면섷ㅎ",
          "original_form": "맞아요 멋진 경치보면섷ㅎ",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100030589.1.13",
          "form": "가을 단풍은 어디가야 제대로 볼까요?",
          "original_form": "가을 단풍은 어디가야 제대로 볼까요?",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100030589.1.14",
          "form": "잘몰라요..단풍구경안다녀봐서",
          "original_form": "잘몰라요..단풍구경안다녀봐서",
          "speaker_id": "1",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100030589.1.15",
          "form": "내장산?",
          "original_form": "내장산?",
          "speaker_id": "1",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100030589.1.16",
          "form": "오좋을거같아요",
          "original_form": "오좋을거같아요",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100030589.1.17",
          "form": "10월엔 강원도 가려고요 가을에 열심히다녀야죠",
          "original_form": "10월엔 강원도 가려고요 가을에 열심히다녀야죠",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100030589.1.18",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
