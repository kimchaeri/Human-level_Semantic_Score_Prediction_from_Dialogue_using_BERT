      "utterance": [
        {
          "id": "MDRW2100029185.1.1",
          "form": "넵 여름내내 수박 맛있게 먹었고",
          "original_form": "넵 여름내내 수박 맛있게 먹었고",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029185.1.2",
          "form": "복숭아도 너무 맛있더라구요",
          "original_form": "복숭아도 너무 맛있더라구요",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029185.1.3",
          "form": "오 수박시원하죠",
          "original_form": "오 수박시원하죠",
          "speaker_id": "1",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029185.1.4",
          "form": "올해 복숭아가 맛있습니디",
          "original_form": "올해 복숭아가 맛있습니디",
          "speaker_id": "1",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029185.1.5",
          "form": "요새 그게 유행이었거든요 복숭아 속 비워서",
          "original_form": "요새 그게 유행이었거든요 복숭아 속 비워서",
          "speaker_id": "2",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029185.1.6",
          "form": "안에 요구르트 넣어먹는거",
          "original_form": "안에 요구르트 넣어먹는거",
          "speaker_id": "2",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029185.1.7",
          "form": "그게 무슨요리죠",
          "original_form": "그게 무슨요리죠",
          "speaker_id": "1",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029185.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029185.1.9",
          "form": "새콤달콤하겠어요",
          "original_form": "새콤달콤하겠어요",
          "speaker_id": "1",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029185.1.10",
          "form": "그릭복숭아?라고 부르던데 맛있었어요",
          "original_form": "그릭복숭아?라고 부르던데 맛있었어요",
          "speaker_id": "2",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029185.1.11",
          "form": "수박은 화채로 먹어도 좋구요",
          "original_form": "수박은 화채로 먹어도 좋구요",
          "speaker_id": "1",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029185.1.12",
          "form": "참외도 드셨나요",
          "original_form": "참외도 드셨나요",
          "speaker_id": "1",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029185.1.13",
          "form": "아니면 크림치즈 채워넣는 사람들도 있더라구요",
          "original_form": "아니면 크림치즈 채워넣는 사람들도 있더라구요",
          "speaker_id": "2",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029185.1.14",
          "form": "수박화채좋죠",
          "original_form": "수박화채좋죠",
          "speaker_id": "2",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029185.1.15",
          "form": "참외도 맛있죠!",
          "original_form": "참외도 맛있죠!",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029185.1.16",
          "form": "전 참외속이 좋은데",
          "original_form": "전 참외속이 좋은데",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029185.1.17",
          "form": "복숭아와 크림치즈 좋은 생각이에요",
          "original_form": "복숭아와 크림치즈 좋은 생각이에요",
          "speaker_id": "1",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029185.1.18",
          "form": "저희 집은 배탈난다고 참외속을 비워먹더라구요",
          "original_form": "저희 집은 배탈난다고 참외속을 비워먹더라구요",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029185.1.19",
          "form": "드실줄 아시네요^^",
          "original_form": "드실줄 아시네요^^",
          "speaker_id": "1",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029185.1.20",
          "form": "맞아요 속이 제일 맛있어요",
          "original_form": "맞아요 속이 제일 맛있어요",
          "speaker_id": "1",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029185.1.21",
          "form": "달콤해요",
          "original_form": "달콤해요",
          "speaker_id": "1",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029185.1.22",
          "form": "복숭아는 딱딱한걸 좋아해요",
          "original_form": "복숭아는 딱딱한걸 좋아해요",
          "speaker_id": "1",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029185.1.23",
          "form": "수박은 씨없는것도 있더라구요",
          "original_form": "수박은 씨없는것도 있더라구요",
          "speaker_id": "1",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029185.1.24",
          "form": "딱복 좋죠 저도 물복보다 딱복이 좋아요! 올해는 물복만 먹었지만",
          "original_form": "딱복 좋죠 저도 물복보다 딱복이 좋아요! 올해는 물복만 먹었지만",
          "speaker_id": "2",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029185.1.25",
          "form": "씨없는 수박 너무 먹고싶어요ㅋㅋㅋㅋㅋ",
          "original_form": "씨없는 수박 너무 먹고싶어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029185.1.26",
          "form": "네 씨앗은 안먹어요",
          "original_form": "네 씨앗은 안먹어요",
          "speaker_id": "1",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029185.1.27",
          "form": "수박 맛있긴한데 씨 골라내는게 너무 번거로워서",
          "original_form": "수박 맛있긴한데 씨 골라내는게 너무 번거로워서",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029185.1.28",
          "form": "자두도 맛있죠",
          "original_form": "자두도 맛있죠",
          "speaker_id": "1",
          "time": "20210903 00:14"
        }
      ]
    }
