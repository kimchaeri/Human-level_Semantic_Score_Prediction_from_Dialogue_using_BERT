      "utterance": [
        {
          "id": "MDRW2100042844.1.1",
          "form": "오늘도 비오고 이렇게 비가 오니까 돌아다니기 싫지만",
          "original_form": "오늘도 비오고 이렇게 비가 오니까 돌아다니기 싫지만",
          "speaker_id": "1",
          "time": "20210907 09:56"
        },
        {
          "id": "MDRW2100042844.1.2",
          "form": "날씨 좋으면 여행가고 싶은 곳 있나요?",
          "original_form": "날씨 좋으면 여행가고 싶은 곳 있나요?",
          "speaker_id": "1",
          "time": "20210907 09:56"
        },
        {
          "id": "MDRW2100042844.1.3",
          "form": "저는 광주에가고싶어요!",
          "original_form": "저는 광주에가고싶어요!",
          "speaker_id": "2",
          "time": "20210907 17:08"
        },
        {
          "id": "MDRW2100042844.1.4",
          "form": "전라도 광주요",
          "original_form": "전라도 광주요",
          "speaker_id": "1",
          "time": "20210907 17:13"
        },
        {
          "id": "MDRW2100042844.1.5",
          "form": "광주에서 뭘 하고 싶나요?",
          "original_form": "광주에서 뭘 하고 싶나요?",
          "speaker_id": "1",
          "time": "20210907 17:13"
        },
        {
          "id": "MDRW2100042844.1.6",
          "form": "제가아는애가 사는데 얼굴은 한번도 안봤는데 걔 볼려구여",
          "original_form": "제가아는애가 사는데 얼굴은 한번도 안봤는데 걔 볼려구여",
          "speaker_id": "2",
          "time": "20210907 17:14"
        },
        {
          "id": "MDRW2100042844.1.7",
          "form": "나중에 기회되믄 광주구경시켜달라구할려구여",
          "original_form": "나중에 기회되믄 광주구경시켜달라구할려구여",
          "speaker_id": "2",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100042844.1.8",
          "form": "아~ 친구보러 가는거군요",
          "original_form": "아~ 친구보러 가는거군요",
          "speaker_id": "1",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100042844.1.9",
          "form": "좋죠 친구도 만나고 광주 구경도 하고",
          "original_form": "좋죠 친구도 만나고 광주 구경도 하고",
          "speaker_id": "1",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100042844.1.10",
          "form": "10년 연하 남자애에용",
          "original_form": "10년 연하 남자애에용",
          "speaker_id": "2",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100042844.1.11",
          "form": "더 좋네요~",
          "original_form": "더 좋네요~",
          "speaker_id": "1",
          "time": "20210907 17:17"
        },
        {
          "id": "MDRW2100042844.1.12",
          "form": "무늬만그래요ㅋㅋㅋ",
          "original_form": "무늬만그래요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100042844.1.13",
          "form": "일단 광주놀러가면 구경시켜준다고하니깐",
          "original_form": "일단 광주놀러가면 구경시켜준다고하니깐",
          "speaker_id": "2",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100042844.1.14",
          "form": "근데너무멀어서ㅜㅜ",
          "original_form": "근데너무멀어서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100042844.1.15",
          "form": "서로둘다 차가 없어서",
          "original_form": "서로둘다 차가 없어서",
          "speaker_id": "2",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100042844.1.16",
          "form": "시외버스타고가야해여",
          "original_form": "시외버스타고가야해여",
          "speaker_id": "2",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100042844.1.17",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100042844.1.18",
          "form": "그참에 여행기분 내는 거죠",
          "original_form": "그참에 여행기분 내는 거죠",
          "speaker_id": "1",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100042844.1.19",
          "form": "그니깐요ㅜㅜ거기가서 확진자만 안부딛히면되오ㅡ",
          "original_form": "그니깐요ㅜㅜ거기가서 확진자만 안부딛히면되오ㅡ",
          "speaker_id": "2",
          "time": "20210907 17:25"
        },
        {
          "id": "MDRW2100042844.1.20",
          "form": "백신 2차까지 맞고 가는건가요?",
          "original_form": "백신 2차까지 맞고 가는건가요?",
          "speaker_id": "1",
          "time": "20210907 17:26"
        },
        {
          "id": "MDRW2100042844.1.21",
          "form": "1차까지만 맞앟어요",
          "original_form": "1차까지만 맞앟어요",
          "speaker_id": "2",
          "time": "20210907 17:26"
        },
        {
          "id": "MDRW2100042844.1.22",
          "form": "연하남은요?",
          "original_form": "연하남은요?",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100042844.1.23",
          "form": "걔는 아직 안맞았어요",
          "original_form": "걔는 아직 안맞았어요",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100042844.1.24",
          "form": "20대초반이라서",
          "original_form": "20대초반이라서",
          "speaker_id": "2",
          "time": "20210907 17:28"
        }
      ]
    }
