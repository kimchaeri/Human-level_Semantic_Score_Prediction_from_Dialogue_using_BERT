      "utterance": [
        {
          "id": "MDRW2100022785.1.1",
          "form": "케어해야 하잖아요",
          "original_form": "케어해야 하잖아요",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022785.1.2",
          "form": "그러네요",
          "original_form": "그러네요",
          "speaker_id": "2",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022785.1.3",
          "form": "룸메랑 살 생각은요",
          "original_form": "룸메랑 살 생각은요",
          "speaker_id": "2",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022785.1.4",
          "form": "코로나때문에",
          "original_form": "코로나때문에",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022785.1.5",
          "form": "타인과는 ㅎ",
          "original_form": "타인과는 ㅎ",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022785.1.6",
          "form": "가족도 근데",
          "original_form": "가족도 근데",
          "speaker_id": "2",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022785.1.7",
          "form": "어찌보면 타인이잖아요",
          "original_form": "어찌보면 타인이잖아요",
          "speaker_id": "2",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022785.1.8",
          "form": "태어나서 계소 봤으니 그정도 면 ㅋㅋ",
          "original_form": "태어나서 계소 봤으니 그정도 면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022785.1.9",
          "form": "그럼 혹시 비혼주의세요?",
          "original_form": "그럼 혹시 비혼주의세요?",
          "speaker_id": "2",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022785.1.10",
          "form": "비혼은 아니에요",
          "original_form": "비혼은 아니에요",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022785.1.11",
          "form": "타인은 좀 그렇다며요",
          "original_form": "타인은 좀 그렇다며요",
          "speaker_id": "2",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100022785.1.12",
          "form": "사랑하면 타인이 아니져",
          "original_form": "사랑하면 타인이 아니져",
          "speaker_id": "1",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100022785.1.13",
          "form": "또다른 나?? ㅋㅋ",
          "original_form": "또다른 나?? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100022785.1.14",
          "form": "그럼 사랑하는 사람이랑 룸메하세요",
          "original_form": "그럼 사랑하는 사람이랑 룸메하세요",
          "speaker_id": "2",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100022785.1.15",
          "form": "네 아직 못구해서 가족이랑 있어요",
          "original_form": "네 아직 못구해서 가족이랑 있어요",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022785.1.16",
          "form": "빠른시일내에 생기길 바라요",
          "original_form": "빠른시일내에 생기길 바라요",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
