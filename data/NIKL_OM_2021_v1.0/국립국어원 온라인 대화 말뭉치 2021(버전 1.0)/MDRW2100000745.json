      "utterance": [
        {
          "id": "MDRW2100000745.1.1",
          "form": "저는 일 그만두고",
          "original_form": "저는 일 그만두고",
          "speaker_id": "1",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000745.1.2",
          "form": "직장구하구있어요",
          "original_form": "직장구하구있어요",
          "speaker_id": "1",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000745.1.3",
          "form": "직종 어떤쪽인지 여쭤봐도될까요??",
          "original_form": "직종 어떤쪽인지 여쭤봐도될까요??",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000745.1.4",
          "form": "저는 간호사였어요",
          "original_form": "저는 간호사였어요",
          "speaker_id": "1",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000745.1.5",
          "form": "교대근무가 넘 힘들어서.. 5년일하구 그만뒀어요",
          "original_form": "교대근무가 넘 힘들어서.. 5년일하구 그만뒀어요",
          "speaker_id": "1",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100000745.1.6",
          "form": "아아ㅠㅠㅠ.. 친척언니들이 간호사인데.... 그렇다고 하더라구요ㅠㅠㅠ",
          "original_form": "아아ㅠㅠㅠ.. 친척언니들이 간호사인데.... 그렇다고 하더라구요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100000745.1.7",
          "form": "간호사는 계속 하시면서 다른 직장 찾으시는거에요?? 아니면 아예 다른계열로하시나요??",
          "original_form": "간호사는 계속 하시면서 다른 직장 찾으시는거에요?? 아니면 아예 다른계열로하시나요??",
          "speaker_id": "2",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100000745.1.8",
          "form": "다른계열로 알아보고있어요",
          "original_form": "다른계열로 알아보고있어요",
          "speaker_id": "1",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100000745.1.9",
          "form": "아픈사람 상대하는게 쉽지가 않아서요",
          "original_form": "아픈사람 상대하는게 쉽지가 않아서요",
          "speaker_id": "1",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100000745.1.10",
          "form": "아아그쵸ㅠㅠㅠ 저는 사회복지사인데",
          "original_form": "아아그쵸ㅠㅠㅠ 저는 사회복지사인데",
          "speaker_id": "2",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100000745.1.11",
          "form": "노인쪽이라...",
          "original_form": "노인쪽이라...",
          "speaker_id": "2",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100000745.1.12",
          "form": "아이구...힘드시겠어요",
          "original_form": "아이구...힘드시겠어요",
          "speaker_id": "1",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100000745.1.13",
          "form": "사복쪽도 쉽지않잖아요!",
          "original_form": "사복쪽도 쉽지않잖아요!",
          "speaker_id": "1",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100000745.1.14",
          "form": "나이가 있으시니까 아프신분들이 많아서",
          "original_form": "나이가 있으시니까 아프신분들이 많아서",
          "speaker_id": "2",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100000745.1.15",
          "form": "안타까운상황들이 많은것같아요 ...ㅠ",
          "original_form": "안타까운상황들이 많은것같아요 ...ㅠ",
          "speaker_id": "2",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100000745.1.16",
          "form": "그쵸ㅠ",
          "original_form": "그쵸ㅠ",
          "speaker_id": "1",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000745.1.17",
          "form": "근데 간호사시면 진짜 공부량 엄청나셨겠어요 ㅠㅠㅠ",
          "original_form": "근데 간호사시면 진짜 공부량 엄청나셨겠어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000745.1.18",
