      "utterance": [
        {
          "id": "MDRW2100008840.1.1",
          "form": "라켓소년단 끝나고 월요일 재미 사라짐",
          "original_form": "라켓소년단 끝나고 월요일 재미 사라짐",
          "speaker_id": "1",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008840.1.2",
          "form": "나 요새 넷플 티빙 웨이브 다 결제해서 본다",
          "original_form": "나 요새 넷플 티빙 웨이브 다 결제해서 본다",
          "speaker_id": "2",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008840.1.3",
          "form": "티빙이랑 웨이브 합쳐야돼",
          "original_form": "티빙이랑 웨이브 합쳐야돼",
          "speaker_id": "2",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008840.1.4",
          "form": "와 너무 무리하는거아님?",
          "original_form": "와 너무 무리하는거아님?",
          "speaker_id": "1",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008840.1.5",
          "form": "그거 한달에 얼마나가는데?",
          "original_form": "그거 한달에 얼마나가는데?",
          "speaker_id": "1",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008840.1.6",
          "form": "나는 요새 보능거 너는나의봄 경찰수업 슬의생 펜하",
          "original_form": "나는 요새 보능거 너는나의봄 경찰수업 슬의생 펜하",
          "speaker_id": "2",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008840.1.7",
          "form": "라켓끝나고 슬의만봐",
          "original_form": "라켓끝나고 슬의만봐",
          "speaker_id": "1",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008840.1.8",
          "form": "다 노잼",
          "original_form": "다 노잼",
          "speaker_id": "1",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008840.1.9",
          "form": "경찰수업 약간 드림하이 경찰대버전임",
          "original_form": "경찰수업 약간 드림하이 경찰대버전임",
          "speaker_id": "2",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008840.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:06"
        },
        {
          "id": "MDRW2100008840.1.11",
          "form": "아ㅋㅋㅋㅋㅋ그래 그렇다더라 비주얼 보는맛이라던대",
          "original_form": "아ㅋㅋㅋㅋㅋ그래 그렇다더라 비주얼 보는맛이라던대",
          "speaker_id": "1",
          "time": "20210822 19:06"
        },
        {
          "id": "MDRW2100008840.1.12",
          "form": "일단 나중에 볼까싶긴해",
          "original_form": "일단 나중에 볼까싶긴해",
          "speaker_id": "1",
          "time": "20210822 19:06"
        },
        {
          "id": "MDRW2100008840.1.13",
          "form": "크리스탈 진영 이면 비주얼 굿",
          "original_form": "크리스탈 진영 이면 비주얼 굿",
          "speaker_id": "1",
          "time": "20210822 19:06"
        },
        {
          "id": "MDRW2100008840.1.14",
          "form": "그 김선호 신민아 주연 드라마 시작했나",
          "original_form": "그 김선호 신민아 주연 드라마 시작했나",
          "speaker_id": "1",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100008840.1.15",
          "form": "크리스탈 진영 비주얼 굿굿 또래케미 굿굿",
          "original_form": "크리스탈 진영 비주얼 굿굿 또래케미 굿굿",
          "speaker_id": "2",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100008840.1.16",
          "form": "그거 담주부터인가 그럴걸?",
          "original_form": "그거 담주부터인가 그럴걸?",
          "speaker_id": "2",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100008840.1.17",
          "form": "갯마을 차차차 기대중ㅋㅋㅋ",
          "original_form": "갯마을 차차차 기대중ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100008840.1.18",
          "form": "나는 유미의세포들도 기대중이야",
          "original_form": "나는 유미의세포들도 기대중이야",
          "speaker_id": "2",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100008840.1.19",
          "form": "사촌오빠가하는거라 보려는데 시간대 너무늦어....",
          "original_form": "사촌오빠가하는거라 보려는데 시간대 너무늦어....",
          "speaker_id": "2",
          "time": "20210822 19:08"
        },
        {
          "id": "MDRW2100008840.1.20",
          "form": "아그래 갯마을차차차 이름이 좀 촌스러서ㅋㅋㅋ",
          "original_form": "아그래 갯마을차차차 이름이 좀 촌스러서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008840.1.21",
          "form": "유미의세포들 고증 쩔더라",
          "original_form": "유미의세포들 고증 쩔더라",
          "speaker_id": "1",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008840.1.22",
          "form": "유미의세포들을 사촌오빠가 하시는겨?",
          "original_form": "유미의세포들을 사촌오빠가 하시는겨?",
          "speaker_id": "1",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008840.1.23",
          "form": "굵직한거 엄청 맡으시네",
          "original_form": "굵직한거 엄청 맡으시네",
          "speaker_id": "1",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008840.1.24",
          "form": "웅ㅋㅋㅋ근데 아직 짬바있는 피디는아니어가지구ㅋㅋㅋ",
          "original_form": "웅ㅋㅋㅋ근데 아직 짬바있는 피디는아니어가지구ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008840.1.25",
          "form": "근데 신민아랑 김선호 잘어울림",
          "original_form": "근데 신민아랑 김선호 잘어울림",
          "speaker_id": "2",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008840.1.26",
          "form": "둘이 케미 기대중",
          "original_form": "둘이 케미 기대중",
          "speaker_id": "1",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008840.1.27",
          "form": "토끼커플 아니냐고",
          "original_form": "토끼커플 아니냐고",
          "speaker_id": "1",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008840.1.28",
          "form": "그니까 둘다 보조개있어서",
          "original_form": "그니까 둘다 보조개있어서",
          "speaker_id": "2",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008840.1.29",
          "form": "잘어울렼ㅋㅋ",
          "original_form": "잘어울렼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008840.1.30",
          "form": "너는나의봄이 김동욱 나오는거였구나",
          "original_form": "너는나의봄이 김동욱 나오는거였구나",
          "speaker_id": "1",
          "time": "20210822 19:51"
        },
        {
          "id": "MDRW2100008840.1.31",
          "form": "너 이거본다고? 어때?",
          "original_form": "너 이거본다고? 어때?",
          "speaker_id": "1",
          "time": "20210822 19:51"
        },
        {
          "id": "MDRW2100008840.1.32",
          "form": "존잼인데 시청률 안나옴ㅠㅠ",
          "original_form": "존잼인데 시청률 안나옴ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 19:52"
        },
        {
          "id": "MDRW2100008840.1.33",
