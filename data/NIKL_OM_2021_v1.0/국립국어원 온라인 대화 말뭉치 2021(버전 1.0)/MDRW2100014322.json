      "utterance": [
        {
          "id": "MDRW2100014322.1.1",
          "form": "네 그거랑 닥터프렌즈요",
          "original_form": "네 그거랑 닥터프렌즈요",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014322.1.2",
          "form": "아 닥터프렌즈도 있나요?ㅋㅋ",
          "original_form": "아 닥터프렌즈도 있나요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014322.1.3",
          "form": "남자 셋 나오는 채널인가요 혹시?",
          "original_form": "남자 셋 나오는 채널인가요 혹시?",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014322.1.4",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014322.1.5",
          "form": "ㅋㅋ아 봤던거 같아요ㅎㅎ",
          "original_form": "ㅋㅋ아 봤던거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014322.1.6",
          "form": "그채널도 재미있어요ㅋㅋ",
          "original_form": "그채널도 재미있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014322.1.7",
          "form": "의사들이 리뷰해주니까 몰랐던 부분들이 보여요ㅋㅋㅋ",
          "original_form": "의사들이 리뷰해주니까 몰랐던 부분들이 보여요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014322.1.8",
          "form": "맞아여 그래서 더재미있더라고요",
          "original_form": "맞아여 그래서 더재미있더라고요",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014322.1.9",
          "form": "ㅋㅋ드라마 보고 리뷰까지 보는걸로 또 한주 기다려요",
          "original_form": "ㅋㅋ드라마 보고 리뷰까지 보는걸로 또 한주 기다려요",
          "speaker_id": "1",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014322.1.10",
          "form": "저랑 똑같으시네요ㅋㅋㅋ",
          "original_form": "저랑 똑같으시네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014322.1.11",
          "form": "ㅋㅋㅋ그러게요",
          "original_form": "ㅋㅋㅋ그러게요",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014322.1.12",
          "form": "유투브도 자주 보는 편이세요?ㅎㅎ",
          "original_form": "유투브도 자주 보는 편이세요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014322.1.13",
          "form": "요즘은 꽤봐요",
          "original_form": "요즘은 꽤봐요",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014322.1.14",
          "form": "혹시 브끄뚱아세여?",
          "original_form": "혹시 브끄뚱아세여?",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014322.1.15",
          "form": "처음 들어요ㅋㅋ그 유투버는 무슨 주제에요?",
          "original_form": "처음 들어요ㅋㅋ그 유투버는 무슨 주제에요?",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014322.1.16",
          "form": "문세윤 가수데뷔요 \n부케가 부끄뚱이요ㅋㅋ",
          "original_form": "문세윤 가수데뷔요 \n부케가 부끄뚱이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014322.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ아아 웃길거 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아아 웃길거 같아요",
