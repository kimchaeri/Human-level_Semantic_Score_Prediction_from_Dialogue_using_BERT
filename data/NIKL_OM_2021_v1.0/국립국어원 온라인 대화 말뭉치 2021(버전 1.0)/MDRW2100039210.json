      "utterance": [
        {
          "id": "MDRW2100039210.1.1",
          "form": "ㅇㅇ.....온갖 징그러운거나",
          "original_form": "ㅇㅇ.....온갖 징그러운거나",
          "speaker_id": "2",
          "time": "20210903 13:54"
        },
        {
          "id": "MDRW2100039210.1.2",
          "form": "개발하고있고",
          "original_form": "개발하고있고",
          "speaker_id": "2",
          "time": "20210903 13:54"
        },
        {
          "id": "MDRW2100039210.1.3",
          "form": "그 해파리좀 저리 치워....",
          "original_form": "그 해파리좀 저리 치워....",
          "speaker_id": "2",
          "time": "20210903 13:54"
        },
        {
          "id": "MDRW2100039210.1.4",
          "form": "스토리 한 절반만 밀어봐",
          "original_form": "스토리 한 절반만 밀어봐",
          "speaker_id": "1",
          "time": "20210903 14:01"
        },
        {
          "id": "MDRW2100039210.1.5",
          "form": "이따가 오후쯤에",
          "original_form": "이따가 오후쯤에",
          "speaker_id": "2",
          "time": "20210903 14:02"
        },
        {
          "id": "MDRW2100039210.1.6",
          "form": "함 밀어볼게",
          "original_form": "함 밀어볼게",
          "speaker_id": "2",
          "time": "20210903 14:02"
        },
        {
          "id": "MDRW2100039210.1.7",
          "form": "아 좀 미루려고했는데",
          "original_form": "아 좀 미루려고했는데",
          "speaker_id": "2",
          "time": "20210903 14:02"
        },
        {
          "id": "MDRW2100039210.1.8",
          "form": "이자식들이 스토리를 조져놨어",
          "original_form": "이자식들이 스토리를 조져놨어",
          "speaker_id": "2",
          "time": "20210903 14:02"
        },
        {
          "id": "MDRW2100039210.1.9",
          "form": "ㅇㅇ 이따 함 밀어봐",
          "original_form": "ㅇㅇ 이따 함 밀어봐",
          "speaker_id": "1",
          "time": "20210903 14:04"
        },
        {
          "id": "MDRW2100039210.1.10",
          "form": "내가 막혓던 부분이",
          "original_form": "내가 막혓던 부분이",
          "speaker_id": "1",
          "time": "20210903 14:04"
        },
        {
          "id": "MDRW2100039210.1.11",
          "form": "반 라이덴 특훈작전 인데",
          "original_form": "반 라이덴 특훈작전 인데",
          "speaker_id": "1",
          "time": "20210903 14:04"
        },
        {
          "id": "MDRW2100039210.1.12",
          "form": "이쯤되면 슬슬 엥? 이게 뭔소리야?",
          "original_form": "이쯤되면 슬슬 엥? 이게 뭔소리야?",
          "speaker_id": "1",
          "time": "20210903 14:04"
        },
        {
          "id": "MDRW2100039210.1.13",
          "form": "싶은 부분이 생김 ㅋㅋㅋㅋㅋ",
          "original_form": "싶은 부분이 생김 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:04"
        },
        {
          "id": "MDRW2100039210.1.14",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039210.1.15",
          "form": "아니 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039210.1.16",
          "form": "??????",
          "original_form": "??????",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039210.1.17",
          "form": "ㅇ ㅏ ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇ ㅏ ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039210.1.18",
          "form": "어이없어",
          "original_form": "어이없어",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039210.1.19",
          "form": "섯섯 빨리 멘스 밀어주면 안돼...?(급",
          "original_form": "섯섯 빨리 멘스 밀어주면 안돼...?(급",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039210.1.20",
          "form": "지금 이 심정을 같이 나눌 사람이 필요해....",
          "original_form": "지금 이 심정을 같이 나눌 사람이 필요해....",
          "speaker_id": "1",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039210.1.21",
          "form": "대체 무슨일이길래......",
          "original_form": "대체 무슨일이길래......",
          "speaker_id": "2",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100039210.1.22",
          "form": "그럼 진짜 밥만 먹고 밀게",
          "original_form": "그럼 진짜 밥만 먹고 밀게",
          "speaker_id": "2",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100039210.1.23",
          "form": "ㅇㅇ...",
          "original_form": "ㅇㅇ...",
          "speaker_id": "1",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039210.1.24",
          "form": "ㅇ ㅏ 이거 중요한 애니메이션마다 끊기거나 멈추는거 짜증나네 진짜",
          "original_form": "ㅇ ㅏ 이거 중요한 애니메이션마다 끊기거나 멈추는거 짜증나네 진짜",
          "speaker_id": "1",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039210.1.25",
          "form": "이거 멘스",
          "original_form": "이거 멘스",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039210.1.26",
          "form": "진짜 앞뒤 안맞지",
          "original_form": "진짜 앞뒤 안맞지",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039210.1.27",
          "form": "아직 거기까진 안보긴 했어",
          "original_form": "아직 거기까진 안보긴 했어",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039210.1.28",
          "form": "중간에 밥하고싶어서",
          "original_form": "중간에 밥하고싶어서",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039210.1.29",
          "form": "잠깐 경책산장 들려서",
          "original_form": "잠깐 경책산장 들려서",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039210.1.30",
          "form": "아니 걍 설정 자체가",
          "original_form": "아니 걍 설정 자체가",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039210.1.31",
          "form": "파밍하고오느라....",
          "original_form": "파밍하고오느라....",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039210.1.32",
          "form": "아 저거?",
          "original_form": "아 저거?",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039210.1.33",
          "form": "저거 설정구멍이야??",
          "original_form": "저거 설정구멍이야??",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039210.1.34",
          "form": "그런거같음",
          "original_form": "그런거같음",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039210.1.35",
          "form": ";;;",
          "original_form": ";;;",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039210.1.36",
          "form": "아 복선이 아니야?",
          "original_form": "아 복선이 아니야?",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039210.1.37",
          "form": "쟤 텟페인가",
          "original_form": "쟤 텟페인가",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039210.1.38",
          "form": "쟤도??",
          "original_form": "쟤도??",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039210.1.39",
          "form": "설정 구멍 장난아님",
          "original_form": "설정 구멍 장난아님",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100039210.1.40",
          "form": "아니야????",
          "original_form": "아니야????",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100039210.1.41",
          "form": "저렇게 수상하게 등장해서",
          "original_form": "저렇게 수상하게 등장해서",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
