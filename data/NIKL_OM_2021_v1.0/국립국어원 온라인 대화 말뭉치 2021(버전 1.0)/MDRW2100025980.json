      "utterance": [
        {
          "id": "MDRW2100025980.1.1",
          "form": "저녁 드셧어요?",
          "original_form": "저녁 드셧어요?",
          "speaker_id": "1",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025980.1.2",
          "form": "먹어야죠",
          "original_form": "먹어야죠",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025980.1.3",
          "form": "지금 커피랑 케이크 마시고 놀고있었어요",
          "original_form": "지금 커피랑 케이크 마시고 놀고있었어요",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025980.1.4",
          "form": "이미 칼로리는 다 채웠네요 ㅋㅋ",
          "original_form": "이미 칼로리는 다 채웠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025980.1.5",
          "form": "ㅋㅋㅋㅋㅋ무슨 케이크 드셧어요?",
          "original_form": "ㅋㅋㅋㅋㅋ무슨 케이크 드셧어요?",
          "speaker_id": "1",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025980.1.6",
          "form": "갸또오쇼콜라인가",
          "original_form": "갸또오쇼콜라인가",
          "speaker_id": "2",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100025980.1.7",
          "form": "초코케이크요",
          "original_form": "초코케이크요",
          "speaker_id": "2",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100025980.1.8",
          "form": "좋아하는 디저트 있으신가여",
          "original_form": "좋아하는 디저트 있으신가여",
          "speaker_id": "2",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025980.1.9",
          "form": "디저트는 다 사랑이죠",
          "original_form": "디저트는 다 사랑이죠",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025980.1.10",
          "form": "케이크는 다 좋아하고 타르트도 좋아해요",
          "original_form": "케이크는 다 좋아하고 타르트도 좋아해요",
          "speaker_id": "1",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100025980.1.11",
          "form": "전 요즘 케이크보다 파운드케이크에 더 관심이 많았어요",
          "original_form": "전 요즘 케이크보다 파운드케이크에 더 관심이 많았어요",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100025980.1.12",
          "form": "올만에 더 전에 땡겨서",
          "original_form": "올만에 더 전에 땡겨서",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100025980.1.13",
          "form": "더 단거",
          "original_form": "더 단거",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100025980.1.14",
          "form": "파운드케이크도 맛잇죠",
          "original_form": "파운드케이크도 맛잇죠",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025980.1.15",
          "form": "롤케잌도 맛잇고",
          "original_form": "롤케잌도 맛잇고",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025980.1.16",
          "form": "케이크는 다 옳은것같아요 ㅋㅋㅋㅋ",
          "original_form": "케이크는 다 옳은것같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025980.1.17",
          "form": "아 마들렌두!",
          "original_form": "아 마들렌두!",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025980.1.18",
          "form": "맞아요 마들렌도 좋아해요",
          "original_form": "맞아요 마들렌도 좋아해요",
          "speaker_id": "1",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025980.1.19",
          "form": "스콘도 좋아합니더",
          "original_form": "스콘도 좋아합니더",
          "speaker_id": "1",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025980.1.20",
          "form": "ㅏ",
          "original_form": "ㅏ",
          "speaker_id": "1",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025980.1.21",
          "form": "또또 까눌레 인가 까눌리인가",
          "original_form": "또또 까눌레 인가 까눌리인가",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025980.1.22",
          "form": "먹고있어도 이야기하면 즐겁군요",
          "original_form": "먹고있어도 이야기하면 즐겁군요",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025980.1.23",
          "form": "까눌레 너무 맛잇어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "까눌레 너무 맛잇어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025980.1.24",
          "form": "아껴먹음 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아껴먹음 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025980.1.25",
          "form": "좀 싸면 좋겠는데",
          "original_form": "좀 싸면 좋겠는데",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025980.1.26",
          "form": "빵을 시키면 항상 아메리카노가 진리인데",
          "original_form": "빵을 시키면 항상 아메리카노가 진리인데",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025980.1.27",
          "form": "좋아하는건 라떼 ㅠ",
          "original_form": "좋아하는건 라떼 ㅠ",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025980.1.28",
          "form": "저도 라떼를 좋아하는데 ㅋㅋㅋㅋㅋ 달달구리한 빵이랑 먹긴 좀 그렇죠ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 라떼를 좋아하는데 ㅋㅋㅋㅋㅋ 달달구리한 빵이랑 먹긴 좀 그렇죠ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100025980.1.29",
          "form": "전 홍차도 좋아합니다",
          "original_form": "전 홍차도 좋아합니다",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100025980.1.30",
          "form": "라떼시키면 배빵빵맨되여",
          "original_form": "라떼시키면 배빵빵맨되여",
          "speaker_id": "2",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100025980.1.31",
          "form": "마르코폴로 드셔보셨나여",
          "original_form": "마르코폴로 드셔보셨나여",
          "speaker_id": "2",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100025980.1.32",
          "form": "네 마르코폴로도 좋아하고 웨딩임페리얼도 좋아해요",
          "original_form": "네 마르코폴로도 좋아하고 웨딩임페리얼도 좋아해요",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100025980.1.33",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025980.1.34",
          "form": "홍차좋아하세요?",
          "original_form": "홍차좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025980.1.35",
          "form": "근데 비쌈 ㅠㅠ",
          "original_form": "근데 비쌈 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025980.1.36",
          "form": "저 마시는 거 다 좋아해여",
          "original_form": "저 마시는 거 다 좋아해여",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025980.1.37",
          "form": "커피 홍차 녹차 다 좋아하시나보다",
          "original_form": "커피 홍차 녹차 다 좋아하시나보다",
          "speaker_id": "1",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100025980.1.38",
          "form": "탄산이나 술도 좋아하세요?",
          "original_form": "탄산이나 술도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100025980.1.39",
          "form": "녹차는 노노노",
          "original_form": "녹차는 노노노",
          "speaker_id": "2",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100025980.1.40",
          "form": "탄산은 잘 안먹구 술도 종류별로 마셔요",
          "original_form": "탄산은 잘 안먹구 술도 종류별로 마셔요",
          "speaker_id": "2",
          "time": "20210901 18:51"
        }
      ]
