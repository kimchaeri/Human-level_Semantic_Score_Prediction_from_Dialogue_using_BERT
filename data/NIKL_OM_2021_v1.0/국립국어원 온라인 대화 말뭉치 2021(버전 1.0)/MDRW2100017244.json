      "utterance": [
        {
          "id": "MDRW2100017244.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017244.1.2",
          "form": "자주하는 커뮤니티 있어요?",
          "original_form": "자주하는 커뮤니티 있어요?",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017244.1.3",
          "form": "게임 카페 정도요",
          "original_form": "게임 카페 정도요",
          "speaker_id": "2",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017244.1.4",
          "form": "무슨 게임 하시는데요?",
          "original_form": "무슨 게임 하시는데요?",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017244.1.5",
          "form": "rpg게임 몇개 해요",
          "original_form": "rpg게임 몇개 해요",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017244.1.6",
          "form": "예전에 가입했던것두 있구요",
          "original_form": "예전에 가입했던것두 있구요",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017244.1.7",
          "form": "와~ 모바일게임이에요?",
          "original_form": "와~ 모바일게임이에요?",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017244.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017244.1.9",
          "form": "커뮤니티 하시는거 있으세요?",
          "original_form": "커뮤니티 하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017244.1.10",
          "form": "피아노 커뮤니티요~",
          "original_form": "피아노 커뮤니티요~",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017244.1.11",
          "form": "거기서 악보 공유하고 동영상도 구경해요!",
          "original_form": "거기서 악보 공유하고 동영상도 구경해요!",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017244.1.12",
          "form": "오... 모임같은것도 있나요?",
          "original_form": "오... 모임같은것도 있나요?",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017244.1.13",
          "form": "요즘 코로나여서 잘 없어요 ㅜㅜ",
          "original_form": "요즘 코로나여서 잘 없어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017244.1.14",
          "form": "콩쿠르 대회 준비하시는 분들끼리 서로 도와주고 그러긴 해요",
          "original_form": "콩쿠르 대회 준비하시는 분들끼리 서로 도와주고 그러긴 해요",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017244.1.15",
          "form": "와 그런건 도움이 많이 될 거 같아요",
          "original_form": "와 그런건 도움이 많이 될 거 같아요",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017244.1.16",
          "form": "그쵸~ 피아노 브랜드 성능이나 가격 공유도 해요 ㅎㅎ",
          "original_form": "그쵸~ 피아노 브랜드 성능이나 가격 공유도 해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017244.1.17",
          "form": "전문가들이 많을거 같네요ㅋㅋ",
          "original_form": "전문가들이 많을거 같네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017244.1.18",
          "form": "게임도 전문가들 많을 것 같아요~",
          "original_form": "게임도 전문가들 많을 것 같아요~",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017244.1.19",
          "form": "주로 어떤 정보 얻으시나요?",
          "original_form": "주로 어떤 정보 얻으시나요?",
          "speaker_id": "1",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017244.1.20",
          "form": "보통 공략 같은거 봐요",
          "original_form": "보통 공략 같은거 봐요",
          "speaker_id": "2",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017244.1.21",
          "form": "육성 팁도 많구요",
          "original_form": "육성 팁도 많구요",
          "speaker_id": "2",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017244.1.22",
          "form": "오 무슨 게임이에요?",
          "original_form": "오 무슨 게임이에요?",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017244.1.23",
          "form": "바람의나라?",
          "original_form": "바람의나라?",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017244.1.24",
          "form": "네 바람의 나라도 가끔해요ㅋㅋ",
          "original_form": "네 바람의 나라도 가끔해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017244.1.25",
          "form": "바람의나라는 문파가입 하려면 커뮤니티 필수 같아요~",
          "original_form": "바람의나라는 문파가입 하려면 커뮤니티 필수 같아요~",
          "speaker_id": "1",
          "time": "20210826 18:30"
        }
      ]
    }
