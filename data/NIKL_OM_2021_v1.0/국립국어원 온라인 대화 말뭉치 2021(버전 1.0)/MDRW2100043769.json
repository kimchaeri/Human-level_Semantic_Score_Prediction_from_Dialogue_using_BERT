      "utterance": [
        {
          "id": "MDRW2100043769.1.1",
          "form": "네 육개장 맛있더라구요!!",
          "original_form": "네 육개장 맛있더라구요!!",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043769.1.2",
          "form": "육개장 웬만한건 다 맛있는거 같아요ㅎㅎ",
          "original_form": "육개장 웬만한건 다 맛있는거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043769.1.3",
          "form": "좋아하는 찌개나 국 있으신가요?",
          "original_form": "좋아하는 찌개나 국 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043769.1.4",
          "form": "저는 말씀하시니까 동태찌개 먹고 싶어요",
          "original_form": "저는 말씀하시니까 동태찌개 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043769.1.5",
          "form": "평소에는 하얀국물 좋아하긴 해요",
          "original_form": "평소에는 하얀국물 좋아하긴 해요",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043769.1.6",
          "form": "오 동태찌개도 맛있겠어요!",
          "original_form": "오 동태찌개도 맛있겠어요!",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043769.1.7",
          "form": "하얀국물 하니까 소고기무국이 먹고싶네요",
          "original_form": "하얀국물 하니까 소고기무국이 먹고싶네요",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043769.1.8",
          "form": "요즘 같은 날씨에 딱이죠",
          "original_form": "요즘 같은 날씨에 딱이죠",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043769.1.9",
          "form": "소고기 무국도 맛있겠다!!",
          "original_form": "소고기 무국도 맛있겠다!!",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043769.1.10",
          "form": "그쵸!",
          "original_form": "그쵸!",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043769.1.11",
          "form": "하얀국물 어떤거 좋아하세요?",
          "original_form": "하얀국물 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043769.1.12",
          "form": "저 지금 이순간엔 콩나물국밥 생각이 나요",
          "original_form": "저 지금 이순간엔 콩나물국밥 생각이 나요",
          "speaker_id": "2",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043769.1.13",
          "form": "아 맛있겠네요ㅠㅠㅠ",
          "original_form": "아 맛있겠네요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043769.1.14",
          "form": "계란풀어서 호로록하면",
          "original_form": "계란풀어서 호로록하면",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043769.1.15",
          "form": "따뜻하고 좋을거같아요",
          "original_form": "따뜻하고 좋을거같아요",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043769.1.16",
          "form": "ㅋㅋㅋ 맞아여 계란 하나를 딱",
          "original_form": "ㅋㅋㅋ 맞아여 계란 하나를 딱",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043769.1.17",
          "form": "맛있져 ㅠㅠ",
          "original_form": "맛있져 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043769.1.18",
          "form": "초밥이나 회도 좋아하시나요?",
          "original_form": "초밥이나 회도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043769.1.19",
          "form": "저 없어서 못먹어요",
          "original_form": "저 없어서 못먹어요",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043769.1.20",
          "form": "ㅋㅋㅋㅋ 맛있죠 name1님은?",
          "original_form": "ㅋㅋㅋㅋ 맛있죠 &name1&님은?",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043769.1.21",
          "form": "저도 없어서 못먹어요....",
          "original_form": "저도 없어서 못먹어요....",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043769.1.22",
          "form": "초밥이랑 회...",
          "original_form": "초밥이랑 회...",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043769.1.23",
          "form": "무슨 초밥 좋아하세요?",
          "original_form": "무슨 초밥 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043769.1.24",
          "form": "생새우 연어 좋아해요 ㅜㅠ",
          "original_form": "생새우 연어 좋아해요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043769.1.25",
          "form": "헐 저도 두개 좋아해요!!!",
          "original_form": "헐 저도 두개 좋아해요!!!",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043769.1.26",
          "form": "연어에 홀스래디쉬 소스 올라간거 좋아하세요?",
          "original_form": "연어에 홀스래디쉬 소스 올라간거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043769.1.27",
          "form": "완전요!!!",
          "original_form": "완전요!!!",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043769.1.28",
          "form": "역시 소스...",
          "original_form": "역시 소스...",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043769.1.29",
          "form": "헐 저두요!!!!!!",
          "original_form": "헐 저두요!!!!!!",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043769.1.30",
          "form": "너무 맛있죠ㅠㅠㅠㅠ",
          "original_form": "너무 맛있죠ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043769.1.31",
          "form": "낼 연어초밥 먹어야겠어요",
          "original_form": "낼 연어초밥 먹어야겠어요",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043769.1.32",
          "form": "완전 먹고싶네요ㅠㅠ",
          "original_form": "완전 먹고싶네요ㅠㅠ",
