      "utterance": [
        {
          "id": "MDRW2100003195.1.1",
          "form": "전시회 좋아하세요?",
          "original_form": "전시회 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 20:24"
        },
        {
          "id": "MDRW2100003195.1.2",
          "form": "전시.. 음 가본적이 많이 없어서 기회되면 가보고싶어요",
          "original_form": "전시.. 음 가본적이 많이 없어서 기회되면 가보고싶어요",
          "speaker_id": "2",
          "time": "20210820 23:58"
        },
        {
          "id": "MDRW2100003195.1.3",
          "form": "name1님은 전시회 좋아하시나요?",
          "original_form": "&name1&님은 전시회 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210820 23:58"
        },
        {
          "id": "MDRW2100003195.1.4",
          "form": "네 저도 나름 좋아해요",
          "original_form": "네 저도 나름 좋아해요",
          "speaker_id": "1",
          "time": "20210821 01:15"
        },
        {
          "id": "MDRW2100003195.1.5",
          "form": "코로나 전에는 그래도 좀 갔어요!",
          "original_form": "코로나 전에는 그래도 좀 갔어요!",
          "speaker_id": "1",
          "time": "20210821 01:16"
        },
        {
          "id": "MDRW2100003195.1.6",
          "form": "어떤 종류의 전시회를 가보셨나요?",
          "original_form": "어떤 종류의 전시회를 가보셨나요?",
          "speaker_id": "2",
          "time": "20210821 06:35"
        },
        {
          "id": "MDRW2100003195.1.7",
          "form": "저 다양하게 다녔는데",
          "original_form": "저 다양하게 다녔는데",
          "speaker_id": "1",
          "time": "20210821 09:51"
        },
        {
          "id": "MDRW2100003195.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210821 09:52"
        },
        {
          "id": "MDRW2100003195.1.9",
          "form": "존레논 전시회 마음에 들었어요",
          "original_form": "존레논 전시회 마음에 들었어요",
          "speaker_id": "1",
          "time": "20210821 09:52"
        },
        {
          "id": "MDRW2100003195.1.10",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210821 09:52"
        },
        {
          "id": "MDRW2100003195.1.11",
          "form": "존레논 비틀즈요?",
          "original_form": "존레논 비틀즈요?",
          "speaker_id": "2",
          "time": "20210821 09:52"
        },
        {
          "id": "MDRW2100003195.1.12",
          "form": "저는 보통 지역에서 하는 작은 전시회같은거밖에 안가봐서 기억에 남는게 음..",
          "original_form": "저는 보통 지역에서 하는 작은 전시회같은거밖에 안가봐서 기억에 남는게 음..",
          "speaker_id": "2",
          "time": "20210821 09:52"
        },
        {
          "id": "MDRW2100003195.1.13",
          "form": "딱히 없어요ㅠ",
          "original_form": "딱히 없어요ㅠ",
          "speaker_id": "2",
          "time": "20210821 09:52"
        },
        {
          "id": "MDRW2100003195.1.14",
          "form": "존레논 전시회에서 가장 기억에 남는 것은 뭐였나요?",
          "original_form": "존레논 전시회에서 가장 기억에 남는 것은 뭐였나요?",
          "speaker_id": "2",
          "time": "20210821 09:53"
        },
        {
          "id": "MDRW2100003195.1.15",
          "form": "네 맞아요 !",
          "original_form": "네 맞아요 !",
          "speaker_id": "1",
          "time": "20210821 09:53"
        },
        {
          "id": "MDRW2100003195.1.16",
          "form": "비틀즈 멤버들끼리 사이가 안좋았대여",
          "original_form": "비틀즈 멤버들끼리 사이가 안좋았대여",
          "speaker_id": "1",
          "time": "20210821 09:53"
        },
        {
          "id": "MDRW2100003195.1.17",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "2",
          "time": "20210821 09:53"
        },
        {
          "id": "MDRW2100003195.1.18",
          "form": "그래사 충격이었어요",
          "original_form": "그래사 충격이었어요",
          "speaker_id": "1",
          "time": "20210821 09:53"
        },
        {
          "id": "MDRW2100003195.1.19",
          "form": "저도 지금 좀 충격",
          "original_form": "저도 지금 좀 충격",
          "speaker_id": "2",
          "time": "20210821 09:54"
        },
        {
          "id": "MDRW2100003195.1.20",
          "form": "사이가 안좋았다니",
          "original_form": "사이가 안좋았다니",
          "speaker_id": "2",
          "time": "20210821 09:54"
        },
        {
          "id": "MDRW2100003195.1.21",
          "form": "그쵸... 전 존레논이 돌아가싱 이유도 처음 알았는데",
          "original_form": "그쵸... 전 존레논이 돌아가싱 이유도 처음 알았는데",
          "speaker_id": "1",
          "time": "20210821 09:57"
        },
        {
          "id": "MDRW2100003195.1.22",
          "form": "뭔가 충격이었어요",
          "original_form": "뭔가 충격이었어요",
          "speaker_id": "1",
          "time": "20210821 09:57"
        }
      ]
