      "utterance": [
        {
          "id": "MDRW2100038122.1.1",
          "form": "구렁이라니 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "구렁이라니 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038122.1.2",
          "form": "황금이였나요",
          "original_form": "황금이였나요",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038122.1.3",
          "form": "진짜 그 때 황금 구렁이가 뭔지",
          "original_form": "진짜 그 때 황금 구렁이가 뭔지",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038122.1.4",
          "form": "네네...",
          "original_form": "네네...",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038122.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ와우",
          "original_form": "ㅋㅋㅋㅋㅋㅋ와우",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038122.1.6",
          "form": "그러고나서는 다른 유산균 먹어도",
          "original_form": "그러고나서는 다른 유산균 먹어도",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038122.1.7",
          "form": "진짜 뽐뿌오네여",
          "original_form": "진짜 뽐뿌오네여",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038122.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038122.1.9",
          "form": "꼭 그 유산균 아니어도",
          "original_form": "꼭 그 유산균 아니어도",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038122.1.10",
          "form": "잘 받더라구요",
          "original_form": "잘 받더라구요",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038122.1.11",
          "form": "제 생각엔 제 대장에 있는 균을",
          "original_form": "제 생각엔 제 대장에 있는 균을",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038122.1.12",
          "form": "크으 걔가 물꼬를",
          "original_form": "크으 걔가 물꼬를",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038122.1.13",
          "form": "한 번 싹 갈아준 것 같아요",
          "original_form": "한 번 싹 갈아준 것 같아요",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038122.1.14",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038122.1.15",
          "form": "한번찾아봐야겠어요",
          "original_form": "한번찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038122.1.16",
          "form": "크리..?",
          "original_form": "크리..?",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038122.1.17",
          "form": "크리스찬 한센",
          "original_form": "크리스찬 한센",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038122.1.18",
          "form": "이에요!",
          "original_form": "이에요!",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038122.1.19",
          "form": "아마 덴마크",
          "original_form": "아마 덴마크",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038122.1.20",
          "form": "에서 들여온 균주일거예요",
          "original_form": "에서 들여온 균주일거예요",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038122.1.21",
          "form": "지금적어둿어요 ㅋㅋㅋㅋㅋ",
          "original_form": "지금적어둿어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038122.1.22",
          "form": "자꾸 머리에 열 오르고 그러는게",
          "original_form": "자꾸 머리에 열 오르고 그러는게",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038122.1.23",
          "form": "장이 약해서 더 그런거같기도 하거든요!",
          "original_form": "장이 약해서 더 그런거같기도 하거든요!",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038122.1.24",
          "form": "맞아요ㅜㅜㅜㅜ",
          "original_form": "맞아요ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038122.1.25",
          "form": "반신욕을 너무 오래 하게 된다면",
          "original_form": "반신욕을 너무 오래 하게 된다면",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038122.1.26",
          "form": "족욕을 하시고",
          "original_form": "족욕을 하시고",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038122.1.27",
          "form": "장에서염증 속에서열나고",
          "original_form": "장에서염증 속에서열나고",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038122.1.28",
          "form": "부글부글하고",
          "original_form": "부글부글하고",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038122.1.29",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 23:28"
        }
      ]
    }
  ]
