      "utterance": [
        {
          "id": "MDRW2100045633.1.1",
          "form": "무슨 떡인지도 맞추겠슴다",
          "original_form": "무슨 떡인지도 맞추겠슴다",
          "speaker_id": "2",
          "time": "20210908 04:29"
        },
        {
          "id": "MDRW2100045633.1.2",
          "form": "ㅋㅋㅋㅋㅋ화이팅",
          "original_form": "ㅋㅋㅋㅋㅋ화이팅",
          "speaker_id": "1",
          "time": "20210908 04:30"
        },
        {
          "id": "MDRW2100045633.1.3",
          "form": "팥이 들어가나요?",
          "original_form": "팥이 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 04:30"
        },
        {
          "id": "MDRW2100045633.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210908 04:30"
        },
        {
          "id": "MDRW2100045633.1.5",
          "form": "질감이 포슬포슬한가요?",
          "original_form": "질감이 포슬포슬한가요?",
          "speaker_id": "2",
          "time": "20210908 04:30"
        },
        {
          "id": "MDRW2100045633.1.6",
          "form": "아니요 쫀득쫀득해요",
          "original_form": "아니요 쫀득쫀득해요",
          "speaker_id": "1",
          "time": "20210908 04:30"
        },
        {
          "id": "MDRW2100045633.1.7",
          "form": "안에 뭔가 들어있나요?",
          "original_form": "안에 뭔가 들어있나요?",
          "speaker_id": "2",
          "time": "20210908 04:31"
        },
        {
          "id": "MDRW2100045633.1.8",
          "form": "안들어잇어요",
          "original_form": "안들어잇어요",
          "speaker_id": "1",
          "time": "20210908 04:31"
        },
        {
          "id": "MDRW2100045633.1.9",
          "form": "그럼 겉에 뭔가 붙어있나요?",
          "original_form": "그럼 겉에 뭔가 붙어있나요?",
          "speaker_id": "2",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045633.1.10",
          "form": "안붙어잇어요",
          "original_form": "안붙어잇어요",
          "speaker_id": "1",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045633.1.11",
          "form": "이럴수가 민둥민둥한 쌀뭉치인가요",
          "original_form": "이럴수가 민둥민둥한 쌀뭉치인가요",
          "speaker_id": "2",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045633.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ네",
          "speaker_id": "1",
          "time": "20210908 04:33"
        },
        {
          "id": "MDRW2100045633.1.13",
          "form": "길이가 길쭉한가요?",
          "original_form": "길이가 길쭉한가요?",
          "speaker_id": "2",
          "time": "20210908 04:33"
        },
        {
          "id": "MDRW2100045633.1.14",
          "form": "떡국 먹으려고 ㅋㅋㅋㅋㅋ",
          "original_form": "떡국 먹으려고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:33"
        },
        {
          "id": "MDRW2100045633.1.15",
          "form": "ㅌㅋㅌㅋㅋㅋ아 거의 다 왔었네요!",
          "original_form": "ㅌㅋㅌㅋㅋㅋ아 거의 다 왔었네요!",
          "speaker_id": "2",
          "time": "20210908 04:34"
        },
        {
          "id": "MDRW2100045633.1.16",
          "form": "가래떡 말하려고 했는디!",
          "original_form": "가래떡 말하려고 했는디!",
          "speaker_id": "2",
          "time": "20210908 04:34"
        },
        {
          "id": "MDRW2100045633.1.17",
          "form": "네 다왓어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 다왓어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:34"
        },
        {
          "id": "MDRW2100045633.1.18",
          "form": "앗 제가 정답유출을 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 제가 정답유출을 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:34"
        },
        {
          "id": "MDRW2100045633.1.19",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:34"
        },
        {
          "id": "MDRW2100045633.1.20",
          "form": "ㅌㅋㅋㅋㅌㅌㅋ무의식 중 유출ㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅌㅌㅋ무의식 중 유출ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:34"
        },
        {
          "id": "MDRW2100045633.1.21",
