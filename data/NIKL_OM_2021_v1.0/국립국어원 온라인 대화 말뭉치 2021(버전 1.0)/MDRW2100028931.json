          "speaker_id": "1",
          "time": "20210903 07:18"
        },
        {
          "id": "MDRW2100028931.1.6",
          "form": "여긴 조금씩 오고 있어요",
          "original_form": "여긴 조금씩 오고 있어요",
          "speaker_id": "2",
          "time": "20210903 07:28"
        },
        {
          "id": "MDRW2100028931.1.7",
          "form": "거기도 비오나요?",
          "original_form": "거기도 비오나요?",
