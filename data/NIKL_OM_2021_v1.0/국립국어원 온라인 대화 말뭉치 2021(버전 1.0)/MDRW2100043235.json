          "speaker_id": "2",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043235.1.3",
          "form": "밑에 사고났어",
          "original_form": "밑에 사고났어",
          "speaker_id": "1",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043235.1.4",
          "form": "사고?",
          "original_form": "사고?",
          "speaker_id": "2",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043235.1.5",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043235.1.6",
          "form": "소리 나더니",
          "original_form": "소리 나더니",
          "speaker_id": "2",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043235.1.7",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043235.1.8",
          "form": "장",
          "original_form": "장",
          "speaker_id": "2",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043235.1.9",
          "form": "창",
          "original_form": "창",
          "speaker_id": "2",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043235.1.10",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043235.1.11",
          "form": "택배차랑 이사짐센터 차 부딪혔다",
          "original_form": "택배차랑 이사짐센터 차 부딪혔다",
          "speaker_id": "1",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043235.1.12",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043235.1.13",
          "form": "사람은? 다쳤어?",
          "original_form": "사람은? 다쳤어?",
          "speaker_id": "2",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100043235.1.14",
          "form": "괜찮나?",
          "original_form": "괜찮나?",
          "speaker_id": "2",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100043235.1.15",
          "form": "몰라 택배차가 단지 나가려고 후진하다",
          "original_form": "몰라 택배차가 단지 나가려고 후진하다",
          "speaker_id": "1",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100043235.1.16",
          "form": "이삿짐센터 차 받았다",
          "original_form": "이삿짐센터 차 받았다",
          "speaker_id": "1",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100043235.1.17",
          "form": "헐.. 택배기사님 어떡해",
          "original_form": "헐.. 택배기사님 어떡해",
          "speaker_id": "2",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100043235.1.18",
          "form": "사고 크게 난거야?",
          "original_form": "사고 크게 난거야?",
          "speaker_id": "2",
          "time": "20210907 16:08"
        },
        {
          "id": "MDRW2100043235.1.19",
          "form": "이삿짐센터 왤케 꼴뵈기 싫냐",
          "original_form": "이삿짐센터 왤케 꼴뵈기 싫냐",
          "speaker_id": "1",
          "time": "20210907 16:08"
        },
        {
          "id": "MDRW2100043235.1.20",
          "form": "저 긴차로 한가운데서 가로막고있잖아 아",
          "original_form": "저 긴차로 한가운데서 가로막고있잖아 아",
          "speaker_id": "1",
          "time": "20210907 16:08"
        },
        {
          "id": "MDRW2100043235.1.21",
          "form": "택배기사가 물어줘야되나ㅠㅜ",
          "original_form": "택배기사가 물어줘야되나ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 16:08"
        },
        {
          "id": "MDRW2100043235.1.22",
          "form": "일도 힘든데 어떡함..",
          "original_form": "일도 힘든데 어떡함..",
          "speaker_id": "2",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100043235.1.23",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100043235.1.24",
          "form": "이삿짐센터",
          "original_form": "이삿짐센터",
          "speaker_id": "1",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100043235.1.25",
          "form": "차",
          "original_form": "차",
          "speaker_id": "1",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100043235.1.26",
          "form": "물어",
          "original_form": "물어",
          "speaker_id": "1",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100043235.1.27",
          "form": "주게",
          "original_form": "주게",
          "speaker_id": "1",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100043235.1.28",
          "form": "생겼네",
          "original_form": "생겼네",
          "speaker_id": "1",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100043235.1.29",
          "form": "저거 쌍방과실 안되나",
          "original_form": "저거 쌍방과실 안되나",
          "speaker_id": "2",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100043235.1.30",
          "form": "이삿짐센터 차는 서있었어서..",
          "original_form": "이삿짐센터 차는 서있었어서..",
          "speaker_id": "1",
          "time": "20210907 16:10"
        },
        {
          "id": "MDRW2100043235.1.31",
          "form": "근데 소방차전용구역에 원래 대면 안되는데 거기 가로질러서",
          "original_form": "근데 소방차전용구역에 원래 대면 안되는데 거기 가로질러서",
          "speaker_id": "2",
          "time": "20210907 16:10"
        },
        {
          "id": "MDRW2100043235.1.32",
          "form": "버티고있었으니까",
          "original_form": "버티고있었으니까",
          "speaker_id": "2",
          "time": "20210907 16:10"
        },
        {
          "id": "MDRW2100043235.1.33",
          "form": "그런거 들어갈지 모르겠다",
          "original_form": "그런거 들어갈지 모르겠다",
          "speaker_id": "1",
          "time": "20210907 16:11"
        },
        {
          "id": "MDRW2100043235.1.34",
          "form": "보험사부르나봐",
          "original_form": "보험사부르나봐",
          "speaker_id": "1",
          "time": "20210907 16:11"
        },
        {
          "id": "MDRW2100043235.1.35",
          "form": "이런거 저런거 다 포함해서 쌍방과실 어느정도 나옴 좋겠다..",
          "original_form": "이런거 저런거 다 포함해서 쌍방과실 어느정도 나옴 좋겠다..",
          "speaker_id": "2",
          "time": "20210907 16:11"
        },
        {
          "id": "MDRW2100043235.1.36",
          "form": "그나마 사람들 안 다쳐서 다행이다",
          "original_form": "그나마 사람들 안 다쳐서 다행이다",
          "speaker_id": "1",
          "time": "20210907 16:11"
        }
      ]
