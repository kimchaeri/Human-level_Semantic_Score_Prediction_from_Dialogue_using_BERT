      "utterance": [
        {
          "id": "MDRW2100007549.1.1",
          "form": "흠 이번엔 게임 어떠세여",
          "original_form": "흠 이번엔 게임 어떠세여",
          "speaker_id": "1",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007549.1.2",
          "form": "게임 요즘 어떤거 하세용",
          "original_form": "게임 요즘 어떤거 하세용",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007549.1.3",
          "form": "솔리테어 이게 게임이름이랬나?",
          "original_form": "솔리테어 이게 게임이름이랬나?",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007549.1.4",
          "form": "응응 카드게임",
          "original_form": "응응 카드게임",
          "speaker_id": "2",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007549.1.5",
          "form": "카드를 순서대로 차곡차곡 정리하는 게임이라고 생각하면 돼",
          "original_form": "카드를 순서대로 차곡차곡 정리하는 게임이라고 생각하면 돼",
          "speaker_id": "2",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007549.1.6",
          "form": "피씨에 기본 게임으로도 깔려있다",
          "original_form": "피씨에 기본 게임으로도 깔려있다",
          "speaker_id": "2",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007549.1.7",
          "form": "앗 그래...?",
          "original_form": "앗 그래...?",
          "speaker_id": "1",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007549.1.8",
          "form": "마이크로소프트 솔리테어 어쩌구 영어로 치면 뜸",
          "original_form": "마이크로소프트 솔리테어 어쩌구 영어로 치면 뜸",
          "speaker_id": "2",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007549.1.9",
          "form": "뭔가 해본듯한데",
          "original_form": "뭔가 해본듯한데",
          "speaker_id": "1",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007549.1.10",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007549.1.11",
          "form": "솔리테어라는 말은 첨들어봐",
          "original_form": "솔리테어라는 말은 첨들어봐",
          "speaker_id": "1",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007549.1.12",
          "form": "트럼프 카드로 하는거 ㅋㅋㅋ",
          "original_form": "트럼프 카드로 하는거 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007549.1.13",
          "form": "a2345678910jqk",
          "original_form": "a2345678910jqk",
          "speaker_id": "2",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007549.1.14",
          "form": "이순서로",
          "original_form": "이순서로",
          "speaker_id": "2",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007549.1.15",
          "form": "카드 정리하는 게임이야 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "카드 정리하는 게임이야 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007549.1.16",
          "form": "핸드폰으로도 앱깔아서 할 수 있는데",
          "original_form": "핸드폰으로도 앱깔아서 할 수 있는데",
          "speaker_id": "2",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007549.1.17",
          "form": "난 솔리테어:덱다웃 깔아서 하는 중!!",
          "original_form": "난 솔리테어:덱다웃 깔아서 하는 중!!",
          "speaker_id": "2",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007549.1.18",
          "form": "이게 고양이 스킨도 있고 귀여워...",
          "original_form": "이게 고양이 스킨도 있고 귀여워...",
          "speaker_id": "2",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007549.1.19",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007549.1.20",
          "form": "트럼프 카드에",
          "original_form": "트럼프 카드에",
          "speaker_id": "1",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007549.1.21",
          "form": "고양이 스킨이 깔리는거야...?",
          "original_form": "고양이 스킨이 깔리는거야...?",
          "speaker_id": "1",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007549.1.22",
          "form": "기엽겟다",
          "original_form": "기엽겟다",
          "speaker_id": "1",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007549.1.23",
          "form": "응응 카드도 고양이로 생겼고",
          "original_form": "응응 카드도 고양이로 생겼고",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007549.1.24",
          "form": "모으는 스킨에 따라서 화면에 고양이 지나가는 이벤트 있고 그래 ㅋㅋㅋㅋㅋ귀엽당",
          "original_form": "모으는 스킨에 따라서 화면에 고양이 지나가는 이벤트 있고 그래 ㅋㅋㅋㅋㅋ귀엽당",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007549.1.25",
          "form": "그리고 무엇보다도 컴퓨터로 하는 솔리테어보다 난이도가 쉬움 ㅠㅋㅋㅋㅋ",
          "original_form": "그리고 무엇보다도 컴퓨터로 하는 솔리테어보다 난이도가 쉬움 ㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007549.1.26",
          "form": "ㅋㅋㅋ그럼 킹 퀸 이런데는",
          "original_form": "ㅋㅋㅋ그럼 킹 퀸 이런데는",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007549.1.27",
          "form": "고양이가 왕이고 여왕이고",
          "original_form": "고양이가 왕이고 여왕이고",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007549.1.28",
          "form": "그런식으로 그려져있는거야?",
          "original_form": "그런식으로 그려져있는거야?",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007549.1.29",
          "form": "아니 그냥 고양이...",
          "original_form": "아니 그냥 고양이...",
          "speaker_id": "2",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007549.1.30",
          "form": "가끔 이벤트로 화면에 고양이도 지나가고",
          "original_form": "가끔 이벤트로 화면에 고양이도 지나가고",
          "speaker_id": "2",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007549.1.31",
          "form": "고양이 말고도 핫도그나 눈사람 뭐 이런 다양한 스킨들 많아 ㅋㅋㅋㅋㅋ",
          "original_form": "고양이 말고도 핫도그나 눈사람 뭐 이런 다양한 스킨들 많아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:23"
        }
