      "utterance": [
        {
          "id": "MDRW2100029460.1.1",
          "form": "ㅎㅇㅎㅇ",
          "original_form": "ㅎㅇㅎㅇ",
          "speaker_id": "1",
          "time": "20210903 08:52"
        },
        {
          "id": "MDRW2100029460.1.2",
          "form": "좋은아침",
          "original_form": "좋은아침",
          "speaker_id": "1",
          "time": "20210903 08:52"
        },
        {
          "id": "MDRW2100029460.1.3",
          "form": "안녕하세요? 좋은아침입니다ㅋ",
          "original_form": "안녕하세요? 좋은아침입니다ㅋ",
          "speaker_id": "2",
          "time": "20210903 08:53"
        },
        {
          "id": "MDRW2100029460.1.4",
          "form": "출근잘했나요",
          "original_form": "출근잘했나요",
          "speaker_id": "1",
          "time": "20210903 08:54"
        },
        {
          "id": "MDRW2100029460.1.5",
          "form": "금요일이라그런지 피곤하네요",
          "original_form": "금요일이라그런지 피곤하네요",
          "speaker_id": "1",
          "time": "20210903 08:54"
        },
        {
          "id": "MDRW2100029460.1.6",
          "form": "내 지금 아르바이트 끝났어요",
          "original_form": "내 지금 아르바이트 끝났어요",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029460.1.7",
          "form": "정말 피곤하네요",
          "original_form": "정말 피곤하네요",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029460.1.8",
          "form": "고생했어요 무슨알바해요?",
          "original_form": "고생했어요 무슨알바해요?",
          "speaker_id": "1",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100029460.1.9",
          "form": "복지관에서 미술보 조강사로 아르바이트 하고 있어요",
          "original_form": "복지관에서 미술보 조강사로 아르바이트 하고 있어요",
          "speaker_id": "2",
          "time": "20210903 11:39"
        },
        {
          "id": "MDRW2100029460.1.10",
          "form": "알바 끝나서 한의원에 침 맞으러 가요",
          "original_form": "알바 끝나서 한의원에 침 맞으러 가요",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029460.1.11",
          "form": "어디아파요? 마침 주제가 건강이네요",
          "original_form": "어디아파요? 마침 주제가 건강이네요",
          "speaker_id": "1",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029460.1.12",
          "form": "저도 어꺠가 안좋아서 재활치료 하고있네요",
          "original_form": "저도 어꺠가 안좋아서 재활치료 하고있네요",
          "speaker_id": "1",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029460.1.13",
          "form": "어깨를 너무 많이쓰셔서 그런거예요?",
          "original_form": "어깨를 너무 많이쓰셔서 그런거예요?",
          "speaker_id": "2",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029460.1.14",
          "form": "네 관절와손 파열이에요 운동을 너무해서ㅋ",
          "original_form": "네 관절와손 파열이에요 운동을 너무해서ㅋ",
          "speaker_id": "1",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100029460.1.15",
          "form": "아하ㅜㅋㅋㅋ그러시구나ㅜㅜ웁스",
          "original_form": "아하ㅜㅋㅋㅋ그러시구나ㅜㅜ웁스",
          "speaker_id": "2",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100029460.1.16",
          "form": "운동 얼마나 많이하셨으면 ㅜㅜ",
          "original_form": "운동 얼마나 많이하셨으면 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 11:53"
        },
        {
          "id": "MDRW2100029460.1.17",
          "form": "재활치료하니 좀 괜찮아지고있나요?",
          "original_form": "재활치료하니 좀 괜찮아지고있나요?",
          "speaker_id": "2",
          "time": "20210903 11:53"
        },
        {
          "id": "MDRW2100029460.1.18",
          "form": "사실 잘모르겠어요 괜찮아지는건지ㅜㅜ",
          "original_form": "사실 잘모르겠어요 괜찮아지는건지ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 12:29"
        },
        {
          "id": "MDRW2100029460.1.19",
          "form": "침맞는건 좀어때요",
          "original_form": "침맞는건 좀어때요",
          "speaker_id": "1",
          "time": "20210903 12:29"
        },
        {
          "id": "MDRW2100029460.1.20",
          "form": "아 ㅋㅋ 저는 소화가 잘 안되서 오늘 맞았는데 많이 좋아진것같아요 ㅋㅋ",
          "original_form": "아 ㅋㅋ 저는 소화가 잘 안되서 오늘 맞았는데 많이 좋아진것같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:14"
        },
        {
          "id": "MDRW2100029460.1.21",
          "form": "오 전 침을 한번도 맞아본적 없어요",
          "original_form": "오 전 침을 한번도 맞아본적 없어요",
          "speaker_id": "1",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100029460.1.22",
          "form": "고슴도치되기 무서워서 ㄷㄷ",
          "original_form": "고슴도치되기 무서워서 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210903 14:17"
        }
      ]
