      "utterance": [
        {
          "id": "MDRW2100005654.1.1",
          "form": "요즘에는 나는 쇼핑을 좀 줄였거든?",
          "original_form": "요즘에는 나는 쇼핑을 좀 줄였거든?",
          "speaker_id": "2",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005654.1.2",
          "form": "웃긴게 누가오면 꼭 택배는 와있음ㅋㅋ",
          "original_form": "웃긴게 누가오면 꼭 택배는 와있음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005654.1.3",
          "form": "ㅋㅋㅋ 아웃겨 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 아웃겨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005654.1.4",
          "form": "그분과 함께 오시네",
          "original_form": "그분과 함께 오시네",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005654.1.5",
          "form": "그러니깐ㅋㅋ잘 안시킨다 해도 안믿음ㅋ",
          "original_form": "그러니깐ㅋㅋ잘 안시킨다 해도 안믿음ㅋ",
          "speaker_id": "2",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005654.1.6",
          "form": "다들 잔다. 조용하다ㅋㅋ",
          "original_form": "다들 잔다. 조용하다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005654.1.7",
          "form": "이럴때 또 쇼핑이나 카페보지 히히",
          "original_form": "이럴때 또 쇼핑이나 카페보지 히히",
          "speaker_id": "2",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005654.1.8",
          "form": "부럽다ㅋㅋㅋ",
          "original_form": "부럽다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005654.1.9",
          "form": "근데지금보니",
          "original_form": "근데지금보니",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005654.1.10",
          "form": "여기도 다자네?",
          "original_form": "여기도 다자네?",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005654.1.11",
          "form": "아 좋다좋다",
          "original_form": "아 좋다좋다",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005654.1.12",
          "form": "돈쓸 궁리",
          "original_form": "돈쓸 궁리",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005654.1.13",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005654.1.14",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005654.1.15",
          "form": "그러니깐ㅋㅋㅋ",
          "original_form": "그러니깐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005654.1.16",
          "form": "내가 맘베 핫딜보고",
          "original_form": "내가 맘베 핫딜보고",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005654.1.17",
          "form": "돈벌고 돈쓰고ㅋㅋ",
          "original_form": "돈벌고 돈쓰고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005654.1.18",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005654.1.19",
          "form": "미니돈까스를 샀거든?",
          "original_form": "미니돈까스를 샀거든?",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005654.1.20",
          "form": "근데 고기함량이 진짜많아",
          "original_form": "근데 고기함량이 진짜많아",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005654.1.21",
          "form": "근데 오빠가 싫데ㅡㅡ",
          "original_form": "근데 오빠가 싫데ㅡㅡ",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005654.1.22",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005654.1.23",
          "form": "왜싫대?",
          "original_form": "왜싫대?",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005654.1.24",
          "form": "맛없대?",
          "original_form": "맛없대?",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005654.1.25",
          "form": "느끼하데",
          "original_form": "느끼하데",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005654.1.26",
          "form": "왜꼬마돈가스는 밀가루가",
          "original_form": "왜꼬마돈가스는 밀가루가",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005654.1.27",
          "form": "많자나",
          "original_form": "많자나",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005654.1.28",
          "form": "얇고ㅋㄱ먼지알지? 그런거좋은데",
          "original_form": "얇고ㅋㄱ먼지알지? 그런거좋은데",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005654.1.29",
          "form": "이건 두툼하고 딱 그냥돈가스고기가 ㅋㅋ씹혀",
          "original_form": "이건 두툼하고 딱 그냥돈가스고기가 ㅋㅋ씹혀",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005654.1.30",
