      "utterance": [
        {
          "id": "MDRW2100036474.1.1",
          "form": "너 배구 좋아하지?",
          "original_form": "너 배구 좋아하지?",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100036474.1.2",
          "form": "어?? 어어 들어봤어",
          "original_form": "어?? 어어 들어봤어",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100036474.1.3",
          "form": "굿즈얘기하니까 잘됔ㅅ댜",
          "original_form": "굿즈얘기하니까 잘됔ㅅ댜",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100036474.1.4",
          "form": "배구 엠디를 판매하거든",
          "original_form": "배구 엠디를 판매하거든",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100036474.1.5",
          "form": "봤어?",
          "original_form": "봤어?",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.6",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.7",
          "form": "근데 되게 못생겼더라",
          "original_form": "근데 되게 못생겼더라",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.8",
          "form": "아니 근데 얘기만들었어",
          "original_form": "아니 근데 얘기만들었어",
          "speaker_id": "1",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.9",
          "form": "어떤 물품들을 팔아mm",
          "original_form": "어떤 물품들을 팔아mm",
          "speaker_id": "1",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.10",
          "form": "너는 못생긴것도 팬심으로 살 수 있어m",
          "original_form": "너는 못생긴것도 팬심으로 살 수 있어m",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.11",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.12",
          "form": "폰케이스를 팔더라",
          "original_form": "폰케이스를 팔더라",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.13",
          "form": "음 나는 못생긴건 괜찮은데",
          "original_form": "음 나는 못생긴건 괜찮은데",
          "speaker_id": "1",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.14",
          "form": "이어폰 케이스들도 있었어",
          "original_form": "이어폰 케이스들도 있었어",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.15",
          "form": "실용성이 중요해",
          "original_form": "실용성이 중요해",
          "speaker_id": "1",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.16",
          "form": "너무 성의가 없는 디자인이었어",
          "original_form": "너무 성의가 없는 디자인이었어",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.17",
          "form": "내가 만드는 게 낫겠어",
          "original_form": "내가 만드는 게 낫겠어",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.18",
          "form": "왜그랬을까...",
          "original_form": "왜그랬을까...",
          "speaker_id": "1",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.19",
          "form": "팬들은 못생겨도 사서 그런가?",
          "original_form": "팬들은 못생겨도 사서 그런가?",
          "speaker_id": "1",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.20",
          "form": "그러게... 센스가 없는 판매자인가봐",
          "original_form": "그러게... 센스가 없는 판매자인가봐",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.21",
          "form": "그건 팬들을 기만하는거야.",
          "original_form": "그건 팬들을 기만하는거야.",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036474.1.22",
          "form": "이왕 만드는거 예쁘게 만들지 ㅠ",
          "original_form": "이왕 만드는거 예쁘게 만들지 ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:31"
        }
      ]
