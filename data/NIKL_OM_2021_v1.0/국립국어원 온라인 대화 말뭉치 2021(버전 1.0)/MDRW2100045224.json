      "utterance": [
        {
          "id": "MDRW2100045224.1.1",
          "form": "먹는 거 좋아하세요?",
          "original_form": "먹는 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045224.1.2",
          "form": "환장하죠",
          "original_form": "환장하죠",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045224.1.3",
          "form": "뭐 좋아하세요",
          "original_form": "뭐 좋아하세요",
          "speaker_id": "1",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045224.1.4",
          "form": "떡볶이요",
          "original_form": "떡볶이요",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045224.1.5",
          "form": "님은여?",
          "original_form": "님은여?",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045224.1.6",
          "form": "헐 저 떡볶이 환장이요",
          "original_form": "헐 저 떡볶이 환장이요",
          "speaker_id": "1",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045224.1.7",
          "form": "긱사 살 때 친구들이랑 떡볶이 자주 먹었는데",
          "original_form": "긱사 살 때 친구들이랑 떡볶이 자주 먹었는데",
          "speaker_id": "1",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045224.1.8",
          "form": "하..긱사가서 시켜먹는음식른",
          "original_form": "하..긱사가서 시켜먹는음식른",
          "speaker_id": "2",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045224.1.9",
          "form": "뭐든 맛잇러요 진짜",
          "original_form": "뭐든 맛잇러요 진짜",
          "speaker_id": "2",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045224.1.10",
          "form": "하 진짜 몰래 먹는 라면이 진짜 맛있거든요",
          "original_form": "하 진짜 몰래 먹는 라면이 진짜 맛있거든요",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045224.1.11",
          "form": "같이 떡볶이 먹고싶어요",
          "original_form": "같이 떡볶이 먹고싶어요",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045224.1.12",
          "form": "라면 신볶드시라고요",
          "original_form": "라면 신볶드시라고요",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045224.1.13",
          "form": "그게 신라면 떡볶이는 아니잖아요",
          "original_form": "그게 신라면 떡볶이는 아니잖아요",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045224.1.14",
          "form": "납작당면 불닭도 궁금해여",
          "original_form": "납작당면 불닭도 궁금해여",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045224.1.15",
          "form": "그때 그 초록색 불닭 맛있었는데",
          "original_form": "그때 그 초록색 불닭 맛있었는데",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045224.1.16",
          "form": "단종됐나요",
          "original_form": "단종됐나요",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045224.1.17",
          "form": "초록색짜장이요?",
          "original_form": "초록색짜장이요?",
          "speaker_id": "2",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045224.1.18",
          "form": "있지않을까요 불닭먹고싶네요",
          "original_form": "있지않을까요 불닭먹고싶네요",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045224.1.19",
          "form": "짜장말고 쫄면 ??",
          "original_form": "짜장말고 쫄면 ??",
          "speaker_id": "1",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045224.1.20",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045224.1.21",
          "form": "그거 단종아닌뎅",
          "original_form": "그거 단종아닌뎅",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045224.1.22",
          "form": "엥 본적이어보어",
          "original_form": "엥 본적이어보어",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045224.1.23",
          "form": "사다줘",
          "original_form": "사다줘",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045224.1.24",
          "form": "쫄면? 울동네오세여",
          "original_form": "쫄면? 울동네오세여",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045224.1.25",
          "form": "있던거같은뎅",
          "original_form": "있던거같은뎅",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045224.1.26",
          "form": "갈게여 쫌만 기다려오ㅓ",
          "original_form": "갈게여 쫌만 기다려오ㅓ",
          "speaker_id": "1",
          "time": "20210908 01:37"
        }
      ]
