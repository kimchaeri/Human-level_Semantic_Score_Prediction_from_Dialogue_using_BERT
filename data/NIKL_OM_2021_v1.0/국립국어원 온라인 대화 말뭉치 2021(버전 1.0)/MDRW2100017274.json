      "utterance": [
        {
          "id": "MDRW2100017274.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017274.1.2",
          "form": "영화 좋아해요!",
          "original_form": "영화 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017274.1.3",
          "form": "어떤 장르 좋아하세요?",
          "original_form": "어떤 장르 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017274.1.4",
          "form": "전 코미디나 로코요!",
          "original_form": "전 코미디나 로코요!",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017274.1.5",
          "form": "name1님은 어떤 장르 좋아하세요?",
          "original_form": "&name1&님은 어떤 장르 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017274.1.6",
          "form": "저는 공포만 아니면 다 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "저는 공포만 아니면 다 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017274.1.7",
          "form": "그럼 인생영화가 뭐예요?",
          "original_form": "그럼 인생영화가 뭐예요?",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017274.1.8",
          "form": "저 신과함께요!",
          "original_form": "저 신과함께요!",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017274.1.9",
          "form": "신과함께 좋아하시는구나",
          "original_form": "신과함께 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017274.1.10",
          "form": "저 신과함께 웹툰은 봤는데 정작 영화는 아직 못봣어요 ㅋㅋㅋㅋ",
          "original_form": "저 신과함께 웹툰은 봤는데 정작 영화는 아직 못봣어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017274.1.11",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017274.1.12",
          "form": "신과함께 완전 슬프도 감동적이여서 제 인생영화에요",
          "original_form": "신과함께 완전 슬프도 감동적이여서 제 인생영화에요",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017274.1.13",
          "form": "네 어쩌다보니 아직 못봣네요 ㅋㅋㅋㅋㅋ 재밋다는 말은 많이들엇는데",
          "original_form": "네 어쩌다보니 아직 못봣네요 ㅋㅋㅋㅋㅋ 재밋다는 말은 많이들엇는데",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017274.1.14",
          "form": "재밌어요 나중에 한번 보세요!",
          "original_form": "재밌어요 나중에 한번 보세요!",
          "speaker_id": "2",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017274.1.15",
          "form": "name1님 인생영화는 어떤건가요?",
          "original_form": "&name1&님 인생영화는 어떤건가요?",
          "speaker_id": "2",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017274.1.16",
          "form": "저는 그래비티 좋아해요",
          "original_form": "저는 그래비티 좋아해요",
          "speaker_id": "1",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017274.1.17",
          "form": "오오 그래비티 들어는 봤는데 본적은 없어요!",
          "original_form": "오오 그래비티 들어는 봤는데 본적은 없어요!",
          "speaker_id": "2",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017274.1.18",
          "form": "우주영화예요",
          "original_form": "우주영화예요",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017274.1.19",
          "form": "오 재밌나요?",
          "original_form": "오 재밌나요?",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017274.1.20",
          "form": "단순히 우주영화가 아니고 꽤 감동적이기도 해서 좋아하실것같아요",
          "original_form": "단순히 우주영화가 아니고 꽤 감동적이기도 해서 좋아하실것같아요",
          "speaker_id": "1",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017274.1.21",
          "form": "오오 나중에 한번 봐볼게요!",
          "original_form": "오오 나중에 한번 봐볼게요!",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017274.1.22",
          "form": "로코 중에는 좋아하는 영화 뭐예요?",
          "original_form": "로코 중에는 좋아하는 영화 뭐예요?",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017274.1.23",
          "form": "키싱부스 재밌게봤어요!",
          "original_form": "키싱부스 재밌게봤어요!",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017274.1.24",
          "form": "오 키싱부스 재밋다는 얘기 많이들엇어요!",
          "original_form": "오 키싱부스 재밋다는 얘기 많이들엇어요!",
          "speaker_id": "1",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017274.1.25",
          "form": "키싱부스도 아직 안봣는데 봐야겟네요",
          "original_form": "키싱부스도 아직 안봣는데 봐야겟네요",
          "speaker_id": "1",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017274.1.26",
          "form": "3까지 나왔다고 들었어요!",
          "original_form": "3까지 나왔다고 들었어요!",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017274.1.27",
          "form": "하이틴로맨스인가요?",
          "original_form": "하이틴로맨스인가요?",
          "speaker_id": "1",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017274.1.28",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017274.1.29",
          "form": "막 간질간질한 설렘이 가득하겟네요",
          "original_form": "막 간질간질한 설렘이 가득하겟네요",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017274.1.30",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210826 18:51"
        },
        {
          "id": "MDRW2100017274.1.31",
          "form": "약간 그래요",
          "original_form": "약간 그래요",
          "speaker_id": "2",
          "time": "20210826 18:51"
        }
      ]
    }
