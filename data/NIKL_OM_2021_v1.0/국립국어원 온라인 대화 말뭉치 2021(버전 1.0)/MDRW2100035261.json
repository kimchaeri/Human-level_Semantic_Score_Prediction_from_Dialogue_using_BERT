      "utterance": [
        {
          "id": "MDRW2100035261.1.1",
          "form": "일 하시나요?",
          "original_form": "일 하시나요?",
          "speaker_id": "1",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100035261.1.2",
          "form": "네 일하고있어요",
          "original_form": "네 일하고있어요",
          "speaker_id": "2",
          "time": "20210829 12:59"
        },
        {
          "id": "MDRW2100035261.1.3",
          "form": "만족하시나요?",
          "original_form": "만족하시나요?",
          "speaker_id": "1",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100035261.1.4",
          "form": "음 ㅋㅋ 글쎄요 아니요",
          "original_form": "음 ㅋㅋ 글쎄요 아니요",
          "speaker_id": "2",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100035261.1.5",
          "form": "가장 스트레스 받는 부분은 뭔가요?",
          "original_form": "가장 스트레스 받는 부분은 뭔가요?",
          "speaker_id": "1",
          "time": "20210830 07:59"
        },
        {
          "id": "MDRW2100035261.1.6",
          "form": "그냥 가정주부로서 아이와ㅡ시간을 더 보내고 싶은것같아요",
          "original_form": "그냥 가정주부로서 아이와ㅡ시간을 더 보내고 싶은것같아요",
          "speaker_id": "2",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100035261.1.7",
          "form": "하지만 그러기엔 여유가ㅡ없어서 ㅎㅎ",
          "original_form": "하지만 그러기엔 여유가ㅡ없어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100035261.1.8",
          "form": "일 하세요?",
          "original_form": "일 하세요?",
          "speaker_id": "2",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100035261.1.9",
          "form": "네~ 십년 넘게 일하는 중이예요",
          "original_form": "네~ 십년 넘게 일하는 중이예요",
          "speaker_id": "1",
          "time": "20210830 17:08"
        },
        {
          "id": "MDRW2100035261.1.10",
          "form": "한 직장에서 십년이나요?",
          "original_form": "한 직장에서 십년이나요?",
          "speaker_id": "2",
          "time": "20210831 14:03"
        },
        {
          "id": "MDRW2100035261.1.11",
          "form": "지점 이동은 두번해서 세번째 지점이예요",
          "original_form": "지점 이동은 두번해서 세번째 지점이예요",
          "speaker_id": "1",
          "time": "20210831 20:31"
        },
        {
          "id": "MDRW2100035261.1.12",
          "form": "아 오래하셨네요 요새 센스있는 가게들도 많아서 그런것도 해보고싶은데 현실은 월급쟁이에요",
          "original_form": "아 오래하셨네요 요새 센스있는 가게들도 많아서 그런것도 해보고싶은데 현실은 월급쟁이에요",
          "speaker_id": "2",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100035261.1.13",
          "form": "저도 여행다니며 놀고 싶은데 현실은 월급쟁이네요 ㅠ",
          "original_form": "저도 여행다니며 놀고 싶은데 현실은 월급쟁이네요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100035261.1.14",
          "form": "수명이 늘어서 언제까자 일을 해야할지도 ㅋㅋ",
          "original_form": "수명이 늘어서 언제까자 일을 해야할지도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100035261.1.15",
          "form": "악!! 하긴 연금도 저희가 퇴직할때 되면 줄겠죠 ?",
          "original_form": "악!! 하긴 연금도 저희가 퇴직할때 되면 줄겠죠 ?",
          "speaker_id": "1",
          "time": "20210902 07:45"
        },
        {
          "id": "MDRW2100035261.1.16",
          "form": "어흑 그것도 그것나름대로 슬픈 일개미 이야기에여",
          "original_form": "어흑 그것도 그것나름대로 슬픈 일개미 이야기에여",
