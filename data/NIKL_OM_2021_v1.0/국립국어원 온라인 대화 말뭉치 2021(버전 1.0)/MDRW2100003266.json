        {
          "id": "MDRW2100003266.1.1",
          "form": "쉑쉑버거 먹어봤어?",
          "original_form": "쉑쉑버거 먹어봤어?",
          "speaker_id": "2",
          "time": "20210820 21:09"
        },
        {
          "id": "MDRW2100003266.1.2",
          "form": "응 옛날에 미국 뉴욕에서 먹어봤어",
          "original_form": "응 옛날에 미국 뉴욕에서 먹어봤어",
          "speaker_id": "1",
          "time": "20210820 21:10"
        },
        {
          "id": "MDRW2100003266.1.3",
          "form": "진짜 맛있어",
          "original_form": "진짜 맛있어",
          "speaker_id": "2",
          "time": "20210820 21:10"
        },
        {
          "id": "MDRW2100003266.1.4",
          "form": "맛아 맛있더라",
          "original_form": "맛아 맛있더라",
          "speaker_id": "1",
          "time": "20210820 21:10"
        },
        {
          "id": "MDRW2100003266.1.5",
          "form": "한국에 첨 들어왔을 때 줄 엄청 길더라",
          "original_form": "한국에 첨 들어왔을 때 줄 엄청 길더라",
          "speaker_id": "1",
          "time": "20210820 21:10"
        },
        {
          "id": "MDRW2100003266.1.6",
          "form": "그니까",
