      "utterance": [
        {
          "id": "MDRW2100027993.1.1",
          "form": "문피아여서 걍... 표지만 잘 내줬으면 좋겠다",
          "original_form": "문피아여서 걍... 표지만 잘 내줬으면 좋겠다",
          "speaker_id": "1",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027993.1.2",
          "form": "그냥 까맣게 내줘도 되는데...",
          "original_form": "그냥 까맣게 내줘도 되는데...",
          "speaker_id": "2",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100027993.1.3",
          "form": "또 나쁜말 썻대",
          "original_form": "또 나쁜말 썻대",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100027993.1.4",
          "form": "과하게 해서 이상하게 나올까바 걱정임",
          "original_form": "과하게 해서 이상하게 나올까바 걱정임",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100027993.1.5",
          "form": "문피아 (나쁜말)",
          "original_form": "문피아 (나쁜말)",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100027993.1.6",
          "form": "필터링 되는 말 썼냐궄ㅋㅋㅋㅋㅋㅋ",
          "original_form": "필터링 되는 말 썼냐궄ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100027993.1.7",
          "form": "욕도 아닌데 자꾸 필터링 되서...",
          "original_form": "욕도 아닌데 자꾸 필터링 되서...",
          "speaker_id": "2",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027993.1.8",
          "form": "기준 모르겠다",
          "original_form": "기준 모르겠다",
          "speaker_id": "2",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027993.1.9",
          "form": "이번에 섭남파업 일러 펑크 난 일 알아?",
          "original_form": "이번에 섭남파업 일러 펑크 난 일 알아?",
          "speaker_id": "1",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100027993.1.10",
          "form": "아니 모르겠는데",
          "original_form": "아니 모르겠는데",
          "speaker_id": "2",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100027993.1.11",
          "form": "예전에 일러레들 터진거랑 같은 느낌인가?",
          "original_form": "예전에 일러레들 터진거랑 같은 느낌인가?",
          "speaker_id": "2",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100027993.1.12",
          "form": "ㅇㅇ 마감 안 지키고 튀었대",
          "original_form": "ㅇㅇ 마감 안 지키고 튀었대",
          "speaker_id": "1",
          "time": "20210902 16:55"
        },
        {
          "id": "MDRW2100027993.1.13",
          "form": "왜 그런데ㅋㅋㅋ 돈은 받았을거 아냐",
          "original_form": "왜 그런데ㅋㅋㅋ 돈은 받았을거 아냐",
          "speaker_id": "2",
          "time": "20210902 17:02"
        },
        {
          "id": "MDRW2100027993.1.14",
          "form": "그치 계약금은 받았겠지",
          "original_form": "그치 계약금은 받았겠지",
          "speaker_id": "1",
          "time": "20210902 17:02"
        },
        {
          "id": "MDRW2100027993.1.15",
          "form": "사기꾼들이 많구나",
          "original_form": "사기꾼들이 많구나",
          "speaker_id": "2",
          "time": "20210902 17:02"
        },
        {
          "id": "MDRW2100027993.1.16",
          "form": "이번에 자까님이 공론화까진 아니고",
          "original_form": "이번에 자까님이 공론화까진 아니고",
          "speaker_id": "1",
          "time": "20210902 17:03"
        },
        {
          "id": "MDRW2100027993.1.17",
          "form": "(일러레 이름을 안 까서",
          "original_form": "(일러레 이름을 안 까서",
          "speaker_id": "1",
          "time": "20210902 17:03"
        },
        {
          "id": "MDRW2100027993.1.18",
          "form": "추석 선물로 보여드리려고 했는데... 하시면서 올리셨드라",
          "original_form": "추석 선물로 보여드리려고 했는데... 하시면서 올리셨드라",
          "speaker_id": "1",
          "time": "20210902 17:04"
        },
        {
          "id": "MDRW2100027993.1.19",
          "form": "ㅠㅠ근데 저번에도 그렇구",
          "original_form": "ㅠㅠ근데 저번에도 그렇구",
          "speaker_id": "2",
          "time": "20210902 17:04"
        },
        {
          "id": "MDRW2100027993.1.20",
          "form": "일러레 잠수타면 조치 하긴 하나?",
          "original_form": "일러레 잠수타면 조치 하긴 하나?",
          "speaker_id": "2",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100027993.1.21",
          "form": "딱히 할게 없대",
          "original_form": "딱히 할게 없대",
          "speaker_id": "1",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100027993.1.22",
          "form": "자까랑 계약한게 아니라 고소도 못 하고....",
          "original_form": "자까랑 계약한게 아니라 고소도 못 하고....",
          "speaker_id": "1",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100027993.1.23",
          "form": "출판사랑 계약하는 거라 작가가 고소 못하고",
          "original_form": "출판사랑 계약하는 거라 작가가 고소 못하고",
          "speaker_id": "1",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100027993.1.24",
          "form": "공론화도 일러레가 피통이 더 쎄서",
          "original_form": "공론화도 일러레가 피통이 더 쎄서",
          "speaker_id": "1",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100027993.1.25",
          "form": "딱히 할게 없는 모양...",
          "original_form": "딱히 할게 없는 모양...",
          "speaker_id": "1",
          "time": "20210902 17:06"
        }
      ]
    }
