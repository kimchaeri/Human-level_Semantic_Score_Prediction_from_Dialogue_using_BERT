      "utterance": [
        {
          "id": "MDRW2100030503.1.1",
          "form": "그쵸ㅋㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:16"
        },
        {
          "id": "MDRW2100030503.1.2",
          "form": "대학생인데 교재값이나 학비가 엄청들더라구요",
          "original_form": "대학생인데 교재값이나 학비가 엄청들더라구요",
          "speaker_id": "2",
          "time": "20210904 02:16"
        },
        {
          "id": "MDRW2100030503.1.3",
          "form": "그래서 시작하게됐어요",
          "original_form": "그래서 시작하게됐어요",
          "speaker_id": "2",
          "time": "20210904 02:16"
        },
        {
          "id": "MDRW2100030503.1.4",
          "form": "아이구.. 그래도 공부 잘하시나봐요 ㅎㅎ 수학과외라니.. 멋져요",
          "original_form": "아이구.. 그래도 공부 잘하시나봐요 ㅎㅎ 수학과외라니.. 멋져요",
          "speaker_id": "1",
          "time": "20210904 02:17"
        },
        {
          "id": "MDRW2100030503.1.5",
          "form": "저는 상상도 못할 일이네요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 상상도 못할 일이네요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:17"
        },
        {
          "id": "MDRW2100030503.1.6",
          "form": "수포자였어가지고",
          "original_form": "수포자였어가지고",
          "speaker_id": "1",
          "time": "20210904 02:17"
        },
        {
          "id": "MDRW2100030503.1.7",
          "form": "아녜요ㅋ큐ㅠㅜ 저도 수능끝난뒤로 공부안했더니",
          "original_form": "아녜요ㅋ큐ㅠㅜ 저도 수능끝난뒤로 공부안했더니",
          "speaker_id": "2",
          "time": "20210904 02:18"
        },
        {
          "id": "MDRW2100030503.1.8",
          "form": "기억안나서 매번 답지 외워가서 뱉고오는 느낌",
          "original_form": "기억안나서 매번 답지 외워가서 뱉고오는 느낌",
          "speaker_id": "2",
          "time": "20210904 02:18"
        },
        {
          "id": "MDRW2100030503.1.9",
          "form": "날씨얘기해요!",
          "original_form": "날씨얘기해요!",
          "speaker_id": "2",
          "time": "20210904 02:18"
        },
        {
          "id": "MDRW2100030503.1.10",
          "form": "내일날씨도 오늘 같았으면 좋겠어요",
          "original_form": "내일날씨도 오늘 같았으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210904 02:18"
        },
        {
          "id": "MDRW2100030503.1.11",
          "form": "요즘 날씨 선선해져서 너무 좋아요",
          "original_form": "요즘 날씨 선선해져서 너무 좋아요",
          "speaker_id": "1",
          "time": "20210904 03:33"
        },
        {
          "id": "MDRW2100030503.1.12",
          "form": "가을날씨..",
          "original_form": "가을날씨..",
          "speaker_id": "1",
          "time": "20210904 03:33"
        },
        {
          "id": "MDRW2100030503.1.13",
          "form": "서늘한 바람 불 때마다 설레지 않나요 ㅎㅎ",
          "original_form": "서늘한 바람 불 때마다 설레지 않나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 03:33"
        },
        {
          "id": "MDRW2100030503.1.14",
          "form": "맞아요 여름에 고생하고 가을 날씨에 위로받는 느낌..",
          "original_form": "맞아요 여름에 고생하고 가을 날씨에 위로받는 느낌..",
          "speaker_id": "2",
          "time": "20210904 03:34"
        },
        {
          "id": "MDRW2100030503.1.15",
          "form": "가을하늘도 깨끗해서 너무 기분 좋더라구요",
          "original_form": "가을하늘도 깨끗해서 너무 기분 좋더라구요",
          "speaker_id": "2",
          "time": "20210904 03:34"
        },
        {
          "id": "MDRW2100030503.1.16",
          "form": "한동안 계속 장마로 비왔어서 그런지 더 좋았어요",
          "original_form": "한동안 계속 장마로 비왔어서 그런지 더 좋았어요",
          "speaker_id": "2",
          "time": "20210904 03:34"
        },
        {
          "id": "MDRW2100030503.1.17",
          "form": "맞아요 이번 여름 특히 더 더웠어서 ㅜㅜ 가을 오는게 더 반가워요",
          "original_form": "맞아요 이번 여름 특히 더 더웠어서 ㅜㅜ 가을 오는게 더 반가워요",
          "speaker_id": "1",
          "time": "20210904 03:35"
        },
        {
          "id": "MDRW2100030503.1.18",
          "form": "빨리 맨투맨 입고 돌아다니고 싶네요 ㅎㅎ",
          "original_form": "빨리 맨투맨 입고 돌아다니고 싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 03:36"
        },
        {
          "id": "MDRW2100030503.1.19",
          "form": "맨투맨 좋죠ㅜㅠ 편하고",
          "original_form": "맨투맨 좋죠ㅜㅠ 편하고",
          "speaker_id": "2",
          "time": "20210904 03:40"
        },
        {
          "id": "MDRW2100030503.1.20",
          "form": "저는 조금 도톰한 가디건이 입고싶네요",
          "original_form": "저는 조금 도톰한 가디건이 입고싶네요",
          "speaker_id": "2",
          "time": "20210904 03:40"
        },
        {
          "id": "MDRW2100030503.1.21",
          "form": "가디건도 좋죠 ㅎㅎ 포근하고",
          "original_form": "가디건도 좋죠 ㅎㅎ 포근하고",
          "speaker_id": "1",
          "time": "20210904 03:44"
        },
        {
          "id": "MDRW2100030503.1.22",
          "form": "요즘 날씨가 좋아서",
          "original_form": "요즘 날씨가 좋아서",
          "speaker_id": "1",
          "time": "20210904 03:45"
        },
        {
          "id": "MDRW2100030503.1.23",
          "form": "산책다니기 딱 좋을 것 같아요",
          "original_form": "산책다니기 딱 좋을 것 같아요",
          "speaker_id": "1",
          "time": "20210904 03:45"
        },
        {
          "id": "MDRW2100030503.1.24",
          "form": "그러게요 미세먼지도 나쁘지 않은 것 같더라구요",
          "original_form": "그러게요 미세먼지도 나쁘지 않은 것 같더라구요",
          "speaker_id": "2",
          "time": "20210904 04:03"
        },
        {
          "id": "MDRW2100030503.1.25",
          "form": "이번여름이 유독더웠듯이",
          "original_form": "이번여름이 유독더웠듯이",
          "speaker_id": "2",
          "time": "20210904 04:03"
        },
        {
          "id": "MDRW2100030503.1.26",
          "form": "겨울은 또 유독 추울까봐",
          "original_form": "겨울은 또 유독 추울까봐",
          "speaker_id": "2",
          "time": "20210904 04:03"
        },
        {
          "id": "MDRW2100030503.1.27",
          "form": "걱정이네요",
          "original_form": "걱정이네요",
          "speaker_id": "2",
          "time": "20210904 04:03"
        },
        {
          "id": "MDRW2100030503.1.28",
          "form": "오늘도 날씨가 엄청 좋아요",
          "original_form": "오늘도 날씨가 엄청 좋아요",
          "speaker_id": "2",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100030503.1.29",
          "form": "그러게요 19년 겨울이 별로 안추웠었는데 ㅎㅎ 이상기온이긴 하지만 그때같았으면 좋겠어요",
          "original_form": "그러게요 19년 겨울이 별로 안추웠었는데 ㅎㅎ 이상기온이긴 하지만 그때같았으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100030503.1.30",
          "form": "20년 2월에 막 긴팔티 하나만 입고 돌아다니고 그랬었는데..",
          "original_form": "20년 2월에 막 긴팔티 하나만 입고 돌아다니고 그랬었는데..",
          "speaker_id": "1",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100030503.1.31",
          "form": "앗 맞아요ㅋㅋㅋ 시원하고 좋았는데",
          "original_form": "앗 맞아요ㅋㅋㅋ 시원하고 좋았는데",
          "speaker_id": "2",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100030503.1.32",
          "form": "혹시 더운날씨랑 추운 날씨중에는 어떤걸 선호하세요?",
          "original_form": "혹시 더운날씨랑 추운 날씨중에는 어떤걸 선호하세요?",
          "speaker_id": "2",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100030503.1.33",
          "form": "둘 다 싫지만.. 고르라면 추운 걸 고를래요 ㅋㅋㅋㅋ",
          "original_form": "둘 다 싫지만.. 고르라면 추운 걸 고를래요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:05"
        },
        {
          "id": "MDRW2100030503.1.34",
          "form": "여름은 너무 지독해서..",
          "original_form": "여름은 너무 지독해서..",
          "speaker_id": "1",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100030503.1.35",
          "form": "저두요!!",
          "original_form": "저두요!!",
          "speaker_id": "2",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100030503.1.36",
          "form": "추우면입으면되지만 여름은 별수없이 더워야해서 힘들더라구요",
          "original_form": "추우면입으면되지만 여름은 별수없이 더워야해서 힘들더라구요",
          "speaker_id": "2",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100030503.1.37",
          "form": "글고 한국 여름은 습도도 너무 높아서 더 불쾌해요 차라리 추워서 떨고 말지.. ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "글고 한국 여름은 습도도 너무 높아서 더 불쾌해요 차라리 추워서 떨고 말지.. ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:10"
        }
      ]
    }
