        },
        {
          "id": "MDRW2100017203.1.2",
          "form": "아 아니욬ㅋㅋㅋㅋ",
          "original_form": "아 아니욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017203.1.3",
          "form": "name1님은 하는거 있으세요",
          "original_form": "&name1&님은 하는거 있으세요",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017203.1.4",
          "form": "크크 아뇨",
          "original_form": "크크 아뇨",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017203.1.5",
          "form": "내성적이고 낯가려서 그런데 안가게되더라구용!!",
          "original_form": "내성적이고 낯가려서 그런데 안가게되더라구용!!",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017203.1.6",
          "form": "근데 보면 관심사 같은사람들 모여서 금방친해지기도하구 그러더라구용",
          "original_form": "근데 보면 관심사 같은사람들 모여서 금방친해지기도하구 그러더라구용",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017203.1.7",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017203.1.8",
          "form": "ㅋㅋㅋㅋ해보고싶으신거 있으세요?",
          "original_form": "ㅋㅋㅋㅋ해보고싶으신거 있으세요?",
          "speaker_id": "1",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017203.1.9",
          "form": "아녀 딱히 없어여",
          "original_form": "아녀 딱히 없어여",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100017203.1.10",
          "form": "name1님은요??",
