      "utterance": [
        {
          "id": "MDRW2100039999.1.1",
          "form": "백신맞으셧어요?",
          "original_form": "백신맞으셧어요?",
          "speaker_id": "1",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100039999.1.2",
          "form": "네 맞았어요!",
          "original_form": "네 맞았어요!",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100039999.1.3",
          "form": "전 오늘 오전에 모더나 맞고왔는데",
          "original_form": "전 오늘 오전에 모더나 맞고왔는데",
          "speaker_id": "1",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100039999.1.4",
          "form": "팔이꽤아프네요ㅠ",
          "original_form": "팔이꽤아프네요ㅠ",
          "speaker_id": "1",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100039999.1.5",
          "form": "아 팔쓰면 안되시는데!!!",
          "original_form": "아 팔쓰면 안되시는데!!!",
          "speaker_id": "2",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100039999.1.6",
          "form": "1차요?",
          "original_form": "1차요?",
          "speaker_id": "2",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100039999.1.7",
          "form": "네네 1차맞고왔어요!",
          "original_form": "네네 1차맞고왔어요!",
          "speaker_id": "1",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100039999.1.8",
          "form": "저는 지난주 금요일에!!",
          "original_form": "저는 지난주 금요일에!!",
          "speaker_id": "2",
          "time": "20210904 07:37"
        },
        {
          "id": "MDRW2100039999.1.9",
          "form": "괜찮으셨나요ㅠ?",
          "original_form": "괜찮으셨나요ㅠ?",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100039999.1.10",
          "form": "이틀동안만!! 팔아프고 멀쩡해요!",
          "original_form": "이틀동안만!! 팔아프고 멀쩡해요!",
          "speaker_id": "2",
          "time": "20210904 11:15"
        },
        {
          "id": "MDRW2100039999.1.11",
          "form": "오늘은 어제보다 아프죠??ㅜ",
          "original_form": "오늘은 어제보다 아프죠??ㅜ",
          "speaker_id": "2",
          "time": "20210904 11:15"
        },
        {
          "id": "MDRW2100039999.1.12",
          "form": "팔은꽤아프네여ㅠㅜ",
          "original_form": "팔은꽤아프네여ㅠㅜ",
          "speaker_id": "1",
          "time": "20210904 11:15"
        },
        {
          "id": "MDRW2100039999.1.13",
          "form": "내일까지만 버티면 괜찮겟죠ㅠㅠ",
          "original_form": "내일까지만 버티면 괜찮겟죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100039999.1.14",
          "form": "네네! 오늘 하루만 지남 진짜 깜쪽같이 사라지더라고요",
          "original_form": "네네! 오늘 하루만 지남 진짜 깜쪽같이 사라지더라고요",
          "speaker_id": "2",
          "time": "20210904 11:19"
        },
        {
          "id": "MDRW2100039999.1.15",
          "form": "큰탈없이 지나가서 다행이에요!!",
          "original_form": "큰탈없이 지나가서 다행이에요!!",
          "speaker_id": "1",
          "time": "20210904 11:20"
        },
        {
          "id": "MDRW2100039999.1.16",
          "form": "네 오늘 무거운거 들지마시고 커피랑 술 주의하라고 하더라고요",
          "original_form": "네 오늘 무거운거 들지마시고 커피랑 술 주의하라고 하더라고요",
          "speaker_id": "2",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100039999.1.17",
          "form": "커피는 말없던데ㅠㅠ 술만 2틀마시지말라하고ㅠㅠ",
          "original_form": "커피는 말없던데ㅠㅠ 술만 2틀마시지말라하고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100039999.1.18",
          "form": "ㅋㅋ안그래도 오늘 친구가 소아과\r\n맞으면서 물어보니까 의사분이 굉장히 미심쩍어했다고ㅜㅋㅋ",
          "original_form": "ㅋㅋ안그래도 오늘 친구가 소아과\r\n맞으면서 물어보니까 의사분이 굉장히 미심쩍어했다고ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100039999.1.19",
          "form": "먹어도 되나봐요ㅜㅋㅋ",
          "original_form": "먹어도 되나봐요ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:20"
        }
      ]
