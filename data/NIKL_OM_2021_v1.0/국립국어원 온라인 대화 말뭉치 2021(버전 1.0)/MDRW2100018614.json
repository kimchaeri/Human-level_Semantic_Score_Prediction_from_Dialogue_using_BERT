        {
          "id": "MDRW2100018614.1.1",
          "form": "뭐하냐?",
          "original_form": "뭐하냐?",
          "speaker_id": "1",
          "time": "20210827 09:07"
        },
        {
          "id": "MDRW2100018614.1.2",
          "form": "나 어제 설거지 안 함ㅋ",
          "original_form": "나 어제 설거지 안 함ㅋ",
          "speaker_id": "2",
          "time": "20210827 09:12"
        },
        {
          "id": "MDRW2100018614.1.3",
          "form": "그냥 잤어",
          "original_form": "그냥 잤어",
          "speaker_id": "2",
          "time": "20210827 09:12"
        },
        {
          "id": "MDRW2100018614.1.4",
          "form": "알아",
          "original_form": "알아",
          "speaker_id": "1",
          "time": "20210827 09:13"
        },
        {
          "id": "MDRW2100018614.1.5",
          "form": "엄마가 하고 감",
          "original_form": "엄마가 하고 감",
          "speaker_id": "1",
          "time": "20210827 09:13"
        },
        {
          "id": "MDRW2100018614.1.6",
          "form": "오늘은 해라",
          "original_form": "오늘은 해라",
          "speaker_id": "1",
          "time": "20210827 09:14"
        },
        {
          "id": "MDRW2100018614.1.7",
          "form": "맛있는 거 사오셈",
          "original_form": "맛있는 거 사오셈",
          "speaker_id": "1",
          "time": "20210827 09:14"
        },
        {
          "id": "MDRW2100018614.1.8",
          "form": "웅 싫어^^",
          "original_form": "웅 싫어^^",
          "speaker_id": "2",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018614.1.9",
          "form": "가족들이 설거지 좀 할 수 있지",
          "original_form": "가족들이 설거지 좀 할 수 있지",
          "speaker_id": "2",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018614.1.10",
          "form": "뭐래니",
          "original_form": "뭐래니",
          "speaker_id": "1",
          "time": "20210827 11:14"
        },
        {
          "id": "MDRW2100018614.1.11",
          "form": "어이없어",
          "original_form": "어이없어",
          "speaker_id": "1",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018614.1.12",
          "form": "야",
          "original_form": "야",
          "speaker_id": "1",
          "time": "20210827 11:19"
        },
        {
          "id": "MDRW2100018614.1.13",
          "form": "답장 보내",
          "original_form": "답장 보내",
          "speaker_id": "1",
          "time": "20210827 11:19"
        },
        {
          "id": "MDRW2100018614.1.14",
          "form": "방 청소도 좀 하고 살아",
          "original_form": "방 청소도 좀 하고 살아",
          "speaker_id": "2",
          "time": "20210827 11:20"
        },
        {
          "id": "MDRW2100018614.1.15",
          "form": "집이 머글머글하다 얘",
          "original_form": "집이 머글머글하다 얘",
          "speaker_id": "2",
          "time": "20210827 11:20"
        },
        {
          "id": "MDRW2100018614.1.16",
          "form": "네가 해",
          "original_form": "네가 해",
          "speaker_id": "1",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018614.1.17",
          "form": "name2야 뭐해?",
          "original_form": "&name2&야 뭐해?",
          "speaker_id": "1",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018614.1.18",
          "form": "name2야 식빵구워",
          "original_form": "&name2&야 식빵구워",
          "speaker_id": "2",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018614.1.19",
          "form": "그리고 청소 좀 해",
          "original_form": "그리고 청소 좀 해",
          "speaker_id": "2",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018614.1.20",
