        {
          "id": "MDRW2100023935.1.21",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100023935.1.22",
          "form": "케이스로 탕진할일은없으니",
          "original_form": "케이스로 탕진할일은없으니",
          "speaker_id": "1",
          "time": "20210831 18:53"
