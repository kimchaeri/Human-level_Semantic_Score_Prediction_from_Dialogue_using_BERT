      "utterance": [
        {
          "id": "MDRW2100023935.1.1",
          "form": "최근 선물하시거나 받은거있나요?",
          "original_form": "최근 선물하시거나 받은거있나요?",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100023935.1.2",
          "form": "저 8월에 생일이 있어서",
          "original_form": "저 8월에 생일이 있어서",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100023935.1.3",
          "form": "치킨쿠폰도받고 파바쿠폰도받고 투썸케이크도받고 화장품도받고 배라쿠폰도 받았어용",
          "original_form": "치킨쿠폰도받고 파바쿠폰도받고 투썸케이크도받고 화장품도받고 배라쿠폰도 받았어용",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100023935.1.4",
          "form": "진짜 많이받으셨네요",
          "original_form": "진짜 많이받으셨네요",
          "speaker_id": "1",
          "time": "20210831 16:05"
        },
        {
          "id": "MDRW2100023935.1.5",
          "form": "근데 막상 쓸때보니 투썸케이크는",
          "original_form": "근데 막상 쓸때보니 투썸케이크는",
          "speaker_id": "2",
          "time": "20210831 18:10"
        },
        {
          "id": "MDRW2100023935.1.6",
          "form": "아이들이랑 같이먹었는데 너무 심하게달고",
          "original_form": "아이들이랑 같이먹었는데 너무 심하게달고",
          "speaker_id": "2",
          "time": "20210831 18:10"
        },
        {
          "id": "MDRW2100023935.1.7",
          "form": "치킨은 2장받았는데 둘다 교촌치킨 쿠폰이었는데",
          "original_form": "치킨은 2장받았는데 둘다 교촌치킨 쿠폰이었는데",
          "speaker_id": "2",
          "time": "20210831 18:10"
        },
        {
          "id": "MDRW2100023935.1.8",
          "form": "너무 짰어요.ㅜㅜ",
          "original_form": "너무 짰어요.ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 18:10"
        },
        {
          "id": "MDRW2100023935.1.9",
          "form": "배라쿠폰도 2장인데 아직못쓰고있네요",
          "original_form": "배라쿠폰도 2장인데 아직못쓰고있네요",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100023935.1.10",
          "form": "아이들이 알레르기가 심해서....안습이네요",
          "original_form": "아이들이 알레르기가 심해서....안습이네요",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100023935.1.11",
          "form": "저 새폰 마련햐서",
          "original_form": "저 새폰 마련햐서",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100023935.1.12",
          "form": "케이스 배송기다리고있어요 ㅎㅎ",
          "original_form": "케이스 배송기다리고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100023935.1.13",
          "form": "아이쿠...ㅜㅜ",
          "original_form": "아이쿠...ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100023935.1.14",
          "form": "플립사셨나요?",
          "original_form": "플립사셨나요?",
          "speaker_id": "1",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100023935.1.15",
          "form": "네넵ㅎㅎㅎ",
          "original_form": "네넵ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100023935.1.16",
          "form": "플립3라벤더 샀어용!!",
          "original_form": "플립3라벤더 샀어용!!",
          "speaker_id": "2",
