        {
          "id": "MDRW2100023935.1.17",
          "form": "오왕",
          "original_form": "오왕",
          "speaker_id": "1",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100023935.1.18",
          "form": "돈쓰는 쇼핑은 언제나 즐겁죠 ㅎㅎㅎㅎ",
          "original_form": "돈쓰는 쇼핑은 언제나 즐겁죠 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100023935.1.19",
          "form": "플립3 케이스들이 예쁜게 많이나와섷",
          "original_form": "플립3 케이스들이 예쁜게 많이나와섷",
          "speaker_id": "2",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100023935.1.20",
          "form": "진짜 너무너무 다 구매하고파요 ㅋㅋ",
          "original_form": "진짜 너무너무 다 구매하고파요 ㅋㅋ",
          "speaker_id": "2",
