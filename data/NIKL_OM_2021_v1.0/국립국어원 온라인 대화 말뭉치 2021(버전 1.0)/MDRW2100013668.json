      "utterance": [
        {
          "id": "MDRW2100013668.1.1",
          "form": "전문적인게 좋은듯 해요",
          "original_form": "전문적인게 좋은듯 해요",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013668.1.2",
          "form": "여행은 좋아하시나요",
          "original_form": "여행은 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013668.1.3",
          "form": "여행좋아하죠ㅎ",
          "original_form": "여행좋아하죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013668.1.4",
          "form": "국내는 운전을 못해서 잘안다니고",
          "original_form": "국내는 운전을 못해서 잘안다니고",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013668.1.5",
          "form": "해외만 한번씩 나갔어요",
          "original_form": "해외만 한번씩 나갔어요",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013668.1.6",
          "form": "여행다녀오신데 있어요?",
          "original_form": "여행다녀오신데 있어요?",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013668.1.7",
          "form": "저도 해외 좋아하는데 코로나땜시ㅠㅠ",
          "original_form": "저도 해외 좋아하는데 코로나땜시ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013668.1.8",
          "form": "저도 재작년에가고 못갔네요",
          "original_form": "저도 재작년에가고 못갔네요",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013668.1.9",
          "form": "우울증 올듯ㅋ",
          "original_form": "우울증 올듯ㅋ",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013668.1.10",
          "form": "19년?",
          "original_form": "19년?",
          "speaker_id": "1",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013668.1.11",
          "form": "저도ㅋㅋㅋㅋ",
          "original_form": "저도ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013668.1.12",
          "form": "작년에 안갔으니 그렇게되나요?",
          "original_form": "작년에 안갔으니 그렇게되나요?",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013668.1.13",
          "form": "어디다녀오셨어요?",
          "original_form": "어디다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013668.1.14",
          "form": "작년에요? 네번 갔어요",
          "original_form": "작년에요? 네번 갔어요",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013668.1.15",
          "form": "작년에 여행을요?ㅎ",
          "original_form": "작년에 여행을요?ㅎ",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013668.1.16",
          "form": "해외여행말고 국내?",
          "original_form": "해외여행말고 국내?",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013668.1.17",
          "form": "아 재작년이요ㅋㅋㅋ",
          "original_form": "아 재작년이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013668.1.18",
          "form": "오 해외를  글케 많이요?",
          "original_form": "오 해외를  글케 많이요?",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013668.1.19",
          "form": "부자시다ㅎ",
          "original_form": "부자시다ㅎ",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013668.1.20",
          "form": "평일에 가면 싸요 ㅎㅎ",
          "original_form": "평일에 가면 싸요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100013668.1.21",
          "form": "짧게 슝슝",
          "original_form": "짧게 슝슝",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100013668.1.22",
          "form": "좋네요ㅎ",
          "original_form": "좋네요ㅎ",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100013668.1.23",
          "form": "어디어디갔다오셨어요?",
          "original_form": "어디어디갔다오셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100013668.1.24",
          "form": "저~~ 홍콩마카오 태국 베트남 대만이요 ㅋㅋ",
          "original_form": "저~~ 홍콩마카오 태국 베트남 대만이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100013668.1.25",
          "form": "많이다녀오셨네요",
          "original_form": "많이다녀오셨네요",
          "speaker_id": "2",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100013668.1.26",
          "form": "저도 다가본듯ㅎ",
          "original_form": "저도 다가본듯ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:14"
        }
      ]
    }
