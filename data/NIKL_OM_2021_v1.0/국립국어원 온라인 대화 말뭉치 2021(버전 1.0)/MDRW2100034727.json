        },
        {
          "id": "MDRW2100034727.1.3",
          "form": "뭐가 제일 갖고싶나요",
          "original_form": "뭐가 제일 갖고싶나요",
          "speaker_id": "1",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100034727.1.4",
          "form": "모니터요",
          "original_form": "모니터요",
          "speaker_id": "2",
          "time": "20210828 18:18"
        },
        {
          "id": "MDRW2100034727.1.5",
          "form": "오 저도 물어봐줘요",
          "original_form": "오 저도 물어봐줘요",
          "speaker_id": "1",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100034727.1.6",
          "form": "뭐가 제일 갖고싶죠?",
          "original_form": "뭐가 제일 갖고싶죠?",
          "speaker_id": "2",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100034727.1.7",
          "form": "집이요.",
          "original_form": "집이요.",
          "speaker_id": "1",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100034727.1.8",
          "form": "꿈이 크시네요",
          "original_form": "꿈이 크시네요",
          "speaker_id": "2",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100034727.1.9",
          "form": "개인 아파트 자가소유",
          "original_form": "개인 아파트 자가소유",
          "speaker_id": "1",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100034727.1.10",
          "form": "원래 꿈은 크게 잡아야해요",
          "original_form": "원래 꿈은 크게 잡아야해요",
          "speaker_id": "1",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100034727.1.11",
          "form": "어디지역요?",
          "original_form": "어디지역요?",
          "speaker_id": "2",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100034727.1.12",
          "form": "지하철 다니는 수도권이엇음 좋겟어요",
          "original_form": "지하철 다니는 수도권이엇음 좋겟어요",
          "speaker_id": "1",
          "time": "20210828 18:21"
