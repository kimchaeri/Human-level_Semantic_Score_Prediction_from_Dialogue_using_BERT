      "utterance": [
        {
          "id": "MDRW2100020665.1.1",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020665.1.2",
          "form": "혹시 이 과자도 아시나요",
          "original_form": "혹시 이 과자도 아시나요",
          "speaker_id": "2",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020665.1.3",
          "form": "아 갑자기 먹고싶다",
          "original_form": "아 갑자기 먹고싶다",
          "speaker_id": "1",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020665.1.4",
          "form": "그 맛이 생각나버렸어",
          "original_form": "그 맛이 생각나버렸어",
          "speaker_id": "1",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020665.1.5",
          "form": "어떤거요",
          "original_form": "어떤거요",
          "speaker_id": "1",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020665.1.6",
          "form": "제 최애과자는 사실",
          "original_form": "제 최애과자는 사실",
          "speaker_id": "2",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020665.1.7",
          "form": "바나나칩입니다",
          "original_form": "바나나칩입니다",
          "speaker_id": "2",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020665.1.8",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020665.1.9",
          "form": "바나나킥아니에요",
          "original_form": "바나나킥아니에요",
          "speaker_id": "2",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020665.1.10",
          "form": "바나나칩",
          "original_form": "바나나칩",
          "speaker_id": "2",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020665.1.11",
          "form": "달달함의 끝판왕",
          "original_form": "달달함의 끝판왕",
          "speaker_id": "1",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020665.1.12",
          "form": "그 바나나 말린거맞죠",
          "original_form": "그 바나나 말린거맞죠",
          "speaker_id": "1",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020665.1.13",
          "form": "오독오독",
          "original_form": "오독오독",
          "speaker_id": "1",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020665.1.14",
          "form": "네 바로 그거에요",
          "original_form": "네 바로 그거에요",
          "speaker_id": "2",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020665.1.15",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "1",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020665.1.16",
          "form": "저 처음에 그거 먹고",
          "original_form": "저 처음에 그거 먹고",
          "speaker_id": "2",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020665.1.17",
          "form": "맞구나 견과류안에 꼭 들어있는",
          "original_form": "맞구나 견과류안에 꼭 들어있는",
          "speaker_id": "1",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020665.1.18",
          "form": "신세계를 느꼈어요",
          "original_form": "신세계를 느꼈어요",
          "speaker_id": "2",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020665.1.19",
          "form": "사실 첨엔 안먹으려고 했거든요",
          "original_form": "사실 첨엔 안먹으려고 했거든요",
          "speaker_id": "2",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020665.1.20",
          "form": "진짜 맛있지않나요",
          "original_form": "진짜 맛있지않나요",
