      "utterance": [
        {
          "id": "MDRW2100035558.1.1",
          "form": "이제 봤넹ㅋㅋ어제 재미난거 봤나유",
          "original_form": "이제 봤넹ㅋㅋ어제 재미난거 봤나유",
          "speaker_id": "2",
          "time": "20210830 06:34"
        },
        {
          "id": "MDRW2100035558.1.2",
          "form": "그냥 뭉찻 ㅋㅋㅋ?",
          "original_form": "그냥 뭉찻 ㅋㅋㅋ?",
          "speaker_id": "1",
          "time": "20210830 07:13"
        },
        {
          "id": "MDRW2100035558.1.3",
          "form": "ㅋㅋㅋ차차차도 안보고 자버렸네 난",
          "original_form": "ㅋㅋㅋ차차차도 안보고 자버렸네 난",
          "speaker_id": "2",
          "time": "20210830 07:53"
        },
        {
          "id": "MDRW2100035558.1.4",
          "form": "나두안봣으 ㅋㅋㅋ아마 몰아볼꺼같으무",
          "original_form": "나두안봣으 ㅋㅋㅋ아마 몰아볼꺼같으무",
          "speaker_id": "1",
          "time": "20210830 07:53"
        },
        {
          "id": "MDRW2100035558.1.5",
          "form": "우리집 tv리모컨 사라짐ㅋㅋㅋ거참 진짜 아무리 찾아도 없네",
          "original_form": "우리집 tv리모컨 사라짐ㅋㅋㅋ거참 진짜 아무리 찾아도 없네",
          "speaker_id": "2",
          "time": "20210830 08:01"
        },
        {
          "id": "MDRW2100035558.1.6",
          "form": "헉 ㅋㅋㅋ나도 가서찾아보고싶댜",
          "original_form": "헉 ㅋㅋㅋ나도 가서찾아보고싶댜",
          "speaker_id": "1",
          "time": "20210830 08:02"
        },
        {
          "id": "MDRW2100035558.1.7",
          "form": "쇼파틈이제일유력아닌가요!?",
          "original_form": "쇼파틈이제일유력아닌가요!?",
          "speaker_id": "1",
          "time": "20210830 08:02"
        },
        {
          "id": "MDRW2100035558.1.8",
          "form": "ㅋㅋㅋ손전등 비춰서 보고 해도 안보임...와",
          "original_form": "ㅋㅋㅋ손전등 비춰서 보고 해도 안보임...와",
          "speaker_id": "2",
          "time": "20210830 08:03"
        },
        {
          "id": "MDRW2100035558.1.9",
          "form": "셋탑박스인가 보면",
          "original_form": "셋탑박스인가 보면",
          "speaker_id": "1",
          "time": "20210830 08:04"
        },
        {
          "id": "MDRW2100035558.1.10",
          "form": "리모콘울리게버튼잇다던데",
          "original_form": "리모콘울리게버튼잇다던데",
          "speaker_id": "1",
          "time": "20210830 08:04"
        },
        {
          "id": "MDRW2100035558.1.11",
          "form": "우리집에껏도어찌하는건질모르겟네",
          "original_form": "우리집에껏도어찌하는건질모르겟네",
          "speaker_id": "1",
          "time": "20210830 08:06"
        },
        {
          "id": "MDRW2100035558.1.12",
          "form": "우리집껀 오래된거라 없을걸..?ㅋㅋㅋ",
          "original_form": "우리집껀 오래된거라 없을걸..?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 08:06"
        },
        {
          "id": "MDRW2100035558.1.13",
          "form": "티비가아니라 셋탑박스야!!!",
          "original_form": "티비가아니라 셋탑박스야!!!",
          "speaker_id": "1",
          "time": "20210830 08:08"
        },
        {
          "id": "MDRW2100035558.1.14",
          "form": "혹시 플러스마이너스버트잇어 ? 길게눌러보래 ㅋㅋㅋ 울집 비티비기준",
          "original_form": "혹시 플러스마이너스버트잇어 ? 길게눌러보래 ㅋㅋㅋ 울집 비티비기준",
          "speaker_id": "1",
          "time": "20210830 08:09"
        },
        {
          "id": "MDRW2100035558.1.15",
          "form": "ㅋㅋ없어유 흑흑 ㅋㅋㅋㅋ 만능리모컨 이런거 사야겠다..",
          "original_form": "ㅋㅋ없어유 흑흑 ㅋㅋㅋㅋ 만능리모컨 이런거 사야겠다..",
          "speaker_id": "2",
          "time": "20210830 08:10"
        },
        {
          "id": "MDRW2100035558.1.16",
          "form": "흐미 ㅠ 오래썻나ㅜ?",
          "original_form": "흐미 ㅠ 오래썻나ㅜ?",
          "speaker_id": "1",
          "time": "20210830 08:12"
        },
        {
          "id": "MDRW2100035558.1.17",
          "form": "난작년엔가 새로바꿈 ㅋㅋ3년약정끝나고 새로함",
          "original_form": "난작년엔가 새로바꿈 ㅋㅋ3년약정끝나고 새로함",
          "speaker_id": "1",
          "time": "20210830 08:12"
        },
        {
          "id": "MDRW2100035558.1.18",
          "form": "ㅋㅋ오래썼을거야 아마! ㅋㅋㅋ 3년은 넘지 않았을까",
          "original_form": "ㅋㅋ오래썼을거야 아마! ㅋㅋㅋ 3년은 넘지 않았을까",
          "speaker_id": "2",
          "time": "20210830 08:13"
        },
        {
          "id": "MDRW2100035558.1.19",
          "form": "재약정혜택같은거받음!?",
          "original_form": "재약정혜택같은거받음!?",
          "speaker_id": "1",
          "time": "20210830 08:13"
        },
        {
          "id": "MDRW2100035558.1.20",
          "form": "엄마가 한거라 모르겄네 ㅠㅠ 기억안난대 흑흑",
          "original_form": "엄마가 한거라 모르겄네 ㅠㅠ 기억안난대 흑흑",
          "speaker_id": "2",
          "time": "20210830 08:14"
        },
        {
          "id": "MDRW2100035558.1.21",
          "form": "저기근데..리모컨왜찾으세요?",
          "original_form": "저기근데..리모컨왜찾으세요?",
          "speaker_id": "1",
          "time": "20210830 08:15"
        },
        {
          "id": "MDRW2100035558.1.22",
          "form": "ㅋㅋㅋ리모컨이 없으면 티비를 어떻게 봐요...",
          "original_form": "ㅋㅋㅋ리모컨이 없으면 티비를 어떻게 봐요...",
          "speaker_id": "2",
          "time": "20210830 08:17"
        },
        {
          "id": "MDRW2100035558.1.23",
          "form": "구해줘홈즈보는제",
          "original_form": "구해줘홈즈보는제",
