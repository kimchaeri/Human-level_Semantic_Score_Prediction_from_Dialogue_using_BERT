      "utterance": [
        {
          "id": "MDRW2100043583.1.1",
          "form": "만두요?",
          "original_form": "만두요?",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100043583.1.2",
          "form": "네! 저는 군만두 좋아해요",
          "original_form": "네! 저는 군만두 좋아해요",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100043583.1.3",
          "form": "군만두 맛있죠",
          "original_form": "군만두 맛있죠",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100043583.1.4",
          "form": "네 삶아서 익힌다음에 겉만 구우면 맛있어요",
          "original_form": "네 삶아서 익힌다음에 겉만 구우면 맛있어요",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100043583.1.5",
          "form": "앗 신기하네요",
          "original_form": "앗 신기하네요",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100043583.1.6",
          "form": "그렇게 하면 겉바 속촉이고 기름도 적게써요!",
          "original_form": "그렇게 하면 겉바 속촉이고 기름도 적게써요!",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100043583.1.7",
          "form": "저는 찐만두 좋아해요",
          "original_form": "저는 찐만두 좋아해요",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100043583.1.8",
          "form": "찐만두도 맛있죠!!",
          "original_form": "찐만두도 맛있죠!!",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100043583.1.9",
          "form": "부추많이 들어간 만두가 맛있는데 먹고싶네요",
          "original_form": "부추많이 들어간 만두가 맛있는데 먹고싶네요",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100043583.1.10",
          "form": "맞아요 만두는 역시 부추!",
          "original_form": "맞아요 만두는 역시 부추!",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100043583.1.11",
          "form": "만두국도 맛있는데..",
          "original_form": "만두국도 맛있는데..",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100043583.1.12",
          "form": "떡만두국으로 끓여먹으면 맛있죠 계란지단도 넣어서!",
          "original_form": "떡만두국으로 끓여먹으면 맛있죠 계란지단도 넣어서!",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100043583.1.13",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100043583.1.14",
          "form": "가을이라 전어 먹고싶네요",
          "original_form": "가을이라 전어 먹고싶네요",
          "speaker_id": "1",
