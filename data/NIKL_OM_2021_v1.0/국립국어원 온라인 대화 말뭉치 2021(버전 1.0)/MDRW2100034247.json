      "utterance": [
        {
          "id": "MDRW2100034247.1.1",
          "form": "3위 패션재벌가 구찌 가문의 비극이래",
          "original_form": "3위 패션재벌가 구찌 가문의 비극이래",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034247.1.2",
          "form": "차달남을 보고 있음",
          "original_form": "차달남을 보고 있음",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034247.1.3",
          "form": "아 이것도 들었던거 같은데 ㅋ",
          "original_form": "아 이것도 들었던거 같은데 ㅋ",
          "speaker_id": "1",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034247.1.4",
          "form": "헐 전부인이 청부해서",
          "original_form": "헐 전부인이 청부해서",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034247.1.5",
          "form": "콩가루 집안이리ㅣ",
          "original_form": "콩가루 집안이리ㅣ",
          "speaker_id": "1",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034247.1.6",
          "form": "가난한 세탁소집안의 딸이었는데 집안 반대가 심해도 살앗는데",
          "original_form": "가난한 세탁소집안의 딸이었는데 집안 반대가 심해도 살앗는데",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034247.1.7",
          "form": "그래서 살해한 이유는?",
          "original_form": "그래서 살해한 이유는?",
          "speaker_id": "1",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034247.1.8",
          "form": "허영심에 의부증까지 있게 되어서 13년만에 이혼",
          "original_form": "허영심에 의부증까지 있게 되어서 13년만에 이혼",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034247.1.9",
          "form": "의부증 심하면 같이 못살지",
          "original_form": "의부증 심하면 같이 못살지",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034247.1.10",
          "form": "어마어마한 위자료를 바았는데 재혼을 한다고 해서리",
          "original_form": "어마어마한 위자료를 바았는데 재혼을 한다고 해서리",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034247.1.11",
          "form": "미쳤군. 돈도 많이 받았는데 그러나?",
          "original_form": "미쳤군. 돈도 많이 받았는데 그러나?",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034247.1.12",
          "form": "이혼하고 구찌가 더 잘되어서 질투가 난나보다",
          "original_form": "이혼하고 구찌가 더 잘되어서 질투가 난나보다",
          "speaker_id": "2",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034247.1.13",
          "form": "그래도 그렇지",
          "original_form": "그래도 그렇지",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034247.1.14",
          "form": "그러게 그냥 우리는 이해 못한다",
          "original_form": "그러게 그냥 우리는 이해 못한다",
          "speaker_id": "2",
          "time": "20210827 22:01"
        }
      ]
