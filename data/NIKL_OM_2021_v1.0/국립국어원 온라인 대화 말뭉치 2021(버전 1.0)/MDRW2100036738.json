      "utterance": [
        {
          "id": "MDRW2100036738.1.1",
          "form": "점심먹었나요?",
          "original_form": "점심먹었나요?",
          "speaker_id": "1",
          "time": "20210831 12:44"
        },
        {
          "id": "MDRW2100036738.1.2",
          "form": "네 배불러요",
          "original_form": "네 배불러요",
          "speaker_id": "2",
          "time": "20210831 14:19"
        },
        {
          "id": "MDRW2100036738.1.3",
          "form": "저녁은 모먹죠?ㅎㅎ",
          "original_form": "저녁은 모먹죠?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 14:19"
        },
        {
          "id": "MDRW2100036738.1.4",
          "form": "저는 스벅먹을려구요",
          "original_form": "저는 스벅먹을려구요",
          "speaker_id": "1",
          "time": "20210831 14:20"
        },
        {
          "id": "MDRW2100036738.1.5",
          "form": "그전에 배스킨먹을예정이에요",
          "original_form": "그전에 배스킨먹을예정이에요",
          "speaker_id": "1",
          "time": "20210831 14:20"
        },
        {
          "id": "MDRW2100036738.1.6",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "2",
          "time": "20210831 14:21"
        },
        {
          "id": "MDRW2100036738.1.7",
          "form": "다 내사랑들",
          "original_form": "다 내사랑들",
          "speaker_id": "2",
          "time": "20210831 14:21"
        },
        {
          "id": "MDRW2100036738.1.8",
          "form": "엄마는외계인맛을 좋아해서",
          "original_form": "엄마는외계인맛을 좋아해서",
          "speaker_id": "1",
          "time": "20210831 14:56"
        },
        {
          "id": "MDRW2100036738.1.9",
          "form": "먹고싶네욧ㅋㅋㅋ",
          "original_form": "먹고싶네욧ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:56"
        },
        {
          "id": "MDRW2100036738.1.10",
          "form": "그 달달한 맛!",
          "original_form": "그 달달한 맛!",
          "speaker_id": "2",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100036738.1.11",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210901 07:51"
        },
        {
          "id": "MDRW2100036738.1.12",
          "form": "배스킨 맛있어요",
          "original_form": "배스킨 맛있어요",
          "speaker_id": "1",
          "time": "20210901 07:51"
        },
        {
          "id": "MDRW2100036738.1.13",
          "form": "군침도네요ㅎ",
          "original_form": "군침도네요ㅎ",
          "speaker_id": "2",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100036738.1.14",
          "form": "저는지금 간식 과자먹고있어요",
          "original_form": "저는지금 간식 과자먹고있어요",
          "speaker_id": "1",
          "time": "20210901 14:35"
        },
        {
          "id": "MDRW2100036738.1.15",
          "form": "전 빵이여",
          "original_form": "전 빵이여",
          "speaker_id": "2",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100036738.1.16",
          "form": "저녁시간이 다되가서 배가고프네요",
          "original_form": "저녁시간이 다되가서 배가고프네요",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100036738.1.17",
          "form": "떡볶이먹고싶네요ㅋ",
          "original_form": "떡볶이먹고싶네요ㅋ",
          "speaker_id": "2",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100036738.1.18",
          "form": "저는 고기류가땡겨요",
          "original_form": "저는 고기류가땡겨요",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100036738.1.19",
          "form": "배가되게 고프네요",
          "original_form": "배가되게 고프네요",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100036738.1.20",
          "form": "고기류를 먹고싶은데",
          "original_form": "고기류를 먹고싶은데",
          "speaker_id": "1",
          "time": "20210901 17:06"
        }
      ]
