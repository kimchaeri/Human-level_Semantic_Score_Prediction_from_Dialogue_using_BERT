      "utterance": [
        {
          "id": "MDRW2100027528.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027528.1.2",
          "form": "좋아는 하는데 자주는 안갔었네요ㅎㅎ",
          "original_form": "좋아는 하는데 자주는 안갔었네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100027528.1.3",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100027528.1.4",
          "form": "저는 좋아합니다",
          "original_form": "저는 좋아합니다",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100027528.1.5",
          "form": "해외무새인데",
          "original_form": "해외무새인데",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027528.1.6",
          "form": "요\u0000새는 어쩔수 없이 ㅠ",
          "original_form": "요\u0000새는 어쩔수 없이 ㅠ",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027528.1.7",
          "form": "오옹 고수셨네요",
          "original_form": "오옹 고수셨네요",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027528.1.8",
          "form": "해외 어디 다녀오셨어요?",
          "original_form": "해외 어디 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027528.1.9",
          "form": "재작년에~~ 홍콩마카오, 태국, 베트남, 대만이요 ㅋㅋㄱ",
          "original_form": "재작년에~~ 홍콩마카오, 태국, 베트남, 대만이요 ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100027528.1.10",
          "form": "우와 엄청 다양하게 다녀오셨네요...!!",
          "original_form": "우와 엄청 다양하게 다녀오셨네요...!!",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100027528.1.11",
          "form": "홍콩은 언제 한번 가보고 싶었는데..!! 음식 맛있나요??!",
          "original_form": "홍콩은 언제 한번 가보고 싶었는데..!! 음식 맛있나요??!",
          "speaker_id": "2",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100027528.1.12",
          "form": "맛은 뭐 ㅋㅋ",
          "original_form": "맛은 뭐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100027528.1.13",
          "form": "당연히 있는데 비싸고...",
          "original_form": "당연히 있는데 비싸고...",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100027528.1.14",
          "form": "지금은 중국땜에 무서워져서 ㅎㅎ",
          "original_form": "지금은 중국땜에 무서워져서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100027528.1.15",
          "form": "아아앟..ㅋㅋㅋ 요즘은 아무래도ㅎㅎ",
          "original_form": "아아앟..ㅋㅋㅋ 요즘은 아무래도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100027528.1.16",
          "form": "대만에서는 둘러보신 곳 있으신가요?",
          "original_form": "대만에서는 둘러보신 곳 있으신가요?",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100027528.1.17",
          "form": "아는 관광명소가 센과 치히로에 나오는 곳 밖에 없어요ㅋㅋ",
          "original_form": "아는 관광명소가 센과 치히로에 나오는 곳 밖에 없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100027528.1.18",
          "form": "가오슝 갔었어요ㅋㅋ",
          "original_form": "가오슝 갔었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100027528.1.19",
          "form": "오 가오슝은 어떤 곳인가요??",
          "original_form": "오 가오슝은 어떤 곳인가요??",
          "speaker_id": "2",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100027528.1.20",
          "form": "대만 한 4~5번째도시에요 ㅎㅎ",
          "original_form": "대만 한 4~5번째도시에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100027528.1.21",
          "form": "조용조용하고 좋은데",
          "original_form": "조용조용하고 좋은데",
          "speaker_id": "1",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100027528.1.22",
          "form": "야시장이 재밌어요",
          "original_form": "야시장이 재밌어요",
          "speaker_id": "1",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100027528.1.23",
          "form": "야시장 좀만 돌아다녀도 해뜨겠네요 먹을것도 많을것 같고ㅎㅎ",
          "original_form": "야시장 좀만 돌아다녀도 해뜨겠네요 먹을것도 많을것 같고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100027528.1.24",
          "form": "네 야시장 진짜 싸요 ㅎㅎ",
          "original_form": "네 야시장 진짜 싸요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:34"
        },
        {
          "id": "MDRW2100027528.1.25",
          "form": "아 물가까지 완벽하네요 대만가면 야시장 무조건 갑니다아..!!",
          "original_form": "아 물가까지 완벽하네요 대만가면 야시장 무조건 갑니다아..!!",
          "speaker_id": "2",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100027528.1.26",
          "form": "코로나야 끝나라아아",
          "original_form": "코로나야 끝나라아아",
