      "utterance": [
        {
          "id": "MDRW2100024264.1.1",
          "form": "피자스쿨 좋아해요?",
          "original_form": "피자스쿨 좋아해요?",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100024264.1.2",
          "form": "피자스쿨 저희동네는 없어서 한번도 안 먹어봤어요ㅜㅜ",
          "original_form": "피자스쿨 저희동네는 없어서 한번도 안 먹어봤어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100024264.1.3",
          "form": "고구마피자가 그리 맛있다던데",
          "original_form": "고구마피자가 그리 맛있다던데",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024264.1.4",
          "form": "헤헤 고구마 피자 진짜 맛있어요",
          "original_form": "헤헤 고구마 피자 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024264.1.5",
          "form": "근데 포테이토도 맛있더라구요",
          "original_form": "근데 포테이토도 맛있더라구요",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024264.1.6",
          "form": "한 조각에 감자 하나씩 들어가서 싸울 일 없어요~",
          "original_form": "한 조각에 감자 하나씩 들어가서 싸울 일 없어요~",
          "speaker_id": "1",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100024264.1.7",
          "form": "저도 먹어보고싶네요 고구마랑 포테이토ㅜㅜ",
          "original_form": "저도 먹어보고싶네요 고구마랑 포테이토ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100024264.1.8",
          "form": "가격도 넘 저렴하더라구요",
          "original_form": "가격도 넘 저렴하더라구요",
          "speaker_id": "2",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100024264.1.9",
          "form": "저도 오늘 피자스쿨 시키려다가 그냥 지나쳤네요",
          "original_form": "저도 오늘 피자스쿨 시키려다가 그냥 지나쳤네요",
          "speaker_id": "1",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100024264.1.10",
          "form": "전 거기서 까르보네피자가 제일 좋더라구요",
          "original_form": "전 거기서 까르보네피자가 제일 좋더라구요",
          "speaker_id": "1",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100024264.1.11",
          "form": "까르보네피자는 첨들어요! 토핑 뭐가 들어가나요?",
          "original_form": "까르보네피자는 첨들어요! 토핑 뭐가 들어가나요?",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100024264.1.12",
          "form": "토핑이 굉장히 독특한데요",
          "original_form": "토핑이 굉장히 독특한데요",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100024264.1.13",
          "form": "한번 맞혀보실래요?ㅎㅎ",
          "original_form": "한번 맞혀보실래요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100024264.1.14",
          "form": "까르보네.... 크림소스 들어갈거 같고",
          "original_form": "까르보네.... 크림소스 들어갈거 같고",
          "speaker_id": "2",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024264.1.15",
          "form": "베이컨도 들어갈거 같은데....",
          "original_form": "베이컨도 들어갈거 같은데....",
          "speaker_id": "2",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024264.1.16",
          "form": "일단 크림소스는 정답이구요",
          "original_form": "일단 크림소스는 정답이구요",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024264.1.17",
          "form": "베이컨도 정답이에요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "베이컨도 정답이에요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024264.1.18",
          "form": "근데 진짜 상상도 못하는게 하나 더 들어가요",
          "original_form": "근데 진짜 상상도 못하는게 하나 더 들어가요",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024264.1.19",
          "form": "계란들어가나요?",
          "original_form": "계란들어가나요?",
          "speaker_id": "2",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024264.1.20",
          "form": "...어 계란은 잘 모르겟는데",
          "original_form": "...어 계란은 잘 모르겟는데",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024264.1.21",
          "form": "파스타면이 들어갑니다 ㅋㅋㅋㅋ",
          "original_form": "파스타면이 들어갑니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024264.1.22",
          "form": "헐 ㅋㅋㅋ 진짜 특이하네요",
          "original_form": "헐 ㅋㅋㅋ 진짜 특이하네요",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024264.1.23",
          "form": "그쵸 ㅋㅋ 이런 피자는 피자스쿨 밖에 안팔더라구요",
          "original_form": "그쵸 ㅋㅋ 이런 피자는 피자스쿨 밖에 안팔더라구요",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024264.1.24",
          "form": "한 번 드셔보는 것도 추천합니다!",
          "original_form": "한 번 드셔보는 것도 추천합니다!",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024264.1.25",
          "form": "뭔가 면과 빵의 조화 상상이 안가요",
          "original_form": "뭔가 면과 빵의 조화 상상이 안가요",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024264.1.26",
          "form": "피자스쿨 까르보네.. 한번 포털에 검색해봐요",
          "original_form": "피자스쿨 까르보네.. 한번 포털에 검색해봐요",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024264.1.27",
          "form": "진짜 먹음직해요",
          "original_form": "진짜 먹음직해요",
          "speaker_id": "1",
          "time": "20210830 23:58"
        }
      ]
    }
