      "utterance": [
        {
          "id": "MDRW2100016189.1.1",
          "form": "ㅋㅋ아녜요 어제 술먹어서",
          "original_form": "ㅋㅋ아녜요 어제 술먹어서",
          "speaker_id": "2",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100016189.1.2",
          "form": "힘드네요 ㅎㅎ",
          "original_form": "힘드네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100016189.1.3",
          "form": "코로나인데 ㅎ 둘이서 드신건가요",
          "original_form": "코로나인데 ㅎ 둘이서 드신건가요",
          "speaker_id": "1",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016189.1.4",
          "form": "아 백신접종 2인 있어서 4인이요ㅋㅋ",
          "original_form": "아 백신접종 2인 있어서 4인이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100016189.1.5",
          "form": "뉴스뜨던데 바로 실천하셨군요ㅎ",
          "original_form": "뉴스뜨던데 바로 실천하셨군요ㅎ",
          "speaker_id": "1",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016189.1.6",
          "form": "회식하신건가요?",
          "original_form": "회식하신건가요?",
          "speaker_id": "1",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100016189.1.7",
          "form": "아님 동기모임?",
          "original_form": "아님 동기모임?",
          "speaker_id": "1",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100016189.1.8",
          "form": "아 고등학교 친구요ㅎㅎ",
          "original_form": "아 고등학교 친구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:32"
        },
        {
          "id": "MDRW2100016189.1.9",
          "form": "해외 출장나간다고 해서 ㅎㅎ",
          "original_form": "해외 출장나간다고 해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:32"
        },
        {
          "id": "MDRW2100016189.1.10",
          "form": "오ㅎ 이와중에 해외출장을",
          "original_form": "오ㅎ 이와중에 해외출장을",
          "speaker_id": "1",
          "time": "20210826 14:46"
        },
        {
          "id": "MDRW2100016189.1.11",
          "form": "ㅋㅋ건설사라서 크게 영향 없는듯요",
          "original_form": "ㅋㅋ건설사라서 크게 영향 없는듯요",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100016189.1.12",
          "form": "가서 팩토리에서 일하니까",
          "original_form": "가서 팩토리에서 일하니까",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100016189.1.13",
          "form": "아ㅎ 그럼 나가야겠네요",
          "original_form": "아ㅎ 그럼 나가야겠네요",
          "speaker_id": "1",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100016189.1.14",
          "form": "장기출장 가시나~",
          "original_form": "장기출장 가시나~",
          "speaker_id": "1",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100016189.1.15",
          "form": "네 그런 개념이죠/~",
          "original_form": "네 그런 개념이죠/~",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100016189.1.16",
          "form": "글쿤요ㅎ 퇴근은 하셨어요?",
          "original_form": "글쿤요ㅎ 퇴근은 하셨어요?",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100016189.1.17",
          "form": "네 ㅎㅎ 오늘 좀 정신없었네요",
          "original_form": "네 ㅎㅎ 오늘 좀 정신없었네요",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100016189.1.18",
          "form": "코로나 선제검사도 받고 ㅎㅎ",
          "original_form": "코로나 선제검사도 받고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100016189.1.19",
          "form": "코로나검사는 왜요?",
          "original_form": "코로나검사는 왜요?",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100016189.1.20",
          "form": "주변에 확진자나왔나요",
          "original_form": "주변에 확진자나왔나요",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100016189.1.21",
          "form": "저희 선제검사 전원 시킵니다........",
          "original_form": "저희 선제검사 전원 시킵니다........",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100016189.1.22",
          "form": "주기적으로요?",
          "original_form": "주기적으로요?",
          "speaker_id": "1",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100016189.1.23",
          "form": "네 ㅋㅋㅋ 이번에는 백신 접종자까지 시켯....",
          "original_form": "네 ㅋㅋㅋ 이번에는 백신 접종자까지 시켯....",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100016189.1.24",
          "form": "아 ㅎ 백신도 맞으셨어요?",
          "original_form": "아 ㅎ 백신도 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100016189.1.25",
          "form": "2차까지?",
          "original_form": "2차까지?",
          "speaker_id": "1",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100016189.1.26",
          "form": "아 전 얀센이오 ㅎㅎ",
          "original_form": "아 전 얀센이오 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100016189.1.27",
          "form": "근데 맞고 나니까 대중교통종사자",
          "original_form": "근데 맞고 나니까 대중교통종사자",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100016189.1.28",
          "form": "화이자 주더라고요....",
          "original_form": "화이자 주더라고요....",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100016189.1.29",
          "form": "아 예비역이라 얀센?",
          "original_form": "아 예비역이라 얀센?",
