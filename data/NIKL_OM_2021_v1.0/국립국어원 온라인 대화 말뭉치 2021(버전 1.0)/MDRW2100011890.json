      "utterance": [
        {
          "id": "MDRW2100011890.1.1",
          "form": "회의적인 반응이",
          "original_form": "회의적인 반응이",
          "speaker_id": "2",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100011890.1.2",
          "form": "결혼식 돌잔치 기타등등으로 다들 수금하던데",
          "original_form": "결혼식 돌잔치 기타등등으로 다들 수금하던데",
          "speaker_id": "1",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100011890.1.3",
          "form": "지금도 엄청나고",
          "original_form": "지금도 엄청나고",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011890.1.4",
          "form": "미래도 다를것같지않고",
          "original_form": "미래도 다를것같지않고",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011890.1.5",
          "form": "그렇군요 ㅜ",
          "original_form": "그렇군요 ㅜ",
          "speaker_id": "1",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011890.1.6",
          "form": "아 돌잔치",
          "original_form": "아 돌잔치",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011890.1.7",
          "form": "맞아요 돌잔치도 잇었죠",
          "original_form": "맞아요 돌잔치도 잇었죠",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011890.1.8",
          "form": "친구가 적어야해요..ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "친구가 적어야해요..ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011890.1.9",
          "form": "돌잔치도 축하할 수 있을지",
          "original_form": "돌잔치도 축하할 수 있을지",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011890.1.10",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011890.1.11",
          "form": "안축하하는데..",
          "original_form": "안축하하는데..",
          "speaker_id": "1",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011890.1.12",
          "form": "그 애를 보면 여러느낌이 들겠어요",
          "original_form": "그 애를 보면 여러느낌이 들겠어요",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011890.1.13",
          "form": "와 내 친구가 키우는 애..",
          "original_form": "와 내 친구가 키우는 애..",
          "speaker_id": "1",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011890.1.14",
          "form": "그 남자의 애..",
          "original_form": "그 남자의 애..",
          "speaker_id": "1",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100011890.1.15",
          "form": "진짜 복잡한 기분일거같네요",
          "original_form": "진짜 복잡한 기분일거같네요",
          "speaker_id": "1",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100011890.1.16",
          "form": "특히나 말썽 피우는 남자애면",
          "original_form": "특히나 말썽 피우는 남자애면",
          "speaker_id": "2",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100011890.1.17",
          "form": "어휴",
          "original_form": "어휴",
          "speaker_id": "2",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100011890.1.18",
          "form": "와 ! 상상만해도 끔찍",
          "original_form": "와 ! 상상만해도 끔찍",
          "speaker_id": "1",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100011890.1.19",
          "form": "쥐어 박을 것 같아요",
          "original_form": "쥐어 박을 것 같아요",
          "speaker_id": "1",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100011890.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100011890.1.21",
          "form": "사회가 말하는 엄마가 되는 친구라니",
          "original_form": "사회가 말하는 엄마가 되는 친구라니",
          "speaker_id": "2",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100011890.1.22",
          "form": "뜯어말리고싶네요",
          "original_form": "뜯어말리고싶네요",
          "speaker_id": "2",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100011890.1.23",
          "form": "헐 내친구가 엄마라니..",
          "original_form": "헐 내친구가 엄마라니..",
          "speaker_id": "1",
          "time": "20210824 17:07"
        }
      ]
