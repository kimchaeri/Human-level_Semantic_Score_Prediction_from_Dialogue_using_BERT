      "utterance": [
        {
          "id": "MDRW2100025743.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 16:41"
        },
        {
          "id": "MDRW2100025743.1.2",
          "form": "점심 드셨나요??",
          "original_form": "점심 드셨나요??",
          "speaker_id": "1",
          "time": "20210901 16:41"
        },
        {
          "id": "MDRW2100025743.1.3",
          "form": "점심으로는 닭강정 먹었어요~",
          "original_form": "점심으로는 닭강정 먹었어요~",
          "speaker_id": "2",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100025743.1.4",
          "form": "name1님은 점심 드셨나용?",
          "original_form": "&name1&님은 점심 드셨나용?",
          "speaker_id": "2",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100025743.1.5",
          "form": "우와 맛있는거 드셨네요",
          "original_form": "우와 맛있는거 드셨네요",
          "speaker_id": "1",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100025743.1.6",
          "form": "네 저는 장조림에 밥 먹었어요",
          "original_form": "네 저는 장조림에 밥 먹었어요",
          "speaker_id": "1",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100025743.1.7",
          "form": "저녁은 정하셨나요?",
          "original_form": "저녁은 정하셨나요?",
          "speaker_id": "1",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100025743.1.8",
          "form": "장조림도 맛있으셨겠어요ㅠㅠ 전 장조림에 간장국물에 밥 비벼먹는것도 좋아하거든요ㅠㅠ",
          "original_form": "장조림도 맛있으셨겠어요ㅠㅠ 전 장조림에 간장국물에 밥 비벼먹는것도 좋아하거든요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025743.1.9",
          "form": "저녁은 아직 못정했어요ㅠㅠ 매일같이 저녁을 뭘 먹어야 좋을지 고민만하네요ㅠㅠ",
          "original_form": "저녁은 아직 못정했어요ㅠㅠ 매일같이 저녁을 뭘 먹어야 좋을지 고민만하네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025743.1.10",
          "form": "아 비벼 먹는거 맛있죠",
          "original_form": "아 비벼 먹는거 맛있죠",
          "speaker_id": "1",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100025743.1.11",
          "form": "밥 정하는것도 일이에요..",
          "original_form": "밥 정하는것도 일이에요..",
          "speaker_id": "1",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100025743.1.12",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100025743.1.13",
          "form": "하루 전날부터 고민해도 너무 어려워요ㅠㅠ",
          "original_form": "하루 전날부터 고민해도 너무 어려워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100025743.1.14",
          "form": "매일 같이 밥 뭐먹을지 고르는거 생각하면 학교다닐때 영양사 선생님이 너무 대단하고 급식먹을때가 좋았구나 싶어요ㅠㅠ",
          "original_form": "매일 같이 밥 뭐먹을지 고르는거 생각하면 학교다닐때 영양사 선생님이 너무 대단하고 급식먹을때가 좋았구나 싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100025743.1.15",
          "form": "맞아요ㅋㅋㅋㅠㅠ",
          "original_form": "맞아요ㅋㅋㅋㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100025743.1.16",
          "form": "매일 사먹는것도 다 돈이고",
          "original_form": "매일 사먹는것도 다 돈이고",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100025743.1.17",
          "form": "주는대로 먹는게 편하죠ㅋㅋㅋ",
          "original_form": "주는대로 먹는게 편하죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100025743.1.18",
          "form": "요즘 아파트에는 식당도 있다더라구요",
          "original_form": "요즘 아파트에는 식당도 있다더라구요",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025743.1.19",
          "form": "급식처럼 주는데가 있대요",
          "original_form": "급식처럼 주는데가 있대요",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025743.1.20",
          "form": "그런데 너무 부러워요ㅠㅠ",
          "original_form": "그런데 너무 부러워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025743.1.21",
          "form": "고민되면 그냥 식당가서 사먹으면 되잖아요ㅠㅠ",
          "original_form": "고민되면 그냥 식당가서 사먹으면 되잖아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025743.1.22",
          "form": "가격도 그렇게 안비싸던데ㅠㅠ",
          "original_form": "가격도 그렇게 안비싸던데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025743.1.23",
          "form": "맞아요 차리지 않아도 되니까 너무편할거같아요",
          "original_form": "맞아요 차리지 않아도 되니까 너무편할거같아요",
          "speaker_id": "1",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100025743.1.24",
          "form": "밥은 보통 혼자 드세요?",
          "original_form": "밥은 보통 혼자 드세요?",
          "speaker_id": "1",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100025743.1.25",
          "form": "같이 먹으려고 노력은 하는데 시간이 안맞아서 혼자 먹게 되네요ㅠㅠ",
          "original_form": "같이 먹으려고 노력은 하는데 시간이 안맞아서 혼자 먹게 되네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025743.1.26",
          "form": "그렇군요ㅠㅠ",
          "original_form": "그렇군요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025743.1.27",
          "form": "저는 혼자 먹으면 좀 빨리 먹게 되는거같아요",
          "original_form": "저는 혼자 먹으면 좀 빨리 먹게 되는거같아요",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025743.1.28",
          "form": "name2님도 그러신가요?",
          "original_form": "&name2&님도 그러신가요?",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025743.1.29",
          "form": "저는 먹는건 빨리 먹고 싶어도 턱이 아파서 천천히 먹게 되더라구요ㅠㅠ",
          "original_form": "저는 먹는건 빨리 먹고 싶어도 턱이 아파서 천천히 먹게 되더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025743.1.30",
          "form": "대신 혼자 먹으면 뭔가 식욕없는 사람처럼 먹어욬ㅋ큐ㅠㅠㅠ",
          "original_form": "대신 혼자 먹으면 뭔가 식욕없는 사람처럼 먹어욬ㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025743.1.31",
          "form": "맛있게 먹어야하는데 혼자 먹는것도 하루이틀이 아니다 보니까 그냥 먹는 느낌으로요ㅠㅠ",
          "original_form": "맛있게 먹어야하는데 혼자 먹는것도 하루이틀이 아니다 보니까 그냥 먹는 느낌으로요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025743.1.32",
          "form": "아아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025743.1.33",
          "form": "그쵸 그냥 먹어야되니까 먹는 느낌..",
          "original_form": "그쵸 그냥 먹어야되니까 먹는 느낌..",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025743.1.34",
          "form": "그냥 기계적으로 먹게 되시는군요",
          "original_form": "그냥 기계적으로 먹게 되시는군요",
