      "utterance": [
        {
          "id": "MDRW2100009831.1.1",
          "form": "우리 남동생 군대댕겨옴",
          "original_form": "우리 남동생 군대댕겨옴",
          "speaker_id": "1",
          "time": "20210823 10:40"
        },
        {
          "id": "MDRW2100009831.1.2",
          "form": "아저씨댓어",
          "original_form": "아저씨댓어",
          "speaker_id": "1",
          "time": "20210823 10:40"
        },
        {
          "id": "MDRW2100009831.1.3",
          "form": "동생은 군대 갔다왔어요?",
          "original_form": "동생은 군대 갔다왔어요?",
          "speaker_id": "2",
          "time": "20210823 10:49"
        },
        {
          "id": "MDRW2100009831.1.4",
          "form": "그람그람",
          "original_form": "그람그람",
          "speaker_id": "1",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100009831.1.5",
          "form": "25살인데 대학생이야",
          "original_form": "25살인데 대학생이야",
          "speaker_id": "1",
          "time": "20210823 10:51"
        },
        {
          "id": "MDRW2100009831.1.6",
          "form": "신군복 때깔 좋더라",
          "original_form": "신군복 때깔 좋더라",
          "speaker_id": "1",
          "time": "20210823 10:51"
        },
        {
          "id": "MDRW2100009831.1.7",
          "form": "일반 육군으로?",
          "original_form": "일반 육군으로?",
          "speaker_id": "2",
          "time": "20210823 10:52"
        },
        {
          "id": "MDRW2100009831.1.8",
          "form": "요샌 폰사용도되니까",
          "original_form": "요샌 폰사용도되니까",
          "speaker_id": "2",
          "time": "20210823 10:52"
        },
        {
          "id": "MDRW2100009831.1.9",
          "form": "마자 밴드에서 다 확인함ㅎㅎ",
          "original_form": "마자 밴드에서 다 확인함ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 10:53"
        },
        {
          "id": "MDRW2100009831.1.10",
          "form": "세련됐어",
          "original_form": "세련됐어",
          "speaker_id": "1",
          "time": "20210823 10:53"
        },
        {
          "id": "MDRW2100009831.1.11",
          "form": "군대는 몰래몰래 넘어가는 비리가 많으니까 차라리 잘된것같아요",
          "original_form": "군대는 몰래몰래 넘어가는 비리가 많으니까 차라리 잘된것같아요",
          "speaker_id": "2",
          "time": "20210823 10:55"
        },
        {
          "id": "MDRW2100009831.1.12",
          "form": "정신병도 심리상담도 줄었대",
          "original_form": "정신병도 심리상담도 줄었대",
          "speaker_id": "1",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100009831.1.13",
          "form": "응 솔직히 군대비리는 아직도많을건데",
          "original_form": "응 솔직히 군대비리는 아직도많을건데",
          "speaker_id": "2",
          "time": "20210823 11:08"
        },
        {
          "id": "MDRW2100009831.1.14",
          "form": "방역비리",
          "original_form": "방역비리",
          "speaker_id": "2",
          "time": "20210823 11:08"
        },
        {
          "id": "MDRW2100009831.1.15",
          "form": "군대 구타문제라도 없애야지싶어요",
          "original_form": "군대 구타문제라도 없애야지싶어요",
          "speaker_id": "2",
          "time": "20210823 11:08"
        },
        {
          "id": "MDRW2100009831.1.16",
          "form": "나빠 방역비리",
          "original_form": "나빠 방역비리",
          "speaker_id": "1",
          "time": "20210823 11:08"
        },
        {
          "id": "MDRW2100009831.1.17",
          "form": "구타문제",
          "original_form": "구타문제",
          "speaker_id": "1",
          "time": "20210823 11:09"
        },
        {
          "id": "MDRW2100009831.1.18",
          "form": "모두 편안하게 일년보내고 오지 참",
          "original_form": "모두 편안하게 일년보내고 오지 참",
          "speaker_id": "1",
          "time": "20210823 11:09"
        },
        {
          "id": "MDRW2100009831.1.19",
          "form": "너무 아직 정신적으로 성숙하지않은 나이에",
          "original_form": "너무 아직 정신적으로 성숙하지않은 나이에",
          "speaker_id": "2",
          "time": "20210823 11:10"
        },
        {
          "id": "MDRW2100009831.1.20",
          "form": "폐쇄적인환경에서",
          "original_form": "폐쇄적인환경에서",
          "speaker_id": "2",
          "time": "20210823 11:10"
        },
        {
          "id": "MDRW2100009831.1.21",
          "form": "병장만되도 갑자기 너무많은 권력을 쥐게되서 그런거같음",
          "original_form": "병장만되도 갑자기 너무많은 권력을 쥐게되서 그런거같음",
          "speaker_id": "2",
          "time": "20210823 11:10"
        },
        {
          "id": "MDRW2100009831.1.22",
          "form": "맞아 근데 나이대가 점점 미성숙해지는듯",
          "original_form": "맞아 근데 나이대가 점점 미성숙해지는듯",
          "speaker_id": "1",
          "time": "20210823 11:10"
        },
        {
          "id": "MDRW2100009831.1.23",
          "form": "옛날엔 슴살이면 의젓했던거 같은데",
          "original_form": "옛날엔 슴살이면 의젓했던거 같은데",
          "speaker_id": "1",
          "time": "20210823 11:11"
        },
        {
          "id": "MDRW2100009831.1.24",
          "form": "다들 귀하게 크고 오냐오냐커서 그럼",
          "original_form": "다들 귀하게 크고 오냐오냐커서 그럼",
          "speaker_id": "2",
          "time": "20210823 11:12"
        },
        {
          "id": "MDRW2100009831.1.25",
          "form": "그냥 내성이 부족한듯",
          "original_form": "그냥 내성이 부족한듯",
          "speaker_id": "2",
          "time": "20210823 11:12"
        },
        {
          "id": "MDRW2100009831.1.26",
          "form": "ㅋㅋ 안타깝네",
          "original_form": "ㅋㅋ 안타깝네",
          "speaker_id": "1",
          "time": "20210823 11:12"
        },
        {
          "id": "MDRW2100009831.1.27",
          "form": "옛날엔 어린나이에도 힘들고 가혹한일을 많이겪었으니",
          "original_form": "옛날엔 어린나이에도 힘들고 가혹한일을 많이겪었으니",
          "speaker_id": "2",
          "time": "20210823 11:12"
        },
        {
          "id": "MDRW2100009831.1.28",
          "form": "그런애들 모아두고 국방지키라니",
          "original_form": "그런애들 모아두고 국방지키라니",
          "speaker_id": "1",
          "time": "20210823 11:12"
        },
        {
          "id": "MDRW2100009831.1.29",
          "form": "그렇다고 딱히 그게 좋았다는건 아니지만",
          "original_form": "그렇다고 딱히 그게 좋았다는건 아니지만",
          "speaker_id": "2",
          "time": "20210823 11:13"
        },
        {
          "id": "MDRW2100009831.1.30",
          "form": "안쓰럽당",
          "original_form": "안쓰럽당",
          "speaker_id": "1",
          "time": "20210823 11:13"
        },
        {
          "id": "MDRW2100009831.1.31",
          "form": "그냥 옛날엔 한국이 가난하고 의식수준도 떨어졌다는 소리니까",
          "original_form": "그냥 옛날엔 한국이 가난하고 의식수준도 떨어졌다는 소리니까",
          "speaker_id": "2",
          "time": "20210823 11:13"
        },
        {
          "id": "MDRW2100009831.1.32",
          "form": "우리도 모병제하는게 맞을지도요",
          "original_form": "우리도 모병제하는게 맞을지도요",
          "speaker_id": "2",
          "time": "20210823 11:13"
        },
        {
          "id": "MDRW2100009831.1.33",
          "form": "미국처럼",
          "original_form": "미국처럼",
          "speaker_id": "2",
          "time": "20210823 11:13"
        }
      ]
    }
