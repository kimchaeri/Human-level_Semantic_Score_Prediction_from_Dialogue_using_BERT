      "utterance": [
        {
          "id": "MDRW2100037768.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋ왠지 내일도 두유라떼 드실거같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ왠지 내일도 두유라떼 드실거같아요",
          "speaker_id": "2",
          "time": "20210901 18:06"
        },
        {
          "id": "MDRW2100037768.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:06"
        },
        {
          "id": "MDRW2100037768.1.3",
          "form": "그럴수도 있어요...",
          "original_form": "그럴수도 있어요...",
          "speaker_id": "1",
          "time": "20210901 18:07"
        },
        {
          "id": "MDRW2100037768.1.4",
          "form": "1일 1라떼 실천중인 저는..",
          "original_form": "1일 1라떼 실천중인 저는..",
          "speaker_id": "1",
          "time": "20210901 18:07"
        },
        {
          "id": "MDRW2100037768.1.5",
          "form": "name2님은 오늘 커피드셨어용?",
          "original_form": "&name2&님은 오늘 커피드셨어용?",
          "speaker_id": "1",
          "time": "20210901 18:07"
        },
        {
          "id": "MDRW2100037768.1.6",
          "form": "아 원래 매일 드시는군여 ㅋㅋㅋㅋㅋ",
          "original_form": "아 원래 매일 드시는군여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:07"
        },
        {
          "id": "MDRW2100037768.1.7",
          "form": "네 저두 매일커피마셔요 ㅋㅋㅋㅋ",
          "original_form": "네 저두 매일커피마셔요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:07"
        },
        {
          "id": "MDRW2100037768.1.8",
          "form": "오늘 아메리카노 드셨어용??",
          "original_form": "오늘 아메리카노 드셨어용??",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100037768.1.9",
          "form": "네네 ㅋㅋㅋㅋ저는 거의 아메리카노 마셔요!",
          "original_form": "네네 ㅋㅋㅋㅋ저는 거의 아메리카노 마셔요!",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037768.1.10",
          "form": "아메리카노도 입가심에 제일인것 같아요!",
          "original_form": "아메리카노도 입가심에 제일인것 같아요!",
          "speaker_id": "1",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100037768.1.11",
          "form": "그리구 그 깊은맛이 끌릴때가 있어요!",
          "original_form": "그리구 그 깊은맛이 끌릴때가 있어요!",
          "speaker_id": "1",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100037768.1.12",
          "form": "뜨아로 드세여 아아로 드세요?",
          "original_form": "뜨아로 드세여 아아로 드세요?",
          "speaker_id": "1",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100037768.1.13",
          "form": "저는 아아로 마셔요! ㅋㅋㅋㅋㅋ",
          "original_form": "저는 아아로 마셔요! ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100037768.1.14",
          "form": "겨울에 아주 가끔만 뜨아 ㅋㅋㅋ",
          "original_form": "겨울에 아주 가끔만 뜨아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100037768.1.15",
          "form": "아메리카노는 아아가 진리죸ㅋㅋㅋ",
          "original_form": "아메리카노는 아아가 진리죸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100037768.1.16",
          "form": "약간 하염없이 물타서 먹는 ㅋㅋㅋ",
          "original_form": "약간 하염없이 물타서 먹는 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100037768.1.17",
          "form": "저는 아아를 여름에 물처럼 마셔욬ㅋㅋㅋ",
          "original_form": "저는 아아를 여름에 물처럼 마셔욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100037768.1.18",
          "form": "ㅋㅋㅋㅋㅋ아그렇군요",
          "original_form": "ㅋㅋㅋㅋㅋ아그렇군요",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100037768.1.19",
          "form": "연하게 드세요?",
          "original_form": "연하게 드세요?",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100037768.1.20",
          "form": "네 ㅎㅎ 제가 아메리카노를 정말 좋아했었는데 위염이 진짜 엄청 심하게 온뒤로ㅠㅠ",
          "original_form": "네 ㅎㅎ 제가 아메리카노를 정말 좋아했었는데 위염이 진짜 엄청 심하게 온뒤로ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100037768.1.21",
          "form": "겁나더라구요....",
          "original_form": "겁나더라구요....",
          "speaker_id": "1",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100037768.1.22",
          "form": "아 맞아요 커피가 위에 자극적이긴해여 ㅜㅠ",
          "original_form": "아 맞아요 커피가 위에 자극적이긴해여 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100037768.1.23",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037768.1.24",
          "form": "커피를 안먹을순없다! 하지만 위가 아프다ㅠㅠ",
          "original_form": "커피를 안먹을순없다! 하지만 위가 아프다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037768.1.25",
          "form": "그렇다면 라떼",
          "original_form": "그렇다면 라떼{emoji:☆}",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037768.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ바닐라라떼같은건 드세요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ바닐라라떼같은건 드세요?",
          "speaker_id": "2",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100037768.1.27",
          "form": "너무 달아서 못먹겠더라구요ㅜㅜ",
          "original_form": "너무 달아서 못먹겠더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100037768.1.28",
          "form": "저는 예전엔 진짜 단음료 많이 먹었는데 언젠가부터 못먹겠어요ㅜㅜ",
          "original_form": "저는 예전엔 진짜 단음료 많이 먹었는데 언젠가부터 못먹겠어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100037768.1.29",
          "form": "아아 맞아요 입맛이 점점 변하더라구요",
          "original_form": "아아 맞아요 입맛이 점점 변하더라구요",
          "speaker_id": "2",
          "time": "20210901 18:45"
        }
      ]
