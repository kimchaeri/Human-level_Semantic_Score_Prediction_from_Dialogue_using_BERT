      "utterance": [
        {
          "id": "MDRW2100031135.1.1",
          "form": "시국 끝나면 어떤 전시회나 공연 보러 가보고 싶음?",
          "original_form": "시국 끝나면 어떤 전시회나 공연 보러 가보고 싶음?",
          "speaker_id": "1",
          "time": "20210904 17:17"
        },
        {
          "id": "MDRW2100031135.1.2",
          "form": "연뮤 쪽도 한번 관람해보고 싶고",
          "original_form": "연뮤 쪽도 한번 관람해보고 싶고",
          "speaker_id": "2",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100031135.1.3",
          "form": "그냥 콘서트도 한번 가보고 싶음",
          "original_form": "그냥 콘서트도 한번 가보고 싶음",
          "speaker_id": "2",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100031135.1.4",
          "form": "콘서트 보내줘 왜 콘서트 못 열어ㅜ",
          "original_form": "콘서트 보내줘 왜 콘서트 못 열어ㅜ",
          "speaker_id": "2",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100031135.1.5",
          "form": "아.. 데한민국 가야하는 그런 공연은 안돼요",
          "original_form": "아.. 데한민국 가야하는 그런 공연은 안돼요",
          "speaker_id": "1",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100031135.1.6",
          "form": "콘서트 얼마나 큰거?",
          "original_form": "콘서트 얼마나 큰거?",
          "speaker_id": "1",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100031135.1.7",
          "form": "진짜 좋은 자리 아니면 재미 없을건데",
          "original_form": "진짜 좋은 자리 아니면 재미 없을건데",
          "speaker_id": "1",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100031135.1.8",
          "form": "데한민국ㅋㅋㅋㅋ",
          "original_form": "데한민국ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031135.1.9",
          "form": "테스타는 바라지도 않는다",
          "original_form": "테스타는 바라지도 않는다",
          "speaker_id": "2",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031135.1.10",
          "form": "규모는 글쎄",
          "original_form": "규모는 글쎄",
          "speaker_id": "2",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031135.1.11",
          "form": "자리는 무조건 스탠딩이지",
          "original_form": "자리는 무조건 스탠딩이지",
          "speaker_id": "2",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031135.1.12",
          "form": "야.. 그거 진짜 체력.. 와르르야..",
          "original_form": "야.. 그거 진짜 체력.. 와르르야..",
          "speaker_id": "1",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031135.1.13",
          "form": "치이고 치이고해서",
          "original_form": "치이고 치이고해서",
          "speaker_id": "1",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031135.1.14",
          "form": "그치만 거기가 제일 재밌을텐데ㅜ",
          "original_form": "그치만 거기가 제일 재밌을텐데ㅜ",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031135.1.15",
          "form": "누구 공연 특별히 보고 싶은거 있어?",
          "original_form": "누구 공연 특별히 보고 싶은거 있어?",
          "speaker_id": "1",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031135.1.16",
          "form": "지금은 위너?",
          "original_form": "지금은 위너?",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031135.1.17",
          "form": "근데 얘네 군대갔어",
          "original_form": "근데 얘네 군대갔어",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031135.1.18",
          "form": "위너.. 오...",
          "original_form": "위너.. 오...",
          "speaker_id": "1",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031135.1.19",
          "form": "새로운 오빠를 만든건...",
          "original_form": "새로운 오빠를 만든건...",
          "speaker_id": "1",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031135.1.20",
          "form": "만들자마자 갔구나",
          "original_form": "만들자마자 갔구나",
          "speaker_id": "1",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031135.1.21",
          "form": "멤버 넷 중에 둘이 군대감",
          "original_form": "멤버 넷 중에 둘이 군대감",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031135.1.22",
          "form": "둘 마저 또 가야함",
          "original_form": "둘 마저 또 가야함",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031135.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031135.1.24",
          "form": "그럼 공연이 안되잖아 ㅠ",
          "original_form": "그럼 공연이 안되잖아 ㅠ",
          "speaker_id": "1",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031135.1.25",
          "form": "차피..코로나도 끝나야..",
          "original_form": "차피..코로나도 끝나야..",
          "speaker_id": "2",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031135.1.26",
          "form": "마저용... 안끝나면 못가",
          "original_form": "마저용... 안끝나면 못가",
          "speaker_id": "1",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031135.1.27",
          "form": "코로나 이후로 콘서트는 열리지도 못하고 있어서",
          "original_form": "코로나 이후로 콘서트는 열리지도 못하고 있어서",
          "speaker_id": "2",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031135.1.28",
          "form": "나는 이무진 공연 한번 직관하러가고 싶어",
          "original_form": "나는 이무진 공연 한번 직관하러가고 싶어",
          "speaker_id": "1",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031135.1.29",
          "form": "온라인콘서트는 몇번 열린데 있다고 알고잇는디",
          "original_form": "온라인콘서트는 몇번 열린데 있다고 알고잇는디",
          "speaker_id": "2",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031135.1.30",
          "form": "좀 작은 공연장에서 가까이 앉아서 볼 수있는 소규모",
          "original_form": "좀 작은 공연장에서 가까이 앉아서 볼 수있는 소규모",
          "speaker_id": "1",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031135.1.31",
          "form": "그런거 말고 현장에서 뛰어야지ㅜ",
          "original_form": "그런거 말고 현장에서 뛰어야지ㅜ",
          "speaker_id": "2",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031135.1.32",
          "form": "그런것도 괜찮지",
          "original_form": "그런것도 괜찮지",
          "speaker_id": "2",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031135.1.33",
          "form": "다같이 노래 부르고 ㅋㅋㅋ",
          "original_form": "다같이 노래 부르고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100031135.1.34",
          "form": "나는 차분한거 최고..",
          "original_form": "나는 차분한거 최고..",
          "speaker_id": "1",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100031135.1.35",
          "form": "오히려 뮤지컬이나 오페라 보러 가고 싶어 ㅠ",
          "original_form": "오히려 뮤지컬이나 오페라 보러 가고 싶어 ㅠ",
          "speaker_id": "1",
          "time": "20210904 17:29"
        }
      ]
    }
