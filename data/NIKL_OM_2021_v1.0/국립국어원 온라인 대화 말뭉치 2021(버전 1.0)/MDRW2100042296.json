      "utterance": [
        {
          "id": "MDRW2100042296.1.1",
          "form": "중고거래 하시나요?",
          "original_form": "중고거래 하시나요?",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042296.1.2",
          "form": "전 사진않고 주로파는편이에요",
          "original_form": "전 사진않고 주로파는편이에요",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042296.1.3",
          "form": "전 사고 팔고 다 많이 해요. 주로 뭐 파시나요?",
          "original_form": "전 사고 팔고 다 많이 해요. 주로 뭐 파시나요?",
          "speaker_id": "2",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042296.1.4",
          "form": "경품받은건데 안쓰는것들요 얼마전에는 체중계팔았어용",
          "original_form": "경품받은건데 안쓰는것들요 얼마전에는 체중계팔았어용",
          "speaker_id": "1",
          "time": "20210906 23:29"
        },
        {
          "id": "MDRW2100042296.1.5",
          "form": "우와!! 경품 받은거 파신가면 새거네요~",
          "original_form": "우와!! 경품 받은거 파신가면 새거네요~",
          "speaker_id": "2",
          "time": "20210907 07:06"
        },
        {
          "id": "MDRW2100042296.1.6",
          "form": "새제품이죠ㅋ",
          "original_form": "새제품이죠ㅋ",
          "speaker_id": "1",
          "time": "20210907 08:12"
        },
        {
          "id": "MDRW2100042296.1.7",
          "form": "제가 사고 싶네요. 또 어떤거 파셨나요?",
          "original_form": "제가 사고 싶네요. 또 어떤거 파셨나요?",
          "speaker_id": "2",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100042296.1.8",
          "form": "음 에어팟은 안팔리네여ㅠㅠ",
          "original_form": "음 에어팟은 안팔리네여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100042296.1.9",
          "form": "에어팟 얼마에 내 놓으셨는데요?",
          "original_form": "에어팟 얼마에 내 놓으셨는데요?",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100042296.1.10",
          "form": "무선충전2 새건데",
          "original_form": "무선충전2 새건데",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100042296.1.11",
          "form": "기억이 가물가물ㅠ",
          "original_form": "기억이 가물가물ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100042296.1.12",
          "form": "채팅은 잘오던데",
          "original_form": "채팅은 잘오던데",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100042296.1.13",
          "form": "채팅은 오는데 안사는건 비싸서일거예요 ㅋㅋ",
          "original_form": "채팅은 오는데 안사는건 비싸서일거예요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100042296.1.14",
          "form": "그런가봐요ㅋㅋ좀더내려야겠네요ㅠ한번은 장난으로 살게요캤다가 10초도안되서",
          "original_form": "그런가봐요ㅋㅋ좀더내려야겠네요ㅠ한번은 장난으로 살게요캤다가 10초도안되서",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100042296.1.15",
          "form": "대답이늦어서 안산다고",
          "original_form": "대답이늦어서 안산다고",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100042296.1.16",
          "form": "콱",
          "original_form": "콱",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100042296.1.17",
          "form": "그건 핑계인것 같아요 사고 싶은건 대답이 3일후에 와도 사요 ㅋㅋ",
          "original_form": "그건 핑계인것 같아요 사고 싶은건 대답이 3일후에 와도 사요 ㅋㅋ",
