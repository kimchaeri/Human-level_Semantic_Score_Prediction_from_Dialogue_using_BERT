      "utterance": [
        {
          "id": "MDRW2100007793.1.1",
          "form": "주말인데 오늘 여행 계획은 없으신가요?",
          "original_form": "주말인데 오늘 여행 계획은 없으신가요?",
          "speaker_id": "1",
          "time": "20210822 10:50"
        },
        {
          "id": "MDRW2100007793.1.2",
          "form": "없죠 ㅋㅋ",
          "original_form": "없죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:50"
        },
        {
          "id": "MDRW2100007793.1.3",
          "form": "코로나라 ㅡㅡ",
          "original_form": "코로나라 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 10:50"
        },
        {
          "id": "MDRW2100007793.1.4",
          "form": "근데 날이 좋아서 고민중요",
          "original_form": "근데 날이 좋아서 고민중요",
          "speaker_id": "2",
          "time": "20210822 10:50"
        },
        {
          "id": "MDRW2100007793.1.5",
          "form": "애들이 맨날 집에만 있으니 답답해해서 어떻게 해야되나 싶네요",
          "original_form": "애들이 맨날 집에만 있으니 답답해해서 어떻게 해야되나 싶네요",
          "speaker_id": "1",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100007793.1.6",
          "form": "계곡에 고기구우러 갈까 고민요",
          "original_form": "계곡에 고기구우러 갈까 고민요",
          "speaker_id": "2",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100007793.1.7",
          "form": "저희눈 커서 나가면 싫어해요 ㅜㅜ",
          "original_form": "저희눈 커서 나가면 싫어해요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100007793.1.8",
          "form": "아 부럽네요",
          "original_form": "아 부럽네요",
          "speaker_id": "2",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100007793.1.9",
          "form": "요샌 아예 같이 안나가요",
          "original_form": "요샌 아예 같이 안나가요",
          "speaker_id": "2",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100007793.1.10",
          "form": "오 좋으네요! 그럼 부부끼리 계곡 가시는 거예요?!",
          "original_form": "오 좋으네요! 그럼 부부끼리 계곡 가시는 거예요?!",
          "speaker_id": "1",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007793.1.11",
          "form": "지방이라 여긴 사람 없어서 갈만해여",
          "original_form": "지방이라 여긴 사람 없어서 갈만해여",
          "speaker_id": "2",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007793.1.12",
          "form": "아이들이 몇살인지 여쭤봐도 될까요?",
          "original_form": "아이들이 몇살인지 여쭤봐도 될까요?",
          "speaker_id": "1",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007793.1.13",
          "form": "부부는 좀 ㅡㅡ",
          "original_form": "부부는 좀 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007793.1.14",
          "form": "고3 초6",
          "original_form": "고3 초6",
          "speaker_id": "2",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007793.1.15",
          "form": "이요 ㅋㅋ",
          "original_form": "이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007793.1.16",
          "form": "앗, ㅋㅋㅋ ㅡㅡ 이렇게 까지나 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗, ㅋㅋㅋ ㅡㅡ 이렇게 까지나 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007793.1.17",
          "form": "어떻게 되세요?",
          "original_form": "어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007793.1.18",
          "form": "아이가 어린가요?",
          "original_form": "아이가 어린가요?",
          "speaker_id": "2",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007793.1.19",
          "form": "다들 놀래요 ㅋㅋ",
          "original_form": "다들 놀래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007793.1.20",
          "form": "아이둘 나이차이가 조금 있네요?",
          "original_form": "아이둘 나이차이가 조금 있네요?",
          "speaker_id": "1",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007793.1.21",
          "form": "저는 초3 초1이예요",
          "original_form": "저는 초3 초1이예요",
          "speaker_id": "1",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007793.1.22",
          "form": "그래서 자꾸 나가자나가자 모드 ㅠㅠ.. \r\n전제가 집에 있고 싶은데..",
          "original_form": "그래서 자꾸 나가자나가자 모드 ㅠㅠ.. \r\n전제가 집에 있고 싶은데..",
          "speaker_id": "1",
          "time": "20210822 10:55"
        },
        {
          "id": "MDRW2100007793.1.23",
          "form": "어머 귀여울때네요",
          "original_form": "어머 귀여울때네요",
          "speaker_id": "2",
          "time": "20210822 10:55"
        },
        {
          "id": "MDRW2100007793.1.24",
          "form": "산책이라도 나가야하나 싶어요",
          "original_form": "산책이라도 나가야하나 싶어요",
          "speaker_id": "1",
          "time": "20210822 10:55"
        }
      ]
    }
