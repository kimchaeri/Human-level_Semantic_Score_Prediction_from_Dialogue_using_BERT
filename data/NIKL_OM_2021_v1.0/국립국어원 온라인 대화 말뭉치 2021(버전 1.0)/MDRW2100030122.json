      "utterance": [
        {
          "id": "MDRW2100030122.1.1",
          "form": "동물키우세요?",
          "original_form": "동물키우세요?",
          "speaker_id": "1",
          "time": "20210904 09:11"
        },
        {
          "id": "MDRW2100030122.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210904 09:11"
        },
        {
          "id": "MDRW2100030122.1.3",
          "form": "아..저두안키워요",
          "original_form": "아..저두안키워요",
          "speaker_id": "1",
          "time": "20210904 09:11"
        },
        {
          "id": "MDRW2100030122.1.4",
          "form": "키우고싶은거있으세요?",
          "original_form": "키우고싶은거있으세요?",
          "speaker_id": "2",
          "time": "20210904 09:12"
        },
        {
          "id": "MDRW2100030122.1.5",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210904 09:12"
        },
        {
          "id": "MDRW2100030122.1.6",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210904 09:12"
        },
        {
          "id": "MDRW2100030122.1.7",
          "form": "고양이는 좀 싫고",
          "original_form": "고양이는 좀 싫고",
          "speaker_id": "2",
          "time": "20210904 09:12"
        },
        {
          "id": "MDRW2100030122.1.8",
          "form": "이떤거 키우고싶으세요?",
          "original_form": "이떤거 키우고싶으세요?",
          "speaker_id": "1",
          "time": "20210904 09:12"
        },
        {
          "id": "MDRW2100030122.1.9",
          "form": "작은종으로",
          "original_form": "작은종으로",
          "speaker_id": "2",
          "time": "20210904 09:12"
        },
        {
          "id": "MDRW2100030122.1.10",
          "form": "아파트이다보니",
          "original_form": "아파트이다보니",
          "speaker_id": "2",
          "time": "20210904 09:12"
        },
        {
          "id": "MDRW2100030122.1.11",
          "form": "너무큰건 감당하기어려워서",
          "original_form": "너무큰건 감당하기어려워서",
          "speaker_id": "2",
          "time": "20210904 09:13"
        },
        {
          "id": "MDRW2100030122.1.12",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210904 09:13"
        },
        {
          "id": "MDRW2100030122.1.13",
          "form": "전 아이들커서 분가하면키우려구요",
          "original_form": "전 아이들커서 분가하면키우려구요",
          "speaker_id": "1",
          "time": "20210904 09:13"
        },
        {
          "id": "MDRW2100030122.1.14",
          "form": "아들이 알러지가있어서",
          "original_form": "아들이 알러지가있어서",
          "speaker_id": "1",
          "time": "20210904 09:13"
        },
        {
          "id": "MDRW2100030122.1.15",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210904 09:13"
        },
        {
          "id": "MDRW2100030122.1.16",
          "form": "털땜에 더 심해져서",
          "original_form": "털땜에 더 심해져서",
          "speaker_id": "1",
          "time": "20210904 09:14"
        },
        {
          "id": "MDRW2100030122.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 09:14"
        },
        {
          "id": "MDRW2100030122.1.18",
          "form": "저는 좋아하는데 아이들이 좀",
          "original_form": "저는 좋아하는데 아이들이 좀",
          "speaker_id": "1",
          "time": "20210904 09:14"
        },
        {
          "id": "MDRW2100030122.1.19",
          "form": "나중에키우시면되겠네요",
          "original_form": "나중에키우시면되겠네요",
          "speaker_id": "2",
          "time": "20210904 09:14"
        }
      ]
