      "utterance": [
        {
          "id": "MDRW2100011299.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 10:51"
        },
        {
          "id": "MDRW2100011299.1.2",
          "form": "안녕하세요 혹시 기억에 남는 여행지 있으실까요?",
          "original_form": "안녕하세요 혹시 기억에 남는 여행지 있으실까요?",
          "speaker_id": "2",
          "time": "20210824 10:53"
        },
        {
          "id": "MDRW2100011299.1.3",
          "form": "국내는 강릉좋아해요",
          "original_form": "국내는 강릉좋아해요",
          "speaker_id": "1",
          "time": "20210824 11:01"
        },
        {
          "id": "MDRW2100011299.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210824 11:01"
        },
        {
          "id": "MDRW2100011299.1.5",
          "form": "저도 강릉 좋아해요",
          "original_form": "저도 강릉 좋아해요",
          "speaker_id": "2",
          "time": "20210824 11:03"
        },
        {
          "id": "MDRW2100011299.1.6",
          "form": "바다도 이쁘고 볼것도 많고",
          "original_form": "바다도 이쁘고 볼것도 많고",
          "speaker_id": "2",
          "time": "20210824 11:03"
        },
        {
          "id": "MDRW2100011299.1.7",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:08"
        },
        {
          "id": "MDRW2100011299.1.8",
          "form": "맛집도 많죠",
          "original_form": "맛집도 많죠",
          "speaker_id": "1",
          "time": "20210824 11:08"
        },
        {
          "id": "MDRW2100011299.1.9",
          "form": "강릉에서 먹었던건 순두부가 기억나네요",
          "original_form": "강릉에서 먹었던건 순두부가 기억나네요",
          "speaker_id": "2",
          "time": "20210824 11:09"
        },
        {
          "id": "MDRW2100011299.1.10",
          "form": "초당순두부",
          "original_form": "초당순두부",
          "speaker_id": "1",
          "time": "20210824 11:17"
        },
        {
          "id": "MDRW2100011299.1.11",
          "form": "유명하죠ㅋㅋㅋ",
          "original_form": "유명하죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:17"
        },
        {
          "id": "MDRW2100011299.1.12",
          "form": "맞아요ㅋㅋ 초당 순두부 아이스크림도 나왔다던데",
          "original_form": "맞아요ㅋㅋ 초당 순두부 아이스크림도 나왔다던데",
          "speaker_id": "2",
          "time": "20210824 11:19"
        },
        {
          "id": "MDRW2100011299.1.13",
          "form": "ㅋㅋㅋ 거기 강릉에",
          "original_form": "ㅋㅋㅋ 거기 강릉에",
          "speaker_id": "1",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100011299.1.14",
          "form": "젤라또도 유명하죠 유행인지",
          "original_form": "젤라또도 유명하죠 유행인지",
          "speaker_id": "1",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100011299.1.15",
          "form": "젤라또는 못먹어 봤어요",
          "original_form": "젤라또는 못먹어 봤어요",
          "speaker_id": "2",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100011299.1.16",
          "form": "무슨 맛이 맛있던가요?",
          "original_form": "무슨 맛이 맛있던가요?",
          "speaker_id": "2",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100011299.1.17",
          "form": "비슷비슷했던거같은데 기억이잘안나네요ㅋㅋㅋ",
          "original_form": "비슷비슷했던거같은데 기억이잘안나네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100011299.1.18",
          "form": "초당순두부맛도 괜찮았던것같아요",
          "original_form": "초당순두부맛도 괜찮았던것같아요",
          "speaker_id": "1",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100011299.1.19",
          "form": "전부 초당순두부군요ㅋㅋ",
          "original_form": "전부 초당순두부군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100011299.1.20",
          "form": "ㅋㅋㅋ맞아요 아예",
          "original_form": "ㅋㅋㅋ맞아요 아예",
          "speaker_id": "1",
          "time": "20210824 11:37"
        },
        {
          "id": "MDRW2100011299.1.21",
          "form": "초당순두부마을이 있잖아요ㅋㅋㅋ",
          "original_form": "초당순두부마을이 있잖아요ㅋㅋㅋ",
