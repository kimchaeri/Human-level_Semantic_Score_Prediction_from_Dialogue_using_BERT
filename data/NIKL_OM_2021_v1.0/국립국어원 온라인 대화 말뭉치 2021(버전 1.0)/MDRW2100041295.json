      "utterance": [
        {
          "id": "MDRW2100041295.1.1",
          "form": "무슨색이예용",
          "original_form": "무슨색이예용",
          "speaker_id": "2",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041295.1.2",
          "form": "브라운으로 샀어요",
          "original_form": "브라운으로 샀어요",
          "speaker_id": "1",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100041295.1.3",
          "form": "사이즈가 다행히 맞아요",
          "original_form": "사이즈가 다행히 맞아요",
          "speaker_id": "1",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100041295.1.4",
          "form": "브라운 좋네용",
          "original_form": "브라운 좋네용",
          "speaker_id": "2",
          "time": "20210905 21:39"
        },
        {
          "id": "MDRW2100041295.1.5",
          "form": "사이즈잘맞을때너무조아요",
          "original_form": "사이즈잘맞을때너무조아요",
          "speaker_id": "2",
          "time": "20210905 21:39"
        },
        {
          "id": "MDRW2100041295.1.6",
          "form": "그쵸 ㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100041295.1.7",
          "form": "안맞아면 스트레스",
          "original_form": "안맞아면 스트레스",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100041295.1.8",
          "form": "고민한 시간 다 날아가자나용ㅋㅋㅋ",
          "original_form": "고민한 시간 다 날아가자나용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100041295.1.9",
          "form": "맞아요ㅋㅋㅋㅋㅋ사이즈교환도짜증난다구요",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ사이즈교환도짜증난다구요",
          "speaker_id": "2",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100041295.1.10",
          "form": "그니까유 ㅋㅋ 그럼 현타와요",
          "original_form": "그니까유 ㅋㅋ 그럼 현타와요",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100041295.1.11",
          "form": "ㅋㅋㅋㅋㅋ흐흐",
          "original_form": "ㅋㅋㅋㅋㅋ흐흐",
          "speaker_id": "2",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100041295.1.12",
          "form": "요샌븉방에",
          "original_form": "요샌븉방에",
          "speaker_id": "2",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100041295.1.13",
          "form": "좋은할인소식이안올라와요",
          "original_form": "좋은할인소식이안올라와요",
          "speaker_id": "2",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100041295.1.14",
          "form": "다들 심심이 한다고 안올려주나",
          "original_form": "다들 심심이 한다고 안올려주나",
          "speaker_id": "1",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100041295.1.15",
          "form": "ㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210905 22:16"
        },
        {
          "id": "MDRW2100041295.1.16",
          "form": "그런것인가",
          "original_form": "그런것인가",
          "speaker_id": "2",
          "time": "20210905 22:16"
        },
        {
          "id": "MDRW2100041295.1.17",
          "form": "그런거아닐까 ㅋㅋ",
          "original_form": "그런거아닐까 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:16"
        },
        {
          "id": "MDRW2100041295.1.18",
          "form": "ㅋㅋㅋㅋ웃기당",
          "original_form": "ㅋㅋㅋㅋ웃기당",
          "speaker_id": "2",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100041295.1.19",
          "form": "같은마음인가봐",
          "original_form": "같은마음인가봐",
