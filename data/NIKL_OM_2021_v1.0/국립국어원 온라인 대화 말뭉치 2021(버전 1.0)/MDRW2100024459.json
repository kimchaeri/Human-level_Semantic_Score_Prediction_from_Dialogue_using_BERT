      "utterance": [
        {
          "id": "MDRW2100024459.1.1",
          "form": "용품 사는게 또 재미입니닷ㅋㅋ",
          "original_form": "용품 사는게 또 재미입니닷ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:37"
        },
        {
          "id": "MDRW2100024459.1.2",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:37"
        },
        {
          "id": "MDRW2100024459.1.3",
          "form": "돈쓰는건 왜 늘 즐거운지ㅋㅋ",
          "original_form": "돈쓰는건 왜 늘 즐거운지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:37"
        },
        {
          "id": "MDRW2100024459.1.4",
          "form": "무슨 장난감을 제일 좋아하나요?",
          "original_form": "무슨 장난감을 제일 좋아하나요?",
          "speaker_id": "1",
          "time": "20210831 10:37"
        },
        {
          "id": "MDRW2100024459.1.5",
          "form": "캣휠...?",
          "original_form": "캣휠...?",
          "speaker_id": "1",
          "time": "20210831 10:37"
        },
        {
          "id": "MDRW2100024459.1.6",
          "form": "샤카샤카를 제일 좋아해요!",
          "original_form": "샤카샤카를 제일 좋아해요!",
          "speaker_id": "2",
          "time": "20210831 10:37"
        },
        {
          "id": "MDRW2100024459.1.7",
          "form": "오 그게 뭔가요??",
          "original_form": "오 그게 뭔가요??",
          "speaker_id": "1",
          "time": "20210831 10:37"
        },
        {
          "id": "MDRW2100024459.1.8",
          "form": "캣휠은 지인짜 가끔 타더라구용.. 아빠랑 맨날 싸워요ㅎ",
          "original_form": "캣휠은 지인짜 가끔 타더라구용.. 아빠랑 맨날 싸워요ㅎ",
          "speaker_id": "2",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024459.1.9",
          "form": "아ㅋㅋㅋㅋ귀여워요",
          "original_form": "아ㅋㅋㅋㅋ귀여워요",
          "speaker_id": "1",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024459.1.10",
          "form": "긴 막대기 끝에 비닐 달린 장난감이에요?",
          "original_form": "긴 막대기 끝에 비닐 달린 장난감이에요?",
          "speaker_id": "2",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024459.1.11",
          "form": "!",
          "original_form": "!",
          "speaker_id": "2",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024459.1.12",
          "form": "오오!!!",
          "original_form": "오오!!!",
          "speaker_id": "1",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024459.1.13",
          "form": "역시 그런 움직이는걸 좋아하는군요",
          "original_form": "역시 그런 움직이는걸 좋아하는군요",
          "speaker_id": "1",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024459.1.14",
          "form": "ㅋㅋㅋㅋ일본건데 비싼데 일회용이에요",
          "original_form": "ㅋㅋㅋㅋ일본건데 비싼데 일회용이에요",
          "speaker_id": "2",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024459.1.15",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024459.1.16",
          "form": "엥 한번만 사용가능해요??",
          "original_form": "엥 한번만 사용가능해요??",
          "speaker_id": "1",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024459.1.17",
          "form": "파닥거리면서 소리내는게 고양이의 본능을 자극하나보더라구용",
          "original_form": "파닥거리면서 소리내는게 고양이의 본능을 자극하나보더라구용",
          "speaker_id": "2",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100024459.1.18",
          "form": "너무 약해요ㅜㅠㅜ",
          "original_form": "너무 약해요ㅜㅠㅜ",
          "speaker_id": "2",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100024459.1.19",
          "form": "끈이 매일 엉켜서 돌돌말려있어요ㅜㅠ",
          "original_form": "끈이 매일 엉켜서 돌돌말려있어요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100024459.1.20",
          "form": "풀어둬도 두 번 움직이면 다시 엉켜서 가버리더라구요",
          "original_form": "풀어둬도 두 번 움직이면 다시 엉켜서 가버리더라구요",
          "speaker_id": "2",
          "time": "20210831 10:39"
        }
      ]
