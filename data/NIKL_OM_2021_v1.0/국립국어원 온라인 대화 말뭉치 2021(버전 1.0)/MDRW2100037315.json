      "utterance": [
        {
          "id": "MDRW2100037315.1.1",
          "form": "패션에 관심 많으신가옹?",
          "original_form": "패션에 관심 많으신가옹?",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037315.1.2",
          "form": "저 편한게최고를 모토로 주워입고다녀요",
          "original_form": "저 편한게최고를 모토로 주워입고다녀요",
          "speaker_id": "2",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100037315.1.3",
          "form": "패션관심많으신가요",
          "original_form": "패션관심많으신가요",
          "speaker_id": "2",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100037315.1.4",
          "form": "저도 편한게 최고라 패션 잘 몰라요ㅋㅋ",
          "original_form": "저도 편한게 최고라 패션 잘 몰라요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037315.1.5",
          "form": "눈으로 보는정도가 좋달까용",
          "original_form": "눈으로 보는정도가 좋달까용",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037315.1.6",
          "form": "그쳐 ㅋㅋㅋㅋ옷사실때 어떤색자주사세요",
          "original_form": "그쳐 ㅋㅋㅋㅋ옷사실때 어떤색자주사세요",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100037315.1.7",
          "form": "흰색이요ㅋㅋㅋㅋ",
          "original_form": "흰색이요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 07:10"
        },
        {
