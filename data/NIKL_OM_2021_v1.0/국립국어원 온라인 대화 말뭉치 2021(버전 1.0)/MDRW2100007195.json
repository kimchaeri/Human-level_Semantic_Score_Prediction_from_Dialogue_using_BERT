      "utterance": [
        {
          "id": "MDRW2100007195.1.1",
          "form": "아 여행가고싶다 근데 제주도 가고싶어",
          "original_form": "아 여행가고싶다 근데 제주도 가고싶어",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007195.1.2",
          "form": "제주도 지금은 못가ㅜㅜ",
          "original_form": "제주도 지금은 못가ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007195.1.3",
          "form": "근데 나도 가고는 싶다",
          "original_form": "근데 나도 가고는 싶다",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007195.1.4",
          "form": "가을에 제주도 가고싶었는데ㅠㅠ이번엔 불가할듯..",
          "original_form": "가을에 제주도 가고싶었는데ㅠㅠ이번엔 불가할듯..",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007195.1.5",
          "form": "나 빼고 다들 제주도 갔다왔어",
          "original_form": "나 빼고 다들 제주도 갔다왔어",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007195.1.6",
          "form": "헐 누가 다녀오심 사무실 사람들?",
          "original_form": "헐 누가 다녀오심 사무실 사람들?",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007195.1.7",
          "form": "ㅇㅇ 학회도 제주도에서 했는데 못갔어",
          "original_form": "ㅇㅇ 학회도 제주도에서 했는데 못갔어",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007195.1.8",
          "form": "근데 학회즈음에 다들 개인적으로 제주도 다녀오더라고",
          "original_form": "근데 학회즈음에 다들 개인적으로 제주도 다녀오더라고",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007195.1.9",
          "form": "아 해외여행 가고싶다",
          "original_form": "아 해외여행 가고싶다",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007195.1.10",
          "form": "브로드웨이 가고싶다 ㅡ",
          "original_form": "브로드웨이 가고싶다 ㅡ",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007195.1.11",
          "form": "어디 갔냐",
          "original_form": "어디 갔냐",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007195.1.12",
          "form": "아 대답할 시간을 좀 주세여",
          "original_form": "아 대답할 시간을 좀 주세여",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007195.1.13",
          "form": "맞다 학회 제주도였지",
          "original_form": "맞다 학회 제주도였지",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007195.1.14",
          "form": "넵... 기다려드리겠읍니다",
          "original_form": "넵... 기다려드리겠읍니다",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007195.1.15",
          "form": "브로드웨이 진짜 한번은 가고싶다",
          "original_form": "브로드웨이 진짜 한번은 가고싶다",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007195.1.16",
          "form": "영국에 있는 뮤지컬 명소 이름이 뭐였지",
          "original_form": "영국에 있는 뮤지컬 명소 이름이 뭐였지",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007195.1.17",
          "form": "웨스트엔드!",
          "original_form": "웨스트엔드!",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007195.1.18",
          "form": "웨엔이랑 브웨는 꼭 가보고싶었는데 코로나...",
          "original_form": "웨엔이랑 브웨는 꼭 가보고싶었는데 코로나...",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007195.1.19",
          "form": "아 그거 웨스트엔드였구나 거기 가보고싶음...",
          "original_form": "아 그거 웨스트엔드였구나 거기 가보고싶음...",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007195.1.20",
          "form": "언젠가 닥터후 투어 가보고싶어",
          "original_form": "언젠가 닥터후 투어 가보고싶어",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007195.1.21",
          "form": "카디프를 가보고야 말겠다",
          "original_form": "카디프를 가보고야 말겠다",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007195.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007195.1.23",
          "form": "카디프가 스코틀랜드에 있던가?",
          "original_form": "카디프가 스코틀랜드에 있던가?",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007195.1.24",
          "form": "거기 말 알아들을 수 있나?",
          "original_form": "거기 말 알아들을 수 있나?",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007195.1.25",
          "form": "웨일스에 있을걸?",
          "original_form": "웨일스에 있을걸?",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007195.1.26",
          "form": "어떻게든 통하지않을까..?그래도 영국인데...?",
          "original_form": "어떻게든 통하지않을까..?그래도 영국인데...?",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007195.1.27",
          "form": "아 웨일스네",
          "original_form": "아 웨일스네",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007195.1.28",
          "form": "나 왜 스코틀랜드라고 생각을 했지",
          "original_form": "나 왜 스코틀랜드라고 생각을 했지",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007195.1.29",
          "form": "전에 일본 갔을때 영어 1/3 되도않는 일본어 1/3 몸짓 1/3 썼었는데 어떻게든 대화는 되더라",
          "original_form": "전에 일본 갔을때 영어 1/3 되도않는 일본어 1/3 몸짓 1/3 썼었는데 어떻게든 대화는 되더라",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007195.1.30",
          "form": "웨일스도 어떻게든 되겠지",
          "original_form": "웨일스도 어떻게든 되겠지",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007195.1.31",
          "form": "ㅋㅋㅋ하긴 사투리 심해도 영어는 영어겠지",
          "original_form": "ㅋㅋㅋ하긴 사투리 심해도 영어는 영어겠지",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007195.1.32",
          "form": "닥터후 전시회랑 카디프랑 웨스트엔드랑 가보고싶다",
          "original_form": "닥터후 전시회랑 카디프랑 웨스트엔드랑 가보고싶다",
          "speaker_id": "2",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007195.1.33",
          "form": "영국을 가야겠네",
          "original_form": "영국을 가야겠네",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007195.1.34",
          "form": "뉴욕은 어때",
          "original_form": "뉴욕은 어때",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007195.1.35",
          "form": "홍콩은 어때?",
          "original_form": "홍콩은 어때?",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007195.1.36",
          "form": "뉴욕에 뭐 있지 브로드웨이가 뉴욕인가?",
          "original_form": "뉴욕에 뭐 있지 브로드웨이가 뉴욕인가?",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007195.1.37",
          "form": "네 브로드웨이가 뉴욕에 있습니다",
          "original_form": "네 브로드웨이가 뉴욕에 있습니다",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007195.1.38",
          "form": "홍콩 가봤는데 좋았음",
          "original_form": "홍콩 가봤는데 좋았음",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007195.1.39",
          "form": "가족들이랑 갔었는데",
          "original_form": "가족들이랑 갔었는데",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007195.1.40",
          "form": "거기 음식 때문에 가고싶어",
          "original_form": "거기 음식 때문에 가고싶어",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007195.1.41",
          "form": "홍콩이랑 대만은 식도락여행하고싶다",
          "original_form": "홍콩이랑 대만은 식도락여행하고싶다",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007195.1.42",
          "form": "그때 갔다오고 한 달 뒤였나 홍콩 법률개정 터지고 시위하고 이래서 거긴 이제 갈 상황이 못 되는 것 같더라",
          "original_form": "그때 갔다오고 한 달 뒤였나 홍콩 법률개정 터지고 시위하고 이래서 거긴 이제 갈 상황이 못 되는 것 같더라",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007195.1.43",
          "form": "대만 좋아 대만 가볼래?",
          "original_form": "대만 좋아 대만 가볼래?",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007195.1.44",
          "form": "아 맞아 홍콩은....",
          "original_form": "아 맞아 홍콩은....",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007195.1.45",
          "form": "대만 좋아",
          "original_form": "대만 좋아",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007195.1.46",
          "form": "거긴 코로나에서 좀 안전한가?",
          "original_form": "거긴 코로나에서 좀 안전한가?",
          "speaker_id": "1",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007195.1.47",
          "form": "거기도 아직은 비슷하지않을까?",
          "original_form": "거기도 아직은 비슷하지않을까?",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007195.1.48",
          "form": "솔직히 내년까지 해외여행은 무리일 것 같아",
          "original_form": "솔직히 내년까지 해외여행은 무리일 것 같아",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007195.1.49",
          "form": "내후년은...?",
          "original_form": "내후년은...?",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007195.1.50",
          "form": "헐 내후년이면 우리 나이가 몇이냐...",
          "original_form": "헐 내후년이면 우리 나이가 몇이냐...",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007195.1.51",
          "form": "내후년이면 여행 정도는 되지않을까...?",
          "original_form": "내후년이면 여행 정도는 되지않을까...?",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007195.1.52",
          "form": "올해 국내여행은 갈 수 있지않으려나",
          "original_form": "올해 국내여행은 갈 수 있지않으려나",
          "speaker_id": "2",
          "time": "20210822 01:08"
        }
      ]
