      "utterance": [
        {
          "id": "MDRW2100034284.1.1",
          "form": "집에서는 하기 싫으면 바로 그만두는데",
          "original_form": "집에서는 하기 싫으면 바로 그만두는데",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100034284.1.2",
          "form": "그래도 피티는 끝까지 끌고가 주니까",
          "original_form": "그래도 피티는 끝까지 끌고가 주니까",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100034284.1.3",
          "form": "효과는  좋죠 스트레스는  좀 받을지언정ㅋㅋㄱ",
          "original_form": "효과는  좋죠 스트레스는  좀 받을지언정ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100034284.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ 그래도",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그래도",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100034284.1.5",
          "form": "너무 힘들면 속으로 피티쌤 욕하게돼요ㅋㅋㅋ",
          "original_form": "너무 힘들면 속으로 피티쌤 욕하게돼요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100034284.1.6",
          "form": "누가 지시해주고 관리해주고 그러니까 더 잘되긴하는군여",
          "original_form": "누가 지시해주고 관리해주고 그러니까 더 잘되긴하는군여",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100034284.1.7",
          "form": "악 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "악 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100034284.1.8",
          "form": "막 식단도 찍어서 보내더라구요",
          "original_form": "막 식단도 찍어서 보내더라구요",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100034284.1.9",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100034284.1.10",
          "form": "힘들거같아요 ㅠㅠ",
          "original_form": "힘들거같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100034284.1.11",
          "form": "그게 진짜 싫어요ㅋㅋ",
          "original_form": "그게 진짜 싫어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100034284.1.12",
          "form": "눈치안보이셨어요?ㅋㅋㅋ",
          "original_form": "눈치안보이셨어요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100034284.1.13",
          "form": "맨날 잔소리하고 그래요ㅠ",
          "original_form": "맨날 잔소리하고 그래요ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100034284.1.14",
          "form": "아 ㅠㅠ 싫을거같아요",
          "original_form": "아 ㅠㅠ 싫을거같아요",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100034284.1.15",
          "form": "눈치보여서 먹어놓고 안먹은척해요ㅋㅋ",
          "original_form": "눈치보여서 먹어놓고 안먹은척해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100034284.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ 뻥치면 눈치 못채시나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 뻥치면 눈치 못채시나요?",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100034284.1.17",
          "form": "근데 몸무게 재면 다티나더라구요ㅋㅋ",
          "original_form": "근데 몸무게 재면 다티나더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100034284.1.18",
          "form": "그거밖에 안먹었는데 몸무게가 는다구요?\n이런식으로요ㅋㅋㅋ",
          "original_form": "그거밖에 안먹었는데 몸무게가 는다구요?\n이런식으로요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100034284.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 너무 웃겨요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 너무 웃겨요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100034284.1.20",
