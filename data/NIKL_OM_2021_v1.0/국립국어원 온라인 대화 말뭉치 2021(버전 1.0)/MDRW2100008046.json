      "utterance": [
        {
          "id": "MDRW2100008046.1.1",
          "form": "둘찌가 윽시 안잔다이;;",
          "original_form": "둘찌가 윽시 안잔다이;;",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008046.1.2",
          "form": "언넝 재우고 밥 맛편허게 먹고픈데이~",
          "original_form": "언넝 재우고 밥 맛편허게 먹고픈데이~",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008046.1.3",
          "form": "ㅋㅋㅋ울집 둘째도 안잔다ㅋㅋ하ㅋㅋ",
          "original_form": "ㅋㅋㅋ울집 둘째도 안잔다ㅋㅋ하ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008046.1.4",
          "form": "편의점에서 삼각김밥먹고 급하게 캠핑왔닼",
          "original_form": "편의점에서 삼각김밥먹고 급하게 캠핑왔닼",
          "speaker_id": "1",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008046.1.5",
          "form": "아 캠핑간다는 집이 여기였나 ㅋㅋㅋㅋㅋ",
          "original_form": "아 캠핑간다는 집이 여기였나 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008046.1.6",
          "form": "응응 급하게 캠핑왔음ㅋㅋ",
          "original_form": "응응 급하게 캠핑왔음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008046.1.7",
          "form": "아효 진짜ㅋㅋㅋㅋㅋ놀이터에서 놀라카다가ㅡㅡㅋㅋ",
          "original_form": "아효 진짜ㅋㅋㅋㅋㅋ놀이터에서 놀라카다가ㅡㅡㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008046.1.8",
          "form": "첫째가 계속 캠핑가자해서 ㅋㅋ",
          "original_form": "첫째가 계속 캠핑가자해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008046.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ캐도 나갔다오면 시간은 잘가잖아~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ캐도 나갔다오면 시간은 잘가잖아~",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008046.1.10",
          "form": "어데로 갔디노?,",
          "original_form": "어데로 갔디노?,",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008046.1.11",
          "form": "응응  고령인데 ㅋㅋㅋ",
          "original_form": "응응  고령인데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008046.1.12",
          "form": "지나가는길에 그냥 막 설치함ㅋㅋ",
          "original_form": "지나가는길에 그냥 막 설치함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008046.1.13",
          "form": "오늘 집안일은 패쓰네~",
          "original_form": "오늘 집안일은 패쓰네~",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008046.1.14",
          "form": "원래 그런게 잼있음 ㅋㅋㅋㅋ",
          "original_form": "원래 그런게 잼있음 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008046.1.15",
          "form": "여치랑 나비잡고ㅋㅋㅋㅋ지금은 자유시간ㅋㅋ",
          "original_form": "여치랑 나비잡고ㅋㅋㅋㅋ지금은 자유시간ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008046.1.16",
          "form": "맞아 ㅋㅋㅋ애들 좋아하니 좋다ㅋ",
          "original_form": "맞아 ㅋㅋㅋ애들 좋아하니 좋다ㅋ",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008046.1.17",
          "form": "우린 온종일 폰이다~",
          "original_form": "우린 온종일 폰이다~",
          "speaker_id": "2",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008046.1.18",
          "form": "할미집 할게 없더유ㅠ",
          "original_form": "할미집 할게 없더유ㅠ",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008046.1.19",
          "form": "맞아 우리도 시댁이나 친정가면",
          "original_form": "맞아 우리도 시댁이나 친정가면",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008046.1.20",
          "form": "종일 폰이다ㅜ",
          "original_form": "종일 폰이다ㅜ",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008046.1.21",
          "form": "할게 없다 진짜~",
          "original_form": "할게 없다 진짜~",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008046.1.22",
          "form": "밖에 뛰놀데도 없고~",
          "original_form": "밖에 뛰놀데도 없고~",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008046.1.23",
          "form": "아예 시골이면 좋긋다 ㅋㅋㅋ",
          "original_form": "아예 시골이면 좋긋다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008046.1.24",
          "form": "맞어 ㅋㅋ 우리사는 동네도 뛰놀데는 많은데ㅋㅋ",
          "original_form": "맞어 ㅋㅋ 우리사는 동네도 뛰놀데는 많은데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008046.1.25",
          "form": "하긴 가까운 과학관도 안가미ㅋㅋㅋㅋㅋ",
          "original_form": "하긴 가까운 과학관도 안가미ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008046.1.26",
          "form": "뭔가 마당에서 뛰놀고픔 ㅋㅋㅋ",
          "original_form": "뭔가 마당에서 뛰놀고픔 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008046.1.27",
          "form": "울타리쳐져있는 ㅋㅋ",
          "original_form": "울타리쳐져있는 ㅋㅋ",
          "speaker_id": "2",
