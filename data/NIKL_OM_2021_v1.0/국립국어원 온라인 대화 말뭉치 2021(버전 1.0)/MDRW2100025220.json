      "utterance": [
        {
          "id": "MDRW2100025220.1.1",
          "form": "ㅋㅋㅋㅋㅋ 음... 그러게요!",
          "original_form": "ㅋㅋㅋㅋㅋ 음... 그러게요!",
          "speaker_id": "2",
