      "utterance": [
        {
          "id": "MDRW2100040352.1.1",
          "form": "피자 저도 좋아해서 자주먹는데",
          "original_form": "피자 저도 좋아해서 자주먹는데",
          "speaker_id": "1",
          "time": "20210904 16:53"
        },
        {
          "id": "MDRW2100040352.1.2",
          "form": "이제 만들어먹을까싶어요",
          "original_form": "이제 만들어먹을까싶어요",
          "speaker_id": "1",
          "time": "20210904 16:53"
        },
        {
          "id": "MDRW2100040352.1.3",
          "form": "오... 요리 잘하세요?",
          "original_form": "오... 요리 잘하세요?",
          "speaker_id": "2",
          "time": "20210904 16:53"
        },
        {
          "id": "MDRW2100040352.1.4",
          "form": "아니요ㅋㅋ 요리하느니 사먹는다주의인데",
          "original_form": "아니요ㅋㅋ 요리하느니 사먹는다주의인데",
          "speaker_id": "1",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100040352.1.5",
          "form": "피자 너무자주시켜먹어서",
          "original_form": "피자 너무자주시켜먹어서",
          "speaker_id": "1",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100040352.1.6",
          "form": "이제 스스로 해보자 싶어서요",
          "original_form": "이제 스스로 해보자 싶어서요",
          "speaker_id": "1",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100040352.1.7",
          "form": "아ㅋㅋ 오븐 쓰는거 넘 어려워서 쉽지 않을거 같아요",
          "original_form": "아ㅋㅋ 오븐 쓰는거 넘 어려워서 쉽지 않을거 같아요",
          "speaker_id": "2",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100040352.1.8",
          "form": "그런가요 한번도 안해봐서 그런건 생각안해봤네요",
          "original_form": "그런가요 한번도 안해봐서 그런건 생각안해봤네요",
          "speaker_id": "1",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100040352.1.9",
          "form": "도우는 일단 생지샀어요ㅎ",
          "original_form": "도우는 일단 생지샀어요ㅎ",
          "speaker_id": "1",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100040352.1.10",
          "form": "반죽은 안해도 되서 다행이네요ㅋㅋㅋ",
          "original_form": "반죽은 안해도 되서 다행이네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:34"
        },
        {
          "id": "MDRW2100040352.1.11",
          "form": "넵 어제 해먹어봤는데",
          "original_form": "넵 어제 해먹어봤는데",
          "speaker_id": "1",
          "time": "20210906 13:41"
        },
        {
          "id": "MDRW2100040352.1.12",
          "form": "오븐에 너무 오래둬서",
          "original_form": "오븐에 너무 오래둬서",
          "speaker_id": "1",
          "time": "20210906 13:42"
        },
        {
          "id": "MDRW2100040352.1.13",
          "form": "바싹 말랐어용",
          "original_form": "바싹 말랐어용",
          "speaker_id": "1",
          "time": "20210906 13:42"
        },
        {
          "id": "MDRW2100040352.1.14",
          "form": "토핑도요? 치즈만 살아있어두 먹을만하던데요",
          "original_form": "토핑도요? 치즈만 살아있어두 먹을만하던데요",
          "speaker_id": "2",
          "time": "20210906 13:44"
        },
        {
          "id": "MDRW2100040352.1.15",
          "form": "네 고구마 올렸는데",
          "original_form": "네 고구마 올렸는데",
          "speaker_id": "1",
          "time": "20210906 13:44"
        },
        {
          "id": "MDRW2100040352.1.16",
          "form": "너무 익었더라구요",
          "original_form": "너무 익었더라구요",
          "speaker_id": "1",
          "time": "20210906 13:44"
        },
        {
          "id": "MDRW2100040352.1.17",
          "form": "아깝네요.. 또 만들어보실건가요?",
          "original_form": "아깝네요.. 또 만들어보실건가요?",
          "speaker_id": "2",
          "time": "20210906 13:49"
        },
        {
          "id": "MDRW2100040352.1.18",
          "form": "아 넵 도우가 아직 남아있어서ㅋㅋ",
          "original_form": "아 넵 도우가 아직 남아있어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:50"
        },
        {
          "id": "MDRW2100040352.1.19",
          "form": "ㅋㅋㅋ한번 실패했으니까 다음엔 성공하지 않을까요?",
          "original_form": "ㅋㅋㅋ한번 실패했으니까 다음엔 성공하지 않을까요?",
          "speaker_id": "2",
          "time": "20210906 14:13"
        },
        {
          "id": "MDRW2100040352.1.20",
          "form": "넵 큰 깨달음을 얻었답니다",
          "original_form": "넵 큰 깨달음을 얻었답니다",
          "speaker_id": "1",
          "time": "20210906 14:14"
        },
        {
          "id": "MDRW2100040352.1.21",
          "form": "역시 조리법을 잘 따라야겠다는거요",
          "original_form": "역시 조리법을 잘 따라야겠다는거요",
