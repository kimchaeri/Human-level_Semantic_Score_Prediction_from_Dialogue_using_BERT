        {
          "id": "MDRW2100031050.1.1",
          "form": "안 매운 음식도 같이 먹었니",
          "original_form": "안 매운 음식도 같이 먹었니",
          "speaker_id": "2",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100031050.1.2",
          "form": "응 안매운 음식이랑 같이 먹었어",
          "original_form": "응 안매운 음식이랑 같이 먹었어",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
