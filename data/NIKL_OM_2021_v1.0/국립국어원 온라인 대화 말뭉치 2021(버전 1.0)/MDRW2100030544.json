      "utterance": [
        {
          "id": "MDRW2100030544.1.1",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 12:39"
        },
        {
          "id": "MDRW2100030544.1.2",
          "form": "그럼 이번 추석에 가족보러 이동 안해도 되겠군요",
          "original_form": "그럼 이번 추석에 가족보러 이동 안해도 되겠군요",
          "speaker_id": "1",
          "time": "20210904 14:37"
        },
        {
          "id": "MDRW2100030544.1.3",
          "form": "항상 이동을 안했어요 ㅎㅎ 친척도 제가 사는 곳에 살아서 ㅎㅎ",
          "original_form": "항상 이동을 안했어요 ㅎㅎ 친척도 제가 사는 곳에 살아서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:40"
        },
        {
          "id": "MDRW2100030544.1.4",
          "form": "그래요? 좋네요",
          "original_form": "그래요? 좋네요",
          "speaker_id": "1",
          "time": "20210904 19:10"
        },
        {
          "id": "MDRW2100030544.1.5",
          "form": "명절때 이동하는 것도 큰 스트레스인데 말이죠",
          "original_form": "명절때 이동하는 것도 큰 스트레스인데 말이죠",
          "speaker_id": "1",
          "time": "20210904 19:11"
        },
        {
          "id": "MDRW2100030544.1.6",
          "form": "ㅎㅎ 한번도 격어보지못했어요",
          "original_form": "ㅎㅎ 한번도 격어보지못했어요",
          "speaker_id": "2",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100030544.1.7",
          "form": "뉴스에서만 봤죠 호",
          "original_form": "뉴스에서만 봤죠 호",
          "speaker_id": "2",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100030544.1.8",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100030544.1.9",
          "form": "장점이 크네요",
          "original_form": "장점이 크네요",
          "speaker_id": "1",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100030544.1.10",
          "form": "저희도 아버님을 모셨기 때문에 그동안 가족들이 우리 집으로 모였습니다",
          "original_form": "저희도 아버님을 모셨기 때문에 그동안 가족들이 우리 집으로 모였습니다",
          "speaker_id": "1",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100030544.1.11",
          "form": "대신 제 아내가 고생이 많았죠",
          "original_form": "대신 제 아내가 고생이 많았죠",
          "speaker_id": "1",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100030544.1.12",
          "form": "그럼 더 좋으셨을거같아요",
          "original_form": "그럼 더 좋으셨을거같아요",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100030544.1.13",
          "form": "음식하느라",
          "original_form": "음식하느라",
          "speaker_id": "1",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100030544.1.14",
          "form": "그쵸..",
          "original_form": "그쵸..",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100030544.1.15",
          "form": "아내분이 힘드시죠 ㅠ",
          "original_form": "아내분이 힘드시죠 ㅠ",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100030544.1.16",
          "form": "제가 장남도 아닌데 아버지를 모시게 되어서",
          "original_form": "제가 장남도 아닌데 아버지를 모시게 되어서",
          "speaker_id": "1",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100030544.1.17",
          "form": "아내가 갑자기 맏며느리 노릇하느라 고생이 많았습니다",
          "original_form": "아내가 갑자기 맏며느리 노릇하느라 고생이 많았습니다",
          "speaker_id": "1",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100030544.1.18",
          "form": "하늘이님 가족은 가까운 친척집으로 가는 편이었나요?",
          "original_form": "하늘이님 가족은 가까운 친척집으로 가는 편이었나요?",
          "speaker_id": "1",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100030544.1.19",
          "form": "어릴땐 갔었어요",
          "original_form": "어릴땐 갔었어요",
          "speaker_id": "2",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100030544.1.20",
          "form": "크고 사이가 안좋아지면서 안가게 되었어요",
          "original_form": "크고 사이가 안좋아지면서 안가게 되었어요",
          "speaker_id": "2",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100030544.1.21",
          "form": "그런 사연이 있었군요",
          "original_form": "그런 사연이 있었군요",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100030544.1.22",
          "form": "그렇다면 어느날 부터는 추석때 가족끼리만 시간을 보냈겠네요",
          "original_form": "그렇다면 어느날 부터는 추석때 가족끼리만 시간을 보냈겠네요",
          "speaker_id": "1",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100030544.1.23",
          "form": "그랬어요 ㅋㅋ 이번에도 그럴듯하네요",
          "original_form": "그랬어요 ㅋㅋ 이번에도 그럴듯하네요",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100030544.1.24",
          "form": "그렇다면 여느 추석과 크게 다르지는 않겠군요",
          "original_form": "그렇다면 여느 추석과 크게 다르지는 않겠군요",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100030544.1.25",
          "form": "음 다를 수도 있긴한데 아직 확정이 안됐어요.",
          "original_form": "음 다를 수도 있긴한데 아직 확정이 안됐어요.",
          "speaker_id": "2",
          "time": "20210905 22:33"
        }
      ]
    }
