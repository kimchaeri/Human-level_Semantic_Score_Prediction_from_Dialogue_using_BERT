      "utterance": [
        {
          "id": "MDRW2100046167.1.1",
          "form": "ㅋㅋ그럼",
          "original_form": "ㅋㅋ그럼",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046167.1.2",
          "form": "갈비찜은요?",
          "original_form": "갈비찜은요?",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046167.1.3",
          "form": "짭쪼롬하고",
          "original_form": "짭쪼롬하고",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046167.1.4",
          "form": "부드러운맛?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "부드러운맛?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046167.1.5",
          "form": "ㅋㅋㅋㅋㅋ갈비찜 맛있나요?",
          "original_form": "ㅋㅋㅋㅋㅋ갈비찜 맛있나요?",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046167.1.6",
          "form": "안 먹은지 오래돼서ㅜ",
          "original_form": "안 먹은지 오래돼서ㅜ",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046167.1.7",
          "form": "사실 제가원한거는",
          "original_form": "사실 제가원한거는",
          "speaker_id": "1",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046167.1.8",
          "form": "아주찐한 갈비찜인데",
          "original_form": "아주찐한 갈비찜인데",
          "speaker_id": "1",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046167.1.9",
          "form": "얘는 맛이 좀 약하더라고요ㅠ",
          "original_form": "얘는 맛이 좀 약하더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046167.1.10",
          "form": "그래도 맛있을거같아요",
          "original_form": "그래도 맛있을거같아요",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046167.1.11",
          "form": "고기 안먹은지 오래ㅠ",
          "original_form": "고기 안먹은지 오래ㅠ",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046167.1.12",
          "form": "고기 드세요ㅜㅜ",
          "original_form": "고기 드세요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046167.1.13",
          "form": "요즘같이 힘들때 고기를 먹어줘야한다구요",
          "original_form": "요즘같이 힘들때 고기를 먹어줘야한다구요",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046167.1.14",
          "form": "삼겹살 너무 비싸서요",
          "original_form": "삼겹살 너무 비싸서요",
          "speaker_id": "2",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046167.1.15",
          "form": "그럴때는",
          "original_form": "그럴때는",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046167.1.16",
          "form": "맛은 좀 덜하지만",
          "original_form": "맛은 좀 덜하지만",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046167.1.17",
          "form": "앞다리살이라도",
          "original_form": "앞다리살이라도",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046167.1.18",
          "form": "불고기나 제육드세요",
          "original_form": "불고기나 제육드세요",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046167.1.19",
          "form": "근데 제가 제육을 별로 안좋아해서..",
          "original_form": "근데 제가 제육을 별로 안좋아해서..",
          "speaker_id": "2",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046167.1.20",
          "form": "왜요??????",
          "original_form": "왜요??????",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046167.1.21",
          "form": "진짜 맛있는데ㅜ",
          "original_form": "진짜 맛있는데ㅜ",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046167.1.22",
          "form": "모르겠어요ㅋ",
          "original_form": "모르겠어요ㅋ",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046167.1.23",
          "form": "뭔가.. 별로예요ㅋ",
          "original_form": "뭔가.. 별로예요ㅋ",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046167.1.24",
          "form": "안타깝다ㅜㅜㅜ 엄청 맛있는데ㅜㅜ",
          "original_form": "안타깝다ㅜㅜㅜ 엄청 맛있는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046167.1.25",
          "form": "삼겹살이 최고예요ㅋㅋ",
          "original_form": "삼겹살이 최고예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:58"
        }
      ]
