      "utterance": [
        {
          "id": "MDRW2100033452.1.1",
          "form": "요새 애들 학교 가나요",
          "original_form": "요새 애들 학교 가나요",
          "speaker_id": "1",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100033452.1.2",
          "form": "네 전면등교요",
          "original_form": "네 전면등교요",
          "speaker_id": "2",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100033452.1.3",
          "form": "좀 낫겠네요",
          "original_form": "좀 낫겠네요",
          "speaker_id": "1",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100033452.1.4",
          "form": "온라인 수업때문에 태블릿이라 노트북 엄청 팔렸다는데",
          "original_form": "온라인 수업때문에 태블릿이라 노트북 엄청 팔렸다는데",
          "speaker_id": "1",
          "time": "20210823 22:55"
        },
        {
          "id": "MDRW2100033452.1.5",
          "form": "그랬데요",
          "original_form": "그랬데요",
          "speaker_id": "2",
          "time": "20210823 22:56"
        },
        {
          "id": "MDRW2100033452.1.6",
          "form": "없는친구는 폰으로 하는데 불편하죠",
          "original_form": "없는친구는 폰으로 하는데 불편하죠",
          "speaker_id": "2",
          "time": "20210823 22:56"
        },
        {
          "id": "MDRW2100033452.1.7",
          "form": "넘 오래 그리수업하니 살수밖에 없어요",
          "original_form": "넘 오래 그리수업하니 살수밖에 없어요",
          "speaker_id": "2",
          "time": "20210823 22:57"
        },
        {
          "id": "MDRW2100033452.1.8",
          "form": "나의 학교생활은 너무",
          "original_form": "나의 학교생활은 너무",
          "speaker_id": "1",
          "time": "20210823 22:58"
        },
        {
          "id": "MDRW2100033452.1.9",
          "form": "오래전 이야기인데",
          "original_form": "오래전 이야기인데",
          "speaker_id": "1",
          "time": "20210823 22:58"
        },
        {
          "id": "MDRW2100033452.1.10",
          "form": "네 어땠나요",
          "original_form": "네 어땠나요",
          "speaker_id": "2",
          "time": "20210823 22:58"
        },
        {
          "id": "MDRW2100033452.1.11",
          "form": "학교 사랑ㅅ",
          "original_form": "학교 사랑ㅅ",
          "speaker_id": "1",
          "time": "20210823 22:58"
        },
        {
          "id": "MDRW2100033452.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 22:58"
        },
        {
          "id": "MDRW2100033452.1.13",
          "form": "학교사람",
          "original_form": "학교사람",
          "speaker_id": "1",
          "time": "20210823 22:58"
        },
        {
          "id": "MDRW2100033452.1.14",
          "form": "해보셔요 풀어라 플어라",
          "original_form": "해보셔요 풀어라 플어라",
          "speaker_id": "2",
          "time": "20210823 22:58"
        },
        {
          "id": "MDRW2100033452.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 22:58"
        },
        {
          "id": "MDRW2100033452.1.16",
          "form": "아 기숙사 생활은 좋았어요",
          "original_form": "아 기숙사 생활은 좋았어요",
          "speaker_id": "1",
          "time": "20210823 22:58"
        },
        {
          "id": "MDRW2100033452.1.17",
          "form": "다른도시로 대학가서 그거하나는 좋았음",
          "original_form": "다른도시로 대학가서 그거하나는 좋았음",
          "speaker_id": "1",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100033452.1.18",
          "form": "네 근데요?",
          "original_form": "네 근데요?",
          "speaker_id": "2",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100033452.1.19",
          "form": "프리덤!",
          "original_form": "프리덤!",
          "speaker_id": "1",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100033452.1.20",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100033452.1.21",
          "form": "자유를 달라",
          "original_form": "자유를 달라",
          "speaker_id": "2",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100033452.1.22",
          "form": "야식 엄청 시켜먹었던 기억이나요",
          "original_form": "야식 엄청 시켜먹었던 기억이나요",
          "speaker_id": "1",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100033452.1.23",
          "form": "기숙사 룸메랑",
          "original_form": "기숙사 룸메랑",
          "speaker_id": "1",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100033452.1.24",
          "form": "오 잼나게 지내셨네요",
          "original_form": "오 잼나게 지내셨네요",
          "speaker_id": "2",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100033452.1.25",
          "form": "아 그러고보니 저희땐 방마다 전화가있었어요",
          "original_form": "아 그러고보니 저희땐 방마다 전화가있었어요",
          "speaker_id": "1",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100033452.1.26",
          "form": "알바 좀 하셔야 했겠는데요",
          "original_form": "알바 좀 하셔야 했겠는데요",
          "speaker_id": "2",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100033452.1.27",
          "form": "와 진짜 추억돋아",
          "original_form": "와 진짜 추억돋아",
          "speaker_id": "1",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100033452.1.28",
          "form": "야식 많이 먹으려면 ㅎㅎ",
          "original_form": "야식 많이 먹으려면 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100033452.1.29",
          "form": "전화 와 오래전이더",
          "original_form": "전화 와 오래전이더",
          "speaker_id": "2",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100033452.1.30",
          "form": "요새 애들 전화 모르죠 ㅋㅋ",
          "original_form": "요새 애들 전화 모르죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100033452.1.31",
          "form": "물론 다들 핸드폰 있던  시대지만",
          "original_form": "물론 다들 핸드폰 있던  시대지만",
          "speaker_id": "1",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100033452.1.32",
          "form": "근데  그전화로",
          "original_form": "근데  그전화로",
          "speaker_id": "1",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100033452.1.33",
          "form": "근데  그전화로",
          "original_form": "근데  그전화로",
          "speaker_id": "1",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100033452.1.34",
          "form": "근데  그전화로",
          "original_form": "근데  그전화로",
          "speaker_id": "1",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100033452.1.35",
          "form": "내몸이불타고있다",
          "original_form": "내몸이불타고있다",
          "speaker_id": "1",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100033452.1.36",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100033452.1.37",
          "form": "이런전화가와서",
          "original_form": "이런전화가와서",
          "speaker_id": "1",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100033452.1.38",
          "form": "ㅋㅋ푸하하 내몸이 불탄다니",
          "original_form": "ㅋㅋ푸하하 내몸이 불탄다니",
          "speaker_id": "2",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100033452.1.39",
          "form": "무섭지만 웃기네요",
          "original_form": "무섭지만 웃기네요",
          "speaker_id": "2",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100033452.1.40",
          "form": "순화 순화 ㅋㅋ",
          "original_form": "순화 순화 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100033452.1.41",
          "form": "아니 무서운게 아니라",
          "original_form": "아니 무서운게 아니라",
          "speaker_id": "1",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100033452.1.42",
          "form": "내몸이 간지럽다?ㅋㅋㅋ",
          "original_form": "내몸이 간지럽다?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100033452.1.43",
          "form": "너는 무얼 입고있느냐 여인네여 ㅋㅋ",
          "original_form": "너는 무얼 입고있느냐 여인네여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100033452.1.44",
          "form": "ㅋㅋ그런 미 녀석이",
          "original_form": "ㅋㅋ그런 미 녀석이",
          "speaker_id": "2",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100033452.1.45",
          "form": "참나 좀 맞아야겠네요",
          "original_form": "참나 좀 맞아야겠네요",
          "speaker_id": "2",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100033452.1.46",
          "form": "변 태 인데요 ㅡㅡ",
          "original_form": "변 태 인데요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100033452.1.47",
          "form": "예전엔 전화로 이상한 전화 많았어요",
          "original_form": "예전엔 전화로 이상한 전화 많았어요",
          "speaker_id": "2",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100033452.1.48",
          "form": "그래서 없어졌었죠 ㅋ아련한 추억 ㅋㅋ",
          "original_form": "그래서 없어졌었죠 ㅋ아련한 추억 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100033452.1.49",
          "form": "웃긴 녀석이네요",
          "original_form": "웃긴 녀석이네요",
          "speaker_id": "2",
          "time": "20210823 23:05"
        }
      ]
