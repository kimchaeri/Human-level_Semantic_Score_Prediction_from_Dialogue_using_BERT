      "utterance": [
        {
          "id": "MDRW2100000596.1.1",
          "form": "반려동물 있으세요?",
          "original_form": "반려동물 있으세요?",
          "speaker_id": "2",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000596.1.2",
          "form": "네 강아지 키워요!",
          "original_form": "네 강아지 키워요!",
          "speaker_id": "1",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000596.1.3",
          "form": "name2님은 반려동물 있으세요?",
          "original_form": "&name2&님은 반려동물 있으세요?",
          "speaker_id": "1",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000596.1.4",
          "form": "키우던 댕댕이 작년에 무지개다리 건넜어요!",
          "original_form": "키우던 댕댕이 작년에 무지개다리 건넜어요!",
          "speaker_id": "2",
          "time": "20210819 17:03"
        },
        {
          "id": "MDRW2100000596.1.5",
          "form": "그래서 아직은 ㅠ_ㅠ",
          "original_form": "그래서 아직은 ㅠ_ㅠ",
          "speaker_id": "2",
          "time": "20210819 17:03"
        },
        {
          "id": "MDRW2100000596.1.6",
          "form": "강아지는 사랑이에요 정말",
          "original_form": "강아지는 사랑이에요 정말",
          "speaker_id": "2",
          "time": "20210819 17:03"
        },
        {
          "id": "MDRW2100000596.1.7",
          "form": "헉,,,ㅠㅜㅜ저희집 애도 나이가 많은 편이라,,하루하루가 더 소중해져요ㅠㅜ",
          "original_form": "헉,,,ㅠㅜㅜ저희집 애도 나이가 많은 편이라,,하루하루가 더 소중해져요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000596.1.8",
          "form": "나이들수록 아픈곳도 많아질텐데 걱정이예요ㅜ",
          "original_form": "나이들수록 아픈곳도 많아질텐데 걱정이예요ㅜ",
          "speaker_id": "1",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000596.1.9",
          "form": "네 맞아요! 우리 댕댕이 19년 같이 살았거든요?",
          "original_form": "네 맞아요! 우리 댕댕이 19년 같이 살았거든요?",
          "speaker_id": "2",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000596.1.10",
          "form": "보내고 나니까 후회되더라고요",
          "original_form": "보내고 나니까 후회되더라고요",
          "speaker_id": "2",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000596.1.11",
          "form": "더 잘해주고 더 많이 사랑해 줄걸",
          "original_form": "더 잘해주고 더 많이 사랑해 줄걸",
          "speaker_id": "2",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000596.1.12",
          "form": "우와 그래도 오래 살았네요ㅠㅜ 저희집 애는 이제 10살인데 벌써부터 너무 걱정돼요ㅠㅜ",
          "original_form": "우와 그래도 오래 살았네요ㅠㅜ 저희집 애는 이제 10살인데 벌써부터 너무 걱정돼요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000596.1.13",
          "form": "그 마음 완전 이해해요!",
          "original_form": "그 마음 완전 이해해요!",
          "speaker_id": "2",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000596.1.14",
          "form": "매일매일 사랑해 주세요!",
          "original_form": "매일매일 사랑해 주세요!",
          "speaker_id": "2",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000596.1.15",
          "form": "그럴려구요ㅠㅜ강아지 보험도 들어놨는데 피보험자에 강아지들 이름 써져있는게 짠하면서도 귀엽더라고요ㅋㅋㅋ",
          "original_form": "그럴려구요ㅠㅜ강아지 보험도 들어놨는데 피보험자에 강아지들 이름 써져있는게 짠하면서도 귀엽더라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000596.1.16",
          "form": "맞아요! 우리 댕댕이 아가때는 그런것도 없고",
          "original_form": "맞아요! 우리 댕댕이 아가때는 그런것도 없고",
          "speaker_id": "2",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000596.1.17",
          "form": "좋은 집, 사료 등등 많지 않았는데",
          "original_form": "좋은 집, 사료 등등 많지 않았는데",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000596.1.18",
          "form": "요즘에는 진짜 좋은 것도 많더라고요",
          "original_form": "요즘에는 진짜 좋은 것도 많더라고요",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000596.1.19",
          "form": "진짜요ㅋㅋㅋ요번에 강아지 영양상태 체크해주는 키트도 샀어요ㅋㅋㅋ",
          "original_form": "진짜요ㅋㅋㅋ요번에 강아지 영양상태 체크해주는 키트도 샀어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000596.1.20",
          "form": "귀여운 아이템들도 많더라고요",
          "original_form": "귀여운 아이템들도 많더라고요",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000596.1.21",
          "form": "우와 진짜 신기방기한 게 많네요",
          "original_form": "우와 진짜 신기방기한 게 많네요",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000596.1.22",
          "form": "그쵸ㅋㅋㅋㅋㅋㅋ옷도 귀여운 게 많고ㅠㅜ해주고싶은게 엄청 많아요ㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋㅋㅋㅋ옷도 귀여운 게 많고ㅠㅜ해주고싶은게 엄청 많아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000596.1.23",
          "form": "완전 애기때는 휴대폰 카메라도 보급되기 전이라",
          "original_form": "완전 애기때는 휴대폰 카메라도 보급되기 전이라",
          "speaker_id": "2",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000596.1.24",
          "form": "사진도 별로 없어서 속상했어요",
          "original_form": "사진도 별로 없어서 속상했어요",
          "speaker_id": "2",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000596.1.25",
          "form": "사진도 많이 많이 찍어두세요!",
          "original_form": "사진도 많이 많이 찍어두세요!",
          "speaker_id": "2",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000596.1.26",
          "form": "네ㅜㅠ동영상도 많이 찍고 있어요ㅠㅜ",
          "original_form": "네ㅜㅠ동영상도 많이 찍고 있어요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210819 17:30"
        }
      ]
