      "utterance": [
        {
          "id": "MDRW2100011715.1.1",
          "form": "반려동물 키우시나요",
          "original_form": "반려동물 키우시나요",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011715.1.2",
          "form": "아뇨 안키워요 name1은요?",
          "original_form": "아뇨 안키워요 &name1&은요?",
          "speaker_id": "2",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011715.1.3",
          "form": "저도 안키워요",
          "original_form": "저도 안키워요",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011715.1.4",
          "form": "어렸을때 닭 키운적있어요",
          "original_form": "어렸을때 닭 키운적있어요",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011715.1.5",
          "form": "오 닭을요",
          "original_form": "오 닭을요",
          "speaker_id": "2",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011715.1.6",
          "form": "병아리때부터 키우셨어요?",
          "original_form": "병아리때부터 키우셨어요?",
          "speaker_id": "2",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011715.1.7",
          "form": "초등학생때 학교앞에서 500원주고 데려와서",
          "original_form": "초등학생때 학교앞에서 500원주고 데려와서",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011715.1.8",
          "form": "애지중지 키웠네요",
          "original_form": "애지중지 키웠네요",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011715.1.9",
          "form": "우와 저는 키우다 죽었는데 잘키우시나봐요",
          "original_form": "우와 저는 키우다 죽었는데 잘키우시나봐요",
          "speaker_id": "2",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011715.1.10",
          "form": "따듯하게 해주면 잘커요",
          "original_form": "따듯하게 해주면 잘커요",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011715.1.11",
          "form": "동물 키워보신적 있으신가요?",
          "original_form": "동물 키워보신적 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011715.1.12",
          "form": "어릴때 청거북이 키워봤어요",
          "original_form": "어릴때 청거북이 키워봤어요",
          "speaker_id": "2",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011715.1.13",
          "form": "거북이 귀엽겠네요",
          "original_form": "거북이 귀엽겠네요",
          "speaker_id": "1",
          "time": "20210824 15:31"
        },
        {
          "id": "MDRW2100011715.1.14",
          "form": "네 ㅎㅎ 그게 사료만 먹는게아니고",
          "original_form": "네 ㅎㅎ 그게 사료만 먹는게아니고",
          "speaker_id": "2",
          "time": "20210824 15:31"
        },
        {
          "id": "MDRW2100011715.1.15",
          "form": "생고기도 주면 먹거든요 야생성이 살아있는거같고 귀여웠어요",
          "original_form": "생고기도 주면 먹거든요 야생성이 살아있는거같고 귀여웠어요",
          "speaker_id": "2",
          "time": "20210824 15:31"
        },
        {
          "id": "MDRW2100011715.1.16",
          "form": "와 거북이가 고기도 먹나요?",
          "original_form": "와 거북이가 고기도 먹나요?",
          "speaker_id": "1",
          "time": "20210824 15:32"
        },
        {
          "id": "MDRW2100011715.1.17",
          "form": "초식동물인줄 알았는데",
          "original_form": "초식동물인줄 알았는데",
          "speaker_id": "1",
          "time": "20210824 15:32"
        },
        {
          "id": "MDRW2100011715.1.18",
          "form": "네 주면 먹던데요 돼지고기로 요리할때 자투리 줘봤어요",
          "original_form": "네 주면 먹던데요 돼지고기로 요리할때 자투리 줘봤어요",
          "speaker_id": "2",
          "time": "20210824 15:32"
        },
        {
          "id": "MDRW2100011715.1.19",
          "form": "유튜브로 야채나 과일먹는것만봐서 그런지",
          "original_form": "유튜브로 야채나 과일먹는것만봐서 그런지",
          "speaker_id": "1",
          "time": "20210824 15:37"
        },
        {
          "id": "MDRW2100011715.1.20",
          "form": "충격이네요..",
          "original_form": "충격이네요..",
          "speaker_id": "1",
          "time": "20210824 15:37"
        }
      ]
