      "utterance": [
        {
          "id": "MDRW2100039977.1.1",
          "form": "방금 니트랑 바지사고왓어요",
          "original_form": "방금 니트랑 바지사고왓어요",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039977.1.2",
          "form": "오 이제 가을 맞이 옷 장만 하셨나여",
          "original_form": "오 이제 가을 맞이 옷 장만 하셨나여",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039977.1.3",
          "form": "네네 고민 오지게하다가 주문했어요 ㅋㅋ",
          "original_form": "네네 고민 오지게하다가 주문했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100039977.1.4",
          "form": "ㅋㅋ잘하셨어요",
          "original_form": "ㅋㅋ잘하셨어요",
          "speaker_id": "2",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100039977.1.5",
          "form": "저는 요번 가을은 꾹 참아볼까해요",
          "original_form": "저는 요번 가을은 꾹 참아볼까해요",
          "speaker_id": "2",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100039977.1.6",
          "form": "매번 옷 값이 만만치가 않더라고요",
          "original_form": "매번 옷 값이 만만치가 않더라고요",
          "speaker_id": "2",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100039977.1.7",
          "form": "맞아요ㅠㅠ 원하는 거 고르면 10만원 훌쩍",
          "original_form": "맞아요ㅠㅠ 원하는 거 고르면 10만원 훌쩍",
          "speaker_id": "1",
          "time": "20210904 14:29"
        },
        {
          "id": "MDRW2100039977.1.8",
          "form": "옷값도 왜이렇게 비싼지 2-3개넣으면 10만원 우습더라고요ㅠ",
          "original_form": "옷값도 왜이렇게 비싼지 2-3개넣으면 10만원 우습더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210904 14:35"
        },
        {
          "id": "MDRW2100039977.1.9",
          "form": "맞죠 요즘 그래여ㅠ",
          "original_form": "맞죠 요즘 그래여ㅠ",
          "speaker_id": "1",
          "time": "20210904 14:35"
        },
        {
          "id": "MDRW2100039977.1.10",
          "form": "지르고 후회",
          "original_form": "지르고 후회",
          "speaker_id": "1",
          "time": "20210904 14:35"
        },
        {
          "id": "MDRW2100039977.1.11",
          "form": "맞아여ㅠㅠ 그래도 사면 잘 입고는 다니니깐...",
          "original_form": "맞아여ㅠㅠ 그래도 사면 잘 입고는 다니니깐...",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100039977.1.12",
          "form": "중고거래도 하시나요?",
          "original_form": "중고거래도 하시나요?",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100039977.1.13",
          "form": "네내 당근마켓하는데",
          "original_form": "네내 당근마켓하는데",
          "speaker_id": "1",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100039977.1.14",
          "form": "멀쩡한 옷인데도 잘 안사가시네요ㅠㅠ",
          "original_form": "멀쩡한 옷인데도 잘 안사가시네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100039977.1.15",
          "form": "오 옷파시구나",
          "original_form": "오 옷파시구나",
          "speaker_id": "2",
          "time": "20210904 16:12"
        },
        {
          "id": "MDRW2100039977.1.16",
          "form": "저는 옷은 한번도 안팔아봤어요 ㅎ",
          "original_form": "저는 옷은 한번도 안팔아봤어요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 16:12"
        },
        {
          "id": "MDRW2100039977.1.17",
          "form": "팔릴때도 있나용?",
          "original_form": "팔릴때도 있나용?",
          "speaker_id": "2",
          "time": "20210904 16:12"
        },
        {
          "id": "MDRW2100039977.1.18",
          "form": "네네 팔링때도있어요",
          "original_form": "네네 팔링때도있어요",
          "speaker_id": "1",
          "time": "20210904 16:29"
        },
        {
          "id": "MDRW2100039977.1.19",
          "form": "가구도 팔고 다 팔아요 ㅋㅋ",
          "original_form": "가구도 팔고 다 팔아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:29"
        },
        {
          "id": "MDRW2100039977.1.20",
          "form": "버리려면 아까운게 많아서ㅠㅠ",
          "original_form": "버리려면 아까운게 많아서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 16:29"
        },
        {
          "id": "MDRW2100039977.1.21",
          "form": "와 팔리는군요 저도 나중에 팔아봐야겠어요!!!",
          "original_form": "와 팔리는군요 저도 나중에 팔아봐야겠어요!!!",
          "speaker_id": "2",
          "time": "20210904 19:05"
        },
        {
          "id": "MDRW2100039977.1.22",
          "form": "옷은 안팔아봤거든요",
          "original_form": "옷은 안팔아봤거든요",
          "speaker_id": "2",
          "time": "20210904 19:05"
        },
        {
          "id": "MDRW2100039977.1.23",
          "form": "옷은 근데 잘 안팔리긴 해요ㅠㅠ 엄청 후려쳐야해요",
          "original_form": "옷은 근데 잘 안팔리긴 해요ㅠㅠ 엄청 후려쳐야해요",
          "speaker_id": "1",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100039977.1.24",
          "form": "헉 그래요? ㅜㅜ 한...천원...? 해야하나요?",
          "original_form": "헉 그래요? ㅜㅜ 한...천원...? 해야하나요?",
          "speaker_id": "2",
          "time": "20210904 22:46"
        },
        {
          "id": "MDRW2100039977.1.25",
          "form": "오천원정도?ㅠㅠ",
          "original_form": "오천원정도?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100039977.1.26",
          "form": "야상점퍼도 만원 아니면 안들고가요ㅠ",
          "original_form": "야상점퍼도 만원 아니면 안들고가요ㅠ",
          "speaker_id": "1",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100039977.1.27",
          "form": "오 5천원 정도면 되군요",
          "original_form": "오 5천원 정도면 되군요",
          "speaker_id": "2",
          "time": "20210904 23:01"
        }
      ]
