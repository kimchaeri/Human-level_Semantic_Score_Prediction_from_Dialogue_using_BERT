      "utterance": [
        {
          "id": "MDRW2100030697.1.1",
          "form": "좋아하는 스포츠 있나요?",
          "original_form": "좋아하는 스포츠 있나요?",
          "speaker_id": "2",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100030697.1.2",
          "form": "배드민턴 정도요 ㅎㅎ 운동을 잘 못해서요",
          "original_form": "배드민턴 정도요 ㅎㅎ 운동을 잘 못해서요",
          "speaker_id": "1",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100030697.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100030697.1.4",
          "form": "전 정말 운동 신경이 둔해요.\r\n못하니까 더 안하게 되네요~",
          "original_form": "전 정말 운동 신경이 둔해요.\r\n못하니까 더 안하게 되네요~",
          "speaker_id": "2",
          "time": "20210905 09:25"
        },
        {
          "id": "MDRW2100030697.1.5",
          "form": "운동 잘 하면 즐길 수 있을텐데요 ㅠ",
          "original_form": "운동 잘 하면 즐길 수 있을텐데요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 09:28"
        },
        {
          "id": "MDRW2100030697.1.6",
          "form": "경기는 보시는 편인가요?",
          "original_form": "경기는 보시는 편인가요?",
          "speaker_id": "1",
          "time": "20210905 09:28"
        },
        {
          "id": "MDRW2100030697.1.7",
          "form": "야구는 야구장에 가서 몇번 봤어요~",
          "original_form": "야구는 야구장에 가서 몇번 봤어요~",
          "speaker_id": "2",
          "time": "20210905 09:29"
        },
        {
          "id": "MDRW2100030697.1.8",
          "form": "와 직관하셨군요",
          "original_form": "와 직관하셨군요",
          "speaker_id": "1",
          "time": "20210905 09:30"
        },
        {
          "id": "MDRW2100030697.1.9",
          "form": "응원하는게 재미있어요~\r\n또 먹는 재미도 있고요~",
          "original_form": "응원하는게 재미있어요~\r\n또 먹는 재미도 있고요~",
          "speaker_id": "2",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100030697.1.10",
          "form": "응원하고 있는 팀 있으세요?",
          "original_form": "응원하고 있는 팀 있으세요?",
          "speaker_id": "1",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100030697.1.11",
          "form": "야구장 다닐때는 응원하는 팀 있었는데 응원하는 팀이 너무 못해서 요즘은 야구에도 관심이 없어져가요~ㅋ",
          "original_form": "야구장 다닐때는 응원하는 팀 있었는데 응원하는 팀이 너무 못해서 요즘은 야구에도 관심이 없어져가요~ㅋ",
          "speaker_id": "2",
          "time": "20210905 09:44"
        },
        {
          "id": "MDRW2100030697.1.12",
          "form": "어느 팀이길래요?",
          "original_form": "어느 팀이길래요?",
          "speaker_id": "1",
          "time": "20210905 09:53"
        },
        {
          "id": "MDRW2100030697.1.13",
          "form": "ㅋㅋ롯데요~\r\n집에 유니폼이랑 응원막대도 있는데~\r\n아직 버리진 않고 있어요~ㅋ",
          "original_form": "ㅋㅋ롯데요~\r\n집에 유니폼이랑 응원막대도 있는데~\r\n아직 버리진 않고 있어요~ㅋ",
          "speaker_id": "2",
          "time": "20210905 09:55"
        },
        {
          "id": "MDRW2100030697.1.14",
          "form": "아 롯데군요 팬이시네요 유니폼이랑 응원막대까지... 직관도 하시고요",
          "original_form": "아 롯데군요 팬이시네요 유니폼이랑 응원막대까지... 직관도 하시고요",
          "speaker_id": "1",
          "time": "20210905 09:59"
        },
        {
          "id": "MDRW2100030697.1.15",
          "form": "다른 경기도 직관이 재미있겠지만 특히 야구는 야구장만의 분위기가 확실히 있는거 같아요",
          "original_form": "다른 경기도 직관이 재미있겠지만 특히 야구는 야구장만의 분위기가 확실히 있는거 같아요",
          "speaker_id": "2",
          "time": "20210905 10:03"
        },
        {
          "id": "MDRW2100030697.1.16",
          "form": "그렇군요 저도 야구 직관 한 번 해보고 싶어지네요",
          "original_form": "그렇군요 저도 야구 직관 한 번 해보고 싶어지네요",
          "speaker_id": "1",
          "time": "20210905 10:04"
        },
        {
          "id": "MDRW2100030697.1.17",
          "form": "한번 해보세요~스트레스 풀려요~",
          "original_form": "한번 해보세요~스트레스 풀려요~",
          "speaker_id": "2",
          "time": "20210905 10:09"
        },
        {
          "id": "MDRW2100030697.1.18",
          "form": "네 코로나 끝나면 한 번 해봐야겠네요",
          "original_form": "네 코로나 끝나면 한 번 해봐야겠네요",
          "speaker_id": "1",
          "time": "20210905 10:10"
        }
      ]
