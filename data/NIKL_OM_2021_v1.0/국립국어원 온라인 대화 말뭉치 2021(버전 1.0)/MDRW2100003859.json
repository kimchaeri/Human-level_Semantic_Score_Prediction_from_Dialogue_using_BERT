        },
        {
          "id": "MDRW2100003859.1.2",
          "form": "네 좋아하죠  여행 다녀오신데 있나요?",
          "original_form": "네 좋아하죠  여행 다녀오신데 있나요?",
          "speaker_id": "2",
          "time": "20210821 12:59"
        },
        {
          "id": "MDRW2100003859.1.3",
          "form": "코로나전에 많이 갔는데",
          "original_form": "코로나전에 많이 갔는데",
          "speaker_id": "1",
          "time": "20210821 13:00"
        },
        {
          "id": "MDRW2100003859.1.4",
          "form": "국내여행이요?",
          "original_form": "국내여행이요?",
          "speaker_id": "2",
          "time": "20210821 13:01"
        },
        {
          "id": "MDRW2100003859.1.5",
          "form": "해외도 갂죠",
