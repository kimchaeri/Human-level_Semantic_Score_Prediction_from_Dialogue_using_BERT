      "utterance": [
        {
          "id": "MDRW2100002976.1.1",
          "form": "오늘 출근 어땠어?",
          "original_form": "오늘 출근 어땠어?",
          "speaker_id": "1",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100002976.1.2",
          "form": "회사야 항상 똑같지.",
          "original_form": "회사야 항상 똑같지.",
          "speaker_id": "2",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100002976.1.3",
          "form": "과장님이 그만둔대.",
          "original_form": "과장님이 그만둔대.",
          "speaker_id": "2",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100002976.1.4",
          "form": "응? 그분 일한 지 한달도 안되지 않았어?",
          "original_form": "응? 그분 일한 지 한달도 안되지 않았어?",
          "speaker_id": "1",
          "time": "20210820 20:39"
        },
        {
          "id": "MDRW2100002976.1.5",
          "form": "내일이 딱 한 달째. 내일까지만 일하기로 했어.",
          "original_form": "내일이 딱 한 달째. 내일까지만 일하기로 했어.",
          "speaker_id": "2",
          "time": "20210820 20:39"
        },
        {
          "id": "MDRW2100002976.1.6",
          "form": "후임은 구했어?",
          "original_form": "후임은 구했어?",
          "speaker_id": "1",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100002976.1.7",
          "form": "완전 갑자기 그만두시네ㅋㅋㅋ",
          "original_form": "완전 갑자기 그만두시네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100002976.1.8",
          "form": "아니 아직.",
          "original_form": "아니 아직.",
          "speaker_id": "2",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100002976.1.9",
          "form": "그러니까. 더 힘들어지게 생겼어",
          "original_form": "그러니까. 더 힘들어지게 생겼어",
          "speaker_id": "2",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100002976.1.10",
          "form": "안 그래도 일 많은데.",
          "original_form": "안 그래도 일 많은데.",
          "speaker_id": "2",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100002976.1.11",
          "form": "빨리 사람 구해야겠다",
          "original_form": "빨리 사람 구해야겠다",
          "speaker_id": "1",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100002976.1.12",
          "form": "오늘 공고 올렸는데 아직 아무도 연락이 없네.",
          "original_form": "오늘 공고 올렸는데 아직 아무도 연락이 없네.",
          "speaker_id": "2",
          "time": "20210820 20:41"
        },
        {
          "id": "MDRW2100002976.1.13",
          "form": "다음주부터 일할 사람 없어서 큰 일이야.",
          "original_form": "다음주부터 일할 사람 없어서 큰 일이야.",
          "speaker_id": "2",
          "time": "20210820 20:42"
        },
        {
          "id": "MDRW2100002976.1.14",
          "form": "너무 급하게 구하면 또 이상한 사람 뽑을 수 있어~",
          "original_form": "너무 급하게 구하면 또 이상한 사람 뽑을 수 있어~",
          "speaker_id": "1",
          "time": "20210820 20:42"
        },
        {
          "id": "MDRW2100002976.1.15",
          "form": "잘 뽑아봐야지.",
          "original_form": "잘 뽑아봐야지.",
          "speaker_id": "2",
          "time": "20210820 20:43"
        },
        {
          "id": "MDRW2100002976.1.16",
          "form": "너는 오늘 편의점 근무했어?",
          "original_form": "너는 오늘 편의점 근무했어?",
          "speaker_id": "2",
          "time": "20210820 20:43"
        },
        {
          "id": "MDRW2100002976.1.17",
          "form": "응 9시부터 4시까지~",
          "original_form": "응 9시부터 4시까지~",
          "speaker_id": "1",
          "time": "20210820 20:44"
        },
        {
          "id": "MDRW2100002976.1.18",
          "form": "오늘 손님 많아서 힘들었어",
          "original_form": "오늘 손님 많아서 힘들었어",
          "speaker_id": "1",
          "time": "20210820 20:44"
        },
        {
          "id": "MDRW2100002976.1.19",
          "form": "고생했네.",
          "original_form": "고생했네.",
          "speaker_id": "2",
          "time": "20210820 20:44"
        },
        {
          "id": "MDRW2100002976.1.20",
          "form": "시급은 얼마 준대?",
          "original_form": "시급은 얼마 준대?",
          "speaker_id": "2",
          "time": "20210820 20:45"
        },
        {
          "id": "MDRW2100002976.1.21",
          "form": "9천원! 그래서 열일하고 왔지",
          "original_form": "9천원! 그래서 열일하고 왔지",
          "speaker_id": "1",
          "time": "20210820 20:46"
        },
        {
          "id": "MDRW2100002976.1.22",
          "form": "잘했네. 일 많이 힘들었어?",
          "original_form": "잘했네. 일 많이 힘들었어?",
          "speaker_id": "2",
          "time": "20210820 20:46"
        },
        {
          "id": "MDRW2100002976.1.23",
          "form": "오늘 어떤 손님이 컵라면 쏟아가지고ㅠㅠ 그거 청소하느라 정신 없었어",
          "original_form": "오늘 어떤 손님이 컵라면 쏟아가지고ㅠㅠ 그거 청소하느라 정신 없었어",
          "speaker_id": "1",
          "time": "20210820 20:47"
        },
        {
          "id": "MDRW2100002976.1.24",
          "form": "아이고 힘들었겠다",
          "original_form": "아이고 힘들었겠다",
          "speaker_id": "2",
          "time": "20210820 20:47"
        },
        {
          "id": "MDRW2100002976.1.25",
          "form": "ㅋㅋㅋ나보다 엄마가 요즘 더 바쁘잖아",
          "original_form": "ㅋㅋㅋ나보다 엄마가 요즘 더 바쁘잖아",
          "speaker_id": "1",
          "time": "20210820 20:47"
        },
        {
          "id": "MDRW2100002976.1.26",
          "form": "이제 곧 결재일이라 더 바빠지겠네",
          "original_form": "이제 곧 결재일이라 더 바빠지겠네",
          "speaker_id": "1",
          "time": "20210820 20:48"
        },
        {
          "id": "MDRW2100002976.1.27",
          "form": "매달 똑같은데 뭘.",
          "original_form": "매달 똑같은데 뭘.",
          "speaker_id": "2",
          "time": "20210820 20:48"
        },
        {
          "id": "MDRW2100002976.1.28",
          "form": "소장님이랑 둘이만 일하니까 일이 많아지긴 했지.",
          "original_form": "소장님이랑 둘이만 일하니까 일이 많아지긴 했지.",
          "speaker_id": "2",
          "time": "20210820 20:48"
        },
        {
          "id": "MDRW2100002976.1.29",
          "form": "거기서 언제까지 일할지 정했어?",
          "original_form": "거기서 언제까지 일할지 정했어?",
          "speaker_id": "1",
          "time": "20210820 20:49"
        },
        {
          "id": "MDRW2100002976.1.30",
          "form": "일단 올해까지는 다녀보려고.",
          "original_form": "일단 올해까지는 다녀보려고.",
          "speaker_id": "2",
          "time": "20210820 20:50"
        },
        {
          "id": "MDRW2100002976.1.31",
          "form": "내년에 그만두면 어디 갈 건데ㅡ",
          "original_form": "내년에 그만두면 어디 갈 건데ㅡ",
          "speaker_id": "1",
          "time": "20210820 20:50"
        },
        {
          "id": "MDRW2100002976.1.32",
          "form": "내년에 그만두면 어디 갈 건데?",
          "original_form": "내년에 그만두면 어디 갈 건데?",
          "speaker_id": "1",
          "time": "20210820 20:50"
        },
        {
          "id": "MDRW2100002976.1.33",
          "form": "어차피 실업급여 나오니까 한달은 쉬면서 지내다가 일자리 소개 받아야지.",
          "original_form": "어차피 실업급여 나오니까 한달은 쉬면서 지내다가 일자리 소개 받아야지.",
          "speaker_id": "2",
          "time": "20210820 20:51"
        },
        {
          "id": "MDRW2100002976.1.34",
          "form": "여기 감사한테 부탁하면 여기저기 알아봐 줄 수 있어.",
          "original_form": "여기 감사한테 부탁하면 여기저기 알아봐 줄 수 있어.",
          "speaker_id": "2",
          "time": "20210820 20:52"
        },
        {
          "id": "MDRW2100002976.1.35",
          "form": "그래 엄마 오래 일했으니까 한두 달은 쉬어도 돼~~",
          "original_form": "그래 엄마 오래 일했으니까 한두 달은 쉬어도 돼~~",
          "speaker_id": "1",
          "time": "20210820 20:53"
        },
        {
          "id": "MDRW2100002976.1.36",
          "form": "알겠어. 근데 내일 잠깐 회사 나가봐야 돼.",
          "original_form": "알겠어. 근데 내일 잠깐 회사 나가봐야 돼.",
          "speaker_id": "2",
          "time": "20210820 20:54"
        },
        {
          "id": "MDRW2100002976.1.37",
          "form": "토요일인데?? 주말에도 출근해야 돼?",
          "original_form": "토요일인데?? 주말에도 출근해야 돼?",
          "speaker_id": "1",
          "time": "20210820 20:55"
        },
        {
          "id": "MDRW2100002976.1.38",
          "form": "오전에만 잠깐. 일 처리해줄게 있어서 그래.",
          "original_form": "오전에만 잠깐. 일 처리해줄게 있어서 그래.",
          "speaker_id": "2",
          "time": "20210820 20:56"
        }
      ]
