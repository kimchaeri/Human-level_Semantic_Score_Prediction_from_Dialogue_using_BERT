      "utterance": [
        {
          "id": "MDRW2100043878.1.1",
          "form": "아니 현명하셨어요!",
          "original_form": "아니 현명하셨어요!",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043878.1.2",
          "form": "전 몸무게좀 줄이고 검진해야지~했는데",
          "original_form": "전 몸무게좀 줄이고 검진해야지~했는데",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043878.1.3",
          "form": "배달음식 덕에 나날이 신기록이네요 ㅋㅋ",
          "original_form": "배달음식 덕에 나날이 신기록이네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043878.1.4",
          "form": "요즘은 배달어플도 쿠폰 자주줘서 안먹음 손해자나요ㅋ",
          "original_form": "요즘은 배달어플도 쿠폰 자주줘서 안먹음 손해자나요ㅋ",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043878.1.5",
          "form": "안쓰면 0원인데 이상하게 아까워요 ㅋㅋ",
          "original_form": "안쓰면 0원인데 이상하게 아까워요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043878.1.6",
          "form": "배달도 엄청 다양해졌더라구요",
          "original_form": "배달도 엄청 다양해졌더라구요",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043878.1.7",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043878.1.8",
          "form": "진짜 요즘은 뭐든 다 배달되여",
          "original_form": "진짜 요즘은 뭐든 다 배달되여",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043878.1.9",
          "form": "잔치국수랑 메밀국수 시켜먹고 좋았어요",
          "original_form": "잔치국수랑 메밀국수 시켜먹고 좋았어요",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043878.1.10",
          "form": "진짜 국수도 안불고 빨리오더라고요",
          "original_form": "진짜 국수도 안불고 빨리오더라고요",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043878.1.11",
          "form": "엄청 신기해요 ㅋㅋ",
          "original_form": "엄청 신기해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043878.1.12",
          "form": "배민1이란거 생겨서 그것도 한집배달 하더라고요",
          "original_form": "배민1이란거 생겨서 그것도 한집배달 하더라고요",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043878.1.13",
          "form": "와 그건 처음들어봐요",
          "original_form": "와 그건 처음들어봐요",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043878.1.14",
          "form": "아예 새 어플이 생긴건가요?",
          "original_form": "아예 새 어플이 생긴건가요?",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043878.1.15",
          "form": "아니요 배민어플 안에 잇어요",
          "original_form": "아니요 배민어플 안에 잇어요",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043878.1.16",
          "form": "근데 배달비가 비싸요",
          "original_form": "근데 배달비가 비싸요",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043878.1.17",
          "form": "번쩍배달이라는 것만 봤어요",
          "original_form": "번쩍배달이라는 것만 봤어요",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043878.1.18",
          "form": "이따 자세히 봐야지..",
          "original_form": "이따 자세히 봐야지..",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043878.1.19",
          "form": "먼가 자꾸 새로운게 나오더라고요ㅋ",
          "original_form": "먼가 자꾸 새로운게 나오더라고요ㅋ",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043878.1.20",
          "form": "그러게요 ㅋㅋ 새로운 서비스하면 또 추가요금 붙고!",
          "original_form": "그러게요 ㅋㅋ 새로운 서비스하면 또 추가요금 붙고!",
          "speaker_id": "2",
          "time": "20210907 21:02"
        }
      ]
