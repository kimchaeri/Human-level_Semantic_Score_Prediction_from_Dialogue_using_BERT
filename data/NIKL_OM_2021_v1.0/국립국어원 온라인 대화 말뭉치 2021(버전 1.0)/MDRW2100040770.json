      "utterance": [
        {
          "id": "MDRW2100040770.1.1",
          "form": "뭔가 필라테스강사님께",
          "original_form": "뭔가 필라테스강사님께",
          "speaker_id": "2",
          "time": "20210905 04:16"
        },
        {
          "id": "MDRW2100040770.1.2",
          "form": "1차몇일전에맞았다고",
          "original_form": "1차몇일전에맞았다고",
          "speaker_id": "2",
          "time": "20210905 04:16"
        },
        {
          "id": "MDRW2100040770.1.3",
          "form": "말씀드렸는데도",
          "original_form": "말씀드렸는데도",
          "speaker_id": "2",
          "time": "20210905 04:16"
        },
        {
          "id": "MDRW2100040770.1.4",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210905 04:16"
        },
        {
          "id": "MDRW2100040770.1.5",
          "form": "수업을해주신건가요",
          "original_form": "수업을해주신건가요",
          "speaker_id": "2",
          "time": "20210905 04:16"
        },
        {
          "id": "MDRW2100040770.1.6",
          "form": "말은 안하고 그냥 갔어요 (?)",
          "original_form": "말은 안하고 그냥 갔어요 (?)",
          "speaker_id": "1",
          "time": "20210905 04:16"
        },
        {
          "id": "MDRW2100040770.1.7",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210905 04:16"
        },
        {
          "id": "MDRW2100040770.1.8",
          "form": "어차피 8대 1 수업이라서",
          "original_form": "어차피 8대 1 수업이라서",
          "speaker_id": "1",
          "time": "20210905 04:16"
        },
        {
          "id": "MDRW2100040770.1.9",
          "form": "너무했네요",
          "original_form": "너무했네요",
          "speaker_id": "2",
          "time": "20210905 04:16"
        },
        {
          "id": "MDRW2100040770.1.10",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210905 04:16"
        },
        {
          "id": "MDRW2100040770.1.11",
          "form": "그리고 자기가 되는 날짜에",
          "original_form": "그리고 자기가 되는 날짜에",
          "speaker_id": "1",
          "time": "20210905 04:16"
        },
        {
          "id": "MDRW2100040770.1.12",
          "form": "예약하고 가는거라",
          "original_form": "예약하고 가는거라",
          "speaker_id": "1",
          "time": "20210905 04:16"
        },
        {
          "id": "MDRW2100040770.1.13",
          "form": "오호...",
          "original_form": "오호...",
          "speaker_id": "2",
          "time": "20210905 04:16"
        },
        {
          "id": "MDRW2100040770.1.14",
          "form": "제 판단으로 수업을 가는거라,,,",
          "original_form": "제 판단으로 수업을 가는거라,,,",
          "speaker_id": "1",
          "time": "20210905 04:16"
        },
        {
          "id": "MDRW2100040770.1.15",
          "form": "아앟",
          "original_form": "아앟",
          "speaker_id": "2",
          "time": "20210905 04:16"
        },
        {
          "id": "MDRW2100040770.1.16",
          "form": "강사님의 책임은 없는?!",
          "original_form": "강사님의 책임은 없는?!",
          "speaker_id": "1",
          "time": "20210905 04:16"
        },
        {
          "id": "MDRW2100040770.1.17",
