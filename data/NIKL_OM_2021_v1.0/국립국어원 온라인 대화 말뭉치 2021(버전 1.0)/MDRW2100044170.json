      "utterance": [
        {
          "id": "MDRW2100044170.1.1",
          "form": "님 저 체데 틀엇음",
          "original_form": "님 저 체데 틀엇음",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044170.1.2",
          "form": "넷플이요",
          "original_form": "넷플이요",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044170.1.3",
          "form": "아 최고 리얼리티죠",
          "original_form": "아 최고 리얼리티죠",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044170.1.4",
          "form": "다보셧나요",
          "original_form": "다보셧나요",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044170.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044170.1.6",
          "form": "감상주새요",
          "original_form": "감상주새요",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044170.1.7",
          "form": "지금 최종선택중이거든요",
          "original_form": "지금 최종선택중이거든요",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044170.1.8",
          "form": "결혼시점이래요 그런가요",
          "original_form": "결혼시점이래요 그런가요",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044170.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ아근데진짜",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아근데진짜",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044170.1.10",
          "form": "그건아닌것같아요 근데 또 일리가있긴한디",
          "original_form": "그건아닌것같아요 근데 또 일리가있긴한디",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044170.1.11",
          "form": "아까워요 결혼하기",
          "original_form": "아까워요 결혼하기",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044170.1.12",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044170.1.13",
          "form": "진록은요?",
          "original_form": "진록은요?",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044170.1.14",
          "form": "후 결혼하기 절대싫은사람",
          "original_form": "후 결혼하기 절대싫은사람",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044170.1.15",
          "form": "근데 저런애가 또 지와이프한테는 잘할것같기도하고...",
          "original_form": "근데 저런애가 또 지와이프한테는 잘할것같기도하고...",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044170.1.16",
          "form": "그럴수도잇죠",
          "original_form": "그럴수도잇죠",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044170.1.17",
          "form": "민선이 기다려본대요",
          "original_form": "민선이 기다려본대요",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044170.1.18",
          "form": "와우",
          "original_form": "와우",
