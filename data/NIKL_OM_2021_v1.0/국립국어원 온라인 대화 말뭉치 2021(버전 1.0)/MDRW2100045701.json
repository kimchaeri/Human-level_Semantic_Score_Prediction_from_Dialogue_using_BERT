      "utterance": [
        {
          "id": "MDRW2100045701.1.1",
          "form": "점심메뉴 감자인가요!?",
          "original_form": "점심메뉴 감자인가요!?",
          "speaker_id": "1",
          "time": "20210908 05:41"
        },
        {
          "id": "MDRW2100045701.1.2",
          "form": "감자인가요!",
          "original_form": "감자인가요!",
          "speaker_id": "1",
          "time": "20210908 05:41"
        },
        {
          "id": "MDRW2100045701.1.3",
          "form": "딩동댕~!!!!!!!! 감자입니다",
          "original_form": "딩동댕~!!!!!!!! 감자입니다",
          "speaker_id": "2",
          "time": "20210908 05:42"
        },
        {
          "id": "MDRW2100045701.1.4",
          "form": "짝짝짝",
          "original_form": "짝짝짝",
          "speaker_id": "2",
          "time": "20210908 05:42"
        },
        {
          "id": "MDRW2100045701.1.5",
          "form": "오예!",
          "original_form": "오예!",
          "speaker_id": "1",
          "time": "20210908 05:42"
        },
        {
          "id": "MDRW2100045701.1.6",
          "form": "조리법도 맞출까요?",
          "original_form": "조리법도 맞출까요?",
          "speaker_id": "1",
          "time": "20210908 05:42"
        },
        {
          "id": "MDRW2100045701.1.7",
          "form": "단일재료라서 어려우셨을텐데 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "단일재료라서 어려우셨을텐데 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:42"
        },
        {
          "id": "MDRW2100045701.1.8",
          "form": "좋아요!",
          "original_form": "좋아요!",
          "speaker_id": "2",
          "time": "20210908 05:42"
        },
        {
          "id": "MDRW2100045701.1.9",
          "form": "아 단일재료였구나",
          "original_form": "아 단일재료였구나",
          "speaker_id": "1",
          "time": "20210908 05:42"
        },
        {
          "id": "MDRW2100045701.1.10",
          "form": "에어프라이어로 구우셨나요?",
          "original_form": "에어프라이어로 구우셨나요?",
          "speaker_id": "1",
          "time": "20210908 05:42"
        },
        {
          "id": "MDRW2100045701.1.11",
          "form": "아니요! 집에 에어프라이어가 없어유 흑흑",
          "original_form": "아니요! 집에 에어프라이어가 없어유 흑흑",
          "speaker_id": "2",
          "time": "20210908 05:44"
        },
        {
          "id": "MDRW2100045701.1.12",
          "form": "앗 저도 없어욬ㅋㅋㅋ사야하는데...",
          "original_form": "앗 저도 없어욬ㅋㅋㅋ사야하는데...",
          "speaker_id": "1",
          "time": "20210908 05:44"
        },
        {
          "id": "MDRW2100045701.1.13",
          "form": "찌셨나요!",
          "original_form": "찌셨나요!",
          "speaker_id": "1",
          "time": "20210908 05:45"
        },
        {
          "id": "MDRW2100045701.1.14",
          "form": "네 맞아요! 찐감자",
          "original_form": "네 맞아요! 찐감자",
          "speaker_id": "2",
          "time": "20210908 05:45"
        },
        {
          "id": "MDRW2100045701.1.15",
          "form": "포슬포슬 쪄서 먹었답니다 후후",
          "original_form": "포슬포슬 쪄서 먹었답니다 후후",
          "speaker_id": "2",
          "time": "20210908 05:45"
        },
        {
          "id": "MDRW2100045701.1.16",
          "form": "찐 감자 최고죠!!!",
          "original_form": "찐 감자 최고죠!!!",
          "speaker_id": "1",
          "time": "20210908 05:46"
        },
        {
          "id": "MDRW2100045701.1.17",
          "form": "소금찍어드셨나요ㅎㅎㅎ!",
          "original_form": "소금찍어드셨나요ㅎㅎㅎ!",
          "speaker_id": "1",
          "time": "20210908 05:46"
        },
        {
          "id": "MDRW2100045701.1.18",
          "form": "아니요 찔 때 소금간 설탕간을 해서 쨨더니",
          "original_form": "아니요 찔 때 소금간 설탕간을 해서 쨨더니",
          "speaker_id": "2",
          "time": "20210908 05:46"
        },
        {
          "id": "MDRW2100045701.1.19",
          "form": "겉이 단짠단짠이라 따로 양념 없이 먹었답니다ㅎㅎㅎㅎㅎㅎ",
          "original_form": "겉이 단짠단짠이라 따로 양념 없이 먹었답니다ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 05:47"
        },
        {
          "id": "MDRW2100045701.1.20",
          "form": "와우 단짠단짠",
          "original_form": "와우 단짠단짠",
          "speaker_id": "1",
          "time": "20210908 05:47"
        },
        {
          "id": "MDRW2100045701.1.21",
          "form": "찌기 전부터 간을 하는군요 처음 안 방식인데",
          "original_form": "찌기 전부터 간을 하는군요 처음 안 방식인데",
          "speaker_id": "1",
          "time": "20210908 05:47"
        },
        {
          "id": "MDRW2100045701.1.22",
          "form": "설탕소금물로 찌면",
          "original_form": "설탕소금물로 찌면",
          "speaker_id": "2",
          "time": "20210908 05:47"
        },
        {
          "id": "MDRW2100045701.1.23",
          "form": "와 천재적이에요",
          "original_form": "와 천재적이에요",
          "speaker_id": "1",
          "time": "20210908 05:47"
        },
        {
          "id": "MDRW2100045701.1.24",
          "form": "그그 쪄내고 나서 바닥에 눌러붙은 감자부스러기",
          "original_form": "그그 쪄내고 나서 바닥에 눌러붙은 감자부스러기",
          "speaker_id": "2",
          "time": "20210908 05:47"
        },
        {
          "id": "MDRW2100045701.1.25",
          "form": "진짜 맛있으니 해 보세요 ㅋㅋㅋ",
          "original_form": "진짜 맛있으니 해 보세요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:47"
        },
        {
          "id": "MDRW2100045701.1.26",
          "form": "와 부스러기까지ㅠㅠㅠㅠㅠ",
          "original_form": "와 부스러기까지ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:48"
        },
        {
          "id": "MDRW2100045701.1.27",
          "form": "진짜 한 먹음하시네요",
          "original_form": "진짜 한 먹음하시네요",
          "speaker_id": "1",
          "time": "20210908 05:48"
        },
        {
          "id": "MDRW2100045701.1.28",
          "form": "이거하면서 맛집정보 음식 맛있게 먹는 정보는 다 알아가네요 진짜",
          "original_form": "이거하면서 맛집정보 음식 맛있게 먹는 정보는 다 알아가네요 진짜",
          "speaker_id": "1",
          "time": "20210908 05:49"
        }
      ]
    }
