      "utterance": [
        {
          "id": "MDRW2100017674.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017674.1.2",
          "form": "돈까스요!",
          "original_form": "돈까스요!",
          "speaker_id": "1",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017674.1.3",
          "form": "오 돈까스 !",
          "original_form": "오 돈까스 !",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017674.1.4",
          "form": "예전에 공대 친구가 저녁먹으러 가자면서",
          "original_form": "예전에 공대 친구가 저녁먹으러 가자면서",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017674.1.5",
          "form": "돈가스먹을래 밥먹을래 물어봐서",
          "original_form": "돈가스먹을래 밥먹을래 물어봐서",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017674.1.6",
          "form": "진짜 공대생들의 소울푸드인가 했었던 적이 있어요",
          "original_form": "진짜 공대생들의 소울푸드인가 했었던 적이 있어요",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017674.1.7",
          "form": "돈까스 사랑입니다ㅠ",
          "original_form": "돈까스 사랑입니다ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017674.1.8",
          "form": "저두 며칠전에 돈까스 먹었어요",
          "original_form": "저두 며칠전에 돈까스 먹었어요",
          "speaker_id": "2",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100017674.1.9",
          "form": "근데 정말 맛없는 돈까스 먹고 제가 돈까스 한 2년 끊은 적 있어요",
          "original_form": "근데 정말 맛없는 돈까스 먹고 제가 돈까스 한 2년 끊은 적 있어요",
          "speaker_id": "2",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100017674.1.10",
          "form": "크레이지카츠 추천합니다",
          "original_form": "크레이지카츠 추천합니다",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100017674.1.11",
          "form": "크레이지카츠 메모해둘게요ㅋㅋ",
          "original_form": "크레이지카츠 메모해둘게요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100017674.1.12",
          "form": "다른음식 또 뭐좋아하세요",
          "original_form": "다른음식 또 뭐좋아하세요",
          "speaker_id": "1",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100017674.1.13",
          "form": "저는 초밥을 젤 좋아해요",
          "original_form": "저는 초밥을 젤 좋아해요",
          "speaker_id": "2",
          "time": "20210827 08:18"
        },
        {
          "id": "MDRW2100017674.1.14",
          "form": "오 저도 초밥",
          "original_form": "오 저도 초밥{emoji:♡}",
          "speaker_id": "1",
          "time": "20210828 15:17"
        },
        {
          "id": "MDRW2100017674.1.15",
          "form": "맛있는 집 찾기가 힘들더라구요",
          "original_form": "맛있는 집 찾기가 힘들더라구요",
