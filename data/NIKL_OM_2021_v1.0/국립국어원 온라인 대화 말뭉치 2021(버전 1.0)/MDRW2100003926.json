        {
          "id": "MDRW2100003926.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210821 13:57"
        },
        {
          "id": "MDRW2100003926.1.3",
          "form": "요즘은 잘 못보고 있어요",
          "original_form": "요즘은 잘 못보고 있어요",
          "speaker_id": "2",
          "time": "20210821 13:57"
        },
        {
          "id": "MDRW2100003926.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "2",
          "time": "20210821 13:57"
        },
        {
          "id": "MDRW2100003926.1.5",
          "form": "저도 밖에 잘 안나가서... ",
          "original_form": "저도 밖에 잘 안나가서... ",
          "speaker_id": "1",
          "time": "20210821 14:01"
        },
        {
          "id": "MDRW2100003926.1.6",
          "form": "근데 저번달에 블랙위도우 봤어요!",
          "original_form": "근데 저번달에 블랙위도우 봤어요!",
          "speaker_id": "1",
          "time": "20210821 14:01"
        },
        {
          "id": "MDRW2100003926.1.7",
          "form": "오 블랙 위도우 핫하던데",
          "original_form": "오 블랙 위도우 핫하던데",
          "speaker_id": "2",
          "time": "20210821 14:01"
        },
        {
          "id": "MDRW2100003926.1.8",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "2",
          "time": "20210821 14:01"
        },
        {
          "id": "MDRW2100003926.1.9",
          "form": "네 저는 마블영화 몇년만에 봤는데 재밌더라구요 ㅋㅋㅋ",
          "original_form": "네 저는 마블영화 몇년만에 봤는데 재밌더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:01"
        },
        {
          "id": "MDRW2100003926.1.10",
          "form": "스칼렛 요한슨이 액션을 아주 그냥 ㅋㅋ 시원했어요",
          "original_form": "스칼렛 요한슨이 액션을 아주 그냥 ㅋㅋ 시원했어요",
          "speaker_id": "1",
          "time": "20210821 14:02"
        },
        {
          "id": "MDRW2100003926.1.11",
          "form": "오 마블 영화 최애가 뭔가요?",
          "original_form": "오 마블 영화 최애가 뭔가요?",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003926.1.12",
          "form": "기억나는게 블랙팬서밖에 없긴 한데 ㅋㅋㅋ",
          "original_form": "기억나는게 블랙팬서밖에 없긴 한데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003926.1.13",
          "form": "이번에 본 블랙위도우!! 좋은거같네요 ㅎㅎ",
          "original_form": "이번에 본 블랙위도우!! 좋은거같네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003926.1.14",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003926.1.15",
          "form": "저도 최애는 없능데 본게",
          "original_form": "저도 최애는 없능데 본게",
          "speaker_id": "2",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003926.1.16",
          "form": "엔트맨 뿐이에요",
          "original_form": "엔트맨 뿐이에요",
          "speaker_id": "2",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003926.1.17",
          "form": "그 작아지는....",
          "original_form": "그 작아지는....",
          "speaker_id": "2",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003926.1.18",
          "form": "ㅋㅋㅋㅋㅋ 그건 안봤는데 ㅋㅋ 엄청 웃긴 캐릭터 아닌가요?",
          "original_form": "ㅋㅋㅋㅋㅋ 그건 안봤는데 ㅋㅋ 엄청 웃긴 캐릭터 아닌가요?",
          "speaker_id": "1",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003926.1.19",
          "form": "마블 너무 잘나가서",
          "original_form": "마블 너무 잘나가서",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003926.1.20",
          "form": "저처럼 잘 안보면 대화에 못끼게 되는거같아요 ㅜ ㅋ",
          "original_form": "저처럼 잘 안보면 대화에 못끼게 되는거같아요 ㅜ ㅋ",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003926.1.21",
          "form": "ㅋㅋㅋㅋㅋ 맞아요 말할 때 이해가 잘안돠는....",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요 말할 때 이해가 잘안돠는....",
          "speaker_id": "2",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003926.1.22",
          "form": "그래도 잘 안보게 돼요 ㅎ",
          "original_form": "그래도 잘 안보게 돼요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003926.1.23",
          "form": "ㅋㅋㅋㅋ 취향이 아닌건 어쩔수 없는듯해요",
          "original_form": "ㅋㅋㅋㅋ 취향이 아닌건 어쩔수 없는듯해요",
          "speaker_id": "1",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003926.1.24",
