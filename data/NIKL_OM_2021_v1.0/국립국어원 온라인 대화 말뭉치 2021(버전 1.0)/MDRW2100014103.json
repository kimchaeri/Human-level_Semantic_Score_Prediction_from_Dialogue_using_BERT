        {
          "id": "MDRW2100014103.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014103.1.4",
          "form": "누나있어요",
          "original_form": "누나있어요",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014103.1.5",
          "form": "헐 부러워",
          "original_form": "헐 부러워",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014103.1.6",
          "form": "오빠분과 사이 어떠세요",
          "original_form": "오빠분과 사이 어떠세요",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014103.1.7",
          "form": "젤부러워",
          "original_form": "젤부러워",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014103.1.8",
          "form": "저 오빠...그 결혼해서",
          "original_form": "저 오빠...그 결혼해서",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014103.1.9",
          "form": "부러워요?",
          "original_form": "부러워요?",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014103.1.10",
          "form": "먼사이죠",
          "original_form": "먼사이죠",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014103.1.11",
          "form": "누나가요?",
          "original_form": "누나가요?",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014103.1.12",
          "form": "머언 사이",
          "original_form": "머언 사이",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014103.1.13",
          "form": "그럴리가",
          "original_form": "그럴리가",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014103.1.14",
          "form": "죄삼다 죄삼다",
          "original_form": "죄삼다 죄삼다",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014103.1.15",
          "form": "누나가 많이 거친 편이신가여(?(",
          "original_form": "누나가 많이 거친 편이신가여(?(",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014103.1.16",
          "form": "아니요 죄송 할 것 까지는..",
          "original_form": "아니요 죄송 할 것 까지는..",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014103.1.17",
          "form": "좀 맞았나요",
          "original_form": "좀 맞았나요",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014103.1.18",
          "form": "많이 맞고 자랐습니다",
          "original_form": "많이 맞고 자랐습니다",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014103.1.19",
          "form": "쓰읍..ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "쓰읍..ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014103.1.20",
          "form": "동생들 한 번쯤 겪어봤을",
          "original_form": "동생들 한 번쯤 겪어봤을",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014103.1.21",
          "form": "야야야야야야야야",
          "original_form": "야야야야야야야야",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014103.1.22",
          "form": "불꺼",
          "original_form": "불꺼",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014103.1.23",
          "form": "씁.",
          "original_form": "씁.",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014103.1.24",
          "form": "저도 오빠한테 많이 맞고..아니 레슬링 체험판으로 맞고 살았슴다",
          "original_form": "저도 오빠한테 많이 맞고..아니 레슬링 체험판으로 맞고 살았슴다",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014103.1.25",
          "form": "쓰읍...",
          "original_form": "쓰읍...",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014103.1.26",
          "form": "동생들한테 다 글케 시키나봐요",
          "original_form": "동생들한테 다 글케 시키나봐요",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014103.1.27",
          "form": "동생들의 비애란..",
          "original_form": "동생들의 비애란..",
