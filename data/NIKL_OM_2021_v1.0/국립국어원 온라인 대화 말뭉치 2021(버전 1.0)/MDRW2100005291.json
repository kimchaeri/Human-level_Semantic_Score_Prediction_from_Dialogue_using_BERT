      "utterance": [
        {
          "id": "MDRW2100005291.1.1",
          "form": "그쳐 저도 제주위에 남미간사람이없어서 얘기할곳없었는데",
          "original_form": "그쳐 저도 제주위에 남미간사람이없어서 얘기할곳없었는데",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005291.1.2",
          "form": "넘재밌네요 ㅋㅋㅋㅋ",
          "original_form": "넘재밌네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005291.1.3",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005291.1.4",
          "form": "다른 분들이랑 대화하면",
          "original_form": "다른 분들이랑 대화하면",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005291.1.5",
          "form": "주제 계속 바꾸는데",
          "original_form": "주제 계속 바꾸는데",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005291.1.6",
          "form": "저희 여행만 계속 해도 할 얘기 생기는 거 같아요 ㅋㅋㅋ",
          "original_form": "저희 여행만 계속 해도 할 얘기 생기는 거 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005291.1.7",
          "form": "ㅋㅋㅋㅋㅋ아무래도 공통점이있으니까여",
          "original_form": "ㅋㅋㅋㅋㅋ아무래도 공통점이있으니까여",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005291.1.8",
          "form": "페루에 그럼 마추픽추도 가셨겠네요",
          "original_form": "페루에 그럼 마추픽추도 가셨겠네요",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005291.1.9",
          "form": "네네! 마추픽추 갔었어요",
          "original_form": "네네! 마추픽추 갔었어요",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005291.1.10",
          "form": "거기 마추픽추마을까지가는길 어떻게가셨어요? 왕복기차타셨나여",
          "original_form": "거기 마추픽추마을까지가는길 어떻게가셨어요? 왕복기차타셨나여",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005291.1.11",
          "form": "저는 쌩고생을해서가서 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 쌩고생을해서가서 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005291.1.12",
          "form": "아직도 생각나여",
          "original_form": "아직도 생각나여",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005291.1.13",
          "form": "왕복버스? 탔던 것 같아요",
          "original_form": "왕복버스? 탔던 것 같아요",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005291.1.14",
          "form": "거기 엄청 높잖아요ㅠㅠㅠ",
          "original_form": "거기 엄청 높잖아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005291.1.15",
          "form": "그니까여 아 버스가는길 저는 봉고차타고갔는데",
          "original_form": "그니까여 아 버스가는길 저는 봉고차타고갔는데",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005291.1.16",
          "form": "기사가 계속 졸아서ㅜㅜㅜ",
          "original_form": "기사가 계속 졸아서ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005291.1.17",
          "form": "핸드폰을하질않나",
          "original_form": "핸드폰을하질않나",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005291.1.18",
          "form": "뒤에 벌벌떨면서갔어요 거기고산지대라 구름끼거 비도자주와서 앞도 흐린데ㅜㅜ",
          "original_form": "뒤에 벌벌떨면서갔어요 거기고산지대라 구름끼거 비도자주와서 앞도 흐린데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005291.1.19",
          "form": "그래서 돌아오는길은 기차타고내려왔어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 돌아오는길은 기차타고내려왔어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005291.1.20",
          "form": "ㅠㅠㅠ와 무섭네요",
          "original_form": "ㅠㅠㅠ와 무섭네요",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005291.1.21",
          "form": "그 버스 막",
          "original_form": "그 버스 막",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005291.1.22",
          "form": "급커브 반복이라 저 엄청 무서웠었어요",
          "original_form": "급커브 반복이라 저 엄청 무서웠었어요",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005291.1.23",
          "form": "진짜 삐끗하면 버스 떨어질 것 같은디",
          "original_form": "진짜 삐끗하면 버스 떨어질 것 같은디",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005291.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005291.1.25",
          "form": "맞아요 버스는 크니까 창문바로옆보면",
          "original_form": "맞아요 버스는 크니까 창문바로옆보면",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005291.1.26",
          "form": "절벽",
          "original_form": "절벽",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005291.1.27",
          "form": "실제로 떨어지는 사고있다고듣고가서 더 벌벌떨었어요",
          "original_form": "실제로 떨어지는 사고있다고듣고가서 더 벌벌떨었어요",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005291.1.28",
          "form": "ㅋㅋㅋㅋㅋ맞아요ㅜㅜ 아 넘나 무서웠던",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요ㅜㅜ 아 넘나 무서웠던",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005291.1.29",
          "form": "근데 거기 올라가자마자",
          "original_form": "근데 거기 올라가자마자",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005291.1.30",
          "form": "여권에 찍을 수 있는 도장?있어서",
          "original_form": "여권에 찍을 수 있는 도장?있어서",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005291.1.31",
          "form": "좋았어요",
          "original_form": "좋았어요",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005291.1.32",
          "form": "맞아요 근데 그거 여권에찍으면안된다는말반 찍어도된다는말반반이어가지고",
          "original_form": "맞아요 근데 그거 여권에찍으면안된다는말반 찍어도된다는말반반이어가지고",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005291.1.33",
          "form": "저 돌아오고 베트남갈때 여권다시 발급받았어요ㅜ",
          "original_form": "저 돌아오고 베트남갈때 여권다시 발급받았어요ㅜ",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005291.1.34",
          "form": "베트남 여권검사 빡세다고해서ㅜ",
          "original_form": "베트남 여권검사 빡세다고해서ㅜ",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005291.1.35",
          "form": "다른나라갈때도 계속 그거도장찍은상태로 잘갔었는데 베트남은 빠꾸당한다는말이많아서ㅜ",
          "original_form": "다른나라갈때도 계속 그거도장찍은상태로 잘갔었는데 베트남은 빠꾸당한다는말이많아서ㅜ",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005291.1.36",
          "form": "헐...? 그런가요",
          "original_form": "헐...? 그런가요",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005291.1.37",
          "form": "저는 어차피 그거 돌아오고 나서",
          "original_form": "저는 어차피 그거 돌아오고 나서",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005291.1.38",
          "form": "여권 만료돼서",
          "original_form": "여권 만료돼서",
          "speaker_id": "1",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005291.1.39",
          "form": "다시 발급받고 베트남 갔어요",
          "original_form": "다시 발급받고 베트남 갔어요",
          "speaker_id": "1",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005291.1.40",
          "form": "네 말들이많아서요ㅜ 그래서 혹시나걍그대로들고갔다가 못들어갈까봐 바꾸고갔어요",
          "original_form": "네 말들이많아서요ㅜ 그래서 혹시나걍그대로들고갔다가 못들어갈까봐 바꾸고갔어요",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005291.1.41",
          "form": "근데 별검사안하더라구여...ㅋㅋㅋㅋㅋ",
          "original_form": "근데 별검사안하더라구여...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
