      "utterance": [
        {
          "id": "MDRW2100035981.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210830 13:07"
        },
        {
          "id": "MDRW2100035981.1.2",
          "form": "식사는 하셨어요?",
          "original_form": "식사는 하셨어요?",
          "speaker_id": "1",
          "time": "20210830 13:07"
        },
        {
          "id": "MDRW2100035981.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210830 15:09"
        },
        {
          "id": "MDRW2100035981.1.4",
          "form": "네 점심먹었어요",
          "original_form": "네 점심먹었어요",
          "speaker_id": "2",
          "time": "20210830 15:09"
        },
        {
          "id": "MDRW2100035981.1.5",
          "form": "점심 맛있는거 드셨나요?",
          "original_form": "점심 맛있는거 드셨나요?",
          "speaker_id": "2",
          "time": "20210830 15:10"
        },
        {
          "id": "MDRW2100035981.1.6",
          "form": "넵!ㅎㅎㅎ 수제버거 먹었어용",
          "original_form": "넵!ㅎㅎㅎ 수제버거 먹었어용",
          "speaker_id": "1",
          "time": "20210830 15:34"
        },
        {
          "id": "MDRW2100035981.1.7",
          "form": "어떤거 드셨어요?",
          "original_form": "어떤거 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 15:34"
        },
        {
          "id": "MDRW2100035981.1.8",
          "form": "오 ㅋㅋ 저는 고추장불고기먹었어요 ㅋㅋ",
          "original_form": "오 ㅋㅋ 저는 고추장불고기먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100035981.1.9",
          "form": "우왕 맛있는거 드셨네용!",
          "original_form": "우왕 맛있는거 드셨네용!",
          "speaker_id": "1",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100035981.1.10",
          "form": "수제버거는 어디서 드셨어요? ㅋㅋ근처에 맛집이 있나요?",
          "original_form": "수제버거는 어디서 드셨어요? ㅋㅋ근처에 맛집이 있나요?",
          "speaker_id": "2",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100035981.1.11",
          "form": "매운음식 잘드세요?",
          "original_form": "매운음식 잘드세요?",
          "speaker_id": "1",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100035981.1.12",
          "form": "신라면 정도의 매운정도까지 먹을 수 있어요 ㅋㅋ",
          "original_form": "신라면 정도의 매운정도까지 먹을 수 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:39"
        },
        {
          "id": "MDRW2100035981.1.13",
          "form": "매운거 잘드세요?",
          "original_form": "매운거 잘드세요?",
          "speaker_id": "2",
          "time": "20210830 15:39"
        },
        {
          "id": "MDRW2100035981.1.14",
          "form": "저 동네에 개인으로 하시는 수제버거집 있는데 맛있어서 종종 사먹어요ㅎㅎ",
          "original_form": "저 동네에 개인으로 하시는 수제버거집 있는데 맛있어서 종종 사먹어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 15:39"
        },
        {
          "id": "MDRW2100035981.1.15",
          "form": "아니용ㅠㅠ 엽기떡볶이 1단계도 매워서 못먹어요ㅠㅠ",
          "original_form": "아니용ㅠㅠ 엽기떡볶이 1단계도 매워서 못먹어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 15:39"
        },
        {
          "id": "MDRW2100035981.1.16",
          "form": "헛 매운거 잘 못드시는군요 ㅠㅠ",
          "original_form": "헛 매운거 잘 못드시는군요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 15:43"
        },
        {
          "id": "MDRW2100035981.1.17",
          "form": "그래도 요즘에는 로제떡볶이 같이 덜 매운것들이 많아져서 다행인것같아요",
          "original_form": "그래도 요즘에는 로제떡볶이 같이 덜 매운것들이 많아져서 다행인것같아요",
          "speaker_id": "2",
          "time": "20210830 15:43"
        },
        {
          "id": "MDRW2100035981.1.18",
          "form": "혹시 드셔보셨나요?",
          "original_form": "혹시 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100035981.1.19",
          "form": "네네ㅠㅠ 맞아요!",
          "original_form": "네네ㅠㅠ 맞아요!",
          "speaker_id": "1",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100035981.1.20",
          "form": "로제떡볶이 좋아해요ㅎㅎ",
          "original_form": "로제떡볶이 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100035981.1.21",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100035981.1.22",
          "form": "저는 저번에 로제찜닭먹어봣는데 맵지않고 맛있더라구요!!",
          "original_form": "저는 저번에 로제찜닭먹어봣는데 맵지않고 맛있더라구요!!",
          "speaker_id": "2",
          "time": "20210830 15:46"
        },
        {
          "id": "MDRW2100035981.1.23",
          "form": "아 거기 그 두찜이라는 찜닭브랜드꺼",
          "original_form": "아 거기 그 두찜이라는 찜닭브랜드꺼",
          "speaker_id": "1",
          "time": "20210830 15:46"
        },
        {
          "id": "MDRW2100035981.1.24",
          "form": "저도 먹어봤어요!",
          "original_form": "저도 먹어봤어요!",
          "speaker_id": "1",
          "time": "20210830 15:47"
        },
        {
          "id": "MDRW2100035981.1.25",
          "form": "오오 맞아요 저도 두찜꺼 먹어봤어요 ㅋㅋㅋ 저는 중국당면도 좋아하는데 넉넉하게 많이들어있더라구요",
          "original_form": "오오 맞아요 저도 두찜꺼 먹어봤어요 ㅋㅋㅋ 저는 중국당면도 좋아하는데 넉넉하게 많이들어있더라구요",
          "speaker_id": "2",
          "time": "20210830 15:50"
        },
        {
          "id": "MDRW2100035981.1.26",
          "form": "힝 저희 동네 지점은 넉넉하지 않았어요ㅠㅠㅠ",
          "original_form": "힝 저희 동네 지점은 넉넉하지 않았어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 15:50"
        },
        {
          "id": "MDRW2100035981.1.27",
          "form": "닭이랑 로제소스가 의외로 잘어울리더라구요",
          "original_form": "닭이랑 로제소스가 의외로 잘어울리더라구요",
          "speaker_id": "1",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100035981.1.28",
          "form": "헐 찜닭의 핵심은 중국당면인데ㅜ 넉넉하지않다니.. 너무하네요",
          "original_form": "헐 찜닭의 핵심은 중국당면인데ㅜ 넉넉하지않다니.. 너무하네요",
          "speaker_id": "2",
          "time": "20210830 15:57"
        },
        {
          "id": "MDRW2100035981.1.29",
          "form": "근데 저도 느끼할까봐 걱정했는데 부드럽고 맛있더라구요!!",
          "original_form": "근데 저도 느끼할까봐 걱정했는데 부드럽고 맛있더라구요!!",
          "speaker_id": "2",
          "time": "20210830 15:57"
        }
      ]
