      "utterance": [
        {
          "id": "MDRW2100021110.1.1",
          "form": "아뇨 잘 못먹어요",
          "original_form": "아뇨 잘 못먹어요",
          "speaker_id": "2",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021110.1.2",
          "form": "매운거 잘 드세요?",
          "original_form": "매운거 잘 드세요?",
          "speaker_id": "2",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021110.1.3",
          "form": "네 저는 좋아해요 ㅎㅎ !",
          "original_form": "네 저는 좋아해요 ㅎㅎ !",
          "speaker_id": "1",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021110.1.4",
          "form": "그럼 불닭볶음면 좋아하세요?",
          "original_form": "그럼 불닭볶음면 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 20:22"
        },
        {
          "id": "MDRW2100021110.1.5",
          "form": "네 좋아해요! 근데",
          "original_form": "네 좋아해요! 근데",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100021110.1.6",
          "form": "까르보불닭을 더 좋아해요 ㅎㅎ",
          "original_form": "까르보불닭을 더 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021110.1.7",
          "form": "아 저도 까르보불닭 좋아해요",
          "original_form": "아 저도 까르보불닭 좋아해요",
          "speaker_id": "2",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021110.1.8",
          "form": "좀 맵긴 한데 중독성 있어요",
          "original_form": "좀 맵긴 한데 중독성 있어요",
          "speaker_id": "2",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021110.1.9",
          "form": "까르보에 타코야끼 같이 드셔보셨어요??",
          "original_form": "까르보에 타코야끼 같이 드셔보셨어요??",
          "speaker_id": "1",
          "time": "20210828 20:25"
        },
        {
          "id": "MDRW2100021110.1.10",
          "form": "파는 타코야끼요??",
          "original_form": "파는 타코야끼요??",
          "speaker_id": "2",
          "time": "20210828 20:25"
        },
        {
          "id": "MDRW2100021110.1.11",
          "form": "네네 !",
          "original_form": "네네 !",
          "speaker_id": "1",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100021110.1.12",
          "form": "아 둘이 잘어울리나요??",
          "original_form": "아 둘이 잘어울리나요??",
          "speaker_id": "2",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100021110.1.13",
          "form": "한번도 상상 못해본 조합이에요!",
          "original_form": "한번도 상상 못해본 조합이에요!",
          "speaker_id": "2",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100021110.1.14",
          "form": "저도 유튜브에서 보고 먹어봤는데",
          "original_form": "저도 유튜브에서 보고 먹어봤는데",
          "speaker_id": "1",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100021110.1.15",
          "form": "잘어울려요 ㅋㅋ !",
          "original_form": "잘어울려요 ㅋㅋ !",
          "speaker_id": "1",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100021110.1.16",
          "form": "한번 드셔보세요 ㅎㅎ",
          "original_form": "한번 드셔보세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100021110.1.17",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100021110.1.18",
          "form": "근처에 타코야끼 파는 곳 있나요?",
          "original_form": "근처에 타코야끼 파는 곳 있나요?",
          "speaker_id": "2",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100021110.1.19",
          "form": "네 ! 타코비라고 체인인거같아요",
          "original_form": "네 ! 타코비라고 체인인거같아요",
          "speaker_id": "1",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100021110.1.20",
          "form": "와 이제 타코야끼도 체인이 있군요",
          "original_form": "와 이제 타코야끼도 체인이 있군요",
          "speaker_id": "2",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100021110.1.21",
          "form": "여긴 아직 돌아다니는 트럭? 같은게 찾아와야해서",
          "original_form": "여긴 아직 돌아다니는 트럭? 같은게 찾아와야해서",
          "speaker_id": "2",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021110.1.22",
          "form": "가끔만 사먹게돼더라구요",
          "original_form": "가끔만 사먹게돼더라구요",
          "speaker_id": "2",
          "time": "20210828 21:53"
        }
      ]
