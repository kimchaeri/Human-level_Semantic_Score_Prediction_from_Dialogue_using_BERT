      "utterance": [
        {
          "id": "MDRW2100038782.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100038782.1.2",
          "form": "전시 보러 다니는거 좋아하세요?",
          "original_form": "전시 보러 다니는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038782.1.3",
          "form": "전시 보다는 공연 보러다니는걸 좋아해요",
          "original_form": "전시 보다는 공연 보러다니는걸 좋아해요",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038782.1.4",
          "form": "어떤 공연보러 다니셨나요",
          "original_form": "어떤 공연보러 다니셨나요",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038782.1.5",
          "form": "지방이라 기회가 많지 않아 자주는 못 보구요",
          "original_form": "지방이라 기회가 많지 않아 자주는 못 보구요",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038782.1.6",
          "form": "뮤지컬이랑 연극이랑 콘서트 가끔씩이요",
          "original_form": "뮤지컬이랑 연극이랑 콘서트 가끔씩이요",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038782.1.7",
          "form": "와 다양하게 보셨네요",
          "original_form": "와 다양하게 보셨네요",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038782.1.8",
          "form": "보러다니는걸 좋아해서요ㅎㅎ\r\n근데 공연 못 본지 정말 오래되었어요ㅜㅜ",
          "original_form": "보러다니는걸 좋아해서요ㅎㅎ\r\n근데 공연 못 본지 정말 오래되었어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100038782.1.9",
          "form": "저두요ㅠㅠ 온라인 콘서트만 즐겼어요",
          "original_form": "저두요ㅠㅠ 온라인 콘서트만 즐겼어요",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038782.1.10",
          "form": "요즘 오프가 불가하니 온라인콘서트도 많이 하는거 같아요!",
          "original_form": "요즘 오프가 불가하니 온라인콘서트도 많이 하는거 같아요!",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038782.1.11",
          "form": "맞아요 다들 온콘하고 있죠",
          "original_form": "맞아요 다들 온콘하고 있죠",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038782.1.12",
          "form": "온콘 뭐 보셨어요?",
          "original_form": "온콘 뭐 보셨어요?",
          "speaker_id": "2",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100038782.1.13",
          "form": "방탄소년단이랑 투바투 온콘 봤었어요",
          "original_form": "방탄소년단이랑 투바투 온콘 봤었어요",
          "speaker_id": "1",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100038782.1.14",
          "form": "저는 아직 온콘은 못 봤어요ㅜㅜ",
          "original_form": "저는 아직 온콘은 못 봤어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100038782.1.15",
          "form": "온콘 볼만해요",
          "original_form": "온콘 볼만해요",
          "speaker_id": "1",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100038782.1.16",
          "form": "영구소장도 가능하구요",
          "original_form": "영구소장도 가능하구요",
          "speaker_id": "1",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100038782.1.17",
          "form": "영구소장도 가능하다니 좋은데요!",
          "original_form": "영구소장도 가능하다니 좋은데요!",
          "speaker_id": "2",
          "time": "20210902 21:30"
        }
      ]
