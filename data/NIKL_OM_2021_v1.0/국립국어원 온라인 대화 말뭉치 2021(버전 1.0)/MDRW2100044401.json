      "utterance": [
        {
          "id": "MDRW2100044401.1.1",
          "form": "맞아요 ㅜㅠ 영화너무 비싸여",
          "original_form": "맞아요 ㅜㅠ 영화너무 비싸여",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044401.1.2",
          "form": "그니깐요 예전에는 그래도 만원은 안넘었던 것 같은데",
          "original_form": "그니깐요 예전에는 그래도 만원은 안넘었던 것 같은데",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044401.1.3",
          "form": "그리고 모가디슈 같은건 영화관에서 보기 아까워요",
          "original_form": "그리고 모가디슈 같은건 영화관에서 보기 아까워요",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044401.1.4",
          "form": "인셉션이나 아바타같은 대작 아니면 ㅠ",
          "original_form": "인셉션이나 아바타같은 대작 아니면 ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044401.1.5",
          "form": "뭔가 영화관에서 봐야 더 즐거운 영화들이 있긴하죠 ㅋㅋㅋ",
          "original_form": "뭔가 영화관에서 봐야 더 즐거운 영화들이 있긴하죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044401.1.6",
          "form": "원래 해외영화 주로 보세요?",
          "original_form": "원래 해외영화 주로 보세요?",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044401.1.7",
          "form": "네네!! 좋아해요 ㅎㅎㅎ",
          "original_form": "네네!! 좋아해요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044401.1.8",
          "form": "한국영화는 좀 뻔한게 많아서 ㅠ",
          "original_form": "한국영화는 좀 뻔한게 많아서 ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044401.1.9",
          "form": "그러시구나 ㅋㅋ 저는 그냥 가리지 않고 보는 편이거든요",
          "original_form": "그러시구나 ㅋㅋ 저는 그냥 가리지 않고 보는 편이거든요",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044401.1.10",
          "form": "혹시 인생영화 있으세요?",
          "original_form": "혹시 인생영화 있으세요?",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044401.1.11",
          "form": "저는.. 마틸다 아세요??",
          "original_form": "저는.. 마틸다 아세요??",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044401.1.12",
          "form": "어... 아뇨 잘 모르겠어요",
          "original_form": "어... 아뇨 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044401.1.13",
          "form": "옛날외국영화 라서 모르실거예요 ㅋㅋㅋㅋ",
          "original_form": "옛날외국영화 라서 모르실거예요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044401.1.14",
          "form": "name2님은요??",
          "original_form": "&name2&님은요??",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044401.1.15",
          "form": "저는 아직 인생영화라고 할 만한 건 없는 것 같아요",
          "original_form": "저는 아직 인생영화라고 할 만한 건 없는 것 같아요",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044401.1.16",
          "form": "근데 겨울왕국2를 많이 관람하긴 했어요 ㅋㅋㅋ",
          "original_form": "근데 겨울왕국2를 많이 관람하긴 했어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044401.1.17",
          "form": "오오 겨울왕국 너무 재밋져 ㅠㅠㅠㅠ",
          "original_form": "오오 겨울왕국 너무 재밋져 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044401.1.18",
          "form": "오에스티가 좋드라구요",
          "original_form": "오에스티가 좋드라구요",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044401.1.19",
          "form": "맞아요!! 노래도 좋고 사실 저는 2의 스토리도 좋아해요",
          "original_form": "맞아요!! 노래도 좋고 사실 저는 2의 스토리도 좋아해요",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044401.1.20",
          "form": "엘사랑 안나가 성장하는 이야기요",
          "original_form": "엘사랑 안나가 성장하는 이야기요",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044401.1.21",
          "form": "제가 성장하는 인물 이야기를 좋아하거든요 ㅋㅋㅋ",
          "original_form": "제가 성장하는 인물 이야기를 좋아하거든요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044401.1.22",
          "form": "오오 저 투 봣는데 기억이 안나요 ㅠㅠ",
          "original_form": "오오 저 투 봣는데 기억이 안나요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044401.1.23",
          "form": "저두요!!! 성장물 너무 좋죠",
          "original_form": "저두요!!! 성장물 너무 좋죠",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044401.1.24",
          "form": "그쵸 막 고난과 역경을 이겨내고 한층 정신적으로나 혹은 능력이 생기는 그런 이야기",
