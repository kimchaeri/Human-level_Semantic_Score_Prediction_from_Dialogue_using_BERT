      "utterance": [
        {
          "id": "MDRW2100038366.1.1",
          "form": "그렇지 정말 문제다",
          "original_form": "그렇지 정말 문제다",
          "speaker_id": "2",
          "time": "20210902 11:24"
        },
        {
          "id": "MDRW2100038366.1.2",
          "form": "그래서 어제는 투랑 같이 게임 같은 거 하면서",
          "original_form": "그래서 어제는 투랑 같이 게임 같은 거 하면서",
          "speaker_id": "2",
          "time": "20210902 11:24"
        },
        {
          "id": "MDRW2100038366.1.3",
          "form": "뭐 좀 가르쳐 줄려고",
          "original_form": "뭐 좀 가르쳐 줄려고",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100038366.1.4",
          "form": "6시에 원 오기 전까지",
          "original_form": "6시에 원 오기 전까지",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100038366.1.5",
          "form": "아무것도 안하고 투한테 시간을 좀 투자해야겠더라고",
          "original_form": "아무것도 안하고 투한테 시간을 좀 투자해야겠더라고",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100038366.1.6",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100038366.1.7",
          "form": "나두 좀 그래야겠다",
          "original_form": "나두 좀 그래야겠다",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100038366.1.8",
          "form": "내가 안 놀아주니까",
          "original_form": "내가 안 놀아주니까",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100038366.1.9",
          "form": "오늘 좀 와닿네ㅜ",
          "original_form": "오늘 좀 와닿네ㅜ",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100038366.1.10",
          "form": "심심하니까 계속 영상 보여달라 하거든",
          "original_form": "심심하니까 계속 영상 보여달라 하거든",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100038366.1.11",
          "form": "맞어 나두그래",
          "original_form": "맞어 나두그래",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100038366.1.12",
          "form": "맞제 나도 약간 난독증 같은 거 될까봐",
          "original_form": "맞제 나도 약간 난독증 같은 거 될까봐",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100038366.1.13",
          "form": "응응 그렇다ㅜ",
          "original_form": "응응 그렇다ㅜ",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100038366.1.14",
          "form": "어제 갑자기 엄청 겁나는 거야",
          "original_form": "어제 갑자기 엄청 겁나는 거야",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100038366.1.15",
          "form": "맞네 나두 좀 미안하네",
          "original_form": "맞네 나두 좀 미안하네",
          "speaker_id": "1",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100038366.1.16",
          "form": "너무 티비만보여줌",
          "original_form": "너무 티비만보여줌",
          "speaker_id": "1",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100038366.1.17",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "1",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100038366.1.18",
          "form": "오늘 쉬고 체력을 좋게해서",
          "original_form": "오늘 쉬고 체력을 좋게해서",
          "speaker_id": "1",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100038366.1.19",
          "form": "놀아줘야겠엉",
          "original_form": "놀아줘야겠엉",
          "speaker_id": "1",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100038366.1.20",
          "form": "동화책도 읽어주고",
          "original_form": "동화책도 읽어주고",
          "speaker_id": "1",
          "time": "20210902 11:27"
        },
        {
          "id": "MDRW2100038366.1.21",
          "form": "글체 ㅠ 아직 지오는 어리니 니가 좀 더 애쓰면 금방 나아지지싶다",
          "original_form": "글체 ㅠ 아직 지오는 어리니 니가 좀 더 애쓰면 금방 나아지지싶다",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100038366.1.22",
          "form": "난 투 애기때 입원하면서",
          "original_form": "난 투 애기때 입원하면서",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100038366.1.23",
          "form": "ㅋㅋ얍얍 언니두 너무 걱정말구",
          "original_form": "ㅋㅋ얍얍 언니두 너무 걱정말구",
          "speaker_id": "1",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100038366.1.24",
          "form": "힘들어서 보여주기 시작해가지고",
          "original_form": "힘들어서 보여주기 시작해가지고",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100038366.1.25",
          "form": "이제 잘 안되네 ㅠ",
          "original_form": "이제 잘 안되네 ㅠ",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100038366.1.26",
          "form": "나두그렇지뭐ㅜ",
          "original_form": "나두그렇지뭐ㅜ",
          "speaker_id": "1",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100038366.1.27",
          "form": "우리 다 분발하자",
          "original_form": "우리 다 분발하자",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100038366.1.28",
          "form": "바보 되면 안되니",
          "original_form": "바보 되면 안되니",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100038366.1.29",
          "form": "그래 엄마들이 노력하자",
          "original_form": "그래 엄마들이 노력하자",
          "speaker_id": "1",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100038366.1.30",
          "form": "홧팅ㅋ",
          "original_form": "홧팅ㅋ",
          "speaker_id": "1",
          "time": "20210902 11:30"
        }
      ]
    }
  ]
