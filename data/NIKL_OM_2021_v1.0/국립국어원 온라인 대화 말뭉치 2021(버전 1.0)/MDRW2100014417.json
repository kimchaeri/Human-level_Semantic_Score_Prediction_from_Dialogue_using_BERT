      "utterance": [
        {
          "id": "MDRW2100014417.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014417.1.2",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014417.1.3",
          "form": "아직이요 ㅠㅠ name1님은 저녁 드셨나요?",
          "original_form": "아직이요 ㅠㅠ &name1&님은 저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014417.1.4",
          "form": "저는 김밥을 먹고싶었는데",
          "original_form": "저는 김밥을 먹고싶었는데",
          "speaker_id": "1",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014417.1.5",
          "form": "사오기로 한 사람이 힘들다고",
          "original_form": "사오기로 한 사람이 힘들다고",
          "speaker_id": "1",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014417.1.6",
          "form": "없던걸로 하자해서",
          "original_form": "없던걸로 하자해서",
          "speaker_id": "1",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014417.1.7",
          "form": "굶을꺼같앙ᆢ",
          "original_form": "굶을꺼같앙ᆢ",
          "speaker_id": "1",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014417.1.8",
          "form": "아요",
          "original_form": "아요",
          "speaker_id": "1",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014417.1.9",
          "form": "밥을 거르면 안 돼요 ㅠㅠ.. 유튜브에 백종원 조리법쳐서 함 따라서 먹어보셔요",
          "original_form": "밥을 거르면 안 돼요 ㅠㅠ.. 유튜브에 백종원 조리법쳐서 함 따라서 먹어보셔요",
          "speaker_id": "2",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014417.1.10",
          "form": "집에서 해먹는거 너무 귀찮아요",
          "original_form": "집에서 해먹는거 너무 귀찮아요",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014417.1.11",
          "form": "특히 설거지가",
          "original_form": "특히 설거지가",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014417.1.12",
          "form": "제일 고역이죠",
          "original_form": "제일 고역이죠",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014417.1.13",
          "form": "맛집이라도 가셔서 드세요 ㅠㅠ 굶는건 안 좋아요",
          "original_form": "맛집이라도 가셔서 드세요 ㅠㅠ 굶는건 안 좋아요",
          "speaker_id": "2",
          "time": "20210825 19:01"
        },
        {
          "id": "MDRW2100014417.1.14",
          "form": "저녁은 안먹어도 괜찮아요 그래도 시현님은 챙겨드세요",
          "original_form": "저녁은 안먹어도 괜찮아요 그래도 시현님은 챙겨드세요",
          "speaker_id": "1",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100014417.1.15",
          "form": "name2님은 좋아하는 음식있나요?",
          "original_form": "&name2&님은 좋아하는 음식있나요?",
          "speaker_id": "1",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014417.1.16",
          "form": "저는 라면을 엄청 좋아해요",
          "original_form": "저는 라면을 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014417.1.17",
          "form": "가끔씩 유튜브에서 다양한 라면 조리법을 보고 따라하기도 한답니다",
          "original_form": "가끔씩 유튜브에서 다양한 라면 조리법을 보고 따라하기도 한답니다",
          "speaker_id": "2",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014417.1.18",
          "form": "오 라면 맛있죠",
          "original_form": "오 라면 맛있죠",
          "speaker_id": "1",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014417.1.19",
          "form": "어느 라면이 최애라면 인가요?",
          "original_form": "어느 라면이 최애라면 인가요?",
          "speaker_id": "1",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014417.1.20",
          "form": "진라면 매운맛이 최애라면이에요 ㅎㅎ 순한맛은 땅바닥에 버려요",
          "original_form": "진라면 매운맛이 최애라면이에요 ㅎㅎ 순한맛은 땅바닥에 버려요",
          "speaker_id": "2",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014417.1.21",
          "form": "저랑 같으시네요",
          "original_form": "저랑 같으시네요",
          "speaker_id": "1",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014417.1.22",
          "form": "진순이랑은 겸상도 안하죠",
          "original_form": "진순이랑은 겸상도 안하죠",
          "speaker_id": "1",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014417.1.23",
          "form": "요즘은 신라면도 좋아해요",
          "original_form": "요즘은 신라면도 좋아해요",
          "speaker_id": "1",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014417.1.24",
          "form": "최애 토핑은 무엇인가요?",
          "original_form": "최애 토핑은 무엇인가요?",
          "speaker_id": "1",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014417.1.25",
          "form": "토핑은 잘 안해먹지만 자주 넣는걸로는 음..",
          "original_form": "토핑은 잘 안해먹지만 자주 넣는걸로는 음..",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014417.1.26",
          "form": "역시 김치죠",
          "original_form": "역시 김치죠",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014417.1.27",
          "form": "나중에 콩나물 넣어서 드셔보세요",
          "original_form": "나중에 콩나물 넣어서 드셔보세요",
          "speaker_id": "1",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014417.1.28",
          "form": "아주시원하답니다",
          "original_form": "아주시원하답니다",
