      "utterance": [
        {
          "id": "MDRW2100046067.1.1",
          "form": "٩(*̀ᴗ́*)و제출했어요",
          "original_form": "٩(*{emoji:•}̀ᴗ{emoji:•}́*)و제출했어요",
          "speaker_id": "1",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046067.1.2",
          "form": "회사신가요?",
          "original_form": "회사신가요?",
          "speaker_id": "1",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100046067.1.3",
          "form": "네! 회사는 아니고ㅜㅜ 학교요!",
          "original_form": "네! 회사는 아니고ㅜㅜ 학교요!",
          "speaker_id": "2",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046067.1.4",
          "form": "이제 막 출근했어요 ㅎㅎ",
          "original_form": "이제 막 출근했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046067.1.5",
          "form": "아.. 조교하신다고 했었네요ㅋㅋㅋㅋ",
          "original_form": "아.. 조교하신다고 했었네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046067.1.6",
          "form": "일은 많이 어렵지않나요?",
          "original_form": "일은 많이 어렵지않나요?",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046067.1.7",
          "form": "이제 첫 학기라서 조금 신경쓸 일이 많은데",
          "original_form": "이제 첫 학기라서 조금 신경쓸 일이 많은데",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046067.1.8",
          "form": "선배랑 친해서 괜찮아요! ㅎㅎ",
          "original_form": "선배랑 친해서 괜찮아요! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046067.1.9",
          "form": "name1님은 직장에서 스트레스 받으실 때 어떻게 해소하시나요?",
          "original_form": "&name1&님은 직장에서 스트레스 받으실 때 어떻게 해소하시나요?",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046067.1.10",
          "form": "저는 무리해서라도 움직여 맛있는거 먹으러가요ㅋㅋㅋㅋ",
          "original_form": "저는 무리해서라도 움직여 맛있는거 먹으러가요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046067.1.11",
          "form": "앗 너무 좋은데요",
          "original_form": "앗 너무 좋은데요",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046067.1.12",
          "form": "최근에는 어떤 걸 드셨어요?!",
          "original_form": "최근에는 어떤 걸 드셨어요?!",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046067.1.13",
          "form": "혼자라도 갑니다",
          "original_form": "혼자라도 갑니다",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046067.1.14",
          "form": "최근에는 무화과 얼그레이케익이요",
          "original_form": "최근에는 무화과 얼그레이케익이요",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046067.1.15",
          "form": "우왓 !",
          "original_form": "우왓 {emoji:•}{emoji:•}{emoji:•}{emoji:•}!",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046067.1.16",
          "form": "아 name1님 그거 아세요?",
          "original_form": "아 &name1&님 그거 아세요?",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046067.1.17",
          "form": "여기선 이모지가",
          "original_form": "여기선 이모지가",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046067.1.18",
          "form": "안보여요 .... !",
          "original_form": "안보여요 .... !",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046067.1.19",
          "form": "༼;´༎ຶ۝༎ຶ༽  그래서 전 이런 이모티콘으로...",
          "original_form": "༼;´༎ຶ۝༎ຶ༽  그래서 전 이런 이모티콘으로...",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046067.1.20",
          "form": "무화과 얼그레이 케이크 무지 맛있었겠어요ㅠ",
          "original_form": "무화과 얼그레이 케이크 무지 맛있었겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046067.1.21",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046067.1.22",
          "form": "망원에 있어요",
          "original_form": "망원에 있어요",
          "speaker_id": "1",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046067.1.23",
          "form": "우와...!!!",
          "original_form": "우와...!!!",
          "speaker_id": "2",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046067.1.24",
          "form": "ᓀ( ́`๑)",
          "original_form": "ᓀ( ́{emoji:◒}`๑)",
          "speaker_id": "1",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046067.1.25",
          "form": "맛있는거 × 맛있는거 네요!",
          "original_form": "맛있는거 × 맛있는거 네요!",
          "speaker_id": "2",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046067.1.26",
          "form": "네네 요즘 무화과에 꼿혀있어요ㅋㅋㅋㅋ",
          "original_form": "네네 요즘 무화과에 꼿혀있어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046067.1.27",
          "form": "무화과는",
          "original_form": "무화과는",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046067.1.28",
          "form": "따로 사먹어도 맛있잖아요?!",
          "original_form": "따로 사먹어도 맛있잖아요?!",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046067.1.29",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "1",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046067.1.30",
          "form": "그래서 같이 먹으면 즐거움이 2배3배요 ㅎㅎ",
