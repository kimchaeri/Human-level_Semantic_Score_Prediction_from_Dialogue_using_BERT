      "utterance": [
        {
          "id": "MDRW2100024499.1.1",
          "form": "어디 가셨나요?",
          "original_form": "어디 가셨나요?",
          "speaker_id": "1",
          "time": "20210831 11:52"
        },
        {
          "id": "MDRW2100024499.1.2",
          "form": "오키나와 갔다왔었어요",
          "original_form": "오키나와 갔다왔었어요",
          "speaker_id": "2",
          "time": "20210831 11:55"
        },
        {
          "id": "MDRW2100024499.1.3",
          "form": "겨울에 여름을 즐기고 왔죠ㅎㅎ",
          "original_form": "겨울에 여름을 즐기고 왔죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 11:55"
        },
        {
          "id": "MDRW2100024499.1.4",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "1",
          "time": "20210831 12:58"
        },
        {
          "id": "MDRW2100024499.1.5",
          "form": "한국사람도 많던데",
          "original_form": "한국사람도 많던데",
          "speaker_id": "1",
          "time": "20210831 12:58"
        },
        {
          "id": "MDRW2100024499.1.6",
          "form": "많아요 패키지 여행으로 갔었는데 다른 한국에서 온 패키지도 많이 봤어요",
          "original_form": "많아요 패키지 여행으로 갔었는데 다른 한국에서 온 패키지도 많이 봤어요",
          "speaker_id": "2",
          "time": "20210831 13:00"
        },
        {
          "id": "MDRW2100024499.1.7",
          "form": "스쿠버다이빙도 강사가 한국인이더라고요",
          "original_form": "스쿠버다이빙도 강사가 한국인이더라고요",
          "speaker_id": "1",
          "time": "20210831 13:05"
        },
        {
          "id": "MDRW2100024499.1.8",
          "form": "한국인이 없는 곳이 있을까요?ㅋㅋ",
          "original_form": "한국인이 없는 곳이 있을까요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:07"
        },
        {
          "id": "MDRW2100024499.1.9",
          "form": "일본은 가까우니",
          "original_form": "일본은 가까우니",
          "speaker_id": "1",
          "time": "20210831 13:08"
        },
        {
          "id": "MDRW2100024499.1.10",
          "form": "스위슨 괜찮았어요",
          "original_form": "스위슨 괜찮았어요",
          "speaker_id": "1",
          "time": "20210831 13:08"
        },
        {
          "id": "MDRW2100024499.1.11",
          "form": "한국사람이 없으면 타지에 온 기분이 더 느껴지셨겠네요",
          "original_form": "한국사람이 없으면 타지에 온 기분이 더 느껴지셨겠네요",
          "speaker_id": "2",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100024499.1.12",
          "form": "자연이 워낙 좋아서 그냥다  좋더라고요",
          "original_form": "자연이 워낙 좋아서 그냥다  좋더라고요",
          "speaker_id": "1",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100024499.1.13",
          "form": "스위스가 마터호른이 있는 곳이던가요",
          "original_form": "스위스가 마터호른이 있는 곳이던가요",
          "speaker_id": "2",
          "time": "20210831 13:14"
        },
        {
          "id": "MDRW2100024499.1.14",
          "form": "넹 그렇네요",
          "original_form": "넹 그렇네요",
          "speaker_id": "1",
          "time": "20210831 13:16"
        },
        {
          "id": "MDRW2100024499.1.15",
          "form": "거긴 안 가셨나요?",
          "original_form": "거긴 안 가셨나요?",
          "speaker_id": "2",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100024499.1.16",
          "form": "전 쉴트호른으로 가서요",
          "original_form": "전 쉴트호른으로 가서요",
          "speaker_id": "1",
          "time": "20210831 13:19"
        },
        {
          "id": "MDRW2100024499.1.17",
          "form": "그 산에 올라가는것보다 산을 마주보고싶어서요",
          "original_form": "그 산에 올라가는것보다 산을 마주보고싶어서요",
          "speaker_id": "1",
          "time": "20210831 13:19"
        },
        {
          "id": "MDRW2100024499.1.18",
          "form": "아하 볼려면 다른 곳으로 올라가는게 맞죠ㅋㅋ",
          "original_form": "아하 볼려면 다른 곳으로 올라가는게 맞죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:24"
        },
        {
          "id": "MDRW2100024499.1.19",
          "form": "넹 알프스산맥 쫙보는게 대박이더라고요  망원경도 공짜라 쫙 볼수있어요 ㅎ",
          "original_form": "넹 알프스산맥 쫙보는게 대박이더라고요  망원경도 공짜라 쫙 볼수있어요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 13:25"
        },
        {
          "id": "MDRW2100024499.1.20",
          "form": "망원경 공짜라는 말에 확 끌리네요",
          "original_form": "망원경 공짜라는 말에 확 끌리네요",
          "speaker_id": "2",
          "time": "20210831 13:27"
        },
        {
          "id": "MDRW2100024499.1.21",
          "form": "그걸로보면 산맥에 이름이랑 높이도 표시되더라고요 신기해요",
          "original_form": "그걸로보면 산맥에 이름이랑 높이도 표시되더라고요 신기해요",
          "speaker_id": "1",
          "time": "20210831 13:29"
        },
        {
          "id": "MDRW2100024499.1.22",
          "form": "기술 좋네요 저도 보고 싶어요",
          "original_form": "기술 좋네요 저도 보고 싶어요",
          "speaker_id": "2",
          "time": "20210831 14:09"
        }
      ]
    }
