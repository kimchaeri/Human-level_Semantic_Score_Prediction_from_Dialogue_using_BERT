      "utterance": [
        {
          "id": "MDRW2100030854.1.1",
          "form": "여행 어디로 가고 싶어?",
          "original_form": "여행 어디로 가고 싶어?",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030854.1.2",
          "form": "나는....해외로 가고 싶어",
          "original_form": "나는....해외로 가고 싶어",
          "speaker_id": "1",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030854.1.3",
          "form": "뭔가 미국 가보고 싶음",
          "original_form": "뭔가 미국 가보고 싶음",
          "speaker_id": "1",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030854.1.4",
          "form": "오 아메리ㅏ",
          "original_form": "오 아메리ㅏ",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030854.1.5",
          "form": "미국 넓잖아",
          "original_form": "미국 넓잖아",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030854.1.6",
          "form": "근데 코로나...때문에...(생략)",
          "original_form": "근데 코로나...때문에...(생략)",
          "speaker_id": "1",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030854.1.7",
          "form": "어디로?",
          "original_form": "어디로?",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030854.1.8",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030854.1.9",
          "form": "ㅋㅋㅋㅋㅋ맞지...코로나 때문에..",
          "original_form": "ㅋㅋㅋㅋㅋ맞지...코로나 때문에..",
          "speaker_id": "2",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030854.1.10",
          "form": "LA랑 뉴욕!",
          "original_form": "LA랑 뉴욕!",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030854.1.11",
          "form": "캘리포니아도 가보고 싶다",
          "original_form": "캘리포니아도 가보고 싶다",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030854.1.12",
          "form": "오오 나두 뉴욕은 한본 거보고 싶더라",
          "original_form": "오오 나두 뉴욕은 한본 거보고 싶더라",
          "speaker_id": "2",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030854.1.13",
          "form": "뉴~욕~ 이거 노래 있지 않나",
          "original_form": "뉴~욕~ 이거 노래 있지 않나",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030854.1.14",
          "form": "유럽도 가보고 싶음",
          "original_form": "유럽도 가보고 싶음",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030854.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030854.1.16",
          "form": "약간 그런 거 배낭여행?",
          "original_form": "약간 그런 거 배낭여행?",
          "speaker_id": "1",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030854.1.17",
          "form": "유럽은 날치기가 너무 많아...",
          "original_form": "유럽은 날치기가 너무 많아...",
          "speaker_id": "2",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030854.1.18",
          "form": "오오 배낭여행 낭만잇어",
          "original_form": "오오 배낭여행 낭만잇어",
          "speaker_id": "2",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030854.1.19",
          "form": "앗 그건 맞아...",
          "original_form": "앗 그건 맞아...",
          "speaker_id": "1",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030854.1.20",
          "form": "ㅠㅠㅠㅠ같이 가고 싶음",
          "original_form": "ㅠㅠㅠㅠ같이 가고 싶음",
          "speaker_id": "1",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030854.1.21",
          "form": "혼자는 조금 무리",
          "original_form": "혼자는 조금 무리",
          "speaker_id": "1",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030854.1.22",
          "form": "친구 1명 같이",
          "original_form": "친구 1명 같이",
          "speaker_id": "1",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030854.1.23",
          "form": "나도ㅜㅠ 너랑 같이 여행가고 싶어",
          "original_form": "나도ㅜㅠ 너랑 같이 여행가고 싶어",
          "speaker_id": "2",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030854.1.24",
          "form": "조건은 영어를 잘해야해",
          "original_form": "조건은 영어를 잘해야해",
          "speaker_id": "1",
          "time": "20210904 13:33"
        }
      ]
    }
