      "utterance": [
        {
          "id": "MDRW2100015304.1.1",
          "form": "군대 갔다오셨어요?ㅎㅎ 저는 4주만갔다와서ㅎ",
          "original_form": "군대 갔다오셨어요?ㅎㅎ 저는 4주만갔다와서ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015304.1.2",
          "form": "전 안 가봤어요",
          "original_form": "전 안 가봤어요",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015304.1.3",
          "form": "가셨을 때 총 잘 쏘셨었나요?",
          "original_form": "가셨을 때 총 잘 쏘셨었나요?",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015304.1.4",
          "form": "20발쐈었나? 그때 3개 빼고 다맞췄네요ㅎ",
          "original_form": "20발쐈었나? 그때 3개 빼고 다맞췄네요ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015304.1.5",
          "form": "몇개맞추셨어요?",
          "original_form": "몇개맞추셨어요?",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015304.1.6",
          "form": "잘 쏘셨네요ㅋㅋ",
          "original_form": "잘 쏘셨네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015304.1.7",
          "form": "전 사격에는 소질이 없더라구요",
          "original_form": "전 사격에는 소질이 없더라구요",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015304.1.8",
          "form": "아ㅎㅎ 군대는 어디 갓다오셨어요m",
          "original_form": "아ㅎㅎ 군대는 어디 갓다오셨어요m",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015304.1.9",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015304.1.10",
          "form": "전 군대 안 갔다왔어요",
          "original_form": "전 군대 안 갔다왔어요",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015304.1.11",
          "form": "아아ㅎㅎㅎ",
          "original_form": "아아ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015304.1.12",
          "form": "남자분이 아니신가요?ㅎ 아니면 신의아들?",
          "original_form": "남자분이 아니신가요?ㅎ 아니면 신의아들?",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015304.1.13",
          "form": "여자입니다",
          "original_form": "여자입니다",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015304.1.14",
          "form": "아ㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "아ㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015304.1.15",
          "form": "전 군대를 선택하셨길래ㅎㅎㅎㅎ",
          "original_form": "전 군대를 선택하셨길래ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015304.1.16",
          "form": "당연히 남자인줄알았어요 죄송해요ㅎㄹ",
          "original_form": "당연히 남자인줄알았어요 죄송해요ㅎㄹ",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015304.1.17",
          "form": "괜찮아요 ㅎㅎ 군생활은 어떠셨나요?",
          "original_form": "괜찮아요 ㅎㅎ 군생활은 어떠셨나요?",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015304.1.18",
          "form": "전 4주훈련만갔다오고",
          "original_form": "전 4주훈련만갔다오고",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015304.1.19",
          "form": "대체복무했어요ㅎㅎ",
          "original_form": "대체복무했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015304.1.20",
          "form": "항해사로 일하면서 군복무 대체햌ㅆ었어요ㅎㅎ",
          "original_form": "항해사로 일하면서 군복무 대체햌ㅆ었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015304.1.21",
          "form": "그럼 군함 말고 일반 배 타셨나보네요",
          "original_form": "그럼 군함 말고 일반 배 타셨나보네요",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015304.1.22",
          "form": "네네ㅎ 상선이요ㅎㅎ",
          "original_form": "네네ㅎ 상선이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015304.1.23",
          "form": "근데 군대보다 길었어요ㅠ 3년 ㅠ",
          "original_form": "근데 군대보다 길었어요ㅠ 3년 ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015304.1.24",
          "form": "같이 일하시던 분들은 어떠셨나요?",
          "original_form": "같이 일하시던 분들은 어떠셨나요?",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015304.1.25",
          "form": "다들 좋은분이셨는데ㅎㅎ",
          "original_form": "다들 좋은분이셨는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015304.1.26",
          "form": "어쩔때보면 안좋으신분들도 있어요ㅎㅎ",
          "original_form": "어쩔때보면 안좋으신분들도 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015304.1.27",
          "form": "아무래도 배생활을 오래하셔서 그런가봐뇨ㅎ",
          "original_form": "아무래도 배생활을 오래하셔서 그런가봐뇨ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015304.1.28",
          "form": "같이 하는 사람이 중요한 것 같아요",
          "original_form": "같이 하는 사람이 중요한 것 같아요",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015304.1.29",
          "form": "그럼 월급은 다른 선원들하고 차이가 있었나요?",
          "original_form": "그럼 월급은 다른 선원들하고 차이가 있었나요?",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015304.1.30",
          "form": "아무래도 부원보다는 사관이니깐 다르나?ㅎㅎ",
          "original_form": "아무래도 부원보다는 사관이니깐 다르나?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015304.1.31",
          "form": "외국인이랑 같이타니깐 한국사람이랑 달라요ㅎㅎ",
          "original_form": "외국인이랑 같이타니깐 한국사람이랑 달라요ㅎㅎ",
