      "utterance": [
        {
          "id": "MDRW2100007472.1.1",
          "form": "초당옥수수 먹어봤어요",
          "original_form": "초당옥수수 먹어봤어요",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007472.1.2",
          "form": "헉 어떤가요",
          "original_form": "헉 어떤가요",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007472.1.3",
          "form": "그것도 맛나나요",
          "original_form": "그것도 맛나나요",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007472.1.4",
          "form": "초당 옥수수",
          "original_form": "초당 옥수수",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007472.1.5",
          "form": "보통 생으로 먹잖아요",
          "original_form": "보통 생으로 먹잖아요",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007472.1.6",
          "form": "생으로 먹는건 별로고 풋내나더라구요",
          "original_form": "생으로 먹는건 별로고 풋내나더라구요",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007472.1.7",
          "form": "엇",
          "original_form": "엇",
          "speaker_id": "2",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007472.1.8",
          "form": "아무래도 익혀먹는게 낫긴한데 찰옥수수만큼은 아니라",
          "original_form": "아무래도 익혀먹는게 낫긴한데 찰옥수수만큼은 아니라",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007472.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007472.1.10",
          "form": "애매하군요",
          "original_form": "애매하군요",
          "speaker_id": "2",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007472.1.11",
          "form": "약간 스위트콘 생각하시면 될거같아요",
          "original_form": "약간 스위트콘 생각하시면 될거같아요",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007472.1.12",
          "form": "아니 스위트콘이 더 나아요",
          "original_form": "아니 스위트콘이 더 나아요",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007472.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007472.1.14",
          "form": "너무 아삭거려서",
          "original_form": "너무 아삭거려서",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007472.1.15",
          "form": "옥수수좋아해요?",
          "original_form": "옥수수좋아해요?",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007472.1.16",
          "form": "앗 아뇨",
          "original_form": "앗 아뇨",
          "speaker_id": "2",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007472.1.17",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007472.1.18",
          "form": "저는 팝콘만 먹어요",
          "original_form": "저는 팝콘만 먹어요",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007472.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007472.1.20",
          "form": "안좋아하시는구나",
          "original_form": "안좋아하시는구나",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007472.1.21",
          "form": "근데 초당옥수숙가",
          "original_form": "근데 초당옥수숙가",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007472.1.22",
          "form": "팝콘 좋아요",
          "original_form": "팝콘 좋아요",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007472.1.23",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007472.1.24",
          "form": "워낙 맛있다해서",
          "original_form": "워낙 맛있다해서",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007472.1.25",
          "form": "한 번 먹어보고싶었자만",
          "original_form": "한 번 먹어보고싶었자만",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007472.1.26",
          "form": "안먹는 것으로",
          "original_form": "안먹는 것으로",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007472.1.27",
          "form": "원래 옥수수 좋아하시나요",
          "original_form": "원래 옥수수 좋아하시나요",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007472.1.28",
          "form": "전 좋아해요ㅋㅋ",
          "original_form": "전 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007472.1.29",
          "form": "떡 옥수수",
          "original_form": "떡 옥수수",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007472.1.30",
          "form": "이런종류",
          "original_form": "이런종류",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007472.1.31",
          "form": "떡 옥수수?",
          "original_form": "떡 옥수수?",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007472.1.32",
          "form": "떡과 옥수수가 최애신 것 같아섴ㅋㅋㅋ",
          "original_form": "떡과 옥수수가 최애신 것 같아섴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007472.1.33",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007472.1.34",
          "form": "품종이 따로 있는 줄ㅋㅋㅋ",
          "original_form": "품종이 따로 있는 줄ㅋㅋㅋ",
