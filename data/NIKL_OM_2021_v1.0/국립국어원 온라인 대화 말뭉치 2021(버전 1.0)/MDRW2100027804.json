      "utterance": [
        {
          "id": "MDRW2100027804.1.1",
          "form": "사과 먹고 왔어요?",
          "original_form": "사과 먹고 왔어요?",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027804.1.2",
          "form": "새로운문제 내줄까요?",
          "original_form": "새로운문제 내줄까요?",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027804.1.3",
          "form": "네 사과 먹고있어요",
          "original_form": "네 사과 먹고있어요",
          "speaker_id": "2",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027804.1.4",
          "form": "저는 사과 껍질채로 먹어요",
          "original_form": "저는 사과 껍질채로 먹어요",
          "speaker_id": "2",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027804.1.5",
          "form": "화끈하네요",
          "original_form": "화끈하네요",
          "speaker_id": "1",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027804.1.6",
          "form": "그게 몸에 더좋죠",
          "original_form": "그게 몸에 더좋죠",
          "speaker_id": "1",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027804.1.7",
          "form": "그 문제는 연습 문제였으니 이번에는",
          "original_form": "그 문제는 연습 문제였으니 이번에는",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027804.1.8",
          "form": "진짜 문제를 내보도록할께요",
          "original_form": "진짜 문제를 내보도록할께요",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027804.1.9",
          "form": "어머니께서는 먹는 게 남는 거라 하셨습니다.",
          "original_form": "어머니께서는 먹는 게 남는 거라 하셨습니다.",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027804.1.10",
          "form": "그럼 열 개의 사과 중에서 세 개를 먹엇다면 남는 것은 몇 개 입니까?",
          "original_form": "그럼 열 개의 사과 중에서 세 개를 먹엇다면 남는 것은 몇 개 입니까?",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027804.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ저는 어머님 말씀을 잘 들으니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저는 어머님 말씀을 잘 들으니까",
          "speaker_id": "2",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027804.1.12",
          "form": "세개가 남는 거라고 할게요",
          "original_form": "세개가 남는 거라고 할게요",
          "speaker_id": "2",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027804.1.13",
          "form": "어? 왜그렇게 생각을하죠?",
          "original_form": "어? 왜그렇게 생각을하죠?",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027804.1.14",
          "form": "어머니가 먹는게 넘는거라 하셨거든요",
          "original_form": "어머니가 먹는게 넘는거라 하셨거든요",
          "speaker_id": "2",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027804.1.15",
          "form": "오~~~문제파악을하셨군요",
          "original_form": "오~~~문제파악을하셨군요",
          "speaker_id": "1",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027804.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ정답인가요 ㅋㅋㅋ 상품으로 과자주세요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ정답인가요 ㅋㅋㅋ 상품으로 과자주세요",
          "speaker_id": "2",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027804.1.17",
          "form": "먹고있는 사과로 일단 우선 대신할께요",
          "original_form": "먹고있는 사과로 일단 우선 대신할께요",
          "speaker_id": "1",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027804.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027804.1.19",
          "form": "문제 또 있어요!!",
          "original_form": "문제 또 있어요!!",
          "speaker_id": "1",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027804.1.20",
          "form": "ㅋㅋ어떤 문제인가요",
          "original_form": "ㅋㅋ어떤 문제인가요",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100027804.1.21",
