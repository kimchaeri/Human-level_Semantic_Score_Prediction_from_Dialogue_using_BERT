      "utterance": [
        {
          "id": "MDRW2100014846.1.1",
          "form": "올림픽 때 배구 봤나??",
          "original_form": "올림픽 때 배구 봤나??",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014846.1.2",
          "form": "이번엔 올림픽을 제대로 못 본 듯",
          "original_form": "이번엔 올림픽을 제대로 못 본 듯",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014846.1.3",
          "form": "봤쥬 봤쥬~",
          "original_form": "봤쥬 봤쥬~",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014846.1.4",
          "form": "양궁 이랑 배구만 겨우 봄",
          "original_form": "양궁 이랑 배구만 겨우 봄",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014846.1.5",
          "form": "김연경이 진짜~!!",
          "original_form": "김연경이 진짜~!!",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014846.1.6",
          "form": "지는 배구밖에는 ㅋㅋㅋㅋㅋ",
          "original_form": "지는 배구밖에는 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014846.1.7",
          "form": "와 진짜 김연경",
          "original_form": "와 진짜 김연경",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014846.1.8",
          "form": "너무 멋있음",
          "original_form": "너무 멋있음",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014846.1.9",
          "form": "김희진 도 너무 귀엽고",
          "original_form": "김희진 도 너무 귀엽고",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014846.1.10",
          "form": "요새도 김연경 유튜브 찾아 봄",
          "original_form": "요새도 김연경 유튜브 찾아 봄",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014846.1.11",
          "form": "그니깐여~진짜잘생김 ㅋㅋㅋ",
          "original_form": "그니깐여~진짜잘생김 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014846.1.12",
          "form": "앵간한 남자보다 멋있음 ㅋㅋㅋㅋㅋ",
          "original_form": "앵간한 남자보다 멋있음 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014846.1.13",
          "form": "어 어  머리 길었을 때는 이쁘더라",
          "original_form": "어 어  머리 길었을 때는 이쁘더라",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014846.1.14",
          "form": "그러이",
          "original_form": "그러이",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014846.1.15",
          "form": "키도 크고",
          "original_form": "키도 크고",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014846.1.16",
          "form": "나 이제 재우러간다",
          "original_form": "나 이제 재우러간다",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014846.1.17",
          "form": "원래 예뻐야 잘생길수 있음 ㅋㅋㅋㅋ",
          "original_form": "원래 예뻐야 잘생길수 있음 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014846.1.18",
          "form": "급 떠나는구만여 ㅋㅋㅋㅋ",
          "original_form": "급 떠나는구만여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014846.1.19",
          "form": "육퇴후 에도 만날 수 있기를",
          "original_form": "육퇴후 에도 만날 수 있기를",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014846.1.20",
          "form": "언니가.돌아오믄 나는 자고있을듯요 ㅋㅋㅋㅋ",
          "original_form": "언니가.돌아오믄 나는 자고있을듯요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014846.1.21",
          "form": "그래 어여 자라",
          "original_form": "그래 어여 자라",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014846.1.22",
          "form": "한방에 성공허고 오셔요 ㅋㅋㅋㅋ",
          "original_form": "한방에 성공허고 오셔요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014846.1.23",
          "form": "내일 일찍 만나자",
          "original_form": "내일 일찍 만나자",
          "speaker_id": "1",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014846.1.24",
          "form": "숙제 잘 챙기고",
          "original_form": "숙제 잘 챙기고",
          "speaker_id": "1",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014846.1.25",
          "form": "넵~단디 챙겨서 울면서 오는일이 없도록 ㅋㅋㅋㅋㅋ",
          "original_form": "넵~단디 챙겨서 울면서 오는일이 없도록 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014846.1.26",
          "form": "일어나요 ㅋㅋㅋㅋㅋ",
          "original_form": "일어나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 07:04"
        },
        {
          "id": "MDRW2100014846.1.27",
          "form": "울애들 운동장 발야구라도 보러가요 ㅋㅋㅋㅋ",
          "original_form": "울애들 운동장 발야구라도 보러가요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 07:04"
        },
        {
          "id": "MDRW2100014846.1.28",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㄱㄱㅋ 우리원 개 발 축구도 보러와",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㄱㄱㅋ 우리원 개 발 축구도 보러와",
