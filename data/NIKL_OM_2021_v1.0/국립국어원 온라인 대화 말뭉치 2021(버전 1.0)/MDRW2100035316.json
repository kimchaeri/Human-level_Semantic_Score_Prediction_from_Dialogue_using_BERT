      "utterance": [
        {
          "id": "MDRW2100035316.1.1",
          "form": "다른걸 먹어도 한식이 잘 맞더라구요",
          "original_form": "다른걸 먹어도 한식이 잘 맞더라구요",
          "speaker_id": "2",
          "time": "20210829 14:33"
        },
        {
          "id": "MDRW2100035316.1.2",
          "form": "떡볶이도 한식인가요?",
          "original_form": "떡볶이도 한식인가요?",
          "speaker_id": "1",
          "time": "20210829 14:33"
        },
        {
          "id": "MDRW2100035316.1.3",
          "form": "한식이죠!",
          "original_form": "한식이죠!",
          "speaker_id": "2",
          "time": "20210829 14:33"
        },
        {
          "id": "MDRW2100035316.1.4",
          "form": "그럼 전 한식도 좋아해요 ㅋㅋ",
          "original_form": "그럼 전 한식도 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:33"
        },
        {
          "id": "MDRW2100035316.1.5",
          "form": "k떡볶이",
          "original_form": "k떡볶이",
          "speaker_id": "2",
          "time": "20210829 14:34"
        },
        {
          "id": "MDRW2100035316.1.6",
          "form": "초등학생때부터",
          "original_form": "초등학생때부터",
          "speaker_id": "2",
          "time": "20210829 14:34"
        },
        {
          "id": "MDRW2100035316.1.7",
          "form": "소울푸드였어요",
          "original_form": "소울푸드였어요",
          "speaker_id": "2",
          "time": "20210829 14:34"
        },
        {
          "id": "MDRW2100035316.1.8",
          "form": "저도 좋아해요 ㅋㅋㅋ",
          "original_form": "저도 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:35"
        },
        {
          "id": "MDRW2100035316.1.9",
          "form": "그런데 밥 한식은 이게 차리는게 힘들어요",
          "original_form": "그런데 밥 한식은 이게 차리는게 힘들어요",
          "speaker_id": "1",
          "time": "20210829 14:35"
        },
        {
          "id": "MDRW2100035316.1.10",
          "form": "반찬이 여러가지구 밥 국 그리고 메인디쉬는 또 다로라서요",
          "original_form": "반찬이 여러가지구 밥 국 그리고 메인디쉬는 또 다로라서요",
          "speaker_id": "1",
          "time": "20210829 14:35"
        },
        {
          "id": "MDRW2100035316.1.11",
          "form": "그건그래요",
          "original_form": "그건그래요",
          "speaker_id": "2",
          "time": "20210829 14:36"
        },
        {
          "id": "MDRW2100035316.1.12",
          "form": "그만큼 손이 많이가죠",
          "original_form": "그만큼 손이 많이가죠",
          "speaker_id": "2",
          "time": "20210829 14:36"
        },
        {
          "id": "MDRW2100035316.1.13",
          "form": "스파게티는 스파게티 하나 만들고 피클만 먹어도",
          "original_form": "스파게티는 스파게티 하나 만들고 피클만 먹어도",
          "speaker_id": "1",
          "time": "20210829 14:36"
        },
        {
          "id": "MDRW2100035316.1.14",
          "form": "괜찮은데",
          "original_form": "괜찮은데",
          "speaker_id": "1",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100035316.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100035316.1.16",
          "form": "한식은 된장국이랑 밥만 딱 떠놓으면",
          "original_form": "한식은 된장국이랑 밥만 딱 떠놓으면",
          "speaker_id": "1",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100035316.1.17",
          "form": "좀 눈물날 것 같은데요 전 이렇게 먹어본적은 없어요",
          "original_form": "좀 눈물날 것 같은데요 전 이렇게 먹어본적은 없어요",
          "speaker_id": "1",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100035316.1.18",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100035316.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100035316.1.20",
          "form": "그냥 김치랑",
          "original_form": "그냥 김치랑",
          "speaker_id": "2",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100035316.1.21",
          "form": "밥이랑",
          "original_form": "밥이랑",
          "speaker_id": "2",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100035316.1.22",
          "form": "반찬 이정도",
          "original_form": "반찬 이정도",
          "speaker_id": "2",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100035316.1.23",
          "form": "멸치나 김치나 김이라도 꺼내니까요 근데 이것도 만드는걸 생각하면 다 노력 ㅠㅠ",
          "original_form": "멸치나 김치나 김이라도 꺼내니까요 근데 이것도 만드는걸 생각하면 다 노력 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100035316.1.24",
          "form": "밎아요",
          "original_form": "밎아요",
          "speaker_id": "2",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100035316.1.25",
          "form": "진짜 저 예전에",
          "original_form": "진짜 저 예전에",
          "speaker_id": "2",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100035316.1.26",
          "form": "음식집에서 일해본적있는데",
          "original_form": "음식집에서 일해본적있는데",
          "speaker_id": "2",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100035316.1.27",
          "form": "매번 다 만드는 그런 노력이 엄청나더라구요",
          "original_form": "매번 다 만드는 그런 노력이 엄청나더라구요",
          "speaker_id": "2",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100035316.1.28",
          "form": "집 반찬도 다 노력이 들어가는데 주로 엄마가 하기 때문에 ㅋㅋㅋ",
          "original_form": "집 반찬도 다 노력이 들어가는데 주로 엄마가 하기 때문에 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100035316.1.29",
          "form": "그리고 이게 반찬도 금방먹어야 맛있어서 먹어야 되는 기한이있어서",
          "original_form": "그리고 이게 반찬도 금방먹어야 맛있어서 먹어야 되는 기한이있어서",
          "speaker_id": "1",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100035316.1.30",
          "form": "먹어야 되는 시간안에 다 먹기가 힘들어요",
          "original_form": "먹어야 되는 시간안에 다 먹기가 힘들어요",
          "speaker_id": "1",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100035316.1.31",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100035316.1.32",
          "form": "저흰 사먹는데",
          "original_form": "저흰 사먹는데",
          "speaker_id": "2",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100035316.1.33",
          "form": "몇번 먹다가도 다 먹기 힘들더라구요",
          "original_form": "몇번 먹다가도 다 먹기 힘들더라구요",
          "speaker_id": "2",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100035316.1.34",
          "form": "아하 사먹으심 좀 덜버리겠네요 소포장이잖아요",
          "original_form": "아하 사먹으심 좀 덜버리겠네요 소포장이잖아요",
          "speaker_id": "1",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100035316.1.35",
          "form": "전 어머니가 아직 만들어서",
          "original_form": "전 어머니가 아직 만들어서",
          "speaker_id": "1",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100035316.1.36",
          "form": "자꾸 다 못먹고 버려가지고",
          "original_form": "자꾸 다 못먹고 버려가지고",
          "speaker_id": "1",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100035316.1.37",
          "form": "엑셀로 재고를 쓰고 선입선출하자고 제안했는데",
          "original_form": "엑셀로 재고를 쓰고 선입선출하자고 제안했는데",
          "speaker_id": "1",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100035316.1.38",
          "form": "선입선출은 먼저 들어온걸 먼저 출고하는거에요",
          "original_form": "선입선출은 먼저 들어온걸 먼저 출고하는거에요",
          "speaker_id": "1",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100035316.1.39",
          "form": "오래된 반찬일수록 우선순위로 빨리 먹잔거였죠",
          "original_form": "오래된 반찬일수록 우선순위로 빨리 먹잔거였죠",
          "speaker_id": "1",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100035316.1.40",
          "form": "그런 말 했다가 싸패취급 받았어요",
          "original_form": "그런 말 했다가 싸패취급 받았어요",
