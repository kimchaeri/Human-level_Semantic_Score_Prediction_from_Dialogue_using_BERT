      "utterance": [
        {
          "id": "MDRW2100028501.1.1",
          "form": "최근에 구매하신 책 궁금해요!",
          "original_form": "최근에 구매하신 책 궁금해요!",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028501.1.2",
          "form": "앗 저ㅋㅋㅋ 청소년 도서중에 긴긴밤이라는 책 있는데 그거랑 윤고은 밤의 여행자들 샀어요!",
          "original_form": "앗 저ㅋㅋㅋ 청소년 도서중에 긴긴밤이라는 책 있는데 그거랑 윤고은 밤의 여행자들 샀어요!",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028501.1.3",
          "form": "한강 작가 신간도 살까 고민중이궁ㅅ",
          "original_form": "한강 작가 신간도 살까 고민중이궁ㅅ",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028501.1.4",
          "form": "아 밤의여행자들! 재밌다더라고요",
          "original_form": "아 밤의여행자들! 재밌다더라고요",
          "speaker_id": "1",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100028501.1.5",
          "form": "한강작가신간 나왔군요ㅠ",
          "original_form": "한강작가신간 나왔군요ㅠ",
          "speaker_id": "1",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100028501.1.6",
          "form": "서랍에 저녁을 넣어두었다 이건가요?!",
          "original_form": "서랍에 저녁을 넣어두었다 이건가요?!",
          "speaker_id": "1",
          "time": "20210902 20:32"
        },
        {
          "id": "MDRW2100028501.1.7",
          "form": "앗 아뇨! 그건 아마 전에 나왔던 시집 재발간 한 게 아닌가 싶은데",
          "original_form": "앗 아뇨! 그건 아마 전에 나왔던 시집 재발간 한 게 아닌가 싶은데",
          "speaker_id": "2",
          "time": "20210902 20:33"
        },
        {
          "id": "MDRW2100028501.1.8",
          "form": "'작별하지 않는다' 요!",
          "original_form": "'작별하지 않는다' 요!",
          "speaker_id": "2",
          "time": "20210902 20:33"
        },
        {
          "id": "MDRW2100028501.1.9",
          "form": "제주 4.3이 소재예요",
          "original_form": "제주 4.3이 소재예요",
          "speaker_id": "2",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028501.1.10",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028501.1.11",
          "form": "필독서네요",
          "original_form": "필독서네요",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028501.1.12",
          "form": "볼게요저도",
          "original_form": "볼게요저도",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028501.1.13",
          "form": "제주4.3은 진짜 더 알려져야하는데",
          "original_form": "제주4.3은 진짜 더 알려져야하는데",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028501.1.14",
          "form": "정말로요 생각보다 잘 모르는 사람들 많아서 놀랐어요ㅜ",
          "original_form": "정말로요 생각보다 잘 모르는 사람들 많아서 놀랐어요ㅜ",
          "speaker_id": "2",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100028501.1.15",
          "form": "토닉님은 좋아하시는 책이나 작가 있으세요?",
          "original_form": "토닉님은 좋아하시는 책이나 작가 있으세요?",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028501.1.16",
          "form": "저 장르안가리고 읽어요!",
          "original_form": "저 장르안가리고 읽어요!",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028501.1.17",
          "form": "최근읽은건 나는소망한다내게금지된것을 이고",
          "original_form": "최근읽은건 나는소망한다내게금지된것을 이고",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028501.1.18",
          "form": "지금 빌려놓은건 김초엽작가 신작이에요!",
          "original_form": "지금 빌려놓은건 김초엽작가 신작이에요!",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028501.1.19",
          "form": "나는소망한다내게금지된것을<이거 궁금해요!",
          "original_form": "나는소망한다내게금지된것을<이거 궁금해요!",
          "speaker_id": "2",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028501.1.20",
          "form": "그시절에 그런글이 나온게 히트인것같아요",
          "original_form": "그시절에 그런글이 나온게 히트인것같아요",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028501.1.21",
          "form": "결말은 맘에들지않았지만",
          "original_form": "결말은 맘에들지않았지만",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028501.1.22",
          "form": "현실적이긴했어요,,,",
          "original_form": "현실적이긴했어요,,,",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028501.1.23",
          "form": "젠더폭력 관한 내용이라던데 함 읽어봐야겠어요",
          "original_form": "젠더폭력 관한 내용이라던데 함 읽어봐야겠어요",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028501.1.24",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028501.1.25",
          "form": "으악ㅋㅋ큐ㅜㅜ 현실적<벌써부터 각오를 다집니다...",
          "original_form": "으악ㅋㅋ큐ㅜㅜ 현실적<벌써부터 각오를 다집니다...",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028501.1.26",
          "form": "결말이 현실.. ㅋㅋㄱㅋ눈에보이는...!",
          "original_form": "결말이 현실.. ㅋㅋㄱㅋ눈에보이는...!",
          "speaker_id": "1",
          "time": "20210902 20:52"
        }
      ]
    }
