      "utterance": [
        {
          "id": "MDRW2100037393.1.1",
          "form": "뭐야 궁금해",
          "original_form": "뭐야 궁금해",
          "speaker_id": "2",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100037393.1.2",
          "form": "커뮤에는 참 별 게 다 올라와.",
          "original_form": "커뮤에는 참 별 게 다 올라와.",
          "speaker_id": "1",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100037393.1.3",
          "form": "엥 그게 무슨???",
          "original_form": "엥 그게 무슨???",
          "speaker_id": "2",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100037393.1.4",
          "form": "강아지 정수리가 보송보송한데 되게 귀엽더라",
          "original_form": "강아지 정수리가 보송보송한데 되게 귀엽더라",
          "speaker_id": "1",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100037393.1.5",
          "form": "영상이었어?",
          "original_form": "영상이었어?",
          "speaker_id": "2",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100037393.1.6",
          "form": "개인적으로 아기들보다 강아지가 훨 귀여워",
          "original_form": "개인적으로 아기들보다 강아지가 훨 귀여워",
          "speaker_id": "1",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100037393.1.7",
          "form": "아니면 사진?",
          "original_form": "아니면 사진?",
          "speaker_id": "2",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100037393.1.8",
          "form": "머리를 마구 쓰다듬고싶어",
          "original_form": "머리를 마구 쓰다듬고싶어",
          "speaker_id": "1",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100037393.1.9",
          "form": "사진@",
          "original_form": "사진@",
          "speaker_id": "1",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100037393.1.10",
          "form": "위에서 찍은 사진들이 쭉 올라와잇엇어",
          "original_form": "위에서 찍은 사진들이 쭉 올라와잇엇어",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100037393.1.11",
          "form": "머리를 쓰다듬으면 따뜻하고 부드러울 거 같아",
          "original_form": "머리를 쓰다듬으면 따뜻하고 부드러울 거 같아",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100037393.1.12",
          "form": "나는 강아지 발도 좋아해",
          "original_form": "나는 강아지 발도 좋아해",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100037393.1.13",
          "form": "꼬순내가 난다는데 맡아본적이 어1ㅅ어",
          "original_form": "꼬순내가 난다는데 맡아본적이 어1ㅅ어",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100037393.1.14",
          "form": "강아지 발냄새 맡아본적없어",
          "original_form": "강아지 발냄새 맡아본적없어",
          "speaker_id": "2",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100037393.1.15",
          "form": "다음에 같이 맡으러가자",
          "original_form": "다음에 같이 맡으러가자",
          "speaker_id": "2",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100037393.1.16",
          "form": "너는 고양이를 키우니까 좀 알겠지?",
          "original_form": "너는 고양이를 키우니까 좀 알겠지?",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100037393.1.17",
          "form": "음 좀 이상한데.. 좋아",
          "original_form": "음 좀 이상한데.. 좋아",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100037393.1.18",
          "form": "음 근데 고양이랑 다를 것 같아",
          "original_form": "음 근데 고양이랑 다를 것 같아",
          "speaker_id": "2",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100037393.1.19",
          "form": "고양이는 모래 냄새가나",
          "original_form": "고양이는 모래 냄새가나",
          "speaker_id": "2",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100037393.1.20",
          "form": "먼지냄새같은건가",
          "original_form": "먼지냄새같은건가",
          "speaker_id": "1",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100037393.1.21",
          "form": "약간 비슷해!",
          "original_form": "약간 비슷해!",
          "speaker_id": "2",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100037393.1.22",
          "form": "모래 냄새를 알아?",
          "original_form": "모래 냄새를 알아?",
          "speaker_id": "1",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100037393.1.23",
          "form": "너 고양이에대해 잘 아는구나",
          "original_form": "너 고양이에대해 잘 아는구나",
          "speaker_id": "2",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100037393.1.24",
          "form": "오 신기하다",
          "original_form": "오 신기하다",
          "speaker_id": "1",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100037393.1.25",
          "form": "응 당연하지",
          "original_form": "응 당연하지",
          "speaker_id": "2",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100037393.1.26",
          "form": "너희집가면 맡아볼래!",
          "original_form": "너희집가면 맡아볼래!",
          "speaker_id": "1",
          "time": "20210831 23:12"
        }
      ]
    }
  ]
