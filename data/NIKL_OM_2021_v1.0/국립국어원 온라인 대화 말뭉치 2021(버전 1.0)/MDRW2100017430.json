      "utterance": [
        {
          "id": "MDRW2100017430.1.1",
          "form": "안녕하세요 혹시 모죠의 일지 보시나요?",
          "original_form": "안녕하세요 혹시 모죠의 일지 보시나요?",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017430.1.2",
          "form": "헉 네네",
          "original_form": "헉 네네",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017430.1.3",
          "form": "모죠작가님 천재잖아요",
          "original_form": "모죠작가님 천재잖아요",
          "speaker_id": "2",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017430.1.4",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017430.1.5",
          "form": "혹시 알바 하시나요?",
          "original_form": "혹시 알바 하시나요?",
          "speaker_id": "2",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017430.1.6",
          "form": "모죠작가님 개그코드 완전 취저에요",
          "original_form": "모죠작가님 개그코드 완전 취저에요",
          "speaker_id": "1",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100017430.1.7",
          "form": "아뇨 알바 안하고있어요",
          "original_form": "아뇨 알바 안하고있어요",
          "speaker_id": "1",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100017430.1.8",
          "form": "오 그러면 방학때 ㅁ..앗 혹시 개강하신거는 아니시죠??",
          "original_form": "오 그러면 방학때 ㅁ..앗 혹시 개강하신거는 아니시죠??",
          "speaker_id": "2",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017430.1.9",
          "form": "방학때 뭐하고 계셨나유",
          "original_form": "방학때 뭐하고 계셨나유",
          "speaker_id": "2",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017430.1.10",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017430.1.11",
          "form": "곧 개강하긴하는데ㅠㅠ",
          "original_form": "곧 개강하긴하는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017430.1.12",
          "form": "방학때 열심히 폰게임하고 놀았어요ㅎㅎ",
          "original_form": "방학때 열심히 폰게임하고 놀았어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017430.1.13",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017430.1.14",
          "form": "알바하시나요?",
          "original_form": "알바하시나요?",
          "speaker_id": "1",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017430.1.15",
          "form": "네네 저는 빠바 알바해요",
          "original_form": "네네 저는 빠바 알바해요",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017430.1.16",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017430.1.17",
          "form": "만약 알바하믄 해보고싶었던 알바가 있나요?",
          "original_form": "만약 알바하믄 해보고싶었던 알바가 있나요?",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017430.1.18",
          "form": "엄... 해보고 싶었던 건 없는데 그나마 제 능력치로 해볼수 있지 않을까 싶은건 편의점..?",
          "original_form": "엄... 해보고 싶었던 건 없는데 그나마 제 능력치로 해볼수 있지 않을까 싶은건 편의점..?",
          "speaker_id": "1",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017430.1.19",
          "form": "계산...?",
          "original_form": "계산...?",
          "speaker_id": "1",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017430.1.20",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017430.1.21",
          "form": "빠바 알바 많이 힘드신가요ㅠ",
          "original_form": "빠바 알바 많이 힘드신가요ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017430.1.22",
          "form": "편의점 좋..좋죠(?)",
          "original_form": "편의점 좋..좋죠(?)",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017430.1.23",
          "form": "앗 아뇨 제가 일하는 빠바는 뭐 거의",
          "original_form": "앗 아뇨 제가 일하는 빠바는 뭐 거의",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017430.1.24",
          "form": "독서실입니다",
          "original_form": "독서실입니다",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017430.1.25",
          "form": "배달만 있고",
          "original_form": "배달만 있고",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017430.1.26",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017430.1.27",
          "form": "알바 처음에 구하실때",
          "original_form": "알바 처음에 구하실때",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017430.1.28",
          "form": "빠바로 알바해야겠다 생각하고 구하셨나요?",
          "original_form": "빠바로 알바해야겠다 생각하고 구하셨나요?",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017430.1.29",
          "form": "아뇨아뇨",
          "original_form": "아뇨아뇨",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017430.1.30",
          "form": "친구가 할래 이랬는데",
          "original_form": "친구가 할래 이랬는데",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017430.1.31",
          "form": "어쩌다보니 제가 하게됐어요",
          "original_form": "어쩌다보니 제가 하게됐어요",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017430.1.32",
          "form": "오잉~ㅋㅋㅋㅋ",
          "original_form": "오잉~ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017430.1.33",
          "form": "친구랑 같이 들어갔다가 친구는 관두고 저만 남았어요",
          "original_form": "친구랑 같이 들어갔다가 친구는 관두고 저만 남았어요",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017430.1.34",
          "form": "운명적인(?) 빠바와의 만남,,",
          "original_form": "운명적인(?) 빠바와의 만남,,",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017430.1.35",
          "form": "예스(?(",
          "original_form": "예스(?(",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017430.1.36",
          "form": "그래도 적성?에 맞으셨나요ㅎㅎㅎ",
          "original_form": "그래도 적성?에 맞으셨나요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017430.1.37",
          "form": "그리고 그 장학생같은거 해줘서",
          "original_form": "그리고 그 장학생같은거 해줘서",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017430.1.38",
          "form": "오래다녓어요",
          "original_form": "오래다녓어요",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017430.1.39",
          "form": "네네 어쩌다보니 적성에 맞더라고요?!",
          "original_form": "네네 어쩌다보니 적성에 맞더라고요?!",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017430.1.40",
          "form": "장학생도 되고 적성도 맞으시다니",
          "original_form": "장학생도 되고 적성도 맞으시다니",
          "speaker_id": "1",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017430.1.41",
          "form": "굿굿",
          "original_form": "굿굿",
          "speaker_id": "1",
