        {
          "id": "MDRW2100007274.1.3",
          "form": "뭐 샀어?",
          "original_form": "뭐 샀어?",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007274.1.4",
          "form": "아이스크림이랑 라면이랑 밀키트",
          "original_form": "아이스크림이랑 라면이랑 밀키트",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007274.1.5",
          "form": "오빠가 맛있다고했던 건면샀어",
          "original_form": "오빠가 맛있다고했던 건면샀어",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007274.1.6",
          "form": "짜파게티는?",
          "original_form": "짜파게티는?",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007274.1.7",
          "form": "짜파게티도샀징",
          "original_form": "짜파게티도샀징",
          "speaker_id": "1",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007274.1.8",
          "form": "티비볼륨좀낮춰줄랭",
          "original_form": "티비볼륨좀낮춰줄랭",
          "speaker_id": "1",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007274.1.9",
          "form": "비빔면은",
          "original_form": "비빔면은",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007274.1.10",
          "form": "비빔면은 안샀는뎅",
          "original_form": "비빔면은 안샀는뎅",
          "speaker_id": "1",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007274.1.11",
          "form": "배홍동 있는데",
          "original_form": "배홍동 있는데",
          "speaker_id": "2",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007274.1.12",
          "form": "배홍동 먹고싶어?",
