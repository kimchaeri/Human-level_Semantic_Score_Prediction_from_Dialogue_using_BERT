        },
        {
          "id": "MDRW2100000603.1.4",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000603.1.5",
          "form": "우리 싱크홀본거 어땠어?",
          "original_form": "우리 싱크홀본거 어땠어?",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000603.1.6",
          "form": "난 별로 재미없었어",
          "original_form": "난 별로 재미없었어",
          "speaker_id": "2",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000603.1.7",
          "form": "왜? 난 그냥 생각없이 보기에 괜찮았는데",
          "original_form": "왜? 난 그냥 생각없이 보기에 괜찮았는데",
          "speaker_id": "1",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000603.1.8",
          "form": "난쫌 지루했어. 모가디슈? 그거볼껄 그랬나봐",
          "original_form": "난쫌 지루했어. 모가디슈? 그거볼껄 그랬나봐",
          "speaker_id": "2",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000603.1.9",
          "form": "담주에 그거 봅시다~~~",
          "original_form": "담주에 그거 봅시다~~~",
          "speaker_id": "1",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000603.1.10",
          "form": "어디못가니 영화나보지뭐~",
          "original_form": "어디못가니 영화나보지뭐~",
          "speaker_id": "2",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000603.1.11",
          "form": "ㅎㅎ 황정민 나오는것도 하던데 그것도보고~~",
          "original_form": "ㅎㅎ 황정민 나오는것도 하던데 그것도보고~~",
          "speaker_id": "1",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000603.1.12",
          "form": "그래. 인질인가? 그거나보러가자",
          "original_form": "그래. 인질인가? 그거나보러가자",
          "speaker_id": "2",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000603.1.13",
          "form": "애들도 보여줘야하는데 딱히 볼께없어서...",
          "original_form": "애들도 보여줘야하는데 딱히 볼께없어서...",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000603.1.14",
          "form": "담에 재미있는거나옴 같이가지뭐",
          "original_form": "담에 재미있는거나옴 같이가지뭐",
          "speaker_id": "2",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000603.1.15",
          "form": "인질 15세면 같이갑시다~~",
          "original_form": "인질 15세면 같이갑시다~~",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000603.1.16",
          "form": "그래. 영화도보고 맛있는것도 먹고하지뭐",
          "original_form": "그래. 영화도보고 맛있는것도 먹고하지뭐",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000603.1.17",
          "form": "휴가도 못갔는데 이런거라도 해줘야지",
          "original_form": "휴가도 못갔는데 이런거라도 해줘야지",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000603.1.18",
          "form": "그래 코로나때매 애들한테 미안하다. 어디도못가고ㅠ",
          "original_form": "그래 코로나때매 애들한테 미안하다. 어디도못가고ㅠ",
          "speaker_id": "2",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000603.1.19",
          "form": "담에 좋은거보고 맛있는거먹고 많이데리고 다님되니 쫌만 기다려야지",
          "original_form": "담에 좋은거보고 맛있는거먹고 많이데리고 다님되니 쫌만 기다려야지",
          "speaker_id": "1",
          "time": "20210819 17:25"
        }
