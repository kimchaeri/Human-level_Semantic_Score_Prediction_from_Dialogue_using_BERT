      "utterance": [
        {
          "id": "MDRW2100022693.1.1",
          "form": "아이돌 노래 좋아하세요?",
          "original_form": "아이돌 노래 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 17:08"
        },
        {
          "id": "MDRW2100022693.1.2",
          "form": "아이돌노래 꽤 듣는편인거같아요 ㅋㅋㅋㅋ 좋아하는 아이돌 있으세요?",
          "original_form": "아이돌노래 꽤 듣는편인거같아요 ㅋㅋㅋㅋ 좋아하는 아이돌 있으세요?",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022693.1.3",
          "form": "오마이걸 좋아하고",
          "original_form": "오마이걸 좋아하고",
          "speaker_id": "1",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100022693.1.4",
          "form": "여돌노래는 즐겨들어요!",
          "original_form": "여돌노래는 즐겨들어요!",
          "speaker_id": "1",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100022693.1.5",
          "form": "누구 좋아하세여?",
          "original_form": "누구 좋아하세여?",
          "speaker_id": "1",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100022693.1.6",
          "form": "오마이걸요 ㅜㅜ",
          "original_form": "오마이걸요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 20:29"
        },
        {
          "id": "MDRW2100022693.1.7",
          "form": "씅씅 ㅜㅜ",
          "original_form": "씅씅 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 20:29"
        },
        {
          "id": "MDRW2100022693.1.8",
          "form": "앗ㅋㅋㅋ취향 많이 겹치네요",
          "original_form": "앗ㅋㅋㅋ취향 많이 겹치네요",
          "speaker_id": "1",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100022693.1.9",
          "form": "저는 좋아한지 얼마 안 돼서",
          "original_form": "저는 좋아한지 얼마 안 돼서",
          "speaker_id": "1",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100022693.1.10",
          "form": "엄청 잘 알지는 못해요",
          "original_form": "엄청 잘 알지는 못해요",
          "speaker_id": "1",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100022693.1.11",
          "form": "노래먼저 스며든타입ㅎㅎ",
          "original_form": "노래먼저 스며든타입ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100022693.1.12",
          "form": "저도 덕후까지는 아니고 노래챙겨듣고 그정도? ㅋㅋ",
          "original_form": "저도 덕후까지는 아니고 노래챙겨듣고 그정도? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100022693.1.13",
          "form": "비니너무예뻐요",
          "original_form": "비니너무예뻐요",
          "speaker_id": "2",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100022693.1.14",
          "form": "아 이번노래 춤도 너무 찰떡이고ㅜㅜ",
          "original_form": "아 이번노래 춤도 너무 찰떡이고ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100022693.1.15",
          "form": "이달의 소녀 아세요?",
          "original_form": "이달의 소녀 아세요?",
          "speaker_id": "2",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100022693.1.16",
          "form": "어.. 잘 몰라요",
          "original_form": "어.. 잘 몰라요",
          "speaker_id": "1",
          "time": "20210830 08:04"
        },
        {
          "id": "MDRW2100022693.1.17",
          "form": "위클리랑 스테이씨 알아요! 얼굴이랑 이름은 모르지맘",
          "original_form": "위클리랑 스테이씨 알아요! 얼굴이랑 이름은 모르지맘",
          "speaker_id": "1",
          "time": "20210830 08:05"
        },
        {
          "id": "MDRW2100022693.1.18",
          "form": "노래너무 좋아서ㅜㅜ",
          "original_form": "노래너무 좋아서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 08:05"
        },
        {
          "id": "MDRW2100022693.1.19",
          "form": "위클리 노래 들었어요! 그 약간 보드 타던?",
          "original_form": "위클리 노래 들었어요! 그 약간 보드 타던?",
          "speaker_id": "2",
          "time": "20210830 11:41"
        },
        {
          "id": "MDRW2100022693.1.20",
          "form": "오어맞아요!!!",
          "original_form": "오어맞아요!!!",
          "speaker_id": "1",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100022693.1.21",
          "form": "우리 스케이트 보드위로~마치 춤을추듯 발을 굴러~~~",
          "original_form": "우리 스케이트 보드위로~마치 춤을추듯 발을 굴러~~~",
          "speaker_id": "1",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100022693.1.22",
          "form": "진짜 요즘완전최애곡ㅜ",
          "original_form": "진짜 요즘완전최애곡ㅜ",
          "speaker_id": "1",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100022693.1.23",
          "form": "그 노래 진짜 좋더라구요 ㅜㅜ",
          "original_form": "그 노래 진짜 좋더라구요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100022693.1.24",
          "form": "없는추억이생기는...",
          "original_form": "없는추억이생기는...",
          "speaker_id": "2",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100022693.1.25",
          "form": "앜ㅋㅋㅋ여고나왔는데",
          "original_form": "앜ㅋㅋㅋ여고나왔는데",
          "speaker_id": "1",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100022693.1.26",
          "form": "남여공학 다닌 추억생기고ㅋㅋㅋㅋㅋ",
          "original_form": "남여공학 다닌 추억생기고ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:47"
        },
        {
          "id": "MDRW2100022693.1.27",
          "form": "하도 노래를 들어서 스케이트탈 수 있을거같아요 참나ㅋㅋㅋ",
          "original_form": "하도 노래를 들어서 스케이트탈 수 있을거같아요 참나ㅋㅋㅋ",
