      "utterance": [
        {
          "id": "MDRW2100023458.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210830 03:48"
        },
        {
          "id": "MDRW2100023458.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210830 03:48"
        },
        {
          "id": "MDRW2100023458.1.3",
          "form": "여행 계획 있으신가여",
          "original_form": "여행 계획 있으신가여",
          "speaker_id": "2",
          "time": "20210830 03:48"
        },
        {
          "id": "MDRW2100023458.1.4",
          "form": "아뇨 관심만 조금 있어요",
          "original_form": "아뇨 관심만 조금 있어요",
          "speaker_id": "1",
          "time": "20210830 03:49"
        },
        {
          "id": "MDRW2100023458.1.5",
          "form": "아 네 제주 여행겸 지내고 있어요",
          "original_form": "아 네 제주 여행겸 지내고 있어요",
          "speaker_id": "2",
          "time": "20210830 03:49"
        },
        {
          "id": "MDRW2100023458.1.6",
          "form": "숙소는 어디서 구하셨어요?",
          "original_form": "숙소는 어디서 구하셨어요?",
          "speaker_id": "1",
          "time": "20210830 03:49"
        },
        {
          "id": "MDRW2100023458.1.7",
          "form": "아무래도 코시국이라",
          "original_form": "아무래도 코시국이라",
          "speaker_id": "2",
          "time": "20210830 03:49"
        },
        {
          "id": "MDRW2100023458.1.8",
          "form": "저는 제주시에 있어요",
          "original_form": "저는 제주시에 있어요",
          "speaker_id": "2",
          "time": "20210830 03:49"
        },
        {
          "id": "MDRW2100023458.1.9",
          "form": "당근마켓에서",
          "original_form": "당근마켓에서",
          "speaker_id": "2",
          "time": "20210830 03:49"
        },
        {
          "id": "MDRW2100023458.1.10",
          "form": "연세 승계헸어요",
          "original_form": "연세 승계헸어요",
          "speaker_id": "2",
          "time": "20210830 03:49"
        },
        {
          "id": "MDRW2100023458.1.11",
          "form": "당근이 좋네요 ㅎㅎ",
          "original_form": "당근이 좋네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 03:50"
        },
        {
          "id": "MDRW2100023458.1.12",
          "form": "시세가 얼마쯤 해요?",
          "original_form": "시세가 얼마쯤 해요?",
          "speaker_id": "1",
          "time": "20210830 03:50"
        },
        {
          "id": "MDRW2100023458.1.13",
          "form": "서울이랑 비슷한 것 같은데",
          "original_form": "서울이랑 비슷한 것 같은데",
          "speaker_id": "2",
          "time": "20210830 03:50"
        },
        {
          "id": "MDRW2100023458.1.14",
          "form": "연세500-600선이예여",
          "original_form": "연세500-600선이예여",
          "speaker_id": "2",
          "time": "20210830 03:50"
        },
        {
          "id": "MDRW2100023458.1.15",
          "form": "제주하면 떠오르는 기념품 있으세요",
          "original_form": "제주하면 떠오르는 기념품 있으세요",
          "speaker_id": "2",
          "time": "20210830 03:51"
        },
        {
          "id": "MDRW2100023458.1.16",
          "form": "그렇군요 서울원룸이랑 비슷하네요",
          "original_form": "그렇군요 서울원룸이랑 비슷하네요",
          "speaker_id": "1",
          "time": "20210830 03:51"
        },
        {
          "id": "MDRW2100023458.1.17",
          "form": "하루방이요 ㅎㅎ",
          "original_form": "하루방이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 03:51"
        },
        {
          "id": "MDRW2100023458.1.18",
          "form": "감귤초콜렛이랑요",
          "original_form": "감귤초콜렛이랑요",
          "speaker_id": "1",
          "time": "20210830 03:51"
        },
        {
          "id": "MDRW2100023458.1.19",
          "form": "요즘엔 스타벅스 한정 굿즈나",
          "original_form": "요즘엔 스타벅스 한정 굿즈나",
          "speaker_id": "2",
          "time": "20210830 03:53"
        },
        {
          "id": "MDRW2100023458.1.20",
          "form": "제철과일 많이사시더라구요",
          "original_form": "제철과일 많이사시더라구요",
          "speaker_id": "2",
          "time": "20210830 03:53"
        },
        {
          "id": "MDRW2100023458.1.21",
          "form": "제주산 애플망고도 있어요",
          "original_form": "제주산 애플망고도 있어요",
          "speaker_id": "2",
          "time": "20210830 03:53"
        },
        {
          "id": "MDRW2100023458.1.22",
          "form": "아하 네 그거맛있어요?",
          "original_form": "아하 네 그거맛있어요?",
          "speaker_id": "1",
          "time": "20210830 03:53"
        },
        {
          "id": "MDRW2100023458.1.23",
          "form": "가격이 넘비싸던데요",
          "original_form": "가격이 넘비싸던데요",
          "speaker_id": "1",
          "time": "20210830 03:53"
        },
        {
          "id": "MDRW2100023458.1.24",
          "form": "비싸요",
          "original_form": "비싸요",
          "speaker_id": "2",
          "time": "20210830 03:54"
        },
        {
          "id": "MDRW2100023458.1.25",
          "form": "전 청귤나오길래",
          "original_form": "전 청귤나오길래",
          "speaker_id": "2",
          "time": "20210830 03:55"
        },
        {
          "id": "MDRW2100023458.1.26",
          "form": "청담아서 친구들 놀러오면 기념으로 줄려구요",
          "original_form": "청담아서 친구들 놀러오면 기념으로 줄려구요",
          "speaker_id": "2",
          "time": "20210830 03:55"
        }
      ]
    }
  ]
