      "utterance": [
        {
          "id": "MDRW2100023993.1.1",
          "form": "네가 최근에 읽은 책이 뭐야?",
          "original_form": "네가 최근에 읽은 책이 뭐야?",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023993.1.2",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023993.1.3",
          "form": "엇",
          "original_form": "엇",
          "speaker_id": "2",
          "time": "20210830 21:05"
        },
        {
          "id": "MDRW2100023993.1.4",
          "form": "책.....",
          "original_form": "책.....",
          "speaker_id": "2",
          "time": "20210830 21:05"
        },
        {
          "id": "MDRW2100023993.1.5",
          "form": "읽은지가 너무 오래되버렸다!",
          "original_form": "읽은지가 너무 오래되버렸다!",
          "speaker_id": "2",
          "time": "20210830 21:05"
        },
        {
          "id": "MDRW2100023993.1.6",
          "form": "넌 최근 읽은 책 있어?",
          "original_form": "넌 최근 읽은 책 있어?",
          "speaker_id": "2",
          "time": "20210830 21:05"
        },
        {
          "id": "MDRW2100023993.1.7",
          "form": "나 최근에 아토믹 헤빗이라는 책 읽고있어",
          "original_form": "나 최근에 아토믹 헤빗이라는 책 읽고있어",
          "speaker_id": "1",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100023993.1.8",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100023993.1.9",
          "form": "무슨 내용이야??",
          "original_form": "무슨 내용이야??",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100023993.1.10",
          "form": "습관 형성에 관련된 내용이야",
          "original_form": "습관 형성에 관련된 내용이야",
          "speaker_id": "1",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100023993.1.11",
          "form": "어떤 습관?",
          "original_form": "어떤 습관?",
          "speaker_id": "2",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100023993.1.12",
          "form": "아무습관!!",
          "original_form": "아무습관!!",
          "speaker_id": "1",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100023993.1.13",
          "form": "엇ㅋㅋㅋ 신기하다",
          "original_form": "엇ㅋㅋㅋ 신기하다",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023993.1.14",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023993.1.15",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023993.1.16",
          "form": "자신을 변화시키는 방법에 관한 책이지..",
          "original_form": "자신을 변화시키는 방법에 관한 책이지..",
          "speaker_id": "1",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023993.1.17",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023993.1.18",
          "form": "최근? 습관에 대한 책을 읽었어",
          "original_form": "최근? 습관에 대한 책을 읽었어",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023993.1.19",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "1",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023993.1.20",
          "form": "기억력에 대한 책이었지",
          "original_form": "기억력에 대한 책이었지",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023993.1.21",
          "form": "제목이",
          "original_form": "제목이",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023993.1.22",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023993.1.23",
          "form": "뭐였더라...",
          "original_form": "뭐였더라...",
          "speaker_id": "2",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100023993.1.24",
          "form": "무슨방밥으로 히는거야??",
          "original_form": "무슨방밥으로 히는거야??",
          "speaker_id": "1",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100023993.1.25",
          "form": "뇌가 섹시해지는 책",
          "original_form": "뇌가 섹시해지는 책",
          "speaker_id": "2",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100023993.1.26",
          "form": "요거였다",
          "original_form": "요거였다",
          "speaker_id": "2",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100023993.1.27",
          "form": "몇가지 방법이 있는데",
          "original_form": "몇가지 방법이 있는데",
          "speaker_id": "2",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100023993.1.28",
          "form": "크게 말하면",
          "original_form": "크게 말하면",
          "speaker_id": "2",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100023993.1.29",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100023993.1.30",
          "form": "묶어서 연상하고",
          "original_form": "묶어서 연상하고",
          "speaker_id": "2",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100023993.1.31",
          "form": "관련지어서 떠올릴 수 있는 다른 생각하고",
          "original_form": "관련지어서 떠올릴 수 있는 다른 생각하고",
          "speaker_id": "2",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100023993.1.32",
          "form": "결합해서",
          "original_form": "결합해서",
          "speaker_id": "2",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100023993.1.33",
          "form": "기억하는거였어!",
          "original_form": "기억하는거였어!",
          "speaker_id": "2",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100023993.1.34",
          "form": "나름 유용하더라고ㅎㅎ",
          "original_form": "나름 유용하더라고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100023993.1.35",
          "form": "오 앞 글자 따기 같은 느낌이다",
          "original_form": "오 앞 글자 따기 같은 느낌이다",
          "speaker_id": "1",
          "time": "20210830 21:11"
        }
      ]
    }
  ]
