      "utterance": [
        {
          "id": "MDRW2100032011.1.1",
          "form": "좋았던 여행지 있으세요?",
          "original_form": "좋았던 여행지 있으세요?",
          "speaker_id": "1",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100032011.1.2",
          "form": "저는 경주 제주 좋았어요!",
          "original_form": "저는 경주 제주 좋았어요!",
          "speaker_id": "2",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100032011.1.3",
          "form": "경주!야경 이쁜가요?",
          "original_form": "경주!야경 이쁜가요?",
          "speaker_id": "1",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100032011.1.4",
          "form": "안압지 밤에 보니까 좋더라구요",
          "original_form": "안압지 밤에 보니까 좋더라구요",
          "speaker_id": "2",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100032011.1.5",
          "form": "다른야경은 모르겠는데 안압지에서 본 야경이 이뻤어요",
          "original_form": "다른야경은 모르겠는데 안압지에서 본 야경이 이뻤어요",
          "speaker_id": "2",
          "time": "20210905 21:19"
        },
        {
          "id": "MDRW2100032011.1.6",
          "form": "ㅎㅎ보고싶어요",
          "original_form": "ㅎㅎ보고싶어요",
          "speaker_id": "1",
          "time": "20210905 21:21"
        },
        {
          "id": "MDRW2100032011.1.7",
          "form": "맛집도 많은가요오?",
          "original_form": "맛집도 많은가요오?",
          "speaker_id": "1",
          "time": "20210905 21:22"
        },
        {
          "id": "MDRW2100032011.1.8",
          "form": "맛집은....  거기서 먹은 떡갈비는 괜찮긴했어요",
          "original_form": "맛집은....  거기서 먹은 떡갈비는 괜찮긴했어요",
          "speaker_id": "2",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100032011.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100032011.1.10",
          "form": "떡갈비 기억할게요",
          "original_form": "떡갈비 기억할게요",
          "speaker_id": "1",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100032011.1.11",
          "form": "ㅋㅋㅋㅋㅋ두번왔어요",
          "original_form": "ㅋㅋㅋㅋㅋ두번왔어요",
          "speaker_id": "1",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100032011.1.12",
          "form": "강조강조",
          "original_form": "강조강조",
          "speaker_id": "1",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100032011.1.13",
          "form": "헐 두번갔나요? 한번 보냈는데 ㅜ",
          "original_form": "헐 두번갔나요? 한번 보냈는데 ㅜ",
          "speaker_id": "2",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100032011.1.14",
          "form": "앗 오륜가봐요",
          "original_form": "앗 오륜가봐요",
          "speaker_id": "1",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100032011.1.15",
          "form": "네 오류 잘 나더라구요 ㅎㅎ",
          "original_form": "네 오류 잘 나더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100032011.1.16",
          "form": "좋았던 여행지 있으셨나요?",
          "original_form": "좋았던 여행지 있으셨나요?",
          "speaker_id": "2",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100032011.1.17",
          "form": "저는 오스트리아 좋았어요!",
          "original_form": "저는 오스트리아 좋았어요!",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100032011.1.18",
          "form": "오 오스트리아! 거기는 뭐가 유명한가요?",
          "original_form": "오 오스트리아! 거기는 뭐가 유명한가요?",
          "speaker_id": "2",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100032011.1.19",
          "form": "음악회랑",
          "original_form": "음악회랑",
          "speaker_id": "1",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100032011.1.20",
          "form": "야경이랑",
          "original_form": "야경이랑",
          "speaker_id": "1",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100032011.1.21",
          "form": "음식도 맛나고",
          "original_form": "음식도 맛나고",
          "speaker_id": "1",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100032011.1.22",
          "form": "대체로 조용해요 ㅎㅎ",
          "original_form": "대체로 조용해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100032011.1.23",
          "form": "아 거기 클래식 유명하죠?",
          "original_form": "아 거기 클래식 유명하죠?",
          "speaker_id": "2",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100032011.1.24",
          "form": "예전에 음악시간에 작곡가 배우면 오스트리아 작곡가 많았던것 같아요",
          "original_form": "예전에 음악시간에 작곡가 배우면 오스트리아 작곡가 많았던것 같아요",
          "speaker_id": "2",
          "time": "20210905 21:32"
        }
      ]
    }
