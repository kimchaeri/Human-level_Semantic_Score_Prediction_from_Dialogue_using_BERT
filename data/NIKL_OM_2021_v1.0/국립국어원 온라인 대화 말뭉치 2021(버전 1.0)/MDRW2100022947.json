      "utterance": [
        {
          "id": "MDRW2100022947.1.1",
          "form": "응 난 눈 새하얗게 내리면 예뻐서 좋아해",
          "original_form": "응 난 눈 새하얗게 내리면 예뻐서 좋아해",
          "speaker_id": "2",
          "time": "20210829 19:27"
        },
        {
          "id": "MDRW2100022947.1.2",
          "form": "춥치않니?",
          "original_form": "춥치않니?",
          "speaker_id": "1",
          "time": "20210829 19:28"
        },
        {
          "id": "MDRW2100022947.1.3",
          "form": "손이 시려워서 싫던데",
          "original_form": "손이 시려워서 싫던데",
          "speaker_id": "1",
          "time": "20210829 19:28"
        },
        {
          "id": "MDRW2100022947.1.4",
          "form": "춥긴 해ㅎㅎㅎ 추위를 잘 타긴 하는데 풍경이 예뻐지는 게 좋은가봐",
          "original_form": "춥긴 해ㅎㅎㅎ 추위를 잘 타긴 하는데 풍경이 예뻐지는 게 좋은가봐",
          "speaker_id": "2",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100022947.1.5",
          "form": "나도 손 시려운 편인데",
          "original_form": "나도 손 시려운 편인데",
          "speaker_id": "2",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100022947.1.6",
          "form": "넌 몸이 차니?",
          "original_form": "넌 몸이 차니?",
          "speaker_id": "2",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100022947.1.7",
          "form": "응 몸이 금방 추워져서",
          "original_form": "응 몸이 금방 추워져서",
          "speaker_id": "1",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100022947.1.8",
          "form": "겨울엔 장판에서 살아",
          "original_form": "겨울엔 장판에서 살아",
          "speaker_id": "1",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100022947.1.9",
          "form": "와 장판 좋지 나도 그래ㅋㅋㅋㅋㅋ",
          "original_form": "와 장판 좋지 나도 그래ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100022947.1.10",
          "form": "나도 몸이 빨리 식어서",
          "original_form": "나도 몸이 빨리 식어서",
          "speaker_id": "2",
          "time": "20210829 19:30"
        },
        {
          "id": "MDRW2100022947.1.11",
          "form": "보온할 게 필요한 편이야",
          "original_form": "보온할 게 필요한 편이야",
          "speaker_id": "2",
          "time": "20210829 19:30"
        },
        {
          "id": "MDRW2100022947.1.12",
          "form": "겨울에는 집에 자주있니",
          "original_form": "겨울에는 집에 자주있니",
          "speaker_id": "1",
          "time": "20210829 19:31"
        },
        {
          "id": "MDRW2100022947.1.13",
          "form": "응 손발이 차서 그러는 편인데",
          "original_form": "응 손발이 차서 그러는 편인데",
          "speaker_id": "2",
          "time": "20210829 19:31"
        },
        {
          "id": "MDRW2100022947.1.14",
          "form": "사실 마음같아선 밖에 돌아다니고 싶어",
          "original_form": "사실 마음같아선 밖에 돌아다니고 싶어",
          "speaker_id": "2",
          "time": "20210829 19:31"
        },
        {
          "id": "MDRW2100022947.1.15",
          "form": "넌 겨울에 잘 안 돌아다녀?",
          "original_form": "넌 겨울에 잘 안 돌아다녀?",
          "speaker_id": "2",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100022947.1.16",
          "form": "응 나는 무조건 집에 박혀있어",
          "original_form": "응 나는 무조건 집에 박혀있어",
          "speaker_id": "1",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100022947.1.17",
          "form": "친구도 집으로 불러",
          "original_form": "친구도 집으로 불러",
          "speaker_id": "1",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100022947.1.18",
          "form": "오 친구도 부르다니",
          "original_form": "오 친구도 부르다니",
          "speaker_id": "2",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100022947.1.19",
          "form": "집에 크리스마스 트리 장식해본 적 있어?",
          "original_form": "집에 크리스마스 트리 장식해본 적 있어?",
          "speaker_id": "2",
          "time": "20210829 19:33"
        },
        {
          "id": "MDRW2100022947.1.20",
          "form": "아니 겨울을 기다리는 많은 이유겠지만",
          "original_form": "아니 겨울을 기다리는 많은 이유겠지만",
          "speaker_id": "1",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022947.1.21",
          "form": "나한텐그냥 휴일이야",
          "original_form": "나한텐그냥 휴일이야",
          "speaker_id": "1",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100022947.1.22",
          "form": "헐 그렇구나..",
          "original_form": "헐 그렇구나..",
          "speaker_id": "2",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100022947.1.23",
          "form": "난 겨울은 귤을 많이 까먹을 수 있어서 좋아해",
          "original_form": "난 겨울은 귤을 많이 까먹을 수 있어서 좋아해",
          "speaker_id": "2",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100022947.1.24",
          "form": "넌 겨울 좋아해?",
          "original_form": "넌 겨울 좋아해?",
          "speaker_id": "2",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100022947.1.25",
          "form": "겨울은 옷을 많이 입을수있어서",
          "original_form": "겨울은 옷을 많이 입을수있어서",
          "speaker_id": "1",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100022947.1.26",
          "form": "그건 너무좋아",
          "original_form": "그건 너무좋아",
          "speaker_id": "1",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100022947.1.27",
          "form": "헉 나도 겨울옷 좋아해",
          "original_form": "헉 나도 겨울옷 좋아해",
          "speaker_id": "2",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100022947.1.28",
          "form": "푹신푹신하잖니",
          "original_form": "푹신푹신하잖니",
          "speaker_id": "2",
          "time": "20210829 19:42"
        }
      ]
    }
  ]
