      "utterance": [
        {
          "id": "MDRW2100022589.1.1",
          "form": "데자와랑 팔공티꺼 먹어요!",
          "original_form": "데자와랑 팔공티꺼 먹어요!",
          "speaker_id": "2",
          "time": "20210829 16:04"
        },
        {
          "id": "MDRW2100022589.1.2",
          "form": "어...............데자와 호불호 심한...ㅎㅎ",
          "original_form": "어...............데자와 호불호 심한...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:05"
        },
        {
          "id": "MDRW2100022589.1.3",
          "form": "전 별로던데 그냥 그런 맛",
          "original_form": "전 별로던데 그냥 그런 맛",
          "speaker_id": "1",
          "time": "20210829 16:05"
        },
        {
          "id": "MDRW2100022589.1.4",
          "form": "저는 그게 첫 밀크티라 그런지 괜찮았어요!",
          "original_form": "저는 그게 첫 밀크티라 그런지 괜찮았어요!",
          "speaker_id": "2",
          "time": "20210829 16:05"
        },
        {
          "id": "MDRW2100022589.1.5",
          "form": "그래서 보이면 먹는 위주?",
          "original_form": "그래서 보이면 먹는 위주?",
          "speaker_id": "2",
          "time": "20210829 16:05"
        },
        {
          "id": "MDRW2100022589.1.6",
          "form": "팔공티 가성비 넘치죠ㅎㅎㅎ",
          "original_form": "팔공티 가성비 넘치죠ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:07"
        },
        {
          "id": "MDRW2100022589.1.7",
          "form": "근데 공차가 더 맛있지않아요?",
          "original_form": "근데 공차가 더 맛있지않아요?",
          "speaker_id": "1",
          "time": "20210829 16:07"
        },
        {
          "id": "MDRW2100022589.1.8",
          "form": "맛이 더 깊지 않아요?",
          "original_form": "맛이 더 깊지 않아요?",
          "speaker_id": "1",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100022589.1.9",
          "form": "공차도 맛있긴한데 가격이 비싸서",
          "original_form": "공차도 맛있긴한데 가격이 비싸서",
          "speaker_id": "2",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022589.1.10",
          "form": "가격싼거 2개먹는게 나은 편이에요 ㅎㅎ",
          "original_form": "가격싼거 2개먹는게 나은 편이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022589.1.11",
          "form": "공차가",
          "original_form": "공차가",
          "speaker_id": "1",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022589.1.12",
          "form": "펄이 더 쫀득쫀득하던데ㅎㅎ",
          "original_form": "펄이 더 쫀득쫀득하던데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022589.1.13",
          "form": "가끔 고급진맛을 먹고싶을땐",
          "original_form": "가끔 고급진맛을 먹고싶을땐",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022589.1.14",
          "form": "먹는편이에요!",
          "original_form": "먹는편이에요!",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022589.1.15",
          "form": "밀크티는 타로맛만 먹어요",
          "original_form": "밀크티는 타로맛만 먹어요",
          "speaker_id": "1",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022589.1.16",
          "form": "그게 달달하고 좋더라구요",
          "original_form": "그게 달달하고 좋더라구요",
          "speaker_id": "1",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022589.1.17",
          "form": "저는 블랙이랑 녹차랑 초코마셔요!",
          "original_form": "저는 블랙이랑 녹차랑 초코마셔요!",
          "speaker_id": "2",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022589.1.18",
          "form": "타로는 맛있긴한데 소화가 잘안되서 ....",
          "original_form": "타로는 맛있긴한데 소화가 잘안되서 ....",
          "speaker_id": "2",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022589.1.19",
          "form": "다양하게 맛보시네요ㅋㅋ",
          "original_form": "다양하게 맛보시네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022589.1.20",
          "form": "다양하게 먹는편이긴한데",
          "original_form": "다양하게 먹는편이긴한데",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022589.1.21",
          "form": "그렇게 다양하게 먹는건 아니에요!!",
          "original_form": "그렇게 다양하게 먹는건 아니에요!!",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022589.1.22",
          "form": "이디야는 안가세요?",
          "original_form": "이디야는 안가세요?",
          "speaker_id": "1",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022589.1.23",
          "form": "이디야는 꿀복숭아 먹어요!",
          "original_form": "이디야는 꿀복숭아 먹어요!",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100022589.1.24",
          "form": "녹차프라프치노도 가끔마시고요!",
          "original_form": "녹차프라프치노도 가끔마시고요!",
          "speaker_id": "2",
