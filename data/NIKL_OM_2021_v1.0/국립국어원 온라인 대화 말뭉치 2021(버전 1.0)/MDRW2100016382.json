      "utterance": [
        {
          "id": "MDRW2100016382.1.1",
          "form": "종교는 있으신가요?",
          "original_form": "종교는 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016382.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016382.1.3",
          "form": "정교 있으세요?",
          "original_form": "정교 있으세요?",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016382.1.4",
          "form": "종교",
          "original_form": "종교",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016382.1.5",
          "form": "네 저희 가족은 기독교인데",
          "original_form": "네 저희 가족은 기독교인데",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016382.1.6",
          "form": "너무 이상한 종교인이 많아요ㅠ",
          "original_form": "너무 이상한 종교인이 많아요ㅠ",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016382.1.7",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016382.1.8",
          "form": "저희동네",
          "original_form": "저희동네",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016382.1.9",
          "form": "심해요",
          "original_form": "심해요",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016382.1.10",
          "form": "지방이라 그런가",
          "original_form": "지방이라 그런가",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016382.1.11",
          "form": "그 도를 아십니까?",
          "original_form": "그 도를 아십니까?",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016382.1.12",
          "form": "그사람들 밤 9시까지 돌아다녀요",
          "original_form": "그사람들 밤 9시까지 돌아다녀요",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016382.1.13",
          "form": "으아",
          "original_form": "으아",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016382.1.14",
          "form": "왜 말을 거는지ㅠ",
          "original_form": "왜 말을 거는지ㅠ",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016382.1.15",
          "form": "코로나인데ㅡㄹ",
          "original_form": "코로나인데ㅡㄹ",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016382.1.16",
          "form": "저 맨날 잡아요",
          "original_form": "저 맨날 잡아요",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016382.1.17",
          "form": "참나 제가 좀 순하게 ㅋㅋ생겼어요",
          "original_form": "참나 제가 좀 순하게 ㅋㅋ생겼어요",
          "speaker_id": "2",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016382.1.18",
          "form": "만만해 보이나봐요",
          "original_form": "만만해 보이나봐요",
          "speaker_id": "2",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016382.1.19",
          "form": "진짜 코로나에 어이 없어요",
          "original_form": "진짜 코로나에 어이 없어요",
          "speaker_id": "2",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016382.1.20",
          "form": "요새 교회 비대면이시라 잘못가시죠?",
          "original_form": "요새 교회 비대면이시라 잘못가시죠?",
          "speaker_id": "2",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100016382.1.21",
          "form": "ㅎㅎ 인상이 좋으시다고 하죠?",
          "original_form": "ㅎㅎ 인상이 좋으시다고 하죠?",
          "speaker_id": "1",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016382.1.22",
          "form": "네 온라인으로만 해요",
          "original_form": "네 온라인으로만 해요",
          "speaker_id": "1",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016382.1.23",
          "form": "근데 일부교회가 코로나를 확대해서 그것도 참 그래요.",
          "original_form": "근데 일부교회가 코로나를 확대해서 그것도 참 그래요.",
          "speaker_id": "1",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016382.1.24",
          "form": "네 같이 아무래도 뭐라 질타를 받죠",
          "original_form": "네 같이 아무래도 뭐라 질타를 받죠",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016382.1.25",
          "form": "요샌 코로나로 종교 활동도 힘들어요",
          "original_form": "요샌 코로나로 종교 활동도 힘들어요",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016382.1.26",
          "form": "가족 모두 종교 없으시면",
          "original_form": "가족 모두 종교 없으시면",
          "speaker_id": "1",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016382.1.27",
          "form": "일요일 아침이 한가하시겠어요ㅎㅎ",
          "original_form": "일요일 아침이 한가하시겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016382.1.28",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016382.1.29",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016382.1.30",
          "form": "사실 일요일 때문에도 무교예요",
          "original_form": "사실 일요일 때문에도 무교예요",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016382.1.31",
          "form": "ㅋㅋ 일요일은",
          "original_form": "ㅋㅋ 일요일은",
          "speaker_id": "1",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016382.1.32",
          "form": "짜파게티",
          "original_form": "짜파게티",
          "speaker_id": "1",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016382.1.33",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016382.1.34",
          "form": "저희 시댁 둘째 서방님은 목사세요",
          "original_form": "저희 시댁 둘째 서방님은 목사세요",
          "speaker_id": "2",
          "time": "20210826 13:14"
        },
        {
          "id": "MDRW2100016382.1.35",
          "form": "와 그럼 시댁쪽은 기독교시려나?",
          "original_form": "와 그럼 시댁쪽은 기독교시려나?",
          "speaker_id": "1",
          "time": "20210826 13:14"
        },
        {
          "id": "MDRW2100016382.1.36",
          "form": "시댁 서방님들만요",
          "original_form": "시댁 서방님들만요",
          "speaker_id": "2",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016382.1.37",
          "form": "아버님은 무교요",
          "original_form": "아버님은 무교요",
          "speaker_id": "2",
          "time": "20210826 13:15"
        }
      ]
    }
