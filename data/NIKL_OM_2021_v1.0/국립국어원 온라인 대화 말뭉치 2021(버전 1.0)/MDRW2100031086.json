        {
          "id": "MDRW2100031086.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100031086.1.2",
          "form": "아니요 당신은요?",
          "original_form": "아니요 당신은요?",
          "speaker_id": "2",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100031086.1.3",
          "form": "구피요",
          "original_form": "구피요",
          "speaker_id": "1",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100031086.1.4",
          "form": "오 구피는 저도 키워본 적 있어요",
          "original_form": "오 구피는 저도 키워본 적 있어요",
          "speaker_id": "2",
          "time": "20210904 18:27"
        },
        {
          "id": "MDRW2100031086.1.5",
          "form": "그렇게 오래 키우진 못했지만...",
          "original_form": "그렇게 오래 키우진 못했지만...",
          "speaker_id": "2",
          "time": "20210904 18:27"
        },
        {
          "id": "MDRW2100031086.1.6",
          "form": "몇마리에요?",
          "original_form": "몇마리에요?",
          "speaker_id": "2",
          "time": "20210904 18:27"
        },
        {
          "id": "MDRW2100031086.1.7",
          "form": "열마리요",
          "original_form": "열마리요",
          "speaker_id": "1",
          "time": "20210904 18:56"
        },
        {
          "id": "MDRW2100031086.1.8",
          "form": "키우는거 어렵지 않아요?",
          "original_form": "키우는거 어렵지 않아요?",
          "speaker_id": "2",
          "time": "20210904 18:59"
        },
        {
          "id": "MDRW2100031086.1.9",
          "form": "물관리힘들잖아요",
          "original_form": "물관리힘들잖아요",
          "speaker_id": "1",
          "time": "20210904 19:02"
        },
        {
          "id": "MDRW2100031086.1.10",
          "form": "구피는 번식도 빠르다던데...그건 어때요",
          "original_form": "구피는 번식도 빠르다던데...그건 어때요",
          "speaker_id": "2",
          "time": "20210904 19:17"
        },
        {
          "id": "MDRW2100031086.1.11",
          "form": "많이 낳아여",
          "original_form": "많이 낳아여",
          "speaker_id": "1",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100031086.1.12",
          "form": "그러면 어떻게 해요? 다 키우나요?",
          "original_form": "그러면 어떻게 해요? 다 키우나요?",
          "speaker_id": "2",
          "time": "20210904 20:27"
        },
