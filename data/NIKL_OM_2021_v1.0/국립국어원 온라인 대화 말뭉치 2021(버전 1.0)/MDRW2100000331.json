      "utterance": [
        {
          "id": "MDRW2100000331.1.1",
          "form": "연봉은 다 세후로 계산해서",
          "original_form": "연봉은 다 세후로 계산해서",
          "speaker_id": "2",
          "time": "20210818 15:47"
        },
        {
          "id": "MDRW2100000331.1.2",
          "form": "세전은 추정치",
          "original_form": "세전은 추정치",
          "speaker_id": "2",
          "time": "20210818 15:47"
        },
        {
          "id": "MDRW2100000331.1.3",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210818 15:47"
        },
        {
          "id": "MDRW2100000331.1.4",
          "form": "의사들은 그렇게 받더라",
          "original_form": "의사들은 그렇게 받더라",
          "speaker_id": "1",
          "time": "20210818 15:47"
        },
        {
          "id": "MDRW2100000331.1.5",
          "form": "좋네",
          "original_form": "좋네",
          "speaker_id": "1",
          "time": "20210818 15:47"
        },
        {
          "id": "MDRW2100000331.1.6",
          "form": "전문직이 최고지",
          "original_form": "전문직이 최고지",
          "speaker_id": "1",
          "time": "20210818 15:47"
        },
        {
          "id": "MDRW2100000331.1.7",
          "form": "정년도 없고",
          "original_form": "정년도 없고",
          "speaker_id": "1",
          "time": "20210818 15:48"
        },
        {
          "id": "MDRW2100000331.1.8",
          "form": "치과가 노다지",
          "original_form": "치과가 노다지",
          "speaker_id": "2",
          "time": "20210818 15:48"
        },
        {
          "id": "MDRW2100000331.1.9",
          "form": "그럴듯",
          "original_form": "그럴듯",
          "speaker_id": "1",
          "time": "20210818 15:48"
        },
        {
          "id": "MDRW2100000331.1.10",
          "form": "의사도 노다지",
          "original_form": "의사도 노다지",
          "speaker_id": "1",
          "time": "20210818 15:48"
        },
        {
          "id": "MDRW2100000331.1.11",
          "form": "산부인과는 일이 없어져",
          "original_form": "산부인과는 일이 없어져",
          "speaker_id": "1",
          "time": "20210818 15:48"
        },
        {
          "id": "MDRW2100000331.1.12",
          "form": "소아과도",
          "original_form": "소아과도",
          "speaker_id": "1",
          "time": "20210818 15:48"
        },
        {
          "id": "MDRW2100000331.1.13",
          "form": "이놈들 빅데이터 정리할 능력이 되나?",
          "original_form": "이놈들 빅데이터 정리할 능력이 되나?",
          "speaker_id": "2",
          "time": "20210818 15:48"
        },
        {
          "id": "MDRW2100000331.1.14",
          "form": "치과가 갑이다",
          "original_form": "치과가 갑이다",
          "speaker_id": "2",
          "time": "20210818 15:49"
        },
        {
          "id": "MDRW2100000331.1.15",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210818 15:49"
        },
        {
          "id": "MDRW2100000331.1.16",
          "form": "의료사고 위험도 적고",
          "original_form": "의료사고 위험도 적고",
          "speaker_id": "2",
          "time": "20210818 15:49"
        },
        {
          "id": "MDRW2100000331.1.17",
          "form": "페이도 더 좋은듯",
          "original_form": "페이도 더 좋은듯",
          "speaker_id": "2",
          "time": "20210818 15:49"
        },
        {
          "id": "MDRW2100000331.1.18",
          "form": "난이도도 상대적으로",
          "original_form": "난이도도 상대적으로",
          "speaker_id": "2",
          "time": "20210818 15:50"
        },
        {
          "id": "MDRW2100000331.1.19",
          "form": "음 그렇군",
          "original_form": "음 그렇군",
          "speaker_id": "1",
          "time": "20210818 15:50"
        },
        {
          "id": "MDRW2100000331.1.20",
          "form": "아 내과 종류도 좋겠네",
          "original_form": "아 내과 종류도 좋겠네",
          "speaker_id": "2",
          "time": "20210818 15:50"
        },
        {
          "id": "MDRW2100000331.1.21",
          "form": "와이프 사촌동생이",
          "original_form": "와이프 사촌동생이",
          "speaker_id": "1",
          "time": "20210818 15:50"
        },
        {
          "id": "MDRW2100000331.1.22",
          "form": "의사집으로 시집갔는데",
          "original_form": "의사집으로 시집갔는데",
          "speaker_id": "1",
          "time": "20210818 15:51"
        },
        {
          "id": "MDRW2100000331.1.23",
          "form": "엄청나더라",
          "original_form": "엄청나더라",
          "speaker_id": "1",
          "time": "20210818 15:51"
        },
        {
          "id": "MDRW2100000331.1.24",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210818 15:51"
        },
        {
          "id": "MDRW2100000331.1.25",
          "form": "취집!",
          "original_form": "취집!",
          "speaker_id": "2",
          "time": "20210818 15:51"
        },
        {
          "id": "MDRW2100000331.1.26",
          "form": "ㅇㅇ 시아버지가",
          "original_form": "ㅇㅇ 시아버지가",
          "speaker_id": "1",
          "time": "20210818 15:53"
        },
        {
          "id": "MDRW2100000331.1.27",
          "form": "해운대서 개인병원하는데",
          "original_form": "해운대서 개인병원하는데",
          "speaker_id": "1",
          "time": "20210818 15:53"
        },
        {
          "id": "MDRW2100000331.1.28",
          "form": "재산이 엄청난게벼",
          "original_form": "재산이 엄청난게벼",
          "speaker_id": "1",
          "time": "20210818 15:53"
        },
        {
          "id": "MDRW2100000331.1.29",
          "form": "위치도 좋네",
          "original_form": "위치도 좋네",
          "speaker_id": "2",
          "time": "20210818 15:53"
        },
        {
          "id": "MDRW2100000331.1.30",
          "form": "응응",
