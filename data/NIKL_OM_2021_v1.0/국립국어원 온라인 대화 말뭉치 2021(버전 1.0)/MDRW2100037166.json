      "utterance": [
        {
          "id": "MDRW2100037166.1.1",
          "form": "이걸 홍콩 여가수가 다르게 부름",
          "original_form": "이걸 홍콩 여가수가 다르게 부름",
          "speaker_id": "2",
          "time": "20210831 21:10"
        },
        {
          "id": "MDRW2100037166.1.2",
          "form": "그 노래 좋아요ㅋ 어떻게요? ㅋㅋ",
          "original_form": "그 노래 좋아요ㅋ 어떻게요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100037166.1.3",
          "form": "파 워 풀 하게 불러서 데뷔하자마자 큰 인기를 얻었다네",
          "original_form": "파 워 풀 하게 불러서 데뷔하자마자 큰 인기를 얻었다네",
          "speaker_id": "2",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100037166.1.4",
          "form": "조용한 노래를 파워풀하게 불렀구나",
          "original_form": "조용한 노래를 파워풀하게 불렀구나",
          "speaker_id": "1",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100037166.1.5",
          "form": "클론 ㄴ노래도 번안했네",
          "original_form": "클론 ㄴ노래도 번안했네",
          "speaker_id": "2",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100037166.1.6",
          "form": "클론은 대만에서 인기 많았던데요",
          "original_form": "클론은 대만에서 인기 많았던데요",
          "speaker_id": "1",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100037166.1.7",
          "form": "중국의 소방차라고 불리는 남성 그룹 초맹이",
          "original_form": "중국의 소방차라고 불리는 남성 그룹 초맹이",
          "speaker_id": "2",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100037166.1.8",
          "form": "초맹이 ㅋㅋ 이름이 왜 그래요?",
          "original_form": "초맹이 ㅋㅋ 이름이 왜 그래요?",
          "speaker_id": "1",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100037166.1.9",
          "form": "쿵따리 샤바라를 가사만 살짝 달리해서",
          "original_form": "쿵따리 샤바라를 가사만 살짝 달리해서",
          "speaker_id": "2",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100037166.1.10",
          "form": "그 노래는 우리나라에서 엄청 히트했죠",
          "original_form": "그 노래는 우리나라에서 엄청 히트했죠",
          "speaker_id": "1",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100037166.1.11",
          "form": "그러게 초맹 ㅋㅋ 심지어 제2의 전성기를 맞았단다",
          "original_form": "그러게 초맹 ㅋㅋ 심지어 제2의 전성기를 맞았단다",
          "speaker_id": "2",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100037166.1.12",
          "form": "오 진짜요?  클론한테 절해야겠다",
          "original_form": "오 진짜요?  클론한테 절해야겠다",
          "speaker_id": "1",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100037166.1.13",
          "form": "한동안 다 클론 노래였지",
          "original_form": "한동안 다 클론 노래였지",
          "speaker_id": "2",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100037166.1.14",
          "form": "그쵸. 엄청 났죠",
          "original_form": "그쵸. 엄청 났죠",
