      "utterance": [
        {
          "id": "MDRW2100017596.1.1",
          "form": "장지갑이네??",
          "original_form": "장지갑이네??",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017596.1.2",
          "form": "웅웅~~",
          "original_form": "웅웅~~",
          "speaker_id": "1",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017596.1.3",
          "form": "외피 내피 다 기스 나지 않는",
          "original_form": "외피 내피 다 기스 나지 않는",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017596.1.4",
          "form": "탄탄한 거라던데~~",
          "original_form": "탄탄한 거라던데~~",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017596.1.5",
          "form": "검은색으로도 있어요~~",
          "original_form": "검은색으로도 있어요~~",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017596.1.6",
          "form": "다른색은??",
          "original_form": "다른색은??",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017596.1.7",
          "form": "빨간색이 내 나이때 쓰기 괜찮을 거 같기도 하고~~",
          "original_form": "빨간색이 내 나이때 쓰기 괜찮을 거 같기도 하고~~",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017596.1.8",
          "form": "어떤 거 같아요?",
          "original_form": "어떤 거 같아요?",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017596.1.9",
          "form": "글쎄??",
          "original_form": "글쎄??",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017596.1.10",
          "form": "딸은 뭐가 맘에 드는데??",
          "original_form": "딸은 뭐가 맘에 드는데??",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017596.1.11",
          "form": "나는 무난한게 좋긴하지~",
          "original_form": "나는 무난한게 좋긴하지~",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017596.1.12",
          "form": "가죽은??",
          "original_form": "가죽은??",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017596.1.13",
          "form": "소가죽이래요~~",
          "original_form": "소가죽이래요~~",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017596.1.14",
          "form": "천연이야??",
          "original_form": "천연이야??",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017596.1.15",
          "form": "웅웅~~ 그래서 좀 더 비싸긴 해요~~ㅋㅋ",
          "original_form": "웅웅~~ 그래서 좀 더 비싸긴 해요~~ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017596.1.16",
          "form": "탄탄해??",
          "original_form": "탄탄해??",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017596.1.17",
          "form": "직원 말로는 외피는 기스가 잘 아는데",
          "original_form": "직원 말로는 외피는 기스가 잘 아는데",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017596.1.18",
          "form": "내피는 기스가 좀 나는 일반 가죽이래요~",
          "original_form": "내피는 기스가 좀 나는 일반 가죽이래요~",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017596.1.19",
          "form": "둘 다 탄탄한 건??",
          "original_form": "둘 다 탄탄한 건??",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017596.1.20",
          "form": "근데 고건 좀 안 이쁜듯,,,",
          "original_form": "근데 고건 좀 안 이쁜듯,,,",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017596.1.21",
          "form": "이건 어떤거 같아요?",
          "original_form": "이건 어떤거 같아요?",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017596.1.22",
          "form": "그냥 무난해보여",
          "original_form": "그냥 무난해보여",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017596.1.23",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017596.1.24",
          "form": "3번째 빨간거",
          "original_form": "3번째 빨간거",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017596.1.25",
