          "form": "ㅇㅏ",
          "original_form": "ㅇㅏ",
          "speaker_id": "2",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100041114.1.4",
          "form": "타타라 모노가타리 하다가",
          "original_form": "타타라 모노가타리 하다가",
          "speaker_id": "2",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100041114.1.5",
          "form": "껐어",
          "original_form": "껐어",
          "speaker_id": "2",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100041114.1.6",
          "form": "그거 아직도 하고잇어???",
          "original_form": "그거 아직도 하고잇어???",
          "speaker_id": "1",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100041114.1.7",
          "form": "진짜 짜증난다",
          "original_form": "진짜 짜증난다",
          "speaker_id": "2",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100041114.1.8",
          "form": "오늘이 마지막인데",
          "original_form": "오늘이 마지막인데",
          "speaker_id": "2",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100041114.1.9",
          "form": "우인단 하나먼 더죽이면 끝인데",
          "original_form": "우인단 하나먼 더죽이면 끝인데",
          "speaker_id": "2",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100041114.1.10",
          "form": "걔가 장치를 부쉈어",
          "original_form": "걔가 장치를 부쉈어",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100041114.1.11",
          "form": "악",
          "original_form": "악",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100041114.1.12",
          "form": "저런....",
          "original_form": "저런....",
          "speaker_id": "1",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100041114.1.13",
          "form": "우인단 진짜 짜증남",
          "original_form": "우인단 진짜 짜증남",
          "speaker_id": "1",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100041114.1.14",
          "form": "아니 왜 자기들끼리",
          "original_form": "아니 왜 자기들끼리",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100041114.1.15",
          "form": "조합맞춰와?",
          "original_form": "조합맞춰와?",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100041114.1.16",
          "form": "아 그리고 스카라무슈가",
          "original_form": "아 그리고 스카라무슈가",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100041114.1.17",
          "form": "라이덴이 만든거였어???",
          "original_form": "라이덴이 만든거였어???",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041114.1.18",
          "form": "ㅇㅇㅋㅋㅋㅋ 그렇대",
          "original_form": "ㅇㅇㅋㅋㅋㅋ 그렇대",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041114.1.19",
          "form": "나 그 트레일러 보고",
          "original_form": "나 그 트레일러 보고",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041114.1.20",
          "form": "사실 이것도 성유물에 스토리 있긴 함..",
          "original_form": "사실 이것도 성유물에 스토리 있긴 함..",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041114.1.21",
          "form": "얼탱이없어서",
          "original_form": "얼탱이없어서",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041114.1.22",
          "form": "야에 미코랑",
          "original_form": "야에 미코랑",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041114.1.23",
          "form": "대화 안하고",
          "original_form": "대화 안하고",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041114.1.24",
          "form": "걍 꺼버렸는데",
          "original_form": "걍 꺼버렸는데",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041114.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041114.1.26",
          "form": "아니 그리고",
          "original_form": "아니 그리고",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041114.1.27",
          "form": "쌍둥이 신이었다고???",
          "original_form": "쌍둥이 신이었다고???",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041114.1.28",
          "form": "장난??",
          "original_form": "장난??",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041114.1.29",
          "form": "ㅋㅋㅋㅋ그니까 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ그니까 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041114.1.30",
          "form": "라이덴이 대체",
          "original_form": "라이덴이 대체",
          "speaker_id": "2",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041114.1.31",
          "form": "이게 뭔....",
          "original_form": "이게 뭔....",
          "speaker_id": "1",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041114.1.32",
          "form": "뭔소리하는건가 했더니",
          "original_form": "뭔소리하는건가 했더니",
          "speaker_id": "2",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041114.1.33",
          "form": "ㅎ어쩐지",
          "original_form": "ㅎ어쩐지",
          "speaker_id": "2",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041114.1.34",
          "form": "이름이 에이더라",
          "original_form": "이름이 에이더라",
          "speaker_id": "2",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041114.1.35",
          "form": "나머지 하나는 엔이겠네",
          "original_form": "나머지 하나는 엔이겠네",
          "speaker_id": "2",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041114.1.36",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "2",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041114.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041114.1.38",
          "form": "근데 일본어로 영원이 뭐야?",
          "original_form": "근데 일본어로 영원이 뭐야?",
          "speaker_id": "1",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041114.1.39",
          "form": "에이엔ㅋ",
          "original_form": "에이엔ㅋ",
          "speaker_id": "2",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041114.1.40",
          "form": "ㅇㅎㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅎㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041114.1.41",
          "form": "어쩐지",
          "original_form": "어쩐지",
          "speaker_id": "1",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041114.1.42",
          "form": "영원 얘기할 때마다",
          "original_form": "영원 얘기할 때마다",
          "speaker_id": "1",
          "time": "20210905 21:04"
        },
        {
          "id": "MDRW2100041114.1.43",
          "form": "이름이 들리는거같은거야 ㅋㅋㅋ",
          "original_form": "이름이 들리는거같은거야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:04"
        },
        {
          "id": "MDRW2100041114.1.44",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래서",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래서",
          "speaker_id": "2",
          "time": "20210905 21:04"
        },
        {
