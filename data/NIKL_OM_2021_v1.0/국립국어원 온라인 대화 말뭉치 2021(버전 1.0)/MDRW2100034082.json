      "utterance": [
        {
          "id": "MDRW2100034082.1.1",
          "form": "공연 좋아하세요",
          "original_form": "공연 좋아하세요",
          "speaker_id": "2",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100034082.1.2",
          "form": "공연좋아하시나요?",
          "original_form": "공연좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100034082.1.3",
          "form": "잘보러다니지는 못하구요 전시회 좋아해요",
          "original_form": "잘보러다니지는 못하구요 전시회 좋아해요",
          "speaker_id": "1",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100034082.1.4",
          "form": "뮤지컬이나 공연 보신적있나요?",
          "original_form": "뮤지컬이나 공연 보신적있나요?",
          "speaker_id": "1",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100034082.1.5",
          "form": "저는 지방 살아서 잘 보지는 못해요",
          "original_form": "저는 지방 살아서 잘 보지는 못해요",
          "speaker_id": "2",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100034082.1.6",
          "form": "제일 재밌게 본 것은 뮤지컬 빨래요!",
          "original_form": "제일 재밌게 본 것은 뮤지컬 빨래요!",
          "speaker_id": "2",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100034082.1.7",
          "form": "빨래에서 기억에 남는 배우 있으세요?",
          "original_form": "빨래에서 기억에 남는 배우 있으세요?",
          "speaker_id": "1",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100034082.1.8",
          "form": "정말 옛날에 봐서 배우는 기억 안나요",
          "original_form": "정말 옛날에 봐서 배우는 기억 안나요",
          "speaker_id": "2",
          "time": "20210827 15:22"
        },
        {
          "id": "MDRW2100034082.1.9",
          "form": "하지만 작은 공연장에서 서울살이에 대한 힘듦과 위로에 대한 내용이 정말 좋았던게 기억에 남아요",
          "original_form": "하지만 작은 공연장에서 서울살이에 대한 힘듦과 위로에 대한 내용이 정말 좋았던게 기억에 남아요",
          "speaker_id": "2",
          "time": "20210827 15:23"
        },
        {
          "id": "MDRW2100034082.1.10",
          "form": "내용이 좋네요...",
          "original_form": "내용이 좋네요...",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100034082.1.11",
          "form": "공감되는부분도 많을거 같구요",
          "original_form": "공감되는부분도 많을거 같구요",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100034082.1.12",
          "form": "서울에 살진 않지만 삶의 외로움같은 부분에서 공감되더라고요",
          "original_form": "서울에 살진 않지만 삶의 외로움같은 부분에서 공감되더라고요",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100034082.1.13",
          "form": "마냥 우울하진 않고 유쾌한 부분도 많은 뮤지컬이에요 ㅋㅋ",
          "original_form": "마냥 우울하진 않고 유쾌한 부분도 많은 뮤지컬이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100034082.1.14",
          "form": "그렇구나 기회되면 넘버 영상이라도 보고싶네요",
          "original_form": "그렇구나 기회되면 넘버 영상이라도 보고싶네요",
          "speaker_id": "1",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100034082.1.15",
          "form": "방금 찾아봤는데 유튜브에 나오네요!",
          "original_form": "방금 찾아봤는데 유튜브에 나오네요!",
          "speaker_id": "2",
          "time": "20210827 16:05"
        },
        {
          "id": "MDRW2100034082.1.16",
          "form": "어떤 전시회를 주로 가세요?",
          "original_form": "어떤 전시회를 주로 가세요?",
          "speaker_id": "2",
          "time": "20210827 16:13"
        },
        {
          "id": "MDRW2100034082.1.17",
          "form": "헉 그렇구나 찾아볼게요 감사해요",
          "original_form": "헉 그렇구나 찾아볼게요 감사해요",
          "speaker_id": "1",
          "time": "20210827 16:13"
        },
        {
          "id": "MDRW2100034082.1.18",
          "form": "그렇게 자주 가는건 아닌데요 체험형식이나",
          "original_form": "그렇게 자주 가는건 아닌데요 체험형식이나",
          "speaker_id": "1",
          "time": "20210827 16:13"
        },
        {
          "id": "MDRW2100034082.1.19",
          "form": "사진전을 주로 가요",
          "original_form": "사진전을 주로 가요",
          "speaker_id": "1",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100034082.1.20",
          "form": "체험형식의 전시회가 어떤건가요?",
          "original_form": "체험형식의 전시회가 어떤건가요?",
          "speaker_id": "2",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100034082.1.21",
          "form": "음 예를들자면",
          "original_form": "음 예를들자면",
          "speaker_id": "1",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100034082.1.22",
          "form": "기억에 남는 전시는",
          "original_form": "기억에 남는 전시는",
          "speaker_id": "1",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100034082.1.23",
          "form": "레인룸이였는데요",
          "original_form": "레인룸이였는데요",
          "speaker_id": "1",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100034082.1.24",
          "form": "비를 안젖으면서 맞아볼수있는 형태의 전시였어요",
          "original_form": "비를 안젖으면서 맞아볼수있는 형태의 전시였어요",
          "speaker_id": "1",
          "time": "20210827 16:17"
        }
      ]
