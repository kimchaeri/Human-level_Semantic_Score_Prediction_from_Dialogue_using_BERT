        },
        {
          "id": "MDRW2100003805.1.2",
          "form": "된장찌개 다",
          "original_form": "된장찌개 다",
          "speaker_id": "1",
          "time": "20210821 12:20"
        },
        {
          "id": "MDRW2100003805.1.3",
          "form": "맛있겠네",
          "original_form": "맛있겠네",
          "speaker_id": "2",
          "time": "20210821 12:20"
        },
        {
          "id": "MDRW2100003805.1.4",
          "form": "당연하지 내가 하는 건 다 맛있다",
          "original_form": "당연하지 내가 하는 건 다 맛있다",
          "speaker_id": "1",
          "time": "20210821 12:20"
        },
        {
          "id": "MDRW2100003805.1.5",
          "form": "그래 우리 와이프가 하는 건 다 맛있지",
          "original_form": "그래 우리 와이프가 하는 건 다 맛있지",
          "speaker_id": "2",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003805.1.6",
          "form": "반찬도 많이 먹어요",
          "original_form": "반찬도 많이 먹어요",
          "speaker_id": "1",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003805.1.7",
          "form": "알았다",
          "original_form": "알았다",
          "speaker_id": "2",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003805.1.8",
          "form": "밥은 너무 많이 먹지 말고",
          "original_form": "밥은 너무 많이 먹지 말고",
          "speaker_id": "1",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003805.1.9",
          "form": "찌개가 맛있는데 밥 많이 먹어야지",
          "original_form": "찌개가 맛있는데 밥 많이 먹어야지",
          "speaker_id": "2",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003805.1.10",
          "form": "적당히 먹어야지",
          "original_form": "적당히 먹어야지",
          "speaker_id": "1",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003805.1.11",
          "form": "찌개 맛이 좋네",
          "original_form": "찌개 맛이 좋네",
          "speaker_id": "2",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003805.1.12",
          "form": "몸에 좋은 것 많이 넣었다",
          "original_form": "몸에 좋은 것 많이 넣었다",
          "speaker_id": "1",
          "time": "20210821 12:22"
