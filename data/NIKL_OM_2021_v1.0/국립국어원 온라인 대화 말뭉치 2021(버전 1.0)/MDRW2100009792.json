      "utterance": [
        {
          "id": "MDRW2100009792.1.1",
          "form": "동호회 활동 해보신적 있으세요?",
          "original_form": "동호회 활동 해보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210823 10:04"
        },
        {
          "id": "MDRW2100009792.1.2",
          "form": "음 오프라인은 최근엔 없네요",
          "original_form": "음 오프라인은 최근엔 없네요",
          "speaker_id": "2",
          "time": "20210823 10:20"
        },
        {
          "id": "MDRW2100009792.1.3",
          "form": "고등학교 때나 대학 때 이후로는 ㄷㄷ",
          "original_form": "고등학교 때나 대학 때 이후로는 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210823 10:20"
        },
        {
          "id": "MDRW2100009792.1.4",
          "form": "저도 대학때가 마지막이었네요:)",
          "original_form": "저도 대학때가 마지막이었네요:)",
          "speaker_id": "1",
          "time": "20210823 10:21"
        },
        {
          "id": "MDRW2100009792.1.5",
          "form": "대학때는 어떤 동아리셨어요?",
          "original_form": "대학때는 어떤 동아리셨어요?",
          "speaker_id": "1",
          "time": "20210823 10:24"
        },
        {
          "id": "MDRW2100009792.1.6",
          "form": "교육봉사라고 해야하나요",
          "original_form": "교육봉사라고 해야하나요",
          "speaker_id": "2",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100009792.1.7",
          "form": "저소득층 아이들 가르치는 일 했었어요ㅎ",
          "original_form": "저소득층 아이들 가르치는 일 했었어요ㅎ",
          "speaker_id": "2",
          "time": "20210823 10:51"
        },
        {
          "id": "MDRW2100009792.1.8",
          "form": "우왕 멋져요bb",
          "original_form": "우왕 멋져요bb",
          "speaker_id": "1",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100009792.1.9",
          "form": "그때 만났던 아이들도\r\n이제 성인이 되었겠죠?",
          "original_form": "그때 만났던 아이들도\r\n이제 성인이 되었겠죠?",
          "speaker_id": "1",
          "time": "20210823 10:57"
        },
        {
          "id": "MDRW2100009792.1.10",
          "form": "딱히 동아리라기보다 제가 그냥 사범대나와서",
          "original_form": "딱히 동아리라기보다 제가 그냥 사범대나와서",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009792.1.11",
          "form": "다같이 과에서 하는 그런 활동이었어요",
          "original_form": "다같이 과에서 하는 그런 활동이었어요",
          "speaker_id": "2",
          "time": "20210823 11:01"
        },
        {
          "id": "MDRW2100009792.1.12",
          "form": "참 20대때가 좋았죠 씁쓸",
          "original_form": "참 20대때가 좋았죠 씁쓸",
          "speaker_id": "2",
          "time": "20210823 11:01"
        },
        {
          "id": "MDRW2100009792.1.13",
          "form": "과에서 하는 활동이어도\r\n선한 영향력을 끼치는\r\n좋은 활동을 하셨었네요:)",
          "original_form": "과에서 하는 활동이어도\r\n선한 영향력을 끼치는\r\n좋은 활동을 하셨었네요:)",
          "speaker_id": "1",
          "time": "20210823 11:14"
        },
        {
          "id": "MDRW2100009792.1.14",
          "form": "저는 학교다닐때 가산점 받으려고\r\n봉사활동 했던거 외에는",
          "original_form": "저는 학교다닐때 가산점 받으려고\r\n봉사활동 했던거 외에는",
          "speaker_id": "1",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009792.1.15",
          "form": "그외에는 얼마전에 다리밑에서",
          "original_form": "그외에는 얼마전에 다리밑에서",
          "speaker_id": "2",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009792.1.16",
          "form": "그 중국에서하는 체조같은거 있죠",
          "original_form": "그 중국에서하는 체조같은거 있죠",
          "speaker_id": "2",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009792.1.17",
          "form": "그거 이름이 뭐더라",
          "original_form": "그거 이름이 뭐더라",
          "speaker_id": "2",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009792.1.18",
          "form": "성인되고 월드비젼 정기후원하면서\r\n봉사 하러 한번 같은게 다네요ㅋ",
          "original_form": "성인되고 월드비젼 정기후원하면서\r\n봉사 하러 한번 같은게 다네요ㅋ",
          "speaker_id": "1",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009792.1.19",
          "form": "천천히 움직이는거던데",
          "original_form": "천천히 움직이는거던데",
          "speaker_id": "2",
          "time": "20210823 11:16"
        },
        {
          "id": "MDRW2100009792.1.20",
          "form": "중국에 아침에 공원가면 할머니 할아버지들 하시는 그 체조",
          "original_form": "중국에 아침에 공원가면 할머니 할아버지들 하시는 그 체조",
          "speaker_id": "2",
          "time": "20210823 11:16"
        },
        {
          "id": "MDRW2100009792.1.21",
          "form": "기체조요?ㅋㄱㅋ",
          "original_form": "기체조요?ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210823 11:16"
        },
        {
          "id": "MDRW2100009792.1.22",
          "form": "아 그거요",
          "original_form": "아 그거요",
          "speaker_id": "2",
          "time": "20210823 11:18"
        },
        {
          "id": "MDRW2100009792.1.23",
          "form": "뭔가 그거 하는분들 봤는데",
          "original_form": "뭔가 그거 하는분들 봤는데",
          "speaker_id": "2",
          "time": "20210823 11:18"
        },
        {
          "id": "MDRW2100009792.1.24",
          "form": "그것도 일종의 동호회겠죠",
          "original_form": "그것도 일종의 동호회겠죠",
          "speaker_id": "2",
          "time": "20210823 11:19"
        },
        {
          "id": "MDRW2100009792.1.25",
          "form": "해보고싶기도하더라고요",
          "original_form": "해보고싶기도하더라고요",
