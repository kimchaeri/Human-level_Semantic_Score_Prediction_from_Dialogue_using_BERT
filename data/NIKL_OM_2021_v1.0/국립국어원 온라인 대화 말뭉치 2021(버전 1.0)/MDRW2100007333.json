          "original_form": "서울에서 공연 봤었지",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007333.1.8",
          "form": "기억해?",
          "original_form": "기억해?",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007333.1.9",
          "form": "그때 자이언티만고고",
          "original_form": "그때 자이언티만고고",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007333.1.10",
          "form": "그때 자이언티가",
          "original_form": "그때 자이언티가",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007333.1.11",
          "form": "신곡 낸다고",
          "original_form": "신곡 낸다고",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007333.1.12",
