      "utterance": [
        {
          "id": "MDRW2100023756.1.1",
          "form": "예전에 가본 기억으론 그렇게 드림시티까지는.,?",
          "original_form": "예전에 가본 기억으론 그렇게 드림시티까지는.,?",
          "speaker_id": "2",
          "time": "20210830 16:43"
        },
        {
          "id": "MDRW2100023756.1.2",
          "form": "바야흐로 가을 모기의 계절이 왔습니다",
          "original_form": "바야흐로 가을 모기의 계절이 왔습니다",
          "speaker_id": "1",
          "time": "20210830 16:44"
        },
        {
          "id": "MDRW2100023756.1.3",
          "form": "그래요?!",
          "original_form": "그래요?!",
          "speaker_id": "1",
          "time": "20210830 16:44"
        },
        {
          "id": "MDRW2100023756.1.4",
          "form": "크고 멋진 건물이 꽤 있고 길도 잘 닦여있지만",
          "original_form": "크고 멋진 건물이 꽤 있고 길도 잘 닦여있지만",
          "speaker_id": "2",
          "time": "20210830 16:44"
        },
        {
          "id": "MDRW2100023756.1.5",
          "form": "사실 서울이 그런데가 더 많죠",
          "original_form": "사실 서울이 그런데가 더 많죠",
          "speaker_id": "2",
          "time": "20210830 16:44"
        },
        {
          "id": "MDRW2100023756.1.6",
          "form": "뭐 제가 그런 현대식 건물에 큰 감흥을 못느끼는 타입이라 그런거기도 해요",
          "original_form": "뭐 제가 그런 현대식 건물에 큰 감흥을 못느끼는 타입이라 그런거기도 해요",
          "speaker_id": "2",
          "time": "20210830 16:45"
        },
        {
          "id": "MDRW2100023756.1.7",
          "form": "아핫 그렇군요~!~!",
          "original_form": "아핫 그렇군요~!~!",
          "speaker_id": "1",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100023756.1.8",
          "form": "그 날은 날씨가 어떠려나~~~",
          "original_form": "그 날은 날씨가 어떠려나~~~",
          "speaker_id": "1",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100023756.1.9",
          "form": "오늘 비 안오겠죠...?",
          "original_form": "오늘 비 안오겠죠...?",
          "speaker_id": "1",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100023756.1.10",
          "form": "구름은 좀 많이 꼈는데",
          "original_form": "구름은 좀 많이 꼈는데",
          "speaker_id": "2",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100023756.1.11",
          "form": "좀 습하기도 하고",
          "original_form": "좀 습하기도 하고",
          "speaker_id": "2",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100023756.1.12",
          "form": "가능성은 있어요",
          "original_form": "가능성은 있어요",
          "speaker_id": "2",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100023756.1.13",
          "form": "히익~!",
          "original_form": "히익~!",
          "speaker_id": "1",
          "time": "20210830 17:04"
        },
        {
          "id": "MDRW2100023756.1.14",
          "form": "제가 사실 금요일에 가져갔던",
          "original_form": "제가 사실 금요일에 가져갔던",
          "speaker_id": "1",
          "time": "20210830 17:04"
        },
        {
          "id": "MDRW2100023756.1.15",
          "form": "우산을 안가져왔거든요...",
          "original_form": "우산을 안가져왔거든요...",
          "speaker_id": "1",
          "time": "20210830 17:04"
        },
        {
          "id": "MDRW2100023756.1.16",
          "form": "정말 예측불허 날씨다,,",
          "original_form": "정말 예측불허 날씨다,,",
          "speaker_id": "1",
          "time": "20210830 17:04"
        },
        {
          "id": "MDRW2100023756.1.17",
          "form": "근데 안올수도 있어요",
          "original_form": "근데 안올수도 있어요",
          "speaker_id": "2",
          "time": "20210830 17:04"
        },
        {
          "id": "MDRW2100023756.1.18",
          "form": "내일은 올거같지만",
          "original_form": "내일은 올거같지만",
          "speaker_id": "2",
          "time": "20210830 17:04"
        },
        {
          "id": "MDRW2100023756.1.19",
          "form": "맞아요 내일은 온대요,,,",
          "original_form": "맞아요 내일은 온대요,,,",
          "speaker_id": "1",
          "time": "20210830 17:14"
        },
        {
          "id": "MDRW2100023756.1.20",
          "form": "아니 근데 저도 뒷목에 모기 물렸어요",
          "original_form": "아니 근데 저도 뒷목에 모기 물렸어요",
          "speaker_id": "1",
          "time": "20210830 17:14"
        },
        {
          "id": "MDRW2100023756.1.21",
          "form": "가을 모기...",
          "original_form": "가을 모기...",
          "speaker_id": "1",
          "time": "20210830 17:14"
        },
        {
          "id": "MDRW2100023756.1.22",
          "form": "으... 모기 싫어",
          "original_form": "으... 모기 싫어",
          "speaker_id": "2",
          "time": "20210830 17:15"
        },
        {
          "id": "MDRW2100023756.1.23",
          "form": "모기는 멸종했으면 좋겠어요",
          "original_form": "모기는 멸종했으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210830 17:24"
        },
        {
          "id": "MDRW2100023756.1.24",
          "form": "굉장히 인간중심적인 생각이지만",
          "original_form": "굉장히 인간중심적인 생각이지만",
          "speaker_id": "2",
          "time": "20210830 17:24"
        },
        {
          "id": "MDRW2100023756.1.25",
          "form": "그러게요 도대체 모기는~!~!!!",
          "original_form": "그러게요 도대체 모기는~!~!!!",
          "speaker_id": "1",
          "time": "20210830 17:25"
        },
        {
          "id": "MDRW2100023756.1.26",
          "form": "이제는 뭘 입고 다녀야할지,,",
          "original_form": "이제는 뭘 입고 다녀야할지,,",
          "speaker_id": "1",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100023756.1.27",
          "form": "반팔에 가디건",
          "original_form": "반팔에 가디건",
          "speaker_id": "2",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100023756.1.28",
          "form": "가장 만만하죠",
          "original_form": "가장 만만하죠",
          "speaker_id": "2",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100023756.1.29",
          "form": "맞습니다,,,",
          "original_form": "맞습니다,,,",
          "speaker_id": "1",
          "time": "20210830 17:28"
        },
        {
          "id": "MDRW2100023756.1.30",
          "form": "비온다!",
          "original_form": "비온다!",
          "speaker_id": "2",
          "time": "20210831 11:01"
        }
      ]
    }
