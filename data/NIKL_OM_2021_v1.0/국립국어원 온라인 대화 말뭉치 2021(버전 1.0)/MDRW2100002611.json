      "utterance": [
        {
          "id": "MDRW2100002611.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002611.1.2",
          "form": "안녕하세요 이제서야봤네요",
          "original_form": "안녕하세요 이제서야봤네요",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002611.1.3",
          "form": "네 반갑습니다 스포츠 좋아하세요?",
          "original_form": "네 반갑습니다 스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002611.1.4",
          "form": "운동하는건 별로안좋아하구요 ㅎ\r\n올림픽보는건 좋아해요",
          "original_form": "운동하는건 별로안좋아하구요 ㅎ\r\n올림픽보는건 좋아해요",
          "speaker_id": "2",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002611.1.5",
          "form": "양궁 보셨나요?",
          "original_form": "양궁 보셨나요?",
          "speaker_id": "2",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002611.1.6",
          "form": "네 양궁봤고",
          "original_form": "네 양궁봤고",
          "speaker_id": "1",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002611.1.7",
          "form": "올림픽 다챙겨봤죠",
          "original_form": "올림픽 다챙겨봤죠",
          "speaker_id": "1",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002611.1.8",
          "form": "저는 육상종목 좋아해서 안타까웟어요",
          "original_form": "저는 육상종목 좋아해서 안타까웟어요",
          "speaker_id": "1",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002611.1.9",
          "form": "한국에선 중계를 안해주니깐",
          "original_form": "한국에선 중계를 안해주니깐",
          "speaker_id": "1",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002611.1.10",
          "form": "아 그러셨겠네요",
          "original_form": "아 그러셨겠네요",
          "speaker_id": "2",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002611.1.11",
          "form": "너무 잘나가는것만 방송해주는것도 좀 그렇더라구요\r\n사기떨어질것같아요",
          "original_form": "너무 잘나가는것만 방송해주는것도 좀 그렇더라구요\r\n사기떨어질것같아요",
          "speaker_id": "2",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002611.1.12",
          "form": "맞아요 실례지만 평소 운동은 안하시나요?",
          "original_form": "맞아요 실례지만 평소 운동은 안하시나요?",
          "speaker_id": "1",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002611.1.13",
          "form": "필라테스 하고 있어요",
          "original_form": "필라테스 하고 있어요",
          "speaker_id": "2",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002611.1.14",
          "form": "멋지네요!!",
          "original_form": "멋지네요!!",
          "speaker_id": "1",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002611.1.15",
          "form": "저는 운동 다좋아해서ㅎㅎ",
          "original_form": "저는 운동 다좋아해서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002611.1.16",
          "form": "허리아파서 시작했는데 하고나서 안아파서 계속하고있어요",
          "original_form": "허리아파서 시작했는데 하고나서 안아파서 계속하고있어요",
          "speaker_id": "2",
          "time": "20210820 17:06"
        },
        {
          "id": "MDRW2100002611.1.17",
          "form": "운동마니아이실것같아요 어떤운동하세요?",
          "original_form": "운동마니아이실것같아요 어떤운동하세요?",
          "speaker_id": "2",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002611.1.18",
          "form": "잘되셧네요",
          "original_form": "잘되셧네요",
          "speaker_id": "1",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002611.1.19",
          "form": "저는 헬스, 러닝, 배드민턴 주로해요 코로나 전엔 수영도했고요",
          "original_form": "저는 헬스, 러닝, 배드민턴 주로해요 코로나 전엔 수영도했고요",
          "speaker_id": "1",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002611.1.20",
          "form": "안하시는게 없으실듯요 ㅎ",
          "original_form": "안하시는게 없으실듯요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002611.1.21",
          "form": "맞아요!@",
          "original_form": "맞아요!@",
          "speaker_id": "1",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002611.1.22",
          "form": "다해요 운동은ㅋㅋㅋ",
          "original_form": "다해요 운동은ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002611.1.23",
          "form": "같이하면 좋겟네요",
          "original_form": "같이하면 좋겟네요",
          "speaker_id": "1",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002611.1.24",
          "form": "골프는 치세요?",
          "original_form": "골프는 치세요?",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002611.1.25",
          "form": "원래운동을 좋아하시나봐요",
          "original_form": "원래운동을 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002611.1.26",
          "form": "골프는 안칩니다 허리에 안좋은거 같아서요",
          "original_form": "골프는 안칩니다 허리에 안좋은거 같아서요",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002611.1.27",
          "form": "아...그러시군요",
          "original_form": "아...그러시군요",
          "speaker_id": "2",
          "time": "20210820 17:26"
        }
      ]
    }
  ]
