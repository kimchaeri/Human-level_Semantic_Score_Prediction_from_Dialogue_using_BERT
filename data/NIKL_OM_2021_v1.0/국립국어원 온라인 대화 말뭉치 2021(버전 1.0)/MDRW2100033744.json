      "utterance": [
        {
          "id": "MDRW2100033744.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100033744.1.2",
          "form": "점심 맛있게 드셨나요?",
          "original_form": "점심 맛있게 드셨나요?",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100033744.1.3",
          "form": "아직이요!",
          "original_form": "아직이요!",
          "speaker_id": "1",
          "time": "20210826 13:41"
        },
        {
          "id": "MDRW2100033744.1.4",
          "form": "님은요?!",
          "original_form": "님은요?!",
          "speaker_id": "1",
          "time": "20210826 13:41"
        },
        {
          "id": "MDRW2100033744.1.5",
          "form": "아 아직 안 드셨군요 저는 제육덮밥 먹었어요",
          "original_form": "아 아직 안 드셨군요 저는 제육덮밥 먹었어요",
          "speaker_id": "2",
          "time": "20210826 13:43"
        },
        {
          "id": "MDRW2100033744.1.6",
          "form": "오 제육덮밥",
          "original_form": "오 제육덮밥",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100033744.1.7",
          "form": "맛있겠어요",
          "original_form": "맛있겠어요",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100033744.1.8",
          "form": "근데 좀 맛이 없었어요..ㅠㅠ 간이 안 맞더라고요",
          "original_form": "근데 좀 맛이 없었어요..ㅠㅠ 간이 안 맞더라고요",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100033744.1.9",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100033744.1.10",
          "form": "가서 먹은건가요?",
          "original_form": "가서 먹은건가요?",
          "speaker_id": "1",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100033744.1.11",
          "form": "네~식당가서 먹었는데 너무 맛이 없더라고요~이제 안 가야 겠어요",
          "original_form": "네~식당가서 먹었는데 너무 맛이 없더라고요~이제 안 가야 겠어요",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100033744.1.12",
          "form": "와우 제육이",
          "original_form": "와우 제육이",
          "speaker_id": "1",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100033744.1.13",
          "form": "맛없기쉽지않을텐데",
          "original_form": "맛없기쉽지않을텐데",
          "speaker_id": "1",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100033744.1.14",
          "form": "그러니까요~제육은 무난한 메뉴잖아요..그런데 너무 싱겁고 고기도 별로 없었어요",
          "original_form": "그러니까요~제육은 무난한 메뉴잖아요..그런데 너무 싱겁고 고기도 별로 없었어요",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100033744.1.15",
          "form": "ㅠㅠ그럴수가",
          "original_form": "ㅠㅠ그럴수가",
          "speaker_id": "1",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100033744.1.16",
          "form": "실패없는제육인데말이죠",
          "original_form": "실패없는제육인데말이죠",
          "speaker_id": "1",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100033744.1.17",
          "form": "네~그래서 앞으로는 안 가려요고",
          "original_form": "네~그래서 앞으로는 안 가려요고",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100033744.1.18",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100033744.1.19",
          "form": "배민등급이 무엇인가요?",
          "original_form": "배민등급이 무엇인가요?",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100033744.1.20",
          "form": "저 배민은 많이 이용안하고 요기요 이용해요",
          "original_form": "저 배민은 많이 이용안하고 요기요 이용해요",
          "speaker_id": "2",
          "time": "20210826 17:16"
        }
      ]
