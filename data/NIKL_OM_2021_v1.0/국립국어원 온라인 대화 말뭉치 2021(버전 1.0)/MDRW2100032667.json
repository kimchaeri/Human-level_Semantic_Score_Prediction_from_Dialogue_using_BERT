      "utterance": [
        {
          "id": "MDRW2100032667.1.1",
          "form": "게임 하시나요?",
          "original_form": "게임 하시나요?",
          "speaker_id": "1",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100032667.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100032667.1.3",
          "form": "님은요 ?",
          "original_form": "님은요 ?",
          "speaker_id": "2",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100032667.1.4",
          "form": "어 저는 예전에 하고",
          "original_form": "어 저는 예전에 하고",
          "speaker_id": "1",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100032667.1.5",
          "form": "요즘엔 안해요",
          "original_form": "요즘엔 안해요",
          "speaker_id": "1",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100032667.1.6",
          "form": "어떤게임을 했어요 ?",
          "original_form": "어떤게임을 했어요 ?",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032667.1.7",
          "form": "크아좋아했어요ㅋㅋ",
          "original_form": "크아좋아했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032667.1.8",
          "form": "재밌나요 ?",
          "original_form": "재밌나요 ?",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032667.1.9",
          "form": "네 귀엽고ㅋㅋㅋ",
          "original_form": "네 귀엽고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032667.1.10",
          "form": "귀여운 캐릭터가 있는 게임인가보네요",
          "original_form": "귀여운 캐릭터가 있는 게임인가보네요",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032667.1.11",
          "form": "네네 ㅋㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032667.1.12",
          "form": "게이머도 좋아하나요 ?",
          "original_form": "게이머도 좋아하나요 ?",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032667.1.13",
          "form": "어 그건아니에요!",
          "original_form": "어 그건아니에요!",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032667.1.14",
          "form": "아 그냥 게임만 좋아하는거군요",
          "original_form": "아 그냥 게임만 좋아하는거군요",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032667.1.15",
          "form": "네ㅋㅋ그냥 단순히 그저 게임만!",
          "original_form": "네ㅋㅋ그냥 단순히 그저 게임만!",
          "speaker_id": "1",
          "time": "20210905 22:06"
