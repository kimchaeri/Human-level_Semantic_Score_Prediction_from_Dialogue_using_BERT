      "utterance": [
        {
          "id": "MDRW2100036421.1.1",
          "form": "그럼 이탈리아에서 관광은 하셨어요?",
          "original_form": "그럼 이탈리아에서 관광은 하셨어요?",
          "speaker_id": "1",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100036421.1.2",
          "form": "아프셔서 숙소에만 계신건 아니쥬ㅠㅠ",
          "original_form": "아프셔서 숙소에만 계신건 아니쥬ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100036421.1.3",
          "form": "그래도 다 따라다녔어요ㅋㅋ",
          "original_form": "그래도 다 따라다녔어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100036421.1.4",
          "form": "ㅋㅋㅋㅋㅋ아파도 볼건봐야죠!!",
          "original_form": "ㅋㅋㅋㅋㅋ아파도 볼건봐야죠!!",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100036421.1.5",
          "form": "어디어디 보셨어요?",
          "original_form": "어디어디 보셨어요?",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100036421.1.6",
          "form": "이탈리아 지역 다 돌아다니신거에여?",
          "original_form": "이탈리아 지역 다 돌아다니신거에여?",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100036421.1.7",
          "form": "아님 한곳에 계셨어요?",
          "original_form": "아님 한곳에 계셨어요?",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100036421.1.8",
          "form": "피렌체부터 베니스 피사 피렌체 로마 폼페이 등등 갔어요",
          "original_form": "피렌체부터 베니스 피사 피렌체 로마 폼페이 등등 갔어요",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100036421.1.9",
          "form": "단체여행이라 이동 하나는 편했죠 ㅋㅋ",
          "original_form": "단체여행이라 이동 하나는 편했죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100036421.1.10",
          "form": "와아~~ 다 보고 오셨네요!!!",
          "original_form": "와아~~ 다 보고 오셨네요!!!",
          "speaker_id": "1",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100036421.1.11",
          "form": "저는 이탈리아 갔지만 피사의사탑은 못봤어요ㅠ",
          "original_form": "저는 이탈리아 갔지만 피사의사탑은 못봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100036421.1.12",
          "form": "젤 좋았던 건 피렌체랑 바티칸이었어요",
          "original_form": "젤 좋았던 건 피렌체랑 바티칸이었어요",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100036421.1.13",
          "form": "피사에는 진짜 피사의사탑밖에 없더라구요 ㅎㅎ",
          "original_form": "피사에는 진짜 피사의사탑밖에 없더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100036421.1.14",
          "form": "앗 ㅋㅋㅋ 거기가면 사람들이 다 손으로 밀고 있다던데",
          "original_form": "앗 ㅋㅋㅋ 거기가면 사람들이 다 손으로 밀고 있다던데",
          "speaker_id": "1",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100036421.1.15",
          "form": "바티칸 진짜 웅장하죠!",
          "original_form": "바티칸 진짜 웅장하죠!",
          "speaker_id": "1",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100036421.1.16",
          "form": "거기에서밖에 볼수없다는 그림도 있구...",
          "original_form": "거기에서밖에 볼수없다는 그림도 있구...",
          "speaker_id": "1",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100036421.1.17",
          "form": "천지창조 천장화는 진짜 신기하더라구요. 경이로울정도",
          "original_form": "천지창조 천장화는 진짜 신기하더라구요. 경이로울정도",
          "speaker_id": "2",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100036421.1.18",
          "form": "그쳐 진짜... 어떻게 그런 그림을 그릴수있는지..",
          "original_form": "그쳐 진짜... 어떻게 그런 그림을 그릴수있는지..",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100036421.1.19",
          "form": "사진못찍게 경호원도 있잖아요~",
          "original_form": "사진못찍게 경호원도 있잖아요~",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100036421.1.20",
          "form": "들어가는 인원도 제한하구",
          "original_form": "들어가는 인원도 제한하구",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100036421.1.21",
          "form": "쉿 하시는 분 기억나요ㅋㅋㅋㅋ",
          "original_form": "쉿 하시는 분 기억나요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100036421.1.22",
          "form": "조용히 하라고",
          "original_form": "조용히 하라고",
          "speaker_id": "2",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100036421.1.23",
          "form": "혼자 온 거였으면 천장만 계속 보고 싶더라구요ㅎㅎ",
          "original_form": "혼자 온 거였으면 천장만 계속 보고 싶더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100036421.1.24",
          "form": "ㅋㅋㅋㅋㅋ근데 들어가는 팀마다 사진찍어서 퇴장당하는 사람 있더라구요",
          "original_form": "ㅋㅋㅋㅋㅋ근데 들어가는 팀마다 사진찍어서 퇴장당하는 사람 있더라구요",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100036421.1.25",
          "form": "하지말라면 좀 하지말라...",
          "original_form": "하지말라면 좀 하지말라...",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100036421.1.26",
          "form": "ㅋㅋ 꼭 말 안 듣는 사람들이 있죠",
          "original_form": "ㅋㅋ 꼭 말 안 듣는 사람들이 있죠",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100036421.1.27",
          "form": "바티칸 광장도 분위기 되게 좋았던 기억이 나네요",
          "original_form": "바티칸 광장도 분위기 되게 좋았던 기억이 나네요",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100036421.1.28",
          "form": "맞아여 그 안에 조경이 되어있었나요?",
          "original_form": "맞아여 그 안에 조경이 되어있었나요?",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036421.1.29",
          "form": "뭔가 특정구역이 푸릇푸릇했던 기억이 나요 ㅎㅎ",
          "original_form": "뭔가 특정구역이 푸릇푸릇했던 기억이 나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036421.1.30",
          "form": "거기 들어가려고 아침부터 일어나서 줄서면서 끼니떼운 생각도 나네여 ㅋㅋ",
          "original_form": "거기 들어가려고 아침부터 일어나서 줄서면서 끼니떼운 생각도 나네여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100036421.1.31",
          "form": "지하철역에서 초밥사고 천도복숭아 사서 줄서면서 먹었어요 ㅋㅋㅋㅋ 기억난다 기억나 ㅋㅋㅋㅋㅋ",
          "original_form": "지하철역에서 초밥사고 천도복숭아 사서 줄서면서 먹었어요 ㅋㅋㅋㅋ 기억난다 기억나 ㅋㅋㅋㅋㅋ",
