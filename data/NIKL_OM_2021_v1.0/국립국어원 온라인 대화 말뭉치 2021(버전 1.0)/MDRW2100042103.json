      "utterance": [
        {
          "id": "MDRW2100042103.1.1",
          "form": "그럼 내일이면",
          "original_form": "그럼 내일이면",
          "speaker_id": "2",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100042103.1.2",
          "form": "원소시야로 공책찾는거 하겠네",
          "original_form": "원소시야로 공책찾는거 하겠네",
          "speaker_id": "2",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100042103.1.3",
          "form": "그거 원소시야로 절대 못찾으니까",
          "original_form": "그거 원소시야로 절대 못찾으니까",
          "speaker_id": "2",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100042103.1.4",
          "form": "걍 공략으로 위치 찍고 가",
          "original_form": "걍 공략으로 위치 찍고 가",
          "speaker_id": "2",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100042103.1.5",
          "form": "아 그래??",
          "original_form": "아 그래??",
          "speaker_id": "1",
          "time": "20210906 19:44"
        },
        {
          "id": "MDRW2100042103.1.6",
          "form": "내가 본 공략에 원소시야로 찾으라고 되어잇던뎈ㅋㅋㅋㅋ",
          "original_form": "내가 본 공략에 원소시야로 찾으라고 되어잇던뎈ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:44"
        },
        {
          "id": "MDRW2100042103.1.7",
          "form": "ㅇㅇ....ㄹㅇ 어디 바위위에",
          "original_form": "ㅇㅇ....ㄹㅇ 어디 바위위에",
          "speaker_id": "2",
          "time": "20210906 19:44"
        },
        {
          "id": "MDRW2100042103.1.8",
          "form": "종이조각 하나 땔렁 있어서",
          "original_form": "종이조각 하나 땔렁 있어서",
          "speaker_id": "2",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100042103.1.9",
          "form": "ㅇㅎㅋㅋㅋㅋ 그렇군",
          "original_form": "ㅇㅎㅋㅋㅋㅋ 그렇군",
          "speaker_id": "1",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100042103.1.10",
          "form": "대충 위치 찍고",
          "original_form": "대충 위치 찍고",
          "speaker_id": "2",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100042103.1.11",
          "form": "난 진짜 혼자선 도저히 못찾겠더라구",
          "original_form": "난 진짜 혼자선 도저히 못찾겠더라구",
          "speaker_id": "2",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100042103.1.12",
          "form": "섯섯이 이 겜에서 젤 힘들어하는거",
          "original_form": "섯섯이 이 겜에서 젤 힘들어하는거",
          "speaker_id": "1",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100042103.1.13",
          "form": "뭐 찾기인듯 ㅋㅋㅋㅋㅋ",
          "original_form": "뭐 찾기인듯 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100042103.1.14",
          "form": "ㄹㅇ.......",
          "original_form": "ㄹㅇ.......",
          "speaker_id": "2",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100042103.1.15",
          "form": "전투보다 탐색이 더 힘든 ㅋㅋㅋㅋㅋ",
          "original_form": "전투보다 탐색이 더 힘든 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100042103.1.16",
          "form": "아니 제발 말만 찾기고",
          "original_form": "아니 제발 말만 찾기고",
          "speaker_id": "2",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100042103.1.17",
          "form": "걍 표시해줘.....",
          "original_form": "걍 표시해줘.....",
          "speaker_id": "2",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100042103.1.18",
          "form": "나 진짜 전혀 다른데 뒤지다가",
          "original_form": "나 진짜 전혀 다른데 뒤지다가",
          "speaker_id": "2",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100042103.1.19",
          "form": "갸박쳐서",
          "original_form": "갸박쳐서",
          "speaker_id": "2",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042103.1.20",
          "form": "검색해봣잔사",
          "original_form": "검색해봣잔사",
          "speaker_id": "2",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042103.1.21",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042103.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
