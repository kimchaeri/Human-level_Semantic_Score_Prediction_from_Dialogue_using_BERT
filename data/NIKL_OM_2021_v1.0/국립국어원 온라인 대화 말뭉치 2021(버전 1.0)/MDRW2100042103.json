          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042103.1.26",
          "form": "가격 저렴했는데",
          "original_form": "가격 저렴했는데",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042103.1.27",
          "form": "아이디 바꾸다가....ㅠ",
          "original_form": "아이디 바꾸다가....ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042103.1.28",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042103.1.29",
          "form": "라이덴 그래서",
          "original_form": "라이덴 그래서",
          "speaker_id": "2",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042103.1.30",
          "form": "뽑아볼거야?",
          "original_form": "뽑아볼거야?",
          "speaker_id": "2",
          "time": "20210906 21:46"
        },
