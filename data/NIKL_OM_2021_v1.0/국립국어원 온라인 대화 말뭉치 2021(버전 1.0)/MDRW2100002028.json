      "utterance": [
        {
          "id": "MDRW2100002028.1.1",
          "form": "게임 어떤거 하세요?",
          "original_form": "게임 어떤거 하세요?",
          "speaker_id": "1",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100002028.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 23:00"
        },
        {
          "id": "MDRW2100002028.1.3",
          "form": "요즘은 많이 안하지만 시뮬레이션게임을 즐겨했어요",
          "original_form": "요즘은 많이 안하지만 시뮬레이션게임을 즐겨했어요",
          "speaker_id": "2",
          "time": "20210819 23:01"
        },
        {
          "id": "MDRW2100002028.1.4",
          "form": "오! 개임 이름이요?",
          "original_form": "오! 개임 이름이요?",
          "speaker_id": "1",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100002028.1.5",
          "form": "타이쿤?",
          "original_form": "타이쿤?",
          "speaker_id": "2",
          "time": "20210819 23:09"
        },
        {
          "id": "MDRW2100002028.1.6",
          "form": "아이 키우면서 안하게됐는데요",
          "original_form": "아이 키우면서 안하게됐는데요",
          "speaker_id": "2",
          "time": "20210819 23:10"
        },
        {
          "id": "MDRW2100002028.1.7",
          "form": "농장 키우는 게임이나",
          "original_form": "농장 키우는 게임이나",
          "speaker_id": "2",
          "time": "20210819 23:10"
        },
        {
          "id": "MDRW2100002028.1.8",
          "form": "ㅋㅋㅋ  고스톱?",
          "original_form": "ㅋㅋㅋ  고스톱?",
          "speaker_id": "2",
          "time": "20210819 23:11"
        },
        {
          "id": "MDRW2100002028.1.9",
          "form": "곰곰희사랑님은 어떤게임을 좋아하세요?",
          "original_form": "곰곰희사랑님은 어떤게임을 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100002028.1.10",
          "form": "저도 게임은 잘 몰라요. 고스톱은 아주 예전에 해본적 있어요.\r\n저는 물방울 같은거 터트리는 거 많이 했어요.",
          "original_form": "저도 게임은 잘 몰라요. 고스톱은 아주 예전에 해본적 있어요.\r\n저는 물방울 같은거 터트리는 거 많이 했어요.",
          "speaker_id": "1",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100002028.1.11",
          "form": "버블버블?",
          "original_form": "버블버블?",
          "speaker_id": "2",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100002028.1.12",
          "form": "저는 그렇게 보니 정말 많은 게임을 했네요ㅋㅋ",
          "original_form": "저는 그렇게 보니 정말 많은 게임을 했네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100002028.1.13",
          "form": "꾸준하게",
          "original_form": "꾸준하게",
          "speaker_id": "2",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100002028.1.14",
          "form": "집에 겜보이가 있었습니다ㅋㅋ",
          "original_form": "집에 겜보이가 있었습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100002028.1.15",
          "form": "겜보이 아세요?",
          "original_form": "겜보이 아세요?",
          "speaker_id": "2",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100002028.1.16",
          "form": "겜보이 들어는 본거 같아요^^\r\n닌텐도 같은 건가요?",
          "original_form": "겜보이 들어는 본거 같아요^^\r\n닌텐도 같은 건가요?",
          "speaker_id": "1",
          "time": "20210819 23:19"
        },
        {
          "id": "MDRW2100002028.1.17",
          "form": "겜보이는 회사구요ㅎㅎ굳이 따지자면 닌텐도 같은거겠죠?",
          "original_form": "겜보이는 회사구요ㅎㅎ굳이 따지자면 닌텐도 같은거겠죠?",
          "speaker_id": "2",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002028.1.18",
          "form": "아. 그렇구나 조그만 게임기 같은 거 겠네요. 팩 같은거 끼워서 게임 하는거죠?",
          "original_form": "아. 그렇구나 조그만 게임기 같은 거 겠네요. 팩 같은거 끼워서 게임 하는거죠?",
          "speaker_id": "1",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002028.1.19",
          "form": "다했어요 팩이고 작은거고ㅎㅎ",
          "original_form": "다했어요 팩이고 작은거고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002028.1.20",
          "form": "오빠가 있어서 생각해보니 게임이 일상이였네요ㅎㅎ",
          "original_form": "오빠가 있어서 생각해보니 게임이 일상이였네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002028.1.21",
          "form": "오빠 하는거 다 해보셨겠네요.\r\n오락실은 안 따라가셨나요? ㅋ",
          "original_form": "오빠 하는거 다 해보셨겠네요.\r\n오락실은 안 따라가셨나요? ㅋ",
          "speaker_id": "1",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100002028.1.22",
          "form": "집에 왠만한 게임은 다 있었고",
          "original_form": "집에 왠만한 게임은 다 있었고",
          "speaker_id": "2",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100002028.1.23",
          "form": "오락실은 고등학교 되면서 다녔어요",
          "original_form": "오락실은 고등학교 되면서 다녔어요",
          "speaker_id": "2",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002028.1.24",
          "form": "펌프ㅋㅋㅋ",
          "original_form": "펌프ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002028.1.25",
          "form": "학교 땡땡이치고 펌프하러갔다가  생활주임 쌤한테 걸려서 잡혀간적도 있어요",
          "original_form": "학교 땡땡이치고 펌프하러갔다가  생활주임 쌤한테 걸려서 잡혀간적도 있어요",
          "speaker_id": "2",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002028.1.26",
          "form": "우와 펌프하다 잡혀가셨다니 한 펌프 하셨나봐요~~~~",
          "original_form": "우와 펌프하다 잡혀가셨다니 한 펌프 하셨나봐요~~~~",
          "speaker_id": "1",
          "time": "20210819 23:40"
        }
      ]
