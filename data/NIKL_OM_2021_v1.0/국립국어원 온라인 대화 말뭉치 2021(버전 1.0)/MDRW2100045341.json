      "utterance": [
        {
          "id": "MDRW2100045341.1.1",
          "form": "아침에 보통 뭐드시나여",
          "original_form": "아침에 보통 뭐드시나여",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045341.1.2",
          "form": "아침 잘 안먹는 편이에요 .. 가끔 먹으면 시리얼이나 두유 정도요!",
          "original_form": "아침 잘 안먹는 편이에요 .. 가끔 먹으면 시리얼이나 두유 정도요!",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045341.1.3",
          "form": "저도 아침보다 잠을선호해여 ㅋㅋㅋㅋ",
          "original_form": "저도 아침보다 잠을선호해여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045341.1.4",
          "form": "시리얼 어떤거 드세요?",
          "original_form": "시리얼 어떤거 드세요?",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045341.1.5",
          "form": "그래놀라시리얼 먹고있어요ㅎㅍ",
          "original_form": "그래놀라시리얼 먹고있어요ㅎㅍ",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045341.1.6",
          "form": "저도 밥보다는 잠을 거의 택해요 ㅋㅋ",
          "original_form": "저도 밥보다는 잠을 거의 택해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045341.1.7",
          "form": "건강시리얼이네여",
          "original_form": "건강시리얼이네여",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045341.1.8",
          "form": "ㅋㅋㅋㅋㅋ네 잠이최고져 밥은 또 두번먹으니까여",
          "original_form": "ㅋㅋㅋㅋㅋ네 잠이최고져 밥은 또 두번먹으니까여",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045341.1.9",
          "form": "그렇죠 밥은 좀만 참으면 먹을수있지만 잠은 끊기먄 안되니까요 ..",
          "original_form": "그렇죠 밥은 좀만 참으면 먹을수있지만 잠은 끊기먄 안되니까요 ..",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045341.1.10",
          "form": "내일 점심 어떤거 드실예정이신가요?",
          "original_form": "내일 점심 어떤거 드실예정이신가요?",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045341.1.11",
          "form": "네 ㅋㅋㅋ그니까여",
          "original_form": "네 ㅋㅋㅋ그니까여",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045341.1.12",
          "form": "전 내일..아마 초밥을먹을거같아여",
          "original_form": "전 내일..아마 초밥을먹을거같아여",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045341.1.13",
          "form": "오 초밥 좋죠 ㅜㅜ",
          "original_form": "오 초밥 좋죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045341.1.14",
          "form": "저도 초밥 좋아해요",
          "original_form": "저도 초밥 좋아해요",
          "speaker_id": "2",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045341.1.15",
          "form": "쿼카님은요",
          "original_form": "쿼카님은요",
          "speaker_id": "1",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045341.1.16",
          "form": "낼 점심 어떤거드실거에요?",
          "original_form": "낼 점심 어떤거드실거에요?",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045341.1.17",
          "form": "저는 순두부찌개 먹을까생각중이에여",
          "original_form": "저는 순두부찌개 먹을까생각중이에여",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045341.1.18",
          "form": "쌀쌀해서 찌개류가 땡기네요",
          "original_form": "쌀쌀해서 찌개류가 땡기네요",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045341.1.19",
          "form": "순찌좋죠ㅜㅜㅜ매운순찌드시나여?백순찌드시나여",
          "original_form": "순찌좋죠ㅜㅜㅜ매운순찌드시나여?백순찌드시나여",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045341.1.20",
          "form": "매운거요! 칼칼하니 맛나죠",
          "original_form": "매운거요! 칼칼하니 맛나죠",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045341.1.21",
          "form": "달걀넣고 끓일려구용 ㅋㅋ",
          "original_form": "달걀넣고 끓일려구용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045341.1.22",
          "form": "크으 그쳐 칼칼 하고",
          "original_form": "크으 그쳐 칼칼 하고",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045341.1.23",
          "form": "직접만들어드시는군여",
          "original_form": "직접만들어드시는군여",
          "speaker_id": "1",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045341.1.24",
          "form": "제가 아니구.. 엄마가용...ㅋㅋ 핫ㅎ..",
          "original_form": "제가 아니구.. 엄마가용...ㅋㅋ 핫ㅎ..",
          "speaker_id": "2",
          "time": "20210908 02:13"
        }
      ]
    }
