      "utterance": [
        {
          "id": "MDRW2100037734.1.1",
          "form": "날씨 너무 더워",
          "original_form": "날씨 너무 더워",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037734.1.2",
          "form": "진짜더워에어컨켰니",
          "original_form": "진짜더워에어컨켰니",
          "speaker_id": "1",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100037734.1.3",
          "form": "아니아니ㅜ",
          "original_form": "아니아니ㅜ",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100037734.1.4",
          "form": "그냥땀흘리고있어",
          "original_form": "그냥땀흘리고있어",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100037734.1.5",
          "form": "ㅋㅋㅋㄱㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100037734.1.6",
          "form": "name3이는 켰니",
          "original_form": "&name3&이는 켰니",
          "speaker_id": "2",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100037734.1.7",
          "form": "언제가을오니",
          "original_form": "언제가을오니",
          "speaker_id": "2",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100037734.1.8",
          "form": "나너무춥고싶다",
          "original_form": "나너무춥고싶다",
          "speaker_id": "2",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100037734.1.9",
          "form": "키고땀식히고있어다음당근을위해",
          "original_form": "키고땀식히고있어다음당근을위해",
          "speaker_id": "1",
          "time": "20210901 17:42"
        },
        {
          "id": "MDRW2100037734.1.10",
          "form": "진짜춥고싶다 돌겠어",
          "original_form": "진짜춥고싶다 돌겠어",
          "speaker_id": "1",
          "time": "20210901 17:42"
        },
        {
          "id": "MDRW2100037734.1.11",
          "form": "너 추위 괜찮겠니?",
          "original_form": "너 추위 괜찮겠니?",
          "speaker_id": "2",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100037734.1.12",
          "form": "다음당근 또 있니",
          "original_form": "다음당근 또 있니",
          "speaker_id": "2",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100037734.1.13",
          "form": "6시반에 또 이 더위를 헤쳐나가(?!) 거래를 해야해",
          "original_form": "6시반에 또 이 더위를 헤쳐나가(?!) 거래를 해야해",
          "speaker_id": "1",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100037734.1.14",
          "form": "사시나무는자신이없지만더위가싫어요..",
          "original_form": "사시나무는자신이없지만더위가싫어요..",
          "speaker_id": "1",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100037734.1.15",
          "form": "저도 더위시러요",
          "original_form": "저도 더위시러요",
          "speaker_id": "2",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100037734.1.16",
          "form": "비오니 시원하네여",
          "original_form": "비오니 시원하네여",
          "speaker_id": "2",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100037734.1.17",
          "form": "근듸ㅣ침수될까두려워요",
          "original_form": "근듸ㅣ침수될까두려워요",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100037734.1.18",
          "form": "이제그쳘어요",
          "original_form": "이제그쳘어요",
          "speaker_id": "2",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100037734.1.19",
          "form": "오여기지금많이오고있어요",
          "original_form": "오여기지금많이오고있어요",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100037734.1.20",
          "form": "오정말요ㅠㅠ",
          "original_form": "오정말요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100037734.1.21",
          "form": "아 name4이가 신라면 극찬햇니",
