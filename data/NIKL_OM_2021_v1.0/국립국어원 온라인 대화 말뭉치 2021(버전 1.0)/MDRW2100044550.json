      "utterance": [
        {
          "id": "MDRW2100044550.1.1",
          "form": "가보고싶은 곳 있으신가요?",
          "original_form": "가보고싶은 곳 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044550.1.2",
          "form": "name2님 코로나 끝나면 어디 여행 가고 싶으세요?",
          "original_form": "&name2&님 코로나 끝나면 어디 여행 가고 싶으세요?",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044550.1.3",
          "form": "ㅋㅋㅋ 저는 유럽이요!",
          "original_form": "ㅋㅋㅋ 저는 유럽이요!",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044550.1.4",
          "form": "저도 유럽이요!",
          "original_form": "저도 유럽이요!",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044550.1.5",
          "form": "전 스페인 가고싶어요",
          "original_form": "전 스페인 가고싶어요",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044550.1.6",
          "form": "요기도 스페인 가고 싶은 분이 계시네요!",
          "original_form": "요기도 스페인 가고 싶은 분이 계시네요!",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044550.1.7",
          "form": "저도 스페인 축제 기간에 가보고 싶어요",
          "original_form": "저도 스페인 축제 기간에 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044550.1.8",
          "form": "name2님은 왜 가고 싶으세요?",
          "original_form": "&name2&님은 왜 가고 싶으세요?",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044550.1.9",
          "form": "갔다온 사람들이 다 추천하더라구요",
          "original_form": "갔다온 사람들이 다 추천하더라구요",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044550.1.10",
          "form": "음식도 맛있고 볼거리도 많다고 해서요!",
          "original_form": "음식도 맛있고 볼거리도 많다고 해서요!",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044550.1.11",
          "form": "주변에 다녀오신 분들이 계시군요!",
          "original_form": "주변에 다녀오신 분들이 계시군요!",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044550.1.12",
          "form": "오오!! 이전에 해외여행 어디 다녀오셨어요?",
          "original_form": "오오!! 이전에 해외여행 어디 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044550.1.13",
          "form": "유럽이랑 일본이요",
          "original_form": "유럽이랑 일본이요",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044550.1.14",
          "form": "해외여행 어디 가셨나요?",
          "original_form": "해외여행 어디 가셨나요?",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044550.1.15",
          "form": "저는 기억에 없지만 캐나다와 일본이요",
          "original_form": "저는 기억에 없지만 캐나다와 일본이요",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044550.1.16",
          "form": "유럽 어디다녀오셨어요?",
          "original_form": "유럽 어디다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044550.1.17",
          "form": "배낭여행으로 영국에서부터 이탈리아까지 갔었어요!",
          "original_form": "배낭여행으로 영국에서부터 이탈리아까지 갔었어요!",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044550.1.18",
          "form": "캐나다 어떠셨나요?",
          "original_form": "캐나다 어떠셨나요?",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044550.1.19",
          "form": "안타깝게도 캐나다는... 제가 걷지도 못할때 실려갔다가 왔답니다 ㅋㅋ",
          "original_form": "안타깝게도 캐나다는... 제가 걷지도 못할때 실려갔다가 왔답니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044550.1.20",
          "form": "name2님 여행하신것 들려주세요!",
          "original_form": "&name2&님 여행하신것 들려주세요!",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044550.1.21",
          "form": "앗 ㅋㅋㅋㅋㅋㅋㅋ 그런 비화가...",
          "original_form": "앗 ㅋㅋㅋㅋㅋㅋㅋ 그런 비화가...",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044550.1.22",
          "form": "일본 어디가셨어요?",
          "original_form": "일본 어디가셨어요?",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044550.1.23",
          "form": "저는 일본은 도쿄 갔었어요",
          "original_form": "저는 일본은 도쿄 갔었어요",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044550.1.24",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044550.1.25",
          "form": "저도 일본 도쿄랑 오사카 갔었어요!",
          "original_form": "저도 일본 도쿄랑 오사카 갔었어요!",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044550.1.26",
          "form": "오사카!",
          "original_form": "오사카!",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044550.1.27",
          "form": "거기도 가보고 싶었는데 ㅠ 시간과 일정상 어쩔 수 없이 가보지 못했어요 ㅠ",
          "original_form": "거기도 가보고 싶었는데 ㅠ 시간과 일정상 어쩔 수 없이 가보지 못했어요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044550.1.28",
          "form": "도쿄는 어디 가셨어요??",
          "original_form": "도쿄는 어디 가셨어요??",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044550.1.29",
          "form": "진짜 온갖곳 다갔어요 ㅋㅋㅋ",
          "original_form": "진짜 온갖곳 다갔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044550.1.30",
          "form": "도쿄투어 급으로 ㅋㅋㅋ",
          "original_form": "도쿄투어 급으로 ㅋㅋㅋ",
          "speaker_id": "1",
