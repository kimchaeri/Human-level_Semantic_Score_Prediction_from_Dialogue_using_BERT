      "utterance": [
        {
          "id": "MDRW2100022602.1.1",
          "form": "꺄~~~",
          "original_form": "꺄~~~",
          "speaker_id": "1",
          "time": "20210829 16:11"
        },
        {
          "id": "MDRW2100022602.1.2",
          "form": "저 요즘에 읽고있는 책이",
          "original_form": "저 요즘에 읽고있는 책이",
          "speaker_id": "1",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100022602.1.3",
          "form": "<부의 추월차선> 이란 책인데",
          "original_form": "<부의 추월차선> 이란 책인데",
          "speaker_id": "1",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100022602.1.4",
          "form": "들어보셨나요???",
          "original_form": "들어보셨나요???",
          "speaker_id": "1",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100022602.1.5",
          "form": "처음 들어봤어요!",
          "original_form": "처음 들어봤어요!",
          "speaker_id": "2",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100022602.1.6",
          "form": "소설은 아닐 것 같고 재테크 책인가요??",
          "original_form": "소설은 아닐 것 같고 재테크 책인가요??",
          "speaker_id": "2",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100022602.1.7",
          "form": "네네!!! 맞아요",
          "original_form": "네네!!! 맞아요",
          "speaker_id": "1",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100022602.1.8",
          "form": "자기계발 도서인데",
          "original_form": "자기계발 도서인데",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022602.1.9",
          "form": "대충 줄거리가 뭐냐면...",
          "original_form": "대충 줄거리가 뭐냐면...",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022602.1.10",
          "form": "부자가 되기 위해선",
          "original_form": "부자가 되기 위해선",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022602.1.11",
          "form": "착실히 조금씩 모으는 것보다",
          "original_form": "착실히 조금씩 모으는 것보다",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022602.1.12",
          "form": "기하급수적으로 벌어들여야한다",
          "original_form": "기하급수적으로 벌어들여야한다",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022602.1.13",
          "form": "이런 이야기예요 ㅋㅋㅋㅋ",
          "original_form": "이런 이야기예요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022602.1.14",
          "form": "오! 근데 그게 현실적으로 가능한 얘기일까요ㅠㅠ",
          "original_form": "오! 근데 그게 현실적으로 가능한 얘기일까요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022602.1.15",
          "form": "내 주식은 언제쯤 대박을 터뜨릴까요 ㅋㅋㅋㅋㅋ",
          "original_form": "내 주식은 언제쯤 대박을 터뜨릴까요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022602.1.16",
          "form": "그니까요...",
          "original_form": "그니까요...",
          "speaker_id": "1",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022602.1.17",
          "form": "작가는 보니까",
          "original_form": "작가는 보니까",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022602.1.18",
          "form": "홈페이지 만들어서",
          "original_form": "홈페이지 만들어서",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022602.1.19",
          "form": "사업 비슷하게 하다가 돈을 벌었더라구요",
          "original_form": "사업 비슷하게 하다가 돈을 벌었더라구요",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022602.1.20",
          "form": "껄껄....",
          "original_form": "껄껄....",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022602.1.21",
          "form": "역시 기술이 있으니까 그런 소리를 하네요ㅠ ㅋㅋㅋㅋ",
          "original_form": "역시 기술이 있으니까 그런 소리를 하네요ㅠ ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100022602.1.22",
          "form": "이제라도 코딩을 배워야할까요.... ㅋㅋ",
          "original_form": "이제라도 코딩을 배워야할까요.... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100022602.1.23",
          "form": "그쵸.....",
          "original_form": "그쵸.....",
          "speaker_id": "1",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100022602.1.24",
          "form": "사실 저도 작자가 뭘로 돈 벌었는지ㅜ듣고",
          "original_form": "사실 저도 작자가 뭘로 돈 벌었는지ㅜ듣고",
          "speaker_id": "1",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100022602.1.25",
          "form": "좀 짜게 식었어요 ㅎㅎ",
          "original_form": "좀 짜게 식었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100022602.1.26",
          "form": "닌텐도에 코딩 게임도 나왔대요,,",
          "original_form": "닌텐도에 코딩 게임도 나왔대요,,",
          "speaker_id": "1",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100022602.1.27",
          "form": "코딩으로 게임을요...?",
          "original_form": "코딩으로 게임을요...?",
          "speaker_id": "2",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100022602.1.28",
          "form": "이렇게 벌써 차별 당하나요ㅠㅠ",
          "original_form": "이렇게 벌써 차별 당하나요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100022602.1.29",
          "form": "코딩이 교육과정에 없던 세대는 슬프네요ㅠㅠ",
          "original_form": "코딩이 교육과정에 없던 세대는 슬프네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100022602.1.30",
          "form": "ㅋㅋㅋㅋㅋ 아 그러니까요~~~!",
          "original_form": "ㅋㅋㅋㅋㅋ 아 그러니까요~~~!",
          "speaker_id": "1",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100022602.1.31",
          "form": "저는 대학생 때",
          "original_form": "저는 대학생 때",
          "speaker_id": "1",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100022602.1.32",
          "form": "코딩의 존재에 대해 겨우 알았는데 말이죠ㅠ",
          "original_form": "코딩의 존재에 대해 겨우 알았는데 말이죠ㅠ",
          "speaker_id": "1",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100022602.1.33",
          "form": "나중에 막 키오스크 같은거 몇 대 세워놓고 자체 코딩해서 용건 해결하라고 할 것 같네요 ㅋㅋㅋㅋ",
          "original_form": "나중에 막 키오스크 같은거 몇 대 세워놓고 자체 코딩해서 용건 해결하라고 할 것 같네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:50"
        },
        {
          "id": "MDRW2100022602.1.34",
          "form": "아나 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아나 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:51"
        },
        {
          "id": "MDRW2100022602.1.35",
          "form": "진짜 그런 일이 있을 수도 있겠네요",
          "original_form": "진짜 그런 일이 있을 수도 있겠네요",
          "speaker_id": "1",
          "time": "20210829 20:51"
        },
        {
          "id": "MDRW2100022602.1.36",
          "form": "세상이 점점 바뀌는데",
          "original_form": "세상이 점점 바뀌는데",
          "speaker_id": "1",
          "time": "20210829 20:51"
        },
        {
          "id": "MDRW2100022602.1.37",
          "form": "전 아직도 전자책보단 종이책이 더 좋아요!",
          "original_form": "전 아직도 전자책보단 종이책이 더 좋아요!",
          "speaker_id": "1",
          "time": "20210829 20:51"
        },
        {
          "id": "MDRW2100022602.1.38",
          "form": "저 그래서 이북리더기 팔았어요 ㅋㅋㅋㅋ",
          "original_form": "저 그래서 이북리더기 팔았어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:52"
        },
        {
          "id": "MDRW2100022602.1.39",
          "form": "책 넘어갈 때 그 화소찌꺼기가 남는걸 못 견디겠더라구요 ㅎㅎ",
          "original_form": "책 넘어갈 때 그 화소찌꺼기가 남는걸 못 견디겠더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:52"
        },
        {
          "id": "MDRW2100022602.1.40",
          "form": "책처럼 깔끔하게 넘어가지도 못하고 ㅎ",
          "original_form": "책처럼 깔끔하게 넘어가지도 못하고 ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100022602.1.41",
          "form": "오오 화소찌꺼기요?!",
          "original_form": "오오 화소찌꺼기요?!",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100022602.1.42",
          "form": "제가 사용해보지 않아서 그런지",
          "original_form": "제가 사용해보지 않아서 그런지",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100022602.1.43",
          "form": "정말 생소하네요!!! 생각해보지 못한 부분이에요!",
          "original_form": "정말 생소하네요!!! 생각해보지 못한 부분이에요!",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100022602.1.44",
          "form": "역시 책은 도서관에서 빌려보는 게 가장 좋은건가봐요 ",
          "original_form": "역시 책은 도서관에서 빌려보는 게 가장 좋은건가봐요 ",
          "speaker_id": "1",
          "time": "20210829 21:44"
        }
      ]
    }
  ]
