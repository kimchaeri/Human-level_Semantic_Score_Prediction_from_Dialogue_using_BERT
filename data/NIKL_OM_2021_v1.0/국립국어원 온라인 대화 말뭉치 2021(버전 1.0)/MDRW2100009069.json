      "utterance": [
        {
          "id": "MDRW2100009069.1.1",
          "form": "정말 떡볶이 맛집이란 가격과 맛이 합쳐져야죠",
          "original_form": "정말 떡볶이 맛집이란 가격과 맛이 합쳐져야죠",
          "speaker_id": "2",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009069.1.2",
          "form": "인정입니다 ㅎㅎ",
          "original_form": "인정입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009069.1.3",
          "form": "맛집이네요",
          "original_form": "맛집이네요",
          "speaker_id": "2",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009069.1.4",
          "form": "막상 대구 떡볶이 맛집 검색하면 다른할매떡볶이가 나오는데",
          "original_form": "막상 대구 떡볶이 맛집 검색하면 다른할매떡볶이가 나오는데",
          "speaker_id": "1",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009069.1.5",
          "form": "여기는 아는사람만아는듯요",
          "original_form": "여기는 아는사람만아는듯요",
          "speaker_id": "1",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009069.1.6",
          "form": "맞아요 이런게 참 아쉬워요",
          "original_form": "맞아요 이런게 참 아쉬워요",
          "speaker_id": "2",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009069.1.7",
          "form": "진짜 맛집은 다른곳이죠",
          "original_form": "진짜 맛집은 다른곳이죠",
          "speaker_id": "2",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009069.1.8",
          "form": "직원수도 많아요 ㅎ",
          "original_form": "직원수도 많아요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009069.1.9",
          "form": "아는 사람만 알때가 ㅈㅗㅎ아요",
          "original_form": "아는 사람만 알때가 ㅈㅗㅎ아요",
          "speaker_id": "2",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009069.1.10",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009069.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009069.1.12",
          "form": "떡볶이집에 비하믄요",
          "original_form": "떡볶이집에 비하믄요",
          "speaker_id": "1",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009069.1.13",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100009069.1.14",
          "form": "직원들도 많으면 박리다매로 많이 파는거네요",
          "original_form": "직원들도 많으면 박리다매로 많이 파는거네요",
          "speaker_id": "2",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100009069.1.15",
          "form": "요리하는 아지매 4~5명되는거같고\r\n주문받는 아저씨 1명",
          "original_form": "요리하는 아지매 4~5명되는거같고\r\n주문받는 아저씨 1명",
          "speaker_id": "1",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100009069.1.16",
          "form": "진짜 힘드시겠다",
          "original_form": "진짜 힘드시겠다",
          "speaker_id": "2",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100009069.1.17",
          "form": "엥 와 대박 이네요",
          "original_form": "엥 와 대박 이네요",
          "speaker_id": "2",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100009069.1.18",
          "form": "진짜 큰집이네요",
          "original_form": "진짜 큰집이네요",
          "speaker_id": "2",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100009069.1.19",
          "form": "테이블정리하시는분 2분정도 되시는거같아요",
          "original_form": "테이블정리하시는분 2분정도 되시는거같아요",
          "speaker_id": "1",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100009069.1.20",
          "form": "그정도 되겠네요",
          "original_form": "그정도 되겠네요",
          "speaker_id": "2",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100009069.1.21",
          "form": "요즘엔 코로나땜에 테이크아웃만해서\r\n요즘은 변했을지도 모르겠지만요",
          "original_form": "요즘엔 코로나땜에 테이크아웃만해서\r\n요즘은 변했을지도 모르겠지만요",
          "speaker_id": "1",
          "time": "20210822 20:59"
        },
        {
          "id": "MDRW2100009069.1.22",
          "form": "그렇게 직원들 월급주고 남는다니 정말 많이 팔리나봐요",
          "original_form": "그렇게 직원들 월급주고 남는다니 정말 많이 팔리나봐요",
          "speaker_id": "2",
          "time": "20210822 20:59"
        },
        {
          "id": "MDRW2100009069.1.23",
          "form": "네 요샌 배달로 전환한데나 테이크 아웃 잘되는곳만 살아 남았대요",
          "original_form": "네 요샌 배달로 전환한데나 테이크 아웃 잘되는곳만 살아 남았대요",
          "speaker_id": "2",
          "time": "20210822 20:59"
        },
        {
          "id": "MDRW2100009069.1.24",
          "form": "신랑이 사러가니 테이크아웃줄이 5명정도 있더래요 ㅎ",
          "original_form": "신랑이 사러가니 테이크아웃줄이 5명정도 있더래요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 20:59"
        },
        {
          "id": "MDRW2100009069.1.25",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100009069.1.26",
          "form": "진짜 넘 궁금해요",
          "original_form": "진짜 넘 궁금해요",
          "speaker_id": "2",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100009069.1.27",
          "form": "먹고싶어요",
          "original_form": "먹고싶어요",
          "speaker_id": "2",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100009069.1.28",
          "form": "신랑도 월급주고 남는지 궁금하다고 하더라구요",
          "original_form": "신랑도 월급주고 남는지 궁금하다고 하더라구요",
          "speaker_id": "1",
          "time": "20210822 21:00"
        }
      ]
