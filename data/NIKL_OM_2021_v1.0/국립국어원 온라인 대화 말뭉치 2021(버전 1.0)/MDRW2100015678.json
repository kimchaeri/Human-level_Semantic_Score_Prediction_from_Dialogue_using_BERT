      "utterance": [
        {
          "id": "MDRW2100015678.1.1",
          "form": "초코라떼 꿀 맛",
          "original_form": "초코라떼 꿀 맛",
          "speaker_id": "2",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015678.1.2",
          "form": "초코라떼?",
          "original_form": "초코라떼?",
          "speaker_id": "1",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015678.1.3",
          "form": "어디꺼가 맛있어?",
          "original_form": "어디꺼가 맛있어?",
          "speaker_id": "1",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015678.1.4",
          "form": "난 할리스나 이디야가 맛있엉",
          "original_form": "난 할리스나 이디야가 맛있엉",
          "speaker_id": "2",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015678.1.5",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "1",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015678.1.6",
          "form": "휘핑 때문인 것 같아",
          "original_form": "휘핑 때문인 것 같아",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015678.1.7",
          "form": "휘핑이 왜?",
          "original_form": "휘핑이 왜?",
          "speaker_id": "1",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015678.1.8",
          "form": "많이 올리면 달달해서 맛있거든",
          "original_form": "많이 올리면 달달해서 맛있거든",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015678.1.9",
          "form": "나도 휘핑 좋아해.",
          "original_form": "나도 휘핑 좋아해.",
          "speaker_id": "1",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015678.1.10",
          "form": "아 넌 무슨 음료 좋아해?",
          "original_form": "아 넌 무슨 음료 좋아해?",
          "speaker_id": "2",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015678.1.11",
          "form": "나는 초콜릿라떼",
          "original_form": "나는 초콜릿라떼",
          "speaker_id": "1",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015678.1.12",
          "form": "너도?",
          "original_form": "너도?",
          "speaker_id": "2",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015678.1.13",
          "form": "이디야꺼가 맛있어.",
          "original_form": "이디야꺼가 맛있어.",
          "speaker_id": "1",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015678.1.14",
          "form": "그리고 또 몰 좋아해?",
          "original_form": "그리고 또 몰 좋아해?",
          "speaker_id": "2",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015678.1.15",
          "form": "나 초코라떼가 제일 좋은데 아이스티를 제일 많이 먹어",
          "original_form": "나 초코라떼가 제일 좋은데 아이스티를 제일 많이 먹어",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015678.1.16",
          "form": "복숭아 아이스티?",
          "original_form": "복숭아 아이스티?",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015678.1.17",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015678.1.18",
          "form": "왜 많이 마시니?",
          "original_form": "왜 많이 마시니?",
          "speaker_id": "2",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015678.1.19",
          "form": "가장 저렴하고 맛도 나쁘지 않거든",
          "original_form": "가장 저렴하고 맛도 나쁘지 않거든",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015678.1.20",
          "form": "아 맞아 여름에 특히 맛있는 것 같아",
          "original_form": "아 맞아 여름에 특히 맛있는 것 같아",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015678.1.21",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "1",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015678.1.22",
          "form": "아 내가 음료 추천해줄까ㅡ",
          "original_form": "아 내가 음료 추천해줄까ㅡ",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015678.1.23",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015678.1.24",
          "form": "해줘",
          "original_form": "해줘",
          "speaker_id": "1",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015678.1.25",
          "form": "레몬셔벗블렌디드 마셔봐!",
          "original_form": "레몬셔벗블렌디드 마셔봐!",
          "speaker_id": "2",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015678.1.26",
          "form": "그건 어디꺼야?",
          "original_form": "그건 어디꺼야?",
          "speaker_id": "1",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015678.1.27",
          "form": "투썸플레이스",
          "original_form": "투썸플레이스",
          "speaker_id": "2",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015678.1.28",
          "form": "나 어제 연어초밥 먹었다",
          "original_form": "나 어제 연어초밥 먹었다",
          "speaker_id": "1",
