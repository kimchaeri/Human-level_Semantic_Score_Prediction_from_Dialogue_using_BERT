      "utterance": [
        {
          "id": "MDRW2100039565.1.1",
          "form": "저는 가본 곳도 또 가고 싶어요",
          "original_form": "저는 가본 곳도 또 가고 싶어요",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100039565.1.2",
          "form": "제주도도 가고 싶구",
          "original_form": "제주도도 가고 싶구",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100039565.1.3",
          "form": "나도나도! 나 부산 예전에 가봤는데",
          "original_form": "나도나도! 나 부산 예전에 가봤는데",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039565.1.4",
          "form": "국내여행이라도 가고 싶어요ㅜㅜ",
          "original_form": "국내여행이라도 가고 싶어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039565.1.5",
          "form": "오 어때요?",
          "original_form": "오 어때요?",
          "speaker_id": "1",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039565.1.6",
          "form": "뭔가 못 가게 되니까 또 가고싶어ㅠㅠ",
          "original_form": "뭔가 못 가게 되니까 또 가고싶어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039565.1.7",
          "form": "제주도도 고등학교 때 가보고.. 안 가봤다ㅠㅠㅠㅠ",
          "original_form": "제주도도 고등학교 때 가보고.. 안 가봤다ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039565.1.8",
          "form": "부산 되게 좋았어",
          "original_form": "부산 되게 좋았어",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039565.1.9",
          "form": "맛있는 것도 많고!",
          "original_form": "맛있는 것도 많고!",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039565.1.10",
          "form": "우와아 부산 뭐가 젤 맛나요?",
          "original_form": "우와아 부산 뭐가 젤 맛나요?",
          "speaker_id": "1",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039565.1.11",
          "form": "난 밀면이 제일 맛있었어!",
          "original_form": "난 밀면이 제일 맛있었어!",
          "speaker_id": "2",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039565.1.12",
          "form": "밀면 맛있겠당",
          "original_form": "밀면 맛있겠당",
          "speaker_id": "1",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039565.1.13",
          "form": "약간 비빔 냉면같은 느낌인데 면이 굵어",
          "original_form": "약간 비빔 냉면같은 느낌인데 면이 굵어",
          "speaker_id": "2",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039565.1.14",
          "form": "완전 맛있어 ㅠㅠ 우리도 나중에 가자",
          "original_form": "완전 맛있어 ㅠㅠ 우리도 나중에 가자",
          "speaker_id": "2",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039565.1.15",
          "form": "저는 그거 생각나요 부산 물방울떡!",
          "original_form": "저는 그거 생각나요 부산 물방울떡!",
          "speaker_id": "1",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039565.1.16",
          "form": "오 좋아요 나중에 같이 가요옹",
          "original_form": "오 좋아요 나중에 같이 가요옹",
          "speaker_id": "1",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039565.1.17",
          "form": "헐 그건 뭐야? 처음 들어봠",
          "original_form": "헐 그건 뭐야? 처음 들어봠",
          "speaker_id": "2",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039565.1.18",
          "form": "물방울떡이라니 벌써 입에 침고여",
          "original_form": "물방울떡이라니 벌써 입에 침고여",
          "speaker_id": "2",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039565.1.19",
          "form": "진짜 귀여워요 그거 ㅋㅋㅋ",
          "original_form": "진짜 귀여워요 그거 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039565.1.20",
          "form": "동글동글하고 근데 보기에만 귀엽긴 해요 맛은 보통?",
          "original_form": "동글동글하고 근데 보기에만 귀엽긴 해요 맛은 보통?",
          "speaker_id": "1",
          "time": "20210903 19:13"
        },
        {
          "id": "MDRW2100039565.1.21",
          "form": "오오 귀여운 모양이라니",
          "original_form": "오오 귀여운 모양이라니",
          "speaker_id": "2",
          "time": "20210903 19:13"
        },
        {
          "id": "MDRW2100039565.1.22",
          "form": "약간 조랭이떡 같은 귀여움이려나?",
          "original_form": "약간 조랭이떡 같은 귀여움이려나?",
          "speaker_id": "2",
          "time": "20210903 19:13"
        },
        {
          "id": "MDRW2100039565.1.23",
          "form": "떡인 중간만 가도 맛있는 것 같아 ㅎㅎㅎ",
          "original_form": "떡인 중간만 가도 맛있는 것 같아 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:13"
        },
        {
          "id": "MDRW2100039565.1.24",
          "form": "떡은.....ㅎㅎ",
          "original_form": "떡은.....ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:13"
        },
        {
          "id": "MDRW2100039565.1.25",
          "form": "네네 약간 투명하고 이뽀요",
          "original_form": "네네 약간 투명하고 이뽀요",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039565.1.26",
          "form": "은주는 가본 곳 중에서 제일 또 가고싶은 데가 어디야?",
          "original_form": "은주는 가본 곳 중에서 제일 또 가고싶은 데가 어디야?",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039565.1.27",
          "form": "저는 헝가리요!",
          "original_form": "저는 헝가리요!",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039565.1.28",
          "form": "오호 다음에 우리 같이 부산 가면 먹어보자!",
          "original_form": "오호 다음에 우리 같이 부산 가면 먹어보자!",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039565.1.29",
          "form": "야경이 예뻤어요~",
          "original_form": "야경이 예뻤어요~",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039565.1.30",
          "form": "헐 대박 헝가리!!!!!",
          "original_form": "헐 대박 헝가리!!!!!",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039565.1.31",
          "form": "좋어요!!",
          "original_form": "좋어요!!",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039565.1.32",
          "form": "헝가리 추천이요옹",
          "original_form": "헝가리 추천이요옹",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039565.1.33",
          "form": "어디쯤에 있지??",
          "original_form": "어디쯤에 있지??",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039565.1.34",
          "form": "헝가리는 그냥 다 제 취향이었어요 ㅋㅋ",
          "original_form": "헝가리는 그냥 다 제 취향이었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:15"
        }
      ]
