      "utterance": [
        {
          "id": "MDRW2100024605.1.1",
          "form": "안녕하세요 오늘은 비가많이오네요",
          "original_form": "안녕하세요 오늘은 비가많이오네요",
          "speaker_id": "1",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100024605.1.2",
          "form": "그러게요... 해가 존재하는지 궁금할 정도로 그 모습을 볼 수가 없네요",
          "original_form": "그러게요... 해가 존재하는지 궁금할 정도로 그 모습을 볼 수가 없네요",
          "speaker_id": "2",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100024605.1.3",
          "form": "그래서 쌀쌀한가봐요ㅠ",
          "original_form": "그래서 쌀쌀한가봐요ㅠ",
          "speaker_id": "1",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100024605.1.4",
          "form": "맞아요 오늘은 유독 춥네요",
          "original_form": "맞아요 오늘은 유독 춥네요",
          "speaker_id": "2",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100024605.1.5",
          "form": "이제야 가을인게 실감나요",
          "original_form": "이제야 가을인게 실감나요",
          "speaker_id": "1",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100024605.1.6",
          "form": "곧있음 단풍도 들겠군요",
          "original_form": "곧있음 단풍도 들겠군요",
          "speaker_id": "2",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100024605.1.7",
          "form": "올해는 단풍구경갈수있으면 좋겠어요",
          "original_form": "올해는 단풍구경갈수있으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100024605.1.8",
          "form": "맞아요 단풍구경 가고 싶어요",
          "original_form": "맞아요 단풍구경 가고 싶어요",
          "speaker_id": "2",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100024605.1.9",
          "form": "어디 공원이라도 나간다던가",
          "original_form": "어디 공원이라도 나간다던가",
          "speaker_id": "2",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100024605.1.10",
          "form": "겨울되기전에 가을옷도 입구요",
          "original_form": "겨울되기전에 가을옷도 입구요",
          "speaker_id": "1",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100024605.1.11",
          "form": "아깝지않게!",
          "original_form": "아깝지않게!",
          "speaker_id": "1",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100024605.1.12",
          "form": "맞아요 가을옷의 가치를 살려야죠",
          "original_form": "맞아요 가을옷의 가치를 살려야죠",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100024605.1.13",
          "form": "가을너무 짧아서 아쉬워요",
          "original_form": "가을너무 짧아서 아쉬워요",
          "speaker_id": "1",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100024605.1.14",
          "form": "왜 이렇게 짧아지는지 예전엔 이렇지 않았는데 말이죠",
          "original_form": "왜 이렇게 짧아지는지 예전엔 이렇지 않았는데 말이죠",
          "speaker_id": "2",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100024605.1.15",
          "form": "맞아요 적어도 두달정도는 가을이던거같은데..",
          "original_form": "맞아요 적어도 두달정도는 가을이던거같은데..",
          "speaker_id": "1",
          "time": "20210902 14:39"
        },
        {
          "id": "MDRW2100024605.1.16",
          "form": "이젠 한달 정도 되는거 같아요",
          "original_form": "이젠 한달 정도 되는거 같아요",
          "speaker_id": "2",
          "time": "20210902 14:40"
        }
