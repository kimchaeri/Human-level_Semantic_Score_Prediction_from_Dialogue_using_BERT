      "utterance": [
        {
          "id": "MDRW2100039513.1.1",
          "form": "유투브는 주로 뭐 보시나요",
          "original_form": "유투브는 주로 뭐 보시나요",
          "speaker_id": "1",
          "time": "20210903 18:33"
        },
        {
          "id": "MDRW2100039513.1.2",
          "form": "저는 유투브로 주로 노래들어용ㅋㅋㅋ",
          "original_form": "저는 유투브로 주로 노래들어용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100039513.1.3",
          "form": "에센셜? 거기 아시나용",
          "original_form": "에센셜? 거기 아시나용",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100039513.1.4",
          "form": "앗 오! 저그분 구독해둿여용ㅋㅋㅋㅋㅋ",
          "original_form": "앗 오! 저그분 구독해둿여용ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100039513.1.5",
          "form": "근데 요새는 팝송이나 그렁거말고 중국노래자주들어요~",
          "original_form": "근데 요새는 팝송이나 그렁거말고 중국노래자주들어요~",
          "speaker_id": "2",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100039513.1.6",
          "form": "그쵸 잘 모아놨던 ㅋㅋㅋ",
          "original_form": "그쵸 잘 모아놨던 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039513.1.7",
          "form": "헉 왜 중국 가수 좋아하시나요?",
          "original_form": "헉 왜 중국 가수 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039513.1.8",
          "form": "중국어과?",
          "original_form": "중국어과?",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039513.1.9",
          "form": "아눀ㅋㅋㅋ 그건아닌데 최근에 어떤 노래 발견한이후로 약간 편견을 깻거든요~ 드라마도 재밋더라구용ㅋㄴㄴ",
          "original_form": "아눀ㅋㅋㅋ 그건아닌데 최근에 어떤 노래 발견한이후로 약간 편견을 깻거든요~ 드라마도 재밋더라구용ㅋㄴㄴ",
          "speaker_id": "2",
          "time": "20210903 18:40"
        },
        {
          "id": "MDRW2100039513.1.10",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100039513.1.11",
          "form": "중국 노래 좋아요",
          "original_form": "중국 노래 좋아요",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100039513.1.12",
          "form": "저는 루한 팬이거든요...ㅎㅎ",
          "original_form": "저는 루한 팬이거든요...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100039513.1.13",
          "form": "어떤 드라마가 재밋던가요",
          "original_form": "어떤 드라마가 재밋던가요",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100039513.1.14",
          "form": "아하~~ 저는 젤최근에 암격리적비밀보고잇는데 이고 재밋어용ㅋㅋㅋ",
          "original_form": "아하~~ 저는 젤최근에 암격리적비밀보고잇는데 이고 재밋어용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100039513.1.15",
          "form": "앗 그러고보니 샤오루란이름 루한좋아하셔서그렇게하신건가봐요??",
          "original_form": "앗 그러고보니 샤오루란이름 루한좋아하셔서그렇게하신건가봐요??",
          "speaker_id": "2",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100039513.1.16",
          "form": "중드보면 체육복이 교복이던데 여긴 진짜 교복이 이쁜편이네요 ㅎㅎ",
          "original_form": "중드보면 체육복이 교복이던데 여긴 진짜 교복이 이쁜편이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039513.1.17",
          "form": "마자요 별명이에요 ㅋㅋ",
          "original_form": "마자요 별명이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:44"
        },
        {
          "id": "MDRW2100039513.1.18",
          "form": "맞아여!!! 저도 중국답지않게 교복보고 넘예쁘다고생각햇어용ㅋㅋㅋ",
          "original_form": "맞아여!!! 저도 중국답지않게 교복보고 넘예쁘다고생각햇어용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100039513.1.19",
          "form": "ㅋㅋㅋㅋ 체육복이 편하긴하지만 몰입방해",
          "original_form": "ㅋㅋㅋㅋ 체육복이 편하긴하지만 몰입방해",
          "speaker_id": "1",
          "time": "20210903 18:46"
        },
        {
          "id": "MDRW2100039513.1.20",
          "form": "맞아여 ㅋㅋ드라마에서 의상 너무중요한거같아요",
          "original_form": "맞아여 ㅋㅋ드라마에서 의상 너무중요한거같아요",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100039513.1.21",
          "form": "상견니도 재밌다던데요",
          "original_form": "상견니도 재밌다던데요",
          "speaker_id": "1",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100039513.1.22",
          "form": "아 상견니도봤어요! 대만드라마중에선 탑인거같아요진짜",
          "original_form": "아 상견니도봤어요! 대만드라마중에선 탑인거같아요진짜",
          "speaker_id": "2",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100039513.1.23",
          "form": "넘잼나죠!상견니 대만드라마중에선 탑인거같아여 진짜!",
          "original_form": "넘잼나죠!상견니 대만드라마중에선 탑인거같아여 진짜!",
          "speaker_id": "2",
          "time": "20210903 18:50"
        }
      ]
    }
