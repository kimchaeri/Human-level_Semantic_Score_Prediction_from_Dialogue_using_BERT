        },
        {
          "id": "MDRW2100031714.1.2",
          "form": "언녕하세요^^",
          "original_form": "언녕하세요^^",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100031714.1.3",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100031714.1.4",
          "form": "경성대때 한번 들어서",
          "original_form": "경성대때 한번 들어서",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100031714.1.5",
          "form": "아 과제땨문에??",
          "original_form": "아 과제땨문에??",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100031714.1.6",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100031714.1.7",
          "form": "괜찮을 줄 알았는데",
          "original_form": "괜찮을 줄 알았는데",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100031714.1.8",
          "form": "전혀 아님",
          "original_form": "전혀 아님",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100031714.1.9",
          "form": "어머..",
          "original_form": "어머..",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100031714.1.10",
          "form": "많이 어려워?",
          "original_form": "많이 어려워?",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100031714.1.11",
          "form": "응...",
          "original_form": "응...",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100031714.1.12",
          "form": "이런...",
          "original_form": "이런...",
          "speaker_id": "1",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100031714.1.13",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "1",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100031714.1.14",
          "form": "교수님이 우리",
          "original_form": "교수님이 우리",
          "speaker_id": "2",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100031714.1.15",
          "form": "내용 대충 알고있는 너한테도 어려운거면",
          "original_form": "내용 대충 알고있는 너한테도 어려운거면",
          "speaker_id": "1",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100031714.1.16",
          "form": "다른학생들한테는 더 어렵지않을까",
          "original_form": "다른학생들한테는 더 어렵지않을까",
          "speaker_id": "1",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100031714.1.17",
          "form": "로스쿨 지원하고 싶어하는 학생인줄 아는것 같음",
          "original_form": "로스쿨 지원하고 싶어하는 학생인줄 아는것 같음",
          "speaker_id": "2",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100031714.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100031714.1.19",
          "form": "사실 경성대때 틀어놓고",
          "original_form": "사실 경성대때 틀어놓고",
          "speaker_id": "2",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100031714.1.20",
          "form": "엄청 곰꼼하게 가르치시나보네",
          "original_form": "엄청 곰꼼하게 가르치시나보네",
          "speaker_id": "1",
          "time": "20210905 19:29"
        },
        {
