        {
          "id": "MDRW2100047518.1.1",
          "form": "일 하고 계시죠?",
          "original_form": "일 하고 계시죠?",
          "speaker_id": "2",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100047518.1.2",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100047518.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100047518.1.4",
          "form": "ㅋㅋ 저듀요 흑",
          "original_form": "ㅋㅋ 저듀요 흑",
          "speaker_id": "2",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100047518.1.5",
          "form": "무슨일 하시나요?",
          "original_form": "무슨일 하시나요?",
          "speaker_id": "1",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100047518.1.6",
          "form": "저 그냘 회사에서 일해요..",
          "original_form": "저 그냘 회사에서 일해요..",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100047518.1.7",
          "form": "별일 아닌...",
          "original_form": "별일 아닌...",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100047518.1.8",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100047518.1.9",
          "form": "저도요...ㅋㅋ마케팅쪽에 있는데 올해를 끝으로 그만두려구요",
          "original_form": "저도요...ㅋㅋ마케팅쪽에 있는데 올해를 끝으로 그만두려구요",
          "speaker_id": "1",
          "time": "20210829 12:18"
        },
        {
          "id": "MDRW2100047518.1.10",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100047518.1.11",
          "form": "그만두는 이유가 있나요",
          "original_form": "그만두는 이유가 있나요",
          "speaker_id": "2",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100047518.1.12",
          "form": "저도 그만두고싶어요..하하",
          "original_form": "저도 그만두고싶어요..하하",
          "speaker_id": "2",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100047518.1.13",
          "form": "그냥 회사가 가망없어보여서요...ㅋㅋ상사들도 짜증나고...",
          "original_form": "그냥 회사가 가망없어보여서요...ㅋㅋ상사들도 짜증나고...",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100047518.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ ㅠ 아하",
          "original_form": "ㅋㅋㅋㅋㅋㅋ ㅠ 아하",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100047518.1.15",
          "form": "이직이에요? 아니면",
          "original_form": "이직이에요? 아니면",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100047518.1.16",
          "form": "그냥 퇴사여?",
          "original_form": "그냥 퇴사여?",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100047518.1.17",
          "form": "퇴사요ㅋㅋㅋ",
          "original_form": "퇴사요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100047518.1.18",
          "form": "저도 이번에 만약 그만두면",
          "original_form": "저도 이번에 만약 그만두면",
          "speaker_id": "2",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100047518.1.19",
          "form": "만약 그만두면 퇴사하실건가욥",
          "original_form": "만약 그만두면 퇴사하실건가욥",
          "speaker_id": "1",
          "time": "20210830 13:06"
        },
        {
          "id": "MDRW2100047518.1.20",
          "form": "넵.. 그럴려고요 ...",
          "original_form": "넵.. 그럴려고요 ...",
          "speaker_id": "2",
          "time": "20210830 15:21"
        },
        {
          "id": "MDRW2100047518.1.21",
          "form": "그리고 쉬고싶어요ㅋㅋㅋㅋㅋㅠ",
          "original_form": "그리고 쉬고싶어요ㅋㅋㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210830 15:21"
        },
        {
          "id": "MDRW2100047518.1.22",
          "form": "근데 그럼 또 취업 어떻게 하나 싶네요",
          "original_form": "근데 그럼 또 취업 어떻게 하나 싶네요",
