      "utterance": [
        {
          "id": "MDRW2100041568.1.1",
          "form": "name2님은 동아리 있나요?",
          "original_form": "&name2&님은 동아리 있나요?",
          "speaker_id": "1",
          "time": "20210906 11:09"
        },
        {
          "id": "MDRW2100041568.1.2",
          "form": "저는 기타 동아리 했었어요",
          "original_form": "저는 기타 동아리 했었어요",
          "speaker_id": "2",
          "time": "20210906 11:19"
        },
        {
          "id": "MDRW2100041568.1.3",
          "form": "우와 기타 잘치셔요?",
          "original_form": "우와 기타 잘치셔요?",
          "speaker_id": "1",
          "time": "20210906 11:28"
        },
        {
          "id": "MDRW2100041568.1.4",
          "form": "도레미만 뗐어요ㅋㅋㅋ",
          "original_form": "도레미만 뗐어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:06"
        },
        {
          "id": "MDRW2100041568.1.5",
          "form": "무슨  곡 배우셨나요?",
          "original_form": "무슨  곡 배우셨나요?",
          "speaker_id": "1",
          "time": "20210906 20:08"
        },
        {
          "id": "MDRW2100041568.1.6",
          "form": "연습곡... 모 그런거였는데 잘 기억 안 나네요 ㅋㅋ",
          "original_form": "연습곡... 모 그런거였는데 잘 기억 안 나네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:10"
        },
        {
          "id": "MDRW2100041568.1.7",
          "form": "재미있으셨나요?",
          "original_form": "재미있으셨나요?",
          "speaker_id": "1",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100041568.1.8",
          "form": "기타 배우는 건 좋았는데 엠티 가서 기합주길래 슬그머니 탈퇴했어요ㅠㅠ",
          "original_form": "기타 배우는 건 좋았는데 엠티 가서 기합주길래 슬그머니 탈퇴했어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100041568.1.9",
          "form": "저런... 요즘에도 똥군기가 있나",
          "original_form": "저런... 요즘에도 똥군기가 있나",
          "speaker_id": "1",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100041568.1.10",
          "form": "것도 동아리에서..",
          "original_form": "것도 동아리에서..",
          "speaker_id": "1",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100041568.1.11",
          "form": "그러니까요.... 그 전까진 없었다고 했는데",
          "original_form": "그러니까요.... 그 전까진 없었다고 했는데",
          "speaker_id": "2",
          "time": "20210906 21:00"
        },
        {
          "id": "MDRW2100041568.1.12",
          "form": "허어..  그럼 바로 윗기수에서 한심한 놈들이 출몰했다는건데",
          "original_form": "허어..  그럼 바로 윗기수에서 한심한 놈들이 출몰했다는건데",
          "speaker_id": "1",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100041568.1.13",
          "form": "전 그런게 진짜 싫어요.",
          "original_form": "전 그런게 진짜 싫어요.",
          "speaker_id": "1",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100041568.1.14",
          "form": "꾹참고 악습을 철폐해놨더니 그걸 지들 좋자고 부활시켜...?  하...",
          "original_form": "꾹참고 악습을 철폐해놨더니 그걸 지들 좋자고 부활시켜...?  하...",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100041568.1.15",
          "form": "저두요 그거 후배들한테 인사 더 잘 받는 거 외에 좋을 점이 머가 있다고 그렇게 부활하는지...",
          "original_form": "저두요 그거 후배들한테 인사 더 잘 받는 거 외에 좋을 점이 머가 있다고 그렇게 부활하는지...",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100041568.1.16",
          "form": "인사도 뭐 기합 안주면 후배들이 안하는것도 아니잖아요",
          "original_form": "인사도 뭐 기합 안주면 후배들이 안하는것도 아니잖아요",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100041568.1.17",
          "form": "기합같은거 없어도 알아서 다들 기본 예의는 지키던데",
          "original_form": "기합같은거 없어도 알아서 다들 기본 예의는 지키던데",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100041568.1.18",
          "form": "앗 지워졌으려나",
          "original_form": "앗 지워졌으려나",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100041568.1.19",
          "form": "저는 분석화학 좋아했어요",
          "original_form": "저는 분석화학 좋아했어요",
          "speaker_id": "2",
          "time": "20210907 18:30"
