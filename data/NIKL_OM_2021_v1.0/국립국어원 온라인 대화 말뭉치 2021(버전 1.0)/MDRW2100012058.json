      "utterance": [
        {
          "id": "MDRW2100012058.1.1",
          "form": "안녕하세요 돼지바 좋아하세요? 넘 반가워요\r\n제 최애 아이스크림 중 하나가 돼지바에요",
          "original_form": "안녕하세요 돼지바 좋아하세요? 넘 반가워요\r\n제 최애 아이스크림 중 하나가 돼지바에요",
          "speaker_id": "1",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100012058.1.2",
          "form": "네 안녕하세요\r\n돼지바 저의 최애입니다",
          "original_form": "네 안녕하세요\r\n돼지바 저의 최애입니다",
          "speaker_id": "2",
          "time": "20210824 18:51"
        },
        {
          "id": "MDRW2100012058.1.3",
          "form": "어떤 아이스크림 좋아하세요",
          "original_form": "어떤 아이스크림 좋아하세요",
          "speaker_id": "2",
          "time": "20210824 18:51"
        },
        {
          "id": "MDRW2100012058.1.4",
          "form": "저 좀 많은데ㅜㅜㅋㅋ 돼지바 쿠앤크 티코 엑설런트 투게더",
          "original_form": "저 좀 많은데ㅜㅜㅋㅋ 돼지바 쿠앤크 티코 엑설런트 투게더",
          "speaker_id": "1",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012058.1.5",
          "form": "붕어싸만코",
          "original_form": "붕어싸만코",
          "speaker_id": "1",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012058.1.6",
          "form": "아 다맛난것들이네요",
          "original_form": "아 다맛난것들이네요",
          "speaker_id": "2",
          "time": "20210824 18:56"
        },
        {
          "id": "MDRW2100012058.1.7",
          "form": "빠바하고 뜨레쥬르.. 어디가 더 맛나나요?",
          "original_form": "빠바하고 뜨레쥬르.. 어디가 더 맛나나요?",
          "speaker_id": "2",
          "time": "20210824 18:56"
        },
        {
          "id": "MDRW2100012058.1.8",
          "form": "저는 개인적으로 빠바를 더 좋아합니다",
          "original_form": "저는 개인적으로 빠바를 더 좋아합니다",
          "speaker_id": "1",
          "time": "20210824 18:57"
        },
        {
          "id": "MDRW2100012058.1.9",
          "form": "특히 생크림케익류는 빠바",
          "original_form": "특히 생크림케익류는 빠바",
          "speaker_id": "1",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100012058.1.10",
          "form": "돼지바님은요?",
          "original_form": "돼지바님은요?",
          "speaker_id": "1",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100012058.1.11",
          "form": "저도 빠바요 ㅎ",
          "original_form": "저도 빠바요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100012058.1.12",
          "form": "집앞에 뜨레쥬르만 있어서 아쉬워요",
          "original_form": "집앞에 뜨레쥬르만 있어서 아쉬워요",
          "speaker_id": "2",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100012058.1.13",
          "form": "뚜쥬는 파운드 롤케익이 맛있어요",
          "original_form": "뚜쥬는 파운드 롤케익이 맛있어요",
          "speaker_id": "1",
          "time": "20210824 19:00"
        },
        {
          "id": "MDRW2100012058.1.14",
          "form": "옛날사라다빵인가 그것도요",
          "original_form": "옛날사라다빵인가 그것도요",
          "speaker_id": "1",
          "time": "20210824 19:00"
        },
        {
          "id": "MDRW2100012058.1.15",
          "form": "옥수수식빵도 뜨레주르가 맛나요",
          "original_form": "옥수수식빵도 뜨레주르가 맛나요",
          "speaker_id": "2",
          "time": "20210824 19:01"
        },
        {
          "id": "MDRW2100012058.1.16",
          "form": "사라다빵맛나요 ㅋ",
          "original_form": "사라다빵맛나요 ㅋ",
          "speaker_id": "2",
          "time": "20210824 19:01"
        },
        {
          "id": "MDRW2100012058.1.17",
          "form": "저녁뭐드세요?",
          "original_form": "저녁뭐드세요?",
          "speaker_id": "2",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100012058.1.18",
          "form": "저는 콩나물 계란 넣고 라면이요 돼지바님은요?",
          "original_form": "저는 콩나물 계란 넣고 라면이요 돼지바님은요?",
          "speaker_id": "1",
          "time": "20210824 19:03"
        },
        {
          "id": "MDRW2100012058.1.19",
          "form": "소고기국 끓였어요 파 듬뿍넣어서요",
          "original_form": "소고기국 끓였어요 파 듬뿍넣어서요",
          "speaker_id": "2",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100012058.1.20",
          "form": "와 듣기만해도 군침이\r\n맛있겠어요",
          "original_form": "와 듣기만해도 군침이\r\n맛있겠어요",
          "speaker_id": "1",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100012058.1.21",
          "form": "네 비오는날에 라면도 맛나겠어요",
          "original_form": "네 비오는날에 라면도 맛나겠어요",
          "speaker_id": "2",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012058.1.22",
          "form": "소고기무국도요 속이 든든해질듯요",
          "original_form": "소고기무국도요 속이 든든해질듯요",
          "speaker_id": "1",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012058.1.23",
          "form": "네 저녁은 든든하게먹을려구요",
          "original_form": "네 저녁은 든든하게먹을려구요",
          "speaker_id": "2",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100012058.1.24",
          "form": "맛있게 드세용",
          "original_form": "맛있게 드세용",
          "speaker_id": "1",
          "time": "20210824 19:11"
        }
      ]
    }
