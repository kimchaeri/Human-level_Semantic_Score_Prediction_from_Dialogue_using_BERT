      "utterance": [
        {
          "id": "MDRW2100046535.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211029 10:52"
        },
        {
          "id": "MDRW2100046535.1.2",
          "form": "안녕하세요! 불금인데 약속 있으신가요!",
          "original_form": "안녕하세요! 불금인데 약속 있으신가요!",
          "speaker_id": "2",
          "time": "20211029 10:53"
        },
        {
          "id": "MDRW2100046535.1.3",
          "form": "네 친구들 만나기로 했어요",
          "original_form": "네 친구들 만나기로 했어요",
          "speaker_id": "1",
          "time": "20211029 10:53"
        },
        {
          "id": "MDRW2100046535.1.4",
          "form": "달려야죠",
          "original_form": "달려야죠",
          "speaker_id": "1",
          "time": "20211029 10:53"
        },
        {
          "id": "MDRW2100046535.1.5",
          "form": "술약속이신가봐요",
          "original_form": "술약속이신가봐요",
          "speaker_id": "2",
          "time": "20211029 10:54"
        },
        {
          "id": "MDRW2100046535.1.6",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211029 10:54"
        },
        {
          "id": "MDRW2100046535.1.7",
          "form": "역시 불금..",
          "original_form": "역시 불금..",
          "speaker_id": "2",
          "time": "20211029 10:54"
        },
        {
          "id": "MDRW2100046535.1.8",
          "form": "여친을 만들어서 여친을 만나야되는데",
          "original_form": "여친을 만들어서 여친을 만나야되는데",
          "speaker_id": "1",
          "time": "20211029 10:54"
        },
        {
          "id": "MDRW2100046535.1.9",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20211029 10:54"
        },
        {
          "id": "MDRW2100046535.1.10",
          "form": "ㅋㅋㅋ친구들 만나서 소개 받으세요",
          "original_form": "ㅋㅋㅋ친구들 만나서 소개 받으세요",
          "speaker_id": "2",
          "time": "20211029 10:54"
        },
        {
          "id": "MDRW2100046535.1.11",
          "form": "소개 받아도 잘안되요 ㅠㅠㅠ",
          "original_form": "소개 받아도 잘안되요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20211029 10:55"
        },
        {
          "id": "MDRW2100046535.1.12",
          "form": "자만추가 맞으실지도 몰라요",
          "original_form": "자만추가 맞으실지도 몰라요",
          "speaker_id": "2",
          "time": "20211029 10:56"
        },
        {
          "id": "MDRW2100046535.1.13",
          "form": "ㅋㅋ 그럴지도",
          "original_form": "ㅋㅋ 그럴지도",
          "speaker_id": "1",
          "time": "20211029 10:56"
        },
        {
          "id": "MDRW2100046535.1.14",
          "form": "불금인데 오늘 뭐하세요?",
          "original_form": "불금인데 오늘 뭐하세요?",
          "speaker_id": "1",
          "time": "20211029 10:56"
        },
        {
          "id": "MDRW2100046535.1.15",
          "form": "저는 일이 많이 있어서..",
          "original_form": "저는 일이 많이 있어서..",
          "speaker_id": "2",
          "time": "20211029 10:57"
        },
        {
          "id": "MDRW2100046535.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20211029 10:57"
        },
        {
          "id": "MDRW2100046535.1.17",
          "form": "불금을 만끽하지 못하겠네요 ㅠ",
          "original_form": "불금을 만끽하지 못하겠네요 ㅠ",
          "speaker_id": "2",
          "time": "20211029 10:57"
        },
        {
          "id": "MDRW2100046535.1.18",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "1",
          "time": "20211029 10:58"
        },
        {
          "id": "MDRW2100046535.1.19",
          "form": "그러게요 그래도 주말 술약 하나 있는걸로 버텨야죠 ",
          "original_form": "그러게요 그래도 주말 술약 하나 있는걸로 버텨야죠 ",
          "speaker_id": "2",
          "time": "20211029 10:58"
        },
        {
          "id": "MDRW2100046535.1.20",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20211029 10:58"
        },
        {
          "id": "MDRW2100046535.1.21",
          "form": "ㅋㅋ불금 잘 보내세요!",
          "original_form": "ㅋㅋ불금 잘 보내세요!",
          "speaker_id": "2",
