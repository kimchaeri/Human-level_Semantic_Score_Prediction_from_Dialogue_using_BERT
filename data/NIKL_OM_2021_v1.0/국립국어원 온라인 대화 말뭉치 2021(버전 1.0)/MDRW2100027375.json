      "utterance": [
        {
          "id": "MDRW2100027375.1.1",
          "form": "물건 구매할때 후기가 도움되요",
          "original_form": "물건 구매할때 후기가 도움되요",
          "speaker_id": "2",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100027375.1.2",
          "form": "저도 후기는 보는 편이에요",
          "original_form": "저도 후기는 보는 편이에요",
          "speaker_id": "1",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100027375.1.3",
          "form": "직접 물건 못보니까요",
          "original_form": "직접 물건 못보니까요",
          "speaker_id": "1",
          "time": "20210902 12:02"
        },
        {
          "id": "MDRW2100027375.1.4",
          "form": "맞아요. 후기 봐야죠",
          "original_form": "맞아요. 후기 봐야죠",
          "speaker_id": "2",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100027375.1.5",
          "form": "근데 사실 그것도 그냥대충 쓴게 많아서ㅋ",
          "original_form": "근데 사실 그것도 그냥대충 쓴게 많아서ㅋ",
          "speaker_id": "1",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100027375.1.6",
          "form": "후기를 여러개 보고 비교해야죠",
          "original_form": "후기를 여러개 보고 비교해야죠",
          "speaker_id": "2",
          "time": "20210902 12:08"
        },
        {
          "id": "MDRW2100027375.1.7",
          "form": "그래서 모하나 사려면 며칠걸려요ㅋ",
          "original_form": "그래서 모하나 사려면 며칠걸려요ㅋ",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027375.1.8",
          "form": "쇼핑에 신중하시군요.",
          "original_form": "쇼핑에 신중하시군요.",
          "speaker_id": "2",
          "time": "20210902 12:10"
        },
        {
          "id": "MDRW2100027375.1.9",
          "form": "그럼요 좋은 물건을 가장저렴하게 샀을때의 뿌듯함이 있죠",
          "original_form": "그럼요 좋은 물건을 가장저렴하게 샀을때의 뿌듯함이 있죠",
          "speaker_id": "1",
          "time": "20210902 12:11"
        },
        {
          "id": "MDRW2100027375.1.10",
          "form": "맞아요. 싸게 사면뿌듯하죠",
          "original_form": "맞아요. 싸게 사면뿌듯하죠",
          "speaker_id": "2",
          "time": "20210902 12:11"
        },
        {
          "id": "MDRW2100027375.1.11",
          "form": "네ㅋㅋ 그래서 싸게 나오면 당장필요없어도 사게되요",
          "original_form": "네ㅋㅋ 그래서 싸게 나오면 당장필요없어도 사게되요",
          "speaker_id": "1",
          "time": "20210902 12:12"
        },
        {
          "id": "MDRW2100027375.1.12",
          "form": "저두요. 바디워시 벌써 6개 구입했어요 ㅋ",
          "original_form": "저두요. 바디워시 벌써 6개 구입했어요 ㅋ",
          "speaker_id": "2",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027375.1.13",
          "form": "쇼핑을 안하는 날이 거의 없는거 같아요",
          "original_form": "쇼핑을 안하는 날이 거의 없는거 같아요",
          "speaker_id": "1",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027375.1.14",
          "form": "자주하게 되는것 같아요",
          "original_form": "자주하게 되는것 같아요",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100027375.1.15",
          "form": "오늘도 아몬드 구매예정이요",
          "original_form": "오늘도 아몬드 구매예정이요",
          "speaker_id": "1",
          "time": "20210902 12:18"
        }
