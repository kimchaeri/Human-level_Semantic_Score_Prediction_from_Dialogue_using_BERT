      "utterance": [
        {
          "id": "MDRW2100030905.1.1",
          "form": "name2님 직장생활 하구 계신가욤?",
          "original_form": "&name2&님 직장생활 하구 계신가욤?",
          "speaker_id": "1",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030905.1.2",
          "form": "대학원이요",
          "original_form": "대학원이요",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030905.1.3",
          "form": "대리시군요",
          "original_form": "대리시군요",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030905.1.4",
          "form": "아 헉 글쿤요. 네네 이제 이직을 하게 되어서 대리로 가게 되었어요 ㅋㅋㅋㅋ",
          "original_form": "아 헉 글쿤요. 네네 이제 이직을 하게 되어서 대리로 가게 되었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030905.1.5",
          "form": "박사 과정이셔요?",
          "original_form": "박사 과정이셔요?",
          "speaker_id": "1",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030905.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030905.1.7",
          "form": "이직 ? 왜햇어요",
          "original_form": "이직 ? 왜햇어요",
          "speaker_id": "2",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030905.1.8",
          "form": "이전에 알던 거래처서 불러주셔서요 ㅋㅋㅋ",
          "original_form": "이전에 알던 거래처서 불러주셔서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030905.1.9",
          "form": "대학원서 어떤거 연구하시나요?!",
          "original_form": "대학원서 어떤거 연구하시나요?!",
          "speaker_id": "1",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030905.1.10",
          "form": "오 더 조은데요?",
          "original_form": "오 더 조은데요?",
          "speaker_id": "2",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030905.1.11",
          "form": "저요...구조해석",
          "original_form": "저요...구조해석",
          "speaker_id": "2",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030905.1.12",
          "form": "헉 데이터쪽요?",
          "original_form": "헉 데이터쪽요?",
          "speaker_id": "1",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100030905.1.13",
          "form": "네네 더 좋은데 가게되었어요 ㅋㅋㅋ",
          "original_form": "네네 더 좋은데 가게되었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100030905.1.14",
          "form": "음..fem해석이용",
          "original_form": "음..fem해석이용",
          "speaker_id": "2",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100030905.1.15",
          "form": "머가더조아요 월급 ?",
          "original_form": "머가더조아요 월급 ?",
          "speaker_id": "2",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100030905.1.16",
          "form": "급여도 그렇고, 회사 규모도 그렇고~",
          "original_form": "급여도 그렇고, 회사 규모도 그렇고~",
          "speaker_id": "1",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030905.1.17",
          "form": "와 방금 뭔지 몰라서 검색해봤는데",
          "original_form": "와 방금 뭔지 몰라서 검색해봤는데",
          "speaker_id": "1",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030905.1.18",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "2",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030905.1.19",
          "form": "소재 관련된것 같기도 하고",
          "original_form": "소재 관련된것 같기도 하고",
          "speaker_id": "1",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030905.1.20",
          "form": "사람도조은가요",
          "original_form": "사람도조은가요",
          "speaker_id": "2",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030905.1.21",
          "form": "제품 관련된것 같기도 하고 많이 딥하네요",
          "original_form": "제품 관련된것 같기도 하고 많이 딥하네요",
          "speaker_id": "1",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030905.1.22",
          "form": "여러가지가 잇습니당",
          "original_form": "여러가지가 잇습니당",
          "speaker_id": "2",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030905.1.23",
          "form": "고럼요. 사람이 이끌어주는거니깐 사람때문에 가긴 가죵",
          "original_form": "고럼요. 사람이 이끌어주는거니깐 사람때문에 가긴 가죵",
          "speaker_id": "1",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030905.1.24",
          "form": "굳굳",
          "original_form": "굳굳",
          "speaker_id": "2",
