      "utterance": [
        {
          "id": "MDRW2100005205.1.1",
          "form": "name2님은 오늘 무슨 맛있는 걸 드셨나요?",
          "original_form": "&name2&님은 오늘 무슨 맛있는 걸 드셨나요?",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005205.1.2",
          "form": "아 전 서대구이 고등어조림하고요",
          "original_form": "아 전 서대구이 고등어조림하고요",
          "speaker_id": "2",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005205.1.3",
          "form": "디저트로 레몬에이드요",
          "original_form": "디저트로 레몬에이드요",
          "speaker_id": "2",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005205.1.4",
          "form": "우와~",
          "original_form": "우와~",
          "speaker_id": "1",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005205.1.5",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005205.1.6",
          "form": "아 초밥 치킨은 저녁이요 ㅋㅋ",
          "original_form": "아 초밥 치킨은 저녁이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005205.1.7",
          "form": "저는 점심에 삼겹살에 비빔국수 먹고",
          "original_form": "저는 점심에 삼겹살에 비빔국수 먹고",
          "speaker_id": "1",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005205.1.8",
          "form": "저녁엔 샐러드를 먹었어요",
          "original_form": "저녁엔 샐러드를 먹었어요",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005205.1.9",
          "form": "세상에 꿀조합이네요",
          "original_form": "세상에 꿀조합이네요",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005205.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005205.1.11",
          "form": "바람직 합니다",
          "original_form": "바람직 합니다",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005205.1.12",
          "form": "점심에 포식해서",
          "original_form": "점심에 포식해서",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005205.1.13",
          "form": "전 아무래도 불량했네요",
          "original_form": "전 아무래도 불량했네요",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005205.1.14",
          "form": "저녁은 양심상ㅋㅋ",
          "original_form": "저녁은 양심상ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005205.1.15",
          "form": "전 계속 포식요 ㅋㅋ",
          "original_form": "전 계속 포식요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005205.1.16",
          "form": "초밥에 치킨이라니",
          "original_form": "초밥에 치킨이라니",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005205.1.17",
          "form": "저 식단도 탐나는걸요",
          "original_form": "저 식단도 탐나는걸요",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005205.1.18",
          "form": "아 저 양심이 없어요 ㅜㅜ어쩌죠",
          "original_form": "아 저 양심이 없어요 ㅜㅜ어쩌죠",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005205.1.19",
          "form": "괜찮아 맛나요",
          "original_form": "괜찮아 맛나요",
          "speaker_id": "2",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005205.1.20",
          "form": "괜찮아요",
          "original_form": "괜찮아요",
          "speaker_id": "2",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005205.1.21",
          "form": "오타가 ㅜㅜ",
          "original_form": "오타가 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005205.1.22",
          "form": "ㅋㅋㅋ맛있으면 0칼로리라니",
          "original_form": "ㅋㅋㅋ맛있으면 0칼로리라니",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005205.1.23",
          "form": "괜찮을 거예요",
          "original_form": "괜찮을 거예요",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005205.1.24",
          "form": "저지금 고민중요",
          "original_form": "저지금 고민중요",
          "speaker_id": "2",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005205.1.25",
          "form": "복숭아 먹을까 하고요 ㅋㅋ",
          "original_form": "복숭아 먹을까 하고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005205.1.26",
          "form": "아 그렇겠죠",
          "original_form": "아 그렇겠죠",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005205.1.27",
          "form": "오 여름밤의 복숭아는",
          "original_form": "오 여름밤의 복숭아는",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005205.1.28",
          "form": "복숭아를 받았는데 엄청크고 맛나요",
          "original_form": "복숭아를 받았는데 엄청크고 맛나요",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005205.1.29",
          "form": "거부할 수 없는 유혹이네요",
          "original_form": "거부할 수 없는 유혹이네요",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005205.1.30",
          "form": "여름 지나면 다시 못먹으니",
          "original_form": "여름 지나면 다시 못먹으니",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005205.1.31",
          "form": "맛있을때 많이 먹어둬야죠",
          "original_form": "맛있을때 많이 먹어둬야죠",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005205.1.32",
          "form": "네 아 딱딱한거 좋아해요",
          "original_form": "네 아 딱딱한거 좋아해요",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005205.1.33",
          "form": "어떤거 좋아 하세요?",
          "original_form": "어떤거 좋아 하세요?",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005205.1.34",
          "form": "저도 어릴땐 물렁복숭아 좋아했는데",
          "original_form": "저도 어릴땐 물렁복숭아 좋아했는데",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005205.1.35",
          "form": "물렁 또는 딱딱",
          "original_form": "물렁 또는 딱딱",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005205.1.36",
          "form": "전 딱딱이요",
          "original_form": "전 딱딱이요",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005205.1.37",
          "form": "요즘은 딱딱한 게 더 좋더라고요",
          "original_form": "요즘은 딱딱한 게 더 좋더라고요",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005205.1.38",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005205.1.39",
          "form": "입맛이 변해요 ㅎㅎ",
          "original_form": "입맛이 변해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005205.1.40",
          "form": "물렁이는 손에 너무 많이 흐르고",
          "original_form": "물렁이는 손에 너무 많이 흐르고",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005205.1.41",
          "form": "한철이라 즐기려고요",
          "original_form": "한철이라 즐기려고요",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005205.1.42",
          "form": "뭔가 음미할 겨를이 없어요",
          "original_form": "뭔가 음미할 겨를이 없어요",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005205.1.43",
          "form": "네 복숭아 드시는 것 저는 찬성!!",
          "original_form": "네 복숭아 드시는 것 저는 찬성!!",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005205.1.44",
          "form": "흘허서 끈적해요",
          "original_form": "흘허서 끈적해요",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005205.1.45",
          "form": "감사합니다 ㅎㅎ",
          "original_form": "감사합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005205.1.46",
          "form": "감동입니다 ㅎㅎ",
          "original_form": "감동입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005205.1.47",
          "form": "커피는 드셨죠?",
          "original_form": "커피는 드셨죠?",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005205.1.48",
          "form": "전 아메리카노 마셨네요",
          "original_form": "전 아메리카노 마셨네요",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005205.1.49",
          "form": "커피는 오늘은 안 마셨네요",
          "original_form": "커피는 오늘은 안 마셨네요",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005205.1.50",
          "form": "지금 마시기엔 너무 늦었고..",
          "original_form": "지금 마시기엔 너무 늦었고..",
          "speaker_id": "1",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005205.1.51",
          "form": "아 전 한잔씩만해요",
          "original_form": "아 전 한잔씩만해요",
          "speaker_id": "2",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005205.1.52",
          "form": "네 잠안와요",
          "original_form": "네 잠안와요",
          "speaker_id": "2",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005205.1.53",
          "form": "저도 하루에 한잔 정도",
          "original_form": "저도 하루에 한잔 정도",
          "speaker_id": "1",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005205.1.54",
          "form": "딱 그게 좋은거 같아요",
          "original_form": "딱 그게 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005205.1.55",
          "form": "주중에 근무할 때는 1일 1커피가 필요한데",
          "original_form": "주중에 근무할 때는 1일 1커피가 필요한데",
          "speaker_id": "1",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005205.1.56",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005205.1.57",
          "form": "주말은 안 마시고 넘어가기도 하네요",
          "original_form": "주말은 안 마시고 넘어가기도 하네요",
          "speaker_id": "1",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005205.1.58",
          "form": "그럴때도 있러야죠",
          "original_form": "그럴때도 있러야죠",
          "speaker_id": "2",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005205.1.59",
          "form": "어차피 근무중엔 피곤해서 마시니까요",
          "original_form": "어차피 근무중엔 피곤해서 마시니까요",
          "speaker_id": "2",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005205.1.60",
          "form": "ㅎㅎ맞아요",
          "original_form": "ㅎㅎ맞아요",
          "speaker_id": "1",
          "time": "20210821 21:20"
        }
      ]
    }
