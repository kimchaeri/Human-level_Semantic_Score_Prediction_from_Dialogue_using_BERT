      "utterance": [
        {
          "id": "MDRW2100014939.1.1",
          "form": "안녕하세요 혹시 신을 믿으시나용",
          "original_form": "안녕하세요 혹시 신을 믿으시나용",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014939.1.2",
          "form": "전 천주교지만",
          "original_form": "전 천주교지만",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014939.1.3",
          "form": "무교가됐어요",
          "original_form": "무교가됐어요",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014939.1.4",
          "form": "신을 안믿을라구요",
          "original_form": "신을 안믿을라구요",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014939.1.5",
          "form": "앗 혹시 무슨 안좋은 일 있으셨어요ㅜㅜ",
          "original_form": "앗 혹시 무슨 안좋은 일 있으셨어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014939.1.6",
          "form": "괜히 나쁜 기억을 여쭤본듯해 죄송해지네요ㅜㅜ",
          "original_form": "괜히 나쁜 기억을 여쭤본듯해 죄송해지네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014939.1.7",
          "form": "어잉 아니에요 ㅎㅎ",
          "original_form": "어잉 아니에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014939.1.8",
          "form": "그냥 태어날때부터 종교를 가진건데",
          "original_form": "그냥 태어날때부터 종교를 가진건데",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014939.1.9",
          "form": "크다보니",
          "original_form": "크다보니",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014939.1.10",
          "form": "귀찮더라고요",
          "original_form": "귀찮더라고요",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014939.1.11",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014939.1.12",
          "form": "나쁜 추억이 있던건 아니에요",
          "original_form": "나쁜 추억이 있던건 아니에요",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014939.1.13",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014939.1.14",
          "form": "맞아요 주말마다 미사드리기 귀찮죠",
          "original_form": "맞아요 주말마다 미사드리기 귀찮죠",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014939.1.15",
          "form": "그래도 성당은 분위기가 넘 좋아요",
          "original_form": "그래도 성당은 분위기가 넘 좋아요",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014939.1.16",
          "form": "맞아요 사람들이 선천적으로 다들 착하시더라구요",
          "original_form": "맞아요 사람들이 선천적으로 다들 착하시더라구요",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014939.1.17",
          "form": "저만 못되먹은것 같기두 하고...ㅋㅋ",
          "original_form": "저만 못되먹은것 같기두 하고...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014939.1.18",
          "form": "아닐거에요 name2님도 분명 선함이 가득한 분일듯",
          "original_form": "아닐거에요 &name2&님도 분명 선함이 가득한 분일듯",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014939.1.19",
          "form": "성당은 주일 말고도 자주 모이지요?",
          "original_form": "성당은 주일 말고도 자주 모이지요?",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014939.1.20",
          "form": "저는 무교인데 성당의 가족적 분위기 따뜻함이 좋아보이더라구요",
          "original_form": "저는 무교인데 성당의 가족적 분위기 따뜻함이 좋아보이더라구요",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014939.1.21",
          "form": "ㅋㅋ말이라도 감사해요!",
          "original_form": "ㅋㅋ말이라도 감사해요!",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014939.1.22",
          "form": "성당은 주로 주말에만 모여요!",
          "original_form": "성당은 주로 주말에만 모여요!",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014939.1.23",
          "form": "종교를 가져볼 생각은 안해보셧나요??",
          "original_form": "종교를 가져볼 생각은 안해보셧나요??",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014939.1.24",
          "form": "네ㅠㅠ 저는 신이 존재한다는게 도저히 믿기지가 않아서",
          "original_form": "네ㅠㅠ 저는 신이 존재한다는게 도저히 믿기지가 않아서",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014939.1.25",
          "form": "근데 직접 보지않는 이상 믿기 힘든건 맞는것 같아요",
          "original_form": "근데 직접 보지않는 이상 믿기 힘든건 맞는것 같아요",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014939.1.26",
          "form": "그래서 저는 진짜 신실한 종교인분들 뵈면 넘 대단해보여요",
          "original_form": "그래서 저는 진짜 신실한 종교인분들 뵈면 넘 대단해보여요",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014939.1.27",
          "form": "나도 저런 경험을 해보고 싶을때도 있고",
          "original_form": "나도 저런 경험을 해보고 싶을때도 있고",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014939.1.28",
          "form": "근데 세상의 많은 슬픔과 부조리를 보다보면 신이 있다는걸 믿을수가....",
          "original_form": "근데 세상의 많은 슬픔과 부조리를 보다보면 신이 있다는걸 믿을수가....",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014939.1.29",
          "form": "그러게요 어떻게 종교에 모든걸 내놓는 사람들이 신기하긴 해요 ㅎㅎ",
          "original_form": "그러게요 어떻게 종교에 모든걸 내놓는 사람들이 신기하긴 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014939.1.30",
          "form": "맞아요 신기신기 뭔가 다른 차원에 있는 것 같다는 느낌을 받을때가 있어요",
          "original_form": "맞아요 신기신기 뭔가 다른 차원에 있는 것 같다는 느낌을 받을때가 있어요",
          "speaker_id": "1",
          "time": "20210825 21:45"
        }
      ]
    }
