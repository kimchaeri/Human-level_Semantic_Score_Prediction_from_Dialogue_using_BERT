      "utterance": [
        {
          "id": "MDRW2100028967.1.1",
          "form": "뭐 연애도 오래하진않았어요",
          "original_form": "뭐 연애도 오래하진않았어요",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028967.1.2",
          "form": "저는 친구가 없거든요..",
          "original_form": "저는 친구가 없거든요..",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028967.1.3",
          "form": "얼마나하심?",
          "original_form": "얼마나하심?",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028967.1.4",
          "form": "외부 활동이 없습니다..\n반면, 왕성한 활동적인 배우자를..",
          "original_form": "외부 활동이 없습니다..\n반면, 왕성한 활동적인 배우자를..",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028967.1.5",
          "form": "아.. 그건본인스타일이니",
          "original_form": "아.. 그건본인스타일이니",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028967.1.6",
          "form": "서로 다름을 인정해줘야죠",
          "original_form": "서로 다름을 인정해줘야죠",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028967.1.7",
          "form": "약6개월..?\n현제 벌써 5년차네요ㅋ",
          "original_form": "약6개월..?\n현제 벌써 5년차네요ㅋ",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028967.1.8",
          "form": "시간이 너무 잘가요",
          "original_form": "시간이 너무 잘가요",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028967.1.9",
          "form": "보상때문에 시작한 심심이지만ㅋ",
          "original_form": "보상때문에 시작한 심심이지만ㅋ",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028967.1.10",
          "form": "나름 익명의? 사람과 스트레스 해소하네요ㅋㅋ",
          "original_form": "나름 익명의? 사람과 스트레스 해소하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028967.1.11",
          "form": "지인에게는 말할수 없는 부부의 세계라고나 할까요",
          "original_form": "지인에게는 말할수 없는 부부의 세계라고나 할까요",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028967.1.12",
          "form": "오~~좋은거네요",
          "original_form": "오~~좋은거네요",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028967.1.13",
          "form": "익명",
          "original_form": "익명",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028967.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028967.1.15",
          "form": "친구도 없지만,\n친구에게 배우자 호박씨는..\n못합니다ㅋㅋ\n가족들에게도",
          "original_form": "친구도 없지만,\n친구에게 배우자 호박씨는..\n못합니다ㅋㅋ\n가족들에게도",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028967.1.16",
          "form": "저도 카톡으로 수다많이해요",
          "original_form": "저도 카톡으로 수다많이해요",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028967.1.17",
          "form": "와이프랑못하는수다",
          "original_form": "와이프랑못하는수다",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028967.1.18",
          "form": "그냥 항상 눈치봐서 눈치보는게 편하네요 저도ㅋ",
          "original_form": "그냥 항상 눈치봐서 눈치보는게 편하네요 저도ㅋ",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028967.1.19",
          "form": "근데 은근 눈치아닌 눈치가 보입니다\n익명의 수다도ㅋ",
          "original_form": "근데 은근 눈치아닌 눈치가 보입니다\n익명의 수다도ㅋ",
          "speaker_id": "2",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028967.1.20",
          "form": "잉(?)  진짜요(?)",
          "original_form": "잉(?)  진짜요(?)",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028967.1.21",
          "form": "에고고",
          "original_form": "에고고",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028967.1.22",
          "form": "공감대형성이 힘들어오",
          "original_form": "공감대형성이 힘들어오",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028967.1.23",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028967.1.24",
          "form": "같이하기에는 이런걸 좋아하는 스타일이 아니라는..",
          "original_form": "같이하기에는 이런걸 좋아하는 스타일이 아니라는..",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028967.1.25",
          "form": "연애때는잘했는데",
          "original_form": "연애때는잘했는데",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028967.1.26",
          "form": "육아가 지치게 하나봅니다..",
          "original_form": "육아가 지치게 하나봅니다..",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028967.1.27",
          "form": "그렇죠 저도 애가3이라",
          "original_form": "그렇죠 저도 애가3이라",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028967.1.28",
          "form": "근데 어떻게보면? 평생 자식이 전부는 아니지 싶어요",
          "original_form": "근데 어떻게보면? 평생 자식이 전부는 아니지 싶어요",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028967.1.29",
          "form": "다시대화?",
          "original_form": "다시대화?",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028967.1.30",
          "form": "다 늙으면 우리둘 남을텐데",
          "original_form": "다 늙으면 우리둘 남을텐데",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028967.1.31",
          "form": "있을때 잘해라는 말이....",
          "original_form": "있을때 잘해라는 말이....",
          "speaker_id": "2",
          "time": "20210902 23:17"
        }
      ]
