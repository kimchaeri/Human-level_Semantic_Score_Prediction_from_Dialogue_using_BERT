      "utterance": [
        {
          "id": "MDRW2100028148.1.1",
          "form": "사이퍼즈얘기하기",
          "original_form": "사이퍼즈얘기하기",
          "speaker_id": "1",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028148.1.2",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028148.1.3",
          "form": "근데 제가 사이퍼즈를 접어서요",
          "original_form": "근데 제가 사이퍼즈를 접어서요",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028148.1.4",
          "form": "왜죠?",
          "original_form": "왜죠?",
          "speaker_id": "1",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100028148.1.5",
          "form": "C를 버리지 마세요",
          "original_form": "C를 버리지 마세요",
          "speaker_id": "1",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100028148.1.6",
          "form": "님접으면 망해요",
          "original_form": "님접으면 망해요",
          "speaker_id": "1",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100028148.1.7",
          "form": "이미 망했어요",
          "original_form": "이미 망했어요",
          "speaker_id": "2",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100028148.1.8",
          "form": "꺄아악",
          "original_form": "꺄아악",
          "speaker_id": "1",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100028148.1.9",
          "form": "근데저도 이자벨 나오고 안하는중",
          "original_form": "근데저도 이자벨 나오고 안하는중",
          "speaker_id": "1",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100028148.1.10",
          "form": "꺄아아아아악",
          "original_form": "꺄아아아아악",
          "speaker_id": "2",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100028148.1.11",
          "form": "캐삭(여기서 캐삭은 캐릭터삭제입니다.)해야해요",
          "original_form": "캐삭(여기서 캐삭은 캐릭터삭제입니다.)해야해요",
          "speaker_id": "2",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100028148.1.12",
          "form": "친절하게 주석달아주는 name2님",
          "original_form": "친절하게 주석달아주는 &name2&님",
          "speaker_id": "1",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100028148.1.13",
          "form": "ㅇㅈ합니다",
          "original_form": "ㅇㅈ합니다",
          "speaker_id": "1",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100028148.1.14",
          "form": "거대한 카인 미연시세계관 멈춰",
          "original_form": "거대한 카인 미연시세계관 멈춰",
          "speaker_id": "1",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028148.1.15",
          "form": "까아악",
          "original_form": "까아악",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028148.1.16",
          "form": "저 버스인데",
          "original_form": "저 버스인데",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028148.1.17",
          "form": "C는 여러분을 환영합니다",
          "original_form": "C는 여러분을 환영합니다",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028148.1.18",
          "form": "요즘 모바일게임은 와이파이가 없으면",
          "original_form": "요즘 모바일게임은 와이파이가 없으면",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028148.1.19",
          "form": "버스에서 사퍼얘기하다 소리지르셧나요?",
          "original_form": "버스에서 사퍼얘기하다 소리지르셧나요?",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028148.1.20",
          "form": "아니구나",
          "original_form": "아니구나",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028148.1.21",
          "form": "광고가 안떠서 좋더라고요",
          "original_form": "광고가 안떠서 좋더라고요",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028148.1.22",
          "form": "네 저 경찰와서 잡혀가는중",
          "original_form": "네 저 경찰와서 잡혀가는중",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028148.1.23",
          "form": "경찰한테도 사퍼영업해요",
          "original_form": "경찰한테도 사퍼영업해요",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028148.1.24",
          "form": "사퍼하세요\r\n사퍼안해요?",
          "original_form": "사퍼하세요\r\n사퍼안해요?",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028148.1.25",
          "form": "노력했는데 가셨어요",
          "original_form": "노력했는데 가셨어요",
          "speaker_id": "2",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028148.1.26",
          "form": "제가 사이퍼즈를 국민겜으로 만들겠습니다",
          "original_form": "제가 사이퍼즈를 국민겜으로 만들겠습니다",
          "speaker_id": "2",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028148.1.27",
          "form": "대선출마해주세요",
          "original_form": "대선출마해주세요",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028148.1.28",
          "form": "사이퍼즈가 먼저다",
          "original_form": "사이퍼즈가 먼저다",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028148.1.29",
          "form": "사퍼가 먼저다",
          "original_form": "사퍼가 먼저다",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028148.1.30",
          "form": "사이퍼즈 말고 또 다른 재미있는 게임을 아십니까?",
          "original_form": "사이퍼즈 말고 또 다른 재미있는 게임을 아십니까?",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028148.1.31",
          "form": "사이퍼즈 짱~",
          "original_form": "사이퍼즈 짱~",
          "speaker_id": "1",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100028148.1.32",
          "form": "님도 사퍼로 연어하게 되실거예요",
          "original_form": "님도 사퍼로 연어하게 되실거예요",
