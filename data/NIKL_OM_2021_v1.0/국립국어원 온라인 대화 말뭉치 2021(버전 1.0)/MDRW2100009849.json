      "utterance": [
        {
          "id": "MDRW2100009849.1.1",
          "form": "술을 하시나요?",
          "original_form": "술을 하시나요?",
          "speaker_id": "1",
          "time": "20210823 10:52"
        },
        {
          "id": "MDRW2100009849.1.2",
          "form": "아.. 저는 술을 안마셔요",
          "original_form": "아.. 저는 술을 안마셔요",
          "speaker_id": "2",
          "time": "20210823 10:52"
        },
        {
          "id": "MDRW2100009849.1.3",
          "form": "술 즐기시는 거 같은데 안주로 뭐 같이 드시나요",
          "original_form": "술 즐기시는 거 같은데 안주로 뭐 같이 드시나요",
          "speaker_id": "2",
          "time": "20210823 10:53"
        },
        {
          "id": "MDRW2100009849.1.4",
          "form": "역시 치킨이지요",
          "original_form": "역시 치킨이지요",
          "speaker_id": "1",
          "time": "20210823 10:53"
        },
        {
          "id": "MDRW2100009849.1.5",
          "form": "술을 안드시면 음료는 좋아하세요?",
          "original_form": "술을 안드시면 음료는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 10:53"
        },
        {
          "id": "MDRW2100009849.1.6",
          "form": "유재석도 그러던데ㅋ",
          "original_form": "유재석도 그러던데ㅋ",
          "speaker_id": "1",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100009849.1.7",
          "form": "네 사이다 좋아해요",
          "original_form": "네 사이다 좋아해요",
          "speaker_id": "2",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100009849.1.8",
          "form": "치킨은 후라이드와 양념 어느쪽을 선호하시나요",
          "original_form": "치킨은 후라이드와 양념 어느쪽을 선호하시나요",
          "speaker_id": "2",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100009849.1.9",
          "form": "둘중에 하나라면 후라이드?ㅎㅎ",
          "original_form": "둘중에 하나라면 후라이드?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100009849.1.10",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100009849.1.11",
          "form": "역시 치킨은 후라이드죠 저도 후라이드 좋아해요",
          "original_form": "역시 치킨은 후라이드죠 저도 후라이드 좋아해요",
          "speaker_id": "2",
          "time": "20210823 10:55"
        },
        {
          "id": "MDRW2100009849.1.12",
          "form": "바삭한게 좋아서..",
          "original_form": "바삭한게 좋아서..",
          "speaker_id": "2",
          "time": "20210823 10:55"
        },
        {
          "id": "MDRW2100009849.1.13",
          "form": "저는 후라이드도",
          "original_form": "저는 후라이드도",
          "speaker_id": "1",
          "time": "20210823 10:55"
        },
        {
          "id": "MDRW2100009849.1.14",
          "form": "가장 기본을 좋아해요ㅋㅋ 특별한 가루 없이ㅋ",
          "original_form": "가장 기본을 좋아해요ㅋㅋ 특별한 가루 없이ㅋ",
          "speaker_id": "1",
          "time": "20210823 10:55"
        },
        {
          "id": "MDRW2100009849.1.15",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210823 10:55"
        },
        {
          "id": "MDRW2100009849.1.16",
          "form": "약간 기본의 맛이 좋아요",
          "original_form": "약간 기본의 맛이 좋아요",
          "speaker_id": "2",
          "time": "20210823 10:55"
        },
        {
          "id": "MDRW2100009849.1.17",
          "form": "앗 좋아하는 집이 있나요?",
          "original_form": "앗 좋아하는 집이 있나요?",
          "speaker_id": "1",
          "time": "20210823 10:55"
        },
        {
          "id": "MDRW2100009849.1.18",
          "form": "전 노랑통닭ㅋㅋ",
          "original_form": "전 노랑통닭ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100009849.1.19",
          "form": "저는 자담치킨 후라이드 즐겨먹어요",
          "original_form": "저는 자담치킨 후라이드 즐겨먹어요",
          "speaker_id": "2",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100009849.1.20",
          "form": "와 먹어봐야겠네요",
          "original_form": "와 먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100009849.1.21",
          "form": "간이 잘 베어있어서 맛있어요",
          "original_form": "간이 잘 베어있어서 맛있어요",
          "speaker_id": "2",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100009849.1.22",
          "form": "맵술랭이 시그니처같은데ㅋ",
          "original_form": "맵술랭이 시그니처같은데ㅋ",
          "speaker_id": "1",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100009849.1.23",
          "form": "그건 너무 자극적..",
          "original_form": "그건 너무 자극적..",
