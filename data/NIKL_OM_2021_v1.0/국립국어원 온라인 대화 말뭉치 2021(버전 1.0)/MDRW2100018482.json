      "utterance": [
        {
          "id": "MDRW2100018482.1.1",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 02:51"
        },
        {
          "id": "MDRW2100018482.1.2",
          "form": "공연 좋아하죠ㅠㅠ 못 간 지 오래네요",
          "original_form": "공연 좋아하죠ㅠㅠ 못 간 지 오래네요",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100018482.1.3",
          "form": "요즘은 유튜브로 대체하고 있어요",
          "original_form": "요즘은 유튜브로 대체하고 있어요",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100018482.1.4",
          "form": "콘서트 가서 라이브 듣는 거 좋아하세요?",
          "original_form": "콘서트 가서 라이브 듣는 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100018482.1.5",
          "form": "네ㅋㅋㅋ저도 좋아해요 ㅠㅠ 코로나 때문에 못가네요",
          "original_form": "네ㅋㅋㅋ저도 좋아해요 ㅠㅠ 코로나 때문에 못가네요",
          "speaker_id": "1",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100018482.1.6",
          "form": "어떤 가수 좋아하세요?",
          "original_form": "어떤 가수 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100018482.1.7",
          "form": "저는 10cm 좋아해요~~",
          "original_form": "저는 10cm 좋아해요~~",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100018482.1.8",
          "form": "라이브가 진짠데 너무 그립네요",
          "original_form": "라이브가 진짠데 너무 그립네요",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100018482.1.9",
          "form": "앗 저도 좋아해요",
          "original_form": "앗 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100018482.1.10",
          "form": "봄마다 너에게 닿기를 들어요ㅋㅋㅋ",
          "original_form": "봄마다 너에게 닿기를 들어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100018482.1.11",
          "form": "아 그곡ㅋㅋㅋ봄에 듣기 좋죠",
          "original_form": "아 그곡ㅋㅋㅋ봄에 듣기 좋죠",
          "speaker_id": "2",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100018482.1.12",
          "form": "악기 사려고.알바로 부른 곡이라던데ㅎㅎ",
          "original_form": "악기 사려고.알바로 부른 곡이라던데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100018482.1.13",
          "form": "헐 그런 비하인드가 있었구나ㅋㅋㅋ",
          "original_form": "헐 그런 비하인드가 있었구나ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100018482.1.14",
          "form": "저는 노래 넘 좋길래 찾아다녔는데",
          "original_form": "저는 노래 넘 좋길래 찾아다녔는데",
          "speaker_id": "1",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100018482.1.15",
          "form": "ㅋㅋㅋㅋ웃기죠",
          "original_form": "ㅋㅋㅋㅋ웃기죠",
          "speaker_id": "2",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100018482.1.16",
          "form": "우연히 애니 노래인거 알고 완전 반가웠어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "우연히 애니 노래인거 알고 완전 반가웠어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100018482.1.17",
          "form": "아 애니랑 엄청 잘 어울린다고들 하시더라고요ㅋㅋㅋ",
          "original_form": "아 애니랑 엄청 잘 어울린다고들 하시더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100018482.1.18",
          "form": "맞아요 일본곡보다 10cm버전이 더 좋았어요",
          "original_form": "맞아요 일본곡보다 10cm버전이 더 좋았어요",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100018482.1.19",
          "form": "다른 가수는 또 누구 좋아하세요?",
          "original_form": "다른 가수는 또 누구 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100018482.1.20",
          "form": "ㅎㅎ괜히 제가 다 기분 좋네요",
          "original_form": "ㅎㅎ괜히 제가 다 기분 좋네요",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100018482.1.21",
          "form": "콜드플레이 아세요??",
          "original_form": "콜드플레이 아세요??",
          "speaker_id": "2",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100018482.1.22",
          "form": "앗물론이죠!",
          "original_form": "앗물론이죠!",
          "speaker_id": "1",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100018482.1.23",
          "form": "내한 너무 가보고 싶었는데",
          "original_form": "내한 너무 가보고 싶었는데",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100018482.1.24",
          "form": "전 지방인이라ㅠㅠ",
          "original_form": "전 지방인이라ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100018482.1.25",
          "form": "그러셨구나 그치만 공연장 옆에 살아도 못가는 공연ㅠㅋㅋㅋㅋㅋㅋ",
