      "utterance": [
        {
          "id": "MDRW2100033081.1.1",
          "form": "쌀 거의 다 먹어서 새로 사야할 것 같아",
          "original_form": "쌀 거의 다 먹어서 새로 사야할 것 같아",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033081.1.2",
          "form": "외식을 많이 안하니 쌀이 금방 떨어지네 ㅎㅎ",
          "original_form": "외식을 많이 안하니 쌀이 금방 떨어지네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033081.1.3",
          "form": "그러게 말야 ㅎㅎ 한달에 20kg은 먹는 것 같아",
          "original_form": "그러게 말야 ㅎㅎ 한달에 20kg은 먹는 것 같아",
          "speaker_id": "1",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100033081.1.4",
          "form": "와 진짜 많이 먹네 ㅋㅋ",
          "original_form": "와 진짜 많이 먹네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100033081.1.5",
          "form": "애들이 워낙 잘먹으니",
          "original_form": "애들이 워낙 잘먹으니",
          "speaker_id": "1",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100033081.1.6",
          "form": "지금 먹는 쌀이 맛있더라",
          "original_form": "지금 먹는 쌀이 맛있더라",
          "speaker_id": "2",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100033081.1.7",
          "form": "ㅇㅇ 황금도깨미인데 밥이 찰지고 맛있어",
          "original_form": "ㅇㅇ 황금도깨미인데 밥이 찰지고 맛있어",
          "speaker_id": "1",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100033081.1.8",
          "form": "햅쌀이야?",
          "original_form": "햅쌀이야?",
          "speaker_id": "2",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100033081.1.9",
          "form": "ㅇㅇ 맞아.. 바로 도정해서 보내주더라고",
          "original_form": "ㅇㅇ 맞아.. 바로 도정해서 보내주더라고",
          "speaker_id": "1",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100033081.1.10",
          "form": "어쩐지 맛있더라",
          "original_form": "어쩐지 맛있더라",
          "speaker_id": "2",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100033081.1.11",
          "form": "파는 곳도 많지 않아.. 온라인 가격도 다 비슷하고",
          "original_form": "파는 곳도 많지 않아.. 온라인 가격도 다 비슷하고",
          "speaker_id": "1",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100033081.1.12",
          "form": "좋은 쌀이니 굳이 가격을 내릴 필요가 없나보네",
          "original_form": "좋은 쌀이니 굳이 가격을 내릴 필요가 없나보네",
          "speaker_id": "2",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100033081.1.13",
          "form": "ㅎㅎ 그래서 그런가? 좀 비싸",
          "original_form": "ㅎㅎ 그래서 그런가? 좀 비싸",
          "speaker_id": "1",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100033081.1.14",
          "form": "어여 주문해요.. 추석에 택배 밀릴라",
          "original_form": "어여 주문해요.. 추석에 택배 밀릴라",
          "speaker_id": "2",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100033081.1.15",
          "form": "아 맞다.. 안그래도 이번주부터 택배 물량 많을듯",
          "original_form": "아 맞다.. 안그래도 이번주부터 택배 물량 많을듯",
          "speaker_id": "1",
