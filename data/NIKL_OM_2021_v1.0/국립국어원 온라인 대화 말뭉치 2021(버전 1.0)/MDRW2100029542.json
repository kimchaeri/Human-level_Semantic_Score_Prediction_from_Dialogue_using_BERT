      "utterance": [
        {
          "id": "MDRW2100029542.1.1",
          "form": "좋아하는 스포츠 있으세요?",
          "original_form": "좋아하는 스포츠 있으세요?",
          "speaker_id": "2",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029542.1.2",
          "form": "야구...를 좋아했었는데",
          "original_form": "야구...를 좋아했었는데",
          "speaker_id": "1",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029542.1.3",
          "form": "여기는 불미스러운 사건을",
          "original_form": "여기는 불미스러운 사건을",
          "speaker_id": "1",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029542.1.4",
          "form": "못 끊어내서",
          "original_form": "못 끊어내서",
          "speaker_id": "1",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029542.1.5",
          "form": "이번에도 무슨일 있었죠...",
          "original_form": "이번에도 무슨일 있었죠...",
          "speaker_id": "2",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029542.1.6",
          "form": "국대 라인업이 바뀌었었다는데 맞나요",
          "original_form": "국대 라인업이 바뀌었었다는데 맞나요",
          "speaker_id": "2",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029542.1.7",
          "form": "흠흠...",
          "original_form": "흠흠...",
          "speaker_id": "1",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100029542.1.8",
          "form": "그리고 결과도",
          "original_form": "그리고 결과도",
          "speaker_id": "1",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100029542.1.9",
          "form": "그다지...",
          "original_form": "그다지...",
          "speaker_id": "1",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100029542.1.10",
          "form": "그러고보니 기사에서도 야구랑",
          "original_form": "그러고보니 기사에서도 야구랑",
          "speaker_id": "1",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100029542.1.11",
          "form": "배구 비교를",
          "original_form": "배구 비교를",
          "speaker_id": "1",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100029542.1.12",
          "form": "많이 하더라고요",
          "original_form": "많이 하더라고요",
          "speaker_id": "1",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100029542.1.13",
          "form": "불미스러운 사건이 있었던 건 같은데",
          "original_form": "불미스러운 사건이 있었던 건 같은데",
          "speaker_id": "2",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100029542.1.14",
          "form": "결과가 달라서 그런거 같아요",
          "original_form": "결과가 달라서 그런거 같아요",
          "speaker_id": "2",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100029542.1.15",
          "form": "기대치도 달랐구요",
          "original_form": "기대치도 달랐구요",
          "speaker_id": "2",
          "time": "20210903 10:19"
        },
        {
          "id": "MDRW2100029542.1.16",
          "form": "그렇긴 하죠",
          "original_form": "그렇긴 하죠",
          "speaker_id": "1",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100029542.1.17",
          "form": "일단 우승팀이었었으니..",
          "original_form": "일단 우승팀이었었으니..",
          "speaker_id": "1",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100029542.1.18",
          "form": "이번엔 진짜 아쉬웠던거 같아요",
          "original_form": "이번엔 진짜 아쉬웠던거 같아요",
          "speaker_id": "2",
          "time": "20210903 10:24"
        },
        {
          "id": "MDRW2100029542.1.19",
          "form": "예",
          "original_form": "예",
          "speaker_id": "1",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100029542.1.20",
          "form": "다만 여튼",
          "original_form": "다만 여튼",
          "speaker_id": "1",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100029542.1.21",
          "form": "야구는 워낙 불미스러운 일이 많다보니",
          "original_form": "야구는 워낙 불미스러운 일이 많다보니",
          "speaker_id": "1",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100029542.1.22",
          "form": "한동안은 직관도 많이 가고 그랬는데",
          "original_form": "한동안은 직관도 많이 가고 그랬는데",
          "speaker_id": "1",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100029542.1.23",
          "form": "좀 뭐랄까...",
          "original_form": "좀 뭐랄까...",
          "speaker_id": "1",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100029542.1.24",
          "form": "정이 떨어지신걸까요",
          "original_form": "정이 떨어지신걸까요",
          "speaker_id": "2",
          "time": "20210903 12:48"
        },
        {
          "id": "MDRW2100029542.1.25",
          "form": "흠흠...",
          "original_form": "흠흠...",
          "speaker_id": "1",
          "time": "20210903 12:49"
        },
        {
          "id": "MDRW2100029542.1.26",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "1",
          "time": "20210903 12:49"
        },
        {
          "id": "MDRW2100029542.1.27",
          "form": "김광현 선수 경기는 챙겨봅니다",
          "original_form": "김광현 선수 경기는 챙겨봅니다",
          "speaker_id": "1",
          "time": "20210903 12:49"
        },
        {
          "id": "MDRW2100029542.1.28",
          "form": "오 잘하는 선수인가봐요",
          "original_form": "오 잘하는 선수인가봐요",
          "speaker_id": "2",
          "time": "20210903 12:49"
        },
        {
          "id": "MDRW2100029542.1.29",
          "form": "어느 팀인가요",
          "original_form": "어느 팀인가요",
          "speaker_id": "2",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100029542.1.30",
          "form": "메이저리그 갔습니다",
          "original_form": "메이저리그 갔습니다",
          "speaker_id": "1",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100029542.1.31",
          "form": "ㅇㅅㅇ)9",
          "original_form": "ㅇㅅㅇ)9",
          "speaker_id": "1",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100029542.1.32",
          "form": "와 메이저리그까지 진출한 선수였군요",
