      "utterance": [
        {
          "id": "MDRW2100029824.1.1",
          "form": "예능 뭐보세요",
          "original_form": "예능 뭐보세요",
          "speaker_id": "1",
          "time": "20210903 14:21"
        },
        {
          "id": "MDRW2100029824.1.2",
          "form": "놀토봐요! 어떤 예능 보세요?",
          "original_form": "놀토봐요! 어떤 예능 보세요?",
          "speaker_id": "2",
          "time": "20210903 14:45"
        },
        {
          "id": "MDRW2100029824.1.3",
          "form": "저는 놀면 뭐하니요!",
          "original_form": "저는 놀면 뭐하니요!",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100029824.1.4",
          "form": "둘다 토욜에 하네요 ㅋㅋ",
          "original_form": "둘다 토욜에 하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100029824.1.5",
          "form": "ㅋㅋ토요일에 재미있는 게 많네요ㅋㅋㅋ",
          "original_form": "ㅋㅋ토요일에 재미있는 게 많네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100029824.1.6",
          "form": "넵 ㅋㅋㅋ 펜트하우스 보셧어요,",
          "original_form": "넵 ㅋㅋㅋ 펜트하우스 보셧어요,",
          "speaker_id": "1",
          "time": "20210904 21:35"
        },
        {
          "id": "MDRW2100029824.1.7",
          "form": "가끔 봤어요ㅎㅎ",
          "original_form": "가끔 봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100029824.1.8",
          "form": "좋아하시나봐요!",
          "original_form": "좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100029824.1.9",
          "form": "네네!! 담주가 마지막회 인거같아요 ㅋㅋ벌써",
          "original_form": "네네!! 담주가 마지막회 인거같아요 ㅋㅋ벌써",
          "speaker_id": "1",
          "time": "20210905 20:15"
        },
        {
          "id": "MDRW2100029824.1.10",
          "form": "세상에 그렇군요!!",
          "original_form": "세상에 그렇군요!!",
          "speaker_id": "2",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100029824.1.11",
          "form": "드라마 어떤거 보세요??",
          "original_form": "드라마 어떤거 보세요??",
          "speaker_id": "1",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100029824.1.12",
          "form": "요즘은 슬기로운 의사생활 보고 있어요ㅎㅎ",
          "original_form": "요즘은 슬기로운 의사생활 보고 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100029824.1.13",
          "form": "오오 시즌 2 나왓죠??",
          "original_form": "오오 시즌 2 나왓죠??",
          "speaker_id": "1",
          "time": "20210906 18:52"
        },
        {
          "id": "MDRW2100029824.1.14",
          "form": "맞아요ㅎㅎ 시즌1 때는 안보다가",
          "original_form": "맞아요ㅎㅎ 시즌1 때는 안보다가",
          "speaker_id": "2",
          "time": "20210906 19:34"
        },
        {
          "id": "MDRW2100029824.1.15",
          "form": "주변에서 추천을 많이 받아서 시즌2 보는데",
          "original_form": "주변에서 추천을 많이 받아서 시즌2 보는데",
          "speaker_id": "2",
          "time": "20210906 19:34"
        },
        {
          "id": "MDRW2100029824.1.16",
          "form": "재미있네요ㅋㅋ",
          "original_form": "재미있네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:34"
        },
        {
          "id": "MDRW2100029824.1.17",
          "form": "오오 저도 봐야겟어요",
          "original_form": "오오 저도 봐야겟어요",
