      "utterance": [
        {
          "id": "MDRW2100025209.1.1",
          "form": "안녕하세요~ 저녁에 어떤 메뉴 드셨었나요?",
          "original_form": "안녕하세요~ 저녁에 어떤 메뉴 드셨었나요?",
          "speaker_id": "1",
          "time": "20210901 00:56"
        },
        {
          "id": "MDRW2100025209.1.2",
          "form": "저는 치킨 너겟에다 치즈 퐁듀 해 먹었어요~",
          "original_form": "저는 치킨 너겟에다 치즈 퐁듀 해 먹었어요~",
          "speaker_id": "2",
          "time": "20210901 00:57"
        },
        {
          "id": "MDRW2100025209.1.3",
          "form": "함께 김치 볶음밥두요",
          "original_form": "함께 김치 볶음밥두요",
          "speaker_id": "2",
          "time": "20210901 00:57"
        },
        {
          "id": "MDRW2100025209.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210901 00:57"
        },
        {
          "id": "MDRW2100025209.1.5",
          "form": "전 비빔밥 먹었어요",
          "original_form": "전 비빔밥 먹었어요",
          "speaker_id": "1",
          "time": "20210901 00:58"
        },
        {
          "id": "MDRW2100025209.1.6",
          "form": "오 뭐 들어갔나요",
          "original_form": "오 뭐 들어갔나요",
          "speaker_id": "2",
          "time": "20210901 00:58"
        },
        {
          "id": "MDRW2100025209.1.7",
          "form": "버섯파프리카볶음 부추무침 호박볶음 넣어서 먹었어요",
          "original_form": "버섯파프리카볶음 부추무침 호박볶음 넣어서 먹었어요",
          "speaker_id": "1",
          "time": "20210901 00:59"
        },
        {
          "id": "MDRW2100025209.1.8",
          "form": "와 요리 잘하시나봐요",
          "original_form": "와 요리 잘하시나봐요",
          "speaker_id": "2",
          "time": "20210901 01:00"
        },
        {
          "id": "MDRW2100025209.1.9",
          "form": "재료가 고급스킬이네요",
          "original_form": "재료가 고급스킬이네요",
          "speaker_id": "2",
          "time": "20210901 01:01"
        },
        {
          "id": "MDRW2100025209.1.10",
          "form": "마땅히 해먹을게 없어 고민하다가 냉장고에 있는 재료들 털었네요ㅎㅎ",
          "original_form": "마땅히 해먹을게 없어 고민하다가 냉장고에 있는 재료들 털었네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 01:01"
        },
        {
          "id": "MDRW2100025209.1.11",
          "form": "맛있으셨겠다!ㅎㅎ",
          "original_form": "맛있으셨겠다!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100025209.1.12",
          "form": "냉장고 파먹는거 은근 맛조합 잘 나오면 기쁩니다",
          "original_form": "냉장고 파먹는거 은근 맛조합 잘 나오면 기쁩니다",
          "speaker_id": "2",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100025209.1.13",
          "form": "나름 소소한 재미예요",
          "original_form": "나름 소소한 재미예요",
          "speaker_id": "2",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100025209.1.14",
          "form": "네ㅎㅎ 최근에 간단하게 대충? 먹다가 밥 다운 밥을 먹으니 좋았어요",
          "original_form": "네ㅎㅎ 최근에 간단하게 대충? 먹다가 밥 다운 밥을 먹으니 좋았어요",
          "speaker_id": "1",
          "time": "20210901 01:03"
        },
        {
          "id": "MDRW2100025209.1.15",
          "form": "웰빙이네요",
          "original_form": "웰빙이네요",
          "speaker_id": "2",
          "time": "20210901 01:04"
        },
        {
          "id": "MDRW2100025209.1.16",
          "form": "비빔밥이 참 맛있는 거 같아요~",
          "original_form": "비빔밥이 참 맛있는 거 같아요~",
          "speaker_id": "2",
          "time": "20210901 01:05"
        },
        {
          "id": "MDRW2100025209.1.17",
          "form": "맞아요~  조금씩 남아있는 재료들 사용하기도 좋구요",
          "original_form": "맞아요~  조금씩 남아있는 재료들 사용하기도 좋구요",
          "speaker_id": "1",
          "time": "20210901 01:05"
        },
        {
          "id": "MDRW2100025209.1.18",
          "form": "냉장고 파먹기 최고왕은 비빔밥!",
          "original_form": "냉장고 파먹기 최고왕은 비빔밥!",
          "speaker_id": "2",
          "time": "20210901 01:06"
        },
        {
          "id": "MDRW2100025209.1.19",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 01:09"
        }
      ]
