      "utterance": [
        {
          "id": "MDRW2100024022.1.1",
          "form": "오 동호회 하시나요 ?",
          "original_form": "오 동호회 하시나요 ?",
          "speaker_id": "1",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024022.1.2",
          "form": "앗 아니용ㅋㅋㅋ",
          "original_form": "앗 아니용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024022.1.3",
          "form": "동호회 활동 하세요??",
          "original_form": "동호회 활동 하세요??",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024022.1.4",
          "form": "아니요ㅋㅋㅋ !",
          "original_form": "아니요ㅋㅋㅋ !",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024022.1.5",
          "form": "늙으면 산악회는 하고싶네욬ㅋ",
          "original_form": "늙으면 산악회는 하고싶네욬ㅋ",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024022.1.6",
          "form": "그 힘든 등산을 늙어서 하신다구요?? ㅋㅋㅋㅋㅋ",
          "original_form": "그 힘든 등산을 늙어서 하신다구요?? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024022.1.7",
          "form": "앞집 아주머니 산악회 다니시던데 항상 파이팅 넘쳐 보이셔서 대단해 보였어요 ㅋㅋㅋㅋ",
          "original_form": "앞집 아주머니 산악회 다니시던데 항상 파이팅 넘쳐 보이셔서 대단해 보였어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024022.1.8",
          "form": "한 50대때 하고싶어요 ㅋㄱㅋㅋ",
          "original_form": "한 50대때 하고싶어요 ㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024022.1.9",
          "form": "지금은 아직까지 산의 기운을 모르겠어욬ㅋㄱㅋ",
          "original_form": "지금은 아직까지 산의 기운을 모르겠어욬ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024022.1.10",
          "form": "저도 아직은 모르겠어요 ㅋㅋㅋㅋ",
          "original_form": "저도 아직은 모르겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100024022.1.11",
          "form": "오르는 마음도 아직은... ㅋㅋㅋㅋ",
          "original_form": "오르는 마음도 아직은... ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100024022.1.12",
          "form": "산 밑에 백숙집 가는 모임은 있으면 하고싶어요 ㅋㅋㅋㅋ",
          "original_form": "산 밑에 백숙집 가는 모임은 있으면 하고싶어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100024022.1.13",
          "form": "아 ㅋㅋㄱㄱㅋㅋㄱ 아니 왠지",
          "original_form": "아 ㅋㅋㄱㄱㅋㅋㄱ 아니 왠지",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100024022.1.14",
          "form": "산은 한 20분 갔다가 백숙집 가실듯한 이 느낌",
          "original_form": "산은 한 20분 갔다가 백숙집 가실듯한 이 느낌",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100024022.1.15",
          "form": "그런 모임이 있다면 지금이라도 가입하겠습니다 ㅋㅋㅋㅋㅋ",
          "original_form": "그런 모임이 있다면 지금이라도 가입하겠습니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100024022.1.16",
          "form": "이번 달은 백숙, 다음 달은 오리백숙 좋네요 ㅋㅋㅋㅋㅋ",
          "original_form": "이번 달은 백숙, 다음 달은 오리백숙 좋네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024022.1.17",
          "form": "약간 계모임 좋아하실거같은데욬ㅋㅋㅋ",
          "original_form": "약간 계모임 좋아하실거같은데욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024022.1.18",
          "form": "한달에 한번씩",
          "original_form": "한달에 한번씩",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024022.1.19",
          "form": "맛집투어하기",
          "original_form": "맛집투어하기",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024022.1.20",
          "form": "오 좋네요! ㅋㅋㅋㅋㅋ",
          "original_form": "오 좋네요! ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024022.1.21",
          "form": "하지만 저는 파워 집순이라서 상상만 합니다 ㅋㅋㅋㅋ",
          "original_form": "하지만 저는 파워 집순이라서 상상만 합니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024022.1.22",
          "form": "계모임 좋아하시나요?",
          "original_form": "계모임 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024022.1.23",
          "form": "저희 어머니는 좋아하세요 ㅎㅎ",
          "original_form": "저희 어머니는 좋아하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024022.1.24",
          "form": "저도 집순이라서",
          "original_form": "저도 집순이라서",
          "speaker_id": "1",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024022.1.25",
          "form": "차라리 혼자 맛집투어를 가는게",
          "original_form": "차라리 혼자 맛집투어를 가는게",
          "speaker_id": "1",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024022.1.26",
          "form": "빠를거같아요 ㅋㅋ",
          "original_form": "빠를거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024022.1.27",
          "form": "저는 그마저도 친구들이 가자 안하면 안 갈것 같아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 그마저도 친구들이 가자 안하면 안 갈것 같아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100024022.1.28",
          "form": "집순이에게 모임활동은 좀 버겁기 때문에 ㅋㅋㅋㅋㅋ",
          "original_form": "집순이에게 모임활동은 좀 버겁기 때문에 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100024022.1.29",
          "form": "맞아욬ㅋㅋㅋㄱ",
          "original_form": "맞아욬ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210830 22:13"
        }
      ]
