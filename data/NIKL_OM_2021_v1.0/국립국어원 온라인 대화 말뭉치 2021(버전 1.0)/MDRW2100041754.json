      "utterance": [
        {
          "id": "MDRW2100041754.1.1",
          "form": "점심 머먹엇어?",
          "original_form": "점심 머먹엇어?",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041754.1.2",
          "form": "피자 먹었어",
          "original_form": "피자 먹었어",
          "speaker_id": "2",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100041754.1.3",
          "form": "우와 어떤 맛?",
          "original_form": "우와 어떤 맛?",
          "speaker_id": "1",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100041754.1.4",
          "form": "아메리칸치킨이래",
          "original_form": "아메리칸치킨이래",
          "speaker_id": "2",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100041754.1.5",
          "form": "너는 점심 뭐 먹었어?",
          "original_form": "너는 점심 뭐 먹었어?",
          "speaker_id": "2",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100041754.1.6",
          "form": "누구랑 먹었어?",
          "original_form": "누구랑 먹었어?",
          "speaker_id": "1",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100041754.1.7",
          "form": "나는 아침 먹고 자느라 점심 못먹었어 ㅋㅋㅋㅋ",
          "original_form": "나는 아침 먹고 자느라 점심 못먹었어 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100041754.1.8",
          "form": "안돼 ㅠㅠ",
          "original_form": "안돼 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100041754.1.9",
          "form": "배 안고파?",
          "original_form": "배 안고파?",
          "speaker_id": "2",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100041754.1.10",
          "form": "ㅠㅠㅠㅠ 오늘만 그런거야!",
          "original_form": "ㅠㅠㅠㅠ 오늘만 그런거야!",
          "speaker_id": "1",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100041754.1.11",
          "form": "배는 괜찮아~",
          "original_form": "배는 괜찮아~",
          "speaker_id": "1",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100041754.1.12",
          "form": "그나마 다행일까",
          "original_form": "그나마 다행일까",
          "speaker_id": "2",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041754.1.13",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041754.1.14",
          "form": "피자는 어디 브랜드야?",
          "original_form": "피자는 어디 브랜드야?",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041754.1.15",
          "form": "도미노피자야",
          "original_form": "도미노피자야",
          "speaker_id": "2",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041754.1.16",
          "form": "오늘 도미노피자가 할인하더라고",
          "original_form": "오늘 도미노피자가 할인하더라고",
          "speaker_id": "2",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041754.1.17",
          "form": "우와 맛있겠다~~",
          "original_form": "우와 맛있겠다~~",
          "speaker_id": "1",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100041754.1.18",
          "form": "나는 도미노 피자 진짜 안먹어본 것 같아",
          "original_form": "나는 도미노 피자 진짜 안먹어본 것 같아",
          "speaker_id": "1",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100041754.1.19",
          "form": "정말? 너는 어디서 시켜?",
          "original_form": "정말? 너는 어디서 시켜?",
          "speaker_id": "2",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100041754.1.20",
          "form": "나는 피자를 잘 안 시켜먹어",
