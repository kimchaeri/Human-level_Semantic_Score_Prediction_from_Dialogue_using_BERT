      "utterance": [
        {
          "id": "MDRW2100043818.1.1",
          "form": "먹는얘기가 젤 재밌네요",
          "original_form": "먹는얘기가 젤 재밌네요",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043818.1.2",
          "form": "ㅋㅋㅋㅋㅋ그러게요",
          "original_form": "ㅋㅋㅋㅋㅋ그러게요",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043818.1.3",
          "form": "name2님 돼지 소 닭",
          "original_form": "&name2&님 돼지 소 닭",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043818.1.4",
          "form": "뭐가 제일 좋으세요",
          "original_form": "뭐가 제일 좋으세요",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043818.1.5",
          "form": "돼지요ㅋㅋㅋ",
          "original_form": "돼지요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043818.1.6",
          "form": "닭은 치킨땜에 자주 먹긴해요",
          "original_form": "닭은 치킨땜에 자주 먹긴해요",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043818.1.7",
          "form": "저도 돼지!!!!!!",
          "original_form": "저도 돼지!!!!!!",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043818.1.8",
          "form": "돼지가 제일 맛있어요",
          "original_form": "돼지가 제일 맛있어요",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043818.1.9",
          "form": "삼겹살이요! 넘 맛있어요",
          "original_form": "삼겹살이요! 넘 맛있어요",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043818.1.10",
          "form": "아악",
          "original_form": "아악",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043818.1.11",
          "form": "저도 삼겹살!!!!",
          "original_form": "저도 삼겹살!!!!",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043818.1.12",
          "form": "너무 좋아!!!여!!!!!",
          "original_form": "너무 좋아!!!여!!!!!",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043818.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043818.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ집에선 그냥 구워먹기만 하는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ집에선 그냥 구워먹기만 하는데",
          "speaker_id": "2",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043818.1.15",
          "form": "고기집가면 볶음밥까지 다 챙겨 먹잖아요",
          "original_form": "고기집가면 볶음밥까지 다 챙겨 먹잖아요",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043818.1.16",
          "form": "당장 삼겹살 꾸워 먹고 싶어요..",
          "original_form": "당장 삼겹살 꾸워 먹고 싶어요..",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043818.1.17",
          "form": "전 목살도 좋아요. 찌개나 카레 넣어도 맛있구요ㅋㅋ",
          "original_form": "전 목살도 좋아요. 찌개나 카레 넣어도 맛있구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043818.1.18",
          "form": "제가 투탑으로 좋아합니다",
          "original_form": "제가 투탑으로 좋아합니다",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043818.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043818.1.20",
          "form": "전 목살 구워먹는것도 좋아해요",
          "original_form": "전 목살 구워먹는것도 좋아해요",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043818.1.21",
          "form": "구워먹어도 맛있져ㅋㅋ",
          "original_form": "구워먹어도 맛있져ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100043818.1.22",
          "form": "전 김치찌개에 자주 넣어요",
          "original_form": "전 김치찌개에 자주 넣어요",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100043818.1.23",
          "form": "저도 주로는 그런것 같아요",
          "original_form": "저도 주로는 그런것 같아요",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100043818.1.24",
          "form": "근데 목살도 비싸지 않아요?ㅠ 삼겹살보다야 싸긴한데",
          "original_form": "근데 목살도 비싸지 않아요?ㅠ 삼겹살보다야 싸긴한데",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100043818.1.25",
          "form": "요즘은 진짜 비싼거같아요",
          "original_form": "요즘은 진짜 비싼거같아요",
