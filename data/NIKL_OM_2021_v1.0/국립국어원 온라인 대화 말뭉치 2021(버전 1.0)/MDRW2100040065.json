      "utterance": [
        {
          "id": "MDRW2100040065.1.1",
          "form": "몰라 갑자기 라노벨의 산지 일본 배경이라",
          "original_form": "몰라 갑자기 라노벨의 산지 일본 배경이라",
          "speaker_id": "2",
          "time": "20210904 02:31"
        },
        {
          "id": "MDRW2100040065.1.2",
          "form": "미쳐날뛰나?",
          "original_form": "미쳐날뛰나?",
          "speaker_id": "2",
          "time": "20210904 02:31"
        },
        {
          "id": "MDRW2100040065.1.3",
          "form": "가뜩이나 스토리도 개연성 날렸는데",
          "original_form": "가뜩이나 스토리도 개연성 날렸는데",
          "speaker_id": "1",
          "time": "20210904 02:31"
        },
        {
          "id": "MDRW2100040065.1.4",
          "form": "ㅇㅇ번역이라도 좀",
          "original_form": "ㅇㅇ번역이라도 좀",
          "speaker_id": "2",
          "time": "20210904 02:31"
        },
        {
          "id": "MDRW2100040065.1.5",
          "form": "번역까지 이상해서",
          "original_form": "번역까지 이상해서",
          "speaker_id": "1",
          "time": "20210904 02:31"
        },
        {
          "id": "MDRW2100040065.1.6",
          "form": "겜에 더 몰입 안됨",
          "original_form": "겜에 더 몰입 안됨",
          "speaker_id": "1",
          "time": "20210904 02:31"
        },
        {
          "id": "MDRW2100040065.1.7",
          "form": "무게감 있게 하란말야",
          "original_form": "무게감 있게 하란말야",
          "speaker_id": "2",
          "time": "20210904 02:31"
        },
        {
          "id": "MDRW2100040065.1.8",
          "form": "선택지도 짜증나",
          "original_form": "선택지도 짜증나",
          "speaker_id": "2",
          "time": "20210904 02:32"
        },
        {
          "id": "MDRW2100040065.1.9",
          "form": "갑자기 야에한테 여우모습 보여달란 선택지가",
          "original_form": "갑자기 야에한테 여우모습 보여달란 선택지가",
          "speaker_id": "2",
          "time": "20210904 02:32"
        },
        {
          "id": "MDRW2100040065.1.10",
          "form": "왜 필요해?",
          "original_form": "왜 필요해?",
          "speaker_id": "2",
          "time": "20210904 02:32"
        },
        {
          "id": "MDRW2100040065.1.11",
          "form": "안궁금해",
          "original_form": "안궁금해",
          "speaker_id": "2",
          "time": "20210904 02:32"
        },
        {
          "id": "MDRW2100040065.1.12",
          "form": "개연성이 없어",
          "original_form": "개연성이 없어",
          "speaker_id": "2",
          "time": "20210904 02:32"
        },
        {
          "id": "MDRW2100040065.1.13",
          "form": "ㅎㅏ",
          "original_form": "ㅎㅏ",
          "speaker_id": "2",
          "time": "20210904 02:33"
        },
        {
          "id": "MDRW2100040065.1.14",
          "form": "일단 쿠죠사라까지 나왔어",
          "original_form": "일단 쿠죠사라까지 나왔어",
          "speaker_id": "2",
          "time": "20210904 02:33"
        },
        {
          "id": "MDRW2100040065.1.15",
          "form": "좀 더 봐보겠어",
          "original_form": "좀 더 봐보겠어",
          "speaker_id": "2",
          "time": "20210904 02:33"
        },
        {
          "id": "MDRW2100040065.1.16",
          "form": "ㅇㅇ 이번 스토리 ㄹㅇ 이상함...",
          "original_form": "ㅇㅇ 이번 스토리 ㄹㅇ 이상함...",
          "speaker_id": "1",
          "time": "20210904 02:40"
        },
        {
          "id": "MDRW2100040065.1.17",
          "form": "갑자기 카미사토 아야카한테",
          "original_form": "갑자기 카미사토 아야카한테",
          "speaker_id": "2",
          "time": "20210904 02:40"
        },
        {
          "id": "MDRW2100040065.1.18",
          "form": "닌자부대가 있다고???",
          "original_form": "닌자부대가 있다고???",
          "speaker_id": "2",
          "time": "20210904 02:40"
        },
        {
          "id": "MDRW2100040065.1.19",
          "form": "갑자기.....라노벨을 바가지로",
          "original_form": "갑자기.....라노벨을 바가지로",
          "speaker_id": "2",
          "time": "20210904 02:40"
        },
        {
          "id": "MDRW2100040065.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄴㄴ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄴㄴ",
          "speaker_id": "1",
          "time": "20210904 02:40"
        },
        {
          "id": "MDRW2100040065.1.21",
          "form": "퍼다 붓는데......",
          "original_form": "퍼다 붓는데......",
          "speaker_id": "2",
          "time": "20210904 02:41"
        },
        {
          "id": "MDRW2100040065.1.22",
          "form": "아야카 오빠인 아야토",
          "original_form": "아야카 오빠인 아야토",
          "speaker_id": "1",
          "time": "20210904 02:41"
        },
        {
          "id": "MDRW2100040065.1.23",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210904 02:41"
        },
        {
          "id": "MDRW2100040065.1.24",
          "form": "여튼",
          "original_form": "여튼",
          "speaker_id": "2",
          "time": "20210904 02:41"
        },
        {
          "id": "MDRW2100040065.1.25",
          "form": "카미사토 가주인데 모습도 안드러내고",
          "original_form": "카미사토 가주인데 모습도 안드러내고",
          "speaker_id": "1",
          "time": "20210904 02:41"
        },
        {
          "id": "MDRW2100040065.1.26",
          "form": "개연성 어디갔어",
          "original_form": "개연성 어디갔어",
          "speaker_id": "2",
          "time": "20210904 02:41"
        },
        {
          "id": "MDRW2100040065.1.27",
          "form": "일단 설정상 중립이래",
          "original_form": "일단 설정상 중립이래",
          "speaker_id": "1",
          "time": "20210904 02:41"
        },
        {
          "id": "MDRW2100040065.1.28",
          "form": "그니까 ㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:41"
        },
        {
          "id": "MDRW2100040065.1.29",
          "form": "아니 뭔 닌자부대야",
          "original_form": "아니 뭔 닌자부대야",
          "speaker_id": "2",
          "time": "20210904 02:41"
        },
        {
          "id": "MDRW2100040065.1.30",
          "form": "애초에 암살자한테",
          "original_form": "애초에 암살자한테",
          "speaker_id": "2",
          "time": "20210904 02:41"
        },
        {
          "id": "MDRW2100040065.1.31",
          "form": "부대가 왜 필요해",
          "original_form": "부대가 왜 필요해",
          "speaker_id": "2",
          "time": "20210904 02:41"
        },
        {
          "id": "MDRW2100040065.1.32",
          "form": "나루토만 보다가 뇌가리 돌아가버렸나",
          "original_form": "나루토만 보다가 뇌가리 돌아가버렸나",
          "speaker_id": "2",
          "time": "20210904 02:41"
        },
        {
          "id": "MDRW2100040065.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:42"
        },
        {
          "id": "MDRW2100040065.1.34",
          "form": "ㅇㅏㅎ",
          "original_form": "ㅇㅏㅎ",
          "speaker_id": "2",
          "time": "20210904 02:44"
        },
        {
          "id": "MDRW2100040065.1.35",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210904 02:44"
        },
        {
          "id": "MDRW2100040065.1.36",
          "form": "쿠죠봉행이 애초에 문서부터 조작했다?",
          "original_form": "쿠죠봉행이 애초에 문서부터 조작했다?",
          "speaker_id": "2",
          "time": "20210904 02:44"
        },
        {
          "id": "MDRW2100040065.1.37",
          "form": "들리나요?",
          "original_form": "들리나요?",
          "speaker_id": "2",
          "time": "20210904 02:44"
        },
        {
          "id": "MDRW2100040065.1.38",
          "form": "세탁기 돌아가는 소리?",
          "original_form": "세탁기 돌아가는 소리?",
          "speaker_id": "2",
          "time": "20210904 02:44"
        },
        {
          "id": "MDRW2100040065.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:44"
        },
        {
          "id": "MDRW2100040065.1.40",
          "form": "뭔 신이",
          "original_form": "뭔 신이",
          "speaker_id": "2",
          "time": "20210904 02:44"
        },
        {
          "id": "MDRW2100040065.1.41",
          "form": "근데 또",
          "original_form": "근데 또",
          "speaker_id": "1",
          "time": "20210904 02:45"
        },
        {
          "id": "MDRW2100040065.1.42",
          "form": "ㅃㄷㄱㄹ도 아니고",
          "original_form": "ㅃㄷㄱㄹ도 아니고",
          "speaker_id": "2",
          "time": "20210904 02:45"
        },
        {
          "id": "MDRW2100040065.1.43",
          "form": "나중에 얘기 나오긴 하지만",
          "original_form": "나중에 얘기 나오긴 하지만",
          "speaker_id": "1",
          "time": "20210904 02:45"
        },
        {
          "id": "MDRW2100040065.1.44",
          "form": "뭔 부작용이 있는지 모르는 것도 아님",
          "original_form": "뭔 부작용이 있는지 모르는 것도 아님",
          "speaker_id": "1",
          "time": "20210904 02:45"
        },
        {
          "id": "MDRW2100040065.1.45",
          "form": "일단 다 알고 있긴 해 ㅇㅇ",
          "original_form": "일단 다 알고 있긴 해 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210904 02:45"
        },
        {
          "id": "MDRW2100040065.1.46",
          "form": "와 근데 그럼 중국이 보는ㅎ 일본이란",
          "original_form": "와 근데 그럼 중국이 보는ㅎ 일본이란",
          "speaker_id": "2",
          "time": "20210904 02:45"
        },
        {
          "id": "MDRW2100040065.1.47",
          "form": "이런건가?",
          "original_form": "이런건가?",
          "speaker_id": "2",
          "time": "20210904 02:45"
        },
        {
          "id": "MDRW2100040065.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:45"
        },
        {
          "id": "MDRW2100040065.1.49",
          "form": "그런듯",
          "original_form": "그런듯",
          "speaker_id": "1",
          "time": "20210904 02:45"
        },
        {
          "id": "MDRW2100040065.1.50",
          "form": "그리고 사실",
          "original_form": "그리고 사실",
          "speaker_id": "1",
