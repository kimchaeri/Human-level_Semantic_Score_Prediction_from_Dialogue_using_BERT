      "utterance": [
        {
          "id": "MDRW2100039962.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100039962.1.2",
          "form": "영화좋아하세요?",
          "original_form": "영화좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100039962.1.3",
          "form": "좋아하는데 보러못간지 꽤됐어요ㅠㅠ",
          "original_form": "좋아하는데 보러못간지 꽤됐어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100039962.1.4",
          "form": "맞아여ㅜ~ 영화관을 언제갔는지기억도안나네요",
          "original_form": "맞아여ㅜ~ 영화관을 언제갔는지기억도안나네요",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100039962.1.5",
          "form": "그래서 티비로 킬러들에 보디가드  결제해서 봤어요",
          "original_form": "그래서 티비로 킬러들에 보디가드  결제해서 봤어요",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100039962.1.6",
          "form": "영화관 가서 봐야 실감나는데ㅠㅠ",
          "original_form": "영화관 가서 봐야 실감나는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100039962.1.7",
          "form": "그쵸ㅠㅜ~~팝콘도뭔가 더 맛잇능거같고ㅠㅠ",
          "original_form": "그쵸ㅠㅜ~~팝콘도뭔가 더 맛잇능거같고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100039962.1.8",
          "form": "맞아요 빵빵한 음질에서 영화를 봐야지 더 실감나는데ㅠㅠ",
          "original_form": "맞아요 빵빵한 음질에서 영화를 봐야지 더 실감나는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039962.1.9",
          "form": "코로나 끝나면 영화 부터 봐야 될 거 같아요",
          "original_form": "코로나 끝나면 영화 부터 봐야 될 거 같아요",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039962.1.10",
          "form": "맞아요~ 음향도 큰몫인거같아요",
          "original_form": "맞아요~ 음향도 큰몫인거같아요",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039962.1.11",
          "form": "꼭 공포 영화가 아니라도 음향은 정말 중요한 거 같아요",
          "original_form": "꼭 공포 영화가 아니라도 음향은 정말 중요한 거 같아요",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039962.1.12",
          "form": "좋아하는 영화 있으세요?",
          "original_form": "좋아하는 영화 있으세요?",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100039962.1.13",
          "form": "전 영화를 크게 즐기는편은아니에요 ㅎ",
          "original_form": "전 영화를 크게 즐기는편은아니에요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100039962.1.14",
          "form": "앗ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039962.1.15",
          "form": "최근에 보신 영화는 뭐에요?ㅋㅋ",
          "original_form": "최근에 보신 영화는 뭐에요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039962.1.16",
          "form": "남한산성이요 ㅋㅋㅋ",
          "original_form": "남한산성이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039962.1.17",
          "form": "역린 ㅎ",
          "original_form": "역린 ㅎ",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039962.1.18",
          "form": "같은날 혼자 밤새면서 봤어요 ㅋㅋ",
          "original_form": "같은날 혼자 밤새면서 봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039962.1.19",
          "form": "아 역시 영화도 연속해서 보는게 재밌죠ㅋㅋ",
          "original_form": "아 역시 영화도 연속해서 보는게 재밌죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039962.1.20",
          "form": "어쩌다 보개되었는데 ㅋㅋㅋ그래도 잼있게봤어요~",
          "original_form": "어쩌다 보개되었는데 ㅋㅋㅋ그래도 잼있게봤어요~",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039962.1.21",
          "form": "재밌게 보셨다니 다행이네요ㅎㅎ",
          "original_form": "재밌게 보셨다니 다행이네요ㅎㅎ",
