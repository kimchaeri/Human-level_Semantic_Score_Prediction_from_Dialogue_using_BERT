      "utterance": [
        {
          "id": "MDRW2100030370.1.1",
          "form": "무슨돈",
          "original_form": "무슨돈",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100030370.1.2",
          "form": "코인하고 남은돈",
          "original_form": "코인하고 남은돈",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100030370.1.3",
          "form": "열심히 원금복구하는 중이지",
          "original_form": "열심히 원금복구하는 중이지",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100030370.1.4",
          "form": "엄마가 잃은건데 왜 내가 복구해줘야되는지 모르겠어",
          "original_form": "엄마가 잃은건데 왜 내가 복구해줘야되는지 모르겠어",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100030370.1.5",
          "form": "부탁혀 너무 많이 잃었어",
          "original_form": "부탁혀 너무 많이 잃었어",
          "speaker_id": "1",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100030370.1.6",
          "form": "복구해주면 뭐해줄거야",
          "original_form": "복구해주면 뭐해줄거야",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100030370.1.7",
          "form": "복구해주면 반줄게",
          "original_form": "복구해주면 반줄게",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100030370.1.8",
          "form": "그럼 열심히 해볼게",
          "original_form": "그럼 열심히 해볼게",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030370.1.9",
          "form": "요즘 장이 어때",
          "original_form": "요즘 장이 어때",
          "speaker_id": "1",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030370.1.10",
          "form": "요즘 장 좋아",
          "original_form": "요즘 장 좋아",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030370.1.11",
          "form": "할만해",
          "original_form": "할만해",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030370.1.12",
          "form": "그럼 나도 다시 들어갈까",
          "original_form": "그럼 나도 다시 들어갈까",
          "speaker_id": "1",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030370.1.13",
          "form": "아니 엄마는 하지마 그냥...",
          "original_form": "아니 엄마는 하지마 그냥...",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030370.1.14",
          "form": "나 몰래 산것도 물려있더만",
          "original_form": "나 몰래 산것도 물려있더만",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030370.1.15",
          "form": "어떻게 알았어",
          "original_form": "어떻게 알았어",
          "speaker_id": "1",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030370.1.16",
          "form": "말안한다고 모를줄알아!!",
          "original_form": "말안한다고 모를줄알아!!",
          "speaker_id": "2",
          "time": "20210904 00:28"
        }
