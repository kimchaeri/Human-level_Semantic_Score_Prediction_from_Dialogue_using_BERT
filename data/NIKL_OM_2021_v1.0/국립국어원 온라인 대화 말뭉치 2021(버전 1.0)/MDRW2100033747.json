      "utterance": [
        {
          "id": "MDRW2100033747.1.1",
          "form": "싱가폴?",
          "original_form": "싱가폴?",
          "speaker_id": "2",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100033747.1.2",
          "form": "어땠냐구?",
          "original_form": "어땠냐구?",
          "speaker_id": "1",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100033747.1.3",
          "form": "나 아무래도 오래전에 간거라",
          "original_form": "나 아무래도 오래전에 간거라",
          "speaker_id": "1",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100033747.1.4",
          "form": "기억이 잘 안나지만",
          "original_form": "기억이 잘 안나지만",
          "speaker_id": "1",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100033747.1.5",
          "form": "아 아니!",
          "original_form": "아 아니!",
          "speaker_id": "2",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100033747.1.6",
          "form": "그래도 몇몇 장소는 생생해",
          "original_form": "그래도 몇몇 장소는 생생해",
          "speaker_id": "1",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100033747.1.7",
          "form": "너 아직 싱가포르 가고싶냐고..!!",
          "original_form": "너 아직 싱가포르 가고싶냐고..!!",
          "speaker_id": "2",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100033747.1.8",
          "form": "아하아하",
          "original_form": "아하아하",
          "speaker_id": "1",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100033747.1.9",
          "form": "나도 싱가포르는 가봐서...",
          "original_form": "나도 싱가포르는 가봐서...",
          "speaker_id": "2",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100033747.1.10",
          "form": "당연하지ㅠㅠ 너무 다시 가고싶어",
          "original_form": "당연하지ㅠㅠ 너무 다시 가고싶어",
          "speaker_id": "1",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100033747.1.11",
          "form": "비행시간은 몇시간 정도였어?",
          "original_form": "비행시간은 몇시간 정도였어?",
          "speaker_id": "2",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100033747.1.12",
          "form": "아무래도 거기는 교통이 좋아서",
          "original_form": "아무래도 거기는 교통이 좋아서",
          "speaker_id": "1",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100033747.1.13",
          "form": "다니기 편했어",
          "original_form": "다니기 편했어",
          "speaker_id": "1",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100033747.1.14",
          "form": "맞아 그치.",
          "original_form": "맞아 그치.",
          "speaker_id": "2",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100033747.1.15",
          "form": "비행시간...잘 기억안나는데",
          "original_form": "비행시간...잘 기억안나는데",
          "speaker_id": "1",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100033747.1.16",
          "form": "6시간이었던 것 같아",
          "original_form": "6시간이었던 것 같아",
          "speaker_id": "1",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100033747.1.17",
          "form": "비행기는 너무 불편했어",
          "original_form": "비행기는 너무 불편했어",
          "speaker_id": "1",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100033747.1.18",
          "form": "왜 불편했지?",
          "original_form": "왜 불편했지?",
          "speaker_id": "2",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100033747.1.19",
          "form": "좁았어?",
          "original_form": "좁았어?",
          "speaker_id": "2",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100033747.1.20",
          "form": "양옆에 남자들이 탔거든",
          "original_form": "양옆에 남자들이 탔거든",
          "speaker_id": "1",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100033747.1.21",
          "form": "누가 코라도 곤거야?",
          "original_form": "누가 코라도 곤거야?",
          "speaker_id": "2",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100033747.1.22",
          "form": "너는 비즈니스 타본적있니?",
          "original_form": "너는 비즈니스 타본적있니?",
          "speaker_id": "1",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100033747.1.23",
          "form": "아아 좁았겠다",
          "original_form": "아아 좁았겠다",
          "speaker_id": "2",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100033747.1.24",
          "form": "당연",
          "original_form": "당연",
          "speaker_id": "2",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100033747.1.25",
          "form": "없지.",
          "original_form": "없지.",
          "speaker_id": "2",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100033747.1.26",
          "form": "난 경제적인 사람이란다.",
          "original_form": "난 경제적인 사람이란다.",
          "speaker_id": "2",
          "time": "20210825 00:06"
        }
      ]
    }
