      "utterance": [
        {
          "id": "MDRW2100036073.1.1",
          "form": "안녕하세요 점심은 어떤거 드셨어용?",
          "original_form": "안녕하세요 점심은 어떤거 드셨어용?",
          "speaker_id": "1",
          "time": "20210830 15:41"
        },
        {
          "id": "MDRW2100036073.1.2",
          "form": "저 파스타요",
          "original_form": "저 파스타요",
          "speaker_id": "2",
          "time": "20210830 15:41"
        },
        {
          "id": "MDRW2100036073.1.3",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210830 15:41"
        },
        {
          "id": "MDRW2100036073.1.4",
          "form": "전 수제버거 먹었어요",
          "original_form": "전 수제버거 먹었어요",
          "speaker_id": "1",
          "time": "20210830 15:41"
        },
        {
          "id": "MDRW2100036073.1.5",
          "form": "어떤 파스타인가용?",
          "original_form": "어떤 파스타인가용?",
          "speaker_id": "1",
          "time": "20210830 15:41"
        },
        {
          "id": "MDRW2100036073.1.6",
          "form": "맛난거드셨네용",
          "original_form": "맛난거드셨네용",
          "speaker_id": "2",
          "time": "20210830 15:41"
        },
        {
          "id": "MDRW2100036073.1.7",
          "form": "알리오올리오요!",
          "original_form": "알리오올리오요!",
          "speaker_id": "2",
          "time": "20210830 15:41"
        },
        {
          "id": "MDRW2100036073.1.8",
          "form": "오 직접 해드신건가요?",
          "original_form": "오 직접 해드신건가요?",
          "speaker_id": "1",
          "time": "20210830 15:41"
        },
        {
          "id": "MDRW2100036073.1.9",
          "form": "넴!!",
          "original_form": "넴!!",
          "speaker_id": "2",
          "time": "20210830 15:45"
        },
        {
          "id": "MDRW2100036073.1.10",
          "form": "오 요리 잘하세요?",
          "original_form": "오 요리 잘하세요?",
          "speaker_id": "1",
          "time": "20210830 15:45"
        },
        {
          "id": "MDRW2100036073.1.11",
          "form": "간단한것만 할줄 알아요ㅎㅎ",
          "original_form": "간단한것만 할줄 알아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 15:56"
        },
        {
          "id": "MDRW2100036073.1.12",
          "form": "그래도 대단하시네용ㅋㅋㅋ 저는 시판소스 사먹는뎅",
          "original_form": "그래도 대단하시네용ㅋㅋㅋ 저는 시판소스 사먹는뎅",
          "speaker_id": "1",
          "time": "20210830 15:57"
        },
        {
          "id": "MDRW2100036073.1.13",
          "form": "저도 시판써요ㅋㅋㅋ",
          "original_form": "저도 시판써요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:58"
        },
        {
          "id": "MDRW2100036073.1.14",
          "form": "직접하면 맛이..",
          "original_form": "직접하면 맛이..",
          "speaker_id": "2",
          "time": "20210830 15:58"
        },
        {
          "id": "MDRW2100036073.1.15",
          "form": "앗ㅋㅋㅋㅋ 네네 맛이...",
          "original_form": "앗ㅋㅋㅋㅋ 네네 맛이...",
          "speaker_id": "1",
          "time": "20210830 15:58"
        },
        {
          "id": "MDRW2100036073.1.16",
          "form": "시판 소스는 청정원께 제일 맛있어요",
          "original_form": "시판 소스는 청정원께 제일 맛있어요",
          "speaker_id": "1",
          "time": "20210830 15:58"
        },
        {
          "id": "MDRW2100036073.1.17",
          "form": "엇 맞아요 저도 그거만먹어요ㅋㅋㅋ",
          "original_form": "엇 맞아요 저도 그거만먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:00"
        },
        {
          "id": "MDRW2100036073.1.18",
          "form": "폰타나보다 더 맛있더라구요ㅋㅋㅋ",
          "original_form": "폰타나보다 더 맛있더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:00"
        },
        {
          "id": "MDRW2100036073.1.19",
          "form": "종류도 다양하고~",
          "original_form": "종류도 다양하고~",
