      "utterance": [
        {
          "id": "MDRW2100043128.1.1",
          "form": "안녕하세요 ~~",
          "original_form": "안녕하세요 ~~",
          "speaker_id": "1",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100043128.1.2",
          "form": "요즘에 코로나때문에 여행가기가 힘들죠 ㅠ ㅜ",
          "original_form": "요즘에 코로나때문에 여행가기가 힘들죠 ㅠ ㅜ",
          "speaker_id": "1",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100043128.1.3",
          "form": "최근에 다녀오신 곳 있으세요?",
          "original_form": "최근에 다녀오신 곳 있으세요?",
          "speaker_id": "1",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100043128.1.4",
          "form": "네ㅜㅜ 최근엔 코로나 때문에 아예 못갔어요",
          "original_form": "네ㅜㅜ 최근엔 코로나 때문에 아예 못갔어요",
          "speaker_id": "2",
          "time": "20210907 14:43"
        },
        {
          "id": "MDRW2100043128.1.5",
          "form": "그쵸 ...",
          "original_form": "그쵸 ...",
          "speaker_id": "1",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100043128.1.6",
          "form": "그럼 제일 마지막으로 다녀오신 곳이 어디세요 ??",
          "original_form": "그럼 제일 마지막으로 다녀오신 곳이 어디세요 ??",
          "speaker_id": "1",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100043128.1.7",
          "form": "음... 기억도 안나요 ㅋㅋㅋㅋㅋ",
          "original_form": "음... 기억도 안나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:46"
        },
        {
          "id": "MDRW2100043128.1.8",
          "form": "name1님은 최근에 어디 가셨어요?",
          "original_form": "&name1&님은 최근에 어디 가셨어요?",
          "speaker_id": "2",
          "time": "20210907 14:46"
        },
        {
          "id": "MDRW2100043128.1.9",
          "form": "저는 여행이라고 하기는 뭐하지만 인사동 다녀왔어요 ~!",
          "original_form": "저는 여행이라고 하기는 뭐하지만 인사동 다녀왔어요 ~!",
          "speaker_id": "1",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043128.1.10",
          "form": "날씨가 진짜 너무 좋아서 다행이었어요 옛날 생각들도 나고요",
          "original_form": "날씨가 진짜 너무 좋아서 다행이었어요 옛날 생각들도 나고요",
          "speaker_id": "1",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043128.1.11",
          "form": "아하 인사동에 뭐있어요?",
          "original_form": "아하 인사동에 뭐있어요?",
          "speaker_id": "2",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043128.1.12",
          "form": "인사동 쌈지길이요! 근데 여기 아니어도 그냥 거리 돌아다니는 것만으로도 힐링아 되더라고요",
          "original_form": "인사동 쌈지길이요! 근데 여기 아니어도 그냥 거리 돌아다니는 것만으로도 힐링아 되더라고요",
          "speaker_id": "1",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100043128.1.13",
          "form": "전시회가 진짜 많았어요 졸업전시회 같은 거요",
          "original_form": "전시회가 진짜 많았어요 졸업전시회 같은 거요",
          "speaker_id": "1",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100043128.1.14",
          "form": "오오 구경 재밌었겠어요!",
          "original_form": "오오 구경 재밌었겠어요!",
          "speaker_id": "2",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100043128.1.15",
          "form": "근처에 대학이 있나요?",
          "original_form": "근처에 대학이 있나요?",
          "speaker_id": "2",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100043128.1.16",
          "form": "그건 모르겠어요 !근데 되게 다양한 대학들에서 하는 것 같더라고요 !",
          "original_form": "그건 모르겠어요 !근데 되게 다양한 대학들에서 하는 것 같더라고요 !",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100043128.1.17",
          "form": "정말 세상엔 대단한 사람들이 정말 많다는 걸 다시한번 느꼈죠",
          "original_form": "정말 세상엔 대단한 사람들이 정말 많다는 걸 다시한번 느꼈죠",
          "speaker_id": "1",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100043128.1.18",
          "form": "코로나 끝나고 놀러가고 싶으신 곳 있으세요 ?",
          "original_form": "코로나 끝나고 놀러가고 싶으신 곳 있으세요 ?",
          "speaker_id": "1",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100043128.1.19",
          "form": "아하 ㅋㅋ",
          "original_form": "아하 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:02"
        },
        {
          "id": "MDRW2100043128.1.20",
          "form": "강릉 가고싶어요!",
          "original_form": "강릉 가고싶어요!",
          "speaker_id": "2",
          "time": "20210907 15:02"
        },
        {
          "id": "MDRW2100043128.1.21",
          "form": "오 강릉 좋죠 !",
          "original_form": "오 강릉 좋죠 !",
          "speaker_id": "1",
          "time": "20210907 15:06"
        },
        {
          "id": "MDRW2100043128.1.22",
          "form": "강릉 가보신 적 있으세요 ?",
          "original_form": "강릉 가보신 적 있으세요 ?",
          "speaker_id": "1",
          "time": "20210907 15:06"
        },
        {
          "id": "MDRW2100043128.1.23",
          "form": "없어요 가볼만한 데 추천해주실 수 있나요?",
          "original_form": "없어요 가볼만한 데 추천해주실 수 있나요?",
          "speaker_id": "2",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043128.1.24",
          "form": "저도 사실 안가봤어요 ,,",
          "original_form": "저도 사실 안가봤어요 ,,",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100043128.1.25",
          "form": "바다 쪽 많이 가시더라고요",
          "original_form": "바다 쪽 많이 가시더라고요",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100043128.1.26",
          "form": "아 ㅋㅋ 그쵸 바다 구경 많이 해야겠네요",
          "original_form": "아 ㅋㅋ 그쵸 바다 구경 많이 해야겠네요",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100043128.1.27",
          "form": "ㅎㅎ 좋아요 !",
          "original_form": "ㅎㅎ 좋아요 !",
          "speaker_id": "1",
          "time": "20210907 23:14"
        }
      ]
