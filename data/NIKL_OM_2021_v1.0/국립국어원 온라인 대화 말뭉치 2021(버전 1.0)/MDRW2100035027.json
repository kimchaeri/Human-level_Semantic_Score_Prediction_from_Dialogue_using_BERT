      "utterance": [
        {
          "id": "MDRW2100035027.1.1",
          "form": "저는 새로운거에 도전하는것도 자신없어해서",
          "original_form": "저는 새로운거에 도전하는것도 자신없어해서",
          "speaker_id": "2",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100035027.1.2",
          "form": "경험담듣는거 좋아해요ㅋㅋㅋㅋ",
          "original_form": "경험담듣는거 좋아해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100035027.1.3",
          "form": "음식에있어서도..! ㅋㅋㅋ",
          "original_form": "음식에있어서도..! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100035027.1.4",
          "form": "제가 감사하죵ㅎㅎ 저는 먹는이야기 다 좋아해요 하는것도 듣는것도",
          "original_form": "제가 감사하죵ㅎㅎ 저는 먹는이야기 다 좋아해요 하는것도 듣는것도",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100035027.1.5",
          "form": "집에 음식관련 책들이 한가득이에요ㅎㅎ",
          "original_form": "집에 음식관련 책들이 한가득이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100035027.1.6",
          "form": "우와 음식관련책이요?",
          "original_form": "우와 음식관련책이요?",
          "speaker_id": "2",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100035027.1.7",
          "form": "요리 아니고 먹는 이야기들이 주예요ㅎ",
          "original_form": "요리 아니고 먹는 이야기들이 주예요ㅎ",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100035027.1.8",
          "form": "요리는...저에겐 허락되지 않은 영역이라",
          "original_form": "요리는...저에겐 허락되지 않은 영역이라",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100035027.1.9",
          "form": "아ㅋㅋㅋ 자꾸 요리책이 떠올라가지구..ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ 자꾸 요리책이 떠올라가지구..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100035027.1.10",
          "form": "진짜 기본적으로 관심이많으시니 새로운것도 도전많이하시구 그러시네요!",
          "original_form": "진짜 기본적으로 관심이많으시니 새로운것도 도전많이하시구 그러시네요!",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100035027.1.11",
          "form": "ㅎㅎ요리만 빼고요ㅎ",
          "original_form": "ㅎㅎ요리만 빼고요ㅎ",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100035027.1.12",
          "form": "최근에 시노다 과장의 삼시세끼라는 책을 읽었는데",
          "original_form": "최근에 시노다 과장의 삼시세끼라는 책을 읽었는데",
          "speaker_id": "1",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100035027.1.13",
          "form": "하루 삼시세끼를 다 그림으로 그리고 옆에 감상을 적었더라고요",
          "original_form": "하루 삼시세끼를 다 그림으로 그리고 옆에 감상을 적었더라고요",
          "speaker_id": "1",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100035027.1.14",
          "form": "일종의 삼시세끼일기?",
          "original_form": "일종의 삼시세끼일기?",
          "speaker_id": "1",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100035027.1.15",
          "form": "우와 재밌을거 같은데요*_*)!!",
          "original_form": "우와 재밌을거 같은데요*_*)!!",
          "speaker_id": "2",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100035027.1.16",
          "form": "그림이 팔할인데 이분에게서 찐 광기를 느꼈습니다ㅎㅎ",
          "original_form": "그림이 팔할인데 이분에게서 찐 광기를 느꼈습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100035027.1.17",
          "form": "찐광기ㅋㅋㅋㄱㅋㅋ",
          "original_form": "찐광기ㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100035027.1.18",
          "form": "삼시세끼일기라니 색다르네요",
          "original_form": "삼시세끼일기라니 색다르네요",
          "speaker_id": "2",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100035027.1.19",
          "form": "그와중에 승진까지하시구 부지런한분이시네요",
          "original_form": "그와중에 승진까지하시구 부지런한분이시네요",
          "speaker_id": "2",
          "time": "20210829 00:25"
        },
        {
          "id": "MDRW2100035027.1.20",
          "form": "ㅋㅋㅋㄱㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:25"
        },
        {
          "id": "MDRW2100035027.1.21",
          "form": "ㅎㅎ 책보면서 이렇게 집요한 사람이니 일도 잘했겠거니 합니다ㅎㅎ",
          "original_form": "ㅎㅎ 책보면서 이렇게 집요한 사람이니 일도 잘했겠거니 합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:25"
        },
        {
          "id": "MDRW2100035027.1.22",
          "form": "먹은 음식들 그림 그리고 색칠까지 했어요ㅎㅎㅎ",
          "original_form": "먹은 음식들 그림 그리고 색칠까지 했어요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100035027.1.23",
          "form": "아무 감동도 없는 세끼 기록책이라",
          "original_form": "아무 감동도 없는 세끼 기록책이라",
          "speaker_id": "1",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100035027.1.24",
          "form": "감동 없는 삼시세끼 기록지라",
          "original_form": "감동 없는 삼시세끼 기록지라",
          "speaker_id": "1",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100035027.1.25",
          "form": "추천은 안할게요ㅎㅎㅎ",
          "original_form": "추천은 안할게요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100035027.1.26",
          "form": "그분 그림은 잘그리세요?",
          "original_form": "그분 그림은 잘그리세요?",
          "speaker_id": "2",
          "time": "20210829 00:28"
