      "utterance": [
        {
          "id": "MDRW2100035975.1.1",
          "form": "콘서트 한 번 보러가고 싶어요",
          "original_form": "콘서트 한 번 보러가고 싶어요",
          "speaker_id": "2",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100035975.1.2",
          "form": "가본적있으세요?!",
          "original_form": "가본적있으세요?!",
          "speaker_id": "1",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100035975.1.3",
          "form": "전에 멜론에서 하는 시상식 가봤어요!!",
          "original_form": "전에 멜론에서 하는 시상식 가봤어요!!",
          "speaker_id": "2",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100035975.1.4",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100035975.1.5",
          "form": "누구보셨어요?",
          "original_form": "누구보셨어요?",
          "speaker_id": "1",
          "time": "20210830 13:04"
        },
        {
          "id": "MDRW2100035975.1.6",
          "form": "엑소때였는데 엑소랑 빅뱅이랑 아이유가 기억에 남아요ㅋㅋ",
          "original_form": "엑소때였는데 엑소랑 빅뱅이랑 아이유가 기억에 남아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:04"
        },
        {
          "id": "MDRW2100035975.1.7",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210830 13:04"
        },
        {
          "id": "MDRW2100035975.1.8",
          "form": "아이유!!",
          "original_form": "아이유!!",
          "speaker_id": "1",
          "time": "20210830 13:04"
        },
        {
          "id": "MDRW2100035975.1.9",
          "form": "진짜 하얗더라구요!",
          "original_form": "진짜 하얗더라구요!",
          "speaker_id": "2",
          "time": "20210830 13:05"
        },
        {
          "id": "MDRW2100035975.1.10",
          "form": "그때 단발머리였는데 딱 백설공주였어요!",
          "original_form": "그때 단발머리였는데 딱 백설공주였어요!",
          "speaker_id": "2",
          "time": "20210830 13:05"
        },
        {
          "id": "MDRW2100035975.1.11",
          "form": "노래도 많이듣고오셨어요?!!",
          "original_form": "노래도 많이듣고오셨어요?!!",
          "speaker_id": "1",
          "time": "20210830 13:05"
        },
        {
          "id": "MDRW2100035975.1.12",
          "form": "저 외출을 안하는 집순이라 마냥 신기",
          "original_form": "저 외출을 안하는 집순이라 마냥 신기",
          "speaker_id": "1",
          "time": "20210830 13:06"
        },
        {
          "id": "MDRW2100035975.1.13",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210830 13:51"
        },
        {
          "id": "MDRW2100035975.1.14",
          "form": "저도 친구따라 갔던건데ㅋㅋ 사람이 이렇게까지 밀릴 수 있구나 느꼈어요ㅋㅋㅋ",
          "original_form": "저도 친구따라 갔던건데ㅋㅋ 사람이 이렇게까지 밀릴 수 있구나 느꼈어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:52"
        },
        {
          "id": "MDRW2100035975.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ인파라는 말이 맞나봐요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ인파라는 말이 맞나봐요",
          "speaker_id": "1",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100035975.1.16",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100035975.1.17",
          "form": "황당했던건 제 머리에 카메라 렌즈 올려놓고 찍는거에요.?!",
          "original_form": "황당했던건 제 머리에 카메라 렌즈 올려놓고 찍는거에요.?!",
          "speaker_id": "2",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100035975.1.18",
          "form": "뒤 돌 수 없어서 슈퍼마리오 했어요ㅋㅋㅋ",
          "original_form": "뒤 돌 수 없어서 슈퍼마리오 했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100035975.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ헐ㅋㅋㅋㅋㅋ거치대가 되버린ㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ헐ㅋㅋㅋㅋㅋ거치대가 되버린ㅋㅋㄱㅋ",
          "speaker_id": "1",
