      "utterance": [
        {
          "id": "MDRW2100012186.1.1",
          "form": "여행갈 생각잇어?",
          "original_form": "여행갈 생각잇어?",
          "speaker_id": "1",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100012186.1.2",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100012186.1.3",
          "form": "어디갈래?",
          "original_form": "어디갈래?",
          "speaker_id": "1",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100012186.1.4",
          "form": "우리 제주도 어때?",
          "original_form": "우리 제주도 어때?",
          "speaker_id": "2",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100012186.1.5",
          "form": "제주도는 저번에 갓잖아",
          "original_form": "제주도는 저번에 갓잖아",
          "speaker_id": "1",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100012186.1.6",
          "form": "저번에 갔다구???",
          "original_form": "저번에 갔다구???",
          "speaker_id": "2",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100012186.1.7",
          "form": "작년에 갔잖아 올해는 여수가자",
          "original_form": "작년에 갔잖아 올해는 여수가자",
          "speaker_id": "1",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100012186.1.8",
          "form": "아ㅋㅋㅋㅋㅋ맞다 ㅇㅋㅇㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ맞다 ㅇㅋㅇㅋ",
          "speaker_id": "2",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100012186.1.9",
          "form": "여수 가서 뭐하지?",
          "original_form": "여수 가서 뭐하지?",
          "speaker_id": "1",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100012186.1.10",
          "form": "여수하면 밤바다 봐야지!",
          "original_form": "여수하면 밤바다 봐야지!",
          "speaker_id": "2",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100012186.1.11",
          "form": "그건 당연한거구 뭐 먹을래?",
          "original_form": "그건 당연한거구 뭐 먹을래?",
          "speaker_id": "1",
          "time": "20210824 20:10"
        },
        {
          "id": "MDRW2100012186.1.12",
          "form": "음 간장게장 먹구싶다",
          "original_form": "음 간장게장 먹구싶다",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012186.1.13",
          "form": "돌게장어때?",
          "original_form": "돌게장어때?",
          "speaker_id": "1",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012186.1.14",
          "form": "헐 돌게장 좋지!돌게장 먹어봤어?",
          "original_form": "헐 돌게장 좋지!돌게장 먹어봤어?",
          "speaker_id": "2",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012186.1.15",
          "form": "아니 한번 먹어보고싶어",
          "original_form": "아니 한번 먹어보고싶어",
          "speaker_id": "1",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012186.1.16",
          "form": "넌 먹어봤어?",
          "original_form": "넌 먹어봤어?",
          "speaker_id": "1",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012186.1.17",
          "form": "응 그냥 게장보다 더 작아서 양이 적어서 아쉬워",
          "original_form": "응 그냥 게장보다 더 작아서 양이 적어서 아쉬워",
          "speaker_id": "2",
          "time": "20210824 20:13"
        },
        {
          "id": "MDRW2100012186.1.18",
          "form": "무한리필집 있지 않을까?",
          "original_form": "무한리필집 있지 않을까?",
          "speaker_id": "1",
          "time": "20210824 20:13"
        },
        {
          "id": "MDRW2100012186.1.19",
          "form": "헐 그럼 돌게장 무한리필 집 가서 먹자!",
          "original_form": "헐 그럼 돌게장 무한리필 집 가서 먹자!",
          "speaker_id": "2",
          "time": "20210824 20:14"
        },
        {
          "id": "MDRW2100012186.1.20",
          "form": "너무 좋다 그리구 여수가면 쑥 아이스크림 먹어야해",
          "original_form": "너무 좋다 그리구 여수가면 쑥 아이스크림 먹어야해",
          "speaker_id": "1",
          "time": "20210824 20:14"
        },
        {
          "id": "MDRW2100012186.1.21",
          "form": "쑥 아이스크림?그게 모여",
          "original_form": "쑥 아이스크림?그게 모여",
          "speaker_id": "2",
          "time": "20210824 20:14"
        },
        {
          "id": "MDRW2100012186.1.22",
          "form": "똥모양 아이스크림인데 맛있대",
          "original_form": "똥모양 아이스크림인데 맛있대",
          "speaker_id": "1",
          "time": "20210824 20:15"
        },
        {
          "id": "MDRW2100012186.1.23",
          "form": "아 더러워;;;;;",
          "original_form": "아 더러워;;;;;",
          "speaker_id": "2",
          "time": "20210824 20:15"
        },
        {
          "id": "MDRW2100012186.1.24",
          "form": "뭐래 엄청 맛있어",
          "original_form": "뭐래 엄청 맛있어",
          "speaker_id": "1",
          "time": "20210824 20:15"
        },
        {
          "id": "MDRW2100012186.1.25",
          "form": "쑥 맛이라고 하니까 상상이 안되네",
          "original_form": "쑥 맛이라고 하니까 상상이 안되네",
          "speaker_id": "2",
          "time": "20210824 20:15"
        },
        {
          "id": "MDRW2100012186.1.26",
          "form": "응 나도 이상한맛일줄 알앗는데 은근 맛있어",
          "original_form": "응 나도 이상한맛일줄 알앗는데 은근 맛있어",
          "speaker_id": "1",
          "time": "20210824 20:16"
        }
      ]
