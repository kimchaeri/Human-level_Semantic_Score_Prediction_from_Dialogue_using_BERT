      "utterance": [
        {
          "id": "MDRW2100030228.1.1",
          "form": "웅 잼잇지",
          "original_form": "웅 잼잇지",
          "speaker_id": "2",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030228.1.2",
          "form": "집에서 보는거랑 또 느낌이 다른거같아 그것두",
          "original_form": "집에서 보는거랑 또 느낌이 다른거같아 그것두",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030228.1.3",
          "form": "하긴 스크린도 크고",
          "original_form": "하긴 스크린도 크고",
          "speaker_id": "2",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030228.1.4",
          "form": "일단 넓고 사람들이 주변에있고",
          "original_form": "일단 넓고 사람들이 주변에있고",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030228.1.5",
          "form": "스피커도 좋으니깐 ㅋㅋ",
          "original_form": "스피커도 좋으니깐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030228.1.6",
          "form": "따뜻한 팝콘 시원한 음료수",
          "original_form": "따뜻한 팝콘 시원한 음료수",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030228.1.7",
          "form": "따끈따끈 버터구이오징어",
          "original_form": "따끈따끈 버터구이오징어",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030228.1.8",
          "form": "그리구 팝콘 ㅎㅎ",
          "original_form": "그리구 팝콘 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030228.1.9",
          "form": "푹신한 의자 ㅎㅎ",
          "original_form": "푹신한 의자 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030228.1.10",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030228.1.11",
          "form": "영화관은 의자 편한게",
          "original_form": "영화관은 의자 편한게",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030228.1.12",
          "form": "영화볼때는 무슨 팝콘이 좋아?",
          "original_form": "영화볼때는 무슨 팝콘이 좋아?",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030228.1.13",
          "form": "중요한거같아",
          "original_form": "중요한거같아",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030228.1.14",
          "form": "난 카라멜",
          "original_form": "난 카라멜",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
