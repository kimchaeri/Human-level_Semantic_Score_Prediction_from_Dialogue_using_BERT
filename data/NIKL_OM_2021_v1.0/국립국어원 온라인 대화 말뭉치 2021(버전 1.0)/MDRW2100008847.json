      "utterance": [
        {
          "id": "MDRW2100008847.1.1",
          "form": "샘네 집은 사부님이 집안일 많이 도와주시나요?",
          "original_form": "샘네 집은 사부님이 집안일 많이 도와주시나요?",
          "speaker_id": "2",
          "time": "20210822 19:06"
        },
        {
          "id": "MDRW2100008847.1.2",
          "form": "나름 도와주려고 하지만...",
          "original_form": "나름 도와주려고 하지만...",
          "speaker_id": "1",
          "time": "20210822 19:08"
        },
        {
          "id": "MDRW2100008847.1.3",
          "form": "점점점...",
          "original_form": "점점점...",
          "speaker_id": "1",
          "time": "20210822 19:08"
        },
        {
          "id": "MDRW2100008847.1.4",
          "form": "아 안도와 주시는게 도와주시는거?? ㅋ",
          "original_form": "아 안도와 주시는게 도와주시는거?? ㅋ",
          "speaker_id": "2",
          "time": "20210822 19:08"
        },
        {
          "id": "MDRW2100008847.1.5",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008847.1.6",
          "form": "가끔 일을 두번하게 만드는 일이...",
          "original_form": "가끔 일을 두번하게 만드는 일이...",
          "speaker_id": "1",
          "time": "20210822 19:10"
        },
        {
          "id": "MDRW2100008847.1.7",
          "form": "우리집은 신랑이 저보다 요리는 잘해요",
          "original_form": "우리집은 신랑이 저보다 요리는 잘해요",
          "speaker_id": "2",
          "time": "20210822 19:10"
        },
        {
          "id": "MDRW2100008847.1.8",
          "form": "저희집도!!",
          "original_form": "저희집도!!",
          "speaker_id": "1",
          "time": "20210822 19:10"
        },
        {
          "id": "MDRW2100008847.1.9",
          "form": "저는 뒷처리를 ㅋㅋㅋ",
          "original_form": "저는 뒷처리를 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:10"
        },
        {
          "id": "MDRW2100008847.1.10",
          "form": "다행이예요 그래서 여태 같이 사나봅니다 ㅋ",
          "original_form": "다행이예요 그래서 여태 같이 사나봅니다 ㅋ",
          "speaker_id": "2",
          "time": "20210822 19:10"
        },
        {
          "id": "MDRW2100008847.1.11",
          "form": "사부님이 육아는 잘하시나요?",
          "original_form": "사부님이 육아는 잘하시나요?",
          "speaker_id": "2",
          "time": "20210822 19:11"
        },
        {
          "id": "MDRW2100008847.1.12",
          "form": "육아는 그닥 소질이 없어 보입니다",
          "original_form": "육아는 그닥 소질이 없어 보입니다",
          "speaker_id": "1",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008847.1.13",
          "form": "저희집도 잘 놀아만 줍니다",
          "original_form": "저희집도 잘 놀아만 줍니다",
          "speaker_id": "2",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008847.1.14",
          "form": "잘 싸우기도 한다는 함정이.. ㅠ",
          "original_form": "잘 싸우기도 한다는 함정이.. ㅠ",
          "speaker_id": "2",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008847.1.15",
          "form": "저희집은 같이 놀다가 애가 꼭 울어요",
          "original_form": "저희집은 같이 놀다가 애가 꼭 울어요",
          "speaker_id": "1",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008847.1.16",
          "form": "ㅋㅋㅋㅋ 우리집도요",
          "original_form": "ㅋㅋㅋㅋ 우리집도요",
          "speaker_id": "2",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008847.1.17",
          "form": "모든 집 남자들이 다 비슷한가봐요",
          "original_form": "모든 집 남자들이 다 비슷한가봐요",
          "speaker_id": "2",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008847.1.18",
          "form": "그래서 육아대신 밥을 차려요",
          "original_form": "그래서 육아대신 밥을 차려요",
          "speaker_id": "1",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008847.1.19",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008847.1.20",
          "form": "교육도 안봐주시죠",
          "original_form": "교육도 안봐주시죠",
          "speaker_id": "2",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008847.1.21",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008847.1.22",
          "form": "우리집은 신랑은 교육엔 완전 무관심이예요",
          "original_form": "우리집은 신랑은 교육엔 완전 무관심이예요",
          "speaker_id": "2",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008847.1.23",
          "form": "교육에 잠깐 신경쓰다가 애랑 트러블 생기고 스탑!!",
          "original_form": "교육에 잠깐 신경쓰다가 애랑 트러블 생기고 스탑!!",
          "speaker_id": "1",
          "time": "20210822 19:45"
        },
        {
          "id": "MDRW2100008847.1.24",
          "form": "저도 신랑이랑 교육관이 너무 안맞아요 ㅠ",
          "original_form": "저도 신랑이랑 교육관이 너무 안맞아요 ㅠ",
          "speaker_id": "2",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008847.1.25",
          "form": "애가 아빠랑 공부하는거 싫다고 해서...ㅠㅠ",
          "original_form": "애가 아빠랑 공부하는거 싫다고 해서...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008847.1.26",
          "form": "악!!! 아빠가 강압적으로 가르치시나봐요",
          "original_form": "악!!! 아빠가 강압적으로 가르치시나봐요",
          "speaker_id": "2",
          "time": "20210822 19:48"
        },
        {
          "id": "MDRW2100008847.1.27",
          "form": "제가 보기엔 좀 그랬던거 같아요",
          "original_form": "제가 보기엔 좀 그랬던거 같아요",
          "speaker_id": "1",
          "time": "20210822 19:48"
        },
        {
          "id": "MDRW2100008847.1.28",
          "form": "전 제가 좀 강압적으로 ㅠ",
          "original_form": "전 제가 좀 강압적으로 ㅠ",
          "speaker_id": "2",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008847.1.29",
          "form": "애가 못하는걸 보면 참을수가 없어요 ㅠ",
          "original_form": "애가 못하는걸 보면 참을수가 없어요 ㅠ",
          "speaker_id": "2",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008847.1.30",
          "form": "저도 한때 학습지 하면서 몇번을 던졌던지",
          "original_form": "저도 한때 학습지 하면서 몇번을 던졌던지",
          "speaker_id": "1",
          "time": "20210822 19:56"
        },
        {
          "id": "MDRW2100008847.1.31",
          "form": "악!!! 전 던지진 않았지만 내 자식을 못가르치겠다 싶었어요 ㅋㅋㅋ",
          "original_form": "악!!! 전 던지진 않았지만 내 자식을 못가르치겠다 싶었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:58"
        },
        {
          "id": "MDRW2100008847.1.32",
          "form": "애는 울면서 학습지 한다고 하고...",
          "original_form": "애는 울면서 학습지 한다고 하고...",
          "speaker_id": "1",
          "time": "20210822 19:58"
        },
        {
          "id": "MDRW2100008847.1.33",
          "form": "그때마다 내가 뭐 하는거지 하지만...",
          "original_form": "그때마다 내가 뭐 하는거지 하지만...",
          "speaker_id": "1",
          "time": "20210822 19:58"
        },
        {
          "id": "MDRW2100008847.1.34",
          "form": "그게 참 딜레마죠?",
          "original_form": "그게 참 딜레마죠?",
          "speaker_id": "2",
          "time": "20210822 19:59"
        },
        {
          "id": "MDRW2100008847.1.35",
          "form": "내 자식은 가르칠 수 없다에 공감합니다~",
          "original_form": "내 자식은 가르칠 수 없다에 공감합니다~",
