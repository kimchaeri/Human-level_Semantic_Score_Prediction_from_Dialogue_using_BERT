      "utterance": [
        {
          "id": "MDRW2100042237.1.1",
          "form": "진짜요..?",
          "original_form": "진짜요..?",
          "speaker_id": "2",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042237.1.2",
          "form": "그런거 잘 안봐서 몰랐어요",
          "original_form": "그런거 잘 안봐서 몰랐어요",
          "speaker_id": "2",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042237.1.3",
          "form": "다 로제 떡볶이 때문인가요",
          "original_form": "다 로제 떡볶이 때문인가요",
          "speaker_id": "2",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042237.1.4",
          "form": "앗 로제 때문이란 말은 없었는데",
          "original_form": "앗 로제 때문이란 말은 없었는데",
          "speaker_id": "1",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100042237.1.5",
          "form": "사람들이 배달 음식을 먹은지 지금",
          "original_form": "사람들이 배달 음식을 먹은지 지금",
          "speaker_id": "1",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100042237.1.6",
          "form": "꽤 되었으니 그 사이에",
          "original_form": "꽤 되었으니 그 사이에",
          "speaker_id": "1",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100042237.1.7",
          "form": "아픈 사람들이 많아진 것 같더라구요,,,",
          "original_form": "아픈 사람들이 많아진 것 같더라구요,,,",
          "speaker_id": "1",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100042237.1.8",
          "form": "아!!",
          "original_form": "아!!",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042237.1.9",
          "form": "그렇군요... 그럴수있을것같아요 ㅠ",
          "original_form": "그렇군요... 그럴수있을것같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042237.1.10",
          "form": "저희 배달음식 줄여요..",
          "original_form": "저희 배달음식 줄여요..",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042237.1.11",
          "form": "유병장수 무서워요",
          "original_form": "유병장수 무서워요",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042237.1.12",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042237.1.13",
          "form": "배달음식 진짜.... 단점이 너무 많죠",
          "original_form": "배달음식 진짜.... 단점이 너무 많죠",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042237.1.14",
          "form": "일회용쓰레기도 많이 나오고",
          "original_form": "일회용쓰레기도 많이 나오고",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042237.1.15",
          "form": "돈도 많이 들고 ㅠㅠ",
          "original_form": "돈도 많이 들고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042237.1.16",
          "form": "맞아요 근데 못 끊네오 ㅜㅜㅜ 너무 어려워요",
          "original_form": "맞아요 근데 못 끊네오 ㅜㅜㅜ 너무 어려워요",
          "speaker_id": "2",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042237.1.17",
          "form": "배달용기는 어떻게 처리하세요?",
          "original_form": "배달용기는 어떻게 처리하세요?",
          "speaker_id": "2",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042237.1.18",
          "form": "물에 헹궈서",
          "original_form": "물에 헹궈서",
          "speaker_id": "1",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100042237.1.19",
          "form": "그냥 재활용 쓰레기통에 두는 게 전부인 것 같아요ㅠ",
          "original_form": "그냥 재활용 쓰레기통에 두는 게 전부인 것 같아요ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100042237.1.20",
          "form": "어흑...",
          "original_form": "어흑...",
          "speaker_id": "1",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100042237.1.21",
          "form": "아 중국당면도 먹고싶네요",
          "original_form": "아 중국당면도 먹고싶네요",
          "speaker_id": "1",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100042237.1.22",
          "form": "저두요 혹시 재활용 버리는 날이 정해져임ㅅ나요ㅡ",
          "original_form": "저두요 혹시 재활용 버리는 날이 정해져임ㅅ나요ㅡ",
          "speaker_id": "2",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100042237.1.23",
          "form": "중국당면 칼로리도 아시나요....?",
          "original_form": "중국당면 칼로리도 아시나요....?",
          "speaker_id": "1",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100042237.1.24",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100042237.1.25",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100042237.1.26",
          "form": "아 저희는",
          "original_form": "아 저희는",
          "speaker_id": "1",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100042237.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100042237.1.28",
          "form": "정해져있다가 이제 사라졌어요!",
          "original_form": "정해져있다가 이제 사라졌어요!",
          "speaker_id": "1",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100042237.1.29",
          "form": "name2님... 말을 잇지 못하는...",
          "original_form": "&name2&님... 말을 잇지 못하는...",
          "speaker_id": "1",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100042237.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100042237.1.31",
          "form": "저는 아파트 거주하는데 일주일에 하루 버릴 수 있거든요",
          "original_form": "저는 아파트 거주하는데 일주일에 하루 버릴 수 있거든요",
          "speaker_id": "2",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100042237.1.32",
          "form": "그래서 어쩔 수 없이 배달음식을 시킨만큼 배달용기가 모이거든요",
          "original_form": "그래서 어쩔 수 없이 배달음식을 시킨만큼 배달용기가 모이거든요",
          "speaker_id": "2",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100042237.1.33",
          "form": "그럼 버리는 날 죄책감이 약간 몰려와요 ㅠ",
          "original_form": "그럼 버리는 날 죄책감이 약간 몰려와요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:11"
        },
        {
          "id": "MDRW2100042237.1.34",
          "form": "아 그러시겠다",
          "original_form": "아 그러시겠다",
          "speaker_id": "1",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042237.1.35",
          "form": "이게 딱 모아놓은 것들이 가시적으로....",
          "original_form": "이게 딱 모아놓은 것들이 가시적으로....",
          "speaker_id": "1",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042237.1.36",
          "form": "보이니까.....",
          "original_form": "보이니까.....",
          "speaker_id": "1",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042237.1.37",
          "form": "으아아아 그러면 좀",
          "original_form": "으아아아 그러면 좀",
          "speaker_id": "1",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042237.1.38",
          "form": "줄여야겠다는 생각이 초...큼...",
          "original_form": "줄여야겠다는 생각이 초...큼...",
          "speaker_id": "1",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042237.1.39",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042237.1.40",
          "form": "들 수도 있겠어요?!",
          "original_form": "들 수도 있겠어요?!",
          "speaker_id": "1",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042237.1.41",
          "form": "네 들어요 ㅠ",
          "original_form": "네 들어요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:13"
        }
      ]
    }
