        },
        {
          "id": "MDRW2100009346.1.2",
          "form": "시험 있지요~",
          "original_form": "시험 있지요~",
          "speaker_id": "2",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100009346.1.3",
          "form": "대학생은 중간 기말 이렇게 봐요",
          "original_form": "대학생은 중간 기말 이렇게 봐요",
          "speaker_id": "2",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100009346.1.4",
          "form": "졸업 한지가 3",
          "original_form": "졸업 한지가 3",
          "speaker_id": "1",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100009346.1.5",
          "form": "꽤",
          "original_form": "꽤",
          "speaker_id": "1",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100009346.1.6",
          "form": "요즘 초등학생들은 시험 보나요?",
          "original_form": "요즘 초등학생들은 시험 보나요?",
          "speaker_id": "2",
          "time": "20210824 12:51"
        },
        {
          "id": "MDRW2100009346.1.7",
          "form": "아마도요",
          "original_form": "아마도요",
          "speaker_id": "1",
          "time": "20210824 12:52"
        },
        {
          "id": "MDRW2100009346.1.8",
          "form": "몇 학년인가요",
          "original_form": "몇 학년인가요",
          "speaker_id": "2",
          "time": "20210824 12:55"
        },
        {
          "id": "MDRW2100009346.1.9",
          "form": "졸업했어요",
          "original_form": "졸업했어요",
          "speaker_id": "1",
          "time": "20210824 12:56"
        },
        {
          "id": "MDRW2100009346.1.10",
          "form": "아이 초등학생이라고 하시지 않았나요",
          "original_form": "아이 초등학생이라고 하시지 않았나요",
          "speaker_id": "2",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100009346.1.11",
          "form": "아 저 말하는 줄 알고",
          "original_form": "아 저 말하는 줄 알고",
          "speaker_id": "1",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100009346.1.12",
          "form": "1학년이요",
          "original_form": "1학년이요",
          "speaker_id": "1",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100009346.1.13",
          "form": "아직 어린이군요...",
          "original_form": "아직 어린이군요...",
          "speaker_id": "2",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100009346.1.14",
          "form": "밖을 못 나가서 답답해하겠어요 ㅜㅜ",
          "original_form": "밖을 못 나가서 답답해하겠어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100009346.1.15",
          "form": "니 방학인데도 싫어해요",
          "original_form": "니 방학인데도 싫어해요",
          "speaker_id": "1",
          "time": "20210824 20:43"
        },
        {
          "id": "MDRW2100009346.1.16",
          "form": "아이고 ㅋㅋ 학원이나 학습지는 다니나요?",
          "original_form": "아이고 ㅋㅋ 학원이나 학습지는 다니나요?",
