      "utterance": [
        {
          "id": "MDRW2100006975.1.1",
          "form": "온라인 커뮤니티 하시나요?",
          "original_form": "온라인 커뮤니티 하시나요?",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006975.1.2",
          "form": "아니오 하시나요?",
          "original_form": "아니오 하시나요?",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006975.1.3",
          "form": "저도 잘은 안해요 ㅎㅎ 네이트판 눈팅 아주 가끔 해요!",
          "original_form": "저도 잘은 안해요 ㅎㅎ 네이트판 눈팅 아주 가끔 해요!",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006975.1.4",
          "form": "오!! 옛날에 엄청 핫했잖아요",
          "original_form": "오!! 옛날에 엄청 핫했잖아요",
          "speaker_id": "2",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006975.1.5",
          "form": "지금도 핫한데 요즘은 인스타에서 나 볼 수 있더라고요",
          "original_form": "지금도 핫한데 요즘은 인스타에서 나 볼 수 있더라고요",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006975.1.6",
          "form": "아 인스타가 핫한거 같아요 아님 트위터?",
          "original_form": "아 인스타가 핫한거 같아요 아님 트위터?",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006975.1.7",
          "form": "저는 트위터는 안해요! 트위터 하시나요?",
          "original_form": "저는 트위터는 안해요! 트위터 하시나요?",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006975.1.8",
          "form": "트위터는 뭔가 기능이 어렵더라고요",
          "original_form": "트위터는 뭔가 기능이 어렵더라고요",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006975.1.9",
          "form": "저는 안하지만 맛집 먹으러갈때 검색하는 정도? 에여",
          "original_form": "저는 안하지만 맛집 먹으러갈때 검색하는 정도? 에여",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006975.1.10",
          "form": "아 트위터에도 맛집 검색 기능이 있군요!",
          "original_form": "아 트위터에도 맛집 검색 기능이 있군요!",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006975.1.11",
          "form": "트위터도 해시태그 있나요?",
          "original_form": "트위터도 해시태그 있나요?",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006975.1.12",
          "form": "넹!! 있어요",
          "original_form": "넹!! 있어요",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006975.1.13",
          "form": "인스타랑 비슷한거 같아요",
          "original_form": "인스타랑 비슷한거 같아요",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006975.1.14",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006975.1.15",
          "form": "트위터는 근데 인스타랑 좀 다른 것같아요",
          "original_form": "트위터는 근데 인스타랑 좀 다른 것같아요",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006975.1.16",
          "form": "맞아요 뭔가 혼자말? 같은 느낌",
