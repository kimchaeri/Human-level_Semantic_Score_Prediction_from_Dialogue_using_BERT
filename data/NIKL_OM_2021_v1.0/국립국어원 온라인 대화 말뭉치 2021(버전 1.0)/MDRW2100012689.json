        },
        {
          "id": "MDRW2100012689.1.2",
          "form": "폰겜이야",
          "original_form": "폰겜이야",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100012689.1.3",
          "form": "저는 포켓몬고 해요",
          "original_form": "저는 포켓몬고 해요",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100012689.1.4",
          "form": "아직요?",
          "original_form": "아직요?",
          "speaker_id": "1",
          "time": "20210826 06:29"
        },
        {
          "id": "MDRW2100012689.1.5",
          "form": "네 ㅎㅎ틈틈히 하다보니",
          "original_form": "네 ㅎㅎ틈틈히 하다보니",
          "speaker_id": "2",
          "time": "20210826 09:16"
        },
        {
          "id": "MDRW2100012689.1.6",
          "form": "전 브롤해요",
          "original_form": "전 브롤해요",
          "speaker_id": "1",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100012689.1.7",
          "form": "브롤이 머죠",
          "original_form": "브롤이 머죠",
          "speaker_id": "2",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100012689.1.8",
          "form": "초딩겜이여",
          "original_form": "초딩겜이여",
          "speaker_id": "1",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100012689.1.9",
          "form": "오 재밌나요",
          "original_form": "오 재밌나요",
          "speaker_id": "2",
          "time": "20210826 12:53"
        },
        {
          "id": "MDRW2100012689.1.10",
          "form": "자냐ㅣㅑ때문애 그난 해요",
          "original_form": "자냐ㅣㅑ때문애 그난 해요",
          "speaker_id": "1",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100012689.1.11",
          "form": "자냐가 뭐에요",
          "original_form": "자냐가 뭐에요",
          "speaker_id": "2",
          "time": "20210826 12:55"
        },
        {
          "id": "MDRW2100012689.1.12",
          "form": "자녀 ㅎㅎ",
          "original_form": "자녀 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100012689.1.13",
          "form": "아 아빠폰에 빨았군요",
          "original_form": "아 아빠폰에 빨았군요",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100012689.1.14",
          "form": "깔았군요",
          "original_form": "깔았군요",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100012689.1.15",
          "form": "포켓몬고도 부모님폰에 깔아서 많이들 하더래요",
          "original_form": "포켓몬고도 부모님폰에 깔아서 많이들 하더래요",
          "speaker_id": "2",
