      "utterance": [
        {
          "id": "MDRW2100036746.1.1",
          "form": "오늘가서 또. 샤핑하겠네ㅎㄹ",
          "original_form": "오늘가서 또. 샤핑하겠네ㅎㄹ",
          "speaker_id": "2",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100036746.1.2",
          "form": "응응 쇼핑해야징 ㅋㅋㅋ",
          "original_form": "응응 쇼핑해야징 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100036746.1.3",
          "form": "KB쿠폰왔당 ㅋㅋ",
          "original_form": "KB쿠폰왔당 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100036746.1.4",
          "form": "또 사고 팔고해야되겠구만 ㅋㅋ",
          "original_form": "또 사고 팔고해야되겠구만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:11"
        },
        {
          "id": "MDRW2100036746.1.5",
          "form": "아근데300아니야?할라고",
          "original_form": "아근데300아니야?할라고",
          "speaker_id": "2",
          "time": "20210831 11:16"
        },
        {
          "id": "MDRW2100036746.1.6",
          "form": "?500이었나",
          "original_form": "?500이었나",
          "speaker_id": "2",
          "time": "20210831 11:16"
        },
        {
          "id": "MDRW2100036746.1.7",
          "form": "난여기서그만하고현금화할까싶은데ㅋ",
          "original_form": "난여기서그만하고현금화할까싶은데ㅋ",
          "speaker_id": "2",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100036746.1.8",
          "form": "하게?",
          "original_form": "하게?",
          "speaker_id": "2",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100036746.1.9",
          "form": "응 500만원까지만 하게 ㅋㅋ",
          "original_form": "응 500만원까지만 하게 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:02"
        },
        {
          "id": "MDRW2100036746.1.10",
          "form": "아니면은 그냥 천만원까지 할까도 생각중 ㅋㅋ 수수료가 크지 않더라고 ㅋ",
          "original_form": "아니면은 그냥 천만원까지 할까도 생각중 ㅋㅋ 수수료가 크지 않더라고 ㅋ",
          "speaker_id": "1",
          "time": "20210831 12:02"
        },
        {
          "id": "MDRW2100036746.1.11",
          "form": "한번에??",
          "original_form": "한번에??",
          "speaker_id": "2",
          "time": "20210831 12:02"
        },
        {
          "id": "MDRW2100036746.1.12",
          "form": "사고 팔고ㅋㅋ현금 천이되?ㅋ",
          "original_form": "사고 팔고ㅋㅋ현금 천이되?ㅋ",
          "speaker_id": "2",
          "time": "20210831 12:02"
        },
        {
          "id": "MDRW2100036746.1.13",
          "form": "이요~",
          "original_form": "이요~",
          "speaker_id": "2",
          "time": "20210831 12:02"
        },
        {
          "id": "MDRW2100036746.1.14",
          "form": "이번에는 500만원이대 ㅋㅋ",
          "original_form": "이번에는 500만원이대 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:03"
        },
        {
          "id": "MDRW2100036746.1.15",
          "form": "이번에 내 하고 오빠한테 송금보내놓고 하라고 하게",
          "original_form": "이번에 내 하고 오빠한테 송금보내놓고 하라고 하게",
          "speaker_id": "1",
          "time": "20210831 12:03"
        },
        {
          "id": "MDRW2100036746.1.16",
          "form": "응ㅋㅋㅋ 아니 나저번에 훅내려가지구",
          "original_form": "응ㅋㅋㅋ 아니 나저번에 훅내려가지구",
          "speaker_id": "2",
          "time": "20210831 12:03"
        },
        {
          "id": "MDRW2100036746.1.17",
          "form": "변동이 크게없나 그건?",
          "original_form": "변동이 크게없나 그건?",
          "speaker_id": "2",
          "time": "20210831 12:04"
        },
        {
          "id": "MDRW2100036746.1.18",
          "form": "응 변동이 크게 없던데 ㅋㅋ수수료도 크지 않구",
          "original_form": "응 변동이 크게 없던데 ㅋㅋ수수료도 크지 않구",
          "speaker_id": "1",
          "time": "20210831 12:05"
        },
        {
          "id": "MDRW2100036746.1.19",
          "form": "괜찮아서 이번에 한번 또 해보려구",
          "original_form": "괜찮아서 이번에 한번 또 해보려구",
          "speaker_id": "1",
          "time": "20210831 12:05"
        },
        {
          "id": "MDRW2100036746.1.20",
          "form": "계속 고정이야?",
          "original_form": "계속 고정이야?",
          "speaker_id": "2",
          "time": "20210831 12:06"
        },
        {
          "id": "MDRW2100036746.1.21",
          "form": "고정은 아닌것같던데 ㅋㅋ 5원씩 차이는 있는것같긴하던데 ㅋㅋ",
          "original_form": "고정은 아닌것같던데 ㅋㅋ 5원씩 차이는 있는것같긴하던데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:21"
        },
        {
          "id": "MDRW2100036746.1.22",
          "form": "내할때는 그래도 고정이었음 ㅋㅋ",
          "original_form": "내할때는 그래도 고정이었음 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:21"
        },
        {
          "id": "MDRW2100036746.1.23",
          "form": "내꺼는 원래도 고정이 아니었는데 하필 살때 주루룩",
          "original_form": "내꺼는 원래도 고정이 아니었는데 하필 살때 주루룩",
          "speaker_id": "2",
          "time": "20210831 12:24"
        },
        {
          "id": "MDRW2100036746.1.24",
          "form": "아아아~~~~~그러면 좀 그렇긴해 ㅠㅠ 무섭...ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아아아~~~~~그러면 좀 그렇긴해 ㅠㅠ 무섭...ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100036746.1.25",
          "form": "지금도 고정이야??",
          "original_form": "지금도 고정이야??",
          "speaker_id": "2",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100036746.1.26",
          "form": "모르겟네 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ못봣어 ㅋㅋㅋ",
          "original_form": "모르겟네 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ못봣어 ㅋㅋㅋ",
