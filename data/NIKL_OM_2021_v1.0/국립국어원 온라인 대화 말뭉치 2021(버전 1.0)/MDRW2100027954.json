      "utterance": [
        {
          "id": "MDRW2100027954.1.1",
          "form": "스피릿페어러 할인한다는 소식을 들었는데",
          "original_form": "스피릿페어러 할인한다는 소식을 들었는데",
          "speaker_id": "2",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100027954.1.2",
          "form": "그건 또 언제 사서 언제 하냐",
          "original_form": "그건 또 언제 사서 언제 하냐",
          "speaker_id": "2",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100027954.1.3",
          "form": "전에 사지 않았음?",
          "original_form": "전에 사지 않았음?",
          "speaker_id": "1",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100027954.1.4",
          "form": "안... 샀을 걸",
          "original_form": "안... 샀을 걸",
          "speaker_id": "2",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027954.1.5",
          "form": "스위치로 샀다고 했었던 것 같은데",
          "original_form": "스위치로 샀다고 했었던 것 같은데",
          "speaker_id": "1",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027954.1.6",
          "form": "그게 충전을 잘못해서",
          "original_form": "그게 충전을 잘못해서",
          "speaker_id": "2",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027954.1.7",
          "form": "나한테 없는 국가 걸로 충전해버림",
          "original_form": "나한테 없는 국가 걸로 충전해버림",
          "speaker_id": "2",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027954.1.8",
          "form": "어이없네...",
          "original_form": "어이없네...",
          "speaker_id": "1",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027954.1.9",
          "form": "그냥 허망하게 냅두는 중",
          "original_form": "그냥 허망하게 냅두는 중",
          "speaker_id": "2",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027954.1.10",
          "form": "그래서 스팀으로 사게",
          "original_form": "그래서 스팀으로 사게",
          "speaker_id": "1",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027954.1.11",
          "form": "그건... 고민 중이야",
          "original_form": "그건... 고민 중이야",
          "speaker_id": "2",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027954.1.12",
          "form": "스위치로 사야 자주 할 것 같긴 한데",
          "original_form": "스위치로 사야 자주 할 것 같긴 한데",
          "speaker_id": "2",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027954.1.13",
          "form": "스팀 결제가 더 쉬워서(;)",
          "original_form": "스팀 결제가 더 쉬워서(;)",
          "speaker_id": "2",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027954.1.14",
          "form": "스위치 결제도... 똑같은데",
          "original_form": "스위치 결제도... 똑같은데",
          "speaker_id": "1",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027954.1.15",
          "form": "그건... 결제하는 거 다른 페이지에서 사야 하잖어",
          "original_form": "그건... 결제하는 거 다른 페이지에서 사야 하잖어",
          "speaker_id": "2",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100027954.1.16",
          "form": "걍 카드 등록하면 결제 돼",
          "original_form": "걍 카드 등록하면 결제 돼",
          "speaker_id": "1",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100027954.1.17",
          "form": "...?",
          "original_form": "...?",
          "speaker_id": "2",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100027954.1.18",
          "form": "나는 그동안 어떤 방법으로",
          "original_form": "나는 그동안 어떤 방법으로",
          "speaker_id": "2",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100027954.1.19",
          "form": "어디 국가 쓰길래",
          "original_form": "어디 국가 쓰길래",
          "speaker_id": "1",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100027954.1.20",
          "form": "한국도 요즘은 걍 카드 결제 되지 않나",
          "original_form": "한국도 요즘은 걍 카드 결제 되지 않나",
          "speaker_id": "1",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100027954.1.21",
          "form": "한국... 일텐데",
          "original_form": "한국... 일텐데",
          "speaker_id": "2",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100027954.1.22",
          "form": "...찾아보겠음",
          "original_form": "...찾아보겠음",
          "speaker_id": "2",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100027954.1.23",
          "form": "한국 요즘도 선불카드 써?",
          "original_form": "한국 요즘도 선불카드 써?",
          "speaker_id": "1",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100027954.1.24",
          "form": "한국 마켓에서 게임 안 산지 오래됨",
          "original_form": "한국 마켓에서 게임 안 산지 오래됨",
          "speaker_id": "1",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100027954.1.25",
          "form": "내가 마지막으로 결제했을 때는 그랬을 걸",
          "original_form": "내가 마지막으로 결제했을 때는 그랬을 걸",
          "speaker_id": "2",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100027954.1.26",
          "form": "근데 한국 마켓에서 게임 살 일이 있나",
          "original_form": "근데 한국 마켓에서 게임 살 일이 있나",
          "speaker_id": "1",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100027954.1.27",
          "form": "나는 게임을 자주 사진 않으니까",
          "original_form": "나는 게임을 자주 사진 않으니까",
          "speaker_id": "2",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100027954.1.28",
          "form": "한국 말고는 거의 다 그냥 결제임",
          "original_form": "한국 말고는 거의 다 그냥 결제임",
          "speaker_id": "1",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100027954.1.29",
          "form": "충격적인데",
          "original_form": "충격적인데",
          "speaker_id": "2",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100027954.1.30",
          "form": "알아봐야겠군...",
          "original_form": "알아봐야겠군...",
          "speaker_id": "2",
          "time": "20210902 16:27"
        }
      ]
    }
  ]
