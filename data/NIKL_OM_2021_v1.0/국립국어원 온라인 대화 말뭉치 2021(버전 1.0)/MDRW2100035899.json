      "utterance": [
        {
          "id": "MDRW2100035899.1.1",
          "form": "그 지코인가 그거 맞져?ㅋㅋㅋㅌ",
          "original_form": "그 지코인가 그거 맞져?ㅋㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210830 11:02"
        },
        {
          "id": "MDRW2100035899.1.2",
          "form": "넼ㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:03"
        },
        {
          "id": "MDRW2100035899.1.3",
          "form": "자도 코코넛 진짜 안좋아하는데",
          "original_form": "자도 코코넛 진짜 안좋아하는데",
          "speaker_id": "1",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035899.1.4",
          "form": "그거먹고 코코넛한테 완전 실망했어욬ㅋㅋㅋ",
          "original_form": "그거먹고 코코넛한테 완전 실망했어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035899.1.5",
          "form": "최악의맛...지코.",
          "original_form": "최악의맛...지코.",
          "speaker_id": "2",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035899.1.6",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035899.1.7",
          "form": "달달할것같은데",
          "original_form": "달달할것같은데",
          "speaker_id": "1",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035899.1.8",
          "form": "행주 빤 맛...",
          "original_form": "행주 빤 맛...",
          "speaker_id": "1",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035899.1.9",
          "form": "그거 먹고 코코넛 안먹는사람 좀 있더라고요 아놔 맞아요",
          "original_form": "그거 먹고 코코넛 안먹는사람 좀 있더라고요 아놔 맞아요",
          "speaker_id": "2",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035899.1.10",
          "form": "행주 물에 담군맛..",
          "original_form": "행주 물에 담군맛..",
          "speaker_id": "2",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035899.1.11",
          "form": "앜ㅋㅋㅋ 맞아요 매니아만 먹는...",
          "original_form": "앜ㅋㅋㅋ 맞아요 매니아만 먹는...",
          "speaker_id": "1",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035899.1.12",
          "form": "커피도 안좋아하고 그런데",
          "original_form": "커피도 안좋아하고 그런데",
          "speaker_id": "1",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035899.1.13",
          "form": "연남동에 콩카페라고",
          "original_form": "연남동에 콩카페라고",
          "speaker_id": "1",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035899.1.14",
          "form": "베트남식 커피 파는곳이 있어용",
          "original_form": "베트남식 커피 파는곳이 있어용",
          "speaker_id": "1",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035899.1.15",
          "form": "여기 커피는 진짜 코코넛만은 거의 안나고 커피가 진짜 맛있어요! 부드럽고ㅠㅠㅠ",
          "original_form": "여기 커피는 진짜 코코넛만은 거의 안나고 커피가 진짜 맛있어요! 부드럽고ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 11:05"
        },
        {
          "id": "MDRW2100035899.1.16",
          "form": "아 헐 콩카페 들어봤어요!!!!",
          "original_form": "아 헐 콩카페 들어봤어요!!!!",
          "speaker_id": "2",
          "time": "20210830 11:05"
        },
        {
          "id": "MDRW2100035899.1.17",
          "form": "거기 진짜 맛있다고 그 사람들이 얘기하더라고요?!?!?!",
          "original_form": "거기 진짜 맛있다고 그 사람들이 얘기하더라고요?!?!?!",
          "speaker_id": "2",
          "time": "20210830 11:06"
        },
        {
          "id": "MDRW2100035899.1.18",
          "form": "네네! 여기 정말정말 맛있어요ㅠㅠ",
          "original_form": "네네! 여기 정말정말 맛있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 11:06"
        },
        {
          "id": "MDRW2100035899.1.19",
          "form": "저처럼 코코넛이랑 커피 안좋아하시는분들한테도",
          "original_form": "저처럼 코코넛이랑 커피 안좋아하시는분들한테도",
          "speaker_id": "1",
          "time": "20210830 11:06"
        },
        {
          "id": "MDRW2100035899.1.20",
          "form": "추천하고싶을정도로@",
          "original_form": "추천하고싶을정도로@",
          "speaker_id": "1",
          "time": "20210830 11:06"
        },
        {
          "id": "MDRW2100035899.1.21",
          "form": "하...진짜 저 꼭 가볼게요 넘 맛있겠다...",
          "original_form": "하...진짜 저 꼭 가볼게요 넘 맛있겠다...",
          "speaker_id": "2",
          "time": "20210830 11:07"
        },
        {
          "id": "MDRW2100035899.1.22",
          "form": "name1님 커피 안 좋아하세요?!",
          "original_form": "&name1&님 커피 안 좋아하세요?!",
          "speaker_id": "2",
          "time": "20210830 11:07"
        },
        {
          "id": "MDRW2100035899.1.23",
          "form": "넹넹 안좋아하다기보다는",
          "original_form": "넹넹 안좋아하다기보다는",
          "speaker_id": "1",
          "time": "20210830 11:07"
        },
        {
          "id": "MDRW2100035899.1.24",
          "form": "카페인때문에ㅠㅠㅠ",
          "original_form": "카페인때문에ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 11:07"
        },
        {
          "id": "MDRW2100035899.1.25",
          "form": "전 커피보단 다른 음료 먹는 편이거든요! ㅎ.ㅎ",
          "original_form": "전 커피보단 다른 음료 먹는 편이거든요! ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210830 11:07"
        },
        {
          "id": "MDRW2100035899.1.26",
          "form": "굳이...아!",
          "original_form": "굳이...아!",
          "speaker_id": "2",
          "time": "20210830 11:07"
        },
        {
          "id": "MDRW2100035899.1.27",
          "form": "심장이 콩닥콩닥",
          "original_form": "심장이 콩닥콩닥",
