      "utterance": [
        {
          "id": "MDRW2100004650.1.1",
          "form": "경주야경 이뻐ㅠㅠㅠ",
          "original_form": "경주야경 이뻐ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:43"
        },
        {
          "id": "MDRW2100004650.1.2",
          "form": "맞아 예뻐. 안압지는 꼭 코스에 넣자",
          "original_form": "맞아 예뻐. 안압지는 꼭 코스에 넣자",
          "speaker_id": "2",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004650.1.3",
          "form": "우리 학창시절 때 자주 갔던 곳보다는 안가본 곳으로 계획을 짜도 재밌을 거 같아",
          "original_form": "우리 학창시절 때 자주 갔던 곳보다는 안가본 곳으로 계획을 짜도 재밌을 거 같아",
          "speaker_id": "2",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004650.1.4",
          "form": "좋아좋아",
          "original_form": "좋아좋아",
          "speaker_id": "1",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004650.1.5",
          "form": "근데 안압지가뭐야?",
          "original_form": "근데 안압지가뭐야?",
          "speaker_id": "1",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004650.1.6",
          "form": "지금은 동궁과 월지라는 이름으로 바뀐 곳인데",
          "original_form": "지금은 동궁과 월지라는 이름으로 바뀐 곳인데",
          "speaker_id": "2",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004650.1.7",
          "form": "궁궐? 뭐라고 해야할까",
          "original_form": "궁궐? 뭐라고 해야할까",
          "speaker_id": "2",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004650.1.8",
          "form": "궁궐터야",
          "original_form": "궁궐터야",
          "speaker_id": "2",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004650.1.9",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004650.1.10",
          "form": "방금 검색해봄ㅎㅎ",
          "original_form": "방금 검색해봄ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004650.1.11",
          "form": "이뻐ㅠㅠㅜㅠ",
          "original_form": "이뻐ㅠㅠㅜㅠ",
          "speaker_id": "1",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004650.1.12",
          "form": "그치, 야경 꼭 볼거야",
          "original_form": "그치, 야경 꼭 볼거야",
          "speaker_id": "2",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004650.1.13",
          "form": "몇 년 전에는 못봐서 아쉬웠어...",
          "original_form": "몇 년 전에는 못봐서 아쉬웠어...",
          "speaker_id": "2",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004650.1.14",
          "form": "난 요즘 낮보다 밤이 더 좋음",
          "original_form": "난 요즘 낮보다 밤이 더 좋음",
          "speaker_id": "1",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004650.1.15",
          "form": "야경 때문에",
          "original_form": "야경 때문에",
          "speaker_id": "1",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004650.1.16",
          "form": "그래서 여행도 일찍 낮에 돌아다니는것보다",
          "original_form": "그래서 여행도 일찍 낮에 돌아다니는것보다",
          "speaker_id": "1",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004650.1.17",
          "form": "밤에 돌아다니는걸 선호해",
          "original_form": "밤에 돌아다니는걸 선호해",
          "speaker_id": "1",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004650.1.18",
          "form": "오, 난 밤에 잘 안돌아다녀봤어\r\n밤 여행도 재밌겠다!",
          "original_form": "오, 난 밤에 잘 안돌아다녀봤어\r\n밤 여행도 재밌겠다!",
          "speaker_id": "2",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100004650.1.19",
          "form": "한번도 안해봤어\r\n너무 해보고싶어",
          "original_form": "한번도 안해봤어\r\n너무 해보고싶어",
          "speaker_id": "2",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100004650.1.20",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "1",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004650.1.21",
          "form": "하긴 다들 집을 좋아해서 일찍 들어가지ㅠ",
          "original_form": "하긴 다들 집을 좋아해서 일찍 들어가지ㅠ",
          "speaker_id": "1",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004650.1.22",
          "form": "언제 한 번 1박 해서 돌아다녀보자구",
          "original_form": "언제 한 번 1박 해서 돌아다녀보자구",
          "speaker_id": "1",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004650.1.23",
          "form": "야경좋음",
          "original_form": "야경좋음",
          "speaker_id": "1",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004650.1.24",
          "form": "특히 여름밤이 좋아",
          "original_form": "특히 여름밤이 좋아",
          "speaker_id": "1",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004650.1.25",
          "form": "여름밤에 매미소리를 들으면서 야경구경하는 상상중임",
          "original_form": "여름밤에 매미소리를 들으면서 야경구경하는 상상중임",
          "speaker_id": "2",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004650.1.26",
          "form": "하, 낭만적이야",
          "original_form": "하, 낭만적이야",
          "speaker_id": "2",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004650.1.27",
          "form": "감동받을거야",
          "original_form": "감동받을거야",
          "speaker_id": "2",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004650.1.28",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004650.1.29",
          "form": "여름밤인데 습하고 더운 열대야 그런게 아니라",
          "original_form": "여름밤인데 습하고 더운 열대야 그런게 아니라",
          "speaker_id": "1",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004650.1.30",
          "form": "선선한 바람 부는 여름 밤이어야함",
          "original_form": "선선한 바람 부는 여름 밤이어야함",
          "speaker_id": "1",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004650.1.31",
          "form": "<<킬포",
          "original_form": "<<킬포",
          "speaker_id": "1",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004650.1.32",
          "form": "아 맞아ㅋㅋ",
          "original_form": "아 맞아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004650.1.33",
          "form": "그럼 여름이 끝날 무렵쯤에 가야겠다",
          "original_form": "그럼 여름이 끝날 무렵쯤에 가야겠다",
          "speaker_id": "2",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004650.1.34",
          "form": "매미 소리도 듣고 선선한 바람도 쐬고",
          "original_form": "매미 소리도 듣고 선선한 바람도 쐬고",
          "speaker_id": "2",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004650.1.35",
          "form": "딱 요즘같은 날씨",
          "original_form": "딱 요즘같은 날씨",
          "speaker_id": "2",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004650.1.36",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210821 18:04"
        }
      ]
    }
