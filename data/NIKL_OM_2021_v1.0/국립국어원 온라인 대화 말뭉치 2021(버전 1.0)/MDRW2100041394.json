      "utterance": [
        {
          "id": "MDRW2100041394.1.1",
          "form": "쓴맛나는건 별로 안좋아해요 ㅋㅋㅋㅋ",
          "original_form": "쓴맛나는건 별로 안좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100041394.1.2",
          "form": "저도요 ..",
          "original_form": "저도요 ..",
          "speaker_id": "1",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100041394.1.3",
          "form": "짜고치는것처럼ㅋㅋㅋㅋ 똑같네여",
          "original_form": "짜고치는것처럼ㅋㅋㅋㅋ 똑같네여",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100041394.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ회만 최애였으면 입맛 소울메이트인데 아쉽네요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ회만 최애였으면 입맛 소울메이트인데 아쉽네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100041394.1.5",
          "form": "ㅋㅋㅋㅋ글게요 그치만 회도 즐겨먹습니당 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ글게요 그치만 회도 즐겨먹습니당 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100041394.1.6",
          "form": "곱창 막창도 잘드세요?",
          "original_form": "곱창 막창도 잘드세요?",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100041394.1.7",
          "form": "막창이나 대창이 더 좋아요",
          "original_form": "막창이나 대창이 더 좋아요",
          "speaker_id": "2",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100041394.1.8",
          "form": "곱창은 ㅂㄹ",
          "original_form": "곱창은 ㅂㄹ",
          "speaker_id": "2",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100041394.1.9",
          "form": "헉 .. 다른게 등장했네요 ㅋㅋ 전 셋다 좋아해용",
          "original_form": "헉 .. 다른게 등장했네요 ㅋㅋ 전 셋다 좋아해용",
          "speaker_id": "1",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041394.1.10",
          "form": "대창덮밥? 아직 못먹어봣는데",
          "original_form": "대창덮밥? 아직 못먹어봣는데",
          "speaker_id": "1",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041394.1.11",
          "form": "먹어보고파용",
          "original_form": "먹어보고파용",
          "speaker_id": "1",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041394.1.12",
          "form": "아 저도 덮밥은 안먹어봤어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 저도 덮밥은 안먹어봤어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100041394.1.13",
          "form": "유명하긴한거같던데 ㅋㅋㅋ대창덮밥",
          "original_form": "유명하긴한거같던데 ㅋㅋㅋ대창덮밥",
          "speaker_id": "2",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100041394.1.14",
          "form": "그 일본식 이름뭐죠",
          "original_form": "그 일본식 이름뭐죠",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100041394.1.15",
          "form": "무슨 나베?",
          "original_form": "무슨 나베?",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100041394.1.16",
          "form": "그것도 아직 못먹어봤어용 ㅠㅠ",
          "original_form": "그것도 아직 못먹어봤어용 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100041394.1.17",
          "form": "아하 나베",
          "original_form": "아하 나베",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100041394.1.18",
          "form": "나베는 좀 물말아먹는기분이라",
          "original_form": "나베는 좀 물말아먹는기분이라",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100041394.1.19",
          "form": "대창들어간 나베도 있더라구용 !!",
          "original_form": "대창들어간 나베도 있더라구용 !!",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100041394.1.20",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100041394.1.21",
          "form": "별로였네욬ㅋㅋ 아니 일본음식이 썩... 호는 아니더라구요",
          "original_form": "별로였네욬ㅋㅋ 아니 일본음식이 썩... 호는 아니더라구요",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100041394.1.22",
          "form": "아하?!",
          "original_form": "아하?!",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100041394.1.23",
          "form": "저도 일식중에서도",
          "original_form": "저도 일식중에서도",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041394.1.24",
          "form": "맞는부분 안맞는부분이 있는거같아용",
          "original_form": "맞는부분 안맞는부분이 있는거같아용",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041394.1.25",
          "form": "그쵸그쵸 보통 그렇죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쵸그쵸 보통 그렇죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041394.1.26",
          "form": "보통은 그런거같아요!",
          "original_form": "보통은 그런거같아요!",
          "speaker_id": "2",
          "time": "20210905 23:07"
        }
      ]
    }
