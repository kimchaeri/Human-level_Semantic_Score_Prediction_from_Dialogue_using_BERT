      "utterance": [
        {
          "id": "MDRW2100019130.1.1",
          "form": "누구좋아하세요?",
          "original_form": "누구좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019130.1.2",
          "form": "최근에 즐겨보신 드라마가있으신가요",
          "original_form": "최근에 즐겨보신 드라마가있으신가요",
          "speaker_id": "1",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019130.1.3",
          "form": "아.. 저 드라마도 잘 안봐서요..",
          "original_form": "아.. 저 드라마도 잘 안봐서요..",
          "speaker_id": "2",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019130.1.4",
          "form": "유튜브만봐요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "유튜브만봐요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019130.1.5",
          "form": "영화보거나",
          "original_form": "영화보거나",
          "speaker_id": "2",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019130.1.6",
          "form": "티비를 안봅니다..",
          "original_form": "티비를 안봅니다..",
          "speaker_id": "2",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019130.1.7",
          "form": "영화 어떤거 보시나요",
          "original_form": "영화 어떤거 보시나요",
          "speaker_id": "1",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019130.1.8",
          "form": "보셨나요",
          "original_form": "보셨나요",
          "speaker_id": "1",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019130.1.9",
          "form": "최근엔 평행이론 살인사건인가",
          "original_form": "최근엔 평행이론 살인사건인가",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019130.1.10",
          "form": "공포영화좋아해요",
          "original_form": "공포영화좋아해요",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019130.1.11",
          "form": "제목이 안정확하니깐 검색은하지마세요",
          "original_form": "제목이 안정확하니깐 검색은하지마세요",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019130.1.12",
          "form": "무서운거 좋아하시는구나",
          "original_form": "무서운거 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019130.1.13",
          "form": "그럼 좀비 이런것도 잘보세요",
          "original_form": "그럼 좀비 이런것도 잘보세요",
          "speaker_id": "1",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019130.1.14",
          "form": "혼자서못보는데 좋아해요",
          "original_form": "혼자서못보는데 좋아해요",
          "speaker_id": "2",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100019130.1.15",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100019130.1.16",
          "form": "잘보는데 혼자는 못봐요",
          "original_form": "잘보는데 혼자는 못봐요",
          "speaker_id": "2",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100019130.1.17",
          "form": "전 혼자서도 둘이서도 못봅니다",
          "original_form": "전 혼자서도 둘이서도 못봅니다",
          "speaker_id": "1",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100019130.1.18",
          "form": "무서워서..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "무서워서..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019130.1.19",
          "form": "어떤거좋아하세요?",
          "original_form": "어떤거좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019130.1.20",
          "form": "로맨스좋아하실듯",
          "original_form": "로맨스좋아하실듯",
          "speaker_id": "2",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019130.1.21",
          "form": "가장 무서웠던 영화는  뭔가요",
          "original_form": "가장 무서웠던 영화는  뭔가요",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019130.1.22",
          "form": "코메디 로맨스도 좋아해요",
          "original_form": "코메디 로맨스도 좋아해요",
          "speaker_id": "1",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019130.1.23",
          "form": "24아이덴티티가 젤 잼게 무러웠어요",
          "original_form": "24아이덴티티가 젤 잼게 무러웠어요",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019130.1.24",
          "form": "슬픈것도",
          "original_form": "슬픈것도",
          "speaker_id": "2",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019130.1.25",
          "form": "보면 잘 우실것같네요",
          "original_form": "보면 잘 우실것같네요",
