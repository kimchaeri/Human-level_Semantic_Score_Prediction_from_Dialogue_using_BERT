      "utterance": [
        {
          "id": "MDRW2100042416.1.1",
          "form": "어제 춥더라니 또?",
          "original_form": "어제 춥더라니 또?",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042416.1.2",
          "form": "ㅎㅎ 에어컨 틀었어요 그래서",
          "original_form": "ㅎㅎ 에어컨 틀었어요 그래서",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042416.1.3",
          "form": "전기세 많이 나오겠다",
          "original_form": "전기세 많이 나오겠다",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042416.1.4",
          "form": "근데 찝찝하고 건조기 못돌리면 빨래도 안말라서",
          "original_form": "근데 찝찝하고 건조기 못돌리면 빨래도 안말라서",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042416.1.5",
          "form": "나중에 돈 많이 벌어서 건조기 사야겠네",
          "original_form": "나중에 돈 많이 벌어서 건조기 사야겠네",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042416.1.6",
          "form": "지금은 안오는거같은데 창문 열어볼까봐요",
          "original_form": "지금은 안오는거같은데 창문 열어볼까봐요",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042416.1.7",
          "form": "열신데 지금 열면 춥지않을까",
          "original_form": "열신데 지금 열면 춥지않을까",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042416.1.8",
          "form": "자기 전엔 닫을거니까 괜찮을걸요",
          "original_form": "자기 전엔 닫을거니까 괜찮을걸요",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042416.1.9",
          "form": "선풍기만 틀어도 추운데 넌 참 열도 많다",
          "original_form": "선풍기만 틀어도 추운데 넌 참 열도 많다",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042416.1.10",
          "form": "여름이라 어쩔수 없어요 9월이면 가을이지만",
          "original_form": "여름이라 어쩔수 없어요 9월이면 가을이지만",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042416.1.11",
          "form": "입추 다 지났는데 무슨 여름이야~",
          "original_form": "입추 다 지났는데 무슨 여름이야~",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042416.1.12",
          "form": "물결도 쓰시고.. 하여튼 더우면 여름이라구요 지금 장마인듯",
          "original_form": "물결도 쓰시고.. 하여튼 더우면 여름이라구요 지금 장마인듯",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042416.1.13",
          "form": "근데 장마 안끝난거면 여름 맞지 않아요?",
          "original_form": "근데 장마 안끝난거면 여름 맞지 않아요?",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042416.1.14",
          "form": "집에 복숭아 사놨다",
          "original_form": "집에 복숭아 사놨다",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042416.1.15",
          "form": "저는 못먹는거 자랑하시구",
          "original_form": "저는 못먹는거 자랑하시구",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042416.1.16",
          "form": "와서 먹어",
          "original_form": "와서 먹어",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042416.1.17",
          "form": "그러고 싶다.. 복숭아 너무 맛있어요 요즘에 너무 못먹었어",
          "original_form": "그러고 싶다.. 복숭아 너무 맛있어요 요즘에 너무 못먹었어",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042416.1.18",
          "form": "수박도 썰어줄게",
          "original_form": "수박도 썰어줄게",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042416.1.19",
          "form": "복숭아랑 수박 잔뜩 먹고싶다˜",
          "original_form": "복숭아랑 수박 잔뜩 먹고싶다˜",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042416.1.20",
          "form": "여름 지나가면 못먹을텐데",
          "original_form": "여름 지나가면 못먹을텐데",
          "speaker_id": "2",
          "time": "20210906 22:05"
        }
