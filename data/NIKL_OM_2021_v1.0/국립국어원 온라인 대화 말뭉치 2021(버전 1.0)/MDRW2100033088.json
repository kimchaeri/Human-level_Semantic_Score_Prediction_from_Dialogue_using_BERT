        {
          "id": "MDRW2100033088.1.1",
          "form": "미용의 기본은 다이어트",
          "original_form": "미용의 기본은 다이어트",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033088.1.2",
          "form": "다이어트 하시나요",
          "original_form": "다이어트 하시나요",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100033088.1.3",
          "form": "항상 하죠",
          "original_form": "항상 하죠",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100033088.1.4",
          "form": "네 저도 요즘 하고 있어요",
          "original_form": "네 저도 요즘 하고 있어요",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100033088.1.5",
          "form": "오호 성과가잇나요",
          "original_form": "오호 성과가잇나요",
          "speaker_id": "1",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033088.1.6",
          "form": "아니여 이제 가열차게 하려구요",
          "original_form": "아니여 이제 가열차게 하려구요",
          "speaker_id": "2",
          "time": "20210906 20:48"
        },
        {
          "id": "MDRW2100033088.1.7",
          "form": "열심히 하세요",
          "original_form": "열심히 하세요",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100033088.1.8",
          "form": "몸 다이어트요\n뇌 다이어트는ㄴㄴ",
          "original_form": "몸 다이어트요\n뇌 다이어트는ㄴㄴ",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100033088.1.9",
          "form": "뇌다이어트 신박하네요",
          "original_form": "뇌다이어트 신박하네요",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100033088.1.10",
          "form": "넵 ㅋ",
          "original_form": "넵 ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100033088.1.11",
          "form": "윗몸일으키기 하려구요",
          "original_form": "윗몸일으키기 하려구요",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100033088.1.12",
          "form": "윗몸일으키기 좋죠",
          "original_form": "윗몸일으키기 좋죠",
          "speaker_id": "1",
          "time": "20210907 07:43"
        },
        {
          "id": "MDRW2100033088.1.13",
          "form": "스텝퍼도 있어서 그거두요",
          "original_form": "스텝퍼도 있어서 그거두요",
          "speaker_id": "2",
          "time": "20210907 07:46"
        },
        {
          "id": "MDRW2100033088.1.14",
          "form": "피곤하네요 푹 자야 건강한데",
          "original_form": "피곤하네요 푹 자야 건강한데",
          "speaker_id": "1",
          "time": "20210907 08:02"
        },
        {
          "id": "MDRW2100033088.1.15",
          "form": "네 잠이 보약이죠",
          "original_form": "네 잠이 보약이죠",
          "speaker_id": "2",
          "time": "20210907 08:08"
        },
        {
          "id": "MDRW2100033088.1.16",
          "form": "네 보약 많이 드셔야져",
          "original_form": "네 보약 많이 드셔야져",
          "speaker_id": "1",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100033088.1.17",
          "form": "넹 홍삼 먹고 있어요",
          "original_form": "넹 홍삼 먹고 있어요",
