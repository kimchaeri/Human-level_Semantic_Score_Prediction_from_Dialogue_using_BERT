      "utterance": [
        {
          "id": "MDRW2100027496.1.1",
          "form": "최근 잘 산템 있나요",
          "original_form": "최근 잘 산템 있나요",
          "speaker_id": "1",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100027496.1.2",
          "form": "저는 비타민C?",
          "original_form": "저는 비타민C?",
          "speaker_id": "2",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100027496.1.3",
          "form": "먹으면 피로 덜해서 좋더라구요",
          "original_form": "먹으면 피로 덜해서 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100027496.1.4",
          "form": "오 비타민씨 전 고려은단꺼 먹어요",
          "original_form": "오 비타민씨 전 고려은단꺼 먹어요",
          "speaker_id": "1",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100027496.1.5",
          "form": "어 저도 ㅋㅋㅋ 고려은단이 제일 믿음직하죠",
          "original_form": "어 저도 ㅋㅋㅋ 고려은단이 제일 믿음직하죠",
          "speaker_id": "2",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100027496.1.6",
          "form": "종합비타민도 먹어봤는데 비타민씨가 더 효과가 즉빵인거 같았어요",
          "original_form": "종합비타민도 먹어봤는데 비타민씨가 더 효과가 즉빵인거 같았어요",
          "speaker_id": "2",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100027496.1.7",
          "form": "맞아요 아하 저도 한알 먹어여겠다",
          "original_form": "맞아요 아하 저도 한알 먹어여겠다",
          "speaker_id": "1",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027496.1.8",
          "form": "지금 먹고 오세요 ㅎㅎ",
          "original_form": "지금 먹고 오세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027496.1.9",
          "form": "뭐 사신거 있으신가요?",
          "original_form": "뭐 사신거 있으신가요?",
          "speaker_id": "2",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027496.1.10",
          "form": "저는 구두 기다리고있슴다 로퍼",
          "original_form": "저는 구두 기다리고있슴다 로퍼",
          "speaker_id": "1",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027496.1.11",
          "form": "오~ 무슨 색이에요?",
          "original_form": "오~ 무슨 색이에요?",
          "speaker_id": "2",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027496.1.12",
          "form": "브라운 구매했어요",
          "original_form": "브라운 구매했어요",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027496.1.13",
          "form": "아이보리가 이쁜데 때탈까봐ㅠㅠ",
          "original_form": "아이보리가 이쁜데 때탈까봐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027496.1.14",
          "form": "크... 아쉽지만 그래도 좋은 선택이네요",
          "original_form": "크... 아쉽지만 그래도 좋은 선택이네요",
          "speaker_id": "2",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027496.1.15",
          "form": "브라운이 무난하고 좋죠!! 잘어울리실거예요",
          "original_form": "브라운이 무난하고 좋죠!! 잘어울리실거예요",
          "speaker_id": "2",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027496.1.16",
          "form": "또 다른거 사료고 준비중",
          "original_form": "또 다른거 사료고 준비중",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027496.1.17",
          "form": "ㅋㅋㅋㅋ 어떤거요",
          "original_form": "ㅋㅋㅋㅋ 어떤거요",
          "speaker_id": "2",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027496.1.18",
          "form": "지네도 아니고 신발욕심이",
          "original_form": "지네도 아니고 신발욕심이",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027496.1.19",
          "form": "메리제인구두요 ㅋㅋ",
          "original_form": "메리제인구두요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027496.1.20",
          "form": "아 ㅋㅋㅋㅋㅋ 이쁘잖아요 어쩔수없죠 ㅠㅠ",
          "original_form": "아 ㅋㅋㅋㅋㅋ 이쁘잖아요 어쩔수없죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100027496.1.21",
          "form": "저도 신발 사야하는데 밖에 잘 안나가니까",
          "original_form": "저도 신발 사야하는데 밖에 잘 안나가니까",
          "speaker_id": "2",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027496.1.22",
          "form": "운동화로 땡치게 되네요 ㅎㅎ",
          "original_form": "운동화로 땡치게 되네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027496.1.23",
          "form": "그쵸ㅠㅠ 저도 사두고 신발장에만 ㅋㅋㅋ",
          "original_form": "그쵸ㅠㅠ 저도 사두고 신발장에만 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027496.1.24",
          "form": "저도 맨날 슬리퍼에요 ㅋㅋ",
          "original_form": "저도 맨날 슬리퍼에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027496.1.25",
          "form": "ㅋㅋㅋㅋ 그치만 그런건 그냥 사고 모으는거에 의의를 두게 돼요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ 그치만 그런건 그냥 사고 모으는거에 의의를 두게 돼요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100027496.1.26",
          "form": "역시 신발장은 장식장이죠 뿌듯",
          "original_form": "역시 신발장은 장식장이죠 뿌듯",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100027496.1.27",
          "form": "저도 안신은 여름구두 있는데 보는것만으로도 좋은 ㅋㅋㅋ",
          "original_form": "저도 안신은 여름구두 있는데 보는것만으로도 좋은 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:16"
        },
        {
          "id": "MDRW2100027496.1.28",
          "form": "저는 이제 속이타고 갖다버려야할거같아요 ㅋㅋㅋ",
          "original_form": "저는 이제 속이타고 갖다버려야할거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:22"
        }
      ]
