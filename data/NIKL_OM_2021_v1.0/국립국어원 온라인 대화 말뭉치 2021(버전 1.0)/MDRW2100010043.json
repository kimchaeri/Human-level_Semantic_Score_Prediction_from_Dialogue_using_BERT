      "utterance": [
        {
          "id": "MDRW2100010043.1.1",
          "form": "태풍오늘와?",
          "original_form": "태풍오늘와?",
          "speaker_id": "1",
          "time": "20210823 13:30"
        },
        {
          "id": "MDRW2100010043.1.2",
          "form": "중부는 오늘 밤부터",
          "original_form": "중부는 오늘 밤부터",
          "speaker_id": "2",
          "time": "20210823 13:30"
        },
        {
          "id": "MDRW2100010043.1.3",
          "form": "영향권",
          "original_form": "영향권",
          "speaker_id": "2",
          "time": "20210823 13:30"
        },
        {
          "id": "MDRW2100010043.1.4",
          "form": "아그림 남부는 이미 영향권이야?",
          "original_form": "아그림 남부는 이미 영향권이야?",
          "speaker_id": "1",
          "time": "20210823 13:30"
        },
        {
          "id": "MDRW2100010043.1.5",
          "form": "ㅁ",
          "original_form": "ㅁ",
          "speaker_id": "2",
          "time": "20210823 13:30"
        },
        {
          "id": "MDRW2100010043.1.6",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210823 13:31"
        },
        {
          "id": "MDRW2100010043.1.7",
          "form": "그래도 시원해서 좋네요",
          "original_form": "그래도 시원해서 좋네요",
          "speaker_id": "1",
          "time": "20210823 13:31"
        },
        {
          "id": "MDRW2100010043.1.8",
          "form": "ㅋㅋ맞아",
          "original_form": "ㅋㅋ맞아",
          "speaker_id": "2",
          "time": "20210823 13:31"
        },
        {
          "id": "MDRW2100010043.1.9",
          "form": "언제쯤 해가날까요...",
          "original_form": "언제쯤 해가날까요...",
          "speaker_id": "1",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100010043.1.10",
          "form": "전 흐린거 싫은뎅",
          "original_form": "전 흐린거 싫은뎅",
          "speaker_id": "1",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100010043.1.11",
          "form": "금방 날거야",
          "original_form": "금방 날거야",
          "speaker_id": "2",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100010043.1.12",
          "form": "화욜이 고비니까",
          "original_form": "화욜이 고비니까",
          "speaker_id": "2",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100010043.1.13",
          "form": "태풍가면 해쨍쨍하지 않을까",
          "original_form": "태풍가면 해쨍쨍하지 않을까",
          "speaker_id": "2",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100010043.1.14",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "1",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100010043.1.15",
          "form": "근데 8월인데 시원해서 신기...",
          "original_form": "근데 8월인데 시원해서 신기...",
          "speaker_id": "1",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100010043.1.16",
          "form": "이제 9월이니까",
          "original_form": "이제 9월이니까",
          "speaker_id": "1",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100010043.1.17",
          "form": "여름이 다 갔나봐",
          "original_form": "여름이 다 갔나봐",
          "speaker_id": "1",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100010043.1.18",
          "form": "응 하나도 안더워",
          "original_form": "응 하나도 안더워",
          "speaker_id": "2",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100010043.1.19",
          "form": "에어컨청소해야지 이제 쓸 일 없겠어",
          "original_form": "에어컨청소해야지 이제 쓸 일 없겠어",
          "speaker_id": "2",
          "time": "20210823 13:34"
        },
        {
          "id": "MDRW2100010043.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210823 13:34"
        },
        {
          "id": "MDRW2100010043.1.21",
          "form": "심지어 밤에는 창문열고자니까",
          "original_form": "심지어 밤에는 창문열고자니까",
          "speaker_id": "1",
          "time": "20210823 13:34"
        },
        {
          "id": "MDRW2100010043.1.22",
          "form": "너무추워,,,",
          "original_form": "너무추워,,,",
          "speaker_id": "1",
          "time": "20210823 13:35"
        },
        {
          "id": "MDRW2100010043.1.23",
          "form": "응 가을에는 일교차심하니까 감기조심해야돼",
          "original_form": "응 가을에는 일교차심하니까 감기조심해야돼",
          "speaker_id": "2",
          "time": "20210823 13:35"
        },
        {
          "id": "MDRW2100010043.1.24",
          "form": "ㅋㅋ벌써 가을",
          "original_form": "ㅋㅋ벌써 가을",
          "speaker_id": "1",
          "time": "20210823 13:35"
        }
      ]
