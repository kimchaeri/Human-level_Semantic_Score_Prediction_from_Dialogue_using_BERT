        },
        {
          "id": "MDRW2100024596.1.3",
          "form": "그럴거 같은데",
          "original_form": "그럴거 같은데",
          "speaker_id": "2",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100024596.1.4",
          "form": "내일도 오나 혹시",
          "original_form": "내일도 오나 혹시",
          "speaker_id": "1",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100024596.1.5",
          "form": "내일도 올거 같은데?",
          "original_form": "내일도 올거 같은데?",
          "speaker_id": "2",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100024596.1.6",
          "form": "찾아보니까 내일 오후나 돼야 그치 려나 봐",
          "original_form": "찾아보니까 내일 오후나 돼야 그치 려나 봐",
          "speaker_id": "1",
