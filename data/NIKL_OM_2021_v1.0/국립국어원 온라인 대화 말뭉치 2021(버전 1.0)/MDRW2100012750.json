        },
        {
          "id": "MDRW2100012750.1.3",
          "form": "다녀오셨나요?ㅋㅋ",
          "original_form": "다녀오셨나요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100012750.1.4",
          "form": "아니요아직 안가봤네요",
          "original_form": "아니요아직 안가봤네요",
          "speaker_id": "1",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100012750.1.5",
          "form": "남자세요?",
          "original_form": "남자세요?",
          "speaker_id": "2",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100012750.1.6",
          "form": "여자에여",
          "original_form": "여자에여",
          "speaker_id": "1",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100012750.1.7",
          "form": "아ㅋㅋ",
          "original_form": "아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100012750.1.8",
          "form": "군대 관련으로 기사 보신적 있으신가요?",
          "original_form": "군대 관련으로 기사 보신적 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100012750.1.9",
          "form": "저도 성.폭.행 관련 기사봤어요",
          "original_form": "저도 성.폭.행 관련 기사봤어요",
          "speaker_id": "2",
          "time": "20210825 00:45"
        },
        {
          "id": "MDRW2100012750.1.10",
          "form": "오호라",
          "original_form": "오호라",
          "speaker_id": "1",
          "time": "20210825 00:45"
        },
        {
          "id": "MDRW2100012750.1.11",
          "form": "청원도 했고요",
          "original_form": "청원도 했고요",
          "speaker_id": "2",
          "time": "20210825 00:45"
        },
        {
          "id": "MDRW2100012750.1.12",
          "form": "하여간 네네",
          "original_form": "하여간 네네",
          "speaker_id": "1",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100012750.1.13",
          "form": "굉장히 폐쇄적이고 여성군인들이 힘든 돗이구나 생각이들었어요",
          "original_form": "굉장히 폐쇄적이고 여성군인들이 힘든 돗이구나 생각이들었어요",
          "speaker_id": "1",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100012750.1.14",
          "form": "여자군인들 대우도 제대로 안해주면서 ",
          "original_form": "여자군인들 대우도 제대로 안해주면서 ",
