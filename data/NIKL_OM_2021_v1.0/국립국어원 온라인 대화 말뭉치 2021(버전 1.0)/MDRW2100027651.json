      "utterance": [
        {
          "id": "MDRW2100027651.1.1",
          "form": "전 토욜에 여친 모시러 가기로 했거든요",
          "original_form": "전 토욜에 여친 모시러 가기로 했거든요",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027651.1.2",
          "form": "네 ㅋㅋㅋ평일엔 진짜 지쳐서 매일 못하겠더라구요ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ평일엔 진짜 지쳐서 매일 못하겠더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027651.1.3",
          "form": "넹 자동차 극장?",
          "original_form": "넹 자동차 극장?",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027651.1.4",
          "form": "그거 3시간전 입장해야된다는덱",
          "original_form": "그거 3시간전 입장해야된다는덱",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027651.1.5",
          "form": "가능할지...ㅜㅜ",
          "original_form": "가능할지...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027651.1.6",
          "form": "좀 빠듯하군용",
          "original_form": "좀 빠듯하군용",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027651.1.7",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027651.1.8",
          "form": "남친만나서 데이트하면",
          "original_form": "남친만나서 데이트하면",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027651.1.9",
          "form": "좋아죽겠어요?",
          "original_form": "좋아죽겠어요?",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027651.1.10",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027651.1.11",
          "form": "네 와 정말",
          "original_form": "네 와 정말",
          "speaker_id": "1",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027651.1.12",
          "form": "어제 좋아해! 하길래",
          "original_form": "어제 좋아해! 하길래",
          "speaker_id": "1",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027651.1.13",
          "form": "저도 나두 좋아해! 했다가",
          "original_form": "저도 나두 좋아해! 했다가",
          "speaker_id": "1",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027651.1.14",
          "form": "아니야 나는 사랑하는 거 같애! 라고 바꿨어요",
          "original_form": "아니야 나는 사랑하는 거 같애! 라고 바꿨어요",
          "speaker_id": "1",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027651.1.15",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027651.1.16",
          "form": "설렌다",
          "original_form": "설렌다",
          "speaker_id": "2",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027651.1.17",
          "form": "표현 잘하는게 좋더라구요 저는",
          "original_form": "표현 잘하는게 좋더라구요 저는",
          "speaker_id": "2",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027651.1.18",
          "form": "여친이 그걸 못해가지고",
          "original_form": "여친이 그걸 못해가지고",
          "speaker_id": "2",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027651.1.19",
          "form": "사랑한다고 나만 1년 혼자했다는...",
          "original_form": "사랑한다고 나만 1년 혼자했다는...",
          "speaker_id": "2",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027651.1.20",
          "form": "맨날 사랑한다 하면 나두",
          "original_form": "맨날 사랑한다 하면 나두",
          "speaker_id": "2",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027651.1.21",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027651.1.22",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "2",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027651.1.23",
          "form": "나두 하면 너무 답답하고",
          "original_form": "나두 하면 너무 답답하고",
          "speaker_id": "1",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027651.1.24",
          "form": "서운해요",
          "original_form": "서운해요",
          "speaker_id": "1",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027651.1.25",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027651.1.26",
          "form": "나 여잔가....",
          "original_form": "나 여잔가....",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027651.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027651.1.28",
          "form": "내가 서운함을 느끼는 지점이니까",
          "original_form": "내가 서운함을 느끼는 지점이니까",
          "speaker_id": "1",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027651.1.29",
          "form": "주선자가 거기 커플은 뒤바뀐거 같다고",
          "original_form": "주선자가 거기 커플은 뒤바뀐거 같다고",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027651.1.30",
          "form": "나는 잘 말해야겠다하는 거죠 ㅋㅋㅋㅋ",
          "original_form": "나는 잘 말해야겠다하는 거죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027651.1.31",
          "form": "하긴했어요",
          "original_form": "하긴했어요",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027651.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027651.1.33",
          "form": "첫시작부터 좀 그랬잖아요",
          "original_form": "첫시작부터 좀 그랬잖아요",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027651.1.34",
          "form": "아주 러블리하군여 ㅋㅋㅋㅋ",
          "original_form": "아주 러블리하군여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027651.1.35",
          "form": "넹 아니 근데 어제 얘기하다가 어쩌다",
          "original_form": "넹 아니 근데 어제 얘기하다가 어쩌다",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027651.1.36",
          "form": "전 연애에 대한 이야기가 나왔는데",
          "original_form": "전 연애에 대한 이야기가 나왔는데",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027651.1.37",
          "form": "여태 사람을 길게 만나질 않았다고",
          "original_form": "여태 사람을 길게 만나질 않았다고",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027651.1.38",
          "form": "그나마 젤 긴 연애가 1년 반 정도?",
          "original_form": "그나마 젤 긴 연애가 1년 반 정도?",
          "speaker_id": "1",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027651.1.39",
          "form": "라고 하길래",
          "original_form": "라고 하길래",
          "speaker_id": "1",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027651.1.40",
          "form": "앜ㅋㅋㅋ1년 정도는 좀 잘 지켜봐야겠다는 생각을",
          "original_form": "앜ㅋㅋㅋ1년 정도는 좀 잘 지켜봐야겠다는 생각을",
          "speaker_id": "1",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027651.1.41",
          "form": "했어요",
          "original_form": "했어요",
          "speaker_id": "1",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027651.1.42",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027651.1.43",
          "form": "근데 결혼할라면",
          "original_form": "근데 결혼할라면",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027651.1.44",
          "form": "1년은 만나봐야죠",
          "original_form": "1년은 만나봐야죠",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027651.1.45",
          "form": "적어도 1년",
          "original_form": "적어도 1년",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027651.1.46",
          "form": "네 그쵸그쵸",
          "original_form": "네 그쵸그쵸",
          "speaker_id": "1",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027651.1.47",
          "form": "1년은 봐야져ㅋㅋㅋㅋ",
          "original_form": "1년은 봐야져ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027651.1.48",
          "form": "그정도 봐줘야",
          "original_form": "그정도 봐줘야",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027651.1.49",
          "form": "이사람이구나",
          "original_form": "이사람이구나",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027651.1.50",
          "form": "싶은 생각이 드는거 같아요",
          "original_form": "싶은 생각이 드는거 같아요",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027651.1.51",
          "form": "결혼이랑 연애는 다르니까",
          "original_form": "결혼이랑 연애는 다르니까",
          "speaker_id": "2",
          "time": "20210902 14:14"
        }
      ]
    }
