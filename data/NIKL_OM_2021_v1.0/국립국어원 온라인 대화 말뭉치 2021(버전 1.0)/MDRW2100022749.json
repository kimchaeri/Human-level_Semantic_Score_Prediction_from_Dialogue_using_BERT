      "utterance": [
        {
          "id": "MDRW2100022749.1.1",
          "form": "곰표 드셔보셨나요?",
          "original_form": "곰표 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022749.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022749.1.3",
          "form": "곰표말표다 안먹어봤어요",
          "original_form": "곰표말표다 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022749.1.4",
          "form": "궁금해요",
          "original_form": "궁금해요",
          "speaker_id": "2",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022749.1.5",
          "form": "곰표 맥주는 맛있더라구요ㅎㅎ",
          "original_form": "곰표 맥주는 맛있더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022749.1.6",
          "form": "말표도드셔보셨어요?",
          "original_form": "말표도드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022749.1.7",
          "form": "말표는 아직이요ㅠ",
          "original_form": "말표는 아직이요ㅠ",
          "speaker_id": "1",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022749.1.8",
          "form": "흑맥주라던데",
          "original_form": "흑맥주라던데",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022749.1.9",
          "form": "기대돼요",
          "original_form": "기대돼요",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022749.1.10",
          "form": "그래요?? 말표도 먹어봐야겠어요",
          "original_form": "그래요?? 말표도 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022749.1.11",
          "form": "그 곰표 큰 과자랑 같이 사서",
          "original_form": "그 곰표 큰 과자랑 같이 사서",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022749.1.12",
          "form": "뚝딱하면좋을것같아요",
          "original_form": "뚝딱하면좋을것같아요",
          "speaker_id": "2",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022749.1.13",
          "form": "그래도 좋죠! 프링글스랑 먹어도 좋구요ㅎㅎ",
          "original_form": "그래도 좋죠! 프링글스랑 먹어도 좋구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022749.1.14",
          "form": "진로 두꺼비는 뭐죠?!",
          "original_form": "진로 두꺼비는 뭐죠?!",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022749.1.15",
          "form": "그 하늘색병 소주인가",
          "original_form": "그 하늘색병 소주인가",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022749.1.16",
          "form": "네네! 하늘색 병에 든 소주요",
          "original_form": "네네! 하늘색 병에 든 소주요",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022749.1.17",
          "form": "아 그거",
          "original_form": "아 그거",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022749.1.18",
          "form": "맛있죠",
          "original_form": "맛있죠",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022749.1.19",
          "form": "주변에 진로만 먹는 애들도 있어요ㅋㅋㅋ",
          "original_form": "주변에 진로만 먹는 애들도 있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022749.1.20",
