        {
          "id": "MDRW2100013853.1.1",
          "form": "머 좋아해요?",
          "original_form": "머 좋아해요?",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013853.1.2",
          "form": "전 고기 좋아해요! 닭고기 돼지고기 소고기!! name2은요?",
          "original_form": "전 고기 좋아해요! 닭고기 돼지고기 소고기!! &name2&은요?",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013853.1.3",
          "form": "저두요!!!돼지고기!!!",
          "original_form": "저두요!!!돼지고기!!!",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013853.1.4",
          "form": "치킨도좋아해요",
          "original_form": "치킨도좋아해요",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013853.1.5",
          "form": "배고프네요ㅜㅜ",
          "original_form": "배고프네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013853.1.6",
          "form": "삼겹살도 먹고싶고 치킨도 먹고싶네요 ㅠㅠ",
          "original_form": "삼겹살도 먹고싶고 치킨도 먹고싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013853.1.7",
          "form": "좋아하는 치킨 브랜드 있나요?",
          "original_form": "좋아하는 치킨 브랜드 있나요?",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013853.1.8",
          "form": "BHC좋아해요",
          "original_form": "BHC좋아해요",
          "speaker_id": "1",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013853.1.9",
          "form": "KFC꺼도맛있고",
          "original_form": "KFC꺼도맛있고",
          "speaker_id": "1",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013853.1.10",
          "form": "저도 bhc 뿌링클 좋아해요!!",
          "original_form": "저도 bhc 뿌링클 좋아해요!!",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013853.1.11",
          "form": "kfc 치킨도 안먹은지 오래되서 먹고싶네요",
          "original_form": "kfc 치킨도 안먹은지 오래되서 먹고싶네요",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013853.1.12",
          "form": "전어제 KFC먹었어요",
          "original_form": "전어제 KFC먹었어요",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013853.1.13",
          "form": "맛있었는데",
          "original_form": "맛있었는데",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013853.1.14",
          "form": "또 먹을려구요",
          "original_form": "또 먹을려구요",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013853.1.15",
          "form": "오늘 또 드시는거에요? 부러워요 ㅠㅠ",
          "original_form": "오늘 또 드시는거에요? 부러워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013853.1.16",
          "form": "너무 맛있어요 땡길때 먹어야죠",
          "original_form": "너무 맛있어요 땡길때 먹어야죠",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013853.1.17",
          "form": "코로나때문에힘든데",
          "original_form": "코로나때문에힘든데",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013853.1.18",
          "form": "그쵸 먹는게 남는거죠!",
          "original_form": "그쵸 먹는게 남는거죠!",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013853.1.19",
          "form": "먹는거라도 먹어야죠",
          "original_form": "먹는거라도 먹어야죠",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013853.1.20",
          "form": "오늘저녁에 햄버거 먹을꺼에요",
          "original_form": "오늘저녁에 햄버거 먹을꺼에요",
