      "utterance": [
        {
          "id": "MDRW2100046910.1.1",
          "form": "너는 지금 학생이야?",
          "original_form": "너는 지금 학생이야?",
          "speaker_id": "1",
          "time": "20211005 21:14"
        },
        {
          "id": "MDRW2100046910.1.2",
          "form": "난 고등학생이야",
          "original_form": "난 고등학생이야",
          "speaker_id": "2",
          "time": "20211005 21:14"
        },
        {
          "id": "MDRW2100046910.1.3",
          "form": "그럼 대체공휴일 확대에 찬성하겠네",
          "original_form": "그럼 대체공휴일 확대에 찬성하겠네",
          "speaker_id": "1",
          "time": "20211005 21:14"
        },
        {
          "id": "MDRW2100046910.1.4",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20211005 21:15"
        },
        {
          "id": "MDRW2100046910.1.5",
          "form": "나는 별로 상관이 없긴 해",
          "original_form": "나는 별로 상관이 없긴 해",
          "speaker_id": "1",
          "time": "20211005 21:15"
        },
        {
          "id": "MDRW2100046910.1.6",
          "form": "쉬는날이",
          "original_form": "쉬는날이",
          "speaker_id": "2",
          "time": "20211005 21:15"
        },
        {
          "id": "MDRW2100046910.1.7",
          "form": "별로야?",
          "original_form": "별로야?",
          "speaker_id": "2",
          "time": "20211005 21:15"
        },
        {
          "id": "MDRW2100046910.1.8",
          "form": "나는 온라인으로만 수업해서 별로 의미가 없어",
          "original_form": "나는 온라인으로만 수업해서 별로 의미가 없어",
          "speaker_id": "1",
          "time": "20211005 21:16"
        },
        {
          "id": "MDRW2100046910.1.9",
          "form": "음 그럴수도 있겠구나",
          "original_form": "음 그럴수도 있겠구나",
          "speaker_id": "2",
          "time": "20211005 21:16"
        },
        {
          "id": "MDRW2100046910.1.10",
          "form": "그래도 취업 후를 생각하면 확대가 좋겠지",
          "original_form": "그래도 취업 후를 생각하면 확대가 좋겠지",
          "speaker_id": "1",
          "time": "20211005 21:16"
        },
        {
          "id": "MDRW2100046910.1.11",
          "form": "적당한 선에서 늘리는건 괜찮다고 생각해",
          "original_form": "적당한 선에서 늘리는건 괜찮다고 생각해",
          "speaker_id": "2",
          "time": "20211005 21:17"
        },
        {
          "id": "MDRW2100046910.1.12",
          "form": "그래봤자 휴일 자체가 많지 않으니 한계고 있고",
          "original_form": "그래봤자 휴일 자체가 많지 않으니 한계고 있고",
          "speaker_id": "1",
          "time": "20211005 21:17"
        },
        {
          "id": "MDRW2100046910.1.13",
          "form": "그것도 맞는거같아",
          "original_form": "그것도 맞는거같아",
          "speaker_id": "2",
          "time": "20211005 21:20"
        },
        {
          "id": "MDRW2100046910.1.14",
          "form": "뭐 그것도 적용되야 좋으니 싫으니 하겠지만",
          "original_form": "뭐 그것도 적용되야 좋으니 싫으니 하겠지만",
          "speaker_id": "1",
          "time": "20211005 21:20"
