      "utterance": [
        {
          "id": "MDRW2100005600.1.1",
          "form": "혹시 해외여행 가보셨어요?",
          "original_form": "혹시 해외여행 가보셨어요?",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005600.1.2",
          "form": "고등학교때는 가보고 성인되고 나서는 못가봤어요",
          "original_form": "고등학교때는 가보고 성인되고 나서는 못가봤어요",
          "speaker_id": "2",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005600.1.3",
          "form": "고등학교때 어디 가보셨어요?",
          "original_form": "고등학교때 어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005600.1.4",
          "form": "늘 해외여행 어디든 가고싶은 생각이 들어요",
          "original_form": "늘 해외여행 어디든 가고싶은 생각이 들어요",
          "speaker_id": "2",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005600.1.5",
          "form": "저는 중국에 다녀왔어요",
          "original_form": "저는 중국에 다녀왔어요",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005600.1.6",
          "form": "최근에 어디 여행다녀오셨어요?",
          "original_form": "최근에 어디 여행다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005600.1.7",
          "form": "아뇨 요즘은 집에만 있는거같아요",
          "original_form": "아뇨 요즘은 집에만 있는거같아요",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005600.1.8",
          "form": "내년이나 내후년에 일본 한번 가고싶네요",
          "original_form": "내년이나 내후년에 일본 한번 가고싶네요",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005600.1.9",
          "form": "중국 어디가 좋았어요?",
          "original_form": "중국 어디가 좋았어요?",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005600.1.10",
          "form": "제 기억에는 만리장성 본게 제일 기억에 남아요 관광장소를 좋아하니깐요",
          "original_form": "제 기억에는 만리장성 본게 제일 기억에 남아요 관광장소를 좋아하니깐요",
          "speaker_id": "2",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005600.1.11",
          "form": "그때 너무 더워서 어디갔는지는 기억이 잘안나네요",
          "original_form": "그때 너무 더워서 어디갔는지는 기억이 잘안나네요",
          "speaker_id": "2",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005600.1.12",
          "form": "거기 음식은 맛있었어요?",
          "original_form": "거기 음식은 맛있었어요?",
          "speaker_id": "1",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005600.1.13",
          "form": "중국 음식은..... 하나도 안맞아서.. 한식으로만 먹었던 기억이 납니다",
          "original_form": "중국 음식은..... 하나도 안맞아서.. 한식으로만 먹었던 기억이 납니다",
          "speaker_id": "2",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005600.1.14",
          "form": "일본은 어떤 목적으로 가시나요?",
          "original_form": "일본은 어떤 목적으로 가시나요?",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005600.1.15",
          "form": "그냥 음식도 먹고 이것저것 먹고싶네요",
          "original_form": "그냥 음식도 먹고 이것저것 먹고싶네요",
          "speaker_id": "1",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005600.1.16",
          "form": "일본음식 좋아하세요?",
          "original_form": "일본음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005600.1.17",
          "form": "일본음식은 친숙하고 라면부터 초밥  많이 먹어봐서 싫어하진 않아요",
          "original_form": "일본음식은 친숙하고 라면부터 초밥  많이 먹어봐서 싫어하진 않아요",
          "speaker_id": "2",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005600.1.18",
          "form": "어떤 초밥이 제일 맛있는거같아요?",
          "original_form": "어떤 초밥이 제일 맛있는거같아요?",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005600.1.19",
          "form": "음 비린거 제외하고 이것저것 잘먹어서 제일 맛있는건 없는거같아요",
          "original_form": "음 비린거 제외하고 이것저것 잘먹어서 제일 맛있는건 없는거같아요",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005600.1.20",
          "form": "국외 여행말고도 국내여행은 자주 하시나요?",
          "original_form": "국외 여행말고도 국내여행은 자주 하시나요?",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005600.1.21",
          "form": "네 저는 서울가는거 좋아해요",
          "original_form": "네 저는 서울가는거 좋아해요",
          "speaker_id": "1",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005600.1.22",
          "form": "서울 최근에 가본적 있어요?",
          "original_form": "서울 최근에 가본적 있어요?",
          "speaker_id": "2",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005600.1.23",
          "form": "최근에는 여행을 잘 못가네요",
          "original_form": "최근에는 여행을 잘 못가네요",
          "speaker_id": "1",
          "time": "20210821 23:34"
        }
      ]
    }
