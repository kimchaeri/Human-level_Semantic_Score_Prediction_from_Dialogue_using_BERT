      "utterance": [
        {
          "id": "MDRW2100037978.1.1",
          "form": "name2님은 집에서 베이킹을 하시나요?",
          "original_form": "&name2&님은 집에서 베이킹을 하시나요?",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100037978.1.2",
          "form": "아 저 딱한번 해봤어요ㅋㅋㄱ오븐사고",
          "original_form": "아 저 딱한번 해봤어요ㅋㅋㄱ오븐사고",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100037978.1.3",
          "form": "전 밥은 지금 좀 일같아서 하기 싫은데 베이킹은 하고 싶어요",
          "original_form": "전 밥은 지금 좀 일같아서 하기 싫은데 베이킹은 하고 싶어요",
          "speaker_id": "1",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100037978.1.4",
          "form": "뭘 구워보셨나요?",
          "original_form": "뭘 구워보셨나요?",
          "speaker_id": "1",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100037978.1.5",
          "form": "저 저탄고지 다이어트 할때 저탄빵 만들어봤어용",
          "original_form": "저 저탄고지 다이어트 할때 저탄빵 만들어봤어용",
          "speaker_id": "2",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100037978.1.6",
          "form": "ㅠ와...막있나요",
          "original_form": "ㅠ와...막있나요",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100037978.1.7",
          "form": "저에게 저탄수화물이란",
          "original_form": "저에게 저탄수화물이란",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100037978.1.8",
          "form": "노맛",
          "original_form": "노맛",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100037978.1.9",
          "form": "어..음.........",
          "original_form": "어..음.........",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100037978.1.10",
          "form": "밀가루가 최곱니다",
          "original_form": "밀가루가 최곱니다",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100037978.1.11",
          "form": "그죠 ㅠ 다음번엔 고탄수 버터 설탕 와따 베이킹을 해보세요",
          "original_form": "그죠 ㅠ 다음번엔 고탄수 버터 설탕 와따 베이킹을 해보세요",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037978.1.12",
          "form": "전 스트레스가 쌓이면 베이킹을 하는데 버터반 밀가루반 설탕반",
          "original_form": "전 스트레스가 쌓이면 베이킹을 하는데 버터반 밀가루반 설탕반",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037978.1.13",
          "form": "ㅋㅋㅋ요즘은 다이어트 안해서 사먹었어여",
          "original_form": "ㅋㅋㅋ요즘은 다이어트 안해서 사먹었어여",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100037978.1.14",
          "form": "요번달에 에그타르트 한번 만들어볼까해요",
          "original_form": "요번달에 에그타르트 한번 만들어볼까해요",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100037978.1.15",
          "form": "그러시다면 에그타르트 쉘로 파이지 완전 추천합니다",
          "original_form": "그러시다면 에그타르트 쉘로 파이지 완전 추천합니다",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100037978.1.16",
          "form": "파이지 잘라다가 까세요",
          "original_form": "파이지 잘라다가 까세요",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100037978.1.17",
          "form": "파이생지요",
          "original_form": "파이생지요",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100037978.1.18",
          "form": "그럼 진짜 파는 맛이 나요",
          "original_form": "그럼 진짜 파는 맛이 나요",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100037978.1.19",
          "form": "행복한 맛 ㅋㅋ",
          "original_form": "행복한 맛 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100037978.1.20",
          "form": "렉이 걸렸나 서버오류인가 지금 계속 말이 가다말다 하네요ㅜ",
          "original_form": "렉이 걸렸나 서버오류인가 지금 계속 말이 가다말다 하네요ㅜ",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100037978.1.21",
          "form": "파이 생지요!? 오 그런게 있군요",
          "original_form": "파이 생지요!? 오 그런게 있군요",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100037978.1.22",
          "form": "담에 만들때 꼭 참고할게요",
          "original_form": "담에 만들때 꼭 참고할게요",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100037978.1.23",
          "form": "맞아요 ㄱ계속 렉걸리네요ㅠ",
          "original_form": "맞아요 ㄱ계속 렉걸리네요ㅠ",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100037978.1.24",
          "form": "베이킹 잘하세요!?",
          "original_form": "베이킹 잘하세요!?",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100037978.1.25",
          "form": "아니오 ㅋㅋ 못해서 그런 꼼수를 써요",
          "original_form": "아니오 ㅋㅋ 못해서 그런 꼼수를 써요",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100037978.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100037978.1.27",
          "form": "아뇨 그런것도 실력이죠",
          "original_form": "아뇨 그런것도 실력이죠",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100037978.1.28",
          "form": "갑자기 버블티가 땡기네요",
          "original_form": "갑자기 버블티가 땡기네요",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100037978.1.29",
          "form": "예전엔",
          "original_form": "예전엔",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100037978.1.30",
          "form": "그럴때마다 나가사먹었는데",
          "original_form": "그럴때마다 나가사먹었는데",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100037978.1.31",
          "form": "이제 우리에겐 배달이 있죠!",
          "original_form": "이제 우리에겐 배달이 있죠!",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100037978.1.32",
          "form": "ㅋㅋ 근데 이제 곧 자야해서 참습니다",
          "original_form": "ㅋㅋ 근데 이제 곧 자야해서 참습니다",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100037978.1.33",
          "form": "그것도 홍차라 먹음 잠이 안오죠 현명하신 선택이에요",
          "original_form": "그것도 홍차라 먹음 잠이 안오죠 현명하신 선택이에요",
          "speaker_id": "1",
          "time": "20210901 23:26"
        }
      ]
    }
