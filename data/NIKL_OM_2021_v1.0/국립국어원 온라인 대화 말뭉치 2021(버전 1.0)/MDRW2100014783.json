      "utterance": [
        {
          "id": "MDRW2100014783.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014783.1.2",
          "form": "넵 안녕하세여!!",
          "original_form": "넵 안녕하세여!!",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014783.1.3",
          "form": "요새 알바나 취업 하셨나요??",
          "original_form": "요새 알바나 취업 하셨나요??",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014783.1.4",
          "form": "아니요ㅠㅠ 취준중이에요",
          "original_form": "아니요ㅠㅠ 취준중이에요",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014783.1.5",
          "form": "취업하셨나요?",
          "original_form": "취업하셨나요?",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014783.1.6",
          "form": "아뇨 ㅋㅋ 저두 그냥 알바중이에요!",
          "original_form": "아뇨 ㅋㅋ 저두 그냥 알바중이에요!",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014783.1.7",
          "form": "요즘 취업하기가 너무 힘들어서ㅠㅠ",
          "original_form": "요즘 취업하기가 너무 힘들어서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014783.1.8",
          "form": "코로나 때문인지 알바도 많이 없는거 같아용",
          "original_form": "코로나 때문인지 알바도 많이 없는거 같아용",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014783.1.9",
          "form": "어떤 알바 하시나여?",
          "original_form": "어떤 알바 하시나여?",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014783.1.10",
          "form": "저는 서포터즈요!! 온라인강의 도우미역할이라고 보면되여ㅋㅋ",
          "original_form": "저는 서포터즈요!! 온라인강의 도우미역할이라고 보면되여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014783.1.11",
          "form": "아하 그런 알바도 있구나ㅋㅋ",
          "original_form": "아하 그런 알바도 있구나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014783.1.12",
          "form": "요즘은 코로나다 보니 온라인 강의 도우미 알바도 있고 신기하네용ㅋㅋㅋ",
          "original_form": "요즘은 코로나다 보니 온라인 강의 도우미 알바도 있고 신기하네용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014783.1.13",
          "form": "맞아요~ 코로나아니었으면 없었을직업이죠 ㅠ",
          "original_form": "맞아요~ 코로나아니었으면 없었을직업이죠 ㅠ",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014783.1.14",
          "form": "혹시 알바 해보신거있으세요??",
          "original_form": "혹시 알바 해보신거있으세요??",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014783.1.15",
          "form": "저는 웨딩촬영 알바랑 파리바게트랑 박람회요ㅋㅋㅋㅋ",
          "original_form": "저는 웨딩촬영 알바랑 파리바게트랑 박람회요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014783.1.16",
          "form": "우와! 엄청다양하게해보셨구나!!",
          "original_form": "우와! 엄청다양하게해보셨구나!!",
          "speaker_id": "1",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100014783.1.17",
          "form": "박람회는 어떤알바였나여??",
          "original_form": "박람회는 어떤알바였나여??",
          "speaker_id": "1",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100014783.1.18",
          "form": "주로 베이비페어 했어요ㅋㅋㅋ",
          "original_form": "주로 베이비페어 했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100014783.1.19",
          "form": "단기알바라서 짧게하고 돈벌기 좋아서 한번 하다보니 있을때마다 하게 되더라구요",
          "original_form": "단기알바라서 짧게하고 돈벌기 좋아서 한번 하다보니 있을때마다 하게 되더라구요",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100014783.1.20",
          "form": "아하! 보기만하고 시도는안해봤네요!",
          "original_form": "아하! 보기만하고 시도는안해봤네요!",
          "speaker_id": "1",
          "time": "20210826 01:46"
        }
      ]
    }
