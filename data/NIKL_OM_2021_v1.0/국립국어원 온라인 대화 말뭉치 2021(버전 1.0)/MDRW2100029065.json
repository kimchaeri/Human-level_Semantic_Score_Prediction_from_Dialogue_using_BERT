        {
          "id": "MDRW2100029065.1.1",
          "form": "name3는 학교 매일가나?",
          "original_form": "&name3&는 학교 매일가나?",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029065.1.2",
          "form": "엉 매일",
          "original_form": "엉 매일",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029065.1.3",
          "form": "올해 매일 갔지?",
          "original_form": "올해 매일 갔지?",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029065.1.4",
          "form": "ㅇㅇ학교가는게낫긴하다",
          "original_form": "ㅇㅇ학교가는게낫긴하다",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029065.1.5",
          "form": "그래도 코로나 환자 안 나왔어?",
          "original_form": "그래도 코로나 환자 안 나왔어?",
          "speaker_id": "1",
          "time": "20210903 07:10"
        },
        {
          "id": "MDRW2100029065.1.6",
          "form": "ㅇㅇ학교엔 조용해",
          "original_form": "ㅇㅇ학교엔 조용해",
          "speaker_id": "2",
          "time": "20210903 08:13"
        },
        {
          "id": "MDRW2100029065.1.7",
          "form": "그래도 다행이다",
          "original_form": "그래도 다행이다",
          "speaker_id": "1",
          "time": "20210903 08:26"
        },
        {
          "id": "MDRW2100029065.1.8",
          "form": "name4는 줌수업하나?",
          "original_form": "&name4&는 줌수업하나?",
          "speaker_id": "2",
          "time": "20210903 08:26"
        },
        {
          "id": "MDRW2100029065.1.9",
          "form": "응 다음주부턴 주2회 나갈듯",
          "original_form": "응 다음주부턴 주2회 나갈듯",
          "speaker_id": "1",
          "time": "20210903 08:37"
        },
        {
          "id": "MDRW2100029065.1.10",
          "form": "10월 4일까지 4단계더라",
          "original_form": "10월 4일까지 4단계더라",
          "speaker_id": "1",
          "time": "20210903 08:37"
        },
        {
          "id": "MDRW2100029065.1.11",
          "form": "그렇구나 요즘확진자가 좀줄었나",
          "original_form": "그렇구나 요즘확진자가 좀줄었나",
          "speaker_id": "2",
          "time": "20210903 08:48"
        },
        {
          "id": "MDRW2100029065.1.12",
          "form": "딱히 줄진않음",
          "original_form": "딱히 줄진않음",
          "speaker_id": "1",
          "time": "20210903 08:50"
        },
        {
          "id": "MDRW2100029065.1.13",
          "form": "반친구도 모르겠네\r\n올해계속 학교안갔제",
          "original_form": "반친구도 모르겠네\r\n올해계속 학교안갔제",
          "speaker_id": "2",
          "time": "20210903 08:57"
        },
        {
          "id": "MDRW2100029065.1.14",
          "form": "아니 주2~3회는 갔음",
          "original_form": "아니 주2~3회는 갔음",
          "speaker_id": "1",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100029065.1.15",
          "form": "아 갔구나 반에 친한친구 있나",
          "original_form": "아 갔구나 반에 친한친구 있나",
          "speaker_id": "2",
          "time": "20210903 09:58"
        },
        {
          "id": "MDRW2100029065.1.16",
          "form": "name5 name6는 아는 얘들이고..근데 쉬는시간이 별로 없어",
          "original_form": "&name5& &name6&는 아는 얘들이고..근데 쉬는시간이 별로 없어",
          "speaker_id": "1",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029065.1.17",
          "form": "같은반이야?",
          "original_form": "같은반이야?",
          "speaker_id": "2",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029065.1.18",
          "form": "그래도 같은반잘됐네 \r\n몇반까지있어?",
          "original_form": "그래도 같은반잘됐네 \r\n몇반까지있어?",
          "speaker_id": "2",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029065.1.19",
