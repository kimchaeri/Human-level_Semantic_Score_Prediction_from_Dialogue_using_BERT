      "utterance": [
        {
          "id": "MDRW2100032869.1.1",
          "form": "머리 염색 해봤어??",
          "original_form": "머리 염색 해봤어??",
          "speaker_id": "2",
          "time": "20210906 02:54"
        },
        {
          "id": "MDRW2100032869.1.2",
          "form": "ㄴㄴㄴ 안 해봤어",
          "original_form": "ㄴㄴㄴ 안 해봤어",
          "speaker_id": "1",
          "time": "20210906 02:55"
        },
        {
          "id": "MDRW2100032869.1.3",
          "form": "검은 머리인 사람은",
          "original_form": "검은 머리인 사람은",
          "speaker_id": "1",
          "time": "20210906 02:55"
        },
        {
          "id": "MDRW2100032869.1.4",
          "form": "자연선택된 것이다 ㅋ",
          "original_form": "자연선택된 것이다 ㅋ",
          "speaker_id": "1",
          "time": "20210906 02:55"
        },
        {
          "id": "MDRW2100032869.1.5",
          "form": "ㅋㅋㅋㅋ 해보고 싶다는 생각은 해본적 없어??",
          "original_form": "ㅋㅋㅋㅋ 해보고 싶다는 생각은 해본적 없어??",
          "speaker_id": "2",
          "time": "20210906 02:56"
        },
        {
          "id": "MDRW2100032869.1.6",
          "form": "해봤지...",
          "original_form": "해봤지...",
          "speaker_id": "1",
          "time": "20210906 02:57"
        },
        {
          "id": "MDRW2100032869.1.7",
          "form": "애쉬핑크나 블루 해보고싶어",
          "original_form": "애쉬핑크나 블루 해보고싶어",
          "speaker_id": "1",
          "time": "20210906 02:57"
        },
        {
          "id": "MDRW2100032869.1.8",
          "form": "베이지도",
          "original_form": "베이지도",
          "speaker_id": "1",
          "time": "20210906 02:57"
        },
        {
          "id": "MDRW2100032869.1.9",
          "form": "근데 나랑은 안 어울릴듯 ㄱㅡ",
          "original_form": "근데 나랑은 안 어울릴듯 ㄱㅡ",
          "speaker_id": "1",
          "time": "20210906 02:57"
        },
        {
          "id": "MDRW2100032869.1.10",
          "form": "애쉬핑크나 블루는 갈릴 수 있어도 베이지는 괜찮지 않아?? 아닌가??",
          "original_form": "애쉬핑크나 블루는 갈릴 수 있어도 베이지는 괜찮지 않아?? 아닌가??",
          "speaker_id": "2",
          "time": "20210906 02:59"
        },
        {
          "id": "MDRW2100032869.1.11",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210906 03:00"
        },
        {
          "id": "MDRW2100032869.1.12",
          "form": "그런가?!?",
          "original_form": "그런가?!?",
          "speaker_id": "1",
          "time": "20210906 03:00"
        },
        {
          "id": "MDRW2100032869.1.13",
          "form": "갑자기 관심생기네",
          "original_form": "갑자기 관심생기네",
          "speaker_id": "1",
          "time": "20210906 03:00"
        },
        {
          "id": "MDRW2100032869.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ탈색 3번은 해야겠지",
          "original_form": "ㅋㅋㅋㅋㅋㅋ탈색 3번은 해야겠지",
          "speaker_id": "1",
          "time": "20210906 03:00"
        },
        {
          "id": "MDRW2100032869.1.15",
          "form": "사람들이 옷 고를때도 베이지, 검정, 하양은 무난템으로 많이 쓰니까!",
          "original_form": "사람들이 옷 고를때도 베이지, 검정, 하양은 무난템으로 많이 쓰니까!",
          "speaker_id": "2",
          "time": "20210906 03:00"
        },
        {
          "id": "MDRW2100032869.1.16",
          "form": "좀 어두운 베이지면 3번까진 안해도 괜찮을껄??",
          "original_form": "좀 어두운 베이지면 3번까진 안해도 괜찮을껄??",
          "speaker_id": "2",
          "time": "20210906 03:01"
        },
        {
          "id": "MDRW2100032869.1.17",
          "form": "탈색하면 머리색 빠지는 것도 있으니까",
          "original_form": "탈색하면 머리색 빠지는 것도 있으니까",
          "speaker_id": "2",
          "time": "20210906 03:01"
        },
        {
          "id": "MDRW2100032869.1.18",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210906 03:01"
        },
        {
          "id": "MDRW2100032869.1.19",
          "form": "하지만 난 밝은 베이지가 아니면.....",
          "original_form": "하지만 난 밝은 베이지가 아니면.....",
          "speaker_id": "1",
          "time": "20210906 03:01"
        },
        {
          "id": "MDRW2100032869.1.20",
          "form": "만족할 수 없는 몸이 되었어",
          "original_form": "만족할 수 없는 몸이 되었어",
          "speaker_id": "1",
          "time": "20210906 03:01"
        },
        {
          "id": "MDRW2100032869.1.21",
          "form": "그럼 나중에 한 번 도전해봐",
          "original_form": "그럼 나중에 한 번 도전해봐",
          "speaker_id": "2",
          "time": "20210906 03:03"
        },
        {
          "id": "MDRW2100032869.1.22",
          "form": "혼자하지말고 미용실가서 꼭",
          "original_form": "혼자하지말고 미용실가서 꼭",
          "speaker_id": "2",
          "time": "20210906 03:03"
        },
        {
          "id": "MDRW2100032869.1.23",
          "form": "아유 당연하지",
          "original_form": "아유 당연하지",
          "speaker_id": "1",
          "time": "20210906 03:03"
        },
        {
          "id": "MDRW2100032869.1.24",
          "form": "울언니 탈색해줬는데",
          "original_form": "울언니 탈색해줬는데",
          "speaker_id": "1",
          "time": "20210906 03:03"
        },
        {
          "id": "MDRW2100032869.1.25",
          "form": "얼룩덜룩하게되더라....",
          "original_form": "얼룩덜룩하게되더라....",
          "speaker_id": "1",
          "time": "20210906 03:03"
        },
        {
          "id": "MDRW2100032869.1.26",
          "form": "집에서 탈색할때는 무조건 많이. 많이 해야돼",
          "original_form": "집에서 탈색할때는 무조건 많이. 많이 해야돼",
          "speaker_id": "2",
          "time": "20210906 03:04"
        },
        {
          "id": "MDRW2100032869.1.27",
          "form": "안그러면 무조건 얼룩졐ㅋㅋㅋ",
          "original_form": "안그러면 무조건 얼룩졐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:05"
        },
        {
          "id": "MDRW2100032869.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 그래서ㅠㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 그래서ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 03:05"
        },
        {
          "id": "MDRW2100032869.1.29",
