      "utterance": [
        {
          "id": "MDRW2100046688.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211105 00:14"
        },
        {
          "id": "MDRW2100046688.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211105 00:15"
        },
        {
          "id": "MDRW2100046688.1.3",
          "form": "아무도 안 계시나요?",
          "original_form": "아무도 안 계시나요?",
          "speaker_id": "1",
          "time": "20211105 00:15"
        },
        {
          "id": "MDRW2100046688.1.4",
          "form": "점심은 드셨나요?",
          "original_form": "점심은 드셨나요?",
          "speaker_id": "2",
          "time": "20211105 00:15"
        },
        {
          "id": "MDRW2100046688.1.5",
          "form": "여기 있습니당 ㅎㅎ",
          "original_form": "여기 있습니당 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 00:15"
        },
        {
          "id": "MDRW2100046688.1.6",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "1",
          "time": "20211105 00:15"
        },
        {
          "id": "MDRW2100046688.1.7",
          "form": "어떤 메뉴 드실 생각이세요!?",
          "original_form": "어떤 메뉴 드실 생각이세요!?",
          "speaker_id": "2",
          "time": "20211105 00:16"
        },
        {
          "id": "MDRW2100046688.1.8",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "1",
          "time": "20211105 00:16"
        },
        {
          "id": "MDRW2100046688.1.9",
          "form": "햄버거",
          "original_form": "햄버거",
          "speaker_id": "1",
          "time": "20211105 00:16"
        },
        {
          "id": "MDRW2100046688.1.10",
          "form": "오 햄버거.... 좋은 선택인데요?",
          "original_form": "오 햄버거.... 좋은 선택인데요?",
          "speaker_id": "2",
          "time": "20211105 00:16"
        },
        {
          "id": "MDRW2100046688.1.11",
          "form": "ㅎㅎ 맛있겠다",
          "original_form": "ㅎㅎ 맛있겠다",
          "speaker_id": "2",
          "time": "20211105 00:16"
        },
        {
          "id": "MDRW2100046688.1.12",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20211105 00:16"
        },
        {
          "id": "MDRW2100046688.1.13",
          "form": "저는 한식뷔페가서 먹으려구요 ㅎㅎ",
          "original_form": "저는 한식뷔페가서 먹으려구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 00:17"
        },
        {
          "id": "MDRW2100046688.1.14",
          "form": "한식뷔페가 밑에 있으니까 메뉴 고민 안해도 되서",
          "original_form": "한식뷔페가 밑에 있으니까 메뉴 고민 안해도 되서",
          "speaker_id": "2",
          "time": "20211105 00:17"
        },
        {
          "id": "MDRW2100046688.1.15",
          "form": "너무 좋아요",
          "original_form": "너무 좋아요",
          "speaker_id": "2",
          "time": "20211105 00:17"
        },
        {
          "id": "MDRW2100046688.1.16",
          "form": "맛있겠어요",
          "original_form": "맛있겠어요",
          "speaker_id": "1",
          "time": "20211105 00:17"
        },
        {
          "id": "MDRW2100046688.1.17",
          "form": "넹 오늘은 된장국에 갈비찜이 메인입니다 쿄쿄",
          "original_form": "넹 오늘은 된장국에 갈비찜이 메인입니다 쿄쿄",
          "speaker_id": "2",
          "time": "20211105 00:17"
        },
        {
          "id": "MDRW2100046688.1.18",
          "form": "햄버거는 어떤 햄버거 좋아하세요?",
          "original_form": "햄버거는 어떤 햄버거 좋아하세요?",
          "speaker_id": "2",
          "time": "20211105 00:17"
        },
        {
          "id": "MDRW2100046688.1.19",
          "form": "화려하네요",
          "original_form": "화려하네요",
          "speaker_id": "1",
          "time": "20211105 00:17"
        },
        {
          "id": "MDRW2100046688.1.20",
          "form": "갈비찜",
          "original_form": "갈비찜",
          "speaker_id": "1",
          "time": "20211105 00:18"
        },
        {
          "id": "MDRW2100046688.1.21",
          "form": "맞아요 제가 일하는 곳은 한식뷔페 경쟁이 치열해서",
          "original_form": "맞아요 제가 일하는 곳은 한식뷔페 경쟁이 치열해서",
          "speaker_id": "2",
          "time": "20211105 00:18"
        },
        {
          "id": "MDRW2100046688.1.22",
          "form": "메뉴가 항상...잘나와요",
          "original_form": "메뉴가 항상...잘나와요",
          "speaker_id": "2",
          "time": "20211105 00:18"
        },
        {
          "id": "MDRW2100046688.1.23",
          "form": "버거킹 좋아요 와퍼",
          "original_form": "버거킹 좋아요 와퍼",
          "speaker_id": "1",
          "time": "20211105 00:18"
        },
        {
          "id": "MDRW2100046688.1.24",
          "form": "크 와퍼 존맛이죠",
          "original_form": "크 와퍼 존맛이죠",
          "speaker_id": "2",
          "time": "20211105 00:18"
        },
        {
          "id": "MDRW2100046688.1.25",
          "form": "이상하게 저는",
          "original_form": "이상하게 저는",
          "speaker_id": "2",
          "time": "20211105 00:18"
        },
        {
          "id": "MDRW2100046688.1.26",
          "form": "와퍼가 작은게 더 맛있더라구요",
          "original_form": "와퍼가 작은게 더 맛있더라구요",
          "speaker_id": "2",
          "time": "20211105 00:18"
        },
        {
          "id": "MDRW2100046688.1.27",
          "form": "뷔페 앞에서는 초라하죠",
          "original_form": "뷔페 앞에서는 초라하죠",
          "speaker_id": "1",
          "time": "20211105 00:19"
        },
        {
          "id": "MDRW2100046688.1.28",
          "form": "ㅎㅎㅎㅎㅎ 아니에요 사실 저도 햄버거가 더 땡기는데",
          "original_form": "ㅎㅎㅎㅎㅎ 아니에요 사실 저도 햄버거가 더 땡기는데",
          "speaker_id": "2",
          "time": "20211105 00:19"
        },
        {
          "id": "MDRW2100046688.1.29",
          "form": "주니어버거는 너무 작아요",
          "original_form": "주니어버거는 너무 작아요",
          "speaker_id": "1",
          "time": "20211105 00:19"
        },
        {
          "id": "MDRW2100046688.1.30",
          "form": "이미 한식뷔페 가기로 정해서 ㅎㅎ ㅠㅠ",
          "original_form": "이미 한식뷔페 가기로 정해서 ㅎㅎ ㅠㅠ",
          "speaker_id": "2",
          "time": "20211105 00:19"
        },
        {
          "id": "MDRW2100046688.1.31",
          "form": "맞아요 작아요... 그래서 사이드를 꼭 같이 먹어줍니다 ㅋㅋㅋㅋ\r\n",
          "original_form": "맞아요 작아요... 그래서 사이드를 꼭 같이 먹어줍니다 ㅋㅋㅋㅋ\r\n",
          "speaker_id": "2",
