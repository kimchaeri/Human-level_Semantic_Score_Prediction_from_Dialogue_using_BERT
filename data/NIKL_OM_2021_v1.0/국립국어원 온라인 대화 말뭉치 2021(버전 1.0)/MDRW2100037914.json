      "utterance": [
        {
          "id": "MDRW2100037914.1.1",
          "form": "맛있는게 이유가있어요",
          "original_form": "맛있는게 이유가있어요",
          "speaker_id": "1",
          "time": "20210902 09:57"
        },
        {
          "id": "MDRW2100037914.1.2",
          "form": "ㅋㅋㅋ 거 든든하네요",
          "original_form": "ㅋㅋㅋ 거 든든하네요",
          "speaker_id": "1",
          "time": "20210902 12:58"
        },
        {
          "id": "MDRW2100037914.1.3",
          "form": "마리모 얼마 하려나",
          "original_form": "마리모 얼마 하려나",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100037914.1.4",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100037914.1.5",
          "form": "진짜 살라구?",
          "original_form": "진짜 살라구?",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100037914.1.6",
          "form": "얼마안할거같은데",
          "original_form": "얼마안할거같은데",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100037914.1.7",
          "form": "당근먼저찾아바바",
          "original_form": "당근먼저찾아바바",
          "speaker_id": "1",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100037914.1.8",
          "form": "아니 근데 좀 무서움",
          "original_form": "아니 근데 좀 무서움",
          "speaker_id": "2",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100037914.1.9",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100037914.1.10",
          "form": "죽어도 죽은줄모를까봐 ㅋㅋ",
          "original_form": "죽어도 죽은줄모를까봐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100037914.1.11",
          "form": "색이 변하겠지",
          "original_form": "색이 변하겠지",
          "speaker_id": "1",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100037914.1.12",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100037914.1.13",
          "form": "아 그런가 기분좋으면 둥둥 뜨기도한데요!",
          "original_form": "아 그런가 기분좋으면 둥둥 뜨기도한데요!",
          "speaker_id": "2",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100037914.1.14",
          "form": "엑 걔 뇌없지않아?",
          "original_form": "엑 걔 뇌없지않아?",
          "speaker_id": "1",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100037914.1.15",
          "form": "아니 뭔가 상상해버렸어ㅜㅜ",
          "original_form": "아니 뭔가 상상해버렸어ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100037914.1.16",
          "form": "아 그런가?",
          "original_form": "아 그런가?",
          "speaker_id": "2",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100037914.1.17",
          "form": "블로그 검색해바바",
          "original_form": "블로그 검색해바바",
          "speaker_id": "1",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100037914.1.18",
          "form": "막 검색하면 진짜 사고싶을까봐 ㅋㅋ",
          "original_form": "막 검색하면 진짜 사고싶을까봐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100037914.1.19",
          "form": "저거 수조? 컵 엎지르면 어캐되지 ㅜㅜ",
          "original_form": "저거 수조? 컵 엎지르면 어캐되지 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100037914.1.20",
          "form": "ㅋㅋ글케얼마안하는거같은데",
          "original_form": "ㅋㅋ글케얼마안하는거같은데",
          "speaker_id": "1",
          "time": "20210902 16:29"
        }
      ]
