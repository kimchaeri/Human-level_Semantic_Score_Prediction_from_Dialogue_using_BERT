      "utterance": [
        {
          "id": "MDRW2100045036.1.1",
          "form": "태백산맥의 정기를 제가 느껴야 할까요?",
          "original_form": "태백산맥의 정기를 제가 느껴야 할까요?",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045036.1.2",
          "form": "느끼면 좋죠~ 다시 태어나는 기분일거예요",
          "original_form": "느끼면 좋죠~ 다시 태어나는 기분일거예요",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045036.1.3",
          "form": "산맥타고 쭉 내려가시면 부산까지도 가실수 있어요",
          "original_form": "산맥타고 쭉 내려가시면 부산까지도 가실수 있어요",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045036.1.4",
          "form": "ㅇ....ㅎ... 그으렇군요?",
          "original_form": "ㅇ....ㅎ... 그으렇군요?",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045036.1.5",
          "form": "그러면 그냥 부산을 가겠어요",
          "original_form": "그러면 그냥 부산을 가겠어요",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045036.1.6",
          "form": "아니 태백의 정기를 느끼며 가셔야 의미가 있죠 !",
          "original_form": "아니 태백의 정기를 느끼며 가셔야 의미가 있죠 !",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045036.1.7",
          "form": "아니요.. 없는 것 같아요",
          "original_form": "아니요.. 없는 것 같아요",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045036.1.8",
          "form": "요즘 관절이 안좋아서",
          "original_form": "요즘 관절이 안좋아서",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045036.1.9",
          "form": "산 못가요",
          "original_form": "산 못가요",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045036.1.10",
          "form": "바다 속에서 관절 주변 근육 강화 운동하게",
          "original_form": "바다 속에서 관절 주변 근육 강화 운동하게",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045036.1.11",
          "form": "부산이나 갈게요",
          "original_form": "부산이나 갈게요",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045036.1.12",
          "form": "관절 주변 근육 강화를 위해 부산에 가시는건가요 ?",
          "original_form": "관절 주변 근육 강화를 위해 부산에 가시는건가요 ?",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045036.1.13",
          "form": "너무 멋지시네요",
          "original_form": "너무 멋지시네요",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045036.1.14",
          "form": "아뇨 그냥 바다보고 싶어서 부산가고 싶어요",
          "original_form": "아뇨 그냥 바다보고 싶어서 부산가고 싶어요",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045036.1.15",
          "form": "그런데 너무 먼것 같아서 부산은 포기하려구요",
          "original_form": "그런데 너무 먼것 같아서 부산은 포기하려구요",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045036.1.16",
          "form": "앗 그럼 속초 어떠세요 ?",
          "original_form": "앗 그럼 속초 어떠세요 ?",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045036.1.17",
          "form": "속초 좋죠 !",
          "original_form": "속초 좋죠 !",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045036.1.18",
          "form": "그런데 거기도 바다가 있어요?",
          "original_form": "그런데 거기도 바다가 있어요?",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045036.1.19",
          "form": "네 ! 속초하면 설악산 ! 이지만",
          "original_form": "네 ! 속초하면 설악산 ! 이지만",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045036.1.20",
          "form": "바다도 있어요 !",
          "original_form": "바다도 있어요 !",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045036.1.21",
          "form": "잘아시네요? 가보셨어요??",
          "original_form": "잘아시네요? 가보셨어요??",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045036.1.22",
          "form": "아뇨 !",
          "original_form": "아뇨 !",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045036.1.23",
          "form": "아니 산부터 나오시네",
          "original_form": "아니 산부터 나오시네",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045036.1.24",
          "form": "산 vs 바다",
          "original_form": "산 vs 바다",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045036.1.25",
          "form": "산 !",
          "original_form": "산 !",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045036.1.26",
          "form": "으 전 바다요",
          "original_form": "으 전 바다요",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045036.1.27",
          "form": "저랑 여행 같이 못 가겠어요",
          "original_form": "저랑 여행 같이 못 가겠어요",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045036.1.28",
          "form": "괜찮아요 한숨 푹 자고 일어나시면",
          "original_form": "괜찮아요 한숨 푹 자고 일어나시면",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045036.1.29",
          "form": "산에 도착해계실거에요",
          "original_form": "산에 도착해계실거에요",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045036.1.30",
          "form": "정말... 즐거운... 여행이 될 것 같네요",
          "original_form": "정말... 즐거운... 여행이 될 것 같네요",
          "speaker_id": "1",
          "time": "20210908 00:57"
        }
      ]
    }
