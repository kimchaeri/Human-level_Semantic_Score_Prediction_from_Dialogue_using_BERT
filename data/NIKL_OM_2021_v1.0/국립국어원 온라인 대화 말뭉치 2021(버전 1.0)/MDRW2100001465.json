        },
        {
          "id": "MDRW2100001465.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 네 안녕하세요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 네 안녕하세요!",
          "speaker_id": "2",
          "time": "20211014 18:23"
        },
        {
          "id": "MDRW2100001465.1.3",
          "form": "혹시 백신은 맞으셨어요?",
          "original_form": "혹시 백신은 맞으셨어요?",
          "speaker_id": "2",
          "time": "20211014 18:23"
        },
        {
          "id": "MDRW2100001465.1.4",
          "form": "네 그럼요",
          "original_form": "네 그럼요",
          "speaker_id": "1",
          "time": "20211014 18:23"
        },
        {
          "id": "MDRW2100001465.1.5",
          "form": "2차까지 맞았습니다",
          "original_form": "2차까지 맞았습니다",
          "speaker_id": "1",
          "time": "20211014 18:24"
        },
        {
          "id": "MDRW2100001465.1.6",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "1",
          "time": "20211014 18:24"
        },
        {
          "id": "MDRW2100001465.1.7",
          "form": "오~~ 화이자로 맞으셨나요?",
          "original_form": "오~~ 화이자로 맞으셨나요?",
          "speaker_id": "2",
          "time": "20211014 18:24"
        },
        {
          "id": "MDRW2100001465.1.8",
          "form": "저는 아직..ㅎㅎ",
          "original_form": "저는 아직..ㅎㅎ",
          "speaker_id": "2",
          "time": "20211014 18:24"
        },
        {
          "id": "MDRW2100001465.1.9",
          "form": "사망 뉴스 나오고 하니까 좀 겁이 나더라구요 ㅠ",
          "original_form": "사망 뉴스 나오고 하니까 좀 겁이 나더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20211014 18:25"
        },
        {
          "id": "MDRW2100001465.1.10",
          "form": "그쵸 걱정되죠",
          "original_form": "그쵸 걱정되죠",
          "speaker_id": "1",
          "time": "20211014 18:25"
        },
        {
          "id": "MDRW2100001465.1.11",
          "form": "저도 맞을 때",
          "original_form": "저도 맞을 때",
          "speaker_id": "1",
          "time": "20211014 18:25"
        },
        {
          "id": "MDRW2100001465.1.12",
          "form": "비행기 처음탈 때처럼 엄청 떨리고",
          "original_form": "비행기 처음탈 때처럼 엄청 떨리고",
          "speaker_id": "1",
          "time": "20211014 18:25"
        },
        {
          "id": "MDRW2100001465.1.13",
          "form": "내일 죽으면 어쩌지? 이랬어요",
          "original_form": "내일 죽으면 어쩌지? 이랬어요",
          "speaker_id": "1",
          "time": "20211014 18:25"
        },
        {
          "id": "MDRW2100001465.1.14",
          "form": "어우 ㅠㅠㅠ 그래도 맞으신게",
          "original_form": "어우 ㅠㅠㅠ 그래도 맞으신게",
          "speaker_id": "2",
          "time": "20211014 18:26"
        },
        {
          "id": "MDRW2100001465.1.15",
          "form": "용기가 대단하시네요 ㅠㅠ",
          "original_form": "용기가 대단하시네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211014 18:26"
        },
        {
          "id": "MDRW2100001465.1.16",
          "form": "전 솔직히 제가 좀 이기적인가 싶긴해서 ㅠ",
          "original_form": "전 솔직히 제가 좀 이기적인가 싶긴해서 ㅠ",
          "speaker_id": "2",
          "time": "20211014 18:27"
        },
        {
          "id": "MDRW2100001465.1.17",
          "form": "에이 목숨이 달렸는데요",
          "original_form": "에이 목숨이 달렸는데요",
          "speaker_id": "1",
          "time": "20211014 18:27"
        },
        {
          "id": "MDRW2100001465.1.18",
          "form": "저도 맞을까말까 고민했는데",
          "original_form": "저도 맞을까말까 고민했는데",
          "speaker_id": "1",
          "time": "20211014 18:27"
        },
        {
          "id": "MDRW2100001465.1.19",
          "form": "공익이라 선택권이 없었습니다",
          "original_form": "공익이라 선택권이 없었습니다",
          "speaker_id": "1",
          "time": "20211014 18:27"
        },
        {
          "id": "MDRW2100001465.1.20",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211014 18:27"
        },
        {
          "id": "MDRW2100001465.1.21",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20211014 18:27"
        },
        {
          "id": "MDRW2100001465.1.22",
          "form": "아직 복무중이세요??",
          "original_form": "아직 복무중이세요??",
          "speaker_id": "2",
          "time": "20211014 18:28"
        },
        {
          "id": "MDRW2100001465.1.23",
          "form": "무조건 맞아야하는구나 ㅠ",
          "original_form": "무조건 맞아야하는구나 ㅠ",
          "speaker_id": "2",
          "time": "20211014 18:28"
        },
        {
          "id": "MDRW2100001465.1.24",
          "form": "네 좀 늦게갔어요",
          "original_form": "네 좀 늦게갔어요",
          "speaker_id": "1",
          "time": "20211014 18:28"
        },
        {
          "id": "MDRW2100001465.1.25",
          "form": "덕분에 백신 맞는 날부터 며칠 쉬고",
          "original_form": "덕분에 백신 맞는 날부터 며칠 쉬고",
          "speaker_id": "1",
          "time": "20211014 18:28"
        },
        {
          "id": "MDRW2100001465.1.26",
          "form": "좋았네요",
          "original_form": "좋았네요",
          "speaker_id": "1",
          "time": "20211014 18:28"
        },
        {
          "id": "MDRW2100001465.1.27",
          "form": "아 그래도 백신휴가 있어서 그나마 다행인거같아요 ㅎㅎ",
          "original_form": "아 그래도 백신휴가 있어서 그나마 다행인거같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211014 18:29"
        },
        {
          "id": "MDRW2100001465.1.28",
          "form": "하여튼 빨리 치료제 나왔으면 좋겠네요...",
          "original_form": "하여튼 빨리 치료제 나왔으면 좋겠네요...",
          "speaker_id": "2",
          "time": "20211014 18:30"
        },
        {
          "id": "MDRW2100001465.1.29",
          "form": "코로나가 2년 동안이나 괴롭힐줄이야..ㅠㅠ",
          "original_form": "코로나가 2년 동안이나 괴롭힐줄이야..ㅠㅠ",
          "speaker_id": "2",
          "time": "20211014 18:31"
        },
        {
          "id": "MDRW2100001465.1.30",
          "form": "작년만해도 올해 추석 안에는 끝날 줄 알았는데",
          "original_form": "작년만해도 올해 추석 안에는 끝날 줄 알았는데",
          "speaker_id": "1",
          "time": "20211014 18:32"
        },
        {
          "id": "MDRW2100001465.1.31",
          "form": "쉽지가 않네요",
          "original_form": "쉽지가 않네요",
          "speaker_id": "1",
          "time": "20211014 18:32"
        },
        {
          "id": "MDRW2100001465.1.32",
          "form": "내년 추석까지 갈 것 같아요",
          "original_form": "내년 추석까지 갈 것 같아요",
          "speaker_id": "1",
          "time": "20211014 18:32"
        },
        {
          "id": "MDRW2100001465.1.33",
          "form": "식사는 하셨나요",
          "original_form": "식사는 하셨나요",
          "speaker_id": "1",
          "time": "20211014 18:33"
        },
        {
          "id": "MDRW2100001465.1.34",
          "form": "내년 추석이라니 진짜 아득해지네요..ㅠ",
          "original_form": "내년 추석이라니 진짜 아득해지네요..ㅠ",
          "speaker_id": "2",
          "time": "20211014 18:33"
        },
        {
          "id": "MDRW2100001465.1.35",
          "form": "네네 이제 하려구요~~",
          "original_form": "네네 이제 하려구요~~",
          "speaker_id": "2",
          "time": "20211014 18:33"
        },
        {
          "id": "MDRW2100001465.1.36",
          "form": "저녁 맛있게 드세요 ㅎㅎ",
          "original_form": "저녁 맛있게 드세요 ㅎㅎ",
