      "utterance": [
        {
          "id": "MDRW2100028521.1.1",
          "form": "여행 좋아하시나요??",
          "original_form": "여행 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210902 20:33"
        },
        {
          "id": "MDRW2100028521.1.2",
          "form": "네~ 좋아해요",
          "original_form": "네~ 좋아해요",
          "speaker_id": "2",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028521.1.3",
          "form": "근데 해외는 가본적이없어요 ㅠㅠ",
          "original_form": "근데 해외는 가본적이없어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028521.1.4",
          "form": "코로나 끝나면 한 번 다녀오세요ㅠㅠ",
          "original_form": "코로나 끝나면 한 번 다녀오세요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028521.1.5",
          "form": "국내 여행지는 어디가 좋으셨어요?",
          "original_form": "국내 여행지는 어디가 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028521.1.6",
          "form": "그래야겠어요 ㅠㅠ",
          "original_form": "그래야겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028521.1.7",
          "form": "아무래도 제주도?",
          "original_form": "아무래도 제주도?",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028521.1.8",
          "form": "바다가 너무이뻐서 좋았어요 ㅎㅎ",
          "original_form": "바다가 너무이뻐서 좋았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028521.1.9",
          "form": "저는 오히려 해외여행만 여러번이고 제주도는 한 번도 못가서 너무 궁금해요 ㅠ",
          "original_form": "저는 오히려 해외여행만 여러번이고 제주도는 한 번도 못가서 너무 궁금해요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028521.1.10",
          "form": "해외여행은 어디가보셨어요~?",
          "original_form": "해외여행은 어디가보셨어요~?",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028521.1.11",
          "form": "추천할만한곳 있나요?",
          "original_form": "추천할만한곳 있나요?",
          "speaker_id": "2",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028521.1.12",
          "form": "저는 캐나다 미국 일본 대만 베트남 가봤어요!",
          "original_form": "저는 캐나다 미국 일본 대만 베트남 가봤어요!",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028521.1.13",
          "form": "와 많이 가보셨네요 ~!",
          "original_form": "와 많이 가보셨네요 ~!",
          "speaker_id": "2",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028521.1.14",
          "form": "대만 가보시는 거 추천할게요!",
          "original_form": "대만 가보시는 거 추천할게요!",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028521.1.15",
          "form": "코로나터질거알았으면...",
          "original_form": "코로나터질거알았으면...",
          "speaker_id": "2",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028521.1.16",
          "form": "대만이요?",
          "original_form": "대만이요?",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028521.1.17",
          "form": "대만 괜찮나요 ?ㅎㅎ",
          "original_form": "대만 괜찮나요 ?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028521.1.18",
          "form": "네네! 대만이 뭔가 일본과 중국의 모습이 다 있어서",
          "original_form": "네네! 대만이 뭔가 일본과 중국의 모습이 다 있어서",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028521.1.19",
          "form": "그 느낌도 재밌고 특유의 아기자기 함이 좋았어요",
          "original_form": "그 느낌도 재밌고 특유의 아기자기 함이 좋았어요",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028521.1.20",
          "form": "향신료 잘 드시면 음식도 괜찮을 거에요!",
          "original_form": "향신료 잘 드시면 음식도 괜찮을 거에요!",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028521.1.21",
          "form": "오~ 그런느낌이구나",
          "original_form": "오~ 그런느낌이구나",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028521.1.22",
          "form": "정말로 코로나 끝나면",
          "original_form": "정말로 코로나 끝나면",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028521.1.23",
          "form": "여행은 꼭 가봐야겠어요 ..ㅠㅠ",
          "original_form": "여행은 꼭 가봐야겠어요 ..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028521.1.24",
          "form": "넘 후회되네여..ㅜ",
          "original_form": "넘 후회되네여..ㅜ",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028521.1.25",
          "form": "그쵸 ㅠㅠㅠ 저도 20년 1월에 해외여행 안간게 너무 후회되더라구요",
          "original_form": "그쵸 ㅠㅠㅠ 저도 20년 1월에 해외여행 안간게 너무 후회되더라구요",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100028521.1.26",
          "form": "조만간 좋아져서 여행 자주 갈 수 있기를 바라는 수밖에 ㅠㅠ",
          "original_form": "조만간 좋아져서 여행 자주 갈 수 있기를 바라는 수밖에 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100028521.1.27",
          "form": "그러니까요 ㅠㅠㅠ 가까운곳도 다니기 힘드니까",
          "original_form": "그러니까요 ㅠㅠㅠ 가까운곳도 다니기 힘드니까",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028521.1.28",
          "form": "너무 답답해요",
          "original_form": "너무 답답해요",
          "speaker_id": "2",
          "time": "20210902 20:44"
        }
      ]
