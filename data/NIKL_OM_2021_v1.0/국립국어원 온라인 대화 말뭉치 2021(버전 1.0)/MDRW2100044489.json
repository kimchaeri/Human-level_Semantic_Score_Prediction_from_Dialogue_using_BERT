      "utterance": [
        {
          "id": "MDRW2100044489.1.1",
          "form": "현미베이크 고런느낌?",
          "original_form": "현미베이크 고런느낌?",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044489.1.2",
          "form": "ㅇㅇㅇㅇ그런느낌",
          "original_form": "ㅇㅇㅇㅇ그런느낌",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044489.1.3",
          "form": "이거 푸라닭 같은 구운? 치킨인가여",
          "original_form": "이거 푸라닭 같은 구운? 치킨인가여",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044489.1.4",
          "form": "노노 완전 튀김ㄴ스",
          "original_form": "노노 완전 튀김ㄴ스",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044489.1.5",
          "form": "오 상상이 안가네 ㅋㅋㅋㅋ",
          "original_form": "오 상상이 안가네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044489.1.6",
          "form": "주말만 기다린다 이제",
          "original_form": "주말만 기다린다 이제",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044489.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ바삭하고 맛잇서 ㅜㅜㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ바삭하고 맛잇서 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044489.1.8",
          "form": "수욜 일욜인가 배민할인하드라",
          "original_form": "수욜 일욜인가 배민할인하드라",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044489.1.9",
          "form": "오...전문가이신데여 ㅋㅋㅋ",
          "original_form": "오...전문가이신데여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044489.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ근데 인기많아서 할인은 딱 한번받아봄ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ근데 인기많아서 할인은 딱 한번받아봄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044489.1.11",
          "form": "ㅋㅋㅋㅋ아 걔는 할인 대상이 아녀?",
          "original_form": "ㅋㅋㅋㅋ아 걔는 할인 대상이 아녀?",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044489.1.12",
          "form": "노노노 할인인데 중간중간에 가게 닫아놓더라",
          "original_form": "노노노 할인인데 중간중간에 가게 닫아놓더라",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044489.1.13",
          "form": "아 그정도 ㄷㄷㄷ",
          "original_form": "아 그정도 ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044489.1.14",
          "form": "근데 김천은 뭐...안그럴수돜ㅋㅋㅋ",
          "original_form": "근데 김천은 뭐...안그럴수돜ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044489.1.15",
          "form": "오 그럭수도잇겟다 여기 지점이 가까운데는 두군데쭌이여",
          "original_form": "오 그럭수도잇겟다 여기 지점이 가까운데는 두군데쭌이여",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044489.1.16",
          "form": "츄라이해보거 후기 알려드리겟슴다",
          "original_form": "츄라이해보거 후기 알려드리겟슴다",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044489.1.17",
          "form": "예스예스 고구마치즈볼도 존맛탱",
          "original_form": "예스예스 고구마치즈볼도 존맛탱",
