      "utterance": [
        {
          "id": "MDRW2100000750.1.1",
          "form": "스타벅스엔",
          "original_form": "스타벅스엔",
          "speaker_id": "2",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100000750.1.2",
          "form": "마성의 메뉴가있지",
          "original_form": "마성의 메뉴가있지",
          "speaker_id": "2",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100000750.1.3",
          "form": "무슨 메뉴인데?",
          "original_form": "무슨 메뉴인데?",
          "speaker_id": "1",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000750.1.4",
          "form": "쿠크앤쿠크로 제조할수도있고",
          "original_form": "쿠크앤쿠크로 제조할수도있고",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000750.1.5",
          "form": "바나나망고 차애",
          "original_form": "바나나망고 차애",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000750.1.6",
          "form": "커피위에?",
          "original_form": "커피위에?",
          "speaker_id": "1",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000750.1.7",
          "form": "응 요청하면 해준대 친구강력추천",
          "original_form": "응 요청하면 해준대 친구강력추천",
          "speaker_id": "2",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100000750.1.8",
          "form": "걸으니까 아이스초코나 먹고싶다",
          "original_form": "걸으니까 아이스초코나 먹고싶다",
          "speaker_id": "1",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100000750.1.9",
          "form": "난 시원한 아메리카노",
          "original_form": "난 시원한 아메리카노",
          "speaker_id": "2",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100000750.1.10",
          "form": "스벅은 과일메뉴가 너무 적어",
          "original_form": "스벅은 과일메뉴가 너무 적어",
          "speaker_id": "1",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100000750.1.11",
          "form": "시즌메뉴로 나와서 그런듯",
          "original_form": "시즌메뉴로 나와서 그런듯",
          "speaker_id": "2",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100000750.1.12",
          "form": "그래서 잘 안가는데 기프티콘땜시 가게됨",
          "original_form": "그래서 잘 안가는데 기프티콘땜시 가게됨",
          "speaker_id": "1",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100000750.1.13",
          "form": "얼마전이었지 청포도 맛있었대",
          "original_form": "얼마전이었지 청포도 맛있었대",
          "speaker_id": "2",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100000750.1.14",
          "form": "오 나중에 마셔봐야겠다",
          "original_form": "오 나중에 마셔봐야겠다",
          "speaker_id": "1",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100000750.1.15",
          "form": "나도 마셔보고싶음",
          "original_form": "나도 마셔보고싶음",
          "speaker_id": "2",
          "time": "20210819 18:59"
