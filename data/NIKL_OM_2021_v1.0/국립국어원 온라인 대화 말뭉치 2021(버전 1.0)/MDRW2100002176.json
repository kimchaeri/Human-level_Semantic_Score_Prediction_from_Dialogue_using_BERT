      "utterance": [
        {
          "id": "MDRW2100002176.1.1",
          "form": "아직 아이가 안자면 언제 주무세요?",
          "original_form": "아직 아이가 안자면 언제 주무세요?",
          "speaker_id": "1",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002176.1.2",
          "form": "전 보통 2시쯩이나 자는거같아여",
          "original_form": "전 보통 2시쯩이나 자는거같아여",
          "speaker_id": "2",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002176.1.3",
          "form": "넘 피곤 하시겠어요",
          "original_form": "넘 피곤 하시겠어요",
          "speaker_id": "1",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002176.1.4",
          "form": "매일 누적되는거같아요ㅎㅎ",
          "original_form": "매일 누적되는거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002176.1.5",
          "form": "아침에 전 6시쯤 일어나는데 힘들더라구요",
          "original_form": "아침에 전 6시쯤 일어나는데 힘들더라구요",
          "speaker_id": "1",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002176.1.6",
          "form": "근데 낮잠자면 또 밤잠을 못자서..",
          "original_form": "근데 낮잠자면 또 밤잠을 못자서..",
          "speaker_id": "2",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002176.1.7",
          "form": "맞아요 그러다 하루 완전 다운돼요",
          "original_form": "맞아요 그러다 하루 완전 다운돼요",
          "speaker_id": "1",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002176.1.8",
          "form": "애매해요",
          "original_form": "애매해요",
          "speaker_id": "2",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002176.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002176.1.10",
          "form": "전 불면증이라 낮잠이 없어요",
          "original_form": "전 불면증이라 낮잠이 없어요",
          "speaker_id": "1",
          "time": "20210820 00:21"
        },
        {
          "id": "MDRW2100002176.1.11",
          "form": "저도 잠을 잘못자서",
          "original_form": "저도 잠을 잘못자서",
          "speaker_id": "2",
          "time": "20210820 00:21"
        },
        {
          "id": "MDRW2100002176.1.12",
          "form": "역도 별로고",
          "original_form": "역도 별로고",
          "speaker_id": "1",
          "time": "20210820 00:21"
        },
        {
          "id": "MDRW2100002176.1.13",
          "form": "애기는 맨날 놀자고하고",
          "original_form": "애기는 맨날 놀자고하고",
          "speaker_id": "2",
          "time": "20210820 00:21"
        },
        {
          "id": "MDRW2100002176.1.14",
          "form": "약이 영 별로고 안들어요",
          "original_form": "약이 영 별로고 안들어요",
          "speaker_id": "1",
          "time": "20210820 00:21"
        },
        {
          "id": "MDRW2100002176.1.15",
          "form": "집안일은 해도 끝도 없고",
          "original_form": "집안일은 해도 끝도 없고",
          "speaker_id": "2",
          "time": "20210820 00:21"
        },
        {
          "id": "MDRW2100002176.1.16",
          "form": "그때 그렇죠",
          "original_form": "그때 그렇죠",
          "speaker_id": "1",
          "time": "20210820 00:21"
        },
        {
          "id": "MDRW2100002176.1.17",
          "form": "네 그리고 속도도 안나요",
          "original_form": "네 그리고 속도도 안나요",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002176.1.18",
          "form": "아이랑 놀아주는개 가장 힘들어요",
          "original_form": "아이랑 놀아주는개 가장 힘들어요",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002176.1.19",
          "form": "저도요!!",
          "original_form": "저도요!!",
          "speaker_id": "2",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002176.1.20",
          "form": "차라리 일하는게 편한거같아요",
          "original_form": "차라리 일하는게 편한거같아요",
          "speaker_id": "2",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002176.1.21",
          "form": "맞아요 일이 나아요",
          "original_form": "맞아요 일이 나아요",
          "speaker_id": "1",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002176.1.22",
          "form": "말도못하는애라..",
          "original_form": "말도못하는애라..",
          "speaker_id": "2",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002176.1.23",
          "form": "그때가 서로 젤 힘들어요",
          "original_form": "그때가 서로 젤 힘들어요",
          "speaker_id": "1",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002176.1.24",
          "form": "휴ㅋㅋ요즘 말안듣는 18개월이라ㅋㅋ미치고잇어ᆞ여",
          "original_form": "휴ㅋㅋ요즘 말안듣는 18개월이라ㅋㅋ미치고잇어ᆞ여",
          "speaker_id": "2",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002176.1.25",
          "form": "18개월 젤힘들죠 ㅜㅜ",
          "original_form": "18개월 젤힘들죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002176.1.26",
          "form": "말이 안통해서요ㅜㅜ",
          "original_form": "말이 안통해서요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002176.1.27",
          "form": "저희애 29개월에 말했어요",
          "original_form": "저희애 29개월에 말했어요",
          "speaker_id": "1",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002176.1.28",
          "form": "정말 힘들었어요",
          "original_form": "정말 힘들었어요",
          "speaker_id": "1",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002176.1.29",
          "form": "천천히 했나봐요 급한게 없으니",
          "original_form": "천천히 했나봐요 급한게 없으니",
          "speaker_id": "2",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002176.1.30",
          "form": "네 그런가봐요",
          "original_form": "네 그런가봐요",
          "speaker_id": "1",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002176.1.31",
          "form": "저희애는 이것저것 떠들긴하는데 저랑 소통이 안되니 매일 화내여",
          "original_form": "저희애는 이것저것 떠들긴하는데 저랑 소통이 안되니 매일 화내여",
          "speaker_id": "2",
          "time": "20210820 00:25"
        }
      ]
    }
