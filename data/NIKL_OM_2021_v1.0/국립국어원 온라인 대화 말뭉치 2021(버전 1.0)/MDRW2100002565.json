      "utterance": [
        {
          "id": "MDRW2100002565.1.1",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002565.1.2",
          "form": "전 넷플릭스 왓챠 유튜브 봐요",
          "original_form": "전 넷플릭스 왓챠 유튜브 봐요",
          "speaker_id": "1",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002565.1.3",
          "form": "안녕하세요~ 저랑 비슷하시네요~ 저도 넷플릭스, 유튜브 즐겨본답니다",
          "original_form": "안녕하세요~ 저랑 비슷하시네요~ 저도 넷플릭스, 유튜브 즐겨본답니다",
          "speaker_id": "2",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002565.1.4",
          "form": "네 유튜브는 주식 방송 자주봐요",
          "original_form": "네 유튜브는 주식 방송 자주봐요",
          "speaker_id": "1",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002565.1.5",
          "form": "음악도 찾아서 듣고요",
          "original_form": "음악도 찾아서 듣고요",
          "speaker_id": "1",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002565.1.6",
          "form": "넷플릭스는 애니하고 영화 시리즈는 위쳐 기다려요",
          "original_form": "넷플릭스는 애니하고 영화 시리즈는 위쳐 기다려요",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002565.1.7",
          "form": "12월에 한다고 해서 좌절중요",
          "original_form": "12월에 한다고 해서 좌절중요",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002565.1.8",
          "form": "저도 주식관심있어서 작년에는 많이 봤는데 올해는 잘 못보고 있네요 ㅠㅠ",
          "original_form": "저도 주식관심있어서 작년에는 많이 봤는데 올해는 잘 못보고 있네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002565.1.9",
          "form": "넷플릭스에서 위쳐 재밌다고 들었어요!",
          "original_form": "넷플릭스에서 위쳐 재밌다고 들었어요!",
          "speaker_id": "2",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002565.1.10",
          "form": "시리즈 몇까지 나온거예요?",
          "original_form": "시리즈 몇까지 나온거예요?",
          "speaker_id": "2",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002565.1.11",
          "form": "1이요",
          "original_form": "1이요",
          "speaker_id": "1",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002565.1.12",
          "form": "2 기다려요",
          "original_form": "2 기다려요",
          "speaker_id": "1",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002565.1.13",
          "form": "현지에서 코로나로 잦은 중단사고 있었대요 ㅜㅜ",
          "original_form": "현지에서 코로나로 잦은 중단사고 있었대요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002565.1.14",
          "form": "ㅠㅠ 팬들에게는 너무 아쉬운 소식이겠는데요 시즌 기다릴 때 완전 애타는데 ㅋㅋ",
          "original_form": "ㅠㅠ 팬들에게는 너무 아쉬운 소식이겠는데요 시즌 기다릴 때 완전 애타는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002565.1.15",
          "form": "그러니까요 12월이라니",
          "original_form": "그러니까요 12월이라니",
          "speaker_id": "1",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002565.1.16",
          "form": "전 애니를 좀봐요",
          "original_form": "전 애니를 좀봐요",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002565.1.17",
          "form": "왓챠도 주술 회전때문에 봤어요",
          "original_form": "왓챠도 주술 회전때문에 봤어요",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002565.1.18",
          "form": "애니는 전 많이 안보는데 어떤류를 많이 보세요?",
          "original_form": "애니는 전 많이 안보는데 어떤류를 많이 보세요?",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002565.1.19",
          "form": "애알못인 제가 알만한 것들 있을까요 ㅋㅋ",
          "original_form": "애알못인 제가 알만한 것들 있을까요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002565.1.20",
          "form": "전 여샌 주술회전 빼곤 귀멸의 칼날요",
          "original_form": "전 여샌 주술회전 빼곤 귀멸의 칼날요",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002565.1.21",
          "form": "맨첨엔 안봤는데 나중에 몰아서 봤어요",
          "original_form": "맨첨엔 안봤는데 나중에 몰아서 봤어요",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002565.1.22",
          "form": "나의 히어로 아카데미 원펀맨 등이요",
          "original_form": "나의 히어로 아카데미 원펀맨 등이요",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002565.1.23",
          "form": "전 취향이 좀 남성적이예요",
          "original_form": "전 취향이 좀 남성적이예요",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002565.1.24",
          "form": "오호 그렇구나 저는 다 처음들어보는것들이긴 해요",
          "original_form": "오호 그렇구나 저는 다 처음들어보는것들이긴 해요",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002565.1.25",
          "form": "네 그럼 뭐 보세요?",
          "original_form": "네 그럼 뭐 보세요?",
          "speaker_id": "1",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002565.1.26",
          "form": "드라마는 펜트하우스봐요 ㅋㅋ",
          "original_form": "드라마는 펜트하우스봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002565.1.27",
          "form": "막장이 ㅜㅜ땡기네요 요새",
          "original_form": "막장이 ㅜㅜ땡기네요 요새",
          "speaker_id": "1",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002565.1.28",
          "form": "ㅋㅋㅋ앜ㅋㅋ 펜트하우스 어머니가 좋아하시던데 저는 안봐봤네요 엄청 재밌어하시더라구요",
          "original_form": "ㅋㅋㅋ앜ㅋㅋ 펜트하우스 어머니가 좋아하시던데 저는 안봐봤네요 엄청 재밌어하시더라구요",
          "speaker_id": "2",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002565.1.29",
          "form": "저는 그냥 티비는 예능같은거보고.. 드라마는 추리들어가있는 수사물 같은거 좋아해요",
          "original_form": "저는 그냥 티비는 예능같은거보고.. 드라마는 추리들어가있는 수사물 같은거 좋아해요",
          "speaker_id": "2",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002565.1.30",
          "form": "어머님 ㅋㅋ 순간 슬퍼졌어요",
          "original_form": "어머님 ㅋㅋ 순간 슬퍼졌어요",
          "speaker_id": "1",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002565.1.31",
          "form": "추리 어떤거요?",
          "original_form": "추리 어떤거요?",
          "speaker_id": "1",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002565.1.32",
          "form": "요새 추리물 있나요?",
          "original_form": "요새 추리물 있나요?",
          "speaker_id": "1",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002565.1.33",
          "form": "요새는 없고 옛날꺼 봣어요!",
          "original_form": "요새는 없고 옛날꺼 봣어요!",
          "speaker_id": "2",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002565.1.34",
          "form": "어머님말고 동생도 좋아한답니닼ㅋㅋ",
          "original_form": "어머님말고 동생도 좋아한답니닼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002565.1.35",
          "form": "넷플릭스 에얼리어니스트 잼나요",
          "original_form": "넷플릭스 에얼리어니스트 잼나요",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002565.1.36",
          "form": "최근에 본거는 보이스4랑 프리즌 브레이크",
          "original_form": "최근에 본거는 보이스4랑 프리즌 브레이크",
          "speaker_id": "2",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002565.1.37",
          "form": "네 다행이네요 ㅋㅋ",
          "original_form": "네 다행이네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002565.1.38",
          "form": "에얼리어니스트 무슨내용이예요?",
          "original_form": "에얼리어니스트 무슨내용이예요?",
          "speaker_id": "2",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002565.1.39",
          "form": "아 전 보이스는 3편까지 넘좋았는데 4편은 조금만 봤네요",
          "original_form": "아 전 보이스는 3편까지 넘좋았는데 4편은 조금만 봤네요",
          "speaker_id": "1",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002565.1.40",
          "form": "수사하며 연쇄 살인범 잡는 내용인데",
          "original_form": "수사하며 연쇄 살인범 잡는 내용인데",
          "speaker_id": "1",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002565.1.41",
          "form": "프로파일러 없던 시절 최초에 프로파일러 설정이죠",
          "original_form": "프로파일러 없던 시절 최초에 프로파일러 설정이죠",
          "speaker_id": "1",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002565.1.42",
          "form": "진짜 잼나요",
          "original_form": "진짜 잼나요",
          "speaker_id": "1",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002565.1.43",
          "form": "저도 4편 의리로 봣어요 ㅋㅋ나름 재밌긴했지만",
          "original_form": "저도 4편 의리로 봣어요 ㅋㅋ나름 재밌긴했지만",
          "speaker_id": "2",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002565.1.44",
          "form": "오 완전 재밌겠는데요? 흥미로워요 이번주말에 당장봐야겠어요",
          "original_form": "오 완전 재밌겠는데요? 흥미로워요 이번주말에 당장봐야겠어요",
          "speaker_id": "2",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002565.1.45",
          "form": "의리로 저도 보려다 하차했어요",
          "original_form": "의리로 저도 보려다 하차했어요",
          "speaker_id": "1",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002565.1.46",
          "form": "ㅋㅋ인정요..",
          "original_form": "ㅋㅋ인정요..",
          "speaker_id": "2",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002565.1.47",
          "form": "설정이 좀 과한듯했어요",
          "original_form": "설정이 좀 과한듯했어요",
          "speaker_id": "2",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002565.1.48",
          "form": "네 ㅎㅎ한번 보세요",
          "original_form": "네 ㅎㅎ한번 보세요",
          "speaker_id": "1",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002565.1.49",
          "form": "저도 그부분이 세상에",
          "original_form": "저도 그부분이 세상에",
          "speaker_id": "1",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002565.1.50",
          "form": "무슨 남자가 가면 같은거 쓰면 체형이 달라지나요 ㅡㅡ어색해요",
          "original_form": "무슨 남자가 가면 같은거 쓰면 체형이 달라지나요 ㅡㅡ어색해요",
          "speaker_id": "1",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002565.1.51",
          "form": "이레귤러스 보셨어요?",
          "original_form": "이레귤러스 보셨어요?",
          "speaker_id": "1",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002565.1.52",
          "form": "그것도 추리물인데 잼나요",
          "original_form": "그것도 추리물인데 잼나요",
          "speaker_id": "1",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002565.1.53",
          "form": "생각해보니 저도 추리물만 보네요",
          "original_form": "생각해보니 저도 추리물만 보네요",
          "speaker_id": "1",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002565.1.54",
          "form": "코난도 좋아해요 ㅎㅎ",
          "original_form": "코난도 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002565.1.55",
          "form": "ㅋㅋㅋㅋㅋ체형ㅋㅋㅋㅋㅋ저도 그부분웃겼어요 말이 되나? 이런느낌",
          "original_form": "ㅋㅋㅋㅋㅋ체형ㅋㅋㅋㅋㅋ저도 그부분웃겼어요 말이 되나? 이런느낌",
          "speaker_id": "2",
          "time": "20210820 14:17"
