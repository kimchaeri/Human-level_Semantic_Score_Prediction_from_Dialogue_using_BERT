      "utterance": [
        {
          "id": "MDRW2100018796.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018796.1.2",
          "form": "안녕하세요! 점심 드셨나요",
          "original_form": "안녕하세요! 점심 드셨나요",
          "speaker_id": "2",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018796.1.3",
          "form": "아뇨 아직이요~",
          "original_form": "아뇨 아직이요~",
          "speaker_id": "1",
          "time": "20210827 12:14"
        },
        {
          "id": "MDRW2100018796.1.4",
          "form": "뭐 드실거에요?",
          "original_form": "뭐 드실거에요?",
          "speaker_id": "2",
          "time": "20210827 12:14"
        },
        {
          "id": "MDRW2100018796.1.5",
          "form": "저는 브리또먹을거에요~ name2님는요??",
          "original_form": "저는 브리또먹을거에요~ &name2&님는요??",
          "speaker_id": "1",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018796.1.6",
          "form": "저는 닭다리 오븐구이가 오늘 식당 메뉴에요ㅎㅎ",
          "original_form": "저는 닭다리 오븐구이가 오늘 식당 메뉴에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:16"
        },
        {
          "id": "MDRW2100018796.1.7",
          "form": "우와~ 식당에서 급식처럼 드시는건가봐요!",
          "original_form": "우와~ 식당에서 급식처럼 드시는건가봐요!",
          "speaker_id": "1",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100018796.1.8",
          "form": "저두 치킨이 급 땡기네요",
          "original_form": "저두 치킨이 급 땡기네요",
          "speaker_id": "1",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100018796.1.9",
          "form": "금요일이라 맛있는 메뉴 나오는 날이에요ㅎㅎ",
          "original_form": "금요일이라 맛있는 메뉴 나오는 날이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018796.1.10",
          "form": "치킨은 언제나 옳아요",
          "original_form": "치킨은 언제나 옳아요",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018796.1.11",
          "form": "그쵸~ 혹시 어느브랜드치킨 좋아하세요?",
          "original_form": "그쵸~ 혹시 어느브랜드치킨 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018796.1.12",
          "form": "저는 주로 시키는건 교촌인데",
          "original_form": "저는 주로 시키는건 교촌인데",
          "speaker_id": "2",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018796.1.13",
          "form": "처갓집 양념통닭도 조아해서 자주 먹어요ㅎㅎ",
          "original_form": "처갓집 양념통닭도 조아해서 자주 먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018796.1.14",
          "form": "아하! 역시 양념은 처갓집이죠 ㅋㅋ 저두교촌좋아해요 근데 집앞에있는매장이 맛이없어서 안먹은지 오래됐어요ㅜ",
          "original_form": "아하! 역시 양념은 처갓집이죠 ㅋㅋ 저두교촌좋아해요 근데 집앞에있는매장이 맛이없어서 안먹은지 오래됐어요ㅜ",
          "speaker_id": "1",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100018796.1.15",
          "form": "저두 집앞에 교촌보다 처갓집이 더 맛있어서ㅋㅋ 식당 다녀왔는데 닭 반마리를 주네요 너무 배불러요",
          "original_form": "저두 집앞에 교촌보다 처갓집이 더 맛있어서ㅋㅋ 식당 다녀왔는데 닭 반마리를 주네요 너무 배불러요",
          "speaker_id": "2",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100018796.1.16",
          "form": "오! 역시 ㅋㅋ",
          "original_form": "오! 역시 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018796.1.17",
          "form": "전요새 치킨은 프랜차이즈보다 그냥 개인사업장가서 포장해와요!",
          "original_form": "전요새 치킨은 프랜차이즈보다 그냥 개인사업장가서 포장해와요!",
          "speaker_id": "1",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018796.1.18",
          "form": "오 로컬맛집 있나봐요",
          "original_form": "오 로컬맛집 있나봐요",
          "speaker_id": "2",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100018796.1.19",
          "form": "음 부산대에 있어요!",
          "original_form": "음 부산대에 있어요!",
          "speaker_id": "1",
          "time": "20210827 14:33"
        },
        {
          "id": "MDRW2100018796.1.20",
          "form": "앗 그렇군요! 전 그 동네 거주자인데 궁금하네요ㅋㅋㅋㅋ",
          "original_form": "앗 그렇군요! 전 그 동네 거주자인데 궁금하네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:37"
        },
        {
          "id": "MDRW2100018796.1.21",
          "form": "오 그래욧?? 부대에 레오앤아그너스라고 브리또파는데 맛나요 ㅎㅎ",
          "original_form": "오 그래욧?? 부대에 레오앤아그너스라고 브리또파는데 맛나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018796.1.22",
          "form": "오 좋아요 담번에 가봐야겠어요ㅋㅋㅋ 브리또집이 생겼었군요",
          "original_form": "오 좋아요 담번에 가봐야겠어요ㅋㅋㅋ 브리또집이 생겼었군요",
          "speaker_id": "2",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100018796.1.23",
          "form": "맛집추천 감사용ㅋㅋㅋㅋ",
          "original_form": "맛집추천 감사용ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100018796.1.24",
          "form": "넹 ㅋㅋㅋㅋ 아참 특정 야채싫어하시면 안좋아하실수도있어요 야채듬뿍이거든요 ㅋㅋ",
          "original_form": "넹 ㅋㅋㅋㅋ 아참 특정 야채싫어하시면 안좋아하실수도있어요 야채듬뿍이거든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100018796.1.25",
          "form": "아하 야채 좋아해요! 서브웨이에서도 빼는 야채없이 다먹어요ㅎㅎ",
          "original_form": "아하 야채 좋아해요! 서브웨이에서도 빼는 야채없이 다먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100018796.1.26",
          "form": "아하! 그러면 좋아하실거같아요 ㅎㅎ",
          "original_form": "아하! 그러면 좋아하실거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:44"
        }
      ]
    }
