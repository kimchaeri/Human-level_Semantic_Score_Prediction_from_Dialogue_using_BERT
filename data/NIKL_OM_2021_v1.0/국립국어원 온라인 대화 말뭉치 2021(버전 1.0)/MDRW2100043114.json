      "utterance": [
        {
          "id": "MDRW2100043114.1.1",
          "form": "걔 잡는데",
          "original_form": "걔 잡는데",
          "speaker_id": "2",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100043114.1.2",
          "form": "2분씩 걸려....",
          "original_form": "2분씩 걸려....",
          "speaker_id": "2",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100043114.1.3",
          "form": "불이 젤 속성 맞추기 힘들어..",
          "original_form": "불이 젤 속성 맞추기 힘들어..",
          "speaker_id": "1",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100043114.1.4",
          "form": "피슬로 톡톡 때려가면서 잡느라",
          "original_form": "피슬로 톡톡 때려가면서 잡느라",
          "speaker_id": "2",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100043114.1.5",
          "form": "근데 불 하나 카운터치자고",
          "original_form": "근데 불 하나 카운터치자고",
          "speaker_id": "2",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100043114.1.6",
          "form": "물 넣긴 좀",
          "original_form": "물 넣긴 좀",
          "speaker_id": "2",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100043114.1.7",
          "form": "일단 물은",
          "original_form": "일단 물은",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043114.1.8",
          "form": "바바라말고 없음...",
          "original_form": "바바라말고 없음...",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043114.1.9",
          "form": "거기다 물포터 쓸만한 애가",
          "original_form": "거기다 물포터 쓸만한 애가",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043114.1.10",
          "form": "타탈 코코미정도?",
          "original_form": "타탈 코코미정도?",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043114.1.11",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043114.1.12",
          "form": "행추",
          "original_form": "행추",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043114.1.13",
          "form": "행추 비경에서도 많이 쓰고",
          "original_form": "행추 비경에서도 많이 쓰고",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043114.1.14",
          "form": "아 행추",
          "original_form": "아 행추",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043114.1.15",
          "form": "꽤 좋대",
          "original_form": "꽤 좋대",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043114.1.16",
          "form": "ㅇㅇ말은 들었어",
          "original_form": "ㅇㅇ말은 들었어",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043114.1.17",
          "form": "그치만 업는걸",
          "original_form": "그치만 업는걸",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043114.1.18",
          "form": "라이덴을 다키우면 내가보기엔",
          "original_form": "라이덴을 다키우면 내가보기엔",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043114.1.19",
          "form": "피슬을 뺄 준비를",
          "original_form": "피슬을 뺄 준비를",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043114.1.20",
          "form": "해야할것같아",
          "original_form": "해야할것같아",
          "speaker_id": "2",
          "time": "20210907 14:31"
        },
        {
          "id": "MDRW2100043114.1.21",
          "form": "피슬이 보조딜러+서포터로",
          "original_form": "피슬이 보조딜러+서포터로",
          "speaker_id": "2",
          "time": "20210907 14:31"
        },
        {
          "id": "MDRW2100043114.1.22",
          "form": "범용성 있게 쓰이는건",
          "original_form": "범용성 있게 쓰이는건",
          "speaker_id": "2",
          "time": "20210907 14:31"
        },
        {
          "id": "MDRW2100043114.1.23",
          "form": "오즈 소환해놓고 번개 짤짤이딜 넣는용인데",
          "original_form": "오즈 소환해놓고 번개 짤짤이딜 넣는용인데",
          "speaker_id": "2",
          "time": "20210907 14:31"
        },
        {
          "id": "MDRW2100043114.1.24",
          "form": "주딜을 라이덴을 쓸거면",
          "original_form": "주딜을 라이덴을 쓸거면",
          "speaker_id": "2",
          "time": "20210907 14:31"
        },
        {
          "id": "MDRW2100043114.1.25",
          "form": "번개원소 추가딜은 필요없어",
          "original_form": "번개원소 추가딜은 필요없어",
          "speaker_id": "2",
          "time": "20210907 14:31"
        },
        {
          "id": "MDRW2100043114.1.26",
          "form": "상자는 찾았어?",
          "original_form": "상자는 찾았어?",
          "speaker_id": "2",
          "time": "20210907 14:33"
        },
        {
          "id": "MDRW2100043114.1.27",
          "form": "ㅇㅇ 찾음!",
          "original_form": "ㅇㅇ 찾음!",
          "speaker_id": "1",
          "time": "20210907 14:38"
        },
        {
          "id": "MDRW2100043114.1.28",
          "form": "어제 일퀘 안해서",
          "original_form": "어제 일퀘 안해서",
          "speaker_id": "1",
          "time": "20210907 14:39"
        },
        {
          "id": "MDRW2100043114.1.29",
          "form": "일퀘 짤짤이 하는줃",
          "original_form": "일퀘 짤짤이 하는줃",
          "speaker_id": "1",
          "time": "20210907 14:39"
        },
        {
          "id": "MDRW2100043114.1.30",
          "form": "그리고 나선비경에선 어차피 두 팀이 들어가야 하니까",
          "original_form": "그리고 나선비경에선 어차피 두 팀이 들어가야 하니까",
          "speaker_id": "1",
          "time": "20210907 14:39"
        },
        {
          "id": "MDRW2100043114.1.31",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 14:39"
        },
        {
          "id": "MDRW2100043114.1.32",
          "form": "계속 쓰긴 할듯",
          "original_form": "계속 쓰긴 할듯",
          "speaker_id": "2",
          "time": "20210907 14:39"
        },
        {
          "id": "MDRW2100043114.1.33",
          "form": "기이한 소라 퀘스트도 해",
          "original_form": "기이한 소라 퀘스트도 해",
          "speaker_id": "1",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100043114.1.34",
          "form": "그거 어디선가",
          "original_form": "그거 어디선가",
          "speaker_id": "2",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100043114.1.35",
          "form": "하나씩 줍는데",
          "original_form": "하나씩 줍는데",
          "speaker_id": "2",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100043114.1.36",
          "form": "어디다 쓰는거야?",
          "original_form": "어디다 쓰는거야?",
          "speaker_id": "2",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100043114.1.37",
          "form": "그거",
          "original_form": "그거",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043114.1.38",
          "form": "타타라스나 석상의",
          "original_form": "타타라스나 석상의",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043114.1.39",
          "form": "왼쪽 위에 붙어잇는 섬 있지?",
          "original_form": "왼쪽 위에 붙어잇는 섬 있지?",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043114.1.40",
          "form": "거기 해안가에 가보면 소라모양 집이랑 npc가 잇음",
          "original_form": "거기 해안가에 가보면 소라모양 집이랑 npc가 잇음",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043114.1.41",
          "form": "ㅇㅓ",
          "original_form": "ㅇㅓ",
          "speaker_id": "2",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043114.1.42",
          "form": "걔한테 매일 3개씩 상자랑 바꿔먹으면 돼",
          "original_form": "걔한테 매일 3개씩 상자랑 바꿔먹으면 돼",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043114.1.43",
          "form": "헐......",
          "original_form": "헐......",
          "speaker_id": "2",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043114.1.44",
          "form": "헷갈리면 이나즈마 기이한 소라 치면 공략 나옴 ㅋㅋㅋ",
          "original_form": "헷갈리면 이나즈마 기이한 소라 치면 공략 나옴 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043114.1.45",
          "form": "리월에  있는 소라모양 집은",
          "original_form": "리월에  있는 소라모양 집은",
          "speaker_id": "2",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043114.1.46",
          "form": "그런거 없엇는데",
          "original_form": "그런거 없엇는데",
          "speaker_id": "2",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043114.1.47",
          "form": "나도 순간",
          "original_form": "나도 순간",
          "speaker_id": "1",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043114.1.48",
          "form": "그거랑 관련있나 싶긴 햇는데",
          "original_form": "그거랑 관련있나 싶긴 햇는데",
          "speaker_id": "1",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043114.1.49",
          "form": "일단 그런 얘기는 안나옴...",
          "original_form": "일단 그런 얘기는 안나옴...",
          "speaker_id": "1",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043114.1.50",
          "form": "그래........",
          "original_form": "그래........",
          "speaker_id": "2",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043114.1.51",
          "form": "이따가 함 가봐야겠다",
          "original_form": "이따가 함 가봐야겠다",
          "speaker_id": "2",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043114.1.52",
          "form": "ㅇㅇ 가다가 길 모르겠으면 말해",
          "original_form": "ㅇㅇ 가다가 길 모르겠으면 말해",
          "speaker_id": "1",
          "time": "20210907 14:56"
        }
      ]
    }
