      "utterance": [
        {
          "id": "MDRW2100020089.1.1",
          "form": "종교활동 하시나요?",
          "original_form": "종교활동 하시나요?",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020089.1.2",
          "form": "저는 무교예요~",
          "original_form": "저는 무교예요~",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020089.1.3",
          "form": "교회가본적 있으신가요?",
          "original_form": "교회가본적 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020089.1.4",
          "form": "저는 과자준대서 어릴때 한번가봤어요",
          "original_form": "저는 과자준대서 어릴때 한번가봤어요",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020089.1.5",
          "form": "네 어릴때 친구따라 간 적 있어요ㅋㅋ",
          "original_form": "네 어릴때 친구따라 간 적 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020089.1.6",
          "form": "이것저것 선물 많이 받았던것같아요",
          "original_form": "이것저것 선물 많이 받았던것같아요",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020089.1.7",
          "form": "그죠 어릴때 다들 한번씩 선물받으러 가신거 같아요ㅋㅋㅋㅋ",
          "original_form": "그죠 어릴때 다들 한번씩 선물받으러 가신거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020089.1.8",
          "form": "종교랑 상관없이요",
          "original_form": "종교랑 상관없이요",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020089.1.9",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020089.1.10",
          "form": "간식도 주고~크리스마스 선물이며 부활절 달걀이며",
          "original_form": "간식도 주고~크리스마스 선물이며 부활절 달걀이며",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020089.1.11",
          "form": "어릴땐 마냥 재밌었죠ㅋㅋ",
          "original_form": "어릴땐 마냥 재밌었죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020089.1.12",
          "form": "그죠ㅋㅋㅋㅋ",
          "original_form": "그죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020089.1.13",
          "form": "절은 가보신적 있나요?",
          "original_form": "절은 가보신적 있나요?",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020089.1.14",
          "form": "절도 가봤어요!",
          "original_form": "절도 가봤어요!",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020089.1.15",
          "form": "등산 다니다보면 절은 한번씩 구경?하러 가게되더라구요",
          "original_form": "등산 다니다보면 절은 한번씩 구경?하러 가게되더라구요",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020089.1.16",
          "form": "절은 대부분 산속에 있어서 구경하기 좋죠",
          "original_form": "절은 대부분 산속에 있어서 구경하기 좋죠",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020089.1.17",
          "form": "조용하기도 하구요",
          "original_form": "조용하기도 하구요",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020089.1.18",
          "form": "힐링하고 오는거죠ㅎㅎ뭔가 절이 더 기도드리기에 부담이 없어서 좋아요",
          "original_form": "힐링하고 오는거죠ㅎㅎ뭔가 절이 더 기도드리기에 부담이 없어서 좋아요",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020089.1.19",
          "form": "교회는 뭔가....해야 할 말이 많은것같은 느낌...",
          "original_form": "교회는 뭔가....해야 할 말이 많은것같은 느낌...",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020089.1.20",
          "form": "등사갔다가 가벼운 마음으로 기도한번 드리고 온적 저도 있어요",
          "original_form": "등사갔다가 가벼운 마음으로 기도한번 드리고 온적 저도 있어요",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020089.1.21",
          "form": "그쵸 그정도가 저한텐 종교생활...?ㅋㅋㅋㅋ",
          "original_form": "그쵸 그정도가 저한텐 종교생활...?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020089.1.22",
          "form": "저두요ㅋㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:47"
        }
      ]
