      "utterance": [
        {
          "id": "MDRW2100002171.1.1",
          "form": "여행 계획 있어?",
          "original_form": "여행 계획 있어?",
          "speaker_id": "2",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002171.1.2",
          "form": "코로나끝나면 해외여행가고싶어",
          "original_form": "코로나끝나면 해외여행가고싶어",
          "speaker_id": "1",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002171.1.3",
          "form": "너는",
          "original_form": "너는",
          "speaker_id": "1",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002171.1.4",
          "form": "나두... 주변에 결혼하는칭구들 신행준비하는거 들어보니까",
          "original_form": "나두... 주변에 결혼하는칭구들 신행준비하는거 들어보니까",
          "speaker_id": "2",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002171.1.5",
          "form": "백신 다맞으면 갈수는 있다고는 하더라",
          "original_form": "백신 다맞으면 갈수는 있다고는 하더라",
          "speaker_id": "2",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002171.1.6",
          "form": "외국 가고싶다 ㅠㅠ",
          "original_form": "외국 가고싶다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002171.1.7",
          "form": "나도 백신 사전예약해서 빨리맞았는데 여행가고싶어서 맞은게 큼",
          "original_form": "나도 백신 사전예약해서 빨리맞았는데 여행가고싶어서 맞은게 큼",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002171.1.8",
          "form": "외국어디가고싶은데",
          "original_form": "외국어디가고싶은데",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002171.1.9",
          "form": "동남아부터 시작해야하지않을까?",
          "original_form": "동남아부터 시작해야하지않을까?",
          "speaker_id": "2",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002171.1.10",
          "form": "서양은 인종차별 무서워 좀 ㅠㅠ",
          "original_form": "서양은 인종차별 무서워 좀 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002171.1.11",
          "form": "뭐 무서운건 다무서울듯 동남아도 막 사고들터지는건있으니까",
          "original_form": "뭐 무서운건 다무서울듯 동남아도 막 사고들터지는건있으니까",
          "speaker_id": "1",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002171.1.12",
          "form": "그럼 여행가본적은 없어?",
          "original_form": "그럼 여행가본적은 없어?",
          "speaker_id": "1",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002171.1.13",
          "form": "나 코로나 직전에 뉴욕다녀왔어",
          "original_form": "나 코로나 직전에 뉴욕다녀왔어",
          "speaker_id": "2",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002171.1.14",
          "form": "19년 10월에 ㅋㅋㅋ 진짜 잘했지",
          "original_form": "19년 10월에 ㅋㅋㅋ 진짜 잘했지",
          "speaker_id": "2",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002171.1.15",
          "form": "오 나도 코로나직전까지 여행다니긴했는데",
          "original_form": "오 나도 코로나직전까지 여행다니긴했는데",
          "speaker_id": "1",
          "time": "20210820 00:26"
        },
        {
          "id": "MDRW2100002171.1.16",
          "form": "뉴욕가보고싶었는데 뉴욕어때",
          "original_form": "뉴욕가보고싶었는데 뉴욕어때",
          "speaker_id": "1",
          "time": "20210820 00:26"
        },
        {
          "id": "MDRW2100002171.1.17",
          "form": "뉴욕 진짜 좋았지...",
          "original_form": "뉴욕 진짜 좋았지...",
          "speaker_id": "2",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002171.1.18",
          "form": "가게 하나하나 다 이쁘고",
          "original_form": "가게 하나하나 다 이쁘고",
          "speaker_id": "2",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002171.1.19",
          "form": "너가 걱정하는 인종차별같은거는???거기서 당해본거야?",
          "original_form": "너가 걱정하는 인종차별같은거는???거기서 당해본거야?",
          "speaker_id": "1",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002171.1.20",
          "form": "아니 미국 사는 친구 몇명 있는데 당분간은 안오는게 좋을거같대",
          "original_form": "아니 미국 사는 친구 몇명 있는데 당분간은 안오는게 좋을거같대",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002171.1.21",
          "form": "나는 영어가 그렇게 잘되는건 아니니깐 ㅠ",
          "original_form": "나는 영어가 그렇게 잘되는건 아니니깐 ㅠ",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002171.1.22",
          "form": "아 코로나터지고나서 상황얘기하는거구나",
          "original_form": "아 코로나터지고나서 상황얘기하는거구나",
          "speaker_id": "1",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002171.1.23",
          "form": "맞아 억울한상황에서 말도못하는데 그럴거같긴해",
          "original_form": "맞아 억울한상황에서 말도못하는데 그럴거같긴해",
          "speaker_id": "1",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002171.1.24",
          "form": "뉴욕은 연말에 진짜 가보고싶었는데",
          "original_form": "뉴욕은 연말에 진짜 가보고싶었는데",
          "speaker_id": "1",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002171.1.25",
          "form": "완전반짝반짝하고 사람많고 그렇잖아",
          "original_form": "완전반짝반짝하고 사람많고 그렇잖아",
          "speaker_id": "1",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002171.1.26",
          "form": "웅 사람이 많은 관광지에서 오는 그 묘한 해방감이 있더라",
          "original_form": "웅 사람이 많은 관광지에서 오는 그 묘한 해방감이 있더라",
          "speaker_id": "2",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002171.1.27",
          "form": "일본 시부야에 스크램블교차로랑 타임스퀘어랑 그런맥락에서 비슷했어",
          "original_form": "일본 시부야에 스크램블교차로랑 타임스퀘어랑 그런맥락에서 비슷했어",
          "speaker_id": "2",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002171.1.28",
          "form": "완전 대도시 여행지는 안가봐서 궁금하더라고",
          "original_form": "완전 대도시 여행지는 안가봐서 궁금하더라고",
          "speaker_id": "1",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002171.1.29",
          "form": "그렇구나 그럼 너는 어디가 제일 좋았어?",
          "original_form": "그렇구나 그럼 너는 어디가 제일 좋았어?",
          "speaker_id": "2",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002171.1.30",
          "form": "난 휴양지를 좋아하긴하는데 젤 생각나는건 우유니랑 마추픽추",
          "original_form": "난 휴양지를 좋아하긴하는데 젤 생각나는건 우유니랑 마추픽추",
          "speaker_id": "1",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002171.1.31",
          "form": "남미가 젤 인상깊어 대자연 ㅋㅋㅋㅋ",
          "original_form": "남미가 젤 인상깊어 대자연 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002171.1.32",
          "form": "우와 대박 진짜 진심 대박이다",
          "original_form": "우와 대박 진짜 진심 대박이다",
          "speaker_id": "2",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002171.1.33",
          "form": "남미 무섭지는 않아? 치안같은거",
          "original_form": "남미 무섭지는 않아? 치안같은거",
          "speaker_id": "2",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002171.1.34",
          "form": "나도 둘다 진짜 인생 버킷리스트다 한번은 가보고싶어",
          "original_form": "나도 둘다 진짜 인생 버킷리스트다 한번은 가보고싶어",
          "speaker_id": "2",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002171.1.35",
          "form": "무서웠는데 위험한곳안가고 밤에안돌아다니면 갈만한것같아",
          "original_form": "무서웠는데 위험한곳안가고 밤에안돌아다니면 갈만한것같아",
          "speaker_id": "1",
          "time": "20210820 00:38"
        },
        {
          "id": "MDRW2100002171.1.36",
          "form": "근데 확실히 치안좋은도시보다는 신경곤두서게됨",
          "original_form": "근데 확실히 치안좋은도시보다는 신경곤두서게됨",
          "speaker_id": "1",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002171.1.37",
          "form": "남미가기전에 캐나다랑 미국먼저 들렸다가서 비교되긴하더라 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "남미가기전에 캐나다랑 미국먼저 들렸다가서 비교되긴하더라 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:39"
        }
      ]
