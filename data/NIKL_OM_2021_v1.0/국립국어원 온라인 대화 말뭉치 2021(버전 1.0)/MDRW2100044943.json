      "utterance": [
        {
          "id": "MDRW2100044943.1.1",
          "form": "나오늘다이소들렀당",
          "original_form": "나오늘다이소들렀당",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044943.1.2",
          "form": "오오 다이소에서 뭐 샀어?",
          "original_form": "오오 다이소에서 뭐 샀어?",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044943.1.3",
          "form": "오늘 물고기 어항 수질 관리하는거랑 입욕제",
          "original_form": "오늘 물고기 어항 수질 관리하는거랑 입욕제",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044943.1.4",
          "form": "샀엉",
          "original_form": "샀엉",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044943.1.5",
          "form": "오 물고기 키워?",
          "original_form": "오 물고기 키워?",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044943.1.6",
          "form": "응 물고기가 자꾸 죽어서 하나 사봤어 ㅠㅠ",
          "original_form": "응 물고기가 자꾸 죽어서 하나 사봤어 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044943.1.7",
          "form": "2촌원밖에안해가지구",
          "original_form": "2촌원밖에안해가지구",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044943.1.8",
          "form": "물 자주 갈아줬어? 많이 갈아주면 안좋아ㅠ",
          "original_form": "물 자주 갈아줬어? 많이 갈아주면 안좋아ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044943.1.9",
          "form": "나 갈고나서 이번에 너무죽엇길래 2일뒤에 다시 갈아주긴햇는데",
          "original_form": "나 갈고나서 이번에 너무죽엇길래 2일뒤에 다시 갈아주긴햇는데",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044943.1.10",
          "form": "새 물 안넣었는데도 죽어서 ㅜㅜ",
          "original_form": "새 물 안넣었는데도 죽어서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044943.1.11",
          "form": "그래? 혹시 히터 있어? 요즘 추워져서 자꾸 죽는걸 수도 있어",
          "original_form": "그래? 혹시 히터 있어? 요즘 추워져서 자꾸 죽는걸 수도 있어",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044943.1.12",
          "form": "혹시 모르니까 히터 사서 넣어줘봐",
          "original_form": "혹시 모르니까 히터 사서 넣어줘봐",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044943.1.13",
          "form": "요새 추워서 그런가.. 그래야겟가 ㅜㅜ",
          "original_form": "요새 추워서 그런가.. 그래야겟가 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044943.1.14",
          "form": "그랭 근데 입욕제 샀다 했잖아 어떤 입욕제 샀어?",
          "original_form": "그랭 근데 입욕제 샀다 했잖아 어떤 입욕제 샀어?",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044943.1.15",
          "form": "나 그 동그라미모양 입욕제!!",
          "original_form": "나 그 동그라미모양 입욕제!!",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044943.1.16",
          "form": "아 가루 뭉쳐서 만든 거? 얼마에 샀어?",
          "original_form": "아 가루 뭉쳐서 만든 거? 얼마에 샀어?",
          "speaker_id": "2",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044943.1.17",
          "form": "응응 3천원밖에 안하더랑 한번 욕조에 써보려고 사봤어",
          "original_form": "응응 3천원밖에 안하더랑 한번 욕조에 써보려고 사봤어",
          "speaker_id": "1",
          "time": "20210908 00:23"
        }
