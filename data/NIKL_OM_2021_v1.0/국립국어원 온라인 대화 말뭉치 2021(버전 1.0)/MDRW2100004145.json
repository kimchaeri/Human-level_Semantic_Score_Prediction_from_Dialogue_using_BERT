      "utterance": [
        {
          "id": "MDRW2100004145.1.1",
          "form": "안녕하세요~ 이번 여자배구 보셨나요",
          "original_form": "안녕하세요~ 이번 여자배구 보셨나요",
          "speaker_id": "1",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004145.1.2",
          "form": "아 정말 재밌게 봤어요!",
          "original_form": "아 정말 재밌게 봤어요!",
          "speaker_id": "2",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004145.1.3",
          "form": "이번에 여자배구 처음보는 거였는데 정말 재밌더라고요",
          "original_form": "이번에 여자배구 처음보는 거였는데 정말 재밌더라고요",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004145.1.4",
          "form": "저두요!!",
          "original_form": "저두요!!",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004145.1.5",
          "form": "저도 제대로 본적없었는데",
          "original_form": "저도 제대로 본적없었는데",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004145.1.6",
          "form": "너무 재밋엇어요",
          "original_form": "너무 재밋엇어요",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004145.1.7",
          "form": "이번 기회에 관심가져보려구요",
          "original_form": "이번 기회에 관심가져보려구요",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004145.1.8",
          "form": "배구는 중학교 체육 시간에 배운 게 다인데",
          "original_form": "배구는 중학교 체육 시간에 배운 게 다인데",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004145.1.9",
          "form": "이번 올림픽 보면서",
          "original_form": "이번 올림픽 보면서",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004145.1.10",
          "form": "더 알고 싶어서 이것저것 용어도 찾아보고 그랬어요",
          "original_form": "더 알고 싶어서 이것저것 용어도 찾아보고 그랬어요",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004145.1.11",
          "form": "헉 맞아요",
          "original_form": "헉 맞아요",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004145.1.12",
          "form": "관심이 생기니까 용어도 찾아보고",
          "original_form": "관심이 생기니까 용어도 찾아보고",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004145.1.13",
          "form": "경기 규칙같은것도 찾아보고",
          "original_form": "경기 규칙같은것도 찾아보고",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004145.1.14",
          "form": "재밋더라구욬ㅋㅋ",
          "original_form": "재밋더라구욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004145.1.15",
          "form": "그리고 이번에 새삼",
          "original_form": "그리고 이번에 새삼",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004145.1.16",
          "form": "김연경선수가 얼마나 대단한 선수인지",
          "original_form": "김연경선수가 얼마나 대단한 선수인지",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004145.1.17",
          "form": "다시 깨달앗어욬ㅋㅋ",
          "original_form": "다시 깨달앗어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004145.1.18",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004145.1.19",
          "form": "올라운더가 정말 대단하더라고요",
          "original_form": "올라운더가 정말 대단하더라고요",
          "speaker_id": "2",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004145.1.20",
          "form": "여자배구 관심없을때는",
          "original_form": "여자배구 관심없을때는",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004145.1.21",
          "form": "어느 위치에서도 너무 멋있게 해내셔서",
          "original_form": "어느 위치에서도 너무 멋있게 해내셔서",
          "speaker_id": "2",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004145.1.22",
          "form": "그냥 아 잘하는 선수래~ 햇는데",
          "original_form": "그냥 아 잘하는 선수래~ 햇는데",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004145.1.23",
          "form": "경기영상 보면 정말 대단한..",
          "original_form": "경기영상 보면 정말 대단한..",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004145.1.24",
          "form": "주장으로서도 너무 멋있었고",
          "original_form": "주장으로서도 너무 멋있었고",
          "speaker_id": "2",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004145.1.25",
          "form": "스파이크가 그렇게 시원하더라고요",
          "original_form": "스파이크가 그렇게 시원하더라고요",
          "speaker_id": "2",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004145.1.26",
          "form": "맞아요 실력도 실력인데",
          "original_form": "맞아요 실력도 실력인데",
          "speaker_id": "1",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004145.1.27",
          "form": "리더십 이런게 빛나는거 같아요",
          "original_form": "리더십 이런게 빛나는거 같아요",
          "speaker_id": "1",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004145.1.28",
          "form": "한일전 보셨나요",
          "original_form": "한일전 보셨나요",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004145.1.29",
          "form": "5세트 보면서 심장 터지는 줄 알았어요",
          "original_form": "5세트 보면서 심장 터지는 줄 알았어요",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004145.1.30",
          "form": "그니까요!!!ㅋㅋㅋㅋ",
          "original_form": "그니까요!!!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004145.1.31",
          "form": "저는 라이브로는 못봤고 떨려서",
          "original_form": "저는 라이브로는 못봤고 떨려서",
          "speaker_id": "1",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004145.1.32",
          "form": "나중에 하이라이트로 보는데",
          "original_form": "나중에 하이라이트로 보는데",
          "speaker_id": "1",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004145.1.33",
          "form": "결과를 아는데도 쫄리더라고요",
          "original_form": "결과를 아는데도 쫄리더라고요",
          "speaker_id": "1",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004145.1.34",
          "form": "제가 스포츠를 좋아하는지 몰랐는데 이번 올림픽 보면서 좋은 취미가 될 수 있을 거 같았어요",
          "original_form": "제가 스포츠를 좋아하는지 몰랐는데 이번 올림픽 보면서 좋은 취미가 될 수 있을 거 같았어요",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004145.1.35",
          "form": "혹시 양궁도 보셨나요",
          "original_form": "혹시 양궁도 보셨나요",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004145.1.36",
          "form": "아 여자선수 경기만 봤어요ㅋㅋ",
          "original_form": "아 여자선수 경기만 봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004145.1.37",
          "form": "혼성이랑",
          "original_form": "혼성이랑",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004145.1.38",
          "form": "안산선수 너무 멋잇어요ㅠ",
          "original_form": "안산선수 너무 멋잇어요ㅠ",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004145.1.39",
          "form": "저도 남자 선수 경기는 못 보고 여자 선수들 것만 봤는데",
          "original_form": "저도 남자 선수 경기는 못 보고 여자 선수들 것만 봤는데",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004145.1.40",
          "form": "진짜 보이지도 않는데 잘 쏘더라고요",
          "original_form": "진짜 보이지도 않는데 잘 쏘더라고요",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004145.1.41",
          "form": "안산 선수 심박수 보고 정말 기절할 뻔했어요",
          "original_form": "안산 선수 심박수 보고 정말 기절할 뻔했어요",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004145.1.42",
          "form": "분명 경기 보는 제가 더 높았을 거예요ㅋㅋㅋ",
          "original_form": "분명 경기 보는 제가 더 높았을 거예요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004145.1.43",
          "form": "맞아욬ㅋㅋㄱㅋ",
          "original_form": "맞아욬ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004145.1.44",
          "form": "보는 사람이 더 떨리는 경기",
          "original_form": "보는 사람이 더 떨리는 경기",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004145.1.45",
          "form": "정말 산처럼 굳건한 사람ㅠ",
          "original_form": "정말 산처럼 굳건한 사람ㅠ",
          "speaker_id": "1",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004145.1.46",
          "form": "다음 올림픽에서도 보고 싶어요",
          "original_form": "다음 올림픽에서도 보고 싶어요",
          "speaker_id": "2",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004145.1.47",
          "form": "금메달 3개 걸고 있는데 너무 멋있었어요",
          "original_form": "금메달 3개 걸고 있는데 너무 멋있었어요",
          "speaker_id": "2",
