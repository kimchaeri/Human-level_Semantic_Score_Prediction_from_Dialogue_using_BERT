      "utterance": [
        {
          "id": "MDRW2100023690.1.1",
          "form": "그거공연보는데 새가 똥싸고감 어깨에",
          "original_form": "그거공연보는데 새가 똥싸고감 어깨에",
          "speaker_id": "1",
          "time": "20210830 14:45"
        },
        {
          "id": "MDRW2100023690.1.2",
          "form": "솔직히 양 많으면 가산점 줘야하는거 아닙니까?",
          "original_form": "솔직히 양 많으면 가산점 줘야하는거 아닙니까?",
          "speaker_id": "2",
          "time": "20210830 14:45"
        },
        {
          "id": "MDRW2100023690.1.3",
          "form": "새똥ㅋㅋㅋㅋ",
          "original_form": "새똥ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:45"
        },
        {
          "id": "MDRW2100023690.1.4",
          "form": "진짜 인정,,길게하면 뭔가억울",
          "original_form": "진짜 인정,,길게하면 뭔가억울",
          "speaker_id": "1",
          "time": "20210830 14:45"
        },
        {
          "id": "MDRW2100023690.1.5",
          "form": "사진안가서 진짜 아쉽다 ㅋㅋㅋㅋㅋ",
          "original_form": "사진안가서 진짜 아쉽다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100023690.1.6",
          "form": "근데 저 8월 휴가에 강보러가서 새똥맞음.. 왼쪽어깨에",
          "original_form": "근데 저 8월 휴가에 강보러가서 새똥맞음.. 왼쪽어깨에",
          "speaker_id": "2",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100023690.1.7",
          "form": "친구가 군무원인데 계룡대에서 일하거든요",
          "original_form": "친구가 군무원인데 계룡대에서 일하거든요",
          "speaker_id": "1",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100023690.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100023690.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 같이 똥맞앗네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 같이 똥맞앗네요",
          "speaker_id": "1",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100023690.1.10",
          "form": "제 친구도 군무원있어요",
          "original_form": "제 친구도 군무원있어요",
          "speaker_id": "2",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100023690.1.11",
          "form": "박보검 봣냐고 물어봣는데",
          "original_form": "박보검 봣냐고 물어봣는데",
          "speaker_id": "1",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100023690.1.12",
          "form": "이번에 걔 보러간건데",
          "original_form": "이번에 걔 보러간건데",
          "speaker_id": "2",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100023690.1.13",
          "form": "군인들이 진짜싫어한대요 ㅋㅋㅋ",
          "original_form": "군인들이 진짜싫어한대요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100023690.1.14",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100023690.1.15",
          "form": "하두 무리지어다녀가지고",
          "original_form": "하두 무리지어다녀가지고",
          "speaker_id": "1",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100023690.1.16",
          "form": "군인같이 해놔도 존잘이라 그런가",
          "original_form": "군인같이 해놔도 존잘이라 그런가",
          "speaker_id": "2",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100023690.1.17",
          "form": "별로라던데",
          "original_form": "별로라던데",
          "speaker_id": "1",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100023690.1.18",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "2",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100023690.1.19",
          "form": "전 이기우랑 현빈 부대에서 봤었는데",
          "original_form": "전 이기우랑 현빈 부대에서 봤었는데",
          "speaker_id": "2",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100023690.1.20",
          "form": "외숙모가 박보검가족이랑 친해서",
          "original_form": "외숙모가 박보검가족이랑 친해서",
          "speaker_id": "1",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100023690.1.21",
          "form": "싸인받아달라고햇는데 ㅋㅋㅋㅋ어캐됏을랑가",
          "original_form": "싸인받아달라고햇는데 ㅋㅋㅋㅋ어캐됏을랑가",
          "speaker_id": "1",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100023690.1.22",
          "form": "어쩔수없나봐요 이기우는 ㄹㅇ재수읍었음",
          "original_form": "어쩔수없나봐요 이기우는 ㄹㅇ재수읍었음",
          "speaker_id": "2",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100023690.1.23",
          "form": "이기우가 누구에요?",
          "original_form": "이기우가 누구에요?",
          "speaker_id": "1",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100023690.1.24",
          "form": "거 있어요 배우인데 인지도 좀 낮은 키큰 아저씨",
          "original_form": "거 있어요 배우인데 인지도 좀 낮은 키큰 아저씨",
          "speaker_id": "2",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100023690.1.25",
          "form": "저도 잘 모름",
          "original_form": "저도 잘 모름",
          "speaker_id": "2",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100023690.1.26",
          "form": "아 얼굴보니까 알겟네",
          "original_form": "아 얼굴보니까 알겟네",
          "speaker_id": "1",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100023690.1.27",
          "form": "네네 얼굴봐야 아? 하는",
          "original_form": "네네 얼굴봐야 아? 하는",
          "speaker_id": "2",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100023690.1.28",
          "form": "현빈은진짜 미쳣엇겟다",
          "original_form": "현빈은진짜 미쳣엇겟다",
          "speaker_id": "1",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100023690.1.29",
          "form": "재밌는 썰 없어요? 군대",
          "original_form": "재밌는 썰 없어요? 군대",
          "speaker_id": "1",
          "time": "20210830 14:49"
        }
      ]
