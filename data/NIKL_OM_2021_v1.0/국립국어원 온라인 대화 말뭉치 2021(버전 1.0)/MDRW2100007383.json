      "utterance": [
        {
          "id": "MDRW2100007383.1.1",
          "form": "저는 오늘 마라샹궈 먹엇어요",
          "original_form": "저는 오늘 마라샹궈 먹엇어요",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007383.1.2",
          "form": "동고에 불날까요?",
          "original_form": "동고에 불날까요?",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007383.1.3",
          "form": "어어어어 저도 오늘 마라샹궈 먹었는데",
          "original_form": "어어어어 저도 오늘 마라샹궈 먹었는데",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007383.1.4",
          "form": "내일 뭐 먹으면 좋을까요",
          "original_form": "내일 뭐 먹으면 좋을까요",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007383.1.5",
          "form": "내일은",
          "original_form": "내일은",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007383.1.6",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007383.1.7",
          "form": "김밥어때요?",
          "original_form": "김밥어때요?",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007383.1.8",
          "form": "오 어떤 김밥 좋아해요?",
          "original_form": "오 어떤 김밥 좋아해요?",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007383.1.9",
          "form": "참치마요김밥이요",
          "original_form": "참치마요김밥이요",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007383.1.10",
          "form": "일단 드시면 안되는건",
          "original_form": "일단 드시면 안되는건",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007383.1.11",
          "form": "미니스탑에",
          "original_form": "미니스탑에",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007383.1.12",
          "form": "전복김밥",
          "original_form": "전복김밥",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007383.1.13",
          "form": "헐 그거 맛없나요?",
          "original_form": "헐 그거 맛없나요?",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007383.1.14",
          "form": "비려요",
          "original_form": "비려요",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007383.1.15",
          "form": "이름은 진짜... 세계최고 짱인데",
          "original_form": "이름은 진짜... 세계최고 짱인데",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007383.1.16",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007383.1.17",
          "form": "그 제주도에",
          "original_form": "그 제주도에",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007383.1.18",
          "form": "그런 맛 상상햇다가",
          "original_form": "그런 맛 상상햇다가",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007383.1.19",
          "form": "그냥 제주 앞바다향만 먹고 왓어요",
          "original_form": "그냥 제주 앞바다향만 먹고 왓어요",
          "speaker_id": "2",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007383.1.20",
          "form": "전복 살이 들어있어요?",
          "original_form": "전복 살이 들어있어요?",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007383.1.21",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007383.1.22",
          "form": "밥이 전복물 뭍어잇을걸요",
          "original_form": "밥이 전복물 뭍어잇을걸요",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007383.1.23",
          "form": "랍스터칩에 랍스터만큼 들어가잇어요 전복이",
          "original_form": "랍스터칩에 랍스터만큼 들어가잇어요 전복이",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007383.1.24",
          "form": "아 그건 좀 최악이다",
          "original_form": "아 그건 좀 최악이다",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007383.1.25",
          "form": "저",
          "original_form": "저",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007383.1.26",
          "form": "지금 맥주를 마시고잇는데",
          "original_form": "지금 맥주를 마시고잇는데",
          "speaker_id": "2",
          "time": "20210822 02:07"
        }
      ]
    }
