      "utterance": [
        {
          "id": "MDRW2100033805.1.1",
          "form": "아르바이트 해보신적 있으신가요?",
          "original_form": "아르바이트 해보신적 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 03:25"
        },
        {
          "id": "MDRW2100033805.1.2",
          "form": "네 피시방알바해봤어요",
          "original_form": "네 피시방알바해봤어요",
          "speaker_id": "2",
          "time": "20210825 03:25"
        },
        {
          "id": "MDRW2100033805.1.3",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210825 03:25"
        },
        {
          "id": "MDRW2100033805.1.4",
          "form": "사무보조 알바해봤어요",
          "original_form": "사무보조 알바해봤어요",
          "speaker_id": "1",
          "time": "20210825 03:26"
        },
        {
          "id": "MDRW2100033805.1.5",
          "form": "피시방 알바 어때요?",
          "original_form": "피시방 알바 어때요?",
          "speaker_id": "1",
          "time": "20210825 03:26"
        },
        {
          "id": "MDRW2100033805.1.6",
          "form": "힘들어요ㅎ",
          "original_form": "힘들어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 03:26"
        },
        {
          "id": "MDRW2100033805.1.7",
          "form": "피시방인데 피시방이 아닌기분",
          "original_form": "피시방인데 피시방이 아닌기분",
          "speaker_id": "2",
          "time": "20210825 03:26"
        },
        {
          "id": "MDRW2100033805.1.8",
          "form": "요즘 요리도 해야되던데",
          "original_form": "요즘 요리도 해야되던데",
          "speaker_id": "1",
          "time": "20210825 03:26"
        },
        {
          "id": "MDRW2100033805.1.9",
          "form": "맞나요?",
          "original_form": "맞나요?",
          "speaker_id": "1",
          "time": "20210825 03:26"
        },
        {
          "id": "MDRW2100033805.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 03:26"
        },
        {
          "id": "MDRW2100033805.1.11",
          "form": "음식점인줄",
          "original_form": "음식점인줄",
          "speaker_id": "2",
          "time": "20210825 03:26"
        },
        {
          "id": "MDRW2100033805.1.12",
          "form": "으..",
          "original_form": "으..",
          "speaker_id": "1",
          "time": "20210825 03:26"
        },
        {
          "id": "MDRW2100033805.1.13",
          "form": "사무 보조 알바는 어때요",
          "original_form": "사무 보조 알바는 어때요",
          "speaker_id": "2",
          "time": "20210825 03:27"
        },
        {
          "id": "MDRW2100033805.1.14",
          "form": "단순 업무여서 편했어요",
          "original_form": "단순 업무여서 편했어요",
          "speaker_id": "1",
          "time": "20210825 03:27"
        },
        {
          "id": "MDRW2100033805.1.15",
          "form": "주로 어떤 일을 했나요",
          "original_form": "주로 어떤 일을 했나요",
          "speaker_id": "2",
          "time": "20210825 03:27"
        },
        {
          "id": "MDRW2100033805.1.16",
          "form": "직장 다니세요?",
          "original_form": "직장 다니세요?",
          "speaker_id": "1",
          "time": "20210825 03:27"
        },
        {
          "id": "MDRW2100033805.1.17",
          "form": "엑셀 작업했어요",
          "original_form": "엑셀 작업했어요",
          "speaker_id": "1",
          "time": "20210825 03:27"
        },
        {
          "id": "MDRW2100033805.1.18",
          "form": "지금은 다니고 있지않아요",
          "original_form": "지금은 다니고 있지않아요",
          "speaker_id": "2",
          "time": "20210825 03:27"
        },
        {
          "id": "MDRW2100033805.1.19",
          "form": "저도 백수예요",
          "original_form": "저도 백수예요",
          "speaker_id": "1",
          "time": "20210825 03:27"
        }
