      "utterance": [
        {
          "id": "MDRW2100043869.1.1",
          "form": "전 일일드라마도 봐요ㅋ",
          "original_form": "전 일일드라마도 봐요ㅋ",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043869.1.2",
          "form": "일일드라마 뭐 보시나요? 안본지 오래됐네요ㅜ",
          "original_form": "일일드라마 뭐 보시나요? 안본지 오래됐네요ㅜ",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100043869.1.3",
          "form": "저도 그러다 우연히 속아도 꿈결봣는데",
          "original_form": "저도 그러다 우연히 속아도 꿈결봣는데",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043869.1.4",
          "form": "한번보니 계속보게 되요",
          "original_form": "한번보니 계속보게 되요",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043869.1.5",
          "form": "일일드라마가 중독성이 있죠~",
          "original_form": "일일드라마가 중독성이 있죠~",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100043869.1.6",
          "form": "네 중독성잇어요ㅋㅋ 욕하면서 보게되는ㄲ",
          "original_form": "네 중독성잇어요ㅋㅋ 욕하면서 보게되는ㄲ",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100043869.1.7",
          "form": "그렇죠~~끊을수가 없는...",
          "original_form": "그렇죠~~끊을수가 없는...",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100043869.1.8",
          "form": "결혼작사이혼작곡이 생각나네요~ㅎㅎ",
          "original_form": "결혼작사이혼작곡이 생각나네요~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100043869.1.9",
          "form": "그것도 막장이엇져 진짜ㅋㅋ",
          "original_form": "그것도 막장이엇져 진짜ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100043869.1.10",
          "form": "네 마지막 엔딩이 충격적이었어요",
          "original_form": "네 마지막 엔딩이 충격적이었어요",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100043869.1.11",
          "form": "팬트하우스도 이번주 끝이라던데 그것도 충격요",
          "original_form": "팬트하우스도 이번주 끝이라던데 그것도 충격요",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100043869.1.12",
          "form": "저는 펜트하우스 무서워서 못 보겠더라구요ㅜ",
          "original_form": "저는 펜트하우스 무서워서 못 보겠더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100043869.1.13",
          "form": "저도 무서워서 끈엇는데 지난주 오랫만에 봣어여",
          "original_form": "저도 무서워서 끈엇는데 지난주 오랫만에 봣어여",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100043869.1.14",
          "form": "여전히 무서워서 저도모르게 욕이 나왓어여ㅋㅋ",
          "original_form": "여전히 무서워서 저도모르게 욕이 나왓어여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100043869.1.15",
          "form": "왜 그렇게 엽기적일까요?",
          "original_form": "왜 그렇게 엽기적일까요?",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100043869.1.16",
          "form": "그러게말이에요 자극적이라 사람들이 좋아하나봐요",
          "original_form": "그러게말이에요 자극적이라 사람들이 좋아하나봐요",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100043869.1.17",
          "form": "정신건강에 안 좋을듯요ㅜ",
          "original_form": "정신건강에 안 좋을듯요ㅜ",
          "speaker_id": "1",
          "time": "20210907 22:45"
        }
