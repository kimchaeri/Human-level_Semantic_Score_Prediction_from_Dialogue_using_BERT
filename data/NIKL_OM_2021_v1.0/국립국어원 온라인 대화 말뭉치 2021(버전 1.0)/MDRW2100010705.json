      "utterance": [
        {
          "id": "MDRW2100010705.1.1",
          "form": "야끼소바 하니까 일본가고싶어요,,,",
          "original_form": "야끼소바 하니까 일본가고싶어요,,,",
          "speaker_id": "2",
          "time": "20210823 20:52"
        },
        {
          "id": "MDRW2100010705.1.2",
          "form": "ㅋㅋㅋ그쵸ㅜㅠ",
          "original_form": "ㅋㅋㅋ그쵸ㅜㅠ",
          "speaker_id": "1",
          "time": "20210823 20:52"
        },
        {
          "id": "MDRW2100010705.1.3",
          "form": "사실 일본이랑 관계가 안좋아지긴했지만",
          "original_form": "사실 일본이랑 관계가 안좋아지긴했지만",
          "speaker_id": "1",
          "time": "20210823 20:52"
        },
        {
          "id": "MDRW2100010705.1.4",
          "form": "자주 놀러갔었는데..먹으러",
          "original_form": "자주 놀러갔었는데..먹으러",
          "speaker_id": "2",
          "time": "20210823 20:52"
        },
        {
          "id": "MDRW2100010705.1.5",
          "form": "일본만큼 해외여행가기 좋은곳도 없죠 사실",
          "original_form": "일본만큼 해외여행가기 좋은곳도 없죠 사실",
          "speaker_id": "1",
          "time": "20210823 20:52"
        },
        {
          "id": "MDRW2100010705.1.6",
          "form": "ㅋㄱㅋㄱㅋ맞아요",
          "original_form": "ㅋㄱㅋㄱㅋ맞아요",
          "speaker_id": "1",
          "time": "20210823 20:52"
        },
        {
          "id": "MDRW2100010705.1.7",
          "form": "맞아요 싸고 가까운 선진국이라",
          "original_form": "맞아요 싸고 가까운 선진국이라",
          "speaker_id": "2",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010705.1.8",
          "form": "여행하기 편하고ㅎㅎ...",
          "original_form": "여행하기 편하고ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010705.1.9",
          "form": "맞죠맞죠ㅎㅎ요즘 번역기도 잘되어있고 구글맵도 잘되고요ㅎㅎ",
          "original_form": "맞죠맞죠ㅎㅎ요즘 번역기도 잘되어있고 구글맵도 잘되고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010705.1.10",
          "form": "그리고 맛있는거랑 쇼핑도 하기좋고!",
          "original_form": "그리고 맛있는거랑 쇼핑도 하기좋고!",
          "speaker_id": "1",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010705.1.11",
          "form": "포켓이나 유심 갖고 구글맵 있으면",
          "original_form": "포켓이나 유심 갖고 구글맵 있으면",
          "speaker_id": "2",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010705.1.12",
          "form": "요즘은 진짜 못 갈데가 없는거같아요",
          "original_form": "요즘은 진짜 못 갈데가 없는거같아요",
          "speaker_id": "2",
          "time": "20210823 20:54"
        },
        {
          "id": "MDRW2100010705.1.13",
          "form": "아~~~~여행가고싶어",
          "original_form": "아~~~~여행가고싶어",
          "speaker_id": "2",
          "time": "20210823 20:54"
        },
        {
          "id": "MDRW2100010705.1.14",
          "form": "혹시 일본 어디 가보신적 있으신가요?",
          "original_form": "혹시 일본 어디 가보신적 있으신가요?",
          "speaker_id": "2",
          "time": "20210823 20:54"
        },
        {
          "id": "MDRW2100010705.1.15",
          "form": "ㅋㅋㅋㅋ맞아요 그 두개만 있으면 어디든 문제없죠ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ맞아요 그 두개만 있으면 어디든 문제없죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:55"
        },
        {
          "id": "MDRW2100010705.1.16",
          "form": "저는 도쿄만 갔었어요!!",
          "original_form": "저는 도쿄만 갔었어요!!",
          "speaker_id": "1",
          "time": "20210823 20:55"
        },
        {
          "id": "MDRW2100010705.1.17",
          "form": "오사카나 삿포로, 오키나와도 가보고싶은데ㅠ",
          "original_form": "오사카나 삿포로, 오키나와도 가보고싶은데ㅠ",
          "speaker_id": "1",
          "time": "20210823 20:55"
        },
        {
          "id": "MDRW2100010705.1.18",
          "form": "일본 어디어디 가보셨나용?",
          "original_form": "일본 어디어디 가보셨나용?",
          "speaker_id": "1",
          "time": "20210823 20:55"
        },
        {
          "id": "MDRW2100010705.1.19",
          "form": "도쿄도 좋죠 신주쿠에서 쇼핑하기도 좋고~~",
          "original_form": "도쿄도 좋죠 신주쿠에서 쇼핑하기도 좋고~~",
          "speaker_id": "2",
          "time": "20210823 20:56"
        },
        {
          "id": "MDRW2100010705.1.20",
          "form": "일본에서 여행한적이있어서",
          "original_form": "일본에서 여행한적이있어서",
          "speaker_id": "2",
          "time": "20210823 20:56"
        },
        {
          "id": "MDRW2100010705.1.21",
          "form": "말씀해주신 곳 다 가봤어요",
          "original_form": "말씀해주신 곳 다 가봤어요",
          "speaker_id": "2",
          "time": "20210823 20:56"
        },
        {
          "id": "MDRW2100010705.1.22",
          "form": "일본같은 곳은 교토였고\r\n바다는 오키나와가 제일 좋았어요",
          "original_form": "일본같은 곳은 교토였고\r\n바다는 오키나와가 제일 좋았어요",
          "speaker_id": "2",
          "time": "20210823 20:56"
        },
        {
          "id": "MDRW2100010705.1.23",
          "form": "와 맞아용ㅎㅎ 신주쿠랑 시부야 다 다시가고싶네용ㅎㅎ",
          "original_form": "와 맞아용ㅎㅎ 신주쿠랑 시부야 다 다시가고싶네용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010705.1.24",
          "form": "오 다들 일본특유의 느낌은",
          "original_form": "오 다들 일본특유의 느낌은",
          "speaker_id": "1",
          "time": "20210823 20:58"
        },
        {
          "id": "MDRW2100010705.1.25",
          "form": "교토에서 제일 많이 느낄수있다고 그러더라구요!!",
          "original_form": "교토에서 제일 많이 느낄수있다고 그러더라구요!!",
          "speaker_id": "1",
          "time": "20210823 20:58"
        },
        {
          "id": "MDRW2100010705.1.26",
          "form": "아무래도 그렇죠 ㅋㅋ",
          "original_form": "아무래도 그렇죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:58"
        },
        {
          "id": "MDRW2100010705.1.27",
          "form": "경주에도 문화유적지 많은데",
          "original_form": "경주에도 문화유적지 많은데",
          "speaker_id": "2",
          "time": "20210823 20:58"
        },
        {
          "id": "MDRW2100010705.1.28",
          "form": "교토는 곳곳에 경주를 모아놓은 느낌!",
          "original_form": "교토는 곳곳에 경주를 모아놓은 느낌!",
          "speaker_id": "2",
          "time": "20210823 20:58"
        },
        {
          "id": "MDRW2100010705.1.29",
          "form": "아하 약간 우리나라의 경주느낌이군요!!!",
          "original_form": "아하 약간 우리나라의 경주느낌이군요!!!",
          "speaker_id": "1",
          "time": "20210823 20:59"
        },
        {
          "id": "MDRW2100010705.1.30",
          "form": "오사카 가는 김에 가기도 편하니까",
          "original_form": "오사카 가는 김에 가기도 편하니까",
          "speaker_id": "2",
          "time": "20210823 20:59"
        },
        {
          "id": "MDRW2100010705.1.31",
          "form": "바로 이해됐어요ㅎㅎ",
          "original_form": "바로 이해됐어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:59"
        },
        {
          "id": "MDRW2100010705.1.32",
          "form": "괜찮으시면 가실때 한번 같이 들러보세요",
          "original_form": "괜찮으시면 가실때 한번 같이 들러보세요",
          "speaker_id": "2",
          "time": "20210823 20:59"
        },
        {
          "id": "MDRW2100010705.1.33",
          "form": "ㅋㅋㅋㅋㅋ오 추천감사해용ㅎㅎ꼭 가볼게요!! 오사카랑 교토 묶어서 많이 가는데 그러면 훨씬 편하겠네요ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ오 추천감사해용ㅎㅎ꼭 가볼게요!! 오사카랑 교토 묶어서 많이 가는데 그러면 훨씬 편하겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:59"
        },
        {
          "id": "MDRW2100010705.1.34",
          "form": "전 사실 유니버셜스튜디오도가보고싶어서",
          "original_form": "전 사실 유니버셜스튜디오도가보고싶어서",
          "speaker_id": "1",
          "time": "20210823 21:00"
        },
        {
          "id": "MDRW2100010705.1.35",
