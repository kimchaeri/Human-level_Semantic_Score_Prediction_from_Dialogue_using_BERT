      "utterance": [
        {
          "id": "MDRW2100029216.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029216.1.2",
          "form": "안녕하세요!! 어떤게임 좋아하세요?",
          "original_form": "안녕하세요!! 어떤게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029216.1.3",
          "form": "저는 rpg게임을 좋아해요! 컴퓨터나 휴대폰 상관 없이요!",
          "original_form": "저는 rpg게임을 좋아해요! 컴퓨터나 휴대폰 상관 없이요!",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029216.1.4",
          "form": "rpg게임이.. 혹시 쿠키런킹덤도 이런 종류에 포함되는건가요?",
          "original_form": "rpg게임이.. 혹시 쿠키런킹덤도 이런 종류에 포함되는건가요?",
          "speaker_id": "2",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029216.1.5",
          "form": "네네! 쿠키런 킹덤 정말 좋아해요! 캐릭터가 너무 귀여워요!",
          "original_form": "네네! 쿠키런 킹덤 정말 좋아해요! 캐릭터가 너무 귀여워요!",
          "speaker_id": "1",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029216.1.6",
          "form": "저도 해요 쪼렙이지만ㅋㅋㅋ",
          "original_form": "저도 해요 쪼렙이지만ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029216.1.7",
          "form": "쿠킹덤최애 쿠키는 뭔가요?",
          "original_form": "쿠킹덤최애 쿠키는 뭔가요?",
          "speaker_id": "2",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029216.1.8",
          "form": "저 석류맛 쿠키요! 예전에 쿠키런 했을 때부터 좋아했어요!",
          "original_form": "저 석류맛 쿠키요! 예전에 쿠키런 했을 때부터 좋아했어요!",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029216.1.9",
          "form": "다른 게임은 하시는 거 있으신가요??",
          "original_form": "다른 게임은 하시는 거 있으신가요??",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029216.1.10",
          "form": "저..쿠키런이요ㅠㅋㅋㅋㄱ",
          "original_form": "저..쿠키런이요ㅠㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029216.1.11",
          "form": "쿠키처돌이같군요ㅜㅋㅋㅋ",
          "original_form": "쿠키처돌이같군요ㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029216.1.12",
          "form": "고전게임은 안좋아하시나요? 전 프린세스메이커같은 육성게임도 좋아해요",
          "original_form": "고전게임은 안좋아하시나요? 전 프린세스메이커같은 육성게임도 좋아해요",
          "speaker_id": "2",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029216.1.13",
          "form": "아하ㅋㅋㅋㅠ 쿠키런도 재밌죠 저 학생 때 많이 했어요ㅜ",
          "original_form": "아하ㅋㅋㅋㅠ 쿠키런도 재밌죠 저 학생 때 많이 했어요ㅜ",
          "speaker_id": "1",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029216.1.14",
          "form": "헉 고전게임은 잘 아는 게 없어서 모르겠어요ㅠ",
          "original_form": "헉 고전게임은 잘 아는 게 없어서 모르겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029216.1.15",
          "form": "캔디크러쉬도 이젠 고전게임일까요?ㅋㅋ",
          "original_form": "캔디크러쉬도 이젠 고전게임일까요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029216.1.16",
          "form": "아하 그런거라면 고전게임 많이 좋아해요ㅋㅋ 시간 떼우기 좋아서 자주 했어요",
          "original_form": "아하 그런거라면 고전게임 많이 좋아해요ㅋㅋ 시간 떼우기 좋아서 자주 했어요",
          "speaker_id": "1",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029216.1.17",
          "form": "주로 핸드폰 게임을 하시는 건가요?",
          "original_form": "주로 핸드폰 게임을 하시는 건가요?",
          "speaker_id": "1",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029216.1.18",
          "form": "네 롤이나 오버워치같은 게임은 한번도 안해봤어요",
          "original_form": "네 롤이나 오버워치같은 게임은 한번도 안해봤어요",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029216.1.19",
          "form": "싸우는 게임이 저한텐 좀 부담스러운듯해요ㅋㅋ",
          "original_form": "싸우는 게임이 저한텐 좀 부담스러운듯해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029216.1.20",
          "form": "아하 저도예요 사람들과 협동해서 하는 게임도 좀 부담스럽더라고요ㅜ",
          "original_form": "아하 저도예요 사람들과 협동해서 하는 게임도 좀 부담스럽더라고요ㅜ",
          "speaker_id": "1",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029216.1.21",
          "form": "맞아요! 제가 못하면 피해입힐것같구",
          "original_form": "맞아요! 제가 못하면 피해입힐것같구",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029216.1.22",
          "form": "저 예전에 농장키우는 게임도 열심히 했어요ㅋㅋ",
          "original_form": "저 예전에 농장키우는 게임도 열심히 했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029216.1.23",
          "form": "정말요 채팅들도 부담스럽고",
          "original_form": "정말요 채팅들도 부담스럽고",
          "speaker_id": "1",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029216.1.24",
          "form": "infp라그런가?(ㅋㅋㅋ)",
          "original_form": "infp라그런가?(ㅋㅋㅋ)",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029216.1.25",
          "form": "스타듀밸리같은???",
          "original_form": "스타듀밸리같은???",
          "speaker_id": "1",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029216.1.26",
          "form": "네! 에브리타운ㅋㅋ",
          "original_form": "네! 에브리타운ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029216.1.27",
          "form": "아! 그거 재밌었죠 제 주위 사람들도 전부 같이 했었어요",
          "original_form": "아! 그거 재밌었죠 제 주위 사람들도 전부 같이 했었어요",
          "speaker_id": "1",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029216.1.28",
          "form": "말하고나니 오랜만에 다시 해보고 싶네요",
          "original_form": "말하고나니 오랜만에 다시 해보고 싶네요",
          "speaker_id": "1",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029216.1.29",
          "form": "역시 마음 편하게 할 수 있는 게임이 최고인 거 같아요",
          "original_form": "역시 마음 편하게 할 수 있는 게임이 최고인 거 같아요",
          "speaker_id": "1",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029216.1.30",
          "form": "맞아요 경쟁하지않고 싸우지않고",
          "original_form": "맞아요 경쟁하지않고 싸우지않고",
          "speaker_id": "2",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100029216.1.31",
          "form": "근데 저희회사친구들은 싸우지않는 게임에 흥미전혀없다고하니",
          "original_form": "근데 저희회사친구들은 싸우지않는 게임에 흥미전혀없다고하니",
          "speaker_id": "2",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100029216.1.32",
          "form": "이건 정말 성향차이인듯해요",
          "original_form": "이건 정말 성향차이인듯해요",
          "speaker_id": "2",
          "time": "20210903 00:47"
        }
      ]
    }
