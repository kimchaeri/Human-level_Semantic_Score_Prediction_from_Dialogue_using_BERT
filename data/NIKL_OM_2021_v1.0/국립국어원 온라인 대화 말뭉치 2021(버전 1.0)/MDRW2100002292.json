      "utterance": [
        {
          "id": "MDRW2100002292.1.1",
          "form": "이상형이 어떻게 되세요?",
          "original_form": "이상형이 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210820 08:34"
        },
        {
          "id": "MDRW2100002292.1.2",
          "form": "음 외적인것 보긴하는데 일단 대화가 되는 사람 이게 중요한거같아요",
          "original_form": "음 외적인것 보긴하는데 일단 대화가 되는 사람 이게 중요한거같아요",
          "speaker_id": "1",
          "time": "20210820 08:36"
        },
        {
          "id": "MDRW2100002292.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210820 08:36"
        },
        {
          "id": "MDRW2100002292.1.4",
          "form": "저두요 말이 잘 통하는 사람 좋아요",
          "original_form": "저두요 말이 잘 통하는 사람 좋아요",
          "speaker_id": "2",
          "time": "20210820 08:38"
        },
        {
          "id": "MDRW2100002292.1.5",
          "form": "하지만 무엇보다 잘생긴게 좋더라구요",
          "original_form": "하지만 무엇보다 잘생긴게 좋더라구요",
          "speaker_id": "2",
          "time": "20210820 08:38"
        },
        {
          "id": "MDRW2100002292.1.6",
          "form": "그쵸 외적인 요소 되게 중요한거같아요",
          "original_form": "그쵸 외적인 요소 되게 중요한거같아요",
          "speaker_id": "1",
          "time": "20210820 08:38"
        },
        {
          "id": "MDRW2100002292.1.7",
          "form": "뭐랄까.. 가장 눈에 들어오는 부분이라 솔직히 안맞으면 아무리 잘해준들..",
          "original_form": "뭐랄까.. 가장 눈에 들어오는 부분이라 솔직히 안맞으면 아무리 잘해준들..",
          "speaker_id": "1",
          "time": "20210820 08:39"
        },
        {
          "id": "MDRW2100002292.1.8",
          "form": "또 보는 조건 있으신가요?",
          "original_form": "또 보는 조건 있으신가요?",
          "speaker_id": "1",
          "time": "20210820 08:40"
        },
        {
          "id": "MDRW2100002292.1.9",
          "form": "맞습니다 먼저 눈이 가니까",
          "original_form": "맞습니다 먼저 눈이 가니까",
          "speaker_id": "2",
          "time": "20210820 08:41"
        },
        {
          "id": "MDRW2100002292.1.10",
          "form": "어쩔 수 없어요 ㅎ",
          "original_form": "어쩔 수 없어요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 08:41"
        },
        {
          "id": "MDRW2100002292.1.11",
          "form": "이런 사람은 절대 싫다 하는 거 있으세요?",
          "original_form": "이런 사람은 절대 싫다 하는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210820 08:42"
        },
        {
          "id": "MDRW2100002292.1.12",
          "form": "이기적인 사람과 배려 안하는 사람이요",
          "original_form": "이기적인 사람과 배려 안하는 사람이요",
          "speaker_id": "2",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100002292.1.13",
          "form": "정말 최악일 거 같아요",
          "original_form": "정말 최악일 거 같아요",
          "speaker_id": "2",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100002292.1.14",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100002292.1.15",
          "form": "저도요 그런 사람 너무 싫어요",
          "original_form": "저도요 그런 사람 너무 싫어요",
          "speaker_id": "1",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100002292.1.16",
          "form": "지밖에 모르는 인간",
          "original_form": "지밖에 모르는 인간",
          "speaker_id": "1",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100002292.1.17",
          "form": "내로남불 진짜 싫어요",
          "original_form": "내로남불 진짜 싫어요",
          "speaker_id": "1",
          "time": "20210820 11:33"
        },
        {
          "id": "MDRW2100002292.1.18",
          "form": "저도요 ㅋㅋㅋㅋㅋ 다릉 사람들도 그렇겠죠?",
          "original_form": "저도요 ㅋㅋㅋㅋㅋ 다릉 사람들도 그렇겠죠?",
          "speaker_id": "2",
          "time": "20210820 11:48"
        },
        {
          "id": "MDRW2100002292.1.19",
          "form": "아무래도요? 음 결혼에 대해 생각해본적이 있나요?",
          "original_form": "아무래도요? 음 결혼에 대해 생각해본적이 있나요?",
          "speaker_id": "1",
          "time": "20210820 11:49"
        },
        {
          "id": "MDRW2100002292.1.20",
          "form": "저는 비혼이라 생각해본적이 없어요",
          "original_form": "저는 비혼이라 생각해본적이 없어요",
          "speaker_id": "1",
          "time": "20210820 11:49"
        },
        {
          "id": "MDRW2100002292.1.21",
          "form": "예전에는 결혼 하고싶었는데 이젠 없어요",
          "original_form": "예전에는 결혼 하고싶었는데 이젠 없어요",
          "speaker_id": "2",
          "time": "20210820 11:49"
        },
        {
          "id": "MDRW2100002292.1.22",
          "form": "오 저도 비혼입니다!!",
          "original_form": "오 저도 비혼입니다!!",
          "speaker_id": "2",
          "time": "20210820 11:49"
        },
        {
          "id": "MDRW2100002292.1.23",
          "form": "뭔가 그렇죠? 시간이 지날 수록 더 없어지는 거 같아요",
          "original_form": "뭔가 그렇죠? 시간이 지날 수록 더 없어지는 거 같아요",
          "speaker_id": "1",
          "time": "20210820 11:49"
        },
        {
          "id": "MDRW2100002292.1.24",
          "form": "약간 다른세계 이야기 같은..",
          "original_form": "약간 다른세계 이야기 같은..",
          "speaker_id": "1",
          "time": "20210820 11:50"
        },
        {
          "id": "MDRW2100002292.1.25",
          "form": "결혼을 하면 제가 포기 해야할 것들이 많아져서 원하지 않아요",
          "original_form": "결혼을 하면 제가 포기 해야할 것들이 많아져서 원하지 않아요",
          "speaker_id": "2",
          "time": "20210820 11:51"
        },
        {
          "id": "MDRW2100002292.1.26",
          "form": "누군가와 평생을 살 자신도 없구요",
          "original_form": "누군가와 평생을 살 자신도 없구요",
