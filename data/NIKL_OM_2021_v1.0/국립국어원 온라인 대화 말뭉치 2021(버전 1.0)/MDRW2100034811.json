      "utterance": [
        {
          "id": "MDRW2100034811.1.1",
          "form": "이게 악령이 씌이는 주제라",
          "original_form": "이게 악령이 씌이는 주제라",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100034811.1.2",
          "form": "대사를 알아듣기가 힘들때가 있는데",
          "original_form": "대사를 알아듣기가 힘들때가 있는데",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100034811.1.3",
          "form": "넷플은 자막 지원이 돼서 좋아요",
          "original_form": "넷플은 자막 지원이 돼서 좋아요",
          "speaker_id": "2",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100034811.1.4",
          "form": "오오 아 악령 ! 완전 재밌을거같아요 ㅋㅋㅋ",
          "original_form": "오오 아 악령 ! 완전 재밌을거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100034811.1.5",
          "form": "그것도 시즌제인가요?",
          "original_form": "그것도 시즌제인가요?",
          "speaker_id": "1",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100034811.1.6",
          "form": "이건 시즌제는 아니에용ㅠㅠㅠ",
          "original_form": "이건 시즌제는 아니에용ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100034811.1.7",
          "form": "2탄 나오면 넘 좋겠지만...ㅠㅠ",
          "original_form": "2탄 나오면 넘 좋겠지만...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100034811.1.8",
          "form": "저 보이스도 재밌게 봤어요",
          "original_form": "저 보이스도 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100034811.1.9",
          "form": "아 보이스 재밌어요 ㅠㅠ",
          "original_form": "아 보이스 재밌어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100034811.1.10",
          "form": "저 전 시리즈 다 봤는데",
          "original_form": "저 전 시리즈 다 봤는데",
          "speaker_id": "2",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100034811.1.11",
          "form": "뭐 보셨어요?",
          "original_form": "뭐 보셨어요?",
          "speaker_id": "2",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034811.1.12",
          "form": "저도 다 봤어요!!",
          "original_form": "저도 다 봤어요!!",
          "speaker_id": "1",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034811.1.13",
          "form": "시즌 1부터 다 챙겨봤어요 ㅋㅋㅋ",
          "original_form": "시즌 1부터 다 챙겨봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034811.1.14",
          "form": "그래도 시즌 1 장혁이",
          "original_form": "그래도 시즌 1 장혁이",
          "speaker_id": "2",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034811.1.15",
          "form": "제일 재밌던것같아요ㅋㅋㅋㅋ",
          "original_form": "제일 재밌던것같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034811.1.16",
          "form": "모태구 역할하신 김재욱 배우님도 찰떡이었어요",
          "original_form": "모태구 역할하신 김재욱 배우님도 찰떡이었어요",
          "speaker_id": "2",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034811.1.17",
          "form": "저두요 ㅋㅋㅋ 시즌 1은 못따라가요 ㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋ 시즌 1은 못따라가요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100034811.1.18",
          "form": "저 보이스보고",
          "original_form": "저 보이스보고",
          "speaker_id": "1",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100034811.1.19",
          "form": "김재욱 엄청 좋아했었어요 ㅋㅋ",
          "original_form": "김재욱 엄청 좋아했었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100034811.1.20",
          "form": "악역인데 너무 매력적이게 나왔던...",
          "original_form": "악역인데 너무 매력적이게 나왔던...",
          "speaker_id": "2",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100034811.1.21",
          "form": "대탈출 보시나요?",
          "original_form": "대탈출 보시나요?",
          "speaker_id": "2",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100034811.1.22",
          "form": "당연하죠 !",
          "original_form": "당연하죠 !",
          "speaker_id": "1",
          "time": "20210828 20:04"
        },
        {
          "id": "MDRW2100034811.1.23",
          "form": "젤 최근껀 못봤는데",
          "original_form": "젤 최근껀 못봤는데",
          "speaker_id": "1",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100034811.1.24",
          "form": "좀비편 ㅋㅋ 재밌게 봤어요",
          "original_form": "좀비편 ㅋㅋ 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100034811.1.25",
          "form": "헉ㅋㅋㅋ",
          "original_form": "헉ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100034811.1.26",
          "form": "천해명 유니버스도 넘 재밌고",
          "original_form": "천해명 유니버스도 넘 재밌고",
          "speaker_id": "2",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100034811.1.27",
          "form": "저번주꺼 대박이었어요!",
          "original_form": "저번주꺼 대박이었어요!",
          "speaker_id": "2",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100034811.1.28",
          "form": "아아 맞아요 저번주께 진짜 재밌었다던데 !!",
          "original_form": "아아 맞아요 저번주께 진짜 재밌었다던데 !!",
          "speaker_id": "1",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100034811.1.29",
          "form": "이번시즌중에 가장 재밌었어요ㅋㅋㅋㅋ 꼭 보세용 내일 본방 같이 달려요",
          "original_form": "이번시즌중에 가장 재밌었어요ㅋㅋㅋㅋ 꼭 보세용 내일 본방 같이 달려요",
          "speaker_id": "2",
          "time": "20210828 20:06"
        }
      ]
