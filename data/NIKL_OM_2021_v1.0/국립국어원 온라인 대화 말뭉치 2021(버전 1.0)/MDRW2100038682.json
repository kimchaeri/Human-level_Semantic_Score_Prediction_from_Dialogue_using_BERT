      "utterance": [
        {
          "id": "MDRW2100038682.1.1",
          "form": "안녕하세요~ 저녁 드셨나요?",
          "original_form": "안녕하세요~ 저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100038682.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100038682.1.3",
          "form": "저녁먹었어요",
          "original_form": "저녁먹었어요",
          "speaker_id": "2",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100038682.1.4",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "2",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100038682.1.5",
          "form": "네 저도 연어샐러드 먹었네요",
          "original_form": "네 저도 연어샐러드 먹었네요",
          "speaker_id": "1",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100038682.1.6",
          "form": "어떤거 드셨어요?",
          "original_form": "어떤거 드셨어요?",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100038682.1.7",
          "form": "저는 돼지 앞다리살 구워먹었어요",
          "original_form": "저는 돼지 앞다리살 구워먹었어요",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100038682.1.8",
          "form": "오 맛있었겠어요 !",
          "original_form": "오 맛있었겠어요 !",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100038682.1.9",
          "form": "점심을 쫒기듯 먹어서 든든하게 먹고싶었어요",
          "original_form": "점심을 쫒기듯 먹어서 든든하게 먹고싶었어요",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100038682.1.10",
          "form": "그러셨구나 ㅠㅠ 그쵸 한끼는 든든하게 먹어야되는 것 같아요",
          "original_form": "그러셨구나 ㅠㅠ 그쵸 한끼는 든든하게 먹어야되는 것 같아요",
          "speaker_id": "1",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100038682.1.11",
          "form": "그죠ㅠㅠ 고기나 해산물을 먹어줘야해요...",
          "original_form": "그죠ㅠㅠ 고기나 해산물을 먹어줘야해요...",
          "speaker_id": "2",
          "time": "20210902 19:16"
        },
        {
          "id": "MDRW2100038682.1.12",
          "form": "해산물 좋아하세요?",
          "original_form": "해산물 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 19:16"
        },
        {
          "id": "MDRW2100038682.1.13",
          "form": "네네! 없어서 못먹죠...",
          "original_form": "네네! 없어서 못먹죠...",
          "speaker_id": "1",
          "time": "20210902 19:18"
        },
        {
          "id": "MDRW2100038682.1.14",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210902 19:18"
        },
        {
          "id": "MDRW2100038682.1.15",
          "form": "저두요 없어서 못먹어요ㅠㅠ",
          "original_form": "저두요 없어서 못먹어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100038682.1.16",
          "form": "요즘은 소라나 새우가 먹고싶네요",
          "original_form": "요즘은 소라나 새우가 먹고싶네요",
          "speaker_id": "2",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100038682.1.17",
          "form": "아 맛있죠... ㅠㅠ 저는 요즘 게가 그렇게 먹고싶더라구요..",
          "original_form": "아 맛있죠... ㅠㅠ 저는 요즘 게가 그렇게 먹고싶더라구요..",
          "speaker_id": "1",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100038682.1.18",
          "form": "헉 금어기 풀려서 가을꽃게이제 나오기 시작하던데",
          "original_form": "헉 금어기 풀려서 가을꽃게이제 나오기 시작하던데",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100038682.1.19",
          "form": "너무 맛있겠어요",
          "original_form": "너무 맛있겠어요",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100038682.1.20",
          "form": "꽃게찜 해먹고 싶네요",
          "original_form": "꽃게찜 해먹고 싶네요",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100038682.1.21",
          "form": "그러니까요.. 살오동통한 꽃게 먹으면 ..진짜 맛있죠 ㅠㅠ",
          "original_form": "그러니까요.. 살오동통한 꽃게 먹으면 ..진짜 맛있죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100038682.1.22",
          "form": "달고 맛있죠 다른양녕도 필요없이맛있을때도 많구요",
          "original_form": "달고 맛있죠 다른양녕도 필요없이맛있을때도 많구요",
          "speaker_id": "2",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100038682.1.23",
          "form": "침나오네요ㅠㅠ",
          "original_form": "침나오네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 19:30"
        }
      ]
