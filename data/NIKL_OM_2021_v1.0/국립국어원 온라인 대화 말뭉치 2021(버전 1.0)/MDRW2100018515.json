      "utterance": [
        {
          "id": "MDRW2100018515.1.1",
          "form": "오늘은 아침부터 흐리네요",
          "original_form": "오늘은 아침부터 흐리네요",
          "speaker_id": "2",
          "time": "20210827 07:30"
        },
        {
          "id": "MDRW2100018515.1.2",
          "form": "비가 조금와요",
          "original_form": "비가 조금와요",
          "speaker_id": "1",
          "time": "20210827 07:31"
        },
        {
          "id": "MDRW2100018515.1.3",
          "form": "오늘부터 또비온데뇨",
          "original_form": "오늘부터 또비온데뇨",
          "speaker_id": "1",
          "time": "20210827 07:31"
        },
        {
          "id": "MDRW2100018515.1.4",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210827 07:31"
        },
        {
          "id": "MDRW2100018515.1.5",
          "form": "세상에.. 어제 맑아서 너무 좋았는데 말이죠ㅠ",
          "original_form": "세상에.. 어제 맑아서 너무 좋았는데 말이죠ㅠ",
          "speaker_id": "2",
          "time": "20210827 07:32"
        },
        {
          "id": "MDRW2100018515.1.6",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210827 07:32"
        },
        {
          "id": "MDRW2100018515.1.7",
          "form": "어제도 습도 높았는데 지금 엄청 습해요",
          "original_form": "어제도 습도 높았는데 지금 엄청 습해요",
          "speaker_id": "1",
          "time": "20210827 07:32"
        },
        {
          "id": "MDRW2100018515.1.8",
          "form": "요새 일부러 에어컨 켜요",
          "original_form": "요새 일부러 에어컨 켜요",
          "speaker_id": "1",
          "time": "20210827 07:32"
        },
        {
          "id": "MDRW2100018515.1.9",
          "form": "그러게요ㅜ 우산 챙기는걸 까먹어서 벌써 슬퍼요",
          "original_form": "그러게요ㅜ 우산 챙기는걸 까먹어서 벌써 슬퍼요",
          "speaker_id": "2",
          "time": "20210827 07:33"
        },
        {
          "id": "MDRW2100018515.1.10",
          "form": "저도요! 습기가 너무 심해요ㅠ",
          "original_form": "저도요! 습기가 너무 심해요ㅠ",
          "speaker_id": "2",
          "time": "20210827 07:33"
        },
        {
          "id": "MDRW2100018515.1.11",
          "form": "엥 어째요",
          "original_form": "엥 어째요",
          "speaker_id": "1",
          "time": "20210827 07:33"
        },
        {
          "id": "MDRW2100018515.1.12",
          "form": "저도 그래서 자꾸 또 우산을 사요",
          "original_form": "저도 그래서 자꾸 또 우산을 사요",
          "speaker_id": "1",
          "time": "20210827 07:33"
        },
        {
          "id": "MDRW2100018515.1.13",
          "form": "숩도 높으면 움직일때 끈적해서 힘들어요",
          "original_form": "숩도 높으면 움직일때 끈적해서 힘들어요",
          "speaker_id": "1",
          "time": "20210827 07:34"
        },
        {
          "id": "MDRW2100018515.1.14",
          "form": "이따 우산을 하나 사려해요ㅠ",
          "original_form": "이따 우산을 하나 사려해요ㅠ",
          "speaker_id": "2",
          "time": "20210827 07:34"
        },
        {
          "id": "MDRW2100018515.1.15",
          "form": "우산 꼭 사세요",
          "original_form": "우산 꼭 사세요",
          "speaker_id": "1",
          "time": "20210827 07:34"
        },
        {
          "id": "MDRW2100018515.1.16",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210827 07:34"
        },
        {
          "id": "MDRW2100018515.1.17",
          "form": "싼곳이 근처 있어야 하는데",
          "original_form": "싼곳이 근처 있어야 하는데",
          "speaker_id": "1",
          "time": "20210827 07:34"
        },
        {
          "id": "MDRW2100018515.1.18",
          "form": "넵! 비오기전에 사야겠어요",
          "original_form": "넵! 비오기전에 사야겠어요",
          "speaker_id": "2",
          "time": "20210827 07:34"
        },
        {
          "id": "MDRW2100018515.1.19",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210827 07:34"
        },
        {
          "id": "MDRW2100018515.1.20",
          "form": "빨래도 안말라서 입으면 냄새나요 ㅜㅜ",
          "original_form": "빨래도 안말라서 입으면 냄새나요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 07:35"
        },
        {
          "id": "MDRW2100018515.1.21",
          "form": "맞아요ㅠ 이때가 제일 힘든거같아요",
          "original_form": "맞아요ㅠ 이때가 제일 힘든거같아요",
          "speaker_id": "2",
          "time": "20210827 07:35"
        },
        {
          "id": "MDRW2100018515.1.22",
          "form": "선선해진다고 좋아했는데 말이죠",
          "original_form": "선선해진다고 좋아했는데 말이죠",
          "speaker_id": "2",
          "time": "20210827 07:36"
        },
        {
          "id": "MDRW2100018515.1.23",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210827 07:36"
        },
        {
          "id": "MDRW2100018515.1.24",
          "form": "좀지나면 나아 지겠죠",
          "original_form": "좀지나면 나아 지겠죠",
          "speaker_id": "1",
          "time": "20210827 07:36"
        },
        {
          "id": "MDRW2100018515.1.25",
          "form": "습기들과 안녕 하고파요 ㅋㅋ",
          "original_form": "습기들과 안녕 하고파요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:36"
        }
      ]
    }
  ]
