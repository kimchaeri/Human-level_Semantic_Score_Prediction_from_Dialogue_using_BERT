      "utterance": [
        {
          "id": "MDRW2100005392.1.1",
          "form": "스트레스 최고봉은 업무 스트레스지여 ㅋㅋㅋ",
          "original_form": "스트레스 최고봉은 업무 스트레스지여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005392.1.2",
          "form": "언니네는 사람 안구해여??",
          "original_form": "언니네는 사람 안구해여??",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005392.1.3",
          "form": "내년에 구하지ㅋ",
          "original_form": "내년에 구하지ㅋ",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005392.1.4",
          "form": "우린 아르바이트수준ㅡㅡ",
          "original_form": "우린 아르바이트수준ㅡㅡ",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005392.1.5",
          "form": "어차피 난 못가요 ㅋ",
          "original_form": "어차피 난 못가요 ㅋ",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005392.1.6",
          "form": "급여가ㅡㅡ",
          "original_form": "급여가ㅡㅡ",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005392.1.7",
          "form": "언니 요것만 하고가요 ㅋ",
          "original_form": "언니 요것만 하고가요 ㅋ",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005392.1.8",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005392.1.9",
          "form": "푸하하",
          "original_form": "푸하하",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005392.1.10",
          "form": "캐도 언니 사대 안드가요?",
          "original_form": "캐도 언니 사대 안드가요?",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005392.1.11",
          "form": "열정적인 ㅋㅋ",
          "original_form": "열정적인 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005392.1.12",
          "form": "들어가지 그러니 실지급이",
          "original_form": "들어가지 그러니 실지급이",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005392.1.13",
          "form": "별로임",
          "original_form": "별로임",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005392.1.14",
          "form": "실업급여??노릴수 없나요??",
          "original_form": "실업급여??노릴수 없나요??",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005392.1.15",
          "form": "아..맞다 세금떼니깜..",
          "original_form": "아..맞다 세금떼니깜..",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005392.1.16",
          "form": "이직고민하는사람있음",
          "original_form": "이직고민하는사람있음",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005392.1.17",
          "form": "받아야지ㅋㅋ",
          "original_form": "받아야지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005392.1.18",
          "form": "캐도 시간 좋은데~",
          "original_form": "캐도 시간 좋은데~",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005392.1.19",
          "form": "우리계약직이잖아",
          "original_form": "우리계약직이잖아",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005392.1.20",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005392.1.21",
          "form": "근데 계약이지만 오래 일했잖아뇨?",
          "original_form": "근데 계약이지만 오래 일했잖아뇨?",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005392.1.22",
          "form": "계야종료되면 받을수있다",
          "original_form": "계야종료되면 받을수있다",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005392.1.23",
          "form": "우리5시간30분",
          "original_form": "우리5시간30분",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005392.1.24",
          "form": "중간에 잠깐 집에와도되고~",
          "original_form": "중간에 잠깐 집에와도되고~",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005392.1.25",
          "form": "요즘시설장이 다혈질이라ㅡㅡ",
          "original_form": "요즘시설장이 다혈질이라ㅡㅡ",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005392.1.26",
          "form": "그런건 쩜 부러워여 ㅋㅋㅋ",
          "original_form": "그런건 쩜 부러워여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005392.1.27",
          "form": "스트레스ㅡㅡ",
          "original_form": "스트레스ㅡㅡ",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005392.1.28",
          "form": "아 그건그렇지",
          "original_form": "아 그건그렇지",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005392.1.29",
          "form": "언니가 더 화내요 ㅋㅋ",
          "original_form": "언니가 더 화내요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005392.1.30",
          "form": "시간이 좀자유롭다",
          "original_form": "시간이 좀자유롭다",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005392.1.31",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005392.1.32",
          "form": "진짜 같이 일하는사람 짱나며뮤",
          "original_form": "진짜 같이 일하는사람 짱나며뮤",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005392.1.33",
          "form": "짤린다ㅋㅋ",
          "original_form": "짤린다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005392.1.34",
          "form": "가늘고길게",
          "original_form": "가늘고길게",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005392.1.35",
          "form": "일하고있음",
          "original_form": "일하고있음",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005392.1.36",
          "form": "그게 최고쥬 온니~",
          "original_form": "그게 최고쥬 온니~",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005392.1.37",
          "form": "고생했아요 ~이제 보내쥬께요ㅋㅋ",
          "original_form": "고생했아요 ~이제 보내쥬께요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005392.1.38",
          "form": "푸하하",
          "original_form": "푸하하",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005392.1.39",
          "form": "안농히 주무시라요^^",
          "original_form": "안농히 주무시라요^^",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005392.1.40",
          "form": "니도 빨리자라",
          "original_form": "니도 빨리자라",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005392.1.41",
          "form": "일찍자믄서ㅋㅋ",
          "original_form": "일찍자믄서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005392.1.42",
          "form": "눈이 감기는구만요;;",
          "original_form": "눈이 감기는구만요;;",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005392.1.43",
          "form": "언니 일어나요 일하러갑시다 ㅋㅋ",
          "original_form": "언니 일어나요 일하러갑시다 ㅋㅋ",
