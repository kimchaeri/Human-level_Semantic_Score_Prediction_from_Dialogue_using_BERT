      "utterance": [
        {
          "id": "MDRW2100032998.1.1",
          "form": "오늘은 2시 정도에 끝날 거 같아",
          "original_form": "오늘은 2시 정도에 끝날 거 같아",
          "speaker_id": "2",
          "time": "20210906 12:39"
        },
        {
          "id": "MDRW2100032998.1.2",
          "form": "오늘부터 전면 등교라 걱정이네",
          "original_form": "오늘부터 전면 등교라 걱정이네",
          "speaker_id": "1",
          "time": "20210906 12:40"
        },
        {
          "id": "MDRW2100032998.1.3",
          "form": "ㅇㅇ 마스크 쓰고 수업이 힘드니깐",
          "original_form": "ㅇㅇ 마스크 쓰고 수업이 힘드니깐",
          "speaker_id": "2",
          "time": "20210906 12:40"
        },
        {
          "id": "MDRW2100032998.1.4",
          "form": "그것도 걱정이고 특히 급식이...",
          "original_form": "그것도 걱정이고 특히 급식이...",
          "speaker_id": "1",
          "time": "20210906 12:40"
        },
        {
          "id": "MDRW2100032998.1.5",
          "form": "이번에는 급식이 선택이 아니라서 마음에 걸려 ㅠ",
          "original_form": "이번에는 급식이 선택이 아니라서 마음에 걸려 ㅠ",
          "speaker_id": "2",
          "time": "20210906 12:41"
        },
        {
          "id": "MDRW2100032998.1.6",
          "form": "지금까지 잘 버텼는데 갑자기 수업 중간에 급식이 있는지..",
          "original_form": "지금까지 잘 버텼는데 갑자기 수업 중간에 급식이 있는지..",
          "speaker_id": "1",
          "time": "20210906 12:41"
        },
        {
          "id": "MDRW2100032998.1.7",
          "form": "나도 잘 이해가 안돼 ㅠ",
          "original_form": "나도 잘 이해가 안돼 ㅠ",
          "speaker_id": "2",
          "time": "20210906 12:41"
        },
        {
          "id": "MDRW2100032998.1.8",
          "form": "학교 점심시간에 애들 많을텐데",
          "original_form": "학교 점심시간에 애들 많을텐데",
          "speaker_id": "1",
          "time": "20210906 12:42"
        },
        {
          "id": "MDRW2100032998.1.9",
          "form": "글치 다 모여서 식사하니까..",
          "original_form": "글치 다 모여서 식사하니까..",
          "speaker_id": "2",
          "time": "20210906 12:43"
        },
        {
          "id": "MDRW2100032998.1.10",
          "form": "애들 오자마자 씻겨야겠다",
          "original_form": "애들 오자마자 씻겨야겠다",
          "speaker_id": "1",
          "time": "20210906 12:43"
        },
        {
          "id": "MDRW2100032998.1.11",
          "form": "그래야할듯.. 우리라도 방역을 철저히 해야지",
          "original_form": "그래야할듯.. 우리라도 방역을 철저히 해야지",
          "speaker_id": "2",
          "time": "20210906 12:43"
        },
        {
          "id": "MDRW2100032998.1.12",
          "form": "ㅇㅇ 오늘부터 등교라서 특히 더 조심해야해",
          "original_form": "ㅇㅇ 오늘부터 등교라서 특히 더 조심해야해",
          "speaker_id": "1",
          "time": "20210906 12:43"
        },
        {
          "id": "MDRW2100032998.1.13",
          "form": "등교하니까 참 걱정이 많네",
          "original_form": "등교하니까 참 걱정이 많네",
          "speaker_id": "2",
          "time": "20210906 12:43"
        },
        {
          "id": "MDRW2100032998.1.14",
          "form": "쉬는 시간도 짧아",
          "original_form": "쉬는 시간도 짧아",
          "speaker_id": "2",
          "time": "20210906 12:44"
        },
        {
          "id": "MDRW2100032998.1.15",
          "form": "몇 분씩 주는데?",
          "original_form": "몇 분씩 주는데?",
          "speaker_id": "1",
          "time": "20210906 12:44"
        },
        {
          "id": "MDRW2100032998.1.16",
          "form": "5분 ㅠ 쉴 시간도 없을듯",
          "original_form": "5분 ㅠ 쉴 시간도 없을듯",
          "speaker_id": "2",
          "time": "20210906 12:44"
        },
        {
          "id": "MDRW2100032998.1.17",
          "form": "에궁.. 오늘 돌아오면 많이 힘들어하겠네",
          "original_form": "에궁.. 오늘 돌아오면 많이 힘들어하겠네",
