      "utterance": [
        {
          "id": "MDRW2100009938.1.1",
          "form": "혹시 어느 전공인지 여쭤봐도 될까요?",
          "original_form": "혹시 어느 전공인지 여쭤봐도 될까요?",
          "speaker_id": "2",
          "time": "20210823 11:59"
        },
        {
          "id": "MDRW2100009938.1.2",
          "form": "저 인문학이에요 !",
          "original_form": "저 인문학이에요 !",
          "speaker_id": "1",
          "time": "20210823 12:11"
        },
        {
          "id": "MDRW2100009938.1.3",
          "form": "ㅋㄴ님은 어떤 전공이신가요?!",
          "original_form": "ㅋㄴ님은 어떤 전공이신가요?!",
          "speaker_id": "1",
          "time": "20210823 12:11"
        },
        {
          "id": "MDRW2100009938.1.4",
          "form": "저는 디자인전공이에요",
          "original_form": "저는 디자인전공이에요",
          "speaker_id": "2",
          "time": "20210823 12:14"
        },
        {
          "id": "MDRW2100009938.1.5",
          "form": "아 저도 복수전공을 미디어 쪽을 했어요",
          "original_form": "아 저도 복수전공을 미디어 쪽을 했어요",
          "speaker_id": "1",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100009938.1.6",
          "form": "학교에서 맨날 밤샜는데 ㅋㅋ ㅜ",
          "original_form": "학교에서 맨날 밤샜는데 ㅋㅋ ㅜ",
          "speaker_id": "1",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100009938.1.7",
          "form": "저도 학교만 안가지 학기중에는 밤 자주 샜어요ㅠ",
          "original_form": "저도 학교만 안가지 학기중에는 밤 자주 샜어요ㅠ",
          "speaker_id": "2",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009938.1.8",
          "form": "집에서 밤새다니..",
          "original_form": "집에서 밤새다니..",
          "speaker_id": "1",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009938.1.9",
          "form": "학교장비라도 돌려야 하는데 ㅜ",
          "original_form": "학교장비라도 돌려야 하는데 ㅜ",
          "speaker_id": "1",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009938.1.10",
          "form": "아직은 노트북만으로 할 수 있는 단계라 불편한건 크게 없었어요",
          "original_form": "아직은 노트북만으로 할 수 있는 단계라 불편한건 크게 없었어요",
          "speaker_id": "2",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009938.1.11",
          "form": "3학년 때부터 교내 장비가 필수라고 하더라구요",
          "original_form": "3학년 때부터 교내 장비가 필수라고 하더라구요",
          "speaker_id": "2",
          "time": "20210823 12:27"
        },
        {
          "id": "MDRW2100009938.1.12",
          "form": "맞아요 랜더링 돌리고 어쩌고 하려면..",
          "original_form": "맞아요 랜더링 돌리고 어쩌고 하려면..",
          "speaker_id": "1",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009938.1.13",
          "form": "3d 들어가면 학교꺼 써야해요",
          "original_form": "3d 들어가면 학교꺼 써야해요",
          "speaker_id": "1",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009938.1.14",
          "form": "ㅜㅜ 얼른 코로나가 끝나야할텐데",
          "original_form": "ㅜㅜ 얼른 코로나가 끝나야할텐데",
          "speaker_id": "1",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009938.1.15",
          "form": "곧 끝날 거예요 끝이 있겠죠",
          "original_form": "곧 끝날 거예요 끝이 있겠죠",
          "speaker_id": "2",
          "time": "20210823 12:30"
        },
        {
          "id": "MDRW2100009938.1.16",
          "form": "수강신청은 잘 하셨나요",
          "original_form": "수강신청은 잘 하셨나요",
          "speaker_id": "1",
          "time": "20210823 12:47"
        },
        {
          "id": "MDRW2100009938.1.17",
          "form": "네 원하는 시간대로 됐고 23학점 신청했어요",
          "original_form": "네 원하는 시간대로 됐고 23학점 신청했어요",
          "speaker_id": "2",
          "time": "20210823 13:12"
        },
        {
          "id": "MDRW2100009938.1.18",
          "form": "와 23학점이면 엄청많이 들으시네요",
          "original_form": "와 23학점이면 엄청많이 들으시네요",
          "speaker_id": "1",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100009938.1.19",
          "form": "1학년때 학점을 많이 따야지 졸작 준비할때 편하다고 해서요",
          "original_form": "1학년때 학점을 많이 따야지 졸작 준비할때 편하다고 해서요",
          "speaker_id": "2",
          "time": "20210823 13:45"
        },
        {
          "id": "MDRW2100009938.1.20",
          "form": "아 그건 그렇네요 미래를 위해 지금 고생하시는군요",
          "original_form": "아 그건 그렇네요 미래를 위해 지금 고생하시는군요",
          "speaker_id": "1",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100009938.1.21",
          "form": "교수님들께서도 팔팔할 때 열심히 하래요",
          "original_form": "교수님들께서도 팔팔할 때 열심히 하래요",
          "speaker_id": "2",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100009938.1.22",
          "form": "물론 1학년이라고 해서 팔팔한건 아닙니다만",
          "original_form": "물론 1학년이라고 해서 팔팔한건 아닙니다만",
          "speaker_id": "2",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100009938.1.23",
          "form": "ㅋㅋㅋㅋ가장젊을때이지만 저도그때 늘 힘없엇던 기억이",
          "original_form": "ㅋㅋㅋㅋ가장젊을때이지만 저도그때 늘 힘없엇던 기억이",
