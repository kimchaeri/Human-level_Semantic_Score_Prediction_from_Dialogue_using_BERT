        },
        {
          "id": "MDRW2100041206.1.2",
          "form": "님은여?",
          "original_form": "님은여?",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100041206.1.3",
          "form": "아침 원래 안먹었어요? 간헐적 단식 중이나요?",
          "original_form": "아침 원래 안먹었어요? 간헐적 단식 중이나요?",
          "speaker_id": "1",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100041206.1.4",
          "form": "아니요 원래 안먹어요 ㅠㅠ",
          "original_form": "아니요 원래 안먹어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041206.1.5",
          "form": "아항.. 습관인거군요",
          "original_form": "아항.. 습관인거군요",
          "speaker_id": "1",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041206.1.6",
          "form": "우리 가족은 모두 아침 먹는 타입이에요",
          "original_form": "우리 가족은 모두 아침 먹는 타입이에요",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100041206.1.7",
          "form": "그냥 생활습관이 아침은 꼭 먹어요 ㅎㅎ",
          "original_form": "그냥 생활습관이 아침은 꼭 먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100041206.1.8",
          "form": "좋은 거 같아요",
          "original_form": "좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041206.1.9",
          "form": "전 잠 때문에 그냥 안먹거든요",
          "original_form": "전 잠 때문에 그냥 안먹거든요",
          "speaker_id": "2",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041206.1.10",
          "form": "잠이 중요한 사람이 있죠",
          "original_form": "잠이 중요한 사람이 있죠",
          "speaker_id": "1",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100041206.1.11",
          "form": "저희집안은 다들 아침형 인간들이라서",
          "original_form": "저희집안은 다들 아침형 인간들이라서",
          "speaker_id": "1",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100041206.1.12",
          "form": "안깨워도 6시쯤이면 다 일어나 있어요",
          "original_form": "안깨워도 6시쯤이면 다 일어나 있어요",
          "speaker_id": "1",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100041206.1.13",
          "form": "그래서 7시쯤 아침을 먹습니다",
          "original_form": "그래서 7시쯤 아침을 먹습니다",
          "speaker_id": "1",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100041206.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ헉 대박이네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ헉 대박이네요",
          "speaker_id": "2",
          "time": "20210905 21:41"
        },
        {
          "id": "MDRW2100041206.1.15",
          "form": "눈이 떠지나요?",
          "original_form": "눈이 떠지나요?",
          "speaker_id": "2",
          "time": "20210905 21:41"
        },
        {
          "id": "MDRW2100041206.1.16",
          "form": "네~ 저절로",
          "original_form": "네~ 저절로",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100041206.1.17",
          "form": "대신 일찍 자죠ㅎㅎㅎ",
          "original_form": "대신 일찍 자죠ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100041206.1.18",
          "form": "우리 식구들은 다들 일찍 자고 일찍 일어나요",
          "original_form": "우리 식구들은 다들 일찍 자고 일찍 일어나요",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100041206.1.19",
          "form": "우와 몇시에 자세요??!?!?",
          "original_form": "우와 몇시에 자세요??!?!?",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041206.1.20",
          "form": "아침 먹으려면 어느정도 자야할지..",
          "original_form": "아침 먹으려면 어느정도 자야할지..",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041206.1.21",
          "form": "곧 자야해요..ㅎㅎㅎㅎ",
          "original_form": "곧 자야해요..ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100041206.1.22",
          "form": "보통 10시면 자는데 요새 이것땜에 조금씩 늦어져요",
          "original_form": "보통 10시면 자는데 요새 이것땜에 조금씩 늦어져요",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100041206.1.23",
          "form": "내일 만납시다",
          "original_form": "내일 만납시다",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100041206.1.24",
          "form": "우와... 대박이네여....",
          "original_form": "우와... 대박이네여....",
          "speaker_id": "2",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100041206.1.25",
          "form": "아침 준비하고 있어요",
          "original_form": "아침 준비하고 있어요",
          "speaker_id": "1",
          "time": "20210906 06:46"
        },
        {
          "id": "MDRW2100041206.1.26",
          "form": "오늘은 요거트에 견과류 넣은 걸로 먹으려고 합니다",
          "original_form": "오늘은 요거트에 견과류 넣은 걸로 먹으려고 합니다",
          "speaker_id": "1",
          "time": "20210906 06:46"
        },
        {
          "id": "MDRW2100041206.1.27",
          "form": "과일 채소도 좀 썰어 넣어야죠",
          "original_form": "과일 채소도 좀 썰어 넣어야죠",
          "speaker_id": "1",
          "time": "20210906 06:46"
        },
        {
          "id": "MDRW2100041206.1.28",
