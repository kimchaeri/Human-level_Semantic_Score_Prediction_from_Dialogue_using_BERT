      "utterance": [
        {
          "id": "MDRW2100038338.1.1",
          "form": "다이어트하시나여?",
          "original_form": "다이어트하시나여?",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100038338.1.2",
          "form": "다이어트 또 시작하려구요",
          "original_form": "다이어트 또 시작하려구요",
          "speaker_id": "1",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100038338.1.3",
          "form": "전에3키로뺐거든요",
          "original_form": "전에3키로뺐거든요",
          "speaker_id": "1",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100038338.1.4",
          "form": "그상태 그대로예요",
          "original_form": "그상태 그대로예요",
          "speaker_id": "1",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100038338.1.5",
          "form": "3키로가 왜이렇게 힘들게하죠?ㅋㅋ",
          "original_form": "3키로가 왜이렇게 힘들게하죠?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100038338.1.6",
          "form": "전 5키로요ㅋㅋㅋ",
          "original_form": "전 5키로요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100038338.1.7",
          "form": "3키로 더 빼야돼요.ㅜ",
          "original_form": "3키로 더 빼야돼요.ㅜ",
          "speaker_id": "1",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100038338.1.8",
          "form": "추석전까지 좀 빼놔야....",
          "original_form": "추석전까지 좀 빼놔야....",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100038338.1.9",
          "form": "그러니깐요.ㅜ와",
          "original_form": "그러니깐요.ㅜ와",
          "speaker_id": "1",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100038338.1.10",
          "form": "많이빼셨네요.",
          "original_form": "많이빼셨네요.",
          "speaker_id": "1",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100038338.1.11",
          "form": "시댁에서 잔소리해요 저는ㅋㅋㅋㄱ짱이져?",
          "original_form": "시댁에서 잔소리해요 저는ㅋㅋㅋㄱ짱이져?",
          "speaker_id": "2",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100038338.1.12",
          "form": "아ㅜㅜ싫네요.ㅜㅜㅋㅋ",
          "original_form": "아ㅜㅜ싫네요.ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100038338.1.13",
          "form": "잔소리ㅜㅜ",
          "original_form": "잔소리ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100038338.1.14",
          "form": "그래도 저를 위한 잔소리로생각하려구요",
          "original_form": "그래도 저를 위한 잔소리로생각하려구요",
          "speaker_id": "2",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100038338.1.15",
          "form": "친정가면 왤케 말랐냐고",
          "original_form": "친정가면 왤케 말랐냐고",
          "speaker_id": "2",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100038338.1.16",
          "form": "더 먹으라고한답니다 ㅎㅎㅎ",
          "original_form": "더 먹으라고한답니다 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100038338.1.17",
          "form": "아ㅋㅋ다르네요ㅋㅋ",
          "original_form": "아ㅋㅋ다르네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100038338.1.18",
          "form": "하긴 저는 살뺐다고하니ㅋㅋ",
          "original_form": "하긴 저는 살뺐다고하니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100038338.1.19",
          "form": "보기좋다하더라구요ㅋ",
          "original_form": "보기좋다하더라구요ㅋ",
          "speaker_id": "1",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100038338.1.20",
          "form": "ㅎㅎ 운동도 하세요?",
          "original_form": "ㅎㅎ 운동도 하세요?",
          "speaker_id": "2",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100038338.1.21",
          "form": "ㅋㅋ  만보걷기랑 줌바요ㅋㅋ,",
          "original_form": "ㅋㅋ  만보걷기랑 줌바요ㅋㅋ,",
          "speaker_id": "1",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100038338.1.22",
          "form": "진짜 근데 매일해야되는데",
          "original_form": "진짜 근데 매일해야되는데",
          "speaker_id": "1",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100038338.1.23",
          "form": "쉽지않네요",
          "original_form": "쉽지않네요",
          "speaker_id": "1",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100038338.1.24",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "1",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100038338.1.25",
          "form": "와 줌바 재미나죠?\r\n저 코시국전에 했는데 ㅎㅎ 쉐킷",
          "original_form": "와 줌바 재미나죠?\r\n저 코시국전에 했는데 ㅎㅎ 쉐킷",
          "speaker_id": "2",
