      "utterance": [
        {
          "id": "MDRW2100028924.1.1",
          "form": "입원하면 힘들어요ㅎㅎ",
          "original_form": "입원하면 힘들어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100028924.1.2",
          "form": "애기가 입원을 많이했엇는데ㅠ 그때마다 주사놓는것도 그렇고",
          "original_form": "애기가 입원을 많이했엇는데ㅠ 그때마다 주사놓는것도 그렇고",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100028924.1.3",
          "form": "전 2번해봣어여",
          "original_form": "전 2번해봣어여",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100028924.1.4",
          "form": "우와ㅎ 진짜요?",
          "original_form": "우와ㅎ 진짜요?",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100028924.1.5",
          "form": "5살인데?",
          "original_form": "5살인데?",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100028924.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100028924.1.7",
          "form": "애가 건강하네요ㅠ",
          "original_form": "애가 건강하네요ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100028924.1.8",
          "form": "돌때 후두염",
          "original_form": "돌때 후두염",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100028924.1.9",
          "form": ".에고에고",
          "original_form": ".에고에고",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100028924.1.10",
          "form": "크게 아펐네요",
          "original_form": "크게 아펐네요",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028924.1.11",
          "form": "30개월대 모세 기관지염  후두염",
          "original_form": "30개월대 모세 기관지염  후두염",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028924.1.12",
          "form": "에고에고",
          "original_form": "에고에고",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028924.1.13",
          "form": "저닮아서 기관지가 안좋은거 같아요",
          "original_form": "저닮아서 기관지가 안좋은거 같아요",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028924.1.14",
          "form": "후두염만 두번이네요",
          "original_form": "후두염만 두번이네요",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028924.1.15",
          "form": "에고 힘드셨겟네요",
          "original_form": "에고 힘드셨겟네요",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028924.1.16",
          "form": "네 그뒤로 엄마한테 부탁해서",
          "original_form": "네 그뒤로 엄마한테 부탁해서",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028924.1.17",
          "form": "저희애기는 맨날 입원해서",
          "original_form": "저희애기는 맨날 입원해서",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028924.1.18",
          "form": "그럼 지금은 괜찮죠?",
          "original_form": "그럼 지금은 괜찮죠?",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028924.1.19",
          "form": "배도라지 수세미즙 먹이고 지금은  효과좀 본거 같아요",
          "original_form": "배도라지 수세미즙 먹이고 지금은  효과좀 본거 같아요",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028924.1.20",
          "form": "저 어릴때부터 달고 살앗던 즙이에요 ㅋㅋ",
          "original_form": "저 어릴때부터 달고 살앗던 즙이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028924.1.21",
          "form": "맞아요ㅎ 그게 효과가 직방이죠ㅎㅎ",
          "original_form": "맞아요ㅎ 그게 효과가 직방이죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028924.1.22",
          "form": "배도라지수세미즙 ㅋ",
          "original_form": "배도라지수세미즙 ㅋ",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028924.1.23",
          "form": "배도라지즙ㅎㅎ",
          "original_form": "배도라지즙ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028924.1.24",
          "form": "맞아요ㅎㅎㅎ",
          "original_form": "맞아요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028924.1.25",
          "form": "그거 저희도 먹였어요ㅎ",
          "original_form": "그거 저희도 먹였어요ㅎ",
          "speaker_id": "2",
