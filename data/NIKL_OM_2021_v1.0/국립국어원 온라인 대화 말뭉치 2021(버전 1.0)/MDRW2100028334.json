      "utterance": [
        {
          "id": "MDRW2100028334.1.1",
          "form": "좋아 요즘 잘해 도련이",
          "original_form": "좋아 요즘 잘해 도련이",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028334.1.2",
          "form": "황태자 됐잖아요",
          "original_form": "황태자 됐잖아요",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028334.1.3",
          "form": "저번에 8이닝 무실점인가 1실점인가 해서",
          "original_form": "저번에 8이닝 무실점인가 1실점인가 해서",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028334.1.4",
          "form": "황태자 미쳣나",
          "original_form": "황태자 미쳣나",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028334.1.5",
          "form": "도련님 볼 짱",
          "original_form": "도련님 볼 짱",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028334.1.6",
          "form": "근데 저번만큼 투구수 관리가 잘되지는 않네",
          "original_form": "근데 저번만큼 투구수 관리가 잘되지는 않네",
          "speaker_id": "2",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100028334.1.7",
          "form": "그래도 잘해...",
          "original_form": "그래도 잘해...",
          "speaker_id": "2",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100028334.1.8",
          "form": "어우 열심히 치네...",
          "original_form": "어우 열심히 치네...",
          "speaker_id": "1",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100028334.1.9",
          "form": "크으 헛삼 최고",
          "original_form": "크으 헛삼 최고",
          "speaker_id": "2",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028334.1.10",
          "form": "아니뭐야 스포당함",
          "original_form": "아니뭐야 스포당함",
          "speaker_id": "1",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028334.1.11",
          "form": "님만 티비로보시나요",
          "original_form": "님만 티비로보시나요",
          "speaker_id": "1",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028334.1.12",
          "form": "엥 나도 네이버로 보는데",
          "original_form": "엥 나도 네이버로 보는데",
          "speaker_id": "2",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028334.1.13",
          "form": "뭐임",
          "original_form": "뭐임",
          "speaker_id": "1",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028334.1.14",
          "form": "뭐로 보고 계신 거예요",
          "original_form": "뭐로 보고 계신 거예요",
          "speaker_id": "2",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028334.1.15",
          "form": "나도 네이번데",
          "original_form": "나도 네이번데",
          "speaker_id": "1",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028334.1.16",
          "form": "실시간 아닌 거 아냐?",
          "original_form": "실시간 아닌 거 아냐?",
          "speaker_id": "2",
          "time": "20210902 19:16"
        },
        {
          "id": "MDRW2100028334.1.17",
          "form": "확장프로그램 설치 안해서 그런가",
          "original_form": "확장프로그램 설치 안해서 그런가",
          "speaker_id": "1",
          "time": "20210902 19:16"
        },
        {
          "id": "MDRW2100028334.1.18",
          "form": "그건 나도 안 하는데",
          "original_form": "그건 나도 안 하는데",
          "speaker_id": "2",
          "time": "20210902 19:16"
