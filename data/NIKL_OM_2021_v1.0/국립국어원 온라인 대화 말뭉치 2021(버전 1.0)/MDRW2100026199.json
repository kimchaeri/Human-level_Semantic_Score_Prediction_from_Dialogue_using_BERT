      "utterance": [
        {
          "id": "MDRW2100026199.1.1",
          "form": "너 룸메이트 있었어?",
          "original_form": "너 룸메이트 있었어?",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026199.1.2",
          "form": "난 줄곧 가족이랑 살아서.. 부모님이 룸메이트엿지",
          "original_form": "난 줄곧 가족이랑 살아서.. 부모님이 룸메이트엿지",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026199.1.3",
          "form": "ㅋㅋㅋㅋㅋ 룸은 같이 안쓰잔항....ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ 룸은 같이 안쓰잔항....ㅎ",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026199.1.4",
          "form": "나 10년전까지 방이 없었거든",
          "original_form": "나 10년전까지 방이 없었거든",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026199.1.5",
          "form": "헐..그럼 어디서 살았어? 거실?",
          "original_form": "헐..그럼 어디서 살았어? 거실?",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026199.1.6",
          "form": "지금 집으로 이사오기전까진 잠은 안방에서 엄빠랑",
          "original_form": "지금 집으로 이사오기전까진 잠은 안방에서 엄빠랑",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026199.1.7",
          "form": "헐 그럼 진짜 룸메이트맞네 ㅋㅋ갈등은 없었니ㅏ?",
          "original_form": "헐 그럼 진짜 룸메이트맞네 ㅋㅋ갈등은 없었니ㅏ?",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026199.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 엄빠가 만히 양보해줬찌.. 어느순간부터 침대는 나혼잤더라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 엄빠가 만히 양보해줬찌.. 어느순간부터 침대는 나혼잤더라",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026199.1.9",
          "form": "ㅋㅇㅋㅋㅋㅋㅋㅋ 이 불효자식같으니",
          "original_form": "ㅋㅇㅋㅋㅋㅋㅋㅋ 이 불효자식같으니",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026199.1.10",
          "form": "책상은/?",
          "original_form": "책상은/?",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026199.1.11",
          "form": "난 책상 지금 집 이사와서 처음 생겼어",
          "original_form": "난 책상 지금 집 이사와서 처음 생겼어",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026199.1.12",
          "form": "책상 없었어",
          "original_form": "책상 없었어",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026199.1.13",
          "form": "그럼 예쩐ㅇ엔?언니오빠꺼 썼니?",
          "original_form": "그럼 예쩐ㅇ엔?언니오빠꺼 썼니?",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026199.1.14",
          "form": "언니만 책상 있었어",
          "original_form": "언니만 책상 있었어",
