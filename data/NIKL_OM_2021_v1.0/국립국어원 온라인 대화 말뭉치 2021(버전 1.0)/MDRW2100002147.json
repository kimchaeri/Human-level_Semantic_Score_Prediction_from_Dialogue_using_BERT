      "utterance": [
        {
          "id": "MDRW2100002147.1.1",
          "form": "즐겨보시는 유투버 있으신가요?",
          "original_form": "즐겨보시는 유투버 있으신가요?",
          "speaker_id": "1",
          "time": "20210820 00:00"
        },
        {
          "id": "MDRW2100002147.1.2",
          "form": "네 저는 우아한 사파리라는 유투버 즐겨봐요!",
          "original_form": "네 저는 우아한 사파리라는 유투버 즐겨봐요!",
          "speaker_id": "2",
          "time": "20210820 00:02"
        },
        {
          "id": "MDRW2100002147.1.3",
          "form": "혹시 넷플릭스 좋아하세요?",
          "original_form": "혹시 넷플릭스 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 00:02"
        },
        {
          "id": "MDRW2100002147.1.4",
          "form": "넷플릭스 알긴 하는데 잘 몰라요,,!",
          "original_form": "넷플릭스 알긴 하는데 잘 몰라요,,!",
          "speaker_id": "1",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002147.1.5",
          "form": "결제 고민인데 혿시 추천할만한 거 있나요 !!!!",
          "original_form": "결제 고민인데 혿시 추천할만한 거 있나요 !!!!",
          "speaker_id": "1",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002147.1.6",
          "form": "킹덤시리즈가 유명하더라구요",
          "original_form": "킹덤시리즈가 유명하더라구요",
          "speaker_id": "2",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100002147.1.7",
          "form": "좀비물이예요",
          "original_form": "좀비물이예요",
          "speaker_id": "2",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100002147.1.8",
          "form": "킹덤 유명하잖아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "킹덤 유명하잖아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002147.1.9",
          "form": "날 잡아서 한번 봐야겠어요",
          "original_form": "날 잡아서 한번 봐야겠어요",
          "speaker_id": "1",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002147.1.10",
          "form": "네네 요번에 개봉한 수어사이어드 스쿼드2도 보세요",
          "original_form": "네네 요번에 개봉한 수어사이어드 스쿼드2도 보세요",
          "speaker_id": "2",
          "time": "20210820 00:09"
        },
        {
          "id": "MDRW2100002147.1.11",
          "form": "재미있어요",
          "original_form": "재미있어요",
          "speaker_id": "2",
          "time": "20210820 00:09"
        },
        {
          "id": "MDRW2100002147.1.12",
          "form": "오.. 그거 그 조커랑 같이 나오는 그거 맞나요??!",
          "original_form": "오.. 그거 그 조커랑 같이 나오는 그거 맞나요??!",
          "speaker_id": "1",
          "time": "20210820 00:10"
        },
        {
          "id": "MDRW2100002147.1.13",
          "form": "할리퀸이요!!",
          "original_form": "할리퀸이요!!",
          "speaker_id": "1",
          "time": "20210820 00:10"
        },
        {
          "id": "MDRW2100002147.1.14",
          "form": "맞아요! 그런데 요번  시리즈는 조커는 안나와요",
          "original_form": "맞아요! 그런데 요번  시리즈는 조커는 안나와요",
          "speaker_id": "2",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002147.1.15",
          "form": "그래도 할리퀸이 멱살붙잡고 끌고가는 영화죠",
          "original_form": "그래도 할리퀸이 멱살붙잡고 끌고가는 영화죠",
          "speaker_id": "2",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002147.1.16",
          "form": "아하 제가 그쪽 영화를 본적 없지만",
          "original_form": "아하 제가 그쪽 영화를 본적 없지만",
          "speaker_id": "1",
          "time": "20210820 00:12"
        },
        {
          "id": "MDRW2100002147.1.17",
          "form": "할리퀸이 아주 매력적인건 알고있어요!!",
          "original_form": "할리퀸이 아주 매력적인건 알고있어요!!",
          "speaker_id": "1",
          "time": "20210820 00:12"
        },
        {
          "id": "MDRW2100002147.1.18",
          "form": "네 맞아요 진짜 매력적이에요",
          "original_form": "네 맞아요 진짜 매력적이에요",
          "speaker_id": "2",
          "time": "20210820 00:13"
        },
        {
          "id": "MDRW2100002147.1.19",
          "form": "괜히 인기가 많은 게 아니더군요ㅋㅋㅋㅋㅋ",
          "original_form": "괜히 인기가 많은 게 아니더군요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002147.1.20",
          "form": "네  혹시 어떤 장르 영화좋아하세요?",
          "original_form": "네  혹시 어떤 장르 영화좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002147.1.21",
          "form": "저 좀비물 좋아합니다",
          "original_form": "저 좀비물 좋아합니다",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002147.1.22",
          "form": "판타지두요 !!!!!",
          "original_form": "판타지두요 !!!!!",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002147.1.23",
          "form": "뭐 좋아하세오?",
          "original_form": "뭐 좋아하세오?",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002147.1.24",
          "form": "저는 가리지않고 좋아해요",
          "original_form": "저는 가리지않고 좋아해요",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002147.1.25",
          "form": "월드워Z  개인적으로 좋아해요",
          "original_form": "월드워Z  개인적으로 좋아해요",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002147.1.26",
          "form": "종종 다시보기해용",
          "original_form": "종종 다시보기해용",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002147.1.27",
          "form": "허 저도 좋아합니다ㅋㅋㅋ ㅋㅋㅋㅋ",
          "original_form": "허 저도 좋아합니다ㅋㅋㅋ ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002147.1.28",
          "form": "티비에서 재방 해주면 봐요ㅋㅋㅋㅋㅋ",
          "original_form": "티비에서 재방 해주면 봐요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002147.1.29",
