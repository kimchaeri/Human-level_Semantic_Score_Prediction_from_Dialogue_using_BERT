      "utterance": [
        {
          "id": "MDRW2100023025.1.1",
          "form": "ㅋㅋㅋㅋㅋ저도 오징어볶음 좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋ저도 오징어볶음 좋아해요",
          "speaker_id": "2",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100023025.1.2",
          "form": "당면 덕분에",
          "original_form": "당면 덕분에",
          "speaker_id": "2",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100023025.1.3",
          "form": "먼 길을 돌아왔다",
          "original_form": "먼 길을 돌아왔다",
          "speaker_id": "2",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100023025.1.4",
          "form": "이번엔 제가 지금 먹고 싶은 음식 맞촤보세요",
          "original_form": "이번엔 제가 지금 먹고 싶은 음식 맞촤보세요",
          "speaker_id": "2",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100023025.1.5",
          "form": "좀 어려울것 같긴 했어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "좀 어려울것 같긴 했어요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100023025.1.6",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100023025.1.7",
          "form": "한식인가요?",
          "original_form": "한식인가요?",
          "speaker_id": "1",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100023025.1.8",
          "form": "ㅋㅋㅋㅋㅋ어려운 질문이예요",
          "original_form": "ㅋㅋㅋㅋㅋ어려운 질문이예요",
          "speaker_id": "2",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100023025.1.9",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100023025.1.10",
          "form": "한국게 맞아요",
          "original_form": "한국게 맞아요",
          "speaker_id": "2",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100023025.1.11",
          "form": "한그릇음식인가요?",
          "original_form": "한그릇음식인가요?",
          "speaker_id": "1",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100023025.1.12",
          "form": "아뇨 단위가 그릇이 아니예요",
          "original_form": "아뇨 단위가 그릇이 아니예요",
          "speaker_id": "2",
          "time": "20210829 20:45"
        },
        {
          "id": "MDRW2100023025.1.13",
          "form": "그러면 n인분... 이런식인가요?",
          "original_form": "그러면 n인분... 이런식인가요?",
          "speaker_id": "1",
          "time": "20210829 20:47"
        },
        {
          "id": "MDRW2100023025.1.14",
          "form": "아니요 보통 사람 수를 세지 않아요",
          "original_form": "아니요 보통 사람 수를 세지 않아요",
          "speaker_id": "2",
          "time": "20210829 20:48"
        },
        {
          "id": "MDRW2100023025.1.15",
          "form": "앗 뭐지",
          "original_form": "앗 뭐지",
          "speaker_id": "1",
          "time": "20210829 20:48"
        },
        {
          "id": "MDRW2100023025.1.16",
          "form": "밥반찬인가요?",
          "original_form": "밥반찬인가요?",
          "speaker_id": "1",
          "time": "20210829 20:48"
        },
        {
          "id": "MDRW2100023025.1.17",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210829 20:48"
        },
        {
          "id": "MDRW2100023025.1.18",
          "form": "그런데 밥반찬으로 먹는 사람도 있어요",
          "original_form": "그런데 밥반찬으로 먹는 사람도 있어요",
          "speaker_id": "2",
          "time": "20210829 20:48"
        },
        {
          "id": "MDRW2100023025.1.19",
          "form": "그 사람들을 이상하게 보는 사람들도 있어요",
          "original_form": "그 사람들을 이상하게 보는 사람들도 있어요",
          "speaker_id": "2",
          "time": "20210829 20:48"
        },
        {
          "id": "MDRW2100023025.1.20",
          "form": "크림스프? ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "크림스프? ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:49"
        },
        {
          "id": "MDRW2100023025.1.21",
          "form": "죄송합니다",
          "original_form": "죄송합니다",
          "speaker_id": "1",
          "time": "20210829 20:49"
        },
        {
          "id": "MDRW2100023025.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ온갖 괴식 나오기 시작하겠어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ온갖 괴식 나오기 시작하겠어요",
          "speaker_id": "2",
          "time": "20210829 20:49"
        },
        {
          "id": "MDRW2100023025.1.23",
          "form": "땡 아쉽게도 아닙니다",
          "original_form": "땡 아쉽게도 아닙니다",
          "speaker_id": "2",
          "time": "20210829 20:50"
        },
        {
          "id": "MDRW2100023025.1.24",
          "form": "근데 밥이랑 먹으면 괴식으로 보이긴 해요",
          "original_form": "근데 밥이랑 먹으면 괴식으로 보이긴 해요",
          "speaker_id": "2",
          "time": "20210829 20:50"
        },
        {
          "id": "MDRW2100023025.1.25",
          "form": "매운가요?",
          "original_form": "매운가요?",
          "speaker_id": "1",
          "time": "20210829 20:50"
        },
        {
          "id": "MDRW2100023025.1.26",
          "form": "안 매운 버전도 있고 매운 버전도 있어요",
          "original_form": "안 매운 버전도 있고 매운 버전도 있어요",
          "speaker_id": "2",
          "time": "20210829 20:51"
        },
        {
          "id": "MDRW2100023025.1.27",
          "form": "혹시 마라탕인가요?",
          "original_form": "혹시 마라탕인가요?",
          "speaker_id": "1",
          "time": "20210829 20:51"
        },
        {
          "id": "MDRW2100023025.1.28",
          "form": "땡~ 아닙니다 히히",
          "original_form": "땡~ 아닙니다 히히",
          "speaker_id": "2",
          "time": "20210829 20:51"
        },
        {
          "id": "MDRW2100023025.1.29",
          "form": "마라탕이랑 밥 같이 먹으면 괴식이예요?!",
          "original_form": "마라탕이랑 밥 같이 먹으면 괴식이예요?!",
          "speaker_id": "2",
          "time": "20210829 20:51"
        },
        {
          "id": "MDRW2100023025.1.30",
          "form": "마라탕면밖에 안 묵어봐서",
          "original_form": "마라탕면밖에 안 묵어봐서",
          "speaker_id": "2",
          "time": "20210829 20:52"
        },
        {
          "id": "MDRW2100023025.1.31",
          "form": "ㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:52"
        },
        {
          "id": "MDRW2100023025.1.32",
          "form": "ㅋㅋㅋㅋㅋ그거 기름쟈서 잘 안어울릴걸요??",
          "original_form": "ㅋㅋㅋㅋㅋ그거 기름쟈서 잘 안어울릴걸요??",
          "speaker_id": "1",
          "time": "20210829 20:53"
        }
      ]
    }
