      "utterance": [
        {
          "id": "MDRW2100035454.1.1",
          "form": "전 요즘 여기저기 청약을 넣고 있는데",
          "original_form": "전 요즘 여기저기 청약을 넣고 있는데",
          "speaker_id": "1",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100035454.1.2",
          "form": "미혼에 부양가족이 없어서 사실 ,, 될 가능성은 거의 없어요",
          "original_form": "미혼에 부양가족이 없어서 사실 ,, 될 가능성은 거의 없어요",
          "speaker_id": "1",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100035454.1.3",
          "form": "경쟁률장난아니죠ㅠ",
          "original_form": "경쟁률장난아니죠ㅠ",
          "speaker_id": "2",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100035454.1.4",
          "form": "아 ㅠ",
          "original_form": "아 ㅠ",
          "speaker_id": "2",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100035454.1.5",
          "form": "저는 넣어본적은없어요ㅠ",
          "original_form": "저는 넣어본적은없어요ㅠ",
          "speaker_id": "2",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100035454.1.6",
          "form": "그래도 혹시나 하고 넣어보는?",
          "original_form": "그래도 혹시나 하고 넣어보는?",
          "speaker_id": "1",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100035454.1.7",
          "form": "통장도 아직 1년도안됫는데ㅠ",
          "original_form": "통장도 아직 1년도안됫는데ㅠ",
          "speaker_id": "2",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100035454.1.8",
          "form": "그래도 넣어보세요 ㅋㅋㅋ",
          "original_form": "그래도 넣어보세요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100035454.1.9",
          "form": "어디에 뭘 짓는다 이런 정보가 저절로 생기더라구요",
          "original_form": "어디에 뭘 짓는다 이런 정보가 저절로 생기더라구요",
          "speaker_id": "1",
          "time": "20210829 18:07"
        },
        {
          "id": "MDRW2100035454.1.10",
          "form": "물론 제가 당첨안되니 약은 오르지만",
          "original_form": "물론 제가 당첨안되니 약은 오르지만",
          "speaker_id": "1",
          "time": "20210829 18:07"
        },
        {
          "id": "MDRW2100035454.1.11",
          "form": "혹시 주택은 생각이 있으신가요?",
          "original_form": "혹시 주택은 생각이 있으신가요?",
          "speaker_id": "1",
          "time": "20210829 18:08"
        },
        {
          "id": "MDRW2100035454.1.12",
          "form": "전 지굼 아파트에 사눈데ㅠ",
          "original_form": "전 지굼 아파트에 사눈데ㅠ",
          "speaker_id": "2",
          "time": "20210829 18:09"
        },
        {
          "id": "MDRW2100035454.1.13",
          "form": "계속 아파트에살고싶긴해요ㅠ",
          "original_form": "계속 아파트에살고싶긴해요ㅠ",
          "speaker_id": "2",
          "time": "20210829 18:09"
        },
        {
          "id": "MDRW2100035454.1.14",
          "form": "부모가 아파트를 증여해주면야 ,,가만히 계셔도 되는데",
          "original_form": "부모가 아파트를 증여해주면야 ,,가만히 계셔도 되는데",
          "speaker_id": "1",
          "time": "20210829 18:09"
        },
        {
          "id": "MDRW2100035454.1.15",
          "form": "아들에게만 준다 이럴것 같으면 형편대로 주워야 할 수도 있어요",
          "original_form": "아들에게만 준다 이럴것 같으면 형편대로 주워야 할 수도 있어요",
          "speaker_id": "1",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100035454.1.16",
          "form": "저흰 아들이없지만 ㅠㅠ",
          "original_form": "저흰 아들이없지만 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100035454.1.17",
          "form": "저희부모님은 아파트에 안살고 계세요",
          "original_form": "저희부모님은 아파트에 안살고 계세요",
          "speaker_id": "2",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100035454.1.18",
          "form": "살아본적이없으시죠ㅠㅠ 아파트에ㅠ",
          "original_form": "살아본적이없으시죠ㅠㅠ 아파트에ㅠ",
          "speaker_id": "2",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100035454.1.19",
          "form": "아항,,",
          "original_form": "아항,,",
          "speaker_id": "1",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100035454.1.20",
          "form": "이게 전략에 따라 다른데 결혼하고 애 낳을거면 청약도 아직은 기회가 있는 것 같구요",
          "original_form": "이게 전략에 따라 다른데 결혼하고 애 낳을거면 청약도 아직은 기회가 있는 것 같구요",
          "speaker_id": "1",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100035454.1.21",
          "form": "미혼이면 뭐라도 주워야 할 시기 가튼??",
          "original_form": "미혼이면 뭐라도 주워야 할 시기 가튼??",
          "speaker_id": "1",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100035454.1.22",
          "form": "제 생각이에요 ㅋㅋ",
          "original_form": "제 생각이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100035454.1.23",
          "form": "그래서 비혼주의면 뭐라도 사야 할때구 그게 아님 좀 관망해도 될것 같다 이런 결론을 내구 알아만 보구 있어요",
          "original_form": "그래서 비혼주의면 뭐라도 사야 할때구 그게 아님 좀 관망해도 될것 같다 이런 결론을 내구 알아만 보구 있어요",
          "speaker_id": "1",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100035454.1.24",
          "form": "지역이 어디세요ㅠ? 엄청 빡쌘곳이신가보네요ㅠ",
          "original_form": "지역이 어디세요ㅠ? 엄청 빡쌘곳이신가보네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100035454.1.25",
          "form": "전 경기도요 ㅋㅋㅋ",
          "original_form": "전 경기도요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:13"
        },
        {
          "id": "MDRW2100035454.1.26",
          "form": "전 경북인데 내년에 경기도로 이사가려해여ㅠㅠ",
          "original_form": "전 경북인데 내년에 경기도로 이사가려해여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 18:13"
        },
        {
          "id": "MDRW2100035454.1.27",
          "form": "그럼 주소를 혹시 빨리 옮길수 있음 빨리 옮기시구",
          "original_form": "그럼 주소를 혹시 빨리 옮길수 있음 빨리 옮기시구",
          "speaker_id": "1",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100035454.1.28",
          "form": "한번이라도 더 청약해보세요",
          "original_form": "한번이라도 더 청약해보세요",
          "speaker_id": "1",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100035454.1.29",
          "form": "0쩜 000001이 0쩜000006이 되니",
          "original_form": "0쩜 000001이 0쩜000006이 되니",
          "speaker_id": "1",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100035454.1.30",
