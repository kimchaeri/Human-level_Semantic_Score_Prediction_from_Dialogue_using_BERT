      "utterance": [
        {
          "id": "MDRW2100015506.1.1",
          "form": "쇼핑자주하세요?",
          "original_form": "쇼핑자주하세요?",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015506.1.2",
          "form": "자주는 아니지만 그래도 가끔 해요. 최근엔 뭐 사셨어요?",
          "original_form": "자주는 아니지만 그래도 가끔 해요. 최근엔 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015506.1.3",
          "form": "전 물티슈요ㅎ",
          "original_form": "전 물티슈요ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015506.1.4",
          "form": "아 맞다ㅎ 옷샀어요ㅎㅎ",
          "original_form": "아 맞다ㅎ 옷샀어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015506.1.5",
          "form": "아이옷ㅎㅎ",
          "original_form": "아이옷ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015506.1.6",
          "form": "어디서 사셨어요?",
          "original_form": "어디서 사셨어요?",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015506.1.7",
          "form": "h패션몰이라고 앱에서요ㅎㅎ",
          "original_form": "h패션몰이라고 앱에서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015506.1.8",
          "form": "이벤트로 샀어요ㅎㅎ 이벤트 가격이 189,000인데 거기에 할인해서 7만원에 샀어요ㅎㅎ",
          "original_form": "이벤트로 샀어요ㅎㅎ 이벤트 가격이 189,000인데 거기에 할인해서 7만원에 샀어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015506.1.9",
          "form": "와 엄청 할인했네요!",
          "original_form": "와 엄청 할인했네요!",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015506.1.10",
          "form": "출첵하고 쿠폰3개적용하니깐 그렇게되더라그요ㅎㅎ",
          "original_form": "출첵하고 쿠폰3개적용하니깐 그렇게되더라그요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015506.1.11",
          "form": "나중에 한번해보셔요ㄹㄹ",
          "original_form": "나중에 한번해보셔요ㄹㄹ",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015506.1.12",
          "form": "찾아봐야겠네요. 현대 계열인가요?",
          "original_form": "찾아봐야겠네요. 현대 계열인가요?",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015506.1.13",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015506.1.14",
          "form": "평소에 하루에 100원씩인데ㅎ",
          "original_form": "평소에 하루에 100원씩인데ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015506.1.15",
          "form": "이번달은 이벤트해서 만근시 2만포인트였어요ㅎ",
          "original_form": "이번달은 이벤트해서 만근시 2만포인트였어요ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015506.1.16",
          "form": "다음달도 추석 이벤트가 있겠죠? ㅋㅋ",
          "original_form": "다음달도 추석 이벤트가 있겠죠? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015506.1.17",
          "form": "2만포인트는 보통 3개월에 한번일꺼에요ㅎㅎ",
          "original_form": "2만포인트는 보통 3개월에 한번일꺼에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015506.1.18",
          "form": "그래도 하루 100원이라도 모으면 좋아요ㅎㅎ",
          "original_form": "그래도 하루 100원이라도 모으면 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015506.1.19",
          "form": "나중에 옷살때 할인많이되요ㅎ",
          "original_form": "나중에 옷살때 할인많이되요ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015506.1.20",
          "form": "ㅋㅋ 출첵이 쏠쏠하긴 한데 은근 귀찮더라구요",
          "original_form": "ㅋㅋ 출첵이 쏠쏠하긴 한데 은근 귀찮더라구요",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015506.1.21",
          "form": "귀찬ㄹ아요ㅎㅎㅎ",
          "original_form": "귀찬ㄹ아요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015506.1.22",
          "form": "저도 하고는있는데 만근을 잘 못하네요ㅎㅎ",
          "original_form": "저도 하고는있는데 만근을 잘 못하네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015506.1.23",
          "form": "쇼핑을 합리적으로 해야지 요즘에는 좋더라구요ㅎㅎ",
          "original_form": "쇼핑을 합리적으로 해야지 요즘에는 좋더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015506.1.24",
          "form": "세일 없이 사려면 괜히 아까워요ㅋㅋ",
          "original_form": "세일 없이 사려면 괜히 아까워요ㅋㅋ",
