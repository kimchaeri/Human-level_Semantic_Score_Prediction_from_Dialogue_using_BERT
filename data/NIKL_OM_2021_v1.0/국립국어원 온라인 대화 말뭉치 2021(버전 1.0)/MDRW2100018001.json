      "utterance": [
        {
          "id": "MDRW2100018001.1.1",
          "form": "저는 겨울이요",
          "original_form": "저는 겨울이요",
          "speaker_id": "2",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100018001.1.2",
          "form": "저는 뭘까요?",
          "original_form": "저는 뭘까요?",
          "speaker_id": "1",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100018001.1.3",
          "form": "겨울??",
          "original_form": "겨울??",
          "speaker_id": "2",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100018001.1.4",
          "form": "딩동댕!",
          "original_form": "딩동댕!",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018001.1.5",
          "form": "그러면 사계절중에 어느계절이 제일 좋은가요",
          "original_form": "그러면 사계절중에 어느계절이 제일 좋은가요",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018001.1.6",
          "form": "봄이용",
          "original_form": "봄이용",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018001.1.7",
          "form": "장점이 뭐인가요",
          "original_form": "장점이 뭐인가요",
          "speaker_id": "2",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018001.1.8",
          "form": "제 생일이 있어요",
          "original_form": "제 생일이 있어요",
          "speaker_id": "1",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018001.1.9",
          "form": "네. 그러면 무슨 장점이있나요?",
          "original_form": "네. 그러면 무슨 장점이있나요?",
          "speaker_id": "2",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018001.1.10",
          "form": "가족들이 축하해준답니닷",
          "original_form": "가족들이 축하해준답니닷",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018001.1.11",
          "form": "저는 봄에 장미축제를해서 좋더라고요",
          "original_form": "저는 봄에 장미축제를해서 좋더라고요",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018001.1.12",
          "form": "장미축제 자주 가시나봐요",
          "original_form": "장미축제 자주 가시나봐요",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018001.1.13",
          "form": "한번 갔다왔는데 너무 좋았어요.",
          "original_form": "한번 갔다왔는데 너무 좋았어요.",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018001.1.14",
          "form": "여름 좋아하세요?",
          "original_form": "여름 좋아하세요?",
          "speaker_id": "1",
