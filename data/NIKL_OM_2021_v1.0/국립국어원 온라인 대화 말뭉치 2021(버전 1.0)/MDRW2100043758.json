      "utterance": [
        {
          "id": "MDRW2100043758.1.1",
          "form": "동물키우시나요",
          "original_form": "동물키우시나요",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043758.1.2",
          "form": "네 전 고양이1마리키우고있어요",
          "original_form": "네 전 고양이1마리키우고있어요",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043758.1.3",
          "form": "name2님 동물키우세요?",
          "original_form": "&name2&님 동물키우세요?",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043758.1.4",
          "form": "저는 개키우고싶어서",
          "original_form": "저는 개키우고싶어서",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043758.1.5",
          "form": "개는 훌륭하다",
          "original_form": "개는 훌륭하다",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043758.1.6",
          "form": "이거봤거든요",
          "original_form": "이거봤거든요",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043758.1.7",
          "form": "근데 망나니들이 많더라구요",
          "original_form": "근데 망나니들이 많더라구요",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043758.1.8",
          "form": "저도 즐겨봤어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 즐겨봤어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043758.1.9",
          "form": "개도 사람도",
          "original_form": "개도 사람도",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043758.1.10",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋ티비나 매체에 나오는애들은",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋ티비나 매체에 나오는애들은",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043758.1.11",
          "form": "그거보고 다시 생각하게됐어요",
          "original_form": "그거보고 다시 생각하게됐어요",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043758.1.12",
          "form": "고부해도 자주보고 배우고이ㅛ어요",
          "original_form": "고부해도 자주보고 배우고이ㅛ어요",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043758.1.13",
          "form": "우리애는 약과구나...",
          "original_form": "우리애는 약과구나...",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043758.1.14",
          "form": "고양이는 참 똑똑하다고 들었는데",
          "original_form": "고양이는 참 똑똑하다고 들었는데",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043758.1.15",
          "form": "ㅋㅋ심통도 자주부린다면서요",
          "original_form": "ㅋㅋ심통도 자주부린다면서요",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043758.1.16",
          "form": "네 ㅋㅋㅋㅋㅋ진짜 자기말안듣잖아여",
          "original_form": "네 ㅋㅋㅋㅋㅋ진짜 자기말안듣잖아여",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043758.1.17",
          "form": "그럼 일부러 보란듯이 가구긁어요",
          "original_form": "그럼 일부러 보란듯이 가구긁어요",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043758.1.18",
          "form": "엄마가 가구긁는거 엄청싫어하거든요 ㅋㅋㅋㅋㅋ전상관없으니까 꼭 엄마한테가서 가구긁어요 ㅋㅋㅋ",
          "original_form": "엄마가 가구긁는거 엄청싫어하거든요 ㅋㅋㅋㅋㅋ전상관없으니까 꼭 엄마한테가서 가구긁어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043758.1.19",
          "form": "헉ㅋㅋ엄마한테 관심받고싶어서",
          "original_form": "헉ㅋㅋ엄마한테 관심받고싶어서",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043758.1.20",
          "form": "그러는거예요?",
          "original_form": "그러는거예요?",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043758.1.21",
          "form": "네 엄마가 긁는거보면 저를부르거든여 그럼 냥이는 저를소환함으로써 놀아달라고하능거에요 ㅋㅋㅋㅋ",
          "original_form": "네 엄마가 긁는거보면 저를부르거든여 그럼 냥이는 저를소환함으로써 놀아달라고하능거에요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043758.1.22",
          "form": "ㅋㅋㅋㅋ고단수네요",
          "original_form": "ㅋㅋㅋㅋ고단수네요",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043758.1.23",
          "form": "발칙하고 귀여운녀석이네요",
          "original_form": "발칙하고 귀여운녀석이네요",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043758.1.24",
          "form": "네 ㅋㅋㅋㅋㅋㅋㅋㅋ귀여워서 봐줍니다",
          "original_form": "네 ㅋㅋㅋㅋㅋㅋㅋㅋ귀여워서 봐줍니다",
          "speaker_id": "1",
