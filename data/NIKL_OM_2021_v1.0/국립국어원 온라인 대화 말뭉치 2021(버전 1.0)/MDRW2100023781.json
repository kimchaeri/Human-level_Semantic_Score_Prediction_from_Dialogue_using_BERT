      "utterance": [
        {
          "id": "MDRW2100023781.1.1",
          "form": "그럴 수도 있겠네",
          "original_form": "그럴 수도 있겠네",
          "speaker_id": "2",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100023781.1.2",
          "form": "비용도 아예 따로고 엄청 비싸 대",
          "original_form": "비용도 아예 따로고 엄청 비싸 대",
          "speaker_id": "1",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100023781.1.3",
          "form": "외국이 더 비싼가?",
          "original_form": "외국이 더 비싼가?",
          "speaker_id": "2",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100023781.1.4",
          "form": "그런가봐",
          "original_form": "그런가봐",
          "speaker_id": "1",
          "time": "20210830 17:27"
        },
        {
          "id": "MDRW2100023781.1.5",
          "form": "그럴 수 있지",
          "original_form": "그럴 수 있지",
          "speaker_id": "2",
          "time": "20210830 17:27"
        },
        {
          "id": "MDRW2100023781.1.6",
          "form": "외국이 은근히 서비스 산업이 별로인 경우가 많아",
          "original_form": "외국이 은근히 서비스 산업이 별로인 경우가 많아",
          "speaker_id": "1",
          "time": "20210830 17:28"
        },
        {
          "id": "MDRW2100023781.1.7",
          "form": "오히려 한국이 더 나은거 같기도 해",
          "original_form": "오히려 한국이 더 나은거 같기도 해",
          "speaker_id": "2",
          "time": "20210830 17:28"
        },
        {
          "id": "MDRW2100023781.1.8",
          "form": "그치 땅이 좁아서 가깝기도 하고",
          "original_form": "그치 땅이 좁아서 가깝기도 하고",
          "speaker_id": "1",
          "time": "20210830 17:29"
        },
        {
          "id": "MDRW2100023781.1.9",
          "form": "아 그래서  그런건가?",
          "original_form": "아 그래서  그런건가?",
          "speaker_id": "2",
          "time": "20210830 17:29"
        },
        {
          "id": "MDRW2100023781.1.10",
          "form": "꼭 그런 건 아닌데 그것도 이유 중에 하나고 우리나라는 우리나라는 서비스 산업이 되게 발달되어 있는 나라 중에 하나야",
          "original_form": "꼭 그런 건 아닌데 그것도 이유 중에 하나고 우리나라는 우리나라는 서비스 산업이 되게 발달되어 있는 나라 중에 하나야",
          "speaker_id": "1",
          "time": "20210830 17:29"
        },
        {
          "id": "MDRW2100023781.1.11",
          "form": "서비스 산업이 발달되어 있는 나라가",
          "original_form": "서비스 산업이 발달되어 있는 나라가",
          "speaker_id": "2",
          "time": "20210830 17:29"
        },
        {
          "id": "MDRW2100023781.1.12",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210830 17:30"
        },
        {
          "id": "MDRW2100023781.1.13",
          "form": "우리나라 말고 또 어디있지?",
          "original_form": "우리나라 말고 또 어디있지?",
          "speaker_id": "2",
          "time": "20210830 17:30"
        },
        {
          "id": "MDRW2100023781.1.14",
          "form": "잘 모르겠어 일본?",
          "original_form": "잘 모르겠어 일본?",
          "speaker_id": "1",
          "time": "20210830 17:30"
        },
        {
          "id": "MDRW2100023781.1.15",
          "form": "아무튼 별로 없지?",
          "original_form": "아무튼 별로 없지?",
          "speaker_id": "2",
          "time": "20210830 17:31"
        },
        {
          "id": "MDRW2100023781.1.16",
          "form": "안 더워지면 머리 자를거야",
          "original_form": "안 더워지면 머리 자를거야",
