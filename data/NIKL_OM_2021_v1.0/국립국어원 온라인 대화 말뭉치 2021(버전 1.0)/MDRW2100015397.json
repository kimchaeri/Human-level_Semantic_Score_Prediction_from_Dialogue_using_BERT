        },
        {
          "id": "MDRW2100015397.1.2",
          "form": "안녕하세요 ㅋㅋ",
          "original_form": "안녕하세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015397.1.3",
          "form": "맨시티 아스날전",
          "original_form": "맨시티 아스날전",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015397.1.4",
          "form": "보실건가요?",
          "original_form": "보실건가요?",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015397.1.5",
          "form": "헐 오늘이에요?",
          "original_form": "헐 오늘이에요?",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015397.1.6",
          "form": "지금 롸잇나우?",
          "original_form": "지금 롸잇나우?",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015397.1.7",
          "form": "ㅋㅋㅋ아니요",
          "original_form": "ㅋㅋㅋ아니요",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015397.1.8",
          "form": "토요일이던가요",
          "original_form": "토요일이던가요",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015397.1.9",
          "form": "누가이길것같아요?",
          "original_form": "누가이길것같아요?",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015397.1.10",
          "form": "전 맨시티요ㅋㅋㅋㅋ",
          "original_form": "전 맨시티요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015397.1.11",
          "form": "아스날이 예전의 아스날이 아니므로 저도 맨시티 ㅋㅋㅋㅋ",
          "original_form": "아스날이 예전의 아스날이 아니므로 저도 맨시티 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015397.1.12",
          "form": "사실 토트넘 말곤 잘 모르지만ㅋㅋㅋ",
          "original_form": "사실 토트넘 말곤 잘 모르지만ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015397.1.13",
          "form": "ㅋㅋㅋ 덕배가",
          "original_form": "ㅋㅋㅋ 덕배가",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015397.1.14",
          "form": "와 덕배 아직도 맨시티 있어요??",
          "original_form": "와 덕배 아직도 맨시티 있어요??",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015397.1.15",
          "form": "ㅋㅋㅋㅋ 네",
          "original_form": "ㅋㅋㅋㅋ 네",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015397.1.16",
          "form": "아직도있답니다",
          "original_form": "아직도있답니다",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015397.1.17",
          "form": "저번 맨시티전때",
          "original_form": "저번 맨시티전때",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015397.1.18",
          "form": "후반교체로 후다닥나왔었ㅈᆢ",
          "original_form": "후반교체로 후다닥나왔었ㅈᆢ",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015397.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그래서 눈에 안보였나봐요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그래서 눈에 안보였나봐요",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015397.1.20",
          "form": "헐 교체자원이에요 이젠?",
          "original_form": "헐 교체자원이에요 이젠?",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015397.1.21",
          "form": "아뇨ㅋㅋㅋ",
          "original_form": "아뇨ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100015397.1.22",
          "form": "그때부상?이었나 뭐회복이었나그래서늦게들어왔고 들어오자마자",
          "original_form": "그때부상?이었나 뭐회복이었나그래서늦게들어왔고 들어오자마자",
          "speaker_id": "2",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100015397.1.23",
          "form": "분위기 싹 바뀌었죠ㅋㅋㅋㅋ",
          "original_form": "분위기 싹 바뀌었죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100015397.1.24",
          "form": "아하 여전하구나 덕배는",
          "original_form": "아하 여전하구나 덕배는",
          "speaker_id": "1",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100015397.1.25",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210826 13:41"
        },
        {
          "id": "MDRW2100015397.1.26",
          "form": "여전히 인기도 많은듯요",
          "original_form": "여전히 인기도 많은듯요",
          "speaker_id": "2",
          "time": "20210826 13:41"
        }
      ]
