      "utterance": [
        {
          "id": "MDRW2100033650.1.1",
          "form": "당근거래 해보셨어요?",
          "original_form": "당근거래 해보셨어요?",
          "speaker_id": "2",
          "time": "20210824 20:40"
        },
        {
          "id": "MDRW2100033650.1.2",
          "form": "저 밥좀 먹고 올게요",
          "original_form": "저 밥좀 먹고 올게요",
          "speaker_id": "1",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100033650.1.3",
          "form": "네 맛있게 드세옹",
          "original_form": "네 맛있게 드세옹",
          "speaker_id": "2",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100033650.1.4",
          "form": "네 당근거래 자주하는편입니다",
          "original_form": "네 당근거래 자주하는편입니다",
          "speaker_id": "1",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100033650.1.5",
          "form": "뭐 주로 하세요??",
          "original_form": "뭐 주로 하세요??",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100033650.1.6",
          "form": "파는 편이고요",
          "original_form": "파는 편이고요",
          "speaker_id": "1",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100033650.1.7",
          "form": "최근에 갤럭시 버즈 팔았어요~",
          "original_form": "최근에 갤럭시 버즈 팔았어요~",
          "speaker_id": "1",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100033650.1.8",
          "form": "오 버즈 파셨군요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 버즈 파셨군요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100033650.1.9",
          "form": "당근이 은근 좋더라구요",
          "original_form": "당근이 은근 좋더라구요",
          "speaker_id": "2",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100033650.1.10",
          "form": "전자기기라서 그런지",
          "original_form": "전자기기라서 그런지",
          "speaker_id": "1",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100033650.1.11",
          "form": "빨리 팔렸어요",
          "original_form": "빨리 팔렸어요",
          "speaker_id": "1",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100033650.1.12",
          "form": "좋긴한데",
          "original_form": "좋긴한데",
          "speaker_id": "1",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100033650.1.13",
          "form": "진상이 많아요ㅋㅋ",
          "original_form": "진상이 많아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100033650.1.14",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100033650.1.15",
          "form": "막 깎아달라고라하죠?",
          "original_form": "막 깎아달라고라하죠?",
          "speaker_id": "2",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100033650.1.16",
          "form": "그리고 갖다달라는 사람도 많아요ㅋㅋ",
          "original_form": "그리고 갖다달라는 사람도 많아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100033650.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ인정 양심없어요 ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ인정 양심없어요 ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:10"
        },
        {
          "id": "MDRW2100033650.1.18",
          "form": "당근 거래 해보셨나요?",
          "original_form": "당근 거래 해보셨나요?",
          "speaker_id": "1",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100033650.1.19",
          "form": "네 저도 하는데",
          "original_form": "네 저도 하는데",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100033650.1.20",
          "form": "양심없는 에눌 해달라는 사람 많아요 ㅎ",
          "original_form": "양심없는 에눌 해달라는 사람 많아요 ㅎ",
