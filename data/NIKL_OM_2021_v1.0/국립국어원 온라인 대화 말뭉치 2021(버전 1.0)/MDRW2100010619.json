      "utterance": [
        {
          "id": "MDRW2100010619.1.1",
          "form": "책은 좋아하세요?",
          "original_form": "책은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100010619.1.2",
          "form": "책 좋아해요",
          "original_form": "책 좋아해요",
          "speaker_id": "1",
          "time": "20210823 20:09"
        },
        {
          "id": "MDRW2100010619.1.3",
          "form": "무슨장르요?",
          "original_form": "무슨장르요?",
          "speaker_id": "2",
          "time": "20210823 20:09"
        },
        {
          "id": "MDRW2100010619.1.4",
          "form": "주로 소설",
          "original_form": "주로 소설",
          "speaker_id": "1",
          "time": "20210823 20:10"
        },
        {
          "id": "MDRW2100010619.1.5",
          "form": "봐요",
          "original_form": "봐요",
          "speaker_id": "1",
          "time": "20210823 20:10"
        },
        {
          "id": "MDRW2100010619.1.6",
          "form": "요즘 무슨 소설 보세요?",
          "original_form": "요즘 무슨 소설 보세요?",
          "speaker_id": "2",
          "time": "20210823 20:10"
        },
        {
          "id": "MDRW2100010619.1.7",
          "form": "전 일본소설 좋아해요",
          "original_form": "전 일본소설 좋아해요",
          "speaker_id": "2",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100010619.1.8",
          "form": "오 저도 일본소설",
          "original_form": "오 저도 일본소설",
          "speaker_id": "1",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100010619.1.9",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "1",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100010619.1.10",
          "form": "감정묘사나",
          "original_form": "감정묘사나",
          "speaker_id": "1",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100010619.1.11",
          "form": "기시유스케 알아요?",
          "original_form": "기시유스케 알아요?",
          "speaker_id": "2",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100010619.1.12",
          "form": "철학적인 부분이 많이나와서",
          "original_form": "철학적인 부분이 많이나와서",
          "speaker_id": "1",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100010619.1.13",
          "form": "작가 이름은 잘몰라요",
          "original_form": "작가 이름은 잘몰라요",
          "speaker_id": "1",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100010619.1.14",
          "form": "오 저도 그런부분 좋아하는데 통하네요",
          "original_form": "오 저도 그런부분 좋아하는데 통하네요",
          "speaker_id": "2",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100010619.1.15",
          "form": "어떤책 제목",
          "original_form": "어떤책 제목",
          "speaker_id": "1",
          "time": "20210823 20:12"
        },
        {
          "id": "MDRW2100010619.1.16",
          "form": "기억나는거 있으세요?",
          "original_form": "기억나는거 있으세요?",
          "speaker_id": "1",
          "time": "20210823 20:12"
        },
        {
          "id": "MDRW2100010619.1.17",
          "form": "\"보리의 바다에 가라앉는 열매\" 나 \"신세계에서\"요!",
          "original_form": "\"보리의 바다에 가라앉는 열매\" 나 \"신세계에서\"요!",
          "speaker_id": "2",
          "time": "20210823 20:12"
        },
        {
          "id": "MDRW2100010619.1.18",
          "form": "처음들어봐요",
          "original_form": "처음들어봐요",
          "speaker_id": "1",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010619.1.19",
          "form": "유명한가요?",
          "original_form": "유명한가요?",
          "speaker_id": "1",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010619.1.20",
          "form": "어 아닐걸요?",
          "original_form": "어 아닐걸요?",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010619.1.21",
          "form": "유명한책중엔 1q84같은 것도 본적 있어요",
          "original_form": "유명한책중엔 1q84같은 것도 본적 있어요",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010619.1.22",
          "form": "무라카미 하루키 책은 다 읽긴 했는데 좋아하진 않아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "무라카미 하루키 책은 다 읽긴 했는데 좋아하진 않아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010619.1.23",
          "form": "무라카미 저도 들어봤어요",
          "original_form": "무라카미 저도 들어봤어요",
          "speaker_id": "1",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010619.1.24",
          "form": "베스트셀러인",
          "original_form": "베스트셀러인",
          "speaker_id": "1",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010619.1.25",
