      "utterance": [
        {
          "id": "MDRW2100024783.1.1",
          "form": "자취하시나요?",
          "original_form": "자취하시나요?",
          "speaker_id": "2",
          "time": "20210904 20:21"
        },
        {
          "id": "MDRW2100024783.1.2",
          "form": "아뇨 부모님집에 캥거루ㅋㅋ",
          "original_form": "아뇨 부모님집에 캥거루ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:21"
        },
        {
          "id": "MDRW2100024783.1.3",
          "form": "그게 좋죠!!! ㅋㅋㅋ 저도 직장만 집 가까웠으면",
          "original_form": "그게 좋죠!!! ㅋㅋㅋ 저도 직장만 집 가까웠으면",
          "speaker_id": "2",
          "time": "20210904 20:28"
        },
        {
          "id": "MDRW2100024783.1.4",
          "form": "같이살고싶어요ㅠㅠㅠ",
          "original_form": "같이살고싶어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 20:28"
        },
        {
          "id": "MDRW2100024783.1.5",
          "form": "아... 맞아요ㅜㅠ 그런데 따로 살면 더 애틋해진대요",
          "original_form": "아... 맞아요ㅜㅠ 그런데 따로 살면 더 애틋해진대요",
          "speaker_id": "1",
          "time": "20210904 20:30"
        },
        {
          "id": "MDRW2100024783.1.6",
          "form": "ㅋㅋㅋㅋ아 맞아요 그런건있어요",
          "original_form": "ㅋㅋㅋㅋ아 맞아요 그런건있어요",
          "speaker_id": "2",
          "time": "20210904 20:30"
        },
        {
          "id": "MDRW2100024783.1.7",
          "form": "같이살면 많이 싸우죠? ㅠ",
          "original_form": "같이살면 많이 싸우죠? ㅠ",
          "speaker_id": "2",
          "time": "20210904 20:31"
        },
        {
          "id": "MDRW2100024783.1.8",
          "form": "말도 마세요 작년 코로나직후엔 매일매일 전쟁... ㅋㅋ",
          "original_form": "말도 마세요 작년 코로나직후엔 매일매일 전쟁... ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100024783.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ재택근무하고 집에있는 시간 늘어나면서  더 그렇게 된거에요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ재택근무하고 집에있는 시간 늘어나면서  더 그렇게 된거에요?",
          "speaker_id": "2",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100024783.1.10",
          "form": "주말에 외출도 못하고 집에 5명이 있으니 전쟁.. ㅋㅋ",
          "original_form": "주말에 외출도 못하고 집에 5명이 있으니 전쟁.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100024783.1.11",
          "form": "하 맞아요 주말엔 진짜 ㅠㅠㅠㅠ ㅋㅋㅋㅋㅋㅋ",
          "original_form": "하 맞아요 주말엔 진짜 ㅠㅠㅠㅠ ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100024783.1.12",
          "form": "오 근데 3남매? 신거에요?",
          "original_form": "오 근데 3남매? 신거에요?",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100024783.1.13",
          "form": "네ㅋㅋ 아무도 결혼을 안하고ㅋㅋ",
          "original_form": "네ㅋㅋ 아무도 결혼을 안하고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100024783.1.14",
          "form": "ㅋㅋㅋㅋ와 저는 동생한명있는데 형제많은거 부러워요!!!",
          "original_form": "ㅋㅋㅋㅋ와 저는 동생한명있는데 형제많은거 부러워요!!!",
