      "utterance": [
        {
          "id": "MDRW2100008544.1.1",
          "form": "시켰는데 재료소진이라 못먹고 다른거 먹었는데 선택미스여서ㅠ",
          "original_form": "시켰는데 재료소진이라 못먹고 다른거 먹었는데 선택미스여서ㅠ",
          "speaker_id": "1",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008544.1.2",
          "form": "로제가대세인것같아요",
          "original_form": "로제가대세인것같아요",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008544.1.3",
          "form": "맞아요~ 근데 엽떡 로제는 조금짜서",
          "original_form": "맞아요~ 근데 엽떡 로제는 조금짜서",
          "speaker_id": "1",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008544.1.4",
          "form": "차라리 그냥 매운게 나은것 같았어요",
          "original_form": "차라리 그냥 매운게 나은것 같았어요",
          "speaker_id": "1",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008544.1.5",
          "form": "로제파스타는좋아하는데",
          "original_form": "로제파스타는좋아하는데",
          "speaker_id": "2",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008544.1.6",
          "form": "다른거에로제는별루인가봐요",
          "original_form": "다른거에로제는별루인가봐요",
          "speaker_id": "2",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008544.1.7",
          "form": "어디꺼드셔보셨어요?ㅋ",
          "original_form": "어디꺼드셔보셨어요?ㅋ",
          "speaker_id": "1",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008544.1.8",
          "form": "저 로제떡볶이는 아직안먹어봤어요",
          "original_form": "저 로제떡볶이는 아직안먹어봤어요",
          "speaker_id": "2",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008544.1.9",
          "form": "엽떡로제도있던데",
          "original_form": "엽떡로제도있던데",
          "speaker_id": "2",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008544.1.10",
          "form": "궁금하긴하네요",
          "original_form": "궁금하긴하네요",
          "speaker_id": "2",
          "time": "20210822 17:00"
        },
        {
          "id": "MDRW2100008544.1.11",
          "form": "다음에 한번 드셔보세요~ ㅎ로제파스타는 어디서 드셔보셨어요?",
          "original_form": "다음에 한번 드셔보세요~ ㅎ로제파스타는 어디서 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210822 17:01"
        },
        {
          "id": "MDRW2100008544.1.12",
          "form": "저는 집에서자주먹어요",
          "original_form": "저는 집에서자주먹어요",
          "speaker_id": "2",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008544.1.13",
          "form": "토마토랑우유있으면가능합니당",
          "original_form": "토마토랑우유있으면가능합니당",
          "speaker_id": "2",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008544.1.14",
          "form": "아 해드신거세요?",
          "original_form": "아 해드신거세요?",
          "speaker_id": "1",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008544.1.15",
          "form": "오 요리잘하시나바요~",
          "original_form": "오 요리잘하시나바요~",
          "speaker_id": "1",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008544.1.16",
          "form": "사먹기도하는데 거의해먹어요 파스타는",
          "original_form": "사먹기도하는데 거의해먹어요 파스타는",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008544.1.17",
          "form": "로제랑 토마토맛있어용",
          "original_form": "로제랑 토마토맛있어용",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008544.1.18",
          "form": "저는 병에든거 사서 시판소스로 ㅋㅋ",
          "original_form": "저는 병에든거 사서 시판소스로 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008544.1.19",
          "form": "요즘 시판소스잘나와서",
          "original_form": "요즘 시판소스잘나와서",
          "speaker_id": "2",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008544.1.20",
          "form": "맛있대요ㅎㅎ",
          "original_form": "맛있대요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008544.1.21",
          "form": "폰타나?거기꺼좋대서 사먹어보려구요",
          "original_form": "폰타나?거기꺼좋대서 사먹어보려구요",
          "speaker_id": "2",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008544.1.22",
          "form": "저는 청정원만 먹어봤어요ㅎ 요새는 알리오올리오도 나오고",
          "original_form": "저는 청정원만 먹어봤어요ㅎ 요새는 알리오올리오도 나오고",
          "speaker_id": "1",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100008544.1.23",
          "form": "잘나오더라구요",
          "original_form": "잘나오더라구요",
          "speaker_id": "1",
