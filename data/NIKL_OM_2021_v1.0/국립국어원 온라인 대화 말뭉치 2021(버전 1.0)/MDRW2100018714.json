        {
          "id": "MDRW2100018714.1.1",
          "form": "전에 내가갔던 곳?",
          "original_form": "전에 내가갔던 곳?",
          "speaker_id": "2",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.2",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.3",
          "form": "가보고파",
          "original_form": "가보고파",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.4",
          "form": "나중에 꼭가자",
          "original_form": "나중에 꼭가자",
          "speaker_id": "2",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.5",
          "form": "돈 많이벌어서",
          "original_form": "돈 많이벌어서",
          "speaker_id": "2",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.6",
          "form": "그랴",
          "original_form": "그랴",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.7",
          "form": "그때 맛있는거사줘",
          "original_form": "그때 맛있는거사줘",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.8",
          "form": "알써",
          "original_form": "알써",
          "speaker_id": "2",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.9",
          "form": "브런치두 먹자",
          "original_form": "브런치두 먹자",
          "speaker_id": "2",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.10",
          "form": "오키",
          "original_form": "오키",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.11",
          "form": "회도먹구",
          "original_form": "회도먹구",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.12",
          "form": "조아조아",
          "original_form": "조아조아",
          "speaker_id": "2",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.13",
          "form": "초밥두 !!!",
          "original_form": "초밥두 !!!",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.14",
          "form": "음..그래",
          "original_form": "음..그래",
          "speaker_id": "2",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.15",
          "form": "그거까지 먹자",
          "original_form": "그거까지 먹자",
          "speaker_id": "2",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.16",
          "form": "아싸리!!",
          "original_form": "아싸리!!",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.17",
          "form": "초밥 진짜좋아",
          "original_form": "초밥 진짜좋아",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018714.1.18",
          "form": "바다보면서",
          "original_form": "바다보면서",
          "speaker_id": "2",
          "time": "20210827 11:18"
        },
        {
          "id": "MDRW2100018714.1.19",
          "form": "먹으면 더 꿀맛이야",
          "original_form": "먹으면 더 꿀맛이야",
          "speaker_id": "2",
          "time": "20210827 11:18"
        },
        {
          "id": "MDRW2100018714.1.20",
          "form": "오션뷰 좋은",
          "original_form": "오션뷰 좋은",
          "speaker_id": "1",
          "time": "20210827 11:18"
        },
        {
          "id": "MDRW2100018714.1.21",
          "form": "시그니엘?",
          "original_form": "시그니엘?",
          "speaker_id": "2",
          "time": "20210827 11:18"
        },
        {
          "id": "MDRW2100018714.1.22",
          "form": "거기가 좋아?",
          "original_form": "거기가 좋아?",
          "speaker_id": "1",
          "time": "20210827 11:18"
        },
        {
          "id": "MDRW2100018714.1.23",
          "form": "그럼 거기루",
          "original_form": "그럼 거기루",
          "speaker_id": "1",
          "time": "20210827 11:18"
        },
        {
          "id": "MDRW2100018714.1.24",
          "form": "오켕",
          "original_form": "오켕",
          "speaker_id": "2",
          "time": "20210827 11:18"
        },
        {
          "id": "MDRW2100018714.1.25",
          "form": "나중에 코로나만 진정되면",
          "original_form": "나중에 코로나만 진정되면",
          "speaker_id": "2",
          "time": "20210827 11:18"
        },
        {
          "id": "MDRW2100018714.1.26",
          "form": "꼭꼭",
