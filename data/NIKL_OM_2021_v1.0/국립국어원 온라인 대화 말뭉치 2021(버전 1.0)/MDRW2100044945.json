      "utterance": [
        {
          "id": "MDRW2100044945.1.1",
          "form": "잘 못먹는 음식 있으신가여?",
          "original_form": "잘 못먹는 음식 있으신가여?",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044945.1.2",
          "form": "회요ㅠㅠㅠ",
          "original_form": "회요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044945.1.3",
          "form": "연어초밥은 좋아하는데 회는 못 먹겠더라구요ㅠㅠ",
          "original_form": "연어초밥은 좋아하는데 회는 못 먹겠더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044945.1.4",
          "form": "아아 저도 초밥은 좋아하는데 회는 가려서 먹어요..!",
          "original_form": "아아 저도 초밥은 좋아하는데 회는 가려서 먹어요..!",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044945.1.5",
          "form": "뼈씹히는 회 너무 실허여..",
          "original_form": "뼈씹히는 회 너무 실허여..",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044945.1.6",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044945.1.7",
          "form": "그쳐 .. 뭔가 이상하고.. 왜먹어야하는지...이해가 되질 않아오",
          "original_form": "그쳐 .. 뭔가 이상하고.. 왜먹어야하는지...이해가 되질 않아오",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044945.1.8",
          "form": "초장맛도 모르겠고",
          "original_form": "초장맛도 모르겠고",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044945.1.9",
          "form": "그러면 다른 해산물도 못드시나요? 오징어회나 조개류나..!",
          "original_form": "그러면 다른 해산물도 못드시나요? 오징어회나 조개류나..!",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044945.1.10",
          "form": "그런건 되게 좋아해요",
          "original_form": "그런건 되게 좋아해요",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044945.1.11",
          "form": "맛있죠 ㅎㅎ !",
          "original_form": "맛있죠 ㅎㅎ !",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044945.1.12",
          "form": "없어서 못먹죠ㅠㅠ",
          "original_form": "없어서 못먹죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044945.1.13",
          "form": "그쵸.. 조개구이 먹으러가고싶네요",
          "original_form": "그쵸.. 조개구이 먹으러가고싶네요",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044945.1.14",
          "form": "ㅠㅠ 으항 ㅠㅠ 무한리필 가보셨나요?",
          "original_form": "ㅠㅠ 으항 ㅠㅠ 무한리필 가보셨나요?",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044945.1.15",
          "form": "아녀 ㅠㅠ 그냥 조개구이맘 먹어봤어요",
          "original_form": "아녀 ㅠㅠ 그냥 조개구이맘 먹어봤어요",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100044945.1.16",
          "form": "무한리필집가서 왕창 먹고싶네요",
          "original_form": "무한리필집가서 왕창 먹고싶네요",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100044945.1.17",
          "form": "조개구이 무한리필집 한번 가봤는데",
          "original_form": "조개구이 무한리필집 한번 가봤는데",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100044945.1.18",
          "form": "괜찮더라구요 !",
          "original_form": "괜찮더라구요 !",
          "speaker_id": "1",
          "time": "20210908 00:38"
        }
      ]
