      "utterance": [
        {
          "id": "MDRW2100039810.1.1",
          "form": "마스크낀채로 말하는거죠?",
          "original_form": "마스크낀채로 말하는거죠?",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039810.1.2",
          "form": "네 저는 계속 껴요",
          "original_form": "네 저는 계속 껴요",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039810.1.3",
          "form": "근데 쌤들은 촬영때는 벗어요ㅜ",
          "original_form": "근데 쌤들은 촬영때는 벗어요ㅜ",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039810.1.4",
          "form": "영상에 마스크끼고 가르칠수는 없어서 ᆢ",
          "original_form": "영상에 마스크끼고 가르칠수는 없어서 ᆢ",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039810.1.5",
          "form": "헐 인강도 끼는줄 알았어요",
          "original_form": "헐 인강도 끼는줄 알았어요",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039810.1.6",
          "form": "무섭긴 하네요",
          "original_form": "무섭긴 하네요",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039810.1.7",
          "form": "촬영때는 안껴요ㅜㅜ",
          "original_form": "촬영때는 안껴요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039810.1.8",
          "form": "촬영 오실때랑",
          "original_form": "촬영 오실때랑",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039810.1.9",
          "form": "끝났을때 다시 끼세요",
          "original_form": "끝났을때 다시 끼세요",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039810.1.10",
          "form": "ebs쌤이 마스크끼고 강의하면 이상하잖아요ㅎ",
          "original_form": "ebs쌤이 마스크끼고 강의하면 이상하잖아요ㅎ",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039810.1.11",
          "form": "발음도 뭉개지고",
          "original_form": "발음도 뭉개지고",
          "speaker_id": "1",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039810.1.12",
          "form": "가둬놓고 찍어야겠어요 ㅜㅜ",
          "original_form": "가둬놓고 찍어야겠어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039810.1.13",
          "form": "카메라를 고정시켜놓고 할순 없나봐요",
          "original_form": "카메라를 고정시켜놓고 할순 없나봐요",
          "speaker_id": "2",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039810.1.14",
          "form": "네네 방하나에 한분 넣어두고 찍는데",
          "original_form": "네네 방하나에 한분 넣어두고 찍는데",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039810.1.15",
          "form": "촬영자가 한명은 있어야해요",
          "original_form": "촬영자가 한명은 있어야해요",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039810.1.16",
          "form": "NG날수도 있고 녹화버튼 눌러야하니까요",
          "original_form": "NG날수도 있고 녹화버튼 눌러야하니까요",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039810.1.17",
          "form": "아 글네요",
          "original_form": "아 글네요",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039810.1.18",
          "form": "편집으로 하고싶다 ㅎㅎ",
          "original_form": "편집으로 하고싶다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039810.1.19",
          "form": "카메라는 거의 고정이예요 ㅎ",
          "original_form": "카메라는 거의 고정이예요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039810.1.20",
          "form": "편집도 편집점 표시해놔야 편해요",
          "original_form": "편집도 편집점 표시해놔야 편해요",
          "speaker_id": "1",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039810.1.21",
          "form": "편집은 다른분이해요?",
          "original_form": "편집은 다른분이해요?",
          "speaker_id": "2",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100039810.1.22",
          "form": "편집도 하긴해요ㅜㅜㅜ",
          "original_form": "편집도 하긴해요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100039810.1.23",
          "form": "다른분이 했으면 좋겠지만 ᆢ",
          "original_form": "다른분이 했으면 좋겠지만 ᆢ",
          "speaker_id": "1",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100039810.1.24",
          "form": "인력이 모자라요ㅜㅜㅜ",
          "original_form": "인력이 모자라요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100039810.1.25",
          "form": "까페는 인력충분하세요?",
          "original_form": "까페는 인력충분하세요?",
          "speaker_id": "1",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039810.1.26",
          "form": "셋이서 일하는데 쫌힘들어요... 거기다 한명쉬면 둘이서 일하니",
          "original_form": "셋이서 일하는데 쫌힘들어요... 거기다 한명쉬면 둘이서 일하니",
          "speaker_id": "2",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039810.1.27",
          "form": "어디나 인력은 모자라네요ㅜㅜ",
          "original_form": "어디나 인력은 모자라네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039810.1.28",
          "form": "까페 큰곳인가요?",
          "original_form": "까페 큰곳인가요?",
          "speaker_id": "1",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039810.1.29",
          "form": "400명 있는건물인데",
          "original_form": "400명 있는건물인데",
          "speaker_id": "2",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100039810.1.30",
          "form": "외부인이 많이온다는",
          "original_form": "외부인이 많이온다는",
          "speaker_id": "2",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100039810.1.31",
          "form": "헉 400명ㆍᆢ진짜 힘드시겠어요ㅜ",
          "original_form": "헉 400명ㆍᆢ진짜 힘드시겠어요ㅜ",
