      "utterance": [
        {
          "id": "MDRW2100040198.1.1",
          "form": "떡볶이 먹었습니다 히히",
          "original_form": "떡볶이 먹었습니다 히히",
          "speaker_id": "2",
          "time": "20210905 11:31"
        },
        {
          "id": "MDRW2100040198.1.2",
          "form": "떡볶이와 오뎅탕",
          "original_form": "떡볶이와 오뎅탕",
          "speaker_id": "2",
          "time": "20210905 11:31"
        },
        {
          "id": "MDRW2100040198.1.3",
          "form": "name1님은 오늘 뭐 드셔요?",
          "original_form": "&name1&님은 오늘 뭐 드셔요?",
          "speaker_id": "2",
          "time": "20210905 11:31"
        },
        {
          "id": "MDRW2100040198.1.4",
          "form": "ㅎ..넘늦게봤네여",
          "original_form": "ㅎ..넘늦게봤네여",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100040198.1.5",
          "form": "전 오늘 잡채랑 밀푀유나베 케이크 엄청먹었어요",
          "original_form": "전 오늘 잡채랑 밀푀유나베 케이크 엄청먹었어요",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100040198.1.6",
          "form": "헐 맛있겠어요 ㅋㅋㅋㅋ",
          "original_form": "헐 맛있겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:13"
        },
        {
          "id": "MDRW2100040198.1.7",
          "form": "늦게보셔도 괜첞슴당 히히",
          "original_form": "늦게보셔도 괜첞슴당 히히",
          "speaker_id": "2",
          "time": "20210907 08:13"
        },
        {
          "id": "MDRW2100040198.1.8",
          "form": "잡채라니... 제 최애음식...",
          "original_form": "잡채라니... 제 최애음식...",
          "speaker_id": "2",
          "time": "20210907 08:14"
        },
        {
          "id": "MDRW2100040198.1.9",
          "form": "ㅎㅎ저도 잡채 넘좋아해요",
          "original_form": "ㅎㅎ저도 잡채 넘좋아해요",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100040198.1.10",
          "form": "오늘 백신2차맞고왔네요 백신푸드 추천해주실거있나여",
          "original_form": "오늘 백신2차맞고왔네요 백신푸드 추천해주실거있나여",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100040198.1.11",
          "form": "오... 백신맞고는 진짜 죽같은거 먹어야하는거 아닌가유",
          "original_form": "오... 백신맞고는 진짜 죽같은거 먹어야하는거 아닌가유",
          "speaker_id": "2",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100040198.1.12",
          "form": "든든하게먹어쥬는게 좋다더라구여",
          "original_form": "든든하게먹어쥬는게 좋다더라구여",
          "speaker_id": "1",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100040198.1.13",
          "form": "영양섭취잘해주면된대서 일단 삼겹살샀습니다 ㅎㅎ",
          "original_form": "영양섭취잘해주면된대서 일단 삼겹살샀습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100040198.1.14",
          "form": "어머세상에 너무좋은 선택이십니다",
          "original_form": "어머세상에 너무좋은 선택이십니다",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100040198.1.15",
          "form": "구워드시나유",
          "original_form": "구워드시나유",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100040198.1.16",
          "form": "ㅋㅋㅋㅋ네네 에프돌릴예정입니다",
          "original_form": "ㅋㅋㅋㅋ네네 에프돌릴예정입니다",
          "speaker_id": "1",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100040198.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 크으 멋진선택",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 크으 멋진선택",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100040198.1.18",
          "form": "저는 저녁은 패수하려구요",
          "original_form": "저는 저녁은 패수하려구요",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100040198.1.19",
          "form": "입맛이없으신가여ㅜ",
          "original_form": "입맛이없으신가여ㅜ",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100040198.1.20",
          "form": "일하는중이라서요ㅜㅜ",
          "original_form": "일하는중이라서요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100040198.1.21",
          "form": "원래 일하면 입맛이 뚝 떨어지더라구요 ㅋㅋㅋㅋ",
          "original_form": "원래 일하면 입맛이 뚝 떨어지더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100040198.1.22",
          "form": "아이고ㅜㅜ그쳐 일늦게까지하시네여 ㅜㅜㅜㅜㅜ그래도 뭔갈넣어야 힘이날텐데요",
          "original_form": "아이고ㅜㅜ그쳐 일늦게까지하시네여 ㅜㅜㅜㅜㅜ그래도 뭔갈넣어야 힘이날텐데요",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100040198.1.23",
          "form": "그건 글킨한데 오히려 먹으면 배탈나는 경우가 많더라구요ㅜㅜ",
          "original_form": "그건 글킨한데 오히려 먹으면 배탈나는 경우가 많더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100040198.1.24",
          "form": "그래서 점심을 든든하게 먹습니다 히히",
          "original_form": "그래서 점심을 든든하게 먹습니다 히히",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100040198.1.25",
          "form": "출근이 오후거든요 ㅇ.<",
          "original_form": "출근이 오후거든요 ㅇ.<",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100040198.1.26",
          "form": "아하 그런반전이!!",
          "original_form": "아하 그런반전이!!",
