      "utterance": [
        {
          "id": "MDRW2100034360.1.1",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100034360.1.2",
          "form": "ㅋㅋㅋㅋ 추천드립니다",
          "original_form": "ㅋㅋㅋㅋ 추천드립니다",
          "speaker_id": "1",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100034360.1.3",
          "form": "제목이랑도 연관되어있나보네요",
          "original_form": "제목이랑도 연관되어있나보네요",
          "speaker_id": "2",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100034360.1.4",
          "form": "이런ㅠ",
          "original_form": "이런ㅠ",
          "speaker_id": "2",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100034360.1.5",
          "form": "제목이요??",
          "original_form": "제목이요??",
          "speaker_id": "1",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100034360.1.6",
          "form": "제목이랑 내용이",
          "original_form": "제목이랑 내용이",
          "speaker_id": "2",
          "time": "20210828 00:23"
        },
        {
          "id": "MDRW2100034360.1.7",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210828 00:23"
        },
        {
          "id": "MDRW2100034360.1.8",
          "form": "채울수없는 맞죠?",
          "original_form": "채울수없는 맞죠?",
          "speaker_id": "2",
          "time": "20210828 00:24"
        },
        {
          "id": "MDRW2100034360.1.9",
          "form": "네 맞아요 ㅋㅋㅋ",
          "original_form": "네 맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100034360.1.10",
          "form": "어떤 장르좋아하세요?",
          "original_form": "어떤 장르좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100034360.1.11",
          "form": "요새 음악은 뭐들으세요?",
          "original_form": "요새 음악은 뭐들으세요?",
          "speaker_id": "2",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100034360.1.12",
          "form": "드라마 장르요?",
          "original_form": "드라마 장르요?",
          "speaker_id": "2",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100034360.1.13",
          "form": "넵 ㅋㅋㅋㅋ",
          "original_form": "넵 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100034360.1.14",
          "form": "저는 퀸덤이요!",
          "original_form": "저는 퀸덤이요!",
          "speaker_id": "1",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100034360.1.15",
          "form": "저는 로코도 좋고 추리? 이런쪽 좋아해요",
          "original_form": "저는 로코도 좋고 추리? 이런쪽 좋아해요",
          "speaker_id": "2",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100034360.1.16",
          "form": "레드벨벳  노래요?",
          "original_form": "레드벨벳  노래요?",
          "speaker_id": "2",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100034360.1.17",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100034360.1.18",
          "form": "오 추천해주실만한게 있나요?",
          "original_form": "오 추천해주실만한게 있나요?",
          "speaker_id": "1",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100034360.1.19",
          "form": "마더라고 티비엔 드라마인데",
          "original_form": "마더라고 티비엔 드라마인데",
          "speaker_id": "2",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100034360.1.20",
          "form": "아 들어봤어요!",
          "original_form": "아 들어봤어요!",
          "speaker_id": "1",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100034360.1.21",
          "form": "초반에는 막 불륜얘기나와서",
          "original_form": "초반에는 막 불륜얘기나와서",
          "speaker_id": "2",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100034360.1.22",
          "form": "그렇고 그런얘기구나 했는데",
          "original_form": "그렇고 그런얘기구나 했는데",
          "speaker_id": "2",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100034360.1.23",
          "form": "좋은드라마더라구요",
          "original_form": "좋은드라마더라구요",
          "speaker_id": "2",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100034360.1.24",
          "form": "약간 여자들이 다하는 드라마랄까요?\n여자들끼리  연대하는게 좋더라구요",
          "original_form": "약간 여자들이 다하는 드라마랄까요?\n여자들끼리  연대하는게 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100034360.1.25",
          "form": "여자들얘기 좋아해요",
          "original_form": "여자들얘기 좋아해요",
          "speaker_id": "1",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100034360.1.26",
          "form": "나중에 한번 찾아봐야겠어요",
          "original_form": "나중에 한번 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 00:30"
        }
      ]
    }
  ]
