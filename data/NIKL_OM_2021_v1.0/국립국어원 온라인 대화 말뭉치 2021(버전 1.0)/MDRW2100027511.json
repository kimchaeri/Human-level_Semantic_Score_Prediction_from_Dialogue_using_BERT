        {
          "id": "MDRW2100027511.1.3",
          "form": "전 홈트요",
          "original_form": "전 홈트요",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027511.1.4",
          "form": "오~ 유투브로 하시는 건가요?",
          "original_form": "오~ 유투브로 하시는 건가요?",
          "speaker_id": "2",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100027511.1.5",
          "form": "네 봐요",
          "original_form": "네 봐요",
          "speaker_id": "1",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100027511.1.6",
          "form": "어떤 채널 보세요?",
          "original_form": "어떤 채널 보세요?",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027511.1.7",
          "form": "땅크봐요",
          "original_form": "땅크봐요",
          "speaker_id": "1",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100027511.1.8",
          "form": "오~ 그 채널 많이들 보더라구요",
          "original_form": "오~ 그 채널 많이들 보더라구요",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100027511.1.9",
          "form": "운동 많이 된다구",
          "original_form": "운동 많이 된다구",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100027511.1.10",
          "form": "전 엄두도 못내지만...",
          "original_form": "전 엄두도 못내지만...",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
