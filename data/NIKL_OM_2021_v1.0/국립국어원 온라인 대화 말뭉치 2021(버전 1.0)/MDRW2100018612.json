      "utterance": [
        {
          "id": "MDRW2100018612.1.1",
          "form": "게임좋아하시나요?",
          "original_form": "게임좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100018612.1.2",
          "form": "저근데 솔직히 게임 모르는데요..",
          "original_form": "저근데 솔직히 게임 모르는데요..",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100018612.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100018612.1.4",
          "form": "어 게임을 자주하지는 않아서",
          "original_form": "어 게임을 자주하지는 않아서",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100018612.1.5",
          "form": "어.. 크레이지 아케이드 해보셨나요ㅋㅋㅋㄲ",
          "original_form": "어.. 크레이지 아케이드 해보셨나요ㅋㅋㅋㄲ",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100018612.1.6",
          "form": "국민 초딩게임..",
          "original_form": "국민 초딩게임..",
          "speaker_id": "1",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100018612.1.7",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "2",
          "time": "20210827 15:24"
        },
        {
          "id": "MDRW2100018612.1.8",
          "form": "크아를 젤 잼게했고",
          "original_form": "크아를 젤 잼게했고",
          "speaker_id": "2",
          "time": "20210827 15:25"
        },
        {
          "id": "MDRW2100018612.1.9",
          "form": "게임하세요?",
          "original_form": "게임하세요?",
          "speaker_id": "2",
          "time": "20210827 15:25"
        },
        {
          "id": "MDRW2100018612.1.10",
          "form": "저는 배그 오버워치 몇번해보긴했는데",
          "original_form": "저는 배그 오버워치 몇번해보긴했는데",
          "speaker_id": "2",
          "time": "20210827 15:25"
        },
        {
          "id": "MDRW2100018612.1.11",
          "form": "아니요 저도 게임쪽은 잘 몰라서ㅋㅋㅋㅋ",
          "original_form": "아니요 저도 게임쪽은 잘 몰라서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:25"
        },
        {
          "id": "MDRW2100018612.1.12",
          "form": "못하는 제자신을 보기싫어서 때려쳤습니닼ㅋㅋㅋㅋ",
          "original_form": "못하는 제자신을 보기싫어서 때려쳤습니닼ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:25"
        },
        {
          "id": "MDRW2100018612.1.13",
          "form": "뭔지 알거같습니다",
          "original_form": "뭔지 알거같습니다",
          "speaker_id": "1",
          "time": "20210827 15:25"
        },
        {
          "id": "MDRW2100018612.1.14",
          "form": "앞선 의지에 그렇지 못한 실력..",
          "original_form": "앞선 의지에 그렇지 못한 실력..",
          "speaker_id": "1",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100018612.1.15",
          "form": "그게 팀전이면.. 더욱 더..",
          "original_form": "그게 팀전이면.. 더욱 더..",
          "speaker_id": "1",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100018612.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ 방송도 안보시는거죠?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 방송도 안보시는거죠?",
          "speaker_id": "2",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100018612.1.17",
          "form": "방송..네.. 혹시 게임 관련 방송을 보시나요",
          "original_form": "방송..네.. 혹시 게임 관련 방송을 보시나요",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100018612.1.18",
          "form": "아뇨 저도 안봐요",
          "original_form": "아뇨 저도 안봐요",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100018612.1.19",
          "form": "아 배그는 가끔 웃긴영상이라고 올라오는거",
          "original_form": "아 배그는 가끔 웃긴영상이라고 올라오는거",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100018612.1.20",
          "form": "그거 말고는 안보는거같네요",
          "original_form": "그거 말고는 안보는거같네요",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100018612.1.21",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100018612.1.22",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100018612.1.23",
