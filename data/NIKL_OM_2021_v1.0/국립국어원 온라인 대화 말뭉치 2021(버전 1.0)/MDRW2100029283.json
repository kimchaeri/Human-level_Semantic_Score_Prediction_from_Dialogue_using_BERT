      "utterance": [
        {
          "id": "MDRW2100029283.1.1",
          "form": "탄산좋아하세요?",
          "original_form": "탄산좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100029283.1.2",
          "form": "탄산은 잘 안먹어요 톡쏘는건 좋은데 달아서요 ㅋㅋ",
          "original_form": "탄산은 잘 안먹어요 톡쏘는건 좋은데 달아서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100029283.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100029283.1.4",
          "form": "네 저 한때 콜라 한페트씩먹었어요 매일",
          "original_form": "네 저 한때 콜라 한페트씩먹었어요 매일",
          "speaker_id": "1",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100029283.1.5",
          "form": "어디 콜라 좋아하세요?",
          "original_form": "어디 콜라 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029283.1.6",
          "form": "역시 코카콜라 시려나??",
          "original_form": "역시 코카콜라 시려나??",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029283.1.7",
          "form": "코카랑 펩시둘다좋아해요!",
          "original_form": "코카랑 펩시둘다좋아해요!",
          "speaker_id": "1",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029283.1.8",
          "form": "찐사랑 이시네요",
          "original_form": "찐사랑 이시네요",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029283.1.9",
          "form": "코카가 탄산이 더쎈거같고\n펩시가 더 단거같아요",
          "original_form": "코카가 탄산이 더쎈거같고\n펩시가 더 단거같아요",
          "speaker_id": "1",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029283.1.10",
          "form": "저칼로리도 좋아하세요?",
          "original_form": "저칼로리도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029283.1.11",
          "form": "아뇨,,",
          "original_form": "아뇨,,",
          "speaker_id": "1",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029283.1.12",
          "form": "ㅋㄱㅋㄱㄱㄱㄱㄱㅋㄱㄱ",
          "original_form": "ㅋㄱㅋㄱㄱㄱㄱㄱㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029283.1.13",
          "form": "맛이 좀 다르죠",
          "original_form": "맛이 좀 다르죠",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029283.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029283.1.15",
          "form": "요즘 근데 제로콜라먹으려고",
          "original_form": "요즘 근데 제로콜라먹으려고",
          "speaker_id": "1",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029283.1.16",
          "form": "노력중이에요",
          "original_form": "노력중이에요",
          "speaker_id": "1",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029283.1.17",
          "form": "좀 탄산이 덜한 느낌",
          "original_form": "좀 탄산이 덜한 느낌",
          "speaker_id": "1",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029283.1.18",
          "form": "콜라 좋아하시는구나 전 사이다가 더 좋아요",
          "original_form": "콜라 좋아하시는구나 전 사이다가 더 좋아요",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029283.1.19",
          "form": "체리 코카콜라였나",
          "original_form": "체리 코카콜라였나",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029283.1.20",
          "form": "닥터페퍼?",
          "original_form": "닥터페퍼?",
          "speaker_id": "1",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029283.1.21",
          "form": "그거 드셔보신적 있으세요??",
          "original_form": "그거 드셔보신적 있으세요??",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029283.1.22",
          "form": "같은건가요?!",
          "original_form": "같은건가요?!",
          "speaker_id": "1",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029283.1.23",
          "form": "시즌 상품으로 나왔던거 같은데",
          "original_form": "시즌 상품으로 나왔던거 같은데",
          "speaker_id": "2",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029283.1.24",
          "form": "보면서 맛있을까 했거든요",
          "original_form": "보면서 맛있을까 했거든요",
          "speaker_id": "2",
          "time": "20210903 01:06"
        }
      ]
