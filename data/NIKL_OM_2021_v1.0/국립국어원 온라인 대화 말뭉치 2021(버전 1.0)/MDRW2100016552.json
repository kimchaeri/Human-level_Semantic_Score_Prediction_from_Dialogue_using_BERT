        {
          "id": "MDRW2100016552.1.1",
          "form": "좋아하는 연예인 있으세요?",
          "original_form": "좋아하는 연예인 있으세요?",
          "speaker_id": "1",
          "time": "20210826 13:43"
        },
        {
          "id": "MDRW2100016552.1.2",
          "form": "저는 SM 아이돌 좋아해요",
          "original_form": "저는 SM 아이돌 좋아해요",
          "speaker_id": "2",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100016552.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100016552.1.4",
          "form": "저도 sm좋아해요! 전 태연이랑 nct좋아해요ㅎㅎ",
          "original_form": "저도 sm좋아해요! 전 태연이랑 nct좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:28"
        },
        {
          "id": "MDRW2100016552.1.5",
          "form": "오옹 저두 nct 좋아해요",
          "original_form": "오옹 저두 nct 좋아해요",
          "speaker_id": "2",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100016552.1.6",
          "form": "최애가 누군가요??",
          "original_form": "최애가 누군가요??",
          "speaker_id": "2",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100016552.1.7",
          "form": "마크요ㅎㅎ",
          "original_form": "마크요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100016552.1.8",
          "form": "name2님 최애는 제가 맞혀봐도 될까요?",
          "original_form": "&name2&님 최애는 제가 맞혀봐도 될까요?",
          "speaker_id": "1",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100016552.1.9",
          "form": "넵!ㅋㅋㅋㅋ",
          "original_form": "넵!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016552.1.10",
          "form": "마크인가요?",
          "original_form": "마크인가요?",
          "speaker_id": "1",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016552.1.11",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "2",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100016552.1.12",
          "form": "마크도 멋진 애옹이지만",
          "original_form": "마크도 멋진 애옹이지만",
          "speaker_id": "2",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100016552.1.13",
          "form": "땡입니당",
          "original_form": "땡입니당",
          "speaker_id": "2",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100016552.1.14",
          "form": "전 해프에요ㅋㅋㅋㅋ",
          "original_form": "전 해프에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100016552.1.15",
          "form": "해찬이요??",
          "original_form": "해찬이요??",
          "speaker_id": "1",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100016552.1.16",
          "form": "네넹ㅋㅋㅋㅋㅋ",
          "original_form": "네넹ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100016552.1.17",
          "form": "입덕 계기는 뭔가요?",
          "original_form": "입덕 계기는 뭔가요?",
          "speaker_id": "1",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100016552.1.18",
          "form": "전 슴덕이라서 저절로 자연스럽게 접하게 된 것같아요",
          "original_form": "전 슴덕이라서 저절로 자연스럽게 접하게 된 것같아요",
          "speaker_id": "2",
