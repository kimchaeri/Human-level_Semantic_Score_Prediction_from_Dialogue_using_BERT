        },
        {
          "id": "MDRW2100036498.1.2",
          "form": "하이요",
          "original_form": "하이요",
          "speaker_id": "2",
          "time": "20210831 00:21"
        },
        {
          "id": "MDRW2100036498.1.3",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 00:45"
        },
        {
          "id": "MDRW2100036498.1.4",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210831 00:52"
        },
        {
          "id": "MDRW2100036498.1.5",
          "form": "여행도 자주하시나요?",
          "original_form": "여행도 자주하시나요?",
          "speaker_id": "1",
          "time": "20210831 01:08"
        },
        {
          "id": "MDRW2100036498.1.6",
          "form": "코로나전엔 그랫죺ㅎ",
          "original_form": "코로나전엔 그랫죺ㅎ",
          "speaker_id": "2",
          "time": "20210831 20:33"
        },
        {
          "id": "MDRW2100036498.1.7",
          "form": "어디어디다녀오셨어요",
          "original_form": "어디어디다녀오셨어요",
          "speaker_id": "1",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100036498.1.8",
          "form": "전국 여기저기요  ㅎㅎㅎ",
          "original_form": "전국 여기저기요  ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100036498.1.9",
          "form": "오 어디가 가장좋으셨어요",
          "original_form": "오 어디가 가장좋으셨어요",
          "speaker_id": "1",
          "time": "20210831 21:10"
        },
        {
          "id": "MDRW2100036498.1.10",
          "form": "전 동해요",
          "original_form": "전 동해요",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100036498.1.11",
          "form": "저도 동해넘좋아해여 사실바다는 다좋아해요",
          "original_form": "저도 동해넘좋아해여 사실바다는 다좋아해요",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100036498.1.12",
          "form": "동해쪽 맛집도가셨나요",
          "original_form": "동해쪽 맛집도가셨나요",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100036498.1.13",
          "form": "맛집은  못가본거 같아욜ㄹ",
          "original_form": "맛집은  못가본거 같아욜ㄹ",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100036498.1.14",
          "form": "아하 전국다니실때 가장기억에남은맛집없으셨나요",
          "original_form": "아하 전국다니실때 가장기억에남은맛집없으셨나요",
          "speaker_id": "1",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100036498.1.15",
          "form": "전 순천  만석꾼  이요",
          "original_form": "전 순천  만석꾼  이요",
          "speaker_id": "2",
          "time": "20210831 22:38"
