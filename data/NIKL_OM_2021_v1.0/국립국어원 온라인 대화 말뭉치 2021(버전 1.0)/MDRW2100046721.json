      "utterance": [
        {
          "id": "MDRW2100046721.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 15:46"
        },
        {
          "id": "MDRW2100046721.1.2",
          "form": "안녕하세요~ 오늘 저녁에 약속 있으신가요?",
          "original_form": "안녕하세요~ 오늘 저녁에 약속 있으신가요?",
          "speaker_id": "2",
          "time": "20211104 15:48"
        },
        {
          "id": "MDRW2100046721.1.3",
          "form": "저녁에 특별한 일은 없습니다",
          "original_form": "저녁에 특별한 일은 없습니다",
          "speaker_id": "1",
          "time": "20211104 15:49"
        },
        {
          "id": "MDRW2100046721.1.4",
          "form": "그냥 따뜻한 물에 샤워하고 누워있으려구요",
          "original_form": "그냥 따뜻한 물에 샤워하고 누워있으려구요",
          "speaker_id": "1",
          "time": "20211104 15:49"
        },
        {
          "id": "MDRW2100046721.1.5",
          "form": "오오 생각만해도 완전 천국이네요!!",
          "original_form": "오오 생각만해도 완전 천국이네요!!",
          "speaker_id": "2",
          "time": "20211104 15:50"
        },
        {
          "id": "MDRW2100046721.1.6",
          "form": "드라마나 영화 한편 같이 보는 건 어떠세요",
          "original_form": "드라마나 영화 한편 같이 보는 건 어떠세요",
          "speaker_id": "2",
          "time": "20211104 15:50"
        },
        {
          "id": "MDRW2100046721.1.7",
          "form": "요즘 부쩍 체력이 떨어지고 있어서  야외활동이 적네요",
          "original_form": "요즘 부쩍 체력이 떨어지고 있어서  야외활동이 적네요",
          "speaker_id": "1",
          "time": "20211104 15:50"
        },
        {
          "id": "MDRW2100046721.1.8",
          "form": "추천좀 해주세요",
          "original_form": "추천좀 해주세요",
          "speaker_id": "1",
          "time": "20211104 15:50"
        },
        {
          "id": "MDRW2100046721.1.9",
          "form": "영화 거의 안 봐서 뭐가 재미있는지",
          "original_form": "영화 거의 안 봐서 뭐가 재미있는지",
          "speaker_id": "1",
          "time": "20211104 15:51"
        },
        {
          "id": "MDRW2100046721.1.10",
          "form": "아무래도 코로나 때문에 활동량이 줄어서 그럴지도요",
          "original_form": "아무래도 코로나 때문에 활동량이 줄어서 그럴지도요",
          "speaker_id": "2",
          "time": "20211104 15:51"
        },
        {
          "id": "MDRW2100046721.1.11",
          "form": "모르겠네요",
          "original_form": "모르겠네요",
          "speaker_id": "1",
          "time": "20211104 15:51"
        },
        {
          "id": "MDRW2100046721.1.12",
          "form": "음.. 어떤 장르 좋아하세요?",
          "original_form": "음.. 어떤 장르 좋아하세요?",
          "speaker_id": "2",
          "time": "20211104 15:51"
        },
        {
          "id": "MDRW2100046721.1.13",
          "form": "공포",
          "original_form": "공포",
          "speaker_id": "1",
          "time": "20211104 15:51"
        },
        {
          "id": "MDRW2100046721.1.14",
          "form": "스릴러",
          "original_form": "스릴러",
          "speaker_id": "1",
          "time": "20211104 15:51"
        },
        {
          "id": "MDRW2100046721.1.15",
          "form": "좋아합니다",
          "original_form": "좋아합니다",
          "speaker_id": "1",
          "time": "20211104 15:51"
        },
        {
          "id": "MDRW2100046721.1.16",
          "form": "반전",
          "original_form": "반전",
          "speaker_id": "1",
          "time": "20211104 15:51"
        },
        {
          "id": "MDRW2100046721.1.17",
          "form": "이런거 좋아하네 요",
          "original_form": "이런거 좋아하네 요",
          "speaker_id": "1",
          "time": "20211104 15:51"
        },
        {
          "id": "MDRW2100046721.1.18",
          "form": "혹시 넷플에 난노 보셨나요?!",
          "original_form": "혹시 넷플에 난노 보셨나요?!",
          "speaker_id": "2",
          "time": "20211104 15:52"
        },
        {
          "id": "MDRW2100046721.1.19",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20211104 15:52"
        },
        {
          "id": "MDRW2100046721.1.20",
          "form": "넷플 없네요",
          "original_form": "넷플 없네요",
          "speaker_id": "1",
          "time": "20211104 15:52"
        },
        {
          "id": "MDRW2100046721.1.21",
          "form": "엇 그럼 사용하고계신 플랫폼 따로 있으신가요!",
          "original_form": "엇 그럼 사용하고계신 플랫폼 따로 있으신가요!",
          "speaker_id": "2",
          "time": "20211104 15:52"
        },
        {
          "id": "MDRW2100046721.1.22",
          "form": "아니요 ㅜㅜ",
          "original_form": "아니요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20211104 15:53"
        },
        {
          "id": "MDRW2100046721.1.23",
          "form": "그냥 유선방송에서 하는",
          "original_form": "그냥 유선방송에서 하는",
          "speaker_id": "1",
          "time": "20211104 15:53"
        },
        {
          "id": "MDRW2100046721.1.24",
          "form": "영화나 간혹 보는 게 답니다",
          "original_form": "영화나 간혹 보는 게 답니다",
          "speaker_id": "1",
          "time": "20211104 15:53"
        },
        {
          "id": "MDRW2100046721.1.25",
          "form": "넷플 한 달에 얼마인가요",
          "original_form": "넷플 한 달에 얼마인가요",
          "speaker_id": "1",
          "time": "20211104 15:53"
        },
        {
          "id": "MDRW2100046721.1.26",
          "form": "그럼 이번 기회에 넷플 한번 둘러보시는건 어떠신가요",
          "original_form": "그럼 이번 기회에 넷플 한번 둘러보시는건 어떠신가요",
          "speaker_id": "2",
          "time": "20211104 15:53"
        },
        {
          "id": "MDRW2100046721.1.27",
          "form": "첫달은 무료고",
          "original_form": "첫달은 무료고",
          "speaker_id": "2",
          "time": "20211104 15:53"
        },
        {
          "id": "MDRW2100046721.1.28",
          "form": "영화 한 편당 결제인가요",
          "original_form": "영화 한 편당 결제인가요",
          "speaker_id": "1",
          "time": "20211104 15:53"
        },
        {
          "id": "MDRW2100046721.1.29",
          "form": "아뇨 한달에 한번 만이천원? 결제하면 됩니다!",
          "original_form": "아뇨 한달에 한번 만이천원? 결제하면 됩니다!",
          "speaker_id": "2",
          "time": "20211104 15:54"
        },
        {
          "id": "MDRW2100046721.1.30",
          "form": "그맇군요",
          "original_form": "그맇군요",
          "speaker_id": "1",
          "time": "20211104 15:54"
        },
        {
          "id": "MDRW2100046721.1.31",
          "form": "그럼 무제한 시청 가능이요!",
          "original_form": "그럼 무제한 시청 가능이요!",
          "speaker_id": "2",
          "time": "20211104 15:54"
        },
        {
          "id": "MDRW2100046721.1.32",
          "form": "Ip tv필수겠죠?",
          "original_form": "Ip tv필수겠죠?",
          "speaker_id": "1",
          "time": "20211104 15:54"
        },
        {
          "id": "MDRW2100046721.1.33",
          "form": "아뇨 핸드폰만 있어도 되는걸요 ㅎㅎ",
          "original_form": "아뇨 핸드폰만 있어도 되는걸요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 15:54"
        },
        {
          "id": "MDRW2100046721.1.34",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20211104 15:54"
        },
        {
          "id": "MDRW2100046721.1.35",
          "form": "퇴근하고 영화나 드라마 틀어놓으면 정말 좋아요 ㅎㅎ",
          "original_form": "퇴근하고 영화나 드라마 틀어놓으면 정말 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 15:55"
        },
        {
          "id": "MDRW2100046721.1.36",
          "form": "그럼 인터넷 신청할 때 티비 없어도",
          "original_form": "그럼 인터넷 신청할 때 티비 없어도",
          "speaker_id": "1",
          "time": "20211104 15:55"
        },
        {
          "id": "MDRW2100046721.1.37",
          "form": "되겠네요",
          "original_form": "되겠네요",
          "speaker_id": "1",
          "time": "20211104 15:55"
        },
        {
          "id": "MDRW2100046721.1.38",
          "form": "네네! 가능합니다!!",
          "original_form": "네네! 가능합니다!!",
          "speaker_id": "2",
          "time": "20211104 15:55"
        },
        {
          "id": "MDRW2100046721.1.39",
          "form": "감사합니다!",
          "original_form": "감사합니다!",
          "speaker_id": "1",
          "time": "20211104 15:55"
        },
        {
          "id": "MDRW2100046721.1.40",
          "form": "아녜요 첫달 무료니까 한번 봐보시고 결정하시면 될것같아요! 공포 스릴러 영화 엄청 많거든요!",
          "original_form": "아녜요 첫달 무료니까 한번 봐보시고 결정하시면 될것같아요! 공포 스릴러 영화 엄청 많거든요!",
