      "utterance": [
        {
          "id": "MDRW2100041901.1.1",
          "form": "요즘학교는 오프라인하나요?!",
          "original_form": "요즘학교는 오프라인하나요?!",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041901.1.2",
          "form": "음 대학원은 다 온라인으로 하는 거 같아요 웬만하면?!",
          "original_form": "음 대학원은 다 온라인으로 하는 거 같아요 웬만하면?!",
          "speaker_id": "1",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100041901.1.3",
          "form": "name2님은 대학생이라고 하셨죠...? 아닌가!!!",
          "original_form": "&name2&님은 대학생이라고 하셨죠...? 아닌가!!!",
          "speaker_id": "1",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100041901.1.4",
          "form": "아뇨! 저는 졸업했습니다",
          "original_form": "아뇨! 저는 졸업했습니다",
          "speaker_id": "2",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100041901.1.5",
          "form": "아하!!!",
          "original_form": "아하!!!",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041901.1.6",
          "form": "교직원으로 일했었어요ㅋㅋ",
          "original_form": "교직원으로 일했었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041901.1.7",
          "form": "저도 졸업하기는 했는데,,, 머쓱",
          "original_form": "저도 졸업하기는 했는데,,, 머쓱",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041901.1.8",
          "form": "우와!!!",
          "original_form": "우와!!!",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041901.1.9",
          "form": "8월에 퇴사한,,,!",
          "original_form": "8월에 퇴사한,,,!",
          "speaker_id": "2",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041901.1.10",
          "form": "대박... 교직원이라니",
          "original_form": "대박... 교직원이라니",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041901.1.11",
          "form": "너무 신기해요!!!",
          "original_form": "너무 신기해요!!!",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041901.1.12",
          "form": "아녜요 저는 계약직이었어요,,,ㅋㄱㄲ",
          "original_form": "아녜요 저는 계약직이었어요,,,ㅋㄱㄲ",
          "speaker_id": "2",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041901.1.13",
          "form": "앗 근데 교직원이 대부분",
          "original_form": "앗 근데 교직원이 대부분",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041901.1.14",
          "form": "계약직이지요...?",
          "original_form": "계약직이지요...?",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041901.1.15",
          "form": "저도 취업자리 교직원 탐냈어서,,",
          "original_form": "저도 취업자리 교직원 탐냈어서,,",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041901.1.16",
          "form": "어 젊은 교직원은 거의없어요ㅠ",
          "original_form": "어 젊은 교직원은 거의없어요ㅠ",
          "speaker_id": "2",
          "time": "20210906 17:29"
        },
        {
          "id": "MDRW2100041901.1.17",
          "form": "아 그렇군요ㅠ",
          "original_form": "아 그렇군요ㅠ",
          "speaker_id": "1",
          "time": "20210906 17:29"
        },
        {
          "id": "MDRW2100041901.1.18",
          "form": "아무래도 어린사람들 굴려먹겄다는 고인물들의 농간,,,",
          "original_form": "아무래도 어린사람들 굴려먹겄다는 고인물들의 농간,,,",
          "speaker_id": "2",
          "time": "20210906 17:29"
