      "utterance": [
        {
          "id": "MDRW2100010081.1.1",
          "form": "달리기를 할 생각은 못해봤는데",
          "original_form": "달리기를 할 생각은 못해봤는데",
          "speaker_id": "2",
          "time": "20210823 14:02"
        },
        {
          "id": "MDRW2100010081.1.2",
          "form": "어쩌다 하게 된 거임??",
          "original_form": "어쩌다 하게 된 거임??",
          "speaker_id": "2",
          "time": "20210823 14:02"
        },
        {
          "id": "MDRW2100010081.1.3",
          "form": "런데이하라고",
          "original_form": "런데이하라고",
          "speaker_id": "1",
          "time": "20210823 14:03"
        },
        {
          "id": "MDRW2100010081.1.4",
          "form": "추천많이받았었는데",
          "original_form": "추천많이받았었는데",
          "speaker_id": "1",
          "time": "20210823 14:04"
        },
        {
          "id": "MDRW2100010081.1.5",
          "form": "귀찮아서안하다가",
          "original_form": "귀찮아서안하다가",
          "speaker_id": "1",
          "time": "20210823 14:04"
        },
        {
          "id": "MDRW2100010081.1.6",
          "form": "집에만있으면진심폐인될거같아서",
          "original_form": "집에만있으면진심폐인될거같아서",
          "speaker_id": "1",
          "time": "20210823 14:04"
        },
        {
          "id": "MDRW2100010081.1.7",
          "form": "하고있음",
          "original_form": "하고있음",
          "speaker_id": "1",
          "time": "20210823 14:04"
        },
        {
          "id": "MDRW2100010081.1.8",
          "form": "너하면",
          "original_form": "너하면",
          "speaker_id": "1",
          "time": "20210823 14:04"
        },
        {
          "id": "MDRW2100010081.1.9",
          "form": "그거친구? 그거 하자",
          "original_form": "그거친구? 그거 하자",
          "speaker_id": "1",
          "time": "20210823 14:04"
        },
        {
          "id": "MDRW2100010081.1.10",
          "form": "아 ㅇㅋㅇㅋ 다운 받으면 바로 알려줄게",
          "original_form": "아 ㅇㅋㅇㅋ 다운 받으면 바로 알려줄게",
          "speaker_id": "2",
          "time": "20210823 14:05"
        },
        {
          "id": "MDRW2100010081.1.11",
          "form": "런메이튼가",
          "original_form": "런메이튼가",
          "speaker_id": "1",
          "time": "20210823 14:05"
        },
        {
          "id": "MDRW2100010081.1.12",
          "form": "같이 뛰면 뭐가 좋아?",
          "original_form": "같이 뛰면 뭐가 좋아?",
          "speaker_id": "2",
          "time": "20210823 14:05"
        },
        {
          "id": "MDRW2100010081.1.13",
          "form": "그냥기분?",
          "original_form": "그냥기분?",
          "speaker_id": "1",
          "time": "20210823 14:05"
        },
        {
          "id": "MDRW2100010081.1.14",
          "form": "근데런메이트하면",
          "original_form": "근데런메이트하면",
          "speaker_id": "1",
          "time": "20210823 14:05"
        },
        {
          "id": "MDRW2100010081.1.15",
          "form": "코칭이런거해주는ai있는데",
          "original_form": "코칭이런거해주는ai있는데",
          "speaker_id": "1",
          "time": "20210823 14:06"
        },
        {
          "id": "MDRW2100010081.1.16",
          "form": "말이너무많음",
          "original_form": "말이너무많음",
          "speaker_id": "1",
          "time": "20210823 14:06"
        },
        {
          "id": "MDRW2100010081.1.17",
          "form": "말이 많아??",
          "original_form": "말이 많아??",
          "speaker_id": "2",
          "time": "20210823 14:06"
        },
        {
          "id": "MDRW2100010081.1.18",
          "form": "거슬릴 정도야??",
          "original_form": "거슬릴 정도야??",
          "speaker_id": "2",
          "time": "20210823 14:06"
        },
        {
          "id": "MDRW2100010081.1.19",
          "form": "일부러지루하지말라고말해주는거같긴한데",
          "original_form": "일부러지루하지말라고말해주는거같긴한데",
          "speaker_id": "1",
          "time": "20210823 14:07"
        },
        {
          "id": "MDRW2100010081.1.20",
          "form": "달리기하면힘드니까",
          "original_form": "달리기하면힘드니까",
          "speaker_id": "1",
          "time": "20210823 14:07"
        },
        {
          "id": "MDRW2100010081.1.21",
          "form": "그냥짜증남",
          "original_form": "그냥짜증남",
          "speaker_id": "1",
          "time": "20210823 14:07"
        },
        {
          "id": "MDRW2100010081.1.22",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:07"
        },
        {
          "id": "MDRW2100010081.1.23",
          "form": "아아 아닠ㅌㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아아 아닠ㅌㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 14:07"
        },
        {
          "id": "MDRW2100010081.1.24",
          "form": "끄진 못해?",
          "original_form": "끄진 못해?",
          "speaker_id": "2",
          "time": "20210823 14:07"
        },
        {
          "id": "MDRW2100010081.1.25",
          "form": "모르겠ㅇ음",
          "original_form": "모르겠ㅇ음",
          "speaker_id": "1",
          "time": "20210823 14:07"
        },
        {
          "id": "MDRW2100010081.1.26",
          "form": "그기능있을수도",
          "original_form": "그기능있을수도",
          "speaker_id": "1",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100010081.1.27",
          "form": "찾아보고알려줌",
          "original_form": "찾아보고알려줌",
          "speaker_id": "1",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100010081.1.28",
          "form": "너수영할줄",
          "original_form": "너수영할줄",
          "speaker_id": "1",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100010081.1.29",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100010081.1.30",
          "form": "냐",
          "original_form": "냐",
          "speaker_id": "1",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100010081.1.31",
          "form": "아니 못해",
          "original_form": "아니 못해",
          "speaker_id": "2",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100010081.1.32",
          "form": "배운 적은 있으나 못해",
          "original_form": "배운 적은 있으나 못해",
          "speaker_id": "2",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100010081.1.33",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100010081.1.34",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "1",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100010081.1.35",
          "form": "언제?",
          "original_form": "언제?",
          "speaker_id": "1",
          "time": "20210823 14:09"
        },
        {
          "id": "MDRW2100010081.1.36",
          "form": "배웠는데",
          "original_form": "배웠는데",
          "speaker_id": "1",
          "time": "20210823 14:09"
        },
        {
          "id": "MDRW2100010081.1.37",
          "form": "나 초등학생 때",
          "original_form": "나 초등학생 때",
          "speaker_id": "2",
          "time": "20210823 14:09"
        }
      ]
    }
