      "utterance": [
        {
          "id": "MDRW2100041474.1.1",
          "form": "현대백화점에 가면 한 달에 두 개씩 음료수를 줘",
          "original_form": "현대백화점에 가면 한 달에 두 개씩 음료수를 줘",
          "speaker_id": "2",
          "time": "20210906 01:00"
        },
        {
          "id": "MDRW2100041474.1.2",
          "form": "어떤 후보가 있니",
          "original_form": "어떤 후보가 있니",
          "speaker_id": "1",
          "time": "20210906 01:00"
        },
        {
          "id": "MDRW2100041474.1.3",
          "form": "너도 들어보고 골라봐",
          "original_form": "너도 들어보고 골라봐",
          "speaker_id": "2",
          "time": "20210906 01:00"
        },
        {
          "id": "MDRW2100041474.1.4",
          "form": "그렇게 말해봐",
          "original_form": "그렇게 말해봐",
          "speaker_id": "1",
          "time": "20210906 01:00"
        },
        {
          "id": "MDRW2100041474.1.5",
          "form": "일단 핫초코와 아이스 초코가 있어",
          "original_form": "일단 핫초코와 아이스 초코가 있어",
          "speaker_id": "2",
          "time": "20210906 01:00"
        },
        {
          "id": "MDRW2100041474.1.6",
          "form": "우와 그거 진짜 맛있겠다",
          "original_form": "우와 그거 진짜 맛있겠다",
          "speaker_id": "1",
          "time": "20210906 01:00"
        },
        {
          "id": "MDRW2100041474.1.7",
          "form": "심지어 스위스 미스 거야",
          "original_form": "심지어 스위스 미스 거야",
          "speaker_id": "2",
          "time": "20210906 01:00"
        },
        {
          "id": "MDRW2100041474.1.8",
          "form": "나 거기 거 부드러워서 진짜 좋아하는",
          "original_form": "나 거기 거 부드러워서 진짜 좋아하는",
          "speaker_id": "1",
          "time": "20210906 01:00"
        },
        {
          "id": "MDRW2100041474.1.9",
          "form": "그래서 여의치 않을 때는 그냥 파우치로 받아와",
          "original_form": "그래서 여의치 않을 때는 그냥 파우치로 받아와",
          "speaker_id": "2",
          "time": "20210906 01:01"
        },
        {
          "id": "MDRW2100041474.1.10",
          "form": "집에서 우유에 타 먹으면 정말 맛있겠다",
          "original_form": "집에서 우유에 타 먹으면 정말 맛있겠다",
          "speaker_id": "1",
          "time": "20210906 01:01"
        },
        {
          "id": "MDRW2100041474.1.11",
          "form": "그거 말고는 도라지차가 있어",
          "original_form": "그거 말고는 도라지차가 있어",
          "speaker_id": "2",
          "time": "20210906 01:01"
        },
        {
          "id": "MDRW2100041474.1.12",
          "form": "아 그건 별론데",
          "original_form": "아 그건 별론데",
          "speaker_id": "1",
          "time": "20210906 01:01"
        },
        {
          "id": "MDRW2100041474.1.13",
          "form": "얼그레이 도 있지",
          "original_form": "얼그레이 도 있지",
          "speaker_id": "2",
          "time": "20210906 01:01"
        },
        {
          "id": "MDRW2100041474.1.14",
          "form": "아이스로 마시면 맛있겠다",
          "original_form": "아이스로 마시면 맛있겠다",
          "speaker_id": "1",
          "time": "20210906 01:01"
        }
