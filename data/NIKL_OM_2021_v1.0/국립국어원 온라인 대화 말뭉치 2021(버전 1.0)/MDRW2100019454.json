      "utterance": [
        {
          "id": "MDRW2100019454.1.1",
          "form": "당근 해봄?",
          "original_form": "당근 해봄?",
          "speaker_id": "1",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100019454.1.2",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100019454.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100019454.1.4",
          "form": "나는 이사할때 몇 번 해봄 ㅋㅋㅋ",
          "original_form": "나는 이사할때 몇 번 해봄 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100019454.1.5",
          "form": "와대박",
          "original_form": "와대박",
          "speaker_id": "2",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100019454.1.6",
          "form": "ㅋㅋㅋㅋ뭐해봤어",
          "original_form": "ㅋㅋㅋㅋ뭐해봤어",
          "speaker_id": "2",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100019454.1.7",
          "form": "나눔도 해보고",
          "original_form": "나눔도 해보고",
          "speaker_id": "1",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100019454.1.8",
          "form": "런닝머신도 팜 ㅋㅋㅋ",
          "original_form": "런닝머신도 팜 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100019454.1.9",
          "form": "헐대박ㅋㅋㅋ",
          "original_form": "헐대박ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100019454.1.10",
          "form": "난아니구 아빠가해봤는데",
          "original_form": "난아니구 아빠가해봤는데",
          "speaker_id": "2",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100019454.1.11",
          "form": "소파 티비",
          "original_form": "소파 티비",
          "speaker_id": "2",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100019454.1.12",
          "form": "이런거 막 사더라",
          "original_form": "이런거 막 사더라",
          "speaker_id": "2",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100019454.1.13",
          "form": "팔기만함?",
          "original_form": "팔기만함?",
          "speaker_id": "2",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019454.1.14",
          "form": "ㅇㅇ 아직 사본적은 없어",
          "original_form": "ㅇㅇ 아직 사본적은 없어",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019454.1.15",
          "form": "중고나라에선 사봤는데",
          "original_form": "중고나라에선 사봤는데",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019454.1.16",
          "form": "ㅋㅋㅋㅋ육아용품처럼",
          "original_form": "ㅋㅋㅋㅋ육아용품처럼",
          "speaker_id": "2",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019454.1.17",
          "form": "시기지나면 못쓰는거",
          "original_form": "시기지나면 못쓰는거",
          "speaker_id": "2",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019454.1.18",
          "form": "이런거 잘사고팔더라",
          "original_form": "이런거 잘사고팔더라",
          "speaker_id": "2",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019454.1.19",
          "form": "중나는 사기가 판치지않음?",
          "original_form": "중나는 사기가 판치지않음?",
          "speaker_id": "2",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019454.1.20",
          "form": "마자 그런면에서 당근이 직거래위주다보니 좀 좋지",
          "original_form": "마자 그런면에서 당근이 직거래위주다보니 좀 좋지",
          "speaker_id": "1",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019454.1.21",
          "form": "ㅇㅇ 근데 아직까진 중나 사기당한적은 없음",
          "original_form": "ㅇㅇ 근데 아직까진 중나 사기당한적은 없음",
          "speaker_id": "1",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019454.1.22",
          "form": "난 대학때 셤치려고",
          "original_form": "난 대학때 셤치려고",
          "speaker_id": "2",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100019454.1.23",
          "form": "재무계산기 사려다가",
          "original_form": "재무계산기 사려다가",
          "speaker_id": "2",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100019454.1.24",
          "form": "사기당해봐서 ㅡㅡ",
          "original_form": "사기당해봐서 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100019454.1.25",
          "form": "헐랭...",
          "original_form": "헐랭...",
          "speaker_id": "1",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100019454.1.26",
          "form": "물건 못받음?",
          "original_form": "물건 못받음?",
          "speaker_id": "1",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100019454.1.27",
          "form": "ㅇㅇ 자기도",
          "original_form": "ㅇㅇ 자기도",
          "speaker_id": "2",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019454.1.28",
          "form": "대학생이고",
          "original_form": "대학생이고",
          "speaker_id": "2",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019454.1.29",
          "form": "시험치려고 어쩌고 하더니",
          "original_form": "시험치려고 어쩌고 하더니",
          "speaker_id": "2",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019454.1.30",
          "form": "그런거한번당하고나서는",
          "original_form": "그런거한번당하고나서는",
          "speaker_id": "2",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019454.1.31",
          "form": "중나 당근 다 못믿잖음ㅋㅋㅋㅋㅋ",
          "original_form": "중나 당근 다 못믿잖음ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019454.1.32",
          "form": "헐...그랬구만",
          "original_form": "헐...그랬구만",
          "speaker_id": "1",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100019454.1.33",
          "form": "그런 경험 있음 아무래도 그럴듯 ㅠ",
          "original_form": "그런 경험 있음 아무래도 그럴듯 ㅠ",
          "speaker_id": "1",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100019454.1.34",
          "form": "월급받고",
          "original_form": "월급받고",
          "speaker_id": "2",
          "time": "20210901 19:28"
        }
      ]
    }
  ]
