      "utterance": [
        {
          "id": "MDRW2100007149.1.1",
          "form": "근데 불닭 컵라면전자렌지에 조리할때요",
          "original_form": "근데 불닭 컵라면전자렌지에 조리할때요",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007149.1.2",
          "form": "아 저는 렌지에는 안넣어여ㅠㅠ",
          "original_form": "아 저는 렌지에는 안넣어여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007149.1.3",
          "form": "뚜껑을 조금 남긴채로 돌렸더니 불이 붙더라고요",
          "original_form": "뚜껑을 조금 남긴채로 돌렸더니 불이 붙더라고요",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007149.1.4",
          "form": "그 용기 되요?? 렌지 그거",
          "original_form": "그 용기 되요?? 렌지 그거",
          "speaker_id": "1",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007149.1.5",
          "form": "놀랐어요 정말 조리할때",
          "original_form": "놀랐어요 정말 조리할때",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007149.1.6",
          "form": "안돨꺼같아여",
          "original_form": "안돨꺼같아여",
          "speaker_id": "1",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007149.1.7",
          "form": "아 그냥 컵라면 통은",
          "original_form": "아 그냥 컵라면 통은",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007149.1.8",
          "form": "렌지에 조리하면 안되나요?",
          "original_form": "렌지에 조리하면 안되나요?",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007149.1.9",
          "form": "헉ㅋㅋㅋ",
          "original_form": "헉ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007149.1.10",
          "form": "컵라면은 ㅋㅋㅋㅋ 뚜껑빼고",
          "original_form": "컵라면은 ㅋㅋㅋㅋ 뚜껑빼고",
          "speaker_id": "1",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007149.1.11",
          "form": "그 되는 용기가 따로 있어요",
          "original_form": "그 되는 용기가 따로 있어요",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007149.1.12",
          "form": "오뚜기 진라면은 된다고 적혀이꺼든요",
          "original_form": "오뚜기 진라면은 된다고 적혀이꺼든요",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007149.1.13",
          "form": "처음 알았어요 아 이것도",
          "original_form": "처음 알았어요 아 이것도",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007149.1.14",
          "form": "근데 신라면은 돌리지 말라고 적혀있어요",
          "original_form": "근데 신라면은 돌리지 말라고 적혀있어요",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007149.1.15",
          "form": "상식이 필요하네요",
          "original_form": "상식이 필요하네요",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007149.1.16",
          "form": "헉... 그냥 다 돌리게 해주지",
          "original_form": "헉... 그냥 다 돌리게 해주지",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007149.1.17",
          "form": "잘못 조리했다가 불나거나",
          "original_form": "잘못 조리했다가 불나거나",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007149.1.18",
          "form": "근데 불닭이 되는지 안되는지는 모르겟어여 저는 슬라이스",
          "original_form": "근데 불닭이 되는지 안되는지는 모르겟어여 저는 슬라이스",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007149.1.19",
          "form": "나쁜거나올수도 있겠네요",
          "original_form": "나쁜거나올수도 있겠네요",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007149.1.20",
          "form": "차즈 넣어서",
          "original_form": "차즈 넣어서",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007149.1.21",
          "form": "슬라이스치즈요?",
          "original_form": "슬라이스치즈요?",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007149.1.22",
          "form": "구냥 녹아서",
          "original_form": "구냥 녹아서",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007149.1.23",
          "form": "네 슬라이스 넣어요 ㅋㅋㅋㅋ",
          "original_form": "네 슬라이스 넣어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007149.1.24",
          "form": "ㅋㅋㅋ전 약간 그그",
          "original_form": "ㅋㅋㅋ전 약간 그그",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007149.1.25",
          "form": "작은 치즈들",
          "original_form": "작은 치즈들",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007149.1.26",
          "form": "모짜",
          "original_form": "모짜",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007149.1.27",
          "form": "모짜렐라!!!",
          "original_form": "모짜렐라!!!",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007149.1.28",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007149.1.29",
          "form": "넣으시나옄ㅋㅋ",
          "original_form": "넣으시나옄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007149.1.30",
          "form": "아니면스트링치즈ㅋㅋㅋ",
          "original_form": "아니면스트링치즈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007149.1.31",
          "form": "눈치가백단이시군요",
          "original_form": "눈치가백단이시군요",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007149.1.32",
          "form": "그건 녹임이 필요하니 렌지가 필요하겠어여 ㅋㅋㅋ.",
          "original_form": "그건 녹임이 필요하니 렌지가 필요하겠어여 ㅋㅋㅋ.",
