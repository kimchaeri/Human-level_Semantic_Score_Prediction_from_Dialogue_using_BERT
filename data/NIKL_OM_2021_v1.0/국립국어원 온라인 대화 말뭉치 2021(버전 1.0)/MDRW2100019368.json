      "utterance": [
        {
          "id": "MDRW2100019368.1.1",
          "form": "헐!!! 재밌었겠다!!!!",
          "original_form": "헐!!! 재밌었겠다!!!!",
          "speaker_id": "2",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100019368.1.2",
          "form": "케잌 꾸미려면 우선 케잌이 있어여하잖아요!!",
          "original_form": "케잌 꾸미려면 우선 케잌이 있어여하잖아요!!",
          "speaker_id": "2",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019368.1.3",
          "form": "맞아여 ㅋㅋㅋ 시트만주고 저희는",
          "original_form": "맞아여 ㅋㅋㅋ 시트만주고 저희는",
          "speaker_id": "1",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019368.1.4",
          "form": "케잌도 구우셨어요?!?!",
          "original_form": "케잌도 구우셨어요?!?!",
          "speaker_id": "2",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019368.1.5",
          "form": "크림바르고 하는거에요 ㅌㅌㅋㅋ",
          "original_form": "크림바르고 하는거에요 ㅌㅌㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019368.1.6",
          "form": "우왕 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "우왕 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019368.1.7",
          "form": "저 유툽에서 베니케이크? 였나 케잌꾸미는 영상봤는데",
          "original_form": "저 유툽에서 베니케이크? 였나 케잌꾸미는 영상봤는데",
          "speaker_id": "2",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019368.1.8",
          "form": "홀리듯이 봤어요",
          "original_form": "홀리듯이 봤어요",
          "speaker_id": "2",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019368.1.9",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019368.1.10",
          "form": "보는거도 재밌져 ㅋㅋㅋ",
          "original_form": "보는거도 재밌져 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019368.1.11",
          "form": "네네 어떻게 저렇게 크림을 짜지 싶고 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네네 어떻게 저렇게 크림을 짜지 싶고 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100019368.1.12",
          "form": "요새는 유튜브에서 케익공장 이런영상도 올라오더라구요 ㅋㅋㅋㅋ",
          "original_form": "요새는 유튜브에서 케익공장 이런영상도 올라오더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100019368.1.13",
          "form": "만드는거봄 신기해요",
          "original_form": "만드는거봄 신기해요",
          "speaker_id": "1",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100019368.1.14",
          "form": "오 ㅋㅋㅋㅋㅋㅋㅋ 케잌도 공장에서 찍듯이 만드는건가요?! ㅋㅋㅋㅋㅋ 크림 바르고 이런것도 기계로 쓱쓱 하는건가",
          "original_form": "오 ㅋㅋㅋㅋㅋㅋㅋ 케잌도 공장에서 찍듯이 만드는건가요?! ㅋㅋㅋㅋㅋ 크림 바르고 이런것도 기계로 쓱쓱 하는건가",
          "speaker_id": "2",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100019368.1.15",
          "form": "네 ㅋㅋㅋ 진짜 사람이 기계처럼 ㄱㅖ속 만들더라구요 슥슥 삭삭",
          "original_form": "네 ㅋㅋㅋ 진짜 사람이 기계처럼 ㄱㅖ속 만들더라구요 슥슥 삭삭",
          "speaker_id": "1",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100019368.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ 유툽에 정말 별게 다 올라오네요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 유툽에 정말 별게 다 올라오네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019368.1.17",
          "form": "레시피 올리는 채널들도 조회수 다 높더라구요 ㅋㅋㅋㅋ",
          "original_form": "레시피 올리는 채널들도 조회수 다 높더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019368.1.18",
          "form": "다들 관심이 많은가바여 ㅋㅋㅋ",
          "original_form": "다들 관심이 많은가바여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019368.1.19",
          "form": "제가 할줄 아는 요리라고는...라면.. 파스타(뚝딱 시판소스넣기)",
          "original_form": "제가 할줄 아는 요리라고는...라면.. 파스타(뚝딱 시판소스넣기)",
