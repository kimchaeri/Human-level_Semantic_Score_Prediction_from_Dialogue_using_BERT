      "utterance": [
        {
          "id": "MDRW2100006296.1.1",
          "form": "최근에 철원에 갔다가 배고플쯤 근처 뼈칼국수집 갔는데",
          "original_form": "최근에 철원에 갔다가 배고플쯤 근처 뼈칼국수집 갔는데",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100006296.1.2",
          "form": "엄청 성공해서 기분 좋았어요",
          "original_form": "엄청 성공해서 기분 좋았어요",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100006296.1.3",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100006296.1.4",
          "form": "뼈칼국수면",
          "original_form": "뼈칼국수면",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100006296.1.5",
          "form": "등뼈 들어가 있는건가요",
          "original_form": "등뼈 들어가 있는건가요",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100006296.1.6",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100006296.1.7",
          "form": "평이담백뼈칼국수라는 집이었어요",
          "original_form": "평이담백뼈칼국수라는 집이었어요",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100006296.1.8",
          "form": "다녀오고 보니 마포구 본점ㅋㅋㅋㅋㅋㅋ",
          "original_form": "다녀오고 보니 마포구 본점ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100006296.1.9",
          "form": "오옼ㅋㅋㅋㅋㅋ",
          "original_form": "오옼ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100006296.1.10",
          "form": "뭔가",
          "original_form": "뭔가",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100006296.1.11",
          "form": "사후 인증받은 기분이었겠네요",
          "original_form": "사후 인증받은 기분이었겠네요",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100006296.1.12",
          "form": "들어갈때는 오 사람 없다 이러면서 들어갔는데",
          "original_form": "들어갈때는 오 사람 없다 이러면서 들어갔는데",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100006296.1.13",
          "form": "알고보니 브레이크타임이라 없던거고",
          "original_form": "알고보니 브레이크타임이라 없던거고",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100006296.1.14",
          "form": "오....",
          "original_form": "오....",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100006296.1.15",
          "form": "저희 달어가자마자 우루루 오더니 풀방..",
          "original_form": "저희 달어가자마자 우루루 오더니 풀방..",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100006296.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100006296.1.17",
          "form": "우연과 운이",
          "original_form": "우연과 운이",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100006296.1.18",
          "form": "되게 성공적인 느낌 뿜뿜이었어요",
          "original_form": "되게 성공적인 느낌 뿜뿜이었어요",
