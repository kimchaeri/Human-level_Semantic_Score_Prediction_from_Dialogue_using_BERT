      "utterance": [
        {
          "id": "MDRW2100047341.1.1",
          "form": "넷플릭스 구독 하시나요?",
          "original_form": "넷플릭스 구독 하시나요?",
          "speaker_id": "1",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100047341.1.2",
          "form": "아뇨 넷플 끊고 요즘 티빙이랑 웨이브 쓰고 있어요",
          "original_form": "아뇨 넷플 끊고 요즘 티빙이랑 웨이브 쓰고 있어요",
          "speaker_id": "2",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100047341.1.3",
          "form": "넷플 좋어하시나요??",
          "original_form": "넷플 좋어하시나요??",
          "speaker_id": "2",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100047341.1.4",
          "form": "전 넷플릭스만 봐요! 끊으신 이유가 있나요?",
          "original_form": "전 넷플릭스만 봐요! 끊으신 이유가 있나요?",
          "speaker_id": "1",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100047341.1.5",
          "form": "오오오 앗 저는 국내 콘텐츠가 더 재미있어서",
          "original_form": "오오오 앗 저는 국내 콘텐츠가 더 재미있어서",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100047341.1.6",
          "form": "그 사실 잘 안보게되더라고요",
          "original_form": "그 사실 잘 안보게되더라고요",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100047341.1.7",
          "form": "영화 말고 예능 위주로 많이봐서 티빙이랑 웨이브가 더 낫더라고요!!",
          "original_form": "영화 말고 예능 위주로 많이봐서 티빙이랑 웨이브가 더 낫더라고요!!",
          "speaker_id": "2",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100047341.1.8",
          "form": "요즘 무슨예능 보세요?",
          "original_form": "요즘 무슨예능 보세요?",
          "speaker_id": "1",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100047341.1.9",
          "form": "저 심야괴담회 좋아해요",
          "original_form": "저 심야괴담회 좋아해요",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100047341.1.10",
          "form": "웨이브에 있어요",
          "original_form": "웨이브에 있어요",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100047341.1.11",
          "form": "공포영화도 좋아하세요?",
          "original_form": "공포영화도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100047341.1.12",
          "form": "엇 네네",
          "original_form": "엇 네네",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100047341.1.13",
          "form": "근데 그",
          "original_form": "근데 그",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100047341.1.14",
          "form": "곤지암이 마지막이었어요",
          "original_form": "곤지암이 마지막이었어요",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100047341.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ랑종 이번에 못봤어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ랑종 이번에 못봤어요",
          "speaker_id": "2",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100047341.1.16",
          "form": "가장 무서운 공포영화는 뭐였나요?",
          "original_form": "가장 무서운 공포영화는 뭐였나요?",
          "speaker_id": "1",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100047341.1.17",
          "form": "저 아무래도 곤지암",
          "original_form": "저 아무래도 곤지암",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100047341.1.18",
          "form": "영화관에서 4번 봤어요",
          "original_form": "영화관에서 4번 봤어요",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100047341.1.19",
          "form": "영화관에서 봐야 잼더라고요",
          "original_form": "영화관에서 봐야 잼더라고요",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100047341.1.20",
          "form": "헉 강심장이세요",
          "original_form": "헉 강심장이세요",
          "speaker_id": "1",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100047341.1.21",
          "form": "마블 영화는 보셨어요?",
          "original_form": "마블 영화는 보셨어요?",
          "speaker_id": "1",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100047341.1.22",
          "form": "헉 네네",
          "original_form": "헉 네네",
          "speaker_id": "2",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100047341.1.23",
          "form": "마블 좋아하시는군요?!!!",
          "original_form": "마블 좋아하시는군요?!!!",
          "speaker_id": "2",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100047341.1.24",
          "form": "이번에 스파이더맨 노웨이 홈 예고 보셨어요?!",
          "original_form": "이번에 스파이더맨 노웨이 홈 예고 보셨어요?!",
          "speaker_id": "2",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100047341.1.25",
          "form": "네~ 두근두근 설레네요ㅎㅎ",
          "original_form": "네~ 두근두근 설레네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100047341.1.26",
          "form": "스파이더맨 보셨어요?",
          "original_form": "스파이더맨 보셨어요?",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100047341.1.27",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100047341.1.28",
          "form": "스파이더맨이 영웅중에 제일 귀엽고",
          "original_form": "스파이더맨이 영웅중에 제일 귀엽고",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100047341.1.29",
          "form": "귀여운 것 중에 제일 영응같아요",
          "original_form": "귀여운 것 중에 제일 영응같아요",
          "speaker_id": "2",
