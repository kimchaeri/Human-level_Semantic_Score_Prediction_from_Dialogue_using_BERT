      "utterance": [
        {
          "id": "MDRW2100038535.1.1",
          "form": "점심 뭐 드셨나요",
          "original_form": "점심 뭐 드셨나요",
          "speaker_id": "1",
          "time": "20210902 15:31"
        },
        {
          "id": "MDRW2100038535.1.2",
          "form": "저 빵 먹었어요",
          "original_form": "저 빵 먹었어요",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038535.1.3",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038535.1.4",
          "form": "저는 불닭먹었어요",
          "original_form": "저는 불닭먹었어요",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038535.1.5",
          "form": "한번 크게 불태웠죠",
          "original_form": "한번 크게 불태웠죠",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038535.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ매운 거 좋아하세요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ매운 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038535.1.7",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "1",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038535.1.8",
          "form": "대신 잘 못먹습니다ㅠ",
          "original_form": "대신 잘 못먹습니다ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038535.1.9",
          "form": "앗 저도요 ㅋㅋㅋ",
          "original_form": "앗 저도요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038535.1.10",
          "form": "어렸을 땐 더 잘 먹었던 것 같은데 요즘은 아닌 것 같아요",
          "original_form": "어렸을 땐 더 잘 먹었던 것 같은데 요즘은 아닌 것 같아요",
          "speaker_id": "2",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100038535.1.11",
          "form": "컨디션 따라가기도 하구요",
          "original_form": "컨디션 따라가기도 하구요",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100038535.1.12",
          "form": "맞아요 그것도 컨디션따라 다르더라고요",
          "original_form": "맞아요 그것도 컨디션따라 다르더라고요",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100038535.1.13",
          "form": "오늘은 좋아서 잘먹었어요",
          "original_form": "오늘은 좋아서 잘먹었어요",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100038535.1.14",
          "form": "오 좋았겠네요ㅋㅋ",
          "original_form": "오 좋았겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:59"
        },
        {
          "id": "MDRW2100038535.1.15",
          "form": "name1 님은 좋아하는 음식 있으세요?",
          "original_form": "&name1& 님은 좋아하는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210903 08:59"
        },
        {
          "id": "MDRW2100038535.1.16",
          "form": "라면 좋아해요",
          "original_form": "라면 좋아해요",
          "speaker_id": "1",
          "time": "20210903 09:00"
        },
        {
          "id": "MDRW2100038535.1.17",
          "form": "하루 종일 라면 가능합니다",
          "original_form": "하루 종일 라면 가능합니다",
          "speaker_id": "1",
          "time": "20210903 09:00"
        },
        {
          "id": "MDRW2100038535.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:02"
        },
        {
          "id": "MDRW2100038535.1.19",
          "form": "전 떡볶이요 일주일에 한번씩 먹었던 적도 있어요",
          "original_form": "전 떡볶이요 일주일에 한번씩 먹었던 적도 있어요",
          "speaker_id": "2",
          "time": "20210903 09:02"
        },
        {
          "id": "MDRW2100038535.1.20",
          "form": "와 찐사랑이시군요",
          "original_form": "와 찐사랑이시군요",
          "speaker_id": "1",
          "time": "20210903 09:03"
        },
        {
          "id": "MDRW2100038535.1.21",
          "form": "어디 떡볶이 제일 좋아하세요?",
          "original_form": "어디 떡볶이 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 09:03"
        },
        {
          "id": "MDRW2100038535.1.22",
          "form": "전 못 골라요 다 좋아요ㅋㅋ",
          "original_form": "전 못 골라요 다 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:03"
        },
        {
          "id": "MDRW2100038535.1.23",
          "form": "체인점 말고 학교앞 분식집 떡볶이도 좋아해요",
          "original_form": "체인점 말고 학교앞 분식집 떡볶이도 좋아해요",
          "speaker_id": "2",
          "time": "20210903 09:03"
        },
        {
          "id": "MDRW2100038535.1.24",
          "form": "분식집 떡볶이 못먹어본지 오래됐어요",
          "original_form": "분식집 떡볶이 못먹어본지 오래됐어요",
          "speaker_id": "1",
          "time": "20210903 09:04"
        },
        {
          "id": "MDRW2100038535.1.25",
          "form": "저희 동네는 이미 체인점이 자리 잡았어요ㅠ",
          "original_form": "저희 동네는 이미 체인점이 자리 잡았어요ㅠ",
          "speaker_id": "1",
          "time": "20210903 09:05"
        },
        {
          "id": "MDRW2100038535.1.26",
          "form": "분식집 없으면 섭섭하죠 체인점은 절대 못 내는 맛이라 가끔 먹어줘야 해요ㅋㅋ",
          "original_form": "분식집 없으면 섭섭하죠 체인점은 절대 못 내는 맛이라 가끔 먹어줘야 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100038535.1.27",
          "form": "그래서 가끔 일부로 멀리 시장 가서 떡볶이 먹어요",
          "original_form": "그래서 가끔 일부로 멀리 시장 가서 떡볶이 먹어요",
          "speaker_id": "1",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100038535.1.28",
          "form": "시장 떡볶이도 맛있죠! 근데 코로나 때문에 걱정돼서 길에서 파는 음식은 못 먹겠어요",
          "original_form": "시장 떡볶이도 맛있죠! 근데 코로나 때문에 걱정돼서 길에서 파는 음식은 못 먹겠어요",
          "speaker_id": "2",
          "time": "20210903 09:54"
        }
      ]
