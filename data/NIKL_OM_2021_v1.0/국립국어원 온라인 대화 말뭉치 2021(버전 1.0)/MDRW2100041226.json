      "utterance": [
        {
          "id": "MDRW2100041226.1.1",
          "form": "저는 얼마전에 워치4샀어요",
          "original_form": "저는 얼마전에 워치4샀어요",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100041226.1.2",
          "form": "오오 ㅋㅋㅋ 좋은거 사셨네요 ㅋㅋ",
          "original_form": "오오 ㅋㅋㅋ 좋은거 사셨네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100041226.1.3",
          "form": "저는 오늘 지출 없이 있다가 ㅋㅋㅋㅋㅋ 자동차 와이퍼랑 필터 샀어요 ㅋㅋ",
          "original_form": "저는 오늘 지출 없이 있다가 ㅋㅋㅋㅋㅋ 자동차 와이퍼랑 필터 샀어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100041226.1.4",
          "form": "가을 옷도 사야되는데 ㅋㅋㅋ 쇼핑몰을 여러군데 보고있어요",
          "original_form": "가을 옷도 사야되는데 ㅋㅋㅋ 쇼핑몰을 여러군데 보고있어요",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041226.1.5",
          "form": "좋은거 샀는데...",
          "original_form": "좋은거 샀는데...",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041226.1.6",
          "form": "모셔놓고 있어요 ㅎㅎㅎ",
          "original_form": "모셔놓고 있어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041226.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ모셔놓으면 안되죠 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ모셔놓으면 안되죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041226.1.8",
          "form": "필름붙여서 쓰고 싶은데...주문이 많은지",
          "original_form": "필름붙여서 쓰고 싶은데...주문이 많은지",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100041226.1.9",
          "form": "중고거래는 하세요? ㅋㅋ",
          "original_form": "중고거래는 하세요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100041226.1.10",
          "form": "9/6부터 순차 발송한다고 ㅎㅎ",
          "original_form": "9/6부터 순차 발송한다고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100041226.1.11",
          "form": "저도 차에서 워셔액",
          "original_form": "저도 차에서 워셔액",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041226.1.12",
          "form": "달라고 띵띵 거리던데 ㅎㅎ",
          "original_form": "달라고 띵띵 거리던데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041226.1.13",
          "form": "중고거래 하죠 당근 주로 이용해요",
          "original_form": "중고거래 하죠 당근 주로 이용해요",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041226.1.14",
          "form": "네네 ㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041226.1.15",
          "form": "저도 당근 이용해요 ㅋㅋ",
          "original_form": "저도 당근 이용해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041226.1.16",
          "form": "뭐 뭐 팔아보셨어요??ㅋㅋ",
          "original_form": "뭐 뭐 팔아보셨어요??ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041226.1.17",
          "form": "저는 주로 아이들 옷 책...같은거요",
          "original_form": "저는 주로 아이들 옷 책...같은거요",
          "speaker_id": "2",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041226.1.18",
          "form": "아 책 많이 사죠 ㅋㅂ",
          "original_form": "아 책 많이 사죠 ㅋㅂ",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041226.1.19",
          "form": "안쓴 새 그릇이나..제가 다 본 책들...",
          "original_form": "안쓴 새 그릇이나..제가 다 본 책들...",
          "speaker_id": "2",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041226.1.20",
          "form": "이것저것 많이 팔았어요 ㅎ",
          "original_form": "이것저것 많이 팔았어요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041226.1.21",
          "form": "저는 책을 팔아보다가 ㅋㅋㅋ 와 파는것도 사는것도 쉽지 않구나 느꼈어요",
          "original_form": "저는 책을 팔아보다가 ㅋㅋㅋ 와 파는것도 사는것도 쉽지 않구나 느꼈어요",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041226.1.22",
          "form": "아 많이 파셨네요 ㅋㅋ",
          "original_form": "아 많이 파셨네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041226.1.23",
          "form": "저는 주로 팔고...아이들꺼 필요한거 있으면",
          "original_form": "저는 주로 팔고...아이들꺼 필요한거 있으면",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041226.1.24",
          "form": "레고같은건 사봤어요",
          "original_form": "레고같은건 사봤어요",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041226.1.25",
          "form": "아 맞아요 저두요 ㅋㅋ",
          "original_form": "아 맞아요 저두요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:41"
        }
      ]
