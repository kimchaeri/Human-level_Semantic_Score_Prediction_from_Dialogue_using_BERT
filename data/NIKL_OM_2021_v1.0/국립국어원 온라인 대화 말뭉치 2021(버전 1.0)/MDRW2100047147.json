      "utterance": [
        {
          "id": "MDRW2100047147.1.1",
          "form": "몬가요!",
          "original_form": "몬가요!",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100047147.1.2",
          "form": "빈센조요!",
          "original_form": "빈센조요!",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100047147.1.3",
          "form": "송중기 배우님 나오는 거 맞죠?",
          "original_form": "송중기 배우님 나오는 거 맞죠?",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100047147.1.4",
          "form": "아 그거 저 볼예정이에요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 그거 저 볼예정이에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100047147.1.5",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100047147.1.6",
          "form": "그거 꼭 보세요 ㅠ",
          "original_form": "그거 꼭 보세요 ㅠ",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100047147.1.7",
          "form": "첫화만 참으면 재밋다고하더라구요",
          "original_form": "첫화만 참으면 재밋다고하더라구요",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100047147.1.8",
          "form": "그 검사 역할 하는 배우님도 너무 웃겼어요",
          "original_form": "그 검사 역할 하는 배우님도 너무 웃겼어요",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100047147.1.9",
          "form": "그 작가님 드라마 다좋아요 ㅠㅠ",
          "original_form": "그 작가님 드라마 다좋아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100047147.1.10",
          "form": "앗 저 빈센조 확 꺼버리려다가 꾹 참고 봤어요 첨에",
          "original_form": "앗 저 빈센조 확 꺼버리려다가 꾹 참고 봤어요 첨에",
          "speaker_id": "1",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100047147.1.11",
          "form": "근데 대사가 너무 웃겨가지고 ㅋㅋㅋ",
          "original_form": "근데 대사가 너무 웃겨가지고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100047147.1.12",
          "form": "ㅋㅋㅋㅋㅋ네 다들 그렇다고하더라구요??",
          "original_form": "ㅋㅋㅋㅋㅋ네 다들 그렇다고하더라구요??",
          "speaker_id": "2",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100047147.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100047147.1.14",
          "form": "김과장이랑 열혈사제 보셧어요?",
          "original_form": "김과장이랑 열혈사제 보셧어요?",
          "speaker_id": "2",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100047147.1.15",
          "form": "송중기 배우님 40대라면서요..",
          "original_form": "송중기 배우님 40대라면서요..",
          "speaker_id": "1",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100047147.1.16",
          "form": "얼굴은 왜 아직도 애기야 ㅜ",
          "original_form": "얼굴은 왜 아직도 애기야 ㅜ",
          "speaker_id": "1",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100047147.1.17",
          "form": "네..송중기 유죄",
          "original_form": "네..송중기 유죄",
          "speaker_id": "2",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100047147.1.18",
          "form": "그거 둘다 처음 들어봤어요",
          "original_form": "그거 둘다 처음 들어봤어요",
          "speaker_id": "1",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100047147.1.19",
          "form": "오 그거 둘다 재밋어여 ㅋㅋㅋㅋ같은 작가님이랍니다",
          "original_form": "오 그거 둘다 재밋어여 ㅋㅋㅋㅋ같은 작가님이랍니다",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100047147.1.20",
          "form": "빈센조랑요",
          "original_form": "빈센조랑요",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100047147.1.21",
          "form": "아~ 진짜요?",
          "original_form": "아~ 진짜요?",
          "speaker_id": "1",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100047147.1.22",
          "form": "그거 두개 다 넷플에 있는 작품인가요?",
          "original_form": "그거 두개 다 넷플에 있는 작품인가요?",
          "speaker_id": "1",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100047147.1.23",
          "form": "유머코드 좋아하시면 둘다 좋아할거에요 ㅋㅋㅋㅋㅋ",
          "original_form": "유머코드 좋아하시면 둘다 좋아할거에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100047147.1.24",
          "form": "앗 그건 잘 모르겟어요!",
          "original_form": "앗 그건 잘 모르겟어요!",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100047147.1.25",
          "form": "저는 옛날에 본방으루봐서",
          "original_form": "저는 옛날에 본방으루봐서",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100047147.1.26",
          "form": "그럼 제가 나중에 찾아볼게요 ㅎㅎ",
          "original_form": "그럼 제가 나중에 찾아볼게요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100047147.1.27",
          "form": "네네 추천드려요 ㅋㅋㅋㅋ",
          "original_form": "네네 추천드려요 ㅋㅋㅋㅋ",
