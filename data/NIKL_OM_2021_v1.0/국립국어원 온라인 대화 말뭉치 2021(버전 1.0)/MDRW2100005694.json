      "utterance": [
        {
          "id": "MDRW2100005694.1.1",
          "form": "오늘도 택배가 하나왔는데",
          "original_form": "오늘도 택배가 하나왔는데",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005694.1.2",
          "form": "그변기에 붙이는 젤같은거ㅋ",
          "original_form": "그변기에 붙이는 젤같은거ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005694.1.3",
          "form": "응응 ㅋㅋ 알지",
          "original_form": "응응 ㅋㅋ 알지",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005694.1.4",
          "form": "화장실 청소도 안하면서 왜 샀나규 얼마나 잔소리를 하는지 휴",
          "original_form": "화장실 청소도 안하면서 왜 샀나규 얼마나 잔소리를 하는지 휴",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005694.1.5",
          "form": "아 웃겨ㅋㅋㅋㅋㅋ",
          "original_form": "아 웃겨ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005694.1.6",
          "form": "오빠는 하루라도",
          "original_form": "오빠는 하루라도",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005694.1.7",
          "form": "잔소리 안하면안된다ㅋㅋ",
          "original_form": "잔소리 안하면안된다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005694.1.8",
          "form": "지친다 지쳐 ㅋㅋㅋ 그만 사래",
          "original_form": "지친다 지쳐 ㅋㅋㅋ 그만 사래",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100005694.1.9",
          "form": "아 그만사라니ㅋㅋ",
          "original_form": "아 그만사라니ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005694.1.10",
          "form": "다 핫딜로사는건데도 ㅋㅋ막 그러나보다ㅋㅋ",
          "original_form": "다 핫딜로사는건데도 ㅋㅋ막 그러나보다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005694.1.11",
          "form": "하긴 우리오빠도",
          "original_form": "하긴 우리오빠도",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005694.1.12",
          "form": "우리오빠도 그런말하더라",
          "original_form": "우리오빠도 그런말하더라",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005694.1.13",
          "form": "뭐 일일이 설명은  안하지만 알아서 샀을거란 생각은 안하나봐",
          "original_form": "뭐 일일이 설명은  안하지만 알아서 샀을거란 생각은 안하나봐",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005694.1.14",
          "form": "안사도 되는건데도 핫딜이라고 사는건아니제?ㅋㅋ",
          "original_form": "안사도 되는건데도 핫딜이라고 사는건아니제?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100005694.1.15",
          "form": "어떨땐 공짜야 해도 사지말래",
          "original_form": "어떨땐 공짜야 해도 사지말래",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100005694.1.16",
          "form": "쓸대 없는대 왜 자꾸 사냐고",
          "original_form": "쓸대 없는대 왜 자꾸 사냐고",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100005694.1.17",
          "form": "이러길래 다 필요한거고 싸게샀다 이러지뭐ㅋ",
          "original_form": "이러길래 다 필요한거고 싸게샀다 이러지뭐ㅋ",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100005694.1.18",
          "form": "맞찌",
          "original_form": "맞찌",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100005694.1.19",
          "form": "공짜면 사야지ㅋㅋ",
          "original_form": "공짜면 사야지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100005694.1.20",
          "form": "왜 안사ㅋㅋ",
          "original_form": "왜 안사ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100005694.1.21",
