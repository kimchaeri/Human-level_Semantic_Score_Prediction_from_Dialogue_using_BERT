      "utterance": [
        {
          "id": "MDRW2100013941.1.1",
          "form": "점심 식사로 메뉴 뭐드셨나요?",
          "original_form": "점심 식사로 메뉴 뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013941.1.2",
          "form": "저 굶었는데.. 짜장면이 먹고 싶었어요..",
          "original_form": "저 굶었는데.. 짜장면이 먹고 싶었어요..",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013941.1.3",
          "form": "님은 뭐 드셨나요?",
          "original_form": "님은 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013941.1.4",
          "form": "저도 간단하게 샌드위치 먹었어요! 짜장면 저도 먹고싶네요ㅠㅠ",
          "original_form": "저도 간단하게 샌드위치 먹었어요! 짜장면 저도 먹고싶네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013941.1.5",
          "form": "아직 뭐 안드셔서 배고프신가봐요",
          "original_form": "아직 뭐 안드셔서 배고프신가봐요",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013941.1.6",
          "form": "얼른 식사하셔요ㅠ",
          "original_form": "얼른 식사하셔요ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013941.1.7",
          "form": "사실 늦게 일어나서 별 생각 없긴해요!",
          "original_form": "사실 늦게 일어나서 별 생각 없긴해요!",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013941.1.8",
          "form": "어떤 샌드위치 드셨나요ㅜㅠㅠ 맛있겠다ㅠㅠㅠ",
          "original_form": "어떤 샌드위치 드셨나요ㅜㅠㅠ 맛있겠다ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013941.1.9",
          "form": "햄치즈 먹었어요! ㅠㅠ 그럼 점저로 드시겠네요",
          "original_form": "햄치즈 먹었어요! ㅠㅠ 그럼 점저로 드시겠네요",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013941.1.10",
          "form": "저녁엔 뭐 드실건가요? 메뉴",
          "original_form": "저녁엔 뭐 드실건가요? 메뉴",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013941.1.11",
          "form": "음.. 다이어트하려고",
          "original_form": "음.. 다이어트하려고",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013941.1.12",
          "form": "뒷다리살 사논 거 있어서",
          "original_form": "뒷다리살 사논 거 있어서",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013941.1.13",
          "form": "그거 구워먹을 거예요.. 행복..",
          "original_form": "그거 구워먹을 거예요.. 행복..",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013941.1.14",
          "form": "와ㅠㅠ 완전 맛있겠어요",
          "original_form": "와ㅠㅠ 완전 맛있겠어요",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013941.1.15",
          "form": "그럼 다이어트하시면 탄수화물은 안드시는거예요?",
          "original_form": "그럼 다이어트하시면 탄수화물은 안드시는거예요?",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013941.1.16",
          "form": "아님 소량만?ㅠㅠ",
          "original_form": "아님 소량만?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013941.1.17",
          "form": "평소보다 소량만요!!!",
          "original_form": "평소보다 소량만요!!!",
          "speaker_id": "2",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100013941.1.18",
          "form": "근데 이게 시간이 지날수록",
          "original_form": "근데 이게 시간이 지날수록",
          "speaker_id": "2",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100013941.1.19",
          "form": "그냥 평소양 + 고기",
          "original_form": "그냥 평소양 + 고기",
          "speaker_id": "2",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100013941.1.20",
          "form": "이조합으로 가는 거 같아요..ㅎ",
          "original_form": "이조합으로 가는 거 같아요..ㅎ",
          "speaker_id": "2",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100013941.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ넘 귀여우시네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ넘 귀여우시네요",
          "speaker_id": "1",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100013941.1.22",
          "form": "저도요ㅋㅋㅋㅋㅋㅋ그랬었어요 넘 웃기네요",
          "original_form": "저도요ㅋㅋㅋㅋㅋㅋ그랬었어요 넘 웃기네요",
          "speaker_id": "1",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100013941.1.23",
          "form": "요즘은",
          "original_form": "요즘은",
          "speaker_id": "2",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100013941.1.24",
          "form": "안 먹던 빵간식도 먹어요",
          "original_form": "안 먹던 빵간식도 먹어요",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100013941.1.25",
          "form": "호두타르트 에그타르트 이런 거....",
          "original_form": "호두타르트 에그타르트 이런 거....",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100013941.1.26",
          "form": "와 대박ㅎㅎ저도요 저는 지금 다이어트는 딱히 아닌데 생각은 하고 있거든요 다이어트 해야되는데 하고",
          "original_form": "와 대박ㅎㅎ저도요 저는 지금 다이어트는 딱히 아닌데 생각은 하고 있거든요 다이어트 해야되는데 하고",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100013941.1.27",
          "form": "근데 요즘 안먹던  빵간식 완전 먹어요",
          "original_form": "근데 요즘 안먹던  빵간식 완전 먹어요",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100013941.1.28",
          "form": "어제는 모카번에 사라다빵 오늘은 치즈케이크 빵",
          "original_form": "어제는 모카번에 사라다빵 오늘은 치즈케이크 빵",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100013941.1.29",
          "form": "혹시 그 뚜레쥬르나 파리바게트에 파는 치즈케이크빵 아세요? 길쭉한거",
          "original_form": "혹시 그 뚜레쥬르나 파리바게트에 파는 치즈케이크빵 아세요? 길쭉한거",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100013941.1.30",
          "form": "헐 넹넹 뭔지 알아요",
          "original_form": "헐 넹넹 뭔지 알아요",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013941.1.31",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠ 진짜 빵..",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠ 진짜 빵..",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013941.1.32",
          "form": "그거먹고 또 샌드위치까지 먹었어욬ㅋㅋㅋ",
          "original_form": "그거먹고 또 샌드위치까지 먹었어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013941.1.33",
          "form": "빵없이 어떻게 살아요...",
          "original_form": "빵없이 어떻게 살아요...",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013941.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아직",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아직",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013941.1.35",
          "form": "요새 빵 왤케 먹죠ㅠ 그러게요",
          "original_form": "요새 빵 왤케 먹죠ㅠ 그러게요",
          "speaker_id": "1",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013941.1.36",
          "form": "다이어트 안하시니까 괜찮아요!!",
          "original_form": "다이어트 안하시니까 괜찮아요!!",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013941.1.37",
          "form": "와 진짜 빵집 털러 가고 싶다..",
          "original_form": "와 진짜 빵집 털러 가고 싶다..",
