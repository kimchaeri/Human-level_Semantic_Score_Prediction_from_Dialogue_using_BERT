        },
        {
          "id": "MDRW2100038635.1.3",
          "form": "채팅에서 만난 운명적인 상대",
          "original_form": "채팅에서 만난 운명적인 상대",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100038635.1.4",
          "form": "어떻게 생각하세요?",
          "original_form": "어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100038635.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ 그런사람 없어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그런사람 없어요",
          "speaker_id": "2",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100038635.1.6",
          "form": "금사식이라요",
          "original_form": "금사식이라요",
          "speaker_id": "2",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100038635.1.7",
          "form": "아 왜요 말하는데 다 웃기고 드립도 다 내스탈이고",
          "original_form": "아 왜요 말하는데 다 웃기고 드립도 다 내스탈이고",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100038635.1.8",
          "form": "그런사람 만난적없어요?",
          "original_form": "그런사람 만난적없어요?",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100038635.1.9",
          "form": "ㅋㅋㅋㅋㅋ오래안가요",
          "original_form": "ㅋㅋㅋㅋㅋ오래안가요",
          "speaker_id": "2",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100038635.1.10",
          "form": "우선 채팅에서 맘에 들었다고 쳐봐요",
          "original_form": "우선 채팅에서 맘에 들었다고 쳐봐요",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100038635.1.11",
          "form": "아직 얼굴은 몰라요",
          "original_form": "아직 얼굴은 몰라요",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100038635.1.12",
          "form": "그사람이 한번 만나보쟤요",
          "original_form": "그사람이 한번 만나보쟤요",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
