      "utterance": [
        {
          "id": "MDRW2100043419.1.1",
          "form": "학교다닐때 어땠어?",
          "original_form": "학교다닐때 어땠어?",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043419.1.2",
          "form": "짜증났지 뭐..",
          "original_form": "짜증났지 뭐..",
          "speaker_id": "1",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043419.1.3",
          "form": "왜 그래두 좋은 추억이 있을거아냐",
          "original_form": "왜 그래두 좋은 추억이 있을거아냐",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043419.1.4",
          "form": "많진않아",
          "original_form": "많진않아",
          "speaker_id": "1",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043419.1.5",
          "form": "친구들이랑은 잘지냈었잖아",
          "original_form": "친구들이랑은 잘지냈었잖아",
          "speaker_id": "2",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043419.1.6",
          "form": "아 학교친구들은 다 괜찮았지",
          "original_form": "아 학교친구들은 다 괜찮았지",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043419.1.7",
          "form": "그럼 뭐가 별로였는데?",
          "original_form": "그럼 뭐가 별로였는데?",
          "speaker_id": "2",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043419.1.8",
          "form": "교사들이 별로였지",
          "original_form": "교사들이 별로였지",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043419.1.9",
          "form": "아이고.. 진짜 짜증났겠네",
          "original_form": "아이고.. 진짜 짜증났겠네",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043419.1.10",
          "form": "회상도 하고싶지 않아",
          "original_form": "회상도 하고싶지 않아",
          "speaker_id": "1",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043419.1.11",
          "form": "애들 안좋아하는 인간은 교사하지말았으면 좋겠어",
          "original_form": "애들 안좋아하는 인간은 교사하지말았으면 좋겠어",
          "speaker_id": "2",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100043419.1.12",
          "form": "맞아 교사들중에 애들 싫어하는 사람들 있어 맨날 화내고",
          "original_form": "맞아 교사들중에 애들 싫어하는 사람들 있어 맨날 화내고",
          "speaker_id": "1",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100043419.1.13",
          "form": "그러면 학교생활이 재밌을수가 없지",
          "original_form": "그러면 학교생활이 재밌을수가 없지",
          "speaker_id": "2",
          "time": "20210907 19:00"
        }
