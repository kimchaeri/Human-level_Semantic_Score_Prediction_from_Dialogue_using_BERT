      "utterance": [
        {
          "id": "MDRW2100019118.1.1",
          "form": "책이랑 생필품요",
          "original_form": "책이랑 생필품요",
          "speaker_id": "2",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100019118.1.2",
          "form": "책 저는 언제 샀는지 가물가물하네요",
          "original_form": "책 저는 언제 샀는지 가물가물하네요",
          "speaker_id": "1",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100019118.1.3",
          "form": "아 영양제도요ㅋ",
          "original_form": "아 영양제도요ㅋ",
          "speaker_id": "2",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100019118.1.4",
          "form": "영양제 뭐로 사셨어요?",
          "original_form": "영양제 뭐로 사셨어요?",
          "speaker_id": "1",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100019118.1.5",
          "form": "오메가3랑",
          "original_form": "오메가3랑",
          "speaker_id": "2",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100019118.1.6",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019118.1.7",
          "form": "프로폴리스, 비타민이요",
          "original_form": "프로폴리스, 비타민이요",
          "speaker_id": "2",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019118.1.8",
          "form": "좋은거로 골라드시네요",
          "original_form": "좋은거로 골라드시네요",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019118.1.9",
          "form": "몸이 예전같지 않아서ㅜㅜㅜ",
          "original_form": "몸이 예전같지 않아서ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019118.1.10",
          "form": "나이드니 그렇더라구요 ㅠ",
          "original_form": "나이드니 그렇더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019118.1.11",
          "form": "어느브랜드드세요?",
          "original_form": "어느브랜드드세요?",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019118.1.12",
          "form": "저 직구해서 오쏘몰이랑",
          "original_form": "저 직구해서 오쏘몰이랑",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019118.1.13",
          "form": "오메가3는 송중기가 선전하는 국산제품ㅋㅋ",
          "original_form": "오메가3는 송중기가 선전하는 국산제품ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019118.1.14",
          "form": "오 직구는 뭔가 더 조을거같아요",
          "original_form": "오 직구는 뭔가 더 조을거같아요",
          "speaker_id": "1",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019118.1.15",
          "form": "그게 쫌 더 함량도 쎄고 가격도 괜찮더라구요",
          "original_form": "그게 쫌 더 함량도 쎄고 가격도 괜찮더라구요",
          "speaker_id": "2",
