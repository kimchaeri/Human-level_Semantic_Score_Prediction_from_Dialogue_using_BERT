      "utterance": [
        {
          "id": "MDRW2100016969.1.1",
          "form": "그건 그런듯...",
          "original_form": "그건 그런듯...",
          "speaker_id": "1",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016969.1.2",
          "form": "기존에 게이머 라고 부르던 사람들은",
          "original_form": "기존에 게이머 라고 부르던 사람들은",
          "speaker_id": "2",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016969.1.3",
          "form": "이미 콘솔로 많이 이동했고",
          "original_form": "이미 콘솔로 많이 이동했고",
          "speaker_id": "2",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016969.1.4",
          "form": "지금 모바일게임을 하는사람들은 기존의 일반인하고 혼재한 느낌",
          "original_form": "지금 모바일게임을 하는사람들은 기존의 일반인하고 혼재한 느낌",
          "speaker_id": "2",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016969.1.5",
          "form": "그렇지 머",
          "original_form": "그렇지 머",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016969.1.6",
          "form": "PC겜 별루 할거도 없구",
          "original_form": "PC겜 별루 할거도 없구",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016969.1.7",
          "form": "롤만 여전히 잘 되네",
          "original_form": "롤만 여전히 잘 되네",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016969.1.8",
          "form": "할게 없으니 유스(ㅋㅋ)들도 다 롤로 가는거같음",
          "original_form": "할게 없으니 유스(ㅋㅋ)들도 다 롤로 가는거같음",
          "speaker_id": "2",
          "time": "20210826 16:39"
        },
        {
          "id": "MDRW2100016969.1.9",
          "form": "ㅋㅋㅋㅋㅋ리그도 잘 진행되더라",
          "original_form": "ㅋㅋㅋㅋㅋ리그도 잘 진행되더라",
          "speaker_id": "1",
          "time": "20210826 16:40"
        },
        {
          "id": "MDRW2100016969.1.10",
          "form": "카트 중단되고 거긴 함",
          "original_form": "카트 중단되고 거긴 함",
          "speaker_id": "1",
          "time": "20210826 16:40"
        },
        {
          "id": "MDRW2100016969.1.11",
          "form": "대체재 없으면 몇년은 더 해먹을듯",
          "original_form": "대체재 없으면 몇년은 더 해먹을듯",
          "speaker_id": "2",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016969.1.12",
          "form": "오버워치도 거의 망했고 뭐ㅋㅋㅋ",
          "original_form": "오버워치도 거의 망했고 뭐ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016969.1.13",
          "form": "옵치는 왜 망한건지",
          "original_form": "옵치는 왜 망한건지",
          "speaker_id": "2",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016969.1.14",
          "form": "업뎃이 너무 느려서 그런가",
          "original_form": "업뎃이 너무 느려서 그런가",
          "speaker_id": "2",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016969.1.15",
          "form": "그런거 같던데",
          "original_form": "그런거 같던데",
          "speaker_id": "1",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100016969.1.16",
          "form": "아니면 페이커처럼 스타플레이어가 없어서 그런가",
          "original_form": "아니면 페이커처럼 스타플레이어가 없어서 그런가",
          "speaker_id": "2",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100016969.1.17",
          "form": "롤은 처음에도 유입많았긴한데",
          "original_form": "롤은 처음에도 유입많았긴한데",
          "speaker_id": "2",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100016969.1.18",
          "form": "매라 이런 스타플레이어들이 선전한것도 큰데",
          "original_form": "매라 이런 스타플레이어들이 선전한것도 큰데",
          "speaker_id": "2",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100016969.1.19",
          "form": "옵치는 그런거 못봤네",
          "original_form": "옵치는 그런거 못봤네",
          "speaker_id": "2",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100016969.1.20",
          "form": "그치.. 나오기 어려운 시스템인가?",
          "original_form": "그치.. 나오기 어려운 시스템인가?",
          "speaker_id": "1",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100016969.1.21",
          "form": "그 대단한 플레이를 화면으로 파악하기 어려워서 그럴수도",
          "original_form": "그 대단한 플레이를 화면으로 파악하기 어려워서 그럴수도",
          "speaker_id": "2",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016969.1.22",
          "form": "겐지 잘하는 선수가 펜타킬 내면 멋있기는 할텐데",
          "original_form": "겐지 잘하는 선수가 펜타킬 내면 멋있기는 할텐데",
          "speaker_id": "2",
          "time": "20210826 16:47"
        },
        {
          "id": "MDRW2100016969.1.23",
          "form": "롤은 아무래도 우리나라 사람들이 스타식 미니맵에 익숙하기도하도",
          "original_form": "롤은 아무래도 우리나라 사람들이 스타식 미니맵에 익숙하기도하도",
          "speaker_id": "2",
          "time": "20210826 16:47"
        },
        {
          "id": "MDRW2100016969.1.24",
          "form": "잘하는게 잘 보이잖아",
          "original_form": "잘하는게 잘 보이잖아",
          "speaker_id": "2",
          "time": "20210826 16:47"
        },
        {
          "id": "MDRW2100016969.1.25",
          "form": "화면이 투디라",
          "original_form": "화면이 투디라",
          "speaker_id": "2",
          "time": "20210826 16:47"
        },
        {
          "id": "MDRW2100016969.1.26",
          "form": "약간 중계화면 문제가 있구나",
          "original_form": "약간 중계화면 문제가 있구나",
          "speaker_id": "1",
          "time": "20210826 16:47"
        },
        {
          "id": "MDRW2100016969.1.27",
          "form": "리그에선 그거 크지..",
          "original_form": "리그에선 그거 크지..",
          "speaker_id": "1",
          "time": "20210826 16:47"
        }
      ]
    }
  ]
