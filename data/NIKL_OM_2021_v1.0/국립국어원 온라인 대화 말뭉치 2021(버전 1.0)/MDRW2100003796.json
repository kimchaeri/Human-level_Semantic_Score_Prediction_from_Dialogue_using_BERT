      "utterance": [
        {
          "id": "MDRW2100003796.1.1",
          "form": "넌 아침으로 뭐 먹었는데?",
          "original_form": "넌 아침으로 뭐 먹었는데?",
          "speaker_id": "2",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003796.1.2",
          "form": "나는 한식!",
          "original_form": "나는 한식!",
          "speaker_id": "1",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003796.1.3",
          "form": "한식 좋지",
          "original_form": "한식 좋지",
          "speaker_id": "2",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003796.1.4",
          "form": "밥이랑 소고기뭇국이랑",
          "original_form": "밥이랑 소고기뭇국이랑",
          "speaker_id": "1",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003796.1.5",
          "form": "소고기 뭇국 맛있어",
          "original_form": "소고기 뭇국 맛있어",
          "speaker_id": "2",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003796.1.6",
          "form": "무말랭이 무침 ㅋㅋㅋ",
          "original_form": "무말랭이 무침 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003796.1.7",
          "form": "보쌈무?",
          "original_form": "보쌈무?",
          "speaker_id": "2",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003796.1.8",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003796.1.9",
          "form": "보쌈 쌈싸먹고 싶다",
          "original_form": "보쌈 쌈싸먹고 싶다",
          "speaker_id": "2",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003796.1.10",
          "form": "원할머니 떡쌈 맛있어",
          "original_form": "원할머니 떡쌈 맛있어",
          "speaker_id": "1",
          "time": "20210821 12:15"
        },
        {
          "id": "MDRW2100003796.1.11",
          "form": "족발 좋아해?",
          "original_form": "족발 좋아해?",
          "speaker_id": "2",
          "time": "20210821 12:15"
        },
        {
          "id": "MDRW2100003796.1.12",
          "form": "좋아해",
          "original_form": "좋아해",
          "speaker_id": "1",
          "time": "20210821 12:15"
        },
        {
          "id": "MDRW2100003796.1.13",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210821 12:15"
        },
        {
          "id": "MDRW2100003796.1.14",
          "form": "나는 족발은 별로",
          "original_form": "나는 족발은 별로",
          "speaker_id": "2",
          "time": "20210821 12:15"
        },
        {
          "id": "MDRW2100003796.1.15",
          "form": "오 왜?",
          "original_form": "오 왜?",
          "speaker_id": "1",
          "time": "20210821 12:15"
        },
        {
          "id": "MDRW2100003796.1.16",
          "form": "한방 냄새 나서 좀 ㅋㅋ",
          "original_form": "한방 냄새 나서 좀 ㅋㅋ",
