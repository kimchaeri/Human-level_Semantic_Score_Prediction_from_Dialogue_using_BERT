      "utterance": [
        {
          "id": "MDRW2100024338.1.1",
          "form": "요즘 일 많이 바빠?",
          "original_form": "요즘 일 많이 바빠?",
          "speaker_id": "1",
          "time": "20210831 01:11"
        },
        {
          "id": "MDRW2100024338.1.2",
          "form": "계속 바쁜거같아\r\n일이 끝이 없어..",
          "original_form": "계속 바쁜거같아\r\n일이 끝이 없어..",
          "speaker_id": "2",
          "time": "20210831 01:13"
        },
        {
          "id": "MDRW2100024338.1.3",
          "form": "오늘은 정신없긴했는데 일하는 느낌없이 시간이 훅훅 가",
          "original_form": "오늘은 정신없긴했는데 일하는 느낌없이 시간이 훅훅 가",
          "speaker_id": "2",
          "time": "20210831 01:13"
        },
        {
          "id": "MDRW2100024338.1.4",
          "form": "우째ㅜㅜ",
          "original_form": "우째ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 01:13"
        },
        {
          "id": "MDRW2100024338.1.5",
          "form": "신제품출시때문인가",
          "original_form": "신제품출시때문인가",
          "speaker_id": "1",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100024338.1.6",
          "form": "아닌거같어",
          "original_form": "아닌거같어",
          "speaker_id": "2",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100024338.1.7",
          "form": "여기 부품특성인가",
          "original_form": "여기 부품특성인가",
          "speaker_id": "2",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100024338.1.8",
          "form": "기본? 불량을 자꾸 내",
          "original_form": "기본? 불량을 자꾸 내",
          "speaker_id": "2",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100024338.1.9",
          "form": "그럼 왜그렇게 바쁘지???",
          "original_form": "그럼 왜그렇게 바쁘지???",
          "speaker_id": "1",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100024338.1.10",
          "form": "사람이 없어?",
          "original_form": "사람이 없어?",
          "speaker_id": "1",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100024338.1.11",
          "form": "뭐 믿을수없어",
          "original_form": "뭐 믿을수없어",
          "speaker_id": "2",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100024338.1.12",
          "form": "여기?",
          "original_form": "여기?",
          "speaker_id": "2",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100024338.1.13",
          "form": "바뿌건 불량나서?",
          "original_form": "바뿌건 불량나서?",
          "speaker_id": "2",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100024338.1.14",
          "form": "ㅇㅇ일에비해 사람이 적은거 아냐?",
          "original_form": "ㅇㅇ일에비해 사람이 적은거 아냐?",
          "speaker_id": "1",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100024338.1.15",
          "form": "불량?",
          "original_form": "불량?",
          "speaker_id": "1",
          "time": "20210831 01:16"
        },
        {
          "id": "MDRW2100024338.1.16",
          "form": "불량 없어도 원래하는일있는데 불량남 대응 후속조치등 바빠",
          "original_form": "불량 없어도 원래하는일있는데 불량남 대응 후속조치등 바빠",
          "speaker_id": "2",
          "time": "20210831 01:16"
        },
        {
          "id": "MDRW2100024338.1.17",
          "form": "그렇구나 불량이 왜 계속날까",
          "original_form": "그렇구나 불량이 왜 계속날까",
          "speaker_id": "1",
          "time": "20210831 01:16"
        },
        {
          "id": "MDRW2100024338.1.18",
          "form": "인원이 작은건가 효율적으로 못하는것도 있는거같어",
          "original_form": "인원이 작은건가 효율적으로 못하는것도 있는거같어",
          "speaker_id": "2",
          "time": "20210831 01:17"
        },
        {
          "id": "MDRW2100024338.1.19",
          "form": "협업? 그게",
          "original_form": "협업? 그게",
          "speaker_id": "2",
          "time": "20210831 01:17"
        },
        {
          "id": "MDRW2100024338.1.20",
          "form": "음 구조개선을 해야겠는데...",
          "original_form": "음 구조개선을 해야겠는데...",
          "speaker_id": "1",
          "time": "20210831 01:17"
        },
        {
          "id": "MDRW2100024338.1.21",
          "form": "자려는데 일얘기하니 뇌가 다시 활성화돼는느낌이야",
          "original_form": "자려는데 일얘기하니 뇌가 다시 활성화돼는느낌이야",
          "speaker_id": "2",
          "time": "20210831 01:17"
        },
        {
          "id": "MDRW2100024338.1.22",
          "form": "헐 미안 ㅋㅋ 자고 일어나서 다시 이야기해~",
          "original_form": "헐 미안 ㅋㅋ 자고 일어나서 다시 이야기해~",
          "speaker_id": "1",
          "time": "20210831 01:18"
        },
        {
          "id": "MDRW2100024338.1.23",
          "form": "내가머캐 개선하나했는데 아냐 난 그럴 방도를 모르겠더라",
          "original_form": "내가머캐 개선하나했는데 아냐 난 그럴 방도를 모르겠더라",
          "speaker_id": "2",
          "time": "20210831 01:18"
        },
        {
          "id": "MDRW2100024338.1.24",
          "form": "내가 어캐 개선하려했는데",
          "original_form": "내가 어캐 개선하려했는데",
          "speaker_id": "2",
          "time": "20210831 01:18"
        },
        {
          "id": "MDRW2100024338.1.25",
          "form": "구조개선은 윗사람이 해야되는데 참 문제를 왜 해결을 안하지",
          "original_form": "구조개선은 윗사람이 해야되는데 참 문제를 왜 해결을 안하지",
          "speaker_id": "1",
          "time": "20210831 01:19"
        },
        {
          "id": "MDRW2100024338.1.26",
          "form": "상사한텐 언지줘도 그냥글코",
          "original_form": "상사한텐 언지줘도 그냥글코",
          "speaker_id": "2",
          "time": "20210831 01:19"
        },
        {
          "id": "MDRW2100024338.1.27",
          "form": "일하고픈데 월급루팡이 최고란걸 느낀다",
          "original_form": "일하고픈데 월급루팡이 최고란걸 느낀다",
          "speaker_id": "2",
          "time": "20210831 01:19"
        }
      ]
    }
