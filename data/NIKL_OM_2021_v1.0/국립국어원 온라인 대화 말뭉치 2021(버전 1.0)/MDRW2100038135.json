        {
          "id": "MDRW2100038135.1.1",
          "form": "name2님 드시고계신 영양제있으신가여",
          "original_form": "&name2&님 드시고계신 영양제있으신가여",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100038135.1.2",
          "form": "전 루테인이랑 비타민이요",
          "original_form": "전 루테인이랑 비타민이요",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100038135.1.3",
          "form": "루테인은 ㅊㅊ해요",
          "original_form": "루테인은 ㅊㅊ해요",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100038135.1.4",
          "form": "오 루테인 효과보셨어요",
          "original_form": "오 루테인 효과보셨어요",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100038135.1.5",
          "form": "살짝 덜 피곤한 느낌?이 있어요",
          "original_form": "살짝 덜 피곤한 느낌?이 있어요",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100038135.1.6",
          "form": "루테인 맛없어서 안먹었는데 다시먹어야겠네요 ㅋㅋㅋㅋ",
          "original_form": "루테인 맛없어서 안먹었는데 다시먹어야겠네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100038135.1.7",
          "form": "제 기분탓일지도요 ㅋㅋ",
          "original_form": "제 기분탓일지도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100038135.1.8",
          "form": "ㅋㅋㅋㅋㅋ기분탓이어도 효과가 느껴지면 좋은어니까여",
          "original_form": "ㅋㅋㅋㅋㅋ기분탓이어도 효과가 느껴지면 좋은어니까여",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038135.1.9",
          "form": "그럼 저렴한걸로 시작해보세요",
          "original_form": "그럼 저렴한걸로 시작해보세요",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038135.1.10",
          "form": "네 집에 있긴한데 안먹었었거든여",
          "original_form": "네 집에 있긴한데 안먹었었거든여",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038135.1.11",
          "form": "아이브라이트를 빼고 루테인을 먹어봐야겠어요",
          "original_form": "아이브라이트를 빼고 루테인을 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100038135.1.12",
          "form": "두개를 한꺼번에 먹음 안대ㅣ나요?",
          "original_form": "두개를 한꺼번에 먹음 안대ㅣ나요?",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100038135.1.13",
          "form": "저 먹고있는 영양제 좀많아서 ㅋㅋㅋㅋ눈이면눈하나만먹게여",
          "original_form": "저 먹고있는 영양제 좀많아서 ㅋㅋㅋㅋ눈이면눈하나만먹게여",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100038135.1.14",
          "form": "그렇군요 성분이 겹치긴 할그에요",
          "original_form": "그렇군요 성분이 겹치긴 할그에요",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100038135.1.15",
          "form": "네네 넘또 과하면안젛다고하니까여",
          "original_form": "네네 넘또 과하면안젛다고하니까여",
          "speaker_id": "1",
          "time": "20210901 23:48"
        }
