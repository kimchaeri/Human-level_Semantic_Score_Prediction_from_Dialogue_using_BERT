      "utterance": [
        {
          "id": "MDRW2100007884.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210822 11:35"
        },
        {
          "id": "MDRW2100007884.1.2",
          "form": "네 밥해서 먹었어요",
          "original_form": "네 밥해서 먹었어요",
          "speaker_id": "2",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007884.1.3",
          "form": "뭐 드셨어요?점심 정해서 드셨나요",
          "original_form": "뭐 드셨어요?점심 정해서 드셨나요",
          "speaker_id": "2",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007884.1.4",
          "form": "국밥이요",
          "original_form": "국밥이요",
          "speaker_id": "1",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007884.1.5",
          "form": "ㅋㅋ 완전 다른거 드셨네요",
          "original_form": "ㅋㅋ 완전 다른거 드셨네요",
          "speaker_id": "2",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007884.1.6",
          "form": "주말 점심 간단히 먹기 좋죠",
          "original_form": "주말 점심 간단히 먹기 좋죠",
          "speaker_id": "2",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007884.1.7",
          "form": "주말도 집안일은 계속돼요",
          "original_form": "주말도 집안일은 계속돼요",
          "speaker_id": "2",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007884.1.8",
          "form": "밥에 설거지에 빨래에",
          "original_form": "밥에 설거지에 빨래에",
          "speaker_id": "2",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007884.1.9",
          "form": "바빴네요 ㅎㅎ",
          "original_form": "바빴네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007884.1.10",
          "form": "청소 하셨어요?",
          "original_form": "청소 하셨어요?",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007884.1.11",
          "form": "주말에 대청소 하시죠?",
          "original_form": "주말에 대청소 하시죠?",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007884.1.12",
          "form": "주중엔 너무 늦으실 테고",
          "original_form": "주중엔 너무 늦으실 테고",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007884.1.13",
          "form": "네 화장실 필수요",
          "original_form": "네 화장실 필수요",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007884.1.14",
          "form": "오 잘하셨어요 ㅎㅎ화장실 더러우면 음",
          "original_form": "오 잘하셨어요 ㅎㅎ화장실 더러우면 음",
          "speaker_id": "2",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007884.1.15",
          "form": "안돼죠",
          "original_form": "안돼죠",
          "speaker_id": "2",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007884.1.16",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007884.1.17",
          "form": "전 배달 어제 시켜서 올만에 아침 패쓰하고 점심겸 스테이크 했어요 ㅋㅋ",
          "original_form": "전 배달 어제 시켜서 올만에 아침 패쓰하고 점심겸 스테이크 했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007884.1.18",
          "form": "맞아요 ㅡㅡ",
          "original_form": "맞아요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007884.1.19",
          "form": "홈스타로 뿌려놓고 좀나면 깨끗해져서 편해요",
          "original_form": "홈스타로 뿌려놓고 좀나면 깨끗해져서 편해요",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007884.1.20",
          "form": "락스냄새 꼭 문열고 하세요",
          "original_form": "락스냄새 꼭 문열고 하세요",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007884.1.21",
          "form": "냄새 질식해요",
          "original_form": "냄새 질식해요",
          "speaker_id": "2",
          "time": "20210822 12:31"
        },
        {
          "id": "MDRW2100007884.1.22",
          "form": "근데 락스 안쓸수 없어요",
          "original_form": "근데 락스 안쓸수 없어요",
          "speaker_id": "2",
          "time": "20210822 12:31"
        },
        {
          "id": "MDRW2100007884.1.23",
          "form": "딴건 청소에 효과 없어요",
          "original_form": "딴건 청소에 효과 없어요",
          "speaker_id": "2",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100007884.1.24",
          "form": "요즘 곰팡이 제거 스프레이도 있더라구요",
          "original_form": "요즘 곰팡이 제거 스프레이도 있더라구요",
          "speaker_id": "1",
          "time": "20210822 12:33"
        },
        {
          "id": "MDRW2100007884.1.25",
          "form": "네 다있됴 어차피 락스 성분이더라구요",
          "original_form": "네 다있됴 어차피 락스 성분이더라구요",
          "speaker_id": "2",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100007884.1.26",
          "form": "전 홈스타 써요",
          "original_form": "전 홈스타 써요",
          "speaker_id": "2",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100007884.1.27",
          "form": "청소 필수품이죠",
          "original_form": "청소 필수품이죠",
          "speaker_id": "2",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100007884.1.28",
          "form": "착착 뿌리고",
          "original_form": "착착 뿌리고",
          "speaker_id": "2",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100007884.1.29",
          "form": "주말엔 요새 코로나로 어디 안가니 청소후엔 여유롭죠",
          "original_form": "주말엔 요새 코로나로 어디 안가니 청소후엔 여유롭죠",
          "speaker_id": "2",
          "time": "20210822 12:37"
        },
        {
          "id": "MDRW2100007884.1.30",
          "form": "딱 낮잠자고 쉬기 좋은가 남편 쭉자던데요",
          "original_form": "딱 낮잠자고 쉬기 좋은가 남편 쭉자던데요",
          "speaker_id": "2",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100007884.1.31",
          "form": "청소하면 삭신이 ㅎㅎ",
          "original_form": "청소하면 삭신이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100007884.1.32",
          "form": "ㅋㅋ엄살이 심하신거 아닐까요",
          "original_form": "ㅋㅋ엄살이 심하신거 아닐까요",
          "speaker_id": "2",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100007884.1.33",
          "form": "오 열정적으로 청소 하시나보디",
          "original_form": "오 열정적으로 청소 하시나보디",
          "speaker_id": "2",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100007884.1.34",
          "form": "다",
          "original_form": "다",
          "speaker_id": "2",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100007884.1.35",
          "form": "것보다 늙은 거죠",
          "original_form": "것보다 늙은 거죠",
          "speaker_id": "1",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100007884.1.36",
          "form": "헉 슬프네요",
          "original_form": "헉 슬프네요",
          "speaker_id": "2",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100007884.1.37",
          "form": "결국 나이는 못속여요",
          "original_form": "결국 나이는 못속여요",
          "speaker_id": "2",
          "time": "20210822 12:51"
        }
      ]
    }
