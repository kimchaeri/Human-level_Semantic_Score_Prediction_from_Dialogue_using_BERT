      "utterance": [
        {
          "id": "MDRW2100021991.1.1",
          "form": "머리띠같은 악세서리도 잘팔려",
          "original_form": "머리띠같은 악세서리도 잘팔려",
          "speaker_id": "2",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021991.1.2",
          "form": "니손재주면..이쁘게 잘뽑을듯",
          "original_form": "니손재주면..이쁘게 잘뽑을듯",
          "speaker_id": "2",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021991.1.3",
          "form": "디자인도 예뻐야하고",
          "original_form": "디자인도 예뻐야하고",
          "speaker_id": "1",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021991.1.4",
          "form": "나는 믿고있다!!!",
          "original_form": "나는 믿고있다!!!",
          "speaker_id": "2",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021991.1.5",
          "form": "일단 디자인이랑 니말대로 퀄 중요할듯",
          "original_form": "일단 디자인이랑 니말대로 퀄 중요할듯",
          "speaker_id": "1",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021991.1.6",
          "form": "데코덴이 나은듭(?",
          "original_form": "데코덴이 나은듭(?",
          "speaker_id": "1",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021991.1.7",
          "form": "아니 그거 안팔린다며 ㅋㅋ",
          "original_form": "아니 그거 안팔린다며 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021991.1.8",
          "form": "안팔리는게 아니라",
          "original_form": "안팔리는게 아니라",
          "speaker_id": "1",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021991.1.9",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "2",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021991.1.10",
          "form": "이건 약간 덕후들용이라서 얘네들은 알잖아",
          "original_form": "이건 약간 덕후들용이라서 얘네들은 알잖아",
          "speaker_id": "1",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021991.1.11",
          "form": "덕후아니라도 좋아할꺼같은데",
          "original_form": "덕후아니라도 좋아할꺼같은데",
          "speaker_id": "2",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021991.1.12",
          "form": "아닌가..",
          "original_form": "아닌가..",
          "speaker_id": "2",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021991.1.13",
          "form": "그런 뭔가~ 그런 팔로워키워서",
          "original_form": "그런 뭔가~ 그런 팔로워키워서",
          "speaker_id": "1",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021991.1.14",
          "form": "이벤트도하고 커미션도 받고 이런식으로 계정을 키워야 가능한데",
          "original_form": "이벤트도하고 커미션도 받고 이런식으로 계정을 키워야 가능한데",
          "speaker_id": "1",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021991.1.15",
          "form": "뭐든 꾸준히 노력해야하네..",
          "original_form": "뭐든 꾸준히 노력해야하네..",
          "speaker_id": "2",
          "time": "20210829 00:33"
        },
        {
          "id": "MDRW2100021991.1.16",
          "form": "그정도까진ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그정도까진ㅋㅋㅋㅋㅋㅋ",
