      "utterance": [
        {
          "id": "MDRW2100028149.1.1",
          "form": "헉 자몽 에이드 땡기네요",
          "original_form": "헉 자몽 에이드 땡기네요",
          "speaker_id": "2",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100028149.1.2",
          "form": "ㅋㅋ탄산음료 먹고싶네요",
          "original_form": "ㅋㅋ탄산음료 먹고싶네요",
          "speaker_id": "1",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028149.1.3",
          "form": "피자나 치킨이랑ㅎ",
          "original_form": "피자나 치킨이랑ㅎ",
          "speaker_id": "1",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028149.1.4",
          "form": "헉. 피자치킨이랑 콜라사이다 너무 먹고 싶어요 너무 땡겨요",
          "original_form": "헉. 피자치킨이랑 콜라사이다 너무 먹고 싶어요 너무 땡겨요",
          "speaker_id": "2",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100028149.1.5",
          "form": "저녁 먹을 시간이라 배고파서 더 생각나네요ㅋㅋㅋ",
          "original_form": "저녁 먹을 시간이라 배고파서 더 생각나네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028149.1.6",
          "form": "진짜요ㅋㅋ 탄산음료 급땡기네요... 마셔야겠어요,,",
          "original_form": "진짜요ㅋㅋ 탄산음료 급땡기네요... 마셔야겠어요,,",
          "speaker_id": "2",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028149.1.7",
          "form": "지나가면 보이는거 다 먹고싶네요",
          "original_form": "지나가면 보이는거 다 먹고싶네요",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028149.1.8",
          "form": "면도 좋아하는데 왜이렇게 면 음식이 많은지ㅋㅋ",
          "original_form": "면도 좋아하는데 왜이렇게 면 음식이 많은지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028149.1.9",
          "form": "저도 면 엄청 좋아해요",
          "original_form": "저도 면 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100028149.1.10",
          "form": "우동.. 라면...",
          "original_form": "우동.. 라면...",
          "speaker_id": "2",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100028149.1.11",
          "form": "짬뽕... 잔치국수...ㅋㅋㅋ",
          "original_form": "짬뽕... 잔치국수...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100028149.1.12",
          "form": "헐... 너무 맛있을 것 같아요 잔치국수 진짜 오랜만에 먹고 싶네요",
          "original_form": "헐... 너무 맛있을 것 같아요 잔치국수 진짜 오랜만에 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028149.1.13",
          "form": "멸치육수에 만든거 맛있잖아요",
          "original_form": "멸치육수에 만든거 맛있잖아요",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028149.1.14",
          "form": "여름에 콩국수도 자주 먹었어요",
          "original_form": "여름에 콩국수도 자주 먹었어요",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028149.1.15",
          "form": "콩국수 맛있나요? 한 두 번 먹어봐서 맛이 기억이 안나네요 흠",
          "original_form": "콩국수 맛있나요? 한 두 번 먹어봐서 맛이 기억이 안나네요 흠",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028149.1.16",
          "form": "콩국물 고소한 맛때문에 먹어요",
          "original_form": "콩국물 고소한 맛때문에 먹어요",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028149.1.17",
          "form": "밍밍하게 느끼는 사람도 많던데 소금이나 설탕 넣으면 되니까요ㅋㅋ",
          "original_form": "밍밍하게 느끼는 사람도 많던데 소금이나 설탕 넣으면 되니까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028149.1.18",
          "form": "그쵸! 생각하니까 시원한 국수 먹고 싶네요",
          "original_form": "그쵸! 생각하니까 시원한 국수 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028149.1.19",
          "form": "열무국수도 맛있겠네요ㅋㅋ",
          "original_form": "열무국수도 맛있겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:21"
        }
