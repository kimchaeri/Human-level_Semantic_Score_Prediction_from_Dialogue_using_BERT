      "utterance": [
        {
          "id": "MDRW2100041861.1.1",
          "form": "뭐주노ㅠㅠ 줄게없다",
          "original_form": "뭐주노ㅠㅠ 줄게없다",
          "speaker_id": "1",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100041861.1.2",
          "form": "ㅋㅋㅋㅋ그러게 ㅋㅋ 진자 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ그러게 ㅋㅋ 진자 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100041861.1.3",
          "form": "다른것들도 줘봤을거 아이가 ㅋㅋ",
          "original_form": "다른것들도 줘봤을거 아이가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100041861.1.4",
          "form": "미치겠다ㅠ 김도안먹으면",
          "original_form": "미치겠다ㅠ 김도안먹으면",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100041861.1.5",
          "form": "뭐 있지 추천점 해줘바",
          "original_form": "뭐 있지 추천점 해줘바",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100041861.1.6",
          "form": "애들 진짜 왠만하면 김 좋아하는데 ㅋ",
          "original_form": "애들 진짜 왠만하면 김 좋아하는데 ㅋ",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100041861.1.7",
          "form": "뭐가 있껬노 ㅋㅋ",
          "original_form": "뭐가 있껬노 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100041861.1.8",
          "form": "우리 두찌는......김......갈비.....고기.......미역국.......특히 좋아하고",
          "original_form": "우리 두찌는......김......갈비.....고기.......미역국.......특히 좋아하고",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100041861.1.9",
          "form": "그니까 김을 안먹는애가 있다니 충격",
          "original_form": "그니까 김을 안먹는애가 있다니 충격",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100041861.1.10",
          "form": "계란은 안좋아하더라",
          "original_form": "계란은 안좋아하더라",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100041861.1.11",
          "form": "그러니깐 ㅋㅋ",
          "original_form": "그러니깐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100041861.1.12",
          "form": "계란 여기도 안먹음 웃긴거는 삶은 계란은 먹음",
          "original_form": "계란 여기도 안먹음 웃긴거는 삶은 계란은 먹음",
          "speaker_id": "1",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100041861.1.13",
          "form": "흰자만",
          "original_form": "흰자만",
          "speaker_id": "1",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100041861.1.14",
          "form": "국도 안먹고ㅠ",
          "original_form": "국도 안먹고ㅠ",
          "speaker_id": "1",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100041861.1.15",
          "form": "ㅋㅋㅋㅋㅋ희자만 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ희자만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100041861.1.16",
          "form": "식감 되게 예민",
          "original_form": "식감 되게 예민",
