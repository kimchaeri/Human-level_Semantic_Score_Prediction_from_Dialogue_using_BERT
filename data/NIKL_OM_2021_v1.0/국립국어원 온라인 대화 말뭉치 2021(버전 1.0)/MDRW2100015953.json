        {
          "id": "MDRW2100015953.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100015953.1.2",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100015953.1.3",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100015953.1.4",
          "form": "아핫",
          "original_form": "아핫",
          "speaker_id": "1",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100015953.1.5",
          "form": "대학생이시겠군요",
          "original_form": "대학생이시겠군요",
          "speaker_id": "1",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100015953.1.6",
          "form": "저는 대학교 졸업했어요!",
          "original_form": "저는 대학교 졸업했어요!",
          "speaker_id": "1",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100015953.1.7",
          "form": "네 다니는 중이에요",
          "original_form": "네 다니는 중이에요",
          "speaker_id": "2",
          "time": "20210826 03:39"
        },
        {
          "id": "MDRW2100015953.1.8",
          "form": "몇학년이세요?",
          "original_form": "몇학년이세요?",
          "speaker_id": "1",
          "time": "20210826 03:39"
        },
        {
          "id": "MDRW2100015953.1.9",
          "form": "2학년이요",
          "original_form": "2학년이요",
          "speaker_id": "2",
          "time": "20210826 03:39"
        },
        {
          "id": "MDRW2100015953.1.10",
          "form": "곧 있으면 개강이죠?",
          "original_form": "곧 있으면 개강이죠?",
          "speaker_id": "1",
          "time": "20210826 03:39"
        },
        {
          "id": "MDRW2100015953.1.11",
          "form": "네.다음주... ㅠㅠ",
          "original_form": "네.다음주... ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 03:40"
        },
        {
          "id": "MDRW2100015953.1.12",
          "form": "요즘도 수업 온라인으로 하나용?",
          "original_form": "요즘도 수업 온라인으로 하나용?",
          "speaker_id": "1",
          "time": "20210826 03:40"
        },
        {
          "id": "MDRW2100015953.1.13",
          "form": "다 비대면이에요 ㅋㅋ 줌으로 해요",
          "original_form": "다 비대면이에요 ㅋㅋ 줌으로 해요",
          "speaker_id": "2",
          "time": "20210826 03:40"
        },
        {
          "id": "MDRW2100015953.1.14",
          "form": "아하ㅠㅠ 코로나 터지고 입학하셔서",
          "original_form": "아하ㅠㅠ 코로나 터지고 입학하셔서",
          "speaker_id": "1",
          "time": "20210826 03:41"
        },
        {
          "id": "MDRW2100015953.1.15",
          "form": "추억 쌓기가 어려우셨겠네요ㅠ",
          "original_form": "추억 쌓기가 어려우셨겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 03:41"
        },
        {
          "id": "MDRW2100015953.1.16",
          "form": "맞아요 학교 가본 게 손에 꼽아요",
          "original_form": "맞아요 학교 가본 게 손에 꼽아요",
