      "utterance": [
        {
          "id": "MDRW2100034224.1.1",
          "form": "엄마 다낭 여행 어땠어?",
          "original_form": "엄마 다낭 여행 어땠어?",
          "speaker_id": "2",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100034224.1.2",
          "form": "좋았지",
          "original_form": "좋았지",
          "speaker_id": "1",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100034224.1.3",
          "form": "뭐가 제일 좋았어?",
          "original_form": "뭐가 제일 좋았어?",
          "speaker_id": "2",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100034224.1.4",
          "form": "마사지 받는거 ㅋㅋ",
          "original_form": "마사지 받는거 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100034224.1.5",
          "form": "오 ㅋㅋ마자 마사지가 짱이었다",
          "original_form": "오 ㅋㅋ마자 마사지가 짱이었다",
          "speaker_id": "2",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100034224.1.6",
          "form": "그리고 열대과일",
          "original_form": "그리고 열대과일",
          "speaker_id": "1",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100034224.1.7",
          "form": "오 ㅠㅠ마자 넘 맛있었지",
          "original_form": "오 ㅠㅠ마자 넘 맛있었지",
          "speaker_id": "2",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100034224.1.8",
          "form": "또 먹고싶네",
          "original_form": "또 먹고싶네",
          "speaker_id": "1",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100034224.1.9",
          "form": "나도.. 넘 좋았다 담에 또 가요",
          "original_form": "나도.. 넘 좋았다 담에 또 가요",
          "speaker_id": "2",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100034224.1.10",
          "form": "그럴까",
          "original_form": "그럴까",
          "speaker_id": "1",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100034224.1.11",
          "form": "다낭 물가도 싸고~ 넘 좋다 ㅋㅋ",
          "original_form": "다낭 물가도 싸고~ 넘 좋다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100034224.1.12",
          "form": "그래 치안도 나름 괜찮은듯",
          "original_form": "그래 치안도 나름 괜찮은듯",
          "speaker_id": "1",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100034224.1.13",
          "form": "ㅋㅋ웅 안전한데만 다니면 된다",
          "original_form": "ㅋㅋ웅 안전한데만 다니면 된다",
