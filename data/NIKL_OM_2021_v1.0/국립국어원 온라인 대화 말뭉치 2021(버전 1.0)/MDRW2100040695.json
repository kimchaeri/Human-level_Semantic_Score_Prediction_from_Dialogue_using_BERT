      "utterance": [
        {
          "id": "MDRW2100040695.1.1",
          "form": "그애엄마한테 물으니??",
          "original_form": "그애엄마한테 물으니??",
          "speaker_id": "2",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040695.1.2",
          "form": "근데 그엄마가 아. 친구들도 사주고 하라고 그렇게 줬어요. 했다든데 ㅋㅋ 볼때마다 만원들고있더란다",
          "original_form": "근데 그엄마가 아. 친구들도 사주고 하라고 그렇게 줬어요. 했다든데 ㅋㅋ 볼때마다 만원들고있더란다",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040695.1.3",
          "form": "그 만원 거의 하루에 다 쓸때도 있고 하는데도",
          "original_form": "그 만원 거의 하루에 다 쓸때도 있고 하는데도",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040695.1.4",
          "form": "주는가보더라고",
          "original_form": "주는가보더라고",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040695.1.5",
          "form": "아 정말 많이 주네",
          "original_form": "아 정말 많이 주네",
          "speaker_id": "2",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040695.1.6",
          "form": "계속 얻어먹을 수도 없고",
          "original_form": "계속 얻어먹을 수도 없고",
          "speaker_id": "2",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040695.1.7",
          "form": "그러이",
          "original_form": "그러이",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040695.1.8",
          "form": "그게문제ㅋㅋ",
          "original_form": "그게문제ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040695.1.9",
          "form": "자꾸 얻어먹는거지",
          "original_form": "자꾸 얻어먹는거지",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040695.1.10",
          "form": "그래 그러면 니 친구는 용돈을 안주나??",
          "original_form": "그래 그러면 니 친구는 용돈을 안주나??",
          "speaker_id": "2",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040695.1.11",
          "form": "공부하면",
          "original_form": "공부하면",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040695.1.12",
          "form": "백원",
          "original_form": "백원",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040695.1.13",
          "form": "오백원 이렇게주는데 일주일에 1-2천원되는것같다고",
          "original_form": "오백원 이렇게주는데 일주일에 1-2천원되는것같다고",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040695.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040695.1.15",
          "form": "갭이 너무 크네",
          "original_form": "갭이 너무 크네",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040695.1.16",
          "form": "하면서ㅋㅋ",
          "original_form": "하면서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040695.1.17",
          "form": "응 그러니깐 친구아들이",
          "original_form": "응 그러니깐 친구아들이",
          "speaker_id": "1",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040695.1.18",
          "form": "아 일주일에 그정도면 괜찮지",
          "original_form": "아 일주일에 그정도면 괜찮지",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040695.1.19",
          "form": "자꾸 얻어먹는거야",
          "original_form": "자꾸 얻어먹는거야",
          "speaker_id": "1",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040695.1.20",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040695.1.21",
          "form": "그래서 넘 신경쓰인다구",
          "original_form": "그래서 넘 신경쓰인다구",
          "speaker_id": "1",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040695.1.22",
          "form": "그럼 또 그렇겠다",
          "original_form": "그럼 또 그렇겠다",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040695.1.23",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040695.1.24",
          "form": "남 아들 용돈 적게 주라고 할 수도 없고",
          "original_form": "남 아들 용돈 적게 주라고 할 수도 없고",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040695.1.25",
          "form": "난감하네",
          "original_form": "난감하네",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040695.1.26",
          "form": "그래서 어떻게해야될지모르겠다구 전화왔더라고ㅋ",
          "original_form": "그래서 어떻게해야될지모르겠다구 전화왔더라고ㅋ",
          "speaker_id": "1",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040695.1.27",
          "form": "그렇다 서로 비슷하게 들고 와야지",
          "original_form": "그렇다 서로 비슷하게 들고 와야지",
          "speaker_id": "2",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100040695.1.28",
          "form": "그러이",
          "original_form": "그러이",
          "speaker_id": "1",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100040695.1.29",
          "form": "그렇네",
          "original_form": "그렇네",
          "speaker_id": "2",
