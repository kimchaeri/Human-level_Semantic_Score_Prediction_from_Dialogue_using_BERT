      "utterance": [
        {
          "id": "MDRW2100002961.1.1",
          "form": "연애하시나요? 결혼하셨나요?",
          "original_form": "연애하시나요? 결혼하셨나요?",
          "speaker_id": "1",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002961.1.2",
          "form": "안녕하세요ㅋㅋㅋ",
          "original_form": "안녕하세요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002961.1.3",
          "form": "대한유부입니다",
          "original_form": "대한유부입니다",
          "speaker_id": "2",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002961.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210820 17:06"
        },
        {
          "id": "MDRW2100002961.1.5",
          "form": "연애하고싶은 유부요...\r\n남편하고 결혼관 잘 맞으세요?",
          "original_form": "연애하고싶은 유부요...\r\n남편하고 결혼관 잘 맞으세요?",
          "speaker_id": "1",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002961.1.6",
          "form": "음.... 결혼빼고 관이 잘 맞을 거 같네요ㅜㅜ",
          "original_form": "음.... 결혼빼고 관이 잘 맞을 거 같네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002961.1.7",
          "form": "결혼생활 너무 어려워요ㅋㅋㅋㅋㅋ",
          "original_form": "결혼생활 너무 어려워요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002961.1.8",
          "form": "하 공감입니다\r\n모든 남자가 이런건가요?",
          "original_form": "하 공감입니다\r\n모든 남자가 이런건가요?",
          "speaker_id": "1",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002961.1.9",
          "form": "ㅋㅋㅋㅋㅋ이래서\r\n유부여도 연애가 고픈가 봅니다",
          "original_form": "ㅋㅋㅋㅋㅋ이래서\r\n유부여도 연애가 고픈가 봅니다",
          "speaker_id": "2",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002961.1.10",
          "form": "죄송하지만 초면에 위안이 되네옄ㅋㅋ",
          "original_form": "죄송하지만 초면에 위안이 되네옄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002961.1.11",
          "form": "연애는오래하셨어요?",
          "original_form": "연애는오래하셨어요?",
          "speaker_id": "1",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002961.1.12",
          "form": "신행 다녀와서가 1년이였어염ㅋㅋㅋ",
          "original_form": "신행 다녀와서가 1년이였어염ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002961.1.13",
          "form": "연애는 짧은데\r\n알고지낸 시간이 긴 사이ㅋㅋ",
          "original_form": "연애는 짧은데\r\n알고지낸 시간이 긴 사이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002961.1.14",
          "form": "연애는 짧게 여러명하고 많이 해보는게좋은것같아요",
          "original_form": "연애는 짧게 여러명하고 많이 해보는게좋은것같아요",
          "speaker_id": "1",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002961.1.15",
          "form": "name1님은 연애 많이 해보셨나요?",
          "original_form": "&name1&님은 연애 많이 해보셨나요?",
          "speaker_id": "2",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002961.1.16",
          "form": "저는 썸까지가 좋아서\r\n고백할거 같음 손절 손절 했었어서ㅋㅋ",
          "original_form": "저는 썸까지가 좋아서\r\n고백할거 같음 손절 손절 했었어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002961.1.17",
          "form": "연애보다 소개팅이요!\r\n모든 남자들의 직업을 파악했던것같아요 ㅎㅎ",
          "original_form": "연애보다 소개팅이요!\r\n모든 남자들의 직업을 파악했던것같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002961.1.18",
          "form": "오ㅋㅋㅋㅋ 저는 자만추라",
          "original_form": "오ㅋㅋㅋㅋ 저는 자만추라",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002961.1.19",
          "form": "소개팅의 그 어색함을 못견뎌서ㅜㅜ",
          "original_form": "소개팅의 그 어색함을 못견뎌서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002961.1.20",
          "form": "몇 번 안해본거 같아요ㅋ",
          "original_form": "몇 번 안해본거 같아요ㅋ",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002961.1.21",
          "form": "낯가림도 심하고ㅋ",
          "original_form": "낯가림도 심하고ㅋ",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002961.1.22",
          "form": "소개팅하면서 파악한\r\n직업중 매력적이었던\r\n직업은 뭐였나요?",
          "original_form": "소개팅하면서 파악한\r\n직업중 매력적이었던\r\n직업은 뭐였나요?",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002961.1.23",
          "form": "자동차딜러요 소개팅나갔다가 제가 살뻔했습니다",
          "original_form": "자동차딜러요 소개팅나갔다가 제가 살뻔했습니다",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002961.1.24",
          "form": "갑자기 설레이고싶어지네요 연애 주제에",
          "original_form": "갑자기 설레이고싶어지네요 연애 주제에",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002961.1.25",
          "form": "ㅋㅋㅋㅋ앜ㅋㅋㅋ 그정도던가요?",
          "original_form": "ㅋㅋㅋㅋ앜ㅋㅋㅋ 그정도던가요?",
          "speaker_id": "2",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002961.1.26",
          "form": "딜러님이 소개팅에서\r\n영업도 하시던가요?",
          "original_form": "딜러님이 소개팅에서\r\n영업도 하시던가요?",
          "speaker_id": "2",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002961.1.27",
          "form": "차 자랑을 하는데 사고싶더라구요ㅎㅎ\r\n진짜 다양한직업에 다양한남자경험이 가능한게 소개팅같아요ㅎㅎ",
          "original_form": "차 자랑을 하는데 사고싶더라구요ㅎㅎ\r\n진짜 다양한직업에 다양한남자경험이 가능한게 소개팅같아요ㅎㅎ",
          "speaker_id": "1",
