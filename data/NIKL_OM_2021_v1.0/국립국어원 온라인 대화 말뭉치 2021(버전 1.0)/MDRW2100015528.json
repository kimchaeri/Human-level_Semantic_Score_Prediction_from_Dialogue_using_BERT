      "utterance": [
        {
          "id": "MDRW2100015528.1.1",
          "form": "여행좋아하시나요",
          "original_form": "여행좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015528.1.2",
          "form": "네 여행 엄청 좋아해요!",
          "original_form": "네 여행 엄청 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015528.1.3",
          "form": "어디 가보셨나요",
          "original_form": "어디 가보셨나요",
          "speaker_id": "1",
          "time": "20210826 10:21"
        },
        {
          "id": "MDRW2100015528.1.4",
          "form": "저 제주도 가봤어요!",
          "original_form": "저 제주도 가봤어요!",
          "speaker_id": "2",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100015528.1.5",
          "form": "오 섭지코지 넘 좋아요",
          "original_form": "오 섭지코지 넘 좋아요",
          "speaker_id": "1",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100015528.1.6",
          "form": "맞아여 ㅠㅠ",
          "original_form": "맞아여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100015528.1.7",
          "form": "경치가 진짜 좋더라구요",
          "original_form": "경치가 진짜 좋더라구요",
          "speaker_id": "1",
          "time": "20210826 12:08"
        },
        {
          "id": "MDRW2100015528.1.8",
          "form": "개인적으로",
          "original_form": "개인적으로",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100015528.1.9",
          "form": "성산일출봉이 진짜 경치보기에 너무 좋은거같더라고요",
          "original_form": "성산일출봉이 진짜 경치보기에 너무 좋은거같더라고요",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100015528.1.10",
          "form": "한라산이랑..",
          "original_form": "한라산이랑..",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100015528.1.11",
          "form": "오백록담 가셨나요",
          "original_form": "오백록담 가셨나요",
          "speaker_id": "1",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100015528.1.12",
          "form": "오백록담도 가봤죠!",
          "original_form": "오백록담도 가봤죠!",
          "speaker_id": "2",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100015528.1.13",
          "form": "그리고 마라도도 가봤구용",
          "original_form": "그리고 마라도도 가봤구용",
          "speaker_id": "2",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100015528.1.14",
          "form": "와 백록담 멋있나요버킷리스트인데",
          "original_form": "와 백록담 멋있나요버킷리스트인데",
          "speaker_id": "1",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100015528.1.15",
          "form": "갠적으로 진짜 한번쯤",
          "original_form": "갠적으로 진짜 한번쯤",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100015528.1.16",
          "form": "가봐야할곳이죠..",
          "original_form": "가봐야할곳이죠..",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100015528.1.17",
          "form": "꼭 가보세요!",
          "original_form": "꼭 가보세요!",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100015528.1.18",
          "form": "오 감사합니다!",
          "original_form": "오 감사합니다!",
          "speaker_id": "1",
          "time": "20210826 12:27"
        }
      ]
