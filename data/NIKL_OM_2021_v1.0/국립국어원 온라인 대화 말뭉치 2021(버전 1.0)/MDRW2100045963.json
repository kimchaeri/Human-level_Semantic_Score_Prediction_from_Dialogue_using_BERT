        {
          "id": "MDRW2100045963.1.1",
          "form": "다음주 식스센스말이야",
          "original_form": "다음주 식스센스말이야",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045963.1.2",
          "form": "예능말하는거지?",
          "original_form": "예능말하는거지?",
          "speaker_id": "1",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045963.1.3",
          "form": "응 그거",
          "original_form": "응 그거",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045963.1.4",
          "form": "그게 왜?",
          "original_form": "그게 왜?",
          "speaker_id": "1",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045963.1.5",
          "form": "다음주 게스트가 키래",
          "original_form": "다음주 게스트가 키래",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045963.1.6",
          "form": "샤이니?",
          "original_form": "샤이니?",
          "speaker_id": "1",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045963.1.7",
          "form": "응 그래섬기대돼",
          "original_form": "응 그래섬기대돼",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045963.1.8",
          "form": "키 예능잘하고 재밌지",
          "original_form": "키 예능잘하고 재밌지",
          "speaker_id": "1",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045963.1.9",
          "form": "그것보다 내가 샤이니 팬이라",
          "original_form": "그것보다 내가 샤이니 팬이라",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045963.1.10",
          "form": "얼른 기대되겠네",
          "original_form": "얼른 기대되겠네",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045963.1.11",
          "form": "안그래도 이 예능재밌는데",
          "original_form": "안그래도 이 예능재밌는데",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045963.1.12",
          "form": "진짜 좋겠다 너",
          "original_form": "진짜 좋겠다 너",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045963.1.13",
          "form": "빨리 담주에 보고싶어",
          "original_form": "빨리 담주에 보고싶어",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045963.1.14",
          "form": "얼른 예고편떴으면좋겠다",
          "original_form": "얼른 예고편떴으면좋겠다",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045963.1.15",
          "form": "그러게",
