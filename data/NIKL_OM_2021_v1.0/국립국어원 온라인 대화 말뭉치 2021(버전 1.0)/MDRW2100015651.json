        },
        {
          "id": "MDRW2100015651.1.2",
          "form": "제 이상형은요",
          "original_form": "제 이상형은요",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015651.1.3",
          "form": "너무 길지만",
          "original_form": "너무 길지만",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015651.1.4",
          "form": "일단 3무",
          "original_form": "일단 3무",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015651.1.5",
          "form": "담배 게임 오토바이 안할것",
          "original_form": "담배 게임 오토바이 안할것",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015651.1.6",
          "form": "두근두근",
          "original_form": "두근두근",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015651.1.7",
          "form": "가장 기초.",
          "original_form": "가장 기초.",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015651.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015651.1.9",
          "form": "저걸 넘어야 카운트 시작",
          "original_form": "저걸 넘어야 카운트 시작",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015651.1.10",
          "form": "이거 3d프린팅으로 맞춤제작해야 한다",
          "original_form": "이거 3d프린팅으로 맞춤제작해야 한다",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015651.1.11",
          "form": "ㅋㅋㅋㅋㅋ인정",
          "original_form": "ㅋㅋㅋㅋㅋ인정",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015651.1.12",
          "form": "얼굴은 뽀얗고 코가 이뻐야함",
          "original_form": "얼굴은 뽀얗고 코가 이뻐야함",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015651.1.13",
          "form": "키는 178이상",
          "original_form": "키는 178이상",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015651.1.14",
          "form": "말이 많지않을것.",
          "original_form": "말이 많지않을것.",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015651.1.15",
          "form": "묵묵하게 자기일하는 타입",
          "original_form": "묵묵하게 자기일하는 타입",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015651.1.16",
          "form": "내 말 잘듣고 토달지말것",
          "original_form": "내 말 잘듣고 토달지말것",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015651.1.17",
          "form": "벌레를 무서워하지 않을것",
          "original_form": "벌레를 무서워하지 않을것",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015651.1.18",
          "form": "ㅋㅋㅋ어떻게 더 말해?",
          "original_form": "ㅋㅋㅋ어떻게 더 말해?",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015651.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015651.1.20",
          "form": "어흌ㅋㅋㅋㅋㅋ",
          "original_form": "어흌ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015651.1.21",
          "form": "맞춤제작 시급해",
          "original_form": "맞춤제작 시급해",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015651.1.22",
          "form": "name1님은 이상형이 어떻게 되시는지^^",
          "original_form": "&name1&님은 이상형이 어떻게 되시는지^^",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015651.1.23",
          "form": "나는 다정하고 나랑 잘 맞을 것",
          "original_form": "나는 다정하고 나랑 잘 맞을 것",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015651.1.24",
          "form": "나를 잘 서포트 해줄 것",
          "original_form": "나를 잘 서포트 해줄 것",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015651.1.25",
          "form": "돈많고 잘생길 것",
          "original_form": "돈많고 잘생길 것",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015651.1.26",
          "form": "여왕 대접 받을 거야",
          "original_form": "여왕 대접 받을 거야",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015651.1.27",
          "form": "공주 ㄴㄴ",
          "original_form": "공주 ㄴㄴ",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015651.1.28",
          "form": "주군 ㅇㅇㅇ",
          "original_form": "주군 ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015651.1.29",
          "form": "ㅋㅋㅋ하",
          "original_form": "ㅋㅋㅋ하",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015651.1.30",
          "form": "같이 마법사나 됩시다",
          "original_form": "같이 마법사나 됩시다",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015651.1.31",
          "form": "다정한게 최고",
          "original_form": "다정한게 최고",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015651.1.32",
          "form": "완벽하군",
          "original_form": "완벽하군",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015651.1.33",
          "form": "근데 말만 다정한건 안됨",
          "original_form": "근데 말만 다정한건 안됨",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015651.1.34",
          "form": "마법사 조건이 완벽해",
          "original_form": "마법사 조건이 완벽해",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015651.1.35",
          "form": "행동없이 말만 번지르르 아웃",
          "original_form": "행동없이 말만 번지르르 아웃",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015651.1.36",
          "form": "당연 행동으로 보여야 그게 찐 다정이지",
          "original_form": "당연 행동으로 보여야 그게 찐 다정이지",
