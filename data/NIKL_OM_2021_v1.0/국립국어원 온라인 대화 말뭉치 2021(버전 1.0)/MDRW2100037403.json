      "utterance": [
        {
          "id": "MDRW2100037403.1.1",
          "form": "저 예전엔 좋아했는데",
          "original_form": "저 예전엔 좋아했는데",
          "speaker_id": "2",
          "time": "20210901 16:33"
        },
        {
          "id": "MDRW2100037403.1.2",
          "form": "요샌 귀찮은 거 다 싫어요.....",
          "original_form": "요샌 귀찮은 거 다 싫어요.....",
          "speaker_id": "2",
          "time": "20210901 16:33"
        },
        {
          "id": "MDRW2100037403.1.3",
          "form": "우와 전 이제 막 시작해ㅐ서",
          "original_form": "우와 전 이제 막 시작해ㅐ서",
          "speaker_id": "1",
          "time": "20210901 16:41"
        },
        {
          "id": "MDRW2100037403.1.4",
          "form": "밀가루 6키로를 샀어요,,",
          "original_form": "밀가루 6키로를 샀어요,,",
          "speaker_id": "1",
          "time": "20210901 16:41"
        },
        {
          "id": "MDRW2100037403.1.5",
          "form": "좀 걱정되네요 밀가루에 쌀벌레가 생기기전에 다 먹어야되는데,,",
          "original_form": "좀 걱정되네요 밀가루에 쌀벌레가 생기기전에 다 먹어야되는데,,",
          "speaker_id": "1",
          "time": "20210901 16:41"
        },
        {
          "id": "MDRW2100037403.1.6",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "2",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100037403.1.7",
          "form": "어떤 거 만드실 건가요?",
          "original_form": "어떤 거 만드실 건가요?",
          "speaker_id": "2",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100037403.1.8",
          "form": "강력분 중력분 두가 삼키로씩 삿어요",
          "original_form": "강력분 중력분 두가 삼키로씩 삿어요",
          "speaker_id": "1",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100037403.1.9",
          "form": "치아바타 식빵, 케이크, 스콘 다 만들고 싶어요",
          "original_form": "치아바타 식빵, 케이크, 스콘 다 만들고 싶어요",
          "speaker_id": "1",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100037403.1.10",
          "form": "우오 치아바타..!",
          "original_form": "우오 치아바타..!",
          "speaker_id": "2",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100037403.1.11",
          "form": "쿠키는 안 만드시나요?",
          "original_form": "쿠키는 안 만드시나요?",
          "speaker_id": "2",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100037403.1.12",
          "form": "브라우니도 만들면 맛잇죠 쩝쩝",
          "original_form": "브라우니도 만들면 맛잇죠 쩝쩝",
          "speaker_id": "2",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100037403.1.13",
          "form": "ㅠㅠ 아직 만든건 케이크 하나여서",
          "original_form": "ㅠㅠ 아직 만든건 케이크 하나여서",
          "speaker_id": "1",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100037403.1.14",
          "form": "전 바스켓형 에어프라이어로 굽거든요",
          "original_form": "전 바스켓형 에어프라이어로 굽거든요",
          "speaker_id": "1",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100037403.1.15",
          "form": "그래서 되게 고민하다가 핸드믹서도 이제야 샀어요",
          "original_form": "그래서 되게 고민하다가 핸드믹서도 이제야 샀어요",
          "speaker_id": "1",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100037403.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100037403.1.17",
          "form": "케이크는 맛있게 되셨나요?",
          "original_form": "케이크는 맛있게 되셨나요?",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100037403.1.18",
          "form": "네 에프라서 반죽 높이를 낮게해서 잘 익었어요",
          "original_form": "네 에프라서 반죽 높이를 낮게해서 잘 익었어요",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100037403.1.19",
          "form": "열선이 위에만 있어서 두껍겐 구울수 없어요",
          "original_form": "열선이 위에만 있어서 두껍겐 구울수 없어요",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100037403.1.20",
          "form": "요새",
          "original_form": "요새",
          "speaker_id": "2",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100037403.1.21",
          "form": "저가형 오븐이 많던데 장만하시는 건 어떤가요",
          "original_form": "저가형 오븐이 많던데 장만하시는 건 어떤가요",
          "speaker_id": "2",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100037403.1.22",
          "form": "최대ㅐ한 짐을 안늘리고 싶어서요,,",
          "original_form": "최대ㅐ한 짐을 안늘리고 싶어서요,,",
          "speaker_id": "1",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100037403.1.23",
          "form": "굽고는 싶은데 장비는 늘리기 싫은 이맘,,",
          "original_form": "굽고는 싶은데 장비는 늘리기 싫은 이맘,,",
          "speaker_id": "1",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100037403.1.24",
          "form": "아앗....",
          "original_form": "아앗....",
          "speaker_id": "2",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100037403.1.25",
          "form": "그러면 쿠키가 딱이잖아요!",
          "original_form": "그러면 쿠키가 딱이잖아요!",
          "speaker_id": "2",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100037403.1.26",
          "form": "음,, 얘기하다보니 좀 ㅍ판단이 서는데 전 케이크를 더 좋아하나봐요",
          "original_form": "음,, 얘기하다보니 좀 ㅍ판단이 서는데 전 케이크를 더 좋아하나봐요",
          "speaker_id": "1",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100037403.1.27",
          "form": "빵집서 사려면 케이크가 더 비싸니까요,,,!",
          "original_form": "빵집서 사려면 케이크가 더 비싸니까요,,,!",
          "speaker_id": "1",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100037403.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037403.1.29",
          "form": "그렇긴하죠..",
          "original_form": "그렇긴하죠..",
          "speaker_id": "2",
          "time": "20210901 16:52"
        }
      ]
