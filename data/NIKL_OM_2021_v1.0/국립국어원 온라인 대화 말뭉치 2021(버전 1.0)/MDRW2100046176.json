      "utterance": [
        {
          "id": "MDRW2100046176.1.1",
          "form": "오다행",
          "original_form": "오다행",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100046176.1.2",
          "form": "아 어제 자다가",
          "original_form": "아 어제 자다가",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100046176.1.3",
          "form": "컨프롱꿈꿨자나",
          "original_form": "컨프롱꿈꿨자나",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100046176.1.4",
          "form": "ㅋㅋㅋㅋㄱㅋㄱ너무하드한노래를",
          "original_form": "ㅋㅋㅋㅋㄱㅋㄱ너무하드한노래를",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046176.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046176.1.6",
          "form": "연달아듣고자서그런가",
          "original_form": "연달아듣고자서그런가",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046176.1.7",
          "form": "저는 무난하게 금악 꿈",
          "original_form": "저는 무난하게 금악 꿈",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046176.1.8",
          "form": "무난하진 않지만...",
          "original_form": "무난하진 않지만...",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046176.1.9",
          "form": "언젠가 지킬 보고 오면",
          "original_form": "언젠가 지킬 보고 오면",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046176.1.10",
          "form": "ㅋㅋㅋㅋㄱㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㄱㅋㄱ",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046176.1.11",
          "form": "그날 컨프롱 꿀듯",
          "original_form": "그날 컨프롱 꿀듯",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046176.1.12",
          "form": "ㅋㄱㅋㅋㅋㅋㄱㄱㄱㅋㅋㅋ",
          "original_form": "ㅋㄱㅋㅋㅋㅋㄱㄱㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046176.1.13",
          "form": "근데현장감이라는건",
          "original_form": "근데현장감이라는건",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046176.1.14",
          "form": "아마무시하니까",
          "original_form": "아마무시하니까",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046176.1.15",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046176.1.16",
          "form": "압도당하는게 있지",
          "original_form": "압도당하는게 있지",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046176.1.17",
          "form": "아니 가사곱씹는데",
          "original_form": "아니 가사곱씹는데",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046176.1.18",
          "form": "소름돋아",
          "original_form": "소름돋아",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046176.1.19",
          "form": "그 무대에서부터 내자리까지 큰 파도같은게 쳐서 밀려오는 소름 쫙 돋는 기분...",
          "original_form": "그 무대에서부터 내자리까지 큰 파도같은게 쳐서 밀려오는 소름 쫙 돋는 기분...",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046176.1.20",
          "form": "살아 난 네안에 영원히",
          "original_form": "살아 난 네안에 영원히",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046176.1.21",
          "form": "컨프롱,",
          "original_form": "컨프롱,",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046176.1.22",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046176.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046176.1.24",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046176.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046176.1.26",
          "form": "라는가사가있는데",
          "original_form": "라는가사가있는데",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046176.1.27",
          "form": "아얼마나끔찍하겠어",
          "original_form": "아얼마나끔찍하겠어",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046176.1.28",
          "form": "그러고보니 지킬 지금이순간도",
          "original_form": "그러고보니 지킬 지금이순간도",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046176.1.29",
          "form": "인체실험 거부당하니까",
          "original_form": "인체실험 거부당하니까",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046176.1.30",
          "form": "지몸에실험하기전에",
          "original_form": "지몸에실험하기전에",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046176.1.31",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046176.1.32",
          "form": "신이있다면 난 신을 거부하고 내길가겠다",
          "original_form": "신이있다면 난 신을 거부하고 내길가겠다",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046176.1.33",
          "form": "그래서 축가로 사랑받는거",
          "original_form": "그래서 축가로 사랑받는거",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046176.1.34",
          "form": "보면서",
          "original_form": "보면서",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046176.1.35",
          "form": "기분이 좀..",
          "original_form": "기분이 좀..",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046176.1.36",
          "form": "넼ㅋㅋㅋㅋㅋㄱㅋㄱ",
          "original_form": "넼ㅋㅋㅋㅋㅋㄱㅋㄱ",
