      "utterance": [
        {
          "id": "MDRW2100033296.1.1",
          "form": "나는 지금 강의만 틀어두고 있어",
          "original_form": "나는 지금 강의만 틀어두고 있어",
          "speaker_id": "2",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100033296.1.2",
          "form": "오늘 책 와서 공부햇어?",
          "original_form": "오늘 책 와서 공부햇어?",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100033296.1.3",
          "form": "불량학생이다 불량학생",
          "original_form": "불량학생이다 불량학생",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033296.1.4",
          "form": "책 온거는 당근 했징!",
          "original_form": "책 온거는 당근 했징!",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033296.1.5",
          "form": "하고 씻었어",
          "original_form": "하고 씻었어",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033296.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033296.1.7",
          "form": "나 10시에 노래들으면서 한국사 공부할려고",
          "original_form": "나 10시에 노래들으면서 한국사 공부할려고",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033296.1.8",
          "form": "하려고? 할려고? 내생각엔 하려고야",
          "original_form": "하려고? 할려고? 내생각엔 하려고야",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033296.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033296.1.10",
          "form": "넘 어려웡",
          "original_form": "넘 어려웡",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033296.1.11",
          "form": "언니는 근데 강의 안들어???",
          "original_form": "언니는 근데 강의 안들어???",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033296.1.12",
          "form": "인턴하면 언제 강의 듣는거야?",
          "original_form": "인턴하면 언제 강의 듣는거야?",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100033296.1.13",
          "form": "언닌 이번에 두 과목밖에 안들어",
          "original_form": "언닌 이번에 두 과목밖에 안들어",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100033296.1.14",
          "form": "하나는 화목인데 10월 후반부터는 실습 예정이라",
          "original_form": "하나는 화목인데 10월 후반부터는 실습 예정이라",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100033296.1.15",
          "form": "연구실에는 말해뒀고",
          "original_form": "연구실에는 말해뒀고",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100033296.1.16",
          "form": "평소에는 비실시간 비대면이라 괜찮아",
          "original_form": "평소에는 비실시간 비대면이라 괜찮아",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033296.1.17",
          "form": "여차하면 지하철에서 듣지 뭐!",
          "original_form": "여차하면 지하철에서 듣지 뭐!",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033296.1.18",
          "form": "우와 역시 4학년!",
          "original_form": "우와 역시 4학년!",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033296.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033296.1.20",
          "form": "두개밖에 없으니까 시험 공부도 여유~",
          "original_form": "두개밖에 없으니까 시험 공부도 여유~",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033296.1.21",
          "form": "비실시간이라 다행이다",
          "original_form": "비실시간이라 다행이다",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033296.1.22",
          "form": "글고 실습은 언니가 이미 배운 실험들을 하더라구",
          "original_form": "글고 실습은 언니가 이미 배운 실험들을 하더라구",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033296.1.23",
          "form": "캬캬",
          "original_form": "캬캬",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033296.1.24",
          "form": "맞넹~~",
          "original_form": "맞넹~~",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033296.1.25",
          "form": "오 멋쟁이자낭",
          "original_form": "오 멋쟁이자낭",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033296.1.26",
          "form": "근데 노래 들으면서",
          "original_form": "근데 노래 들으면서",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033296.1.27",
          "form": "한국사 공부를 어떻게 해",
          "original_form": "한국사 공부를 어떻게 해",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033296.1.28",
          "form": "왜 못해!",
          "original_form": "왜 못해!",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100033296.1.29",
          "form": "그냥 외우면 되는거자나",
          "original_form": "그냥 외우면 되는거자나",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100033296.1.30",
          "form": "언니도 클레식 노래 들으면서 공부하잖아",
          "original_form": "언니도 클레식 노래 들으면서 공부하잖아",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100033296.1.31",
          "form": "흠냐링",
          "original_form": "흠냐링",
          "speaker_id": "1",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100033296.1.32",
          "form": "언니는 그냥 외부 소리 차단용으로",
          "original_form": "언니는 그냥 외부 소리 차단용으로",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100033296.1.33",
          "form": "노래를 듣는 거에 가까워",
          "original_form": "노래를 듣는 거에 가까워",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100033296.1.34",
          "form": "공부에 집중하기 위해!",
          "original_form": "공부에 집중하기 위해!",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100033296.1.35",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100033296.1.36",
          "form": "난 사실 노래들으면 공부 집중 못해",
          "original_form": "난 사실 노래들으면 공부 집중 못해",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100033296.1.37",
          "form": "히히",
