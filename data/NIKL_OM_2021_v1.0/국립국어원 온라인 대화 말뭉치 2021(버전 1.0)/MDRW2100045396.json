      "utterance": [
        {
          "id": "MDRW2100045396.1.1",
          "form": "어머님 작업실에서 보든",
          "original_form": "어머님 작업실에서 보든",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.2",
          "form": "랜선으로 따로 보든ㅇㅇㅇ",
          "original_form": "랜선으로 따로 보든ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.3",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.4",
          "form": "일단 왓챠에 좀",
          "original_form": "일단 왓챠에 좀",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.5",
          "form": "어떤거든 좋아여",
          "original_form": "어떤거든 좋아여",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.6",
          "form": "올려보렴",
          "original_form": "올려보렴",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.7",
          "form": "시나위야",
          "original_form": "시나위야",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.8",
          "form": "아 들어왔으면 좋겠다",
          "original_form": "아 들어왔으면 좋겠다",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.9",
          "form": "아근데",
          "original_form": "아근데",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.11",
          "form": "중계방송 빨리나왔으면",
          "original_form": "중계방송 빨리나왔으면",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.12",
          "form": "저도 궁금",
          "original_form": "저도 궁금",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ이게",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ이게",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.14",
          "form": "씬 몇개는",
          "original_form": "씬 몇개는",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.15",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.16",
          "form": "거기서 풀어주긴 했거든여",
          "original_form": "거기서 풀어주긴 했거든여",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.17",
          "form": "업슨ㄴ걸 보더라도",
          "original_form": "업슨ㄴ걸 보더라도",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.18",
          "form": "그거라도 봐보실래여..?",
          "original_form": "그거라도 봐보실래여..?",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045396.1.19",
          "form": "이해는 가나여?",
          "original_form": "이해는 가나여?",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045396.1.20",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045396.1.21",
          "form": "느낌정도만...?",
          "original_form": "느낌정도만...?",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045396.1.22",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045396.1.23",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045396.1.24",
          "form": "보내주세여",
          "original_form": "보내주세여",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045396.1.25",
          "form": "ㅇㅇㅇ 그게 노래만이 아니고 그 부근 연기도 안자르고 올려놔서",
          "original_form": "ㅇㅇㅇ 그게 노래만이 아니고 그 부근 연기도 안자르고 올려놔서",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045396.1.26",
          "form": "잠시만여",
          "original_form": "잠시만여",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045396.1.27",
          "form": "애애하다",
          "original_form": "애애하다",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045396.1.28",
          "form": "제몸ㄱ이 특이하다",
          "original_form": "제몸ㄱ이 특이하다",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045396.1.29",
          "form": "오오이이쁘다",
          "original_form": "오오이이쁘다",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045396.1.30",
          "form": "오오 무녀이야기인가여",
          "original_form": "오오 무녀이야기인가여",
          "speaker_id": "1",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100045396.1.31",
          "form": "이런분위기 좋아.....",
          "original_form": "이런분위기 좋아.....",
          "speaker_id": "1",
          "time": "20210908 02:20"
