      "utterance": [
        {
          "id": "MDRW2100020620.1.1",
          "form": "본진 그룹이 어디세요 그러면?",
          "original_form": "본진 그룹이 어디세요 그러면?",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020620.1.2",
          "form": "지금은 마마무이긴한데\r\n제가 좀 입덕을 많이 해가지구..",
          "original_form": "지금은 마마무이긴한데\r\n제가 좀 입덕을 많이 해가지구..",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020620.1.3",
          "form": "지금 한두달 사이로 계속 입덕해요ㅋㅋ",
          "original_form": "지금 한두달 사이로 계속 입덕해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020620.1.4",
          "form": "ㅋㅋㅋㅋ 귀여우셔라",
          "original_form": "ㅋㅋㅋㅋ 귀여우셔라",
          "speaker_id": "1",
          "time": "20210828 10:30"
        },
        {
          "id": "MDRW2100020620.1.5",
          "form": "저도 그래요 근데",
          "original_form": "저도 그래요 근데",
          "speaker_id": "1",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020620.1.6",
          "form": "온니원! 은 아닌 것 같아요",
          "original_form": "온니원! 은 아닌 것 같아요",
          "speaker_id": "1",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020620.1.7",
          "form": "제가 이렇게 쉬운사람인줄 몰랐어요ㅋㅋㅋ",
          "original_form": "제가 이렇게 쉬운사람인줄 몰랐어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020620.1.8",
          "form": "좋은 곡들이 너무 많아서 ㅎㅎ",
          "original_form": "좋은 곡들이 너무 많아서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020620.1.9",
          "form": "유튜브에서 영상 많이 찾아보시나요?",
          "original_form": "유튜브에서 영상 많이 찾아보시나요?",
          "speaker_id": "2",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020620.1.10",
          "form": "네네 직캠 좋아해요",
          "original_form": "네네 직캠 좋아해요",
          "speaker_id": "1",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020620.1.11",
          "form": "맞아요 좋은 노래가 왜이리많은지",
          "original_form": "맞아요 좋은 노래가 왜이리많은지",
          "speaker_id": "2",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020620.1.12",
          "form": "스키즈 직캠도 자주 봐요",
          "original_form": "스키즈 직캠도 자주 봐요",
          "speaker_id": "1",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020620.1.13",
          "form": "오!직캠좋죠!",
          "original_form": "오!직캠좋죠!",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020620.1.14",
          "form": "어제는 원위 직캠도 봤고",
          "original_form": "어제는 원위 직캠도 봤고",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020620.1.15",
          "form": "주변에서 이 그룹 좋다하면",
          "original_form": "주변에서 이 그룹 좋다하면",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020620.1.16",
          "form": "한번씩은 다 봐요",
          "original_form": "한번씩은 다 봐요",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020620.1.17",
          "form": "스키즈면 스트레이키즈 말하시는거죠?",
          "original_form": "스키즈면 스트레이키즈 말하시는거죠?",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020620.1.18",
          "form": "오 진짜 여러그룹 많이 보시네요",
          "original_form": "오 진짜 여러그룹 많이 보시네요",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020620.1.19",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020620.1.20",
          "form": "제가 한국어 가르치는 일을 해서",
          "original_form": "제가 한국어 가르치는 일을 해서",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020620.1.21",
          "form": "저 스키즈도 입덕했었는데ㅋㅋㅋ\r\n아이엔이 참 좋더라구요",
          "original_form": "저 스키즈도 입덕했었는데ㅋㅋㅋ\r\n아이엔이 참 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020620.1.22",
          "form": "학생들이 어린 외국학생들이라",
          "original_form": "학생들이 어린 외국학생들이라",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020620.1.23",
          "form": "케이팝으로 흥미유도를 위해 ㅋㅋㅋ",
          "original_form": "케이팝으로 흥미유도를 위해 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020620.1.24",
          "form": "좋다는 그룹 곡은 다 들어봐요",
          "original_form": "좋다는 그룹 곡은 다 들어봐요",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020620.1.25",
          "form": "아하! 그래서 많이 접하시는군요",
          "original_form": "아하! 그래서 많이 접하시는군요",
          "speaker_id": "2",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020620.1.26",
          "form": "공감대 형성...",
          "original_form": "공감대 형성...",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020620.1.27",
          "form": "그러다 저도 스며들고...",
          "original_form": "그러다 저도 스며들고...",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020620.1.28",
          "form": "마자요ㅋㅋㅋㅋㄱ",
          "original_form": "마자요ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020620.1.29",
          "form": "스키즈도 입덕 ㅋㅋㅋㅋ",
          "original_form": "스키즈도 입덕 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:35"
        }
      ]
    }
