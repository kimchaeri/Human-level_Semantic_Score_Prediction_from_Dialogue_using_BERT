      "utterance": [
        {
          "id": "MDRW2100030994.1.1",
          "form": "아니요? 그거는 어디서보셨어요?!",
          "original_form": "아니요? 그거는 어디서보셨어요?!",
          "speaker_id": "2",
          "time": "20210904 15:15"
        },
        {
          "id": "MDRW2100030994.1.2",
          "form": "유툽에 치면나올거에요!!!",
          "original_form": "유툽에 치면나올거에요!!!",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030994.1.3",
          "form": "흑백인데",
          "original_form": "흑백인데",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030994.1.4",
          "form": "진짜 잘만들었어요",
          "original_form": "진짜 잘만들었어요",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030994.1.5",
          "form": "!!",
          "original_form": "!!",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030994.1.6",
          "form": "헐 오늘 저녁에 봐볼게요! 대박 두근두근",
          "original_form": "헐 오늘 저녁에 봐볼게요! 대박 두근두근",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100030994.1.7",
          "form": "제가 본 디즈니 단편중에서 제일 좋아해요",
          "original_form": "제가 본 디즈니 단편중에서 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100030994.1.8",
          "form": "ㅋㅋㅋㅋ음악이랑 딱맞아떨어지는 영상도 귀엽고",
          "original_form": "ㅋㅋㅋㅋ음악이랑 딱맞아떨어지는 영상도 귀엽고",
          "speaker_id": "1",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100030994.1.9",
          "form": "페이퍼 맨 기억해두겠습니다 ㅋㅋㅋㅋ",
          "original_form": "페이퍼 맨 기억해두겠습니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100030994.1.10",
          "form": "근데정말 그 겨울왕국때 나무나 파도같은것도 거의 실사같구 요즘 애니메이션 구현 실력이 너무 발전한것 같아요",
          "original_form": "근데정말 그 겨울왕국때 나무나 파도같은것도 거의 실사같구 요즘 애니메이션 구현 실력이 너무 발전한것 같아요",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100030994.1.11",
          "form": "이젠 게임그래픽도 거의 실사같구",
          "original_form": "이젠 게임그래픽도 거의 실사같구",
          "speaker_id": "1",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100030994.1.12",
          "form": "실제로 게임으로 쓰이는 그래픽으로 영화에 쓰이기도한대요!",
          "original_form": "실제로 게임으로 쓰이는 그래픽으로 영화에 쓰이기도한대요!",
          "speaker_id": "1",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100030994.1.13",
          "form": "우와 그렇구나 신기해요!!!",
          "original_form": "우와 그렇구나 신기해요!!!",
          "speaker_id": "2",
          "time": "20210904 19:21"
        },
        {
          "id": "MDRW2100030994.1.14",
          "form": "아 계속 영화 얘기하니까 영화관 가서 재밌는 영화 보고싶어지네요 ㅠㅠ",
          "original_form": "아 계속 영화 얘기하니까 영화관 가서 재밌는 영화 보고싶어지네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 19:21"
        },
        {
          "id": "MDRW2100030994.1.15",
          "form": "그니깐여",
          "original_form": "그니깐여",
          "speaker_id": "1",
          "time": "20210905 00:34"
        },
        {
          "id": "MDRW2100030994.1.16",
          "form": "ㅠㅠㅠㅠ코로나 얼른 종식돼버려라!!!",
          "original_form": "ㅠㅠㅠㅠ코로나 얼른 종식돼버려라!!!",
          "speaker_id": "1",
          "time": "20210905 00:34"
        },
        {
          "id": "MDRW2100030994.1.17",
          "form": "아맞아 넷플릭스 작품들 극장에 올라온다던데",
          "original_form": "아맞아 넷플릭스 작품들 극장에 올라온다던데",
          "speaker_id": "1",
          "time": "20210905 00:35"
        },
        {
          "id": "MDRW2100030994.1.18",
          "form": "한번 봐보세용",
          "original_form": "한번 봐보세용",
          "speaker_id": "1",
          "time": "20210905 00:35"
        },
        {
          "id": "MDRW2100030994.1.19",
          "form": "헐 대박!!!! 넷플릭스 오리지널 작품들이요?!?!?",
          "original_form": "헐 대박!!!! 넷플릭스 오리지널 작품들이요?!?!?",
          "speaker_id": "2",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100030994.1.20",
          "form": "네 그렇다구 하더라구여",
          "original_form": "네 그렇다구 하더라구여",
          "speaker_id": "1",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100030994.1.21",
          "form": "언젠지는 자세히모르지만",
          "original_form": "언젠지는 자세히모르지만",
          "speaker_id": "1",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100030994.1.22",
          "form": "크그브에서 하는걸루알구있어요",
          "original_form": "크그브에서 하는걸루알구있어요",
          "speaker_id": "1",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100030994.1.23",
          "form": "찾아보겠습니다 ㅋㅋㅋ",
          "original_form": "찾아보겠습니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:43"
        },
        {
          "id": "MDRW2100030994.1.24",
          "form": "노래듣는것도 좋아하시나요?!",
          "original_form": "노래듣는것도 좋아하시나요?!",
          "speaker_id": "2",
          "time": "20210905 09:43"
        },
        {
          "id": "MDRW2100030994.1.25",
          "form": "저 노래많이듣죠",
          "original_form": "저 노래많이듣죠",
          "speaker_id": "1",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100030994.1.26",
          "form": "약간 집에 아무소리도 안나고 있으면 뭔가 심심해서",
          "original_form": "약간 집에 아무소리도 안나고 있으면 뭔가 심심해서",
          "speaker_id": "1",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100030994.1.27",
          "form": "스피커로 잔잔한 음악이라도 많이틀어놔여",
          "original_form": "스피커로 잔잔한 음악이라도 많이틀어놔여",
          "speaker_id": "1",
          "time": "20210905 12:22"
        },
        {
          "id": "MDRW2100030994.1.28",
          "form": "맞아요ㅋㅋㅋ 저도 늘 틀어두는 편이에요 ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ 저도 늘 틀어두는 편이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:42"
        },
        {
          "id": "MDRW2100030994.1.29",
          "form": "저는 잔잔한거 틀때도 있지만 보통 그냥 멜론 탑백.. ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 잔잔한거 틀때도 있지만 보통 그냥 멜론 탑백.. ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:42"
        },
        {
          "id": "MDRW2100030994.1.30",
          "form": "아항ㅋㅋㅋㅋㅋㅋ저는 편의점이나 프랜차이즈에서 알바했었어서",
          "original_form": "아항ㅋㅋㅋㅋㅋㅋ저는 편의점이나 프랜차이즈에서 알바했었어서",
          "speaker_id": "1",
          "time": "20210905 14:23"
        },
        {
          "id": "MDRW2100030994.1.31",
          "form": "계속 같은 노래만 나오니까 너무 힘들더라구여",
          "original_form": "계속 같은 노래만 나오니까 너무 힘들더라구여",
          "speaker_id": "1",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100030994.1.32",
          "form": "아아아 그러면 진짜 탑백은 질리게 들으셨겠어요ㅋㅋㅋ",
          "original_form": "아아아 그러면 진짜 탑백은 질리게 들으셨겠어요ㅋㅋㅋ",
