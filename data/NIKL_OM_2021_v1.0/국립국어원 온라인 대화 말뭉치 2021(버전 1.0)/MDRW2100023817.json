      "utterance": [
        {
          "id": "MDRW2100023817.1.1",
          "form": "재택하세요?",
          "original_form": "재택하세요?",
          "speaker_id": "2",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100023817.1.2",
          "form": "저는 집에서 있는 걸 힘들어 하는 타입이라 재택근무를 못해요",
          "original_form": "저는 집에서 있는 걸 힘들어 하는 타입이라 재택근무를 못해요",
          "speaker_id": "1",
          "time": "20210901 06:47"
        },
        {
          "id": "MDRW2100023817.1.3",
          "form": "일을 하더라도 출퇴근 하는게 좋네요",
          "original_form": "일을 하더라도 출퇴근 하는게 좋네요",
          "speaker_id": "1",
          "time": "20210901 06:47"
        },
        {
          "id": "MDRW2100023817.1.4",
          "form": "님은 재택 좋아하나요? 가능하다면 하고 싶나요?",
          "original_form": "님은 재택 좋아하나요? 가능하다면 하고 싶나요?",
          "speaker_id": "1",
          "time": "20210901 06:47"
        },
        {
          "id": "MDRW2100023817.1.5",
          "form": "재택이 편하져",
          "original_form": "재택이 편하져",
          "speaker_id": "2",
          "time": "20210901 07:15"
        },
        {
          "id": "MDRW2100023817.1.6",
          "form": "그렇군요.. 전 일단 나가야해서요",
          "original_form": "그렇군요.. 전 일단 나가야해서요",
          "speaker_id": "1",
          "time": "20210901 07:17"
        },
        {
          "id": "MDRW2100023817.1.7",
          "form": "출근요?",
          "original_form": "출근요?",
          "speaker_id": "2",
          "time": "20210901 07:18"
        },
        {
          "id": "MDRW2100023817.1.8",
          "form": "일을 하게 됨다면 출퇴근 되는 일 하고 싶어요",
          "original_form": "일을 하게 됨다면 출퇴근 되는 일 하고 싶어요",
          "speaker_id": "1",
          "time": "20210901 07:19"
        },
        {
          "id": "MDRW2100023817.1.9",
          "form": "재택말구요",
          "original_form": "재택말구요",
          "speaker_id": "1",
          "time": "20210901 07:19"
        },
        {
          "id": "MDRW2100023817.1.10",
          "form": "집에 있으면 갑갑해요",
          "original_form": "집에 있으면 갑갑해요",
          "speaker_id": "1",
          "time": "20210901 07:19"
        },
        {
          "id": "MDRW2100023817.1.11",
          "form": "전 출근이 싫어서 집이 좋아요",
          "original_form": "전 출근이 싫어서 집이 좋아요",
          "speaker_id": "2",
          "time": "20210901 07:21"
        },
        {
          "id": "MDRW2100023817.1.12",
          "form": "저랑 반대시군요..",
          "original_form": "저랑 반대시군요..",
          "speaker_id": "1",
          "time": "20210901 07:22"
        },
        {
          "id": "MDRW2100023817.1.13",
          "form": "집돌이신가봐요",
          "original_form": "집돌이신가봐요",
          "speaker_id": "1",
          "time": "20210901 07:22"
        },
        {
          "id": "MDRW2100023817.1.14",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 07:29"
        },
        {
          "id": "MDRW2100023817.1.15",
          "form": "지금은 출근하는 일이죠?",
          "original_form": "지금은 출근하는 일이죠?",
          "speaker_id": "1",
          "time": "20210901 10:07"
        },
        {
          "id": "MDRW2100023817.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100023817.1.17",
          "form": "그럼 그 자체가 좀 힘들겠네요",
          "original_form": "그럼 그 자체가 좀 힘들겠네요",
