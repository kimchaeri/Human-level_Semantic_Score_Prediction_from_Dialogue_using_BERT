      "utterance": [
        {
          "id": "MDRW2100018520.1.1",
          "form": "혹시 넷플릭스 보세요?",
          "original_form": "혹시 넷플릭스 보세요?",
          "speaker_id": "1",
          "time": "20210827 07:37"
        },
        {
          "id": "MDRW2100018520.1.2",
          "form": "그거로 보다 최근엔 웨이브로 갈아탔어요ㅋㅋ",
          "original_form": "그거로 보다 최근엔 웨이브로 갈아탔어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:37"
        },
        {
          "id": "MDRW2100018520.1.3",
          "form": "넷플릭스 보시나요?",
          "original_form": "넷플릭스 보시나요?",
          "speaker_id": "2",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100018520.1.4",
          "form": "오홍 네네! 저는 넷플릭스 왓챠 번갈아가면서 봐용ㅎㅎ",
          "original_form": "오홍 네네! 저는 넷플릭스 왓챠 번갈아가면서 봐용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100018520.1.5",
          "form": "오오ㅎㅎ 저는 티빙이요!",
          "original_form": "오오ㅎㅎ 저는 티빙이요!",
          "speaker_id": "2",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100018520.1.6",
          "form": "하나는 모자라더라구요ㅎㅎ",
          "original_form": "하나는 모자라더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100018520.1.7",
          "form": "티빙 여고추리반 재밌게 봤어용ㅎㅎ",
          "original_form": "티빙 여고추리반 재밌게 봤어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100018520.1.8",
          "form": "하나는 부족하죠ㅠㅠ",
          "original_form": "하나는 부족하죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100018520.1.9",
          "form": "그거 여자대탈출이라 해서 보려고 했어요!",
          "original_form": "그거 여자대탈출이라 해서 보려고 했어요!",
          "speaker_id": "2",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100018520.1.10",
          "form": "맞아요ㅠ 꼭 조금씩 없더라구요",
          "original_form": "맞아요ㅠ 꼭 조금씩 없더라구요",
          "speaker_id": "2",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100018520.1.11",
          "form": "ㅎㅎ 인기가 많아서 그런지 시즌투도 나온다고 하더라구용",
          "original_form": "ㅎㅎ 인기가 많아서 그런지 시즌투도 나온다고 하더라구용",
          "speaker_id": "1",
          "time": "20210827 07:41"
        },
        {
          "id": "MDRW2100018520.1.12",
          "form": "하나로 통합됐으면 좋겠야용",
          "original_form": "하나로 통합됐으면 좋겠야용",
          "speaker_id": "1",
          "time": "20210827 07:41"
        },
        {
          "id": "MDRW2100018520.1.13",
          "form": "오? 시즌이라니 얼른 봐야겠어요",
          "original_form": "오? 시즌이라니 얼른 봐야겠어요",
          "speaker_id": "2",
          "time": "20210827 07:42"
        },
        {
          "id": "MDRW2100018520.1.14",
          "form": "맞아요ㅋㅋ 돈도 절약되고",
          "original_form": "맞아요ㅋㅋ 돈도 절약되고",
          "speaker_id": "2",
          "time": "20210827 07:42"
        },
        {
          "id": "MDRW2100018520.1.15",
          "form": "요즘은 집에서도 볼게 넘쳐서 좋은것같아요ㅋㅋㅋ",
          "original_form": "요즘은 집에서도 볼게 넘쳐서 좋은것같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100018520.1.16",
          "form": "맞아요ㅋㅋ 이렇게 집순이가 될 수밖에 없는거 같아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ 이렇게 집순이가 될 수밖에 없는거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:44"
        },
        {
          "id": "MDRW2100018520.1.17",
          "form": "오징어게임 곧 오픈하는거 아세요?",
          "original_form": "오징어게임 곧 오픈하는거 아세요?",
          "speaker_id": "1",
          "time": "20210827 07:44"
        },
        {
          "id": "MDRW2100018520.1.18",
          "form": "넷플릭스",
          "original_form": "넷플릭스",
          "speaker_id": "1",
          "time": "20210827 07:44"
        },
        {
          "id": "MDRW2100018520.1.19",
          "form": "엇 대박 곧이였나요?",
          "original_form": "엇 대박 곧이였나요?",
          "speaker_id": "2",
          "time": "20210827 07:46"
        },
        {
          "id": "MDRW2100018520.1.20",
          "form": "네네! 곧 개봉예정이라",
          "original_form": "네네! 곧 개봉예정이라",
          "speaker_id": "1",
          "time": "20210827 07:47"
        },
        {
          "id": "MDRW2100018520.1.21",
          "form": "등급심의까지 마쳤다고 하더라구요",
          "original_form": "등급심의까지 마쳤다고 하더라구요",
          "speaker_id": "1",
          "time": "20210827 07:47"
        }
      ]
    }
