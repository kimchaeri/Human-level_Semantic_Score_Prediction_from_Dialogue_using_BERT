      "utterance": [
        {
          "id": "MDRW2100007541.1.1",
          "form": "당근 해봤어?",
          "original_form": "당근 해봤어?",
          "speaker_id": "1",
          "time": "20210822 03:04"
        },
        {
          "id": "MDRW2100007541.1.2",
          "form": "응 아직 구매한 적은 없는데 가끔 들어가서 오늘은 뭐 올라왔는지 구경해",
          "original_form": "응 아직 구매한 적은 없는데 가끔 들어가서 오늘은 뭐 올라왔는지 구경해",
          "speaker_id": "2",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007541.1.3",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007541.1.4",
          "form": "파는 것도 재밌오",
          "original_form": "파는 것도 재밌오",
          "speaker_id": "1",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007541.1.5",
          "form": "ㅋㅎㅋ",
          "original_form": "ㅋㅎㅋ",
          "speaker_id": "1",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007541.1.6",
          "form": "그래? 그럼 나중에 팔아봐야겠다",
          "original_form": "그래? 그럼 나중에 팔아봐야겠다",
          "speaker_id": "2",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007541.1.7",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007541.1.8",
          "form": "자전거 팔아줘 나 자전거 원해",
          "original_form": "자전거 팔아줘 나 자전거 원해",
          "speaker_id": "1",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007541.1.9",
          "form": "두발자전거?",
          "original_form": "두발자전거?",
          "speaker_id": "2",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007541.1.10",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210822 03:21"
        },
        {
          "id": "MDRW2100007541.1.11",
          "form": "두발 자전거 샀는데",
          "original_form": "두발 자전거 샀는데",
          "speaker_id": "1",
          "time": "20210822 03:21"
        },
        {
          "id": "MDRW2100007541.1.12",
          "form": "다른 자전거도 타고싶어",
          "original_form": "다른 자전거도 타고싶어",
          "speaker_id": "1",
          "time": "20210822 03:21"
        },
        {
          "id": "MDRW2100007541.1.13",
          "form": "외발자전거도 타봐 아마 마켓에 팔 수도 있어",
          "original_form": "외발자전거도 타봐 아마 마켓에 팔 수도 있어",
          "speaker_id": "2",
          "time": "20210822 03:21"
        },
        {
          "id": "MDRW2100007541.1.14",
          "form": ".......",
          "original_form": ".......",
          "speaker_id": "1",
          "time": "20210822 03:22"
        },
        {
          "id": "MDRW2100007541.1.15",
          "form": "20대되면 상처 잘 안 나아",
          "original_form": "20대되면 상처 잘 안 나아",
          "speaker_id": "1",
          "time": "20210822 03:22"
        },
        {
          "id": "MDRW2100007541.1.16",
          "form": "아... 그럼 전동 자전거 같은 건 어때",
          "original_form": "아... 그럼 전동 자전거 같은 건 어때",
          "speaker_id": "2",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100007541.1.17",
          "form": "ㅋㅋㅋㅋ그래 좋다",
          "original_form": "ㅋㅋㅋㅋ그래 좋다",
          "speaker_id": "1",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100007541.1.18",
          "form": "돈 좀 쓰지모",
          "original_form": "돈 좀 쓰지모",
          "speaker_id": "1",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100007541.1.19",
          "form": "완전 빠르던뎅",
          "original_form": "완전 빠르던뎅",
          "speaker_id": "1",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100007541.1.20",
          "form": "맞아 나 그거 완전 타고 싶었어 예전에 얻어 타본 적 있는데 되게 빠르더라",
          "original_form": "맞아 나 그거 완전 타고 싶었어 예전에 얻어 타본 적 있는데 되게 빠르더라",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100007541.1.21",
          "form": "헐 구래??",
          "original_form": "헐 구래??",
          "speaker_id": "1",
          "time": "20210822 14:14"
        },
        {
          "id": "MDRW2100007541.1.22",
          "form": "어 맞아 면허딸 때 보니까 웬만한 전동자전거는 차로 분류할 정도더라",
          "original_form": "어 맞아 면허딸 때 보니까 웬만한 전동자전거는 차로 분류할 정도더라",
          "speaker_id": "1",
          "time": "20210822 14:14"
        }
      ]
