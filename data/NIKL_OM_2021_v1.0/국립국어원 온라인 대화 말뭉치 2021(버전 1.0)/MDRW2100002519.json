      "utterance": [
        {
          "id": "MDRW2100002519.1.1",
          "form": "요즘 배달 시켜먹으니까 돈 엄청 쓰더라",
          "original_form": "요즘 배달 시켜먹으니까 돈 엄청 쓰더라",
          "speaker_id": "1",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002519.1.2",
          "form": "넌 최근에 시켜먹은거 있어?",
          "original_form": "넌 최근에 시켜먹은거 있어?",
          "speaker_id": "1",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002519.1.3",
          "form": "나 최근에 찜닭 시켜먹었어",
          "original_form": "나 최근에 찜닭 시켜먹었어",
          "speaker_id": "2",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002519.1.4",
          "form": "너는 뭐 시켜먹었는데?",
          "original_form": "너는 뭐 시켜먹었는데?",
          "speaker_id": "2",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002519.1.5",
          "form": "난 회 시켜먹었지",
          "original_form": "난 회 시켜먹었지",
          "speaker_id": "1",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002519.1.6",
          "form": "연어 광어 정도?",
          "original_form": "연어 광어 정도?",
          "speaker_id": "1",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002519.1.7",
          "form": "막 초밥 같은것도 먹고 싶더라",
          "original_form": "막 초밥 같은것도 먹고 싶더라",
          "speaker_id": "1",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002519.1.8",
          "form": "혹시 나중에 시켜먹을거면 뭐 시켜먹고 싶어?",
          "original_form": "혹시 나중에 시켜먹을거면 뭐 시켜먹고 싶어?",
          "speaker_id": "1",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002519.1.9",
          "form": "어려운데",
          "original_form": "어려운데",
          "speaker_id": "2",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002519.1.10",
          "form": "나 이런거 선택 잘 못해서",
          "original_form": "나 이런거 선택 잘 못해서",
          "speaker_id": "2",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002519.1.11",
          "form": "그럼 시켜먹는거 말고 음료수 같은거 뭐 좋아해?",
          "original_form": "그럼 시켜먹는거 말고 음료수 같은거 뭐 좋아해?",
          "speaker_id": "1",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002519.1.12",
          "form": "탄산음료 많이 마셔",
          "original_form": "탄산음료 많이 마셔",
          "speaker_id": "2",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100002519.1.13",
          "form": "너는 좋아하는 음료수 있어?",
          "original_form": "너는 좋아하는 음료수 있어?",
          "speaker_id": "2",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100002519.1.14",
          "form": "난 좋아한다기 보단 있으면 마시는편?",
          "original_form": "난 좋아한다기 보단 있으면 마시는편?",
          "speaker_id": "1",
          "time": "20210820 13:15"
        },
        {
          "id": "MDRW2100002519.1.15",
          "form": "그나마 자주 마시는건 콜라정도야",
          "original_form": "그나마 자주 마시는건 콜라정도야",
          "speaker_id": "1",
          "time": "20210820 13:15"
        },
        {
          "id": "MDRW2100002519.1.16",
          "form": "나도 콜라 많이 마셔",
          "original_form": "나도 콜라 많이 마셔",
          "speaker_id": "2",
          "time": "20210820 13:17"
        },
        {
          "id": "MDRW2100002519.1.17",
          "form": "배달음식 시키면 음료는 콜라로 와서",
          "original_form": "배달음식 시키면 음료는 콜라로 와서",
          "speaker_id": "2",
          "time": "20210820 13:17"
        },
        {
          "id": "MDRW2100002519.1.18",
          "form": "어떤 콜라 오는데?",
          "original_form": "어떤 콜라 오는데?",
          "speaker_id": "1",
          "time": "20210820 13:19"
        },
        {
          "id": "MDRW2100002519.1.19",
          "form": "코카? 펩시?",
          "original_form": "코카? 펩시?",
          "speaker_id": "1",
          "time": "20210820 13:19"
        },
        {
          "id": "MDRW2100002519.1.20",
          "form": "펩시가 많이 오더라",
          "original_form": "펩시가 많이 오더라",
          "speaker_id": "2",
          "time": "20210820 13:20"
        },
        {
          "id": "MDRW2100002519.1.21",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210820 13:20"
        },
        {
          "id": "MDRW2100002519.1.22",
          "form": "코카가 탄산이 더 많고 좋던데",
          "original_form": "코카가 탄산이 더 많고 좋던데",
          "speaker_id": "1",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002519.1.23",
          "form": "넌 어때?",
          "original_form": "넌 어때?",
          "speaker_id": "1",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002519.1.24",
          "form": "나도 탄산 많은게 좋아서 코카가 좋아",
          "original_form": "나도 탄산 많은게 좋아서 코카가 좋아",
          "speaker_id": "2",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002519.1.25",
          "form": "오오 뭘좀 아네",
          "original_form": "오오 뭘좀 아네",
          "speaker_id": "1",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002519.1.26",
          "form": "난 너무 단것도 안좋아하기도 해서",
          "original_form": "난 너무 단것도 안좋아하기도 해서",
          "speaker_id": "1",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002519.1.27",
          "form": "펩시가 좀 더 달게 느껴지더라고",
          "original_form": "펩시가 좀 더 달게 느껴지더라고",
          "speaker_id": "1",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002519.1.28",
          "form": "그래서 코카콜라를 더 많이 먹는것 같아",
          "original_form": "그래서 코카콜라를 더 많이 먹는것 같아",
          "speaker_id": "1",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002519.1.29",
          "form": "펩시가 더 달구나",
          "original_form": "펩시가 더 달구나",
          "speaker_id": "2",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002519.1.30",
          "form": "그건 몰랐네",
          "original_form": "그건 몰랐네",
          "speaker_id": "2",
          "time": "20210820 13:23"
        }
      ]
    }
