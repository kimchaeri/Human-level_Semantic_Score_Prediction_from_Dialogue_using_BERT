      "utterance": [
        {
          "id": "MDRW2100031949.1.1",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210905 09:40"
        },
        {
          "id": "MDRW2100031949.1.2",
          "form": "매일마셔요",
          "original_form": "매일마셔요",
          "speaker_id": "2",
          "time": "20210905 09:40"
        },
        {
          "id": "MDRW2100031949.1.3",
          "form": "매일 마셔도 괜찮나봅니다",
          "original_form": "매일 마셔도 괜찮나봅니다",
          "speaker_id": "1",
          "time": "20210905 09:44"
        },
        {
          "id": "MDRW2100031949.1.4",
          "form": "전 좋아하지만 잠을 못자서 어쩌다 마셔요",
          "original_form": "전 좋아하지만 잠을 못자서 어쩌다 마셔요",
          "speaker_id": "1",
          "time": "20210905 09:44"
        },
        {
          "id": "MDRW2100031949.1.5",
          "form": "매일마셔도",
          "original_form": "매일마셔도",
          "speaker_id": "2",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100031949.1.6",
          "form": "집에오면 졸려요 ㅋㅋㅋㅋ",
          "original_form": "집에오면 졸려요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100031949.1.7",
          "form": "그래요? 마셔도 잘 잘 수 있으면 저도 매일 마실텐데요",
          "original_form": "그래요? 마셔도 잘 잘 수 있으면 저도 매일 마실텐데요",
          "speaker_id": "1",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100031949.1.8",
          "form": "원두 내려 마시나요?",
          "original_form": "원두 내려 마시나요?",
          "speaker_id": "1",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100031949.1.9",
          "form": "회사에선 카누",
          "original_form": "회사에선 카누",
          "speaker_id": "2",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100031949.1.10",
          "form": "집에서 머신있어서",
          "original_form": "집에서 머신있어서",
          "speaker_id": "2",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100031949.1.11",
          "form": "내려먹죠",
          "original_form": "내려먹죠",
          "speaker_id": "2",
          "time": "20210905 12:22"
        },
        {
          "id": "MDRW2100031949.1.12",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100031949.1.13",
          "form": "커피말고 즐겨 마시는 음료 또 있나요?",
          "original_form": "커피말고 즐겨 마시는 음료 또 있나요?",
          "speaker_id": "1",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100031949.1.14",
          "form": "음....",
          "original_form": "음....",
          "speaker_id": "2",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100031949.1.15",
          "form": "둥굴레차?",
          "original_form": "둥굴레차?",
          "speaker_id": "2",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100031949.1.16",
          "form": "그거 좋죠",
          "original_form": "그거 좋죠",
          "speaker_id": "1",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100031949.1.17",
          "form": "곡차 좋아해서 보리차 둥글레차 현미차 결명자차 등도 잘 마셔요",
          "original_form": "곡차 좋아해서 보리차 둥글레차 현미차 결명자차 등도 잘 마셔요",
          "speaker_id": "1",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100031949.1.18",
          "form": "저도 보리차도 좋아해요",
          "original_form": "저도 보리차도 좋아해요",
          "speaker_id": "2",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100031949.1.19",
          "form": "커피가 갈증해소엔 진짜 별로라",
          "original_form": "커피가 갈증해소엔 진짜 별로라",
          "speaker_id": "2",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100031949.1.20",
          "form": "갈증해소는 안돼죠.. 전 향으로 마시는거라..ㅎ",
          "original_form": "갈증해소는 안돼죠.. 전 향으로 마시는거라..ㅎ",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100031949.1.21",
          "form": "아메리카노의 맛의차이를 느끼기 시작한지 얼마 안됐어요 ㅎㅎ",
          "original_form": "아메리카노의 맛의차이를 느끼기 시작한지 얼마 안됐어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100031949.1.22",
          "form": "아직 맛의 차이까지 몰라서요",
          "original_form": "아직 맛의 차이까지 몰라서요",
          "speaker_id": "1",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100031949.1.23",
          "form": "알면 더 좋을 거 같네요",
          "original_form": "알면 더 좋을 거 같네요",
          "speaker_id": "1",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100031949.1.24",
          "form": "계속드시다보면",
          "original_form": "계속드시다보면",
          "speaker_id": "2",
          "time": "20210905 18:03"
        },
        {
          "id": "MDRW2100031949.1.25",
          "form": "자연스럽게 ㅎㅎ",
          "original_form": "자연스럽게 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 18:03"
        },
        {
          "id": "MDRW2100031949.1.26",
          "form": "저녁은 맛있게 먹었나요?",
          "original_form": "저녁은 맛있게 먹었나요?",
          "speaker_id": "1",
          "time": "20210905 20:02"
        }
      ]
