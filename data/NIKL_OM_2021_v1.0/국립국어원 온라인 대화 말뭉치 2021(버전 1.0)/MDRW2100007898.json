      "utterance": [
        {
          "id": "MDRW2100007898.1.1",
          "form": "티비봐요?",
          "original_form": "티비봐요?",
          "speaker_id": "1",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007898.1.2",
          "form": "티비는 음악방송 빼고 잘 안봐요",
          "original_form": "티비는 음악방송 빼고 잘 안봐요",
          "speaker_id": "2",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007898.1.3",
          "form": "주로 유튜브만 봐요",
          "original_form": "주로 유튜브만 봐요",
          "speaker_id": "2",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007898.1.4",
          "form": "전 런닝맨",
          "original_form": "전 런닝맨",
          "speaker_id": "1",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007898.1.5",
          "form": "런닝맨은 이광수 빠지고",
          "original_form": "런닝맨은 이광수 빠지고",
          "speaker_id": "2",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007898.1.6",
          "form": "거의 본적이 없어요",
          "original_form": "거의 본적이 없어요",
          "speaker_id": "2",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007898.1.7",
          "form": "빠져도 잼나요",
          "original_form": "빠져도 잼나요",
          "speaker_id": "1",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007898.1.8",
          "form": "한번 봐야겠네요",
          "original_form": "한번 봐야겠네요",
          "speaker_id": "2",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007898.1.9",
          "form": "지석진 잼나요",
          "original_form": "지석진 잼나요",
          "speaker_id": "1",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007898.1.10",
          "form": "지석진님 좋죠",
          "original_form": "지석진님 좋죠",
          "speaker_id": "2",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007898.1.11",
          "form": "저번에 msg워너비 병행하실때",
          "original_form": "저번에 msg워너비 병행하실때",
          "speaker_id": "2",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007898.1.12",
          "form": "요즘 웃겨요",
          "original_form": "요즘 웃겨요",
          "speaker_id": "1",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007898.1.13",
          "form": "목 아끼신다고 말하셔서 엄청 웃었어요",
          "original_form": "목 아끼신다고 말하셔서 엄청 웃었어요",
          "speaker_id": "2",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007898.1.14",
          "form": "노래도 잘하드라요",
          "original_form": "노래도 잘하드라요",
          "speaker_id": "1",
          "time": "20210822 11:44"
        },
        {
          "id": "MDRW2100007898.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 11:45"
        },
        {
          "id": "MDRW2100007898.1.16",
          "form": "코가 노래주머니인듯요",
          "original_form": "코가 노래주머니인듯요",
