      "utterance": [
        {
          "id": "MDRW2100016266.1.1",
          "form": "저랑 코로나 백신얘기해요~",
          "original_form": "저랑 코로나 백신얘기해요~",
          "speaker_id": "1",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100016266.1.2",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210826 11:31"
        },
        {
          "id": "MDRW2100016266.1.3",
          "form": "예약만 하고 아직 안 맞았어요",
          "original_form": "예약만 하고 아직 안 맞았어요",
          "speaker_id": "1",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016266.1.4",
          "form": "생리기간이랑 화이자가 겹치면 한달간 하혈한다는 사람이 주변에 많아서",
          "original_form": "생리기간이랑 화이자가 겹치면 한달간 하혈한다는 사람이 주변에 많아서",
          "speaker_id": "1",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016266.1.5",
          "form": "떨리시겠네요",
          "original_form": "떨리시겠네요",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016266.1.6",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016266.1.7",
          "form": "생리안겹치게 예약하려고",
          "original_form": "생리안겹치게 예약하려고",
          "speaker_id": "1",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016266.1.8",
          "form": "좀 늦게 맞아요",
          "original_form": "좀 늦게 맞아요",
          "speaker_id": "1",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016266.1.9",
          "form": "1차이신가요?",
          "original_form": "1차이신가요?",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016266.1.10",
          "form": "저 잠시 다른 분과 밥먹고올게요 죄송해요 ㅜ",
          "original_form": "저 잠시 다른 분과 밥먹고올게요 죄송해요 ㅜ",
          "speaker_id": "1",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100016266.1.11",
          "form": "네 편히 나눠요 ㅎ",
          "original_form": "네 편히 나눠요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100016266.1.12",
          "form": "먹고 왔어요 ㅎㅎㅎ 그래서 제 친구들 중 임신중임 친구들은 안맞는다는 애들도 있어요.",
          "original_form": "먹고 왔어요 ㅎㅎㅎ 그래서 제 친구들 중 임신중임 친구들은 안맞는다는 애들도 있어요.",
          "speaker_id": "1",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016266.1.13",
          "form": "전 임신중은 아니지만 한달간 생리한 친구얘길 듣고 생리기간을 피해서 잡았어요.",
          "original_form": "전 임신중은 아니지만 한달간 생리한 친구얘길 듣고 생리기간을 피해서 잡았어요.",
          "speaker_id": "1",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100016266.1.14",
          "form": "부작용이 무섭더라고요",
          "original_form": "부작용이 무섭더라고요",
          "speaker_id": "2",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100016266.1.15",
          "form": "그죠 보상받기 힘드니 더 무서워요",
          "original_form": "그죠 보상받기 힘드니 더 무서워요",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016266.1.16",
          "form": "무사히 아무탈없이 지나가길바래요",
          "original_form": "무사히 아무탈없이 지나가길바래요",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016266.1.17",
          "form": "그랬음 좋겠어요 ㅠㅜ",
          "original_form": "그랬음 좋겠어요 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210826 14:41"
        }
      ]
