      "utterance": [
        {
          "id": "MDRW2100023408.1.1",
          "form": "역시 한국인의 매운맛",
          "original_form": "역시 한국인의 매운맛",
          "speaker_id": "2",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023408.1.2",
          "form": "네넼ㅋㅋㅋ",
          "original_form": "네넼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023408.1.3",
          "form": "고추장 가져가라고들 하더니",
          "original_form": "고추장 가져가라고들 하더니",
          "speaker_id": "2",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023408.1.4",
          "form": "그래도 계란요리하나는 기깔나게 잘하더라구요!",
          "original_form": "그래도 계란요리하나는 기깔나게 잘하더라구요!",
          "speaker_id": "1",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100023408.1.5",
          "form": "일본 계란 요리는 입에서 녹아요",
          "original_form": "일본 계란 요리는 입에서 녹아요",
          "speaker_id": "1",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023408.1.6",
          "form": "캬 맞아요",
          "original_form": "캬 맞아요",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023408.1.7",
          "form": "보드랍게?",
          "original_form": "보드랍게?",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023408.1.8",
          "form": "하는거같던데",
          "original_form": "하는거같던데",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023408.1.9",
          "form": "반숙 완숙 스크램블 다 잘해요",
          "original_form": "반숙 완숙 스크램블 다 잘해요",
          "speaker_id": "1",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023408.1.10",
          "form": "계란요리로 일본에선 주로 뭐나와요?",
          "original_form": "계란요리로 일본에선 주로 뭐나와요?",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023408.1.11",
          "form": "앜ㅋㅋ",
          "original_form": "앜ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023408.1.12",
          "form": "어딜가나 식당가면 계란이 늘 있던것같은데",
          "original_form": "어딜가나 식당가면 계란이 늘 있던것같은데",
          "speaker_id": "1",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023408.1.13",
          "form": "반숙 완숙 스크램블도 달라요??",
          "original_form": "반숙 완숙 스크램블도 달라요??",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023408.1.14",
          "form": "저는 계란말이 많이 먹었어용",
          "original_form": "저는 계란말이 많이 먹었어용",
          "speaker_id": "1",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100023408.1.15",
          "form": "넹넹!",
          "original_form": "넹넹!",
          "speaker_id": "1",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023408.1.16",
          "form": "하긴 한국에서도 유명한 라멘집가면",
          "original_form": "하긴 한국에서도 유명한 라멘집가면",
          "speaker_id": "2",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023408.1.17",
          "form": "아지타마고?",
          "original_form": "아지타마고?",
          "speaker_id": "2",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023408.1.18",
          "form": "라면서",
          "original_form": "라면서",
          "speaker_id": "2",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023408.1.19",
          "form": "아아 마자여ㅋㅋㅋ 그런거! 있었어요",
          "original_form": "아아 마자여ㅋㅋㅋ 그런거! 있었어요",
          "speaker_id": "1",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023408.1.20",
          "form": "좀특이한 반숙계란 줬던거 같긴 해요",
          "original_form": "좀특이한 반숙계란 줬던거 같긴 해요",
          "speaker_id": "2",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023408.1.21",
          "form": "가보고싶은곳 있으세용?",
          "original_form": "가보고싶은곳 있으세용?",
          "speaker_id": "1",
          "time": "20210830 01:41"
        }
      ]
    }
