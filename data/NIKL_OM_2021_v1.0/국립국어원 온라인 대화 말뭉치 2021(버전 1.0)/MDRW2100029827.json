        {
          "id": "MDRW2100029827.1.1",
          "form": "운동해요?",
          "original_form": "운동해요?",
          "speaker_id": "1",
          "time": "20210903 14:25"
        },
        {
          "id": "MDRW2100029827.1.2",
          "form": "안타깝게도",
          "original_form": "안타깝게도",
          "speaker_id": "2",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100029827.1.3",
          "form": "계획을 의지가 못 따라가는지라...",
          "original_form": "계획을 의지가 못 따라가는지라...",
          "speaker_id": "2",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100029827.1.4",
          "form": "전 홈트해요",
          "original_form": "전 홈트해요",
          "speaker_id": "1",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100029827.1.5",
          "form": "아 확실히",
          "original_form": "아 확실히",
          "speaker_id": "2",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100029827.1.6",
          "form": "요즘에는",
          "original_form": "요즘에는",
          "speaker_id": "2",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100029827.1.7",
          "form": "홈트 많이 하더라고요",
          "original_form": "홈트 많이 하더라고요",
          "speaker_id": "2",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100029827.1.8",
          "form": "저는 자취방이 좁아서...",
          "original_form": "저는 자취방이 좁아서...",
          "speaker_id": "2",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100029827.1.9",
          "form": "는 물론 변명이지만서도",
          "original_form": "는 물론 변명이지만서도",
          "speaker_id": "2",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100029827.1.10",
          "form": "플랭크해요",
          "original_form": "플랭크해요",
          "speaker_id": "1",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100029827.1.11",
          "form": "확실히 공간이 필요없는 운동이긴 하죠",
          "original_form": "확실히 공간이 필요없는 운동이긴 하죠",
          "speaker_id": "2",
          "time": "20210903 14:48"
        },
        {
          "id": "MDRW2100029827.1.12",
          "form": "맨손체조네요",
          "original_form": "맨손체조네요",
          "speaker_id": "1",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100029827.1.13",
          "form": "홈트 하시면",
          "original_form": "홈트 하시면",
          "speaker_id": "2",
          "time": "20210903 15:59"
        },
        {
          "id": "MDRW2100029827.1.14",
          "form": "뭐 어떤거 통해서 하시나요",
          "original_form": "뭐 어떤거 통해서 하시나요",
          "speaker_id": "2",
          "time": "20210903 15:59"
        },
        {
          "id": "MDRW2100029827.1.15",
          "form": "유투브봐요",
          "original_form": "유투브봐요",
          "speaker_id": "1",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100029827.1.16",
          "form": "확실히",
          "original_form": "확실히",
          "speaker_id": "2",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100029827.1.17",
          "form": "운동 채널 요즘 잘 되어 있긴 하죠",
          "original_form": "운동 채널 요즘 잘 되어 있긴 하죠",
