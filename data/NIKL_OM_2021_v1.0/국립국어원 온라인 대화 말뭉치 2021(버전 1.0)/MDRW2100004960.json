      "utterance": [
        {
          "id": "MDRW2100004960.1.1",
          "form": "오늘 갑자기 날씨가",
          "original_form": "오늘 갑자기 날씨가",
          "speaker_id": "1",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004960.1.2",
          "form": "엄청났어",
          "original_form": "엄청났어",
          "speaker_id": "1",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004960.1.3",
          "form": "그렇게 쏟아질줄이야",
          "original_form": "그렇게 쏟아질줄이야",
          "speaker_id": "1",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004960.1.4",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004960.1.5",
          "form": "지금은 좀 잠잠하네",
          "original_form": "지금은 좀 잠잠하네",
          "speaker_id": "1",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004960.1.6",
          "form": "너 사는 동네는 어땠어?",
          "original_form": "너 사는 동네는 어땠어?",
          "speaker_id": "1",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004960.1.7",
          "form": "여기도 엄청 쏟아졌어",
          "original_form": "여기도 엄청 쏟아졌어",
          "speaker_id": "2",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004960.1.8",
          "form": "바람도 불고",
          "original_form": "바람도 불고",
          "speaker_id": "2",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004960.1.9",
          "form": "아니 비만 오면 괜찮은데",
          "original_form": "아니 비만 오면 괜찮은데",
          "speaker_id": "2",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004960.1.10",
          "form": "바람까지 부니깐 우산쓰기 너무 힘든거야",
          "original_form": "바람까지 부니깐 우산쓰기 너무 힘든거야",
          "speaker_id": "2",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004960.1.11",
          "form": "아 바람...비오는 데 바람불면 진짜 최악이야",
          "original_form": "아 바람...비오는 데 바람불면 진짜 최악이야",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004960.1.12",
          "form": "우산을 쓴 보람이 없다고ㅋㅋ",
          "original_form": "우산을 쓴 보람이 없다고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004960.1.13",
          "form": "지 멋대로 바람 타고 댄쓰를 춘다고",
          "original_form": "지 멋대로 바람 타고 댄쓰를 춘다고",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004960.1.14",
          "form": "그리고 비 그대로 다 맞겠지",
          "original_form": "그리고 비 그대로 다 맞겠지",
          "speaker_id": "1",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004960.1.15",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004960.1.16",
          "form": "그래서 오늘",
          "original_form": "그래서 오늘",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004960.1.17",
          "form": "우산안쓰고 비옷 입고 돌아다님",
          "original_form": "우산안쓰고 비옷 입고 돌아다님",
          "speaker_id": "2",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004960.1.18",
          "form": "급하게 비옷 산거야?",
          "original_form": "급하게 비옷 산거야?",
          "speaker_id": "1",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004960.1.19",
          "form": "첨엔 우산 쓸랬는데",
          "original_form": "첨엔 우산 쓸랬는데",
          "speaker_id": "2",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004960.1.20",
          "form": "우산이 뒤집힐려는 거야",
          "original_form": "우산이 뒤집힐려는 거야",
          "speaker_id": "2",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004960.1.21",
          "form": "그래서 냅두고 우비 꺼냄",
          "original_form": "그래서 냅두고 우비 꺼냄",
          "speaker_id": "2",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004960.1.22",
          "form": "비옷은 저때 장마철때 사둔거야",
          "original_form": "비옷은 저때 장마철때 사둔거야",
          "speaker_id": "2",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004960.1.23",
          "form": "내년에 꺼내 입을 줄 알았는데",
          "original_form": "내년에 꺼내 입을 줄 알았는데",
          "speaker_id": "2",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004960.1.24",
          "form": "오, 일회용이 아니라 튼튼한 걸로 산거야?",
          "original_form": "오, 일회용이 아니라 튼튼한 걸로 산거야?",
          "speaker_id": "1",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004960.1.25",
          "form": "얍",
          "original_form": "얍",
          "speaker_id": "2",
          "time": "20210821 19:47"
        },
        {
          "id": "MDRW2100004960.1.26",
          "form": "일회용도 사봤는데",
          "original_form": "일회용도 사봤는데",
          "speaker_id": "2",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004960.1.27",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004960.1.28",
          "form": "몇 번쓰고 버리게 되더라구",
          "original_form": "몇 번쓰고 버리게 되더라구",
          "speaker_id": "2",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004960.1.29",
          "form": "비가 새는 것 같기도하고",
          "original_form": "비가 새는 것 같기도하고",
          "speaker_id": "2",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004960.1.30",
          "form": "일회용인 이유가 있지",
          "original_form": "일회용인 이유가 있지",
          "speaker_id": "1",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004960.1.31",
          "form": "그래서 괜찮은걸로 구했음",
          "original_form": "그래서 괜찮은걸로 구했음",
          "speaker_id": "2",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004960.1.32",
          "form": "잘했다 잘했다",
          "original_form": "잘했다 잘했다",
          "speaker_id": "1",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004960.1.33",
          "form": "이쁜것들 있더라",
          "original_form": "이쁜것들 있더라",
          "speaker_id": "2",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004960.1.34",
          "form": "레인코트 해서",
          "original_form": "레인코트 해서",
          "speaker_id": "2",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004960.1.35",
          "form": "고급지구만, 레인코트",
          "original_form": "고급지구만, 레인코트",
          "speaker_id": "1",
          "time": "20210821 19:48"
        }
      ]
    }
