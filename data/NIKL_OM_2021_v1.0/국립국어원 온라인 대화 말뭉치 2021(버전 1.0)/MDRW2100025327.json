      "utterance": [
        {
          "id": "MDRW2100025327.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210901 09:33"
        },
        {
          "id": "MDRW2100025327.1.2",
          "form": "네 저는 당근마켓 잘 씁니다",
          "original_form": "네 저는 당근마켓 잘 씁니다",
          "speaker_id": "2",
          "time": "20210901 09:34"
        },
        {
          "id": "MDRW2100025327.1.3",
          "form": "저도 당근",
          "original_form": "저도 당근",
          "speaker_id": "1",
          "time": "20210901 09:35"
        },
        {
          "id": "MDRW2100025327.1.4",
          "form": "주로 파세요? 사세요?",
          "original_form": "주로 파세요? 사세요?",
          "speaker_id": "2",
          "time": "20210901 09:36"
        },
        {
          "id": "MDRW2100025327.1.5",
          "form": "팔기만뇨",
          "original_form": "팔기만뇨",
          "speaker_id": "1",
          "time": "20210901 09:36"
        },
        {
          "id": "MDRW2100025327.1.6",
          "form": "최근에 뭐 파셨어요?",
          "original_form": "최근에 뭐 파셨어요?",
          "speaker_id": "2",
          "time": "20210901 09:36"
        },
        {
          "id": "MDRW2100025327.1.7",
          "form": "공기청정기요",
          "original_form": "공기청정기요",
          "speaker_id": "1",
          "time": "20210901 09:37"
        },
        {
          "id": "MDRW2100025327.1.8",
          "form": "오~ 돈 많이 받으셨겠어요",
          "original_form": "오~ 돈 많이 받으셨겠어요",
          "speaker_id": "2",
          "time": "20210901 09:37"
        },
        {
          "id": "MDRW2100025327.1.9",
          "form": "차량용이라소형이에요",
          "original_form": "차량용이라소형이에요",
          "speaker_id": "1",
          "time": "20210901 09:57"
        },
        {
          "id": "MDRW2100025327.1.10",
          "form": "아 그렇군요!",
          "original_form": "아 그렇군요!",
          "speaker_id": "2",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100025327.1.11",
          "form": "저는 당근으로 최근에 책 구매했어요ㅎ",
          "original_form": "저는 당근으로 최근에 책 구매했어요ㅎ",
          "speaker_id": "2",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100025327.1.12",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210901 10:04"
        },
        {
          "id": "MDRW2100025327.1.13",
          "form": "데미안 구입했어요",
          "original_form": "데미안 구입했어요",
