      "utterance": [
        {
          "id": "MDRW2100047300.1.1",
          "form": "점심 드셨나요",
          "original_form": "점심 드셨나요",
          "speaker_id": "2",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100047300.1.2",
          "form": "뭐 먹을지 고민이에요",
          "original_form": "뭐 먹을지 고민이에요",
          "speaker_id": "1",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100047300.1.3",
          "form": "name2님은 드셨어요?",
          "original_form": "&name2&님은 드셨어요?",
          "speaker_id": "1",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100047300.1.4",
          "form": "네 집밥이요 엄마표 고구마순김치에 달걀말이^^",
          "original_form": "네 집밥이요 엄마표 고구마순김치에 달걀말이^^",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100047300.1.5",
          "form": "와 고구마순김치라니 맛있겠어요",
          "original_form": "와 고구마순김치라니 맛있겠어요",
          "speaker_id": "1",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100047300.1.6",
          "form": "좋아하는데 먹을 기회가 잘 없죠",
          "original_form": "좋아하는데 먹을 기회가 잘 없죠",
          "speaker_id": "1",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100047300.1.7",
          "form": "백반집에서도 잘 안 나오고ㅜ",
          "original_form": "백반집에서도 잘 안 나오고ㅜ",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100047300.1.8",
          "form": "맞아요 손이 많이 가는 반찬이죠 껍질을 벗겨야하니!",
          "original_form": "맞아요 손이 많이 가는 반찬이죠 껍질을 벗겨야하니!",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100047300.1.9",
          "form": "아 그래서였군요!",
          "original_form": "아 그래서였군요!",
          "speaker_id": "1",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100047300.1.10",
          "form": "여름이면 생각나는 반찬이예요!",
          "original_form": "여름이면 생각나는 반찬이예요!",
          "speaker_id": "2",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100047300.1.11",
          "form": "어쩐지 가게에서도 잘 없더라니",
          "original_form": "어쩐지 가게에서도 잘 없더라니",
          "speaker_id": "1",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100047300.1.12",
          "form": "요리 잘 하시나봐요",
          "original_form": "요리 잘 하시나봐요",
          "speaker_id": "1",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100047300.1.13",
          "form": "아..엄마표예요^^ 저는..ㄸ손..ㅋㅋ",
          "original_form": "아..엄마표예요^^ 저는..ㄸ손..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100047300.1.14",
          "form": "아ㅎㅎ 저는 고구마순 껍질 벗겨야하는지도 몰랐는걸요ㅎ",
          "original_form": "아ㅎㅎ 저는 고구마순 껍질 벗겨야하는지도 몰랐는걸요ㅎ",
          "speaker_id": "1",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100047300.1.15",
          "form": "제 인생의 요리는 김볶과 라면...",
          "original_form": "제 인생의 요리는 김볶과 라면...",
          "speaker_id": "1",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100047300.1.16",
          "form": "ㅋㅋㅋ 맛있잖아유..김볶잘하는 노하우 있으신가요",
          "original_form": "ㅋㅋㅋ 맛있잖아유..김볶잘하는 노하우 있으신가요",
          "speaker_id": "2",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100047300.1.17",
          "form": "삼겹살을 조금 구워서 그 기름으로 합니다ㅎ",
          "original_form": "삼겹살을 조금 구워서 그 기름으로 합니다ㅎ",
          "speaker_id": "1",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100047300.1.18",
          "form": "토핑도 삼겹. 치트키지요",
          "original_form": "토핑도 삼겹. 치트키지요",
          "speaker_id": "1",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100047300.1.19",
          "form": "그외에는 고추장 기름이 다예요ㅎ",
          "original_form": "그외에는 고추장 기름이 다예요ㅎ",
          "speaker_id": "1",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100047300.1.20",
          "form": "김볶 어떻게 하세요?",
          "original_form": "김볶 어떻게 하세요?",
          "speaker_id": "1",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100047300.1.21",
          "form": "오...저는 김볶을 잘 안해요 ㅠㅠ 애들이랑 같이 못먹다보니 ㅠ",
          "original_form": "오...저는 김볶을 잘 안해요 ㅠㅠ 애들이랑 같이 못먹다보니 ㅠ",
