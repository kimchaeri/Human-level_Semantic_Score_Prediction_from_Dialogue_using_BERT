      "utterance": [
        {
          "id": "MDRW2100007583.1.1",
          "form": "걍 눈팅만!",
          "original_form": "걍 눈팅만!",
          "speaker_id": "2",
          "time": "20210822 12:44"
        },
        {
          "id": "MDRW2100007583.1.2",
          "form": "별로 살만한게 업서",
          "original_form": "별로 살만한게 업서",
          "speaker_id": "2",
          "time": "20210822 12:45"
        },
        {
          "id": "MDRW2100007583.1.3",
          "form": "그렇구만",
          "original_form": "그렇구만",
          "speaker_id": "1",
          "time": "20210825 09:34"
        },
        {
          "id": "MDRW2100007583.1.4",
          "form": "난 요즘 옷을 어디서 사야될지 모르겠어",
          "original_form": "난 요즘 옷을 어디서 사야될지 모르겠어",
          "speaker_id": "1",
          "time": "20210825 09:35"
        },
        {
          "id": "MDRW2100007583.1.5",
          "form": "인쇼도 다 망하고 있는 옷 돌려막는 중인데 백화점두 비싸구 너는?",
          "original_form": "인쇼도 다 망하고 있는 옷 돌려막는 중인데 백화점두 비싸구 너는?",
          "speaker_id": "1",
          "time": "20210825 09:35"
        },
        {
          "id": "MDRW2100007583.1.6",
          "form": "나 옷 안산지 몇달됐어ㅋㅋㅋㅋ 거지처럼 입고다님",
          "original_form": "나 옷 안산지 몇달됐어ㅋㅋㅋㅋ 거지처럼 입고다님",
          "speaker_id": "2",
          "time": "20210825 09:48"
        },
        {
          "id": "MDRW2100007583.1.7",
          "form": "진짜로 옷 살데가 없다ㅠ",
          "original_form": "진짜로 옷 살데가 없다ㅠ",
          "speaker_id": "2",
          "time": "20210825 09:48"
        },
        {
          "id": "MDRW2100007583.1.8",
          "form": "ㅋㅋㅋㅋ맞지ㅋㅋㅋ옛날엔 인쇼에서라도 샀지",
          "original_form": "ㅋㅋㅋㅋ맞지ㅋㅋㅋ옛날엔 인쇼에서라도 샀지",
          "speaker_id": "1",
          "time": "20210825 09:48"
        },
        {
          "id": "MDRW2100007583.1.9",
          "form": "진짜 인쇼도 마땅히 어디가 좋은지 모르겠고 백화점은 안간지 몇년댄거같아",
          "original_form": "진짜 인쇼도 마땅히 어디가 좋은지 모르겠고 백화점은 안간지 몇년댄거같아",
          "speaker_id": "2",
          "time": "20210825 09:48"
        },
        {
          "id": "MDRW2100007583.1.10",
          "form": "슬슬 인쇼입기 애매해지는 나이가 온다...",
          "original_form": "슬슬 인쇼입기 애매해지는 나이가 온다...",
          "speaker_id": "1",
          "time": "20210825 09:48"
        },
        {
          "id": "MDRW2100007583.1.11",
          "form": "아울렛도 가도 안싸구...",
          "original_form": "아울렛도 가도 안싸구...",
          "speaker_id": "2",
          "time": "20210825 09:48"
        },
        {
          "id": "MDRW2100007583.1.12",
          "form": "그그그 신세계 파미에타운에 지금 그거하거든",
          "original_form": "그그그 신세계 파미에타운에 지금 그거하거든",
          "speaker_id": "1",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100007583.1.13",
          "form": "행사매대",
          "original_form": "행사매대",
          "speaker_id": "1",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100007583.1.14",
          "form": "ㅋㅋㅋㅋㅋ아 왜ㅠ 30대도 인쇼에서 옷 산다구....",
          "original_form": "ㅋㅋㅋㅋㅋ아 왜ㅠ 30대도 인쇼에서 옷 산다구....",
          "speaker_id": "2",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100007583.1.15",
          "form": "그거 맨날하지않아??",
          "original_form": "그거 맨날하지않아??",
          "speaker_id": "2",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100007583.1.16",
          "form": "근데 거기 세일률이 미쳤음 난 거기서 최근에 4번삼",
          "original_form": "근데 거기 세일률이 미쳤음 난 거기서 최근에 4번삼",
          "speaker_id": "1",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100007583.1.17",
          "form": "한 한두달째 하고잇는거같긴함",
          "original_form": "한 한두달째 하고잇는거같긴함",
          "speaker_id": "1",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100007583.1.18",
          "form": "반디앤루니스없어지고",
          "original_form": "반디앤루니스없어지고",
          "speaker_id": "1",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100007583.1.19",
          "form": "정가 16만원아우터 7만원에 두벌삼",
          "original_form": "정가 16만원아우터 7만원에 두벌삼",
          "speaker_id": "1",
          "time": "20210825 09:50"
        },
        {
          "id": "MDRW2100007583.1.20",
          "form": "헐 반디앤루니스 없어졌다고??헐",
          "original_form": "헐 반디앤루니스 없어졌다고??헐",
          "speaker_id": "2",
          "time": "20210825 09:52"
        },
        {
          "id": "MDRW2100007583.1.21",
          "form": "우와 진짜 싸게 잘샀네????",
          "original_form": "우와 진짜 싸게 잘샀네????",
          "speaker_id": "2",
          "time": "20210825 09:52"
        },
        {
          "id": "MDRW2100007583.1.22",
          "form": "ㅇㅇ그래서 거기 매대가 들어왔는데 주로 나이키나 아디다스 무신사브랜드위주고",
          "original_form": "ㅇㅇ그래서 거기 매대가 들어왔는데 주로 나이키나 아디다스 무신사브랜드위주고",
          "speaker_id": "1",
          "time": "20210825 09:53"
        },
        {
          "id": "MDRW2100007583.1.23",
          "form": "기본 티셔츠 1만~2만이라서 인쇼대신 거기서삼 아빠것도 거기서사고 엄마도 거기서 원피스삼",
          "original_form": "기본 티셔츠 1만~2만이라서 인쇼대신 거기서삼 아빠것도 거기서사고 엄마도 거기서 원피스삼",
          "speaker_id": "1",
          "time": "20210825 09:54"
        },
        {
          "id": "MDRW2100007583.1.24",
          "form": "헐 티셔츠가 일이만원밖에 안하나?? 왜캐싸노 진짜",
          "original_form": "헐 티셔츠가 일이만원밖에 안하나?? 왜캐싸노 진짜",
          "speaker_id": "2",
          "time": "20210825 09:54"
        },
        {
          "id": "MDRW2100007583.1.25",
          "form": "나도 함 가봐야겟네!!ㄱㅅㄱㅅ",
          "original_form": "나도 함 가봐야겟네!!ㄱㅅㄱㅅ",
          "speaker_id": "2",
          "time": "20210825 09:54"
        },
        {
          "id": "MDRW2100007583.1.26",
          "form": "나 옷 그지임 진짜ㅠ 입을 옷이 1도 없어",
          "original_form": "나 옷 그지임 진짜ㅠ 입을 옷이 1도 없어",
          "speaker_id": "2",
          "time": "20210825 09:55"
        },
        {
          "id": "MDRW2100007583.1.27",
          "form": "진짜 가성비쇼핑 굿임 내가갓을땐 나이키 기본티셔츠도 2만5천원 정도엿음",
          "original_form": "진짜 가성비쇼핑 굿임 내가갓을땐 나이키 기본티셔츠도 2만5천원 정도엿음",
          "speaker_id": "1",
          "time": "20210825 09:55"
        },
        {
          "id": "MDRW2100007583.1.28",
          "form": "나도 근데 슬슬 계절바껴서",
          "original_form": "나도 근데 슬슬 계절바껴서",
          "speaker_id": "1",
          "time": "20210825 09:55"
        },
        {
          "id": "MDRW2100007583.1.29",
          "form": "가을이 진짜 헬임",
          "original_form": "가을이 진짜 헬임",
