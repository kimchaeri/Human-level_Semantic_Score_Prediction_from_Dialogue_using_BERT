        },
        {
          "id": "MDRW2100006602.1.2",
          "form": "민트초코!!",
          "original_form": "민트초코!!",
          "speaker_id": "1",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100006602.1.3",
          "form": "요즘에 갑자기 땡기더라?",
          "original_form": "요즘에 갑자기 땡기더라?",
          "speaker_id": "1",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100006602.1.4",
          "form": "머야",
          "original_form": "머야",
          "speaker_id": "2",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100006602.1.5",
          "form": "진짜루?",
          "original_form": "진짜루?",
          "speaker_id": "2",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100006602.1.6",
          "form": "환영입니다",
          "original_form": "환영입니다",
          "speaker_id": "2",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100006602.1.7",
          "form": "민초의 세계로",
          "original_form": "민초의 세계로",
          "speaker_id": "2",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100006602.1.8",
          "form": "응응 갑자기 그렇더라",
          "original_form": "응응 갑자기 그렇더라",
          "speaker_id": "1",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100006602.1.9",
          "form": "어느순간 먹었는데 뭐야?괜찮은데? 싶어졌어",
          "original_form": "어느순간 먹었는데 뭐야?괜찮은데? 싶어졌어",
          "speaker_id": "1",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100006602.1.10",
          "form": "크 민초는 승리한다",
          "original_form": "크 민초는 승리한다",
          "speaker_id": "2",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100006602.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ대박 좋이",
          "original_form": "ㅋㅋㅋㅋㅋㅋ대박 좋이",
          "speaker_id": "2",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100006602.1.12",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "2",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100006602.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ응응",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ응응",
          "speaker_id": "1",
          "time": "20210908 03:10"
        },
        {
          "id": "MDRW2100006602.1.14",
          "form": "그 터미널 앞쪽에 초코파이? 그거 있잖아",
          "original_form": "그 터미널 앞쪽에 초코파이? 그거 있잖아",
          "speaker_id": "1",
          "time": "20210908 03:10"
        },
        {
          "id": "MDRW2100006602.1.15",
          "form": "터미널쪽 맞나? 그거 보다가 갑자기 생각났어",
          "original_form": "터미널쪽 맞나? 그거 보다가 갑자기 생각났어",
          "speaker_id": "1",
          "time": "20210908 03:10"
        },
        {
          "id": "MDRW2100006602.1.16",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "2",
          "time": "20210908 03:10"
        },
        {
          "id": "MDRW2100006602.1.17",
          "form": "너 터미널이야.?",
          "original_form": "너 터미널이야.?",
          "speaker_id": "2",
          "time": "20210908 03:10"
        },
        {
          "id": "MDRW2100006602.1.18",
          "form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100006602.1.19",
          "form": "청주 올 때 그냥 생각났어",
          "original_form": "청주 올 때 그냥 생각났어",
          "speaker_id": "1",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100006602.1.20",
          "form": "아이 내가또",
          "original_form": "아이 내가또",
          "speaker_id": "2",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100006602.1.21",
          "form": "민초 전도해버렸네",
          "original_form": "민초 전도해버렸네",
          "speaker_id": "2",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100006602.1.22",
          "form": "아이쿠",
          "original_form": "아이쿠",
          "speaker_id": "2",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100006602.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100006602.1.24",
          "form": "지금 뿌듯한거야??",
          "original_form": "지금 뿌듯한거야??",
          "speaker_id": "1",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100006602.1.25",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006602.1.26",
          "form": "너도 민초파 입단하면 이제",
          "original_form": "너도 민초파 입단하면 이제",
          "speaker_id": "2",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006602.1.27",
          "form": "이렇게 된닷",
          "original_form": "이렇게 된닷",
          "speaker_id": "2",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006602.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ좋아좋아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ좋아좋아",
          "speaker_id": "1",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006602.1.29",
          "form": "만나도 할게 많겠네 민초도 먹고",
          "original_form": "만나도 할게 많겠네 민초도 먹고",
          "speaker_id": "1",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100006602.1.30",
          "form": "뭐 이것저것 ㅋㅋㅋ",
          "original_form": "뭐 이것저것 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100006602.1.31",
          "form": "ㅋㅋㅋㅋㅋ아 너 거짓말이면",
          "original_form": "ㅋㅋㅋㅋㅋ아 너 거짓말이면",
          "speaker_id": "2",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006602.1.32",
          "form": "다신안봐ㅡㅡ 민초가지고",
          "original_form": "다신안봐ㅡㅡ 민초가지고",
          "speaker_id": "2",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006602.1.33",
          "form": "진짜인데...",
          "original_form": "진짜인데...",
          "speaker_id": "1",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006602.1.34",
          "form": "농락하는거아니다",
          "original_form": "농락하는거아니다",
          "speaker_id": "2",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006602.1.35",
          "form": "사람 왜 거짓말하는거로 만들고 그래!!",
          "original_form": "사람 왜 거짓말하는거로 만들고 그래!!",
          "speaker_id": "1",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006602.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006602.1.37",
          "form": "지금 쎄했어",
          "original_form": "지금 쎄했어",
          "speaker_id": "2",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006602.1.38",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "1",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100006602.1.39",
          "form": "그냥 먼가 혹시 했어",
          "original_form": "그냥 먼가 혹시 했어",
