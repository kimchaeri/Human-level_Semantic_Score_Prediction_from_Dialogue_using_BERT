      "utterance": [
        {
          "id": "MDRW2100019860.1.1",
          "form": "아아 조아요!ㅋㅋㅋㅋ",
          "original_form": "아아 조아요!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019860.1.2",
          "form": "요새 음악 뭐들으세여??",
          "original_form": "요새 음악 뭐들으세여??",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019860.1.3",
          "form": "음 m4-내사랑 즐겨들어요!",
          "original_form": "음 m4-내사랑 즐겨들어요!",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019860.1.4",
          "form": "오호라 첨들어보는데 검색해봐야겠어요!!",
          "original_form": "오호라 첨들어보는데 검색해봐야겠어요!!",
          "speaker_id": "1",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019860.1.5",
          "form": "오옹 가사가 매우",
          "original_form": "오옹 가사가 매우",
          "speaker_id": "2",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019860.1.6",
          "form": "달달하달까??",
          "original_form": "달달하달까??",
          "speaker_id": "2",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019860.1.7",
          "form": "ㅋㅋㅋㅋㅋ 저지금 듣고있는데 감미롭네요~ 감미료뿌린줄",
          "original_form": "ㅋㅋㅋㅋㅋ 저지금 듣고있는데 감미롭네요~ 감미료뿌린줄",
          "speaker_id": "1",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019860.1.8",
          "form": "ㄴㅋㅋㅋ옛날노래지만",
          "original_form": "ㄴㅋㅋㅋ옛날노래지만",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019860.1.9",
          "form": "가사하나는,,,",
          "original_form": "가사하나는,,,",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019860.1.10",
          "form": "고거랑",
          "original_form": "고거랑",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019860.1.11",
          "form": "데이브레이크 silly 자주들어요!",
          "original_form": "데이브레이크 silly 자주들어요!",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019860.1.12",
          "form": "어머 ㅋㅋㅋ 에러났나봐요 ㅋㅋㅋ 두개씩보내지네요 ㅋㅋ",
          "original_form": "어머 ㅋㅋㅋ 에러났나봐요 ㅋㅋㅋ 두개씩보내지네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019860.1.13",
          "form": "그것도 들어보러가볼게여 ㅋㅋㅋ",
          "original_form": "그것도 들어보러가볼게여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019860.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ다녀와요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ다녀와요",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019860.1.15",
          "form": "",
          "original_form": "",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019860.1.16",
          "form": "이거 내사랑이랑 분위기 완전비슷~ 이런노래좋아하나봐여!!",
          "original_form": "이거 내사랑이랑 분위기 완전비슷~ 이런노래좋아하나봐여!!",
          "speaker_id": "1",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019860.1.17",
          "form": "맞아요!!!!",
          "original_form": "맞아요!!!!",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019860.1.18",
          "form": "달달한거좋아해뇨!",
          "original_form": "달달한거좋아해뇨!",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019860.1.19",
          "form": "앜ㅋㅋㅋㅋㅋㅋ 느낌알겠어요 ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ 느낌알겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019860.1.20",
          "form": "그나저나 요즘 이런노래 잘 안나오는거같아요ㅛ",
          "original_form": "그나저나 요즘 이런노래 잘 안나오는거같아요ㅛ",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019860.1.21",
          "form": "그죠 ㅠㅠㅠ어떤노랴좋아해요!",
          "original_form": "그죠 ㅠㅠㅠ어떤노랴좋아해요!",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019860.1.22",
          "form": "음 좋아하는노래 물어본거맞죠?",
          "original_form": "음 좋아하는노래 물어본거맞죠?",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019860.1.23",
          "form": "저는 요즘 자기전에 에담의 12:45 라는 노래들어요~ㅋㅋ",
          "original_form": "저는 요즘 자기전에 에담의 12:45 라는 노래들어요~ㅋㅋ",
          "speaker_id": "1",
