      "utterance": [
        {
          "id": "MDRW2100017789.1.1",
          "form": "런닝맨에서 누구좋아하세요",
          "original_form": "런닝맨에서 누구좋아하세요",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017789.1.2",
          "form": "멤버를 좋아하기 보다는",
          "original_form": "멤버를 좋아하기 보다는",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017789.1.3",
          "form": "그냥 웃겨서 좋아해요",
          "original_form": "그냥 웃겨서 좋아해요",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017789.1.4",
          "form": "무한도전만한 프로가 없는거같아요 ㅠ",
          "original_form": "무한도전만한 프로가 없는거같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017789.1.5",
          "form": "무한도전 진짜 재밋엇엇죠ㅠㅠ",
          "original_form": "무한도전 진짜 재밋엇엇죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017789.1.6",
          "form": "지금은 슬의생 보는중요 ㅎ",
          "original_form": "지금은 슬의생 보는중요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017789.1.7",
          "form": "오오 재밋어요?",
          "original_form": "오오 재밋어요?",
          "speaker_id": "2",
          "time": "20210827 00:22"
        },
        {
          "id": "MDRW2100017789.1.8",
          "form": "네네 재밌더라구용",
          "original_form": "네네 재밌더라구용",
          "speaker_id": "1",
          "time": "20210827 10:20"
        },
        {
          "id": "MDRW2100017789.1.9",
          "form": "오 담에 시간나면 한번 정주행 해봐야겟어요 ㅋㅋ",
          "original_form": "오 담에 시간나면 한번 정주행 해봐야겟어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100017789.1.10",
          "form": "퇴근 ㅠ 집가서 한잔하면서 드라마 봐야겠어용",
          "original_form": "퇴근 ㅠ 집가서 한잔하면서 드라마 봐야겠어용",
          "speaker_id": "1",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100017789.1.11",
          "form": "퇴근 축하드려용 ㅋㅋ",
          "original_form": "퇴근 축하드려용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100017789.1.12",
          "form": "유투브 자주보세요?",
          "original_form": "유투브 자주보세요?",
          "speaker_id": "1",
          "time": "20210828 07:59"
        },
        {
          "id": "MDRW2100017789.1.13",
          "form": "넹넹 자주보죠",
          "original_form": "넹넹 자주보죠",
          "speaker_id": "2",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100017789.1.14",
          "form": "어떤류 보시나요",
          "original_form": "어떤류 보시나요",
          "speaker_id": "1",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100017789.1.15",
          "form": "음 브이로그도 보고 게임도 보고 공연영상 등 다보는거같아요 ㅋㅋㅋ",
