      "utterance": [
        {
          "id": "MDRW2100010643.1.1",
          "form": "좋아하는 그룹 있어요?",
          "original_form": "좋아하는 그룹 있어요?",
          "speaker_id": "1",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010643.1.2",
          "form": "당연히 방탄 아니겠어요?",
          "original_form": "당연히 방탄 아니겠어요?",
          "speaker_id": "2",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010643.1.3",
          "form": "아 아워 싱어죠",
          "original_form": "아 아워 싱어죠",
          "speaker_id": "1",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010643.1.4",
          "form": "요즘 코로나때문에 콘서트 안열려서",
          "original_form": "요즘 코로나때문에 콘서트 안열려서",
          "speaker_id": "1",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010643.1.5",
          "form": "하... 콘서트 실물로 본것도 어언 1년은 무슨 더 지났어요...",
          "original_form": "하... 콘서트 실물로 본것도 어언 1년은 무슨 더 지났어요...",
          "speaker_id": "2",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010643.1.6",
          "form": "서운하시겠음 ㅠㅠ",
          "original_form": "서운하시겠음 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010643.1.7",
          "form": "이러다 탈덕하는거 아닌가 몰라요",
          "original_form": "이러다 탈덕하는거 아닌가 몰라요",
          "speaker_id": "2",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010643.1.8",
          "form": "앨범 다 양도해야하나 생각중이에요",
          "original_form": "앨범 다 양도해야하나 생각중이에요",
          "speaker_id": "2",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010643.1.9",
          "form": "공연뽕을 맞아야",
          "original_form": "공연뽕을 맞아야",
          "speaker_id": "2",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010643.1.10",
          "form": "덕후가 지속되는건데...!",
          "original_form": "덕후가 지속되는건데...!",
          "speaker_id": "2",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010643.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010643.1.12",
          "form": "진정해여",
          "original_form": "진정해여",
          "speaker_id": "1",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010643.1.13",
          "form": "오프로 뛰고싶어요...",
          "original_form": "오프로 뛰고싶어요...",
          "speaker_id": "2",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010643.1.14",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010643.1.15",
          "form": "저 그 콘서트 예약",
          "original_form": "저 그 콘서트 예약",
          "speaker_id": "1",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010643.1.16",
          "form": "친구때문에 같이 해봤는데",
          "original_form": "친구때문에 같이 해봤는데",
          "speaker_id": "1",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010643.1.17",
          "form": "무슨 콘이었죠?",
          "original_form": "무슨 콘이었죠?",
          "speaker_id": "2",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010643.1.18",
          "form": "포도송이 잡는게",
          "original_form": "포도송이 잡는게",
          "speaker_id": "1",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010643.1.19",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010643.1.20",
          "form": "와 진짜 어려워요",
          "original_form": "와 진짜 어려워요",
          "speaker_id": "1",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010643.1.21",
          "form": "이름은 잘 몰라옄ㅋㅋ",
          "original_form": "이름은 잘 몰라옄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010643.1.22",
          "form": "와 장난 아니죠",
          "original_form": "와 장난 아니죠",
          "speaker_id": "2",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010643.1.23",
          "form": "모든 가수의 콘은 다 힘들어요",
          "original_form": "모든 가수의 콘은 다 힘들어요",
          "speaker_id": "2",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010643.1.24",
          "form": "좌석이 작을수록 특히요!",
          "original_form": "좌석이 작을수록 특히요!",
          "speaker_id": "2",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010643.1.25",
          "form": "방탄...콘... 같이 잡으러 가봤는데",
          "original_form": "방탄...콘... 같이 잡으러 가봤는데",
          "speaker_id": "1",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010643.1.26",
          "form": "와 장난아니여",
          "original_form": "와 장난아니여",
          "speaker_id": "1",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010643.1.27",
          "form": "장난 아니긴 한데 지금은 추첨으로 바뀌었잖아요",
          "original_form": "장난 아니긴 한데 지금은 추첨으로 바뀌었잖아요",
          "speaker_id": "2",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100010643.1.28",
          "form": "전 차라리 티켓팅이 더 좋았어요",
          "original_form": "전 차라리 티켓팅이 더 좋았어요",
          "speaker_id": "2",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100010643.1.29",
          "form": "그건 잡을 수 있다는 확신이 있는데",
          "original_form": "그건 잡을 수 있다는 확신이 있는데",
          "speaker_id": "2",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010643.1.30",
          "form": "추첨은 진짜 천운이니까ㅜㄴ",
          "original_form": "추첨은 진짜 천운이니까ㅜㄴ",
          "speaker_id": "2",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010643.1.31",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "1",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010643.1.32",
          "form": "ㅜㅠ",
          "original_form": "ㅜㅠ",
          "speaker_id": "2",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010643.1.33",
          "form": "숙련된 고수시군옄ㅋㅋㅋ",
          "original_form": "숙련된 고수시군옄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010643.1.34",
          "form": "방법이 있죠...그리고 티켓팅이면 취켓팅이라던가 희망도 보이는데 추첨은 아니잖아요..",
          "original_form": "방법이 있죠...그리고 티켓팅이면 취켓팅이라던가 희망도 보이는데 추첨은 아니잖아요..",
          "speaker_id": "2",
          "time": "20210823 20:22"
        },
        {
          "id": "MDRW2100010643.1.35",
          "form": "제가 보기엔 둘 다 별따기인디...",
          "original_form": "제가 보기엔 둘 다 별따기인디...",
          "speaker_id": "1",
          "time": "20210823 20:22"
        },
        {
          "id": "MDRW2100010643.1.36",
          "form": "티켓팅을 잘 안해봐서 그래요ㅋㅋㄱㅋ 이것도 단련될 수 있다구요",
          "original_form": "티켓팅을 잘 안해봐서 그래요ㅋㅋㄱㅋ 이것도 단련될 수 있다구요",
          "speaker_id": "2",
          "time": "20210823 20:23"
        }
      ]
    }
