      "utterance": [
        {
          "id": "MDRW2100039197.1.1",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "2",
          "time": "20210903 15:49"
        },
        {
          "id": "MDRW2100039197.1.2",
          "form": "여긴 비오고 흐려요",
          "original_form": "여긴 비오고 흐려요",
          "speaker_id": "2",
          "time": "20210903 15:49"
        },
        {
          "id": "MDRW2100039197.1.3",
          "form": "맑은하늘이 이제 그리워요",
          "original_form": "맑은하늘이 이제 그리워요",
          "speaker_id": "2",
          "time": "20210903 15:49"
        },
        {
          "id": "MDRW2100039197.1.4",
          "form": "그쪽은 아직 흐리구나",
          "original_form": "그쪽은 아직 흐리구나",
          "speaker_id": "1",
          "time": "20210903 19:30"
        },
        {
          "id": "MDRW2100039197.1.5",
          "form": "여긴 하루종일 화창했어요",
          "original_form": "여긴 하루종일 화창했어요",
          "speaker_id": "1",
          "time": "20210903 19:30"
        },
        {
          "id": "MDRW2100039197.1.6",
          "form": "얼마만인지",
          "original_form": "얼마만인지",
          "speaker_id": "1",
          "time": "20210903 19:30"
        },
        {
          "id": "MDRW2100039197.1.7",
          "form": "내일은 비소식이 없던데",
          "original_form": "내일은 비소식이 없던데",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039197.1.8",
          "form": "내일 여기도 화창했으면 좋겠네요",
          "original_form": "내일 여기도 화창했으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039197.1.9",
          "form": "그러게요.. 정말 며칠이라도 화창함이 쭉 이어졌으면 좋겠어요",
          "original_form": "그러게요.. 정말 며칠이라도 화창함이 쭉 이어졌으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039197.1.10",
          "form": "글쵸",
          "original_form": "글쵸",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039197.1.11",
          "form": "비님덕에 청소를 해도 계속 꿉꿉한거 같아요",
          "original_form": "비님덕에 청소를 해도 계속 꿉꿉한거 같아요",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039197.1.12",
          "form": "며칠에 한번씩 오면 공기청소도 되고 좋은데",
          "original_form": "며칠에 한번씩 오면 공기청소도 되고 좋은데",
          "speaker_id": "1",
          "time": "20210904 05:12"
        },
        {
          "id": "MDRW2100039197.1.13",
          "form": "여러날은 연속으로 오면 참 난감해요",
          "original_form": "여러날은 연속으로 오면 참 난감해요",
          "speaker_id": "1",
          "time": "20210904 05:12"
        },
        {
          "id": "MDRW2100039197.1.14",
          "form": "글쵸",
          "original_form": "글쵸",
          "speaker_id": "2",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100039197.1.15",
          "form": "그래도 오늘은 해가나네요",
          "original_form": "그래도 오늘은 해가나네요",
          "speaker_id": "2",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100039197.1.16",
          "form": "바람이 선선해요",
          "original_form": "바람이 선선해요",
          "speaker_id": "2",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100039197.1.17",
          "form": "네 해는 나면서 바람이 선선해서 가을 느낌이 물씬 납니다",
          "original_form": "네 해는 나면서 바람이 선선해서 가을 느낌이 물씬 납니다",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100039197.1.18",
          "form": "이런날 먼가 빨래해서 널고싶더라구요",
          "original_form": "이런날 먼가 빨래해서 널고싶더라구요",
          "speaker_id": "2",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100039197.1.19",
          "form": "그래서 어제 빨래 해서 널었어요.. 오늘까지 해가 난다고 해서요",
          "original_form": "그래서 어제 빨래 해서 널었어요.. 오늘까지 해가 난다고 해서요",
          "speaker_id": "1",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100039197.1.20",
          "form": "저는 오늘 아이들 카시트 커버 다 벗겨서 빨려구요 ㅎ",
          "original_form": "저는 오늘 아이들 카시트 커버 다 벗겨서 빨려구요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100039197.1.21",
          "form": "좋아요 좋아요~ 오랜만에 만난 햇님을 잘 이용하자구요~",
          "original_form": "좋아요 좋아요~ 오랜만에 만난 햇님을 잘 이용하자구요~",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100039197.1.22",
          "form": "글쵸 그리고 빨래는 요런 햇살에 널어서 말리는게",
          "original_form": "글쵸 그리고 빨래는 요런 햇살에 널어서 말리는게",
          "speaker_id": "2",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100039197.1.23",
          "form": "기분 좋더라구요",
          "original_form": "기분 좋더라구요",
          "speaker_id": "2",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100039197.1.24",
          "form": "건조기에 넘 익숙해져서 ㅎㅎ",
          "original_form": "건조기에 넘 익숙해져서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100039197.1.25",
