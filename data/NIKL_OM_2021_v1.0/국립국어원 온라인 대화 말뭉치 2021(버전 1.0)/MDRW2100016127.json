      "utterance": [
        {
          "id": "MDRW2100016127.1.1",
          "form": "한번에 끝나는거 맞으신줄!",
          "original_form": "한번에 끝나는거 맞으신줄!",
          "speaker_id": "2",
          "time": "20210826 09:59"
        },
        {
          "id": "MDRW2100016127.1.2",
          "form": "아아니엿지헷갈렷네ㅠㅠ",
          "original_form": "아아니엿지헷갈렷네ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 09:59"
        },
        {
          "id": "MDRW2100016127.1.3",
          "form": "az맞으러갔어!!",
          "original_form": "az맞으러갔어!!",
          "speaker_id": "1",
          "time": "20210826 10:04"
        },
        {
          "id": "MDRW2100016127.1.4",
          "form": "언니 부모님은 언제 2차 맞으셔??",
          "original_form": "언니 부모님은 언제 2차 맞으셔??",
          "speaker_id": "1",
          "time": "20210826 10:04"
        },
        {
          "id": "MDRW2100016127.1.5",
          "form": "미뤄져서 10일인가?",
          "original_form": "미뤄져서 10일인가?",
          "speaker_id": "2",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100016127.1.6",
          "form": "3일에서미뤄짐 ㅠ",
          "original_form": "3일에서미뤄짐 ㅠ",
          "speaker_id": "2",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100016127.1.7",
          "form": "와 근데 미뤄져도 며칠 안늘어났네?? ㅠㅠ 울엄만 거의 2주 밀렸는데",
          "original_form": "와 근데 미뤄져도 며칠 안늘어났네?? ㅠㅠ 울엄만 거의 2주 밀렸는데",
          "speaker_id": "1",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100016127.1.8",
          "form": "일주일인가",
          "original_form": "일주일인가",
          "speaker_id": "2",
          "time": "20210826 10:16"
        },
        {
          "id": "MDRW2100016127.1.9",
          "form": "그럼 17일수도잇어 ㅠㅠ",
          "original_form": "그럼 17일수도잇어 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 10:17"
        },
        {
          "id": "MDRW2100016127.1.10",
          "form": "ㅠㅠ넘 많이 밀려서 걱정이군 효과없을까봐",
          "original_form": "ㅠㅠ넘 많이 밀려서 걱정이군 효과없을까봐",
          "speaker_id": "1",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100016127.1.11",
          "form": "그럼 오셨어ㅠ? 푹쉬셔야할텐데ㅠ",
          "original_form": "그럼 오셨어ㅠ? 푹쉬셔야할텐데ㅠ",
          "speaker_id": "2",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100016127.1.12",
          "form": "그래도 진짜 날이풀려서 그나마다행이야ㅠ",
          "original_form": "그래도 진짜 날이풀려서 그나마다행이야ㅠ",
          "speaker_id": "2",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016127.1.13",
          "form": "아직 안오셨어 그거 맞고 15분인가 대기하고 와야되서 그런가봐!!",
          "original_form": "아직 안오셨어 그거 맞고 15분인가 대기하고 와야되서 그런가봐!!",
          "speaker_id": "1",
          "time": "20210826 10:20"
        },
        {
          "id": "MDRW2100016127.1.14",
          "form": "마자마자 ㅠㅠ",
          "original_form": "마자마자 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 10:21"
        },
        {
          "id": "MDRW2100016127.1.15",
          "form": "언니 동생도 2차까지 끝났어??",
          "original_form": "언니 동생도 2차까지 끝났어??",
          "speaker_id": "1",
          "time": "20210826 10:22"
        },
        {
          "id": "MDRW2100016127.1.16",
          "form": "아직아닐껄 ㅠ엄빠랑비슷할때맞아서",
          "original_form": "아직아닐껄 ㅠ엄빠랑비슷할때맞아서",
          "speaker_id": "2",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016127.1.17",
          "form": "이미 그 때 맞은게 2차인 줄 알았네!ㅠㅠ",
          "original_form": "이미 그 때 맞은게 2차인 줄 알았네!ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 10:28"
        },
        {
          "id": "MDRW2100016127.1.18",
          "form": "내가제일꼴번이얌 ㅋㅋ",
          "original_form": "내가제일꼴번이얌 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:30"
        },
        {
          "id": "MDRW2100016127.1.19",
          "form": "나두 우리집 마지막이야 ㅋㅋㅋ",
