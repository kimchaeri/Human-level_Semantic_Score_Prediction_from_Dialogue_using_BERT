      "utterance": [
        {
          "id": "MDRW2100006442.1.1",
          "form": "아빠 머리가 나면 좋겠다 ㅠㅠ",
          "original_form": "아빠 머리가 나면 좋겠다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100006442.1.2",
          "form": "왜 정수리쪽만 계속 빠지는것같지..",
          "original_form": "왜 정수리쪽만 계속 빠지는것같지..",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100006442.1.3",
          "form": "탈모 넘 슬프다 ㅠ",
          "original_form": "탈모 넘 슬프다 ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100006442.1.4",
          "form": "어쩔수없다",
          "original_form": "어쩔수없다",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100006442.1.5",
          "form": "왜 어쩔수없어!",
          "original_form": "왜 어쩔수없어!",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100006442.1.6",
          "form": "이미 늦은듯 ㅠ",
          "original_form": "이미 늦은듯 ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100006442.1.7",
          "form": "ㅋㅋ 아니다! 영양제 챙겨먹으면돼요",
          "original_form": "ㅋㅋ 아니다! 영양제 챙겨먹으면돼요",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100006442.1.8",
          "form": "집에 있는거?",
          "original_form": "집에 있는거?",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100006442.1.9",
          "form": "웅웅 다 먹으면 또 사올게",
          "original_form": "웅웅 다 먹으면 또 사올게",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100006442.1.10",
          "form": "넘 걱정마요~",
          "original_form": "넘 걱정마요~",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100006442.1.11",
          "form": "맞다! ㅋㅋ걱정안해도 되어용",
          "original_form": "맞다! ㅋㅋ걱정안해도 되어용",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100006442.1.12",
          "form": "알았다",
          "original_form": "알았다",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100006442.1.13",
          "form": "스트레스받는게 더 안좋음",
          "original_form": "스트레스받는게 더 안좋음",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100006442.1.14",
          "form": "그렇다 ㅋㅋ",
          "original_form": "그렇다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100006442.1.15",
          "form": "그래 스트레스 받지 말아야지",
          "original_form": "그래 스트레스 받지 말아야지",
          "speaker_id": "2",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100006442.1.16",
          "form": "스트레스가 만병의 근원~",
          "original_form": "스트레스가 만병의 근원~",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100006442.1.17",
          "form": "그렇다 ㅋ 맘을 편하게 먹어야겠군",
          "original_form": "그렇다 ㅋ 맘을 편하게 먹어야겠군",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100006442.1.18",
          "form": "웅웅~머리는 다시 자랄겨",
          "original_form": "웅웅~머리는 다시 자랄겨",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100006442.1.19",
          "form": "아빠 요새 운동 많이해서 뱃살 금방 빠질듯",
          "original_form": "아빠 요새 운동 많이해서 뱃살 금방 빠질듯",
