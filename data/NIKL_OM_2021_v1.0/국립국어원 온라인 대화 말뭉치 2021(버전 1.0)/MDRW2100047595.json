      "utterance": [
        {
          "id": "MDRW2100047595.1.1",
          "form": "뉴스에 이재명 뭐라던데",
          "original_form": "뉴스에 이재명 뭐라던데",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100047595.1.2",
          "form": "기본소득 얘기하네.  수술 같은거라고",
          "original_form": "기본소득 얘기하네.  수술 같은거라고",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100047595.1.3",
          "form": "추석전까지 지급한다고 하더니",
          "original_form": "추석전까지 지급한다고 하더니",
          "speaker_id": "2",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100047595.1.4",
          "form": "아 재난지원금 말고 자기가 맨날 얘기하는 기본소득",
          "original_form": "아 재난지원금 말고 자기가 맨날 얘기하는 기본소득",
          "speaker_id": "1",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100047595.1.5",
          "form": "아 그 기본소득  우리나라 재정상태가 안되는데 되겠나",
          "original_form": "아 그 기본소득  우리나라 재정상태가 안되는데 되겠나",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100047595.1.6",
          "form": "몰라 범위내에서 하긋지 뭐",
          "original_form": "몰라 범위내에서 하긋지 뭐",
          "speaker_id": "1",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100047595.1.7",
          "form": "그리고 그 정도의 금액으로 생활에 도움이 안된다",
          "original_form": "그리고 그 정도의 금액으로 생활에 도움이 안된다",
          "speaker_id": "2",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100047595.1.8",
          "form": "몇분만에 끝났다.",
          "original_form": "몇분만에 끝났다.",
          "speaker_id": "1",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100047595.1.9",
          "form": "별 얘기 안 나눔ㅋㅋㅋ",
          "original_form": "별 얘기 안 나눔ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100047595.1.10",
          "form": "ㅋㅋ뉴스보니 언론중재법 통과 못했다",
          "original_form": "ㅋㅋ뉴스보니 언론중재법 통과 못했다",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100047595.1.11",
          "form": "말이  많드라 아니가",
          "original_form": "말이  많드라 아니가",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100047595.1.12",
          "form": "응 어떻게 될지 모르겠다",
          "original_form": "응 어떻게 될지 모르겠다",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100047595.1.13",
          "form": "축구 보나?  케인인가 손흥민 있는 곳에 잔류하기로 했다네",
          "original_form": "축구 보나?  케인인가 손흥민 있는 곳에 잔류하기로 했다네",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100047595.1.14",
          "form": "그렇다고 하더라 난 유승준 뉴스 있어서 봤다",
          "original_form": "그렇다고 하더라 난 유승준 뉴스 있어서 봤다",
