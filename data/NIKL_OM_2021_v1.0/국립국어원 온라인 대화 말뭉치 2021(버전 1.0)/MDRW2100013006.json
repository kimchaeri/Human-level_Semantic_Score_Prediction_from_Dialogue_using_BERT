      "utterance": [
        {
          "id": "MDRW2100013006.1.1",
          "form": "오늘 비 엄청 많이 와요ㅜㅜ",
          "original_form": "오늘 비 엄청 많이 와요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100013006.1.2",
          "form": "그러네요ㅠㅠ 하늘 무너지는줄 알았어요",
          "original_form": "그러네요ㅠㅠ 하늘 무너지는줄 알았어요",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100013006.1.3",
          "form": "태풍 피해는 없으셨나요?",
          "original_form": "태풍 피해는 없으셨나요?",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100013006.1.4",
          "form": "일단지금까지는없어요ㅜㅜ",
          "original_form": "일단지금까지는없어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100013006.1.5",
          "form": "근데 비랑 싸워야해요",
          "original_form": "근데 비랑 싸워야해요",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100013006.1.6",
          "form": "ㅠㅠ지금도 비가 많이 오나봐요",
          "original_form": "ㅠㅠ지금도 비가 많이 오나봐요",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100013006.1.7",
          "form": "전 부산인데 지금 갑자기 그쳤어요",
          "original_form": "전 부산인데 지금 갑자기 그쳤어요",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100013006.1.8",
          "form": "비오늘게 워낙 불규칙적이라...",
          "original_form": "비오늘게 워낙 불규칙적이라...",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100013006.1.9",
          "form": "날씨가 변동이 심해요",
          "original_form": "날씨가 변동이 심해요",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100013006.1.10",
          "form": "얼른 겨울되서 따신옷 입고 싶어요",
          "original_form": "얼른 겨울되서 따신옷 입고 싶어요",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100013006.1.11",
          "form": "저두요 그게 더 나아요",
          "original_form": "저두요 그게 더 나아요",
          "speaker_id": "1",
          "time": "20210826 06:56"
        },
        {
          "id": "MDRW2100013006.1.12",
          "form": "겨울 날씨 좋아하세요?",
          "original_form": "겨울 날씨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 08:28"
        },
        {
          "id": "MDRW2100013006.1.13",
          "form": "겨울 날씨 좋아해요",
          "original_form": "겨울 날씨 좋아해요",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100013006.1.14",
          "form": "저도 겨울 날씨 좋아해요 ㅋㅋ",
          "original_form": "저도 겨울 날씨 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100013006.1.15",
          "form": "저도 겨울좋아해요!!",
          "original_form": "저도 겨울좋아해요!!",
          "speaker_id": "1",
          "time": "20210827 10:21"
        },
        {
          "id": "MDRW2100013006.1.16",
          "form": "지금 너무 더워서 빨리 겨울이 왔으면 좋겠어요",
          "original_form": "지금 너무 더워서 빨리 겨울이 왔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210827 10:22"
        },
        {
          "id": "MDRW2100013006.1.17",
          "form": "밤에는 바람이 시원한데 낮에는 확실히 덥죵..ㅎㅎ",
          "original_form": "밤에는 바람이 시원한데 낮에는 확실히 덥죵..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100013006.1.18",
          "form": "꿉꿉하다고해야하나..",
          "original_form": "꿉꿉하다고해야하나..",
          "speaker_id": "2",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100013006.1.19",
          "form": "맞아요 비오다 안오다 그래서 좀 찝찝해요ㅜㅜ",
          "original_form": "맞아요 비오다 안오다 그래서 좀 찝찝해요ㅜㅜ",
