      "utterance": [
        {
          "id": "MDRW2100040153.1.1",
          "form": "소니 니콘",
          "original_form": "소니 니콘",
          "speaker_id": "2",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040153.1.2",
          "form": "캐논도 일본 건가?",
          "original_form": "캐논도 일본 건가?",
          "speaker_id": "2",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040153.1.3",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040153.1.4",
          "form": "카메라는",
          "original_form": "카메라는",
          "speaker_id": "1",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040153.1.5",
          "form": "일본이진짜",
          "original_form": "일본이진짜",
          "speaker_id": "1",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040153.1.6",
          "form": "대박이구나",
          "original_form": "대박이구나",
          "speaker_id": "1",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040153.1.7",
          "form": "ㅇㅇㅇㅇㅇ 거기가 꽉 잡고 있지",
          "original_form": "ㅇㅇㅇㅇㅇ 거기가 꽉 잡고 있지",
          "speaker_id": "2",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040153.1.8",
          "form": "그래서 한창 카메라 볼때",
          "original_form": "그래서 한창 카메라 볼때",
          "speaker_id": "2",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040153.1.9",
          "form": "일본 가고 싶었음",
          "original_form": "일본 가고 싶었음",
          "speaker_id": "2",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100040153.1.10",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040153.1.11",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040153.1.12",
          "form": "정작 일본 갔을 땐 목도리 사들고 왔지만",
          "original_form": "정작 일본 갔을 땐 목도리 사들고 왔지만",
          "speaker_id": "2",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040153.1.13",
          "form": "일본은아직도",
          "original_form": "일본은아직도",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040153.1.14",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040153.1.15",
          "form": "카메라",
          "original_form": "카메라",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040153.1.16",
          "form": "많이쓰더라",
          "original_form": "많이쓰더라",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040153.1.17",
          "form": "그니까 신기했음",
          "original_form": "그니까 신기했음",
          "speaker_id": "2",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040153.1.18",
          "form": "일회용카메라같은거",
          "original_form": "일회용카메라같은거",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040153.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ목도리",
          "original_form": "ㅋㅋㅋㅋㅋㅋ목도리",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040153.1.20",
          "form": "추웠어",
          "original_form": "추웠어",
          "speaker_id": "2",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040153.1.21",
          "form": "ㅇ그목도리잘쓰고있음?",
          "original_form": "ㅇ그목도리잘쓰고있음?",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040153.1.22",
          "form": "지금도",
          "original_form": "지금도",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100040153.1.23",
          "form": "ㅋㅋㅋㅋㅋ아니",
          "original_form": "ㅋㅋㅋㅋㅋ아니",
          "speaker_id": "1",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100040153.1.24",
          "form": "아니 일단 내가 목도리를 잘 안함",
          "original_form": "아니 일단 내가 목도리를 잘 안함",
          "speaker_id": "2",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100040153.1.25",
          "form": "겨울에 밖에 잘 안나감",
          "original_form": "겨울에 밖에 잘 안나감",
          "speaker_id": "2",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100040153.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ뭐야",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ뭐야",
          "speaker_id": "1",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100040153.1.27",
          "form": "쓰려고 노력은 함",
          "original_form": "쓰려고 노력은 함",
          "speaker_id": "2",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100040153.1.28",
          "form": "왜산거야",
          "original_form": "왜산거야",
          "speaker_id": "1",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100040153.1.29",
          "form": "그니까 그냥 거기가 너무 추워서 잠깐",
          "original_form": "그니까 그냥 거기가 너무 추워서 잠깐",
          "speaker_id": "2",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100040153.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아",
          "speaker_id": "1",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100040153.1.31",
          "form": "그리고 오기전에 돈키호테 들려서 너네 선물 사고",
          "original_form": "그리고 오기전에 돈키호테 들려서 너네 선물 사고",
          "speaker_id": "2",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100040153.1.32",
          "form": "아그거",
          "original_form": "아그거",
          "speaker_id": "1",
          "time": "20210904 12:18"
        },
        {
          "id": "MDRW2100040153.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:18"
        },
        {
          "id": "MDRW2100040153.1.34",
          "form": "손거울",
          "original_form": "손거울",
          "speaker_id": "1",
          "time": "20210904 12:18"
