      "utterance": [
        {
          "id": "MDRW2100031704.1.1",
          "form": "고등학교 기억나나요",
          "original_form": "고등학교 기억나나요",
          "speaker_id": "1",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031704.1.2",
          "form": "네 재밌었어요",
          "original_form": "네 재밌었어요",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031704.1.3",
          "form": "기억이 미화된것도 있겠지만요",
          "original_form": "기억이 미화된것도 있겠지만요",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031704.1.4",
          "form": "그래도 돌아가고싶진않네요 이상하게",
          "original_form": "그래도 돌아가고싶진않네요 이상하게",
          "speaker_id": "1",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031704.1.5",
          "form": "고등학교 기억하시나요",
          "original_form": "고등학교 기억하시나요",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031704.1.6",
          "form": "저도 돌아가고싶진 않아요",
          "original_form": "저도 돌아가고싶진 않아요",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031704.1.7",
          "form": "기억납니다 ㅋㅋ",
          "original_form": "기억납니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031704.1.8",
          "form": "동아리도 그러코",
          "original_form": "동아리도 그러코",
          "speaker_id": "1",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031704.1.9",
          "form": "돌아간다면 오로지 공부만 빡공하러 갑니다 ㅋㅋ",
          "original_form": "돌아간다면 오로지 공부만 빡공하러 갑니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031704.1.10",
          "form": "진짜 공부나항걸 그랬어요",
          "original_form": "진짜 공부나항걸 그랬어요",
          "speaker_id": "1",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031704.1.11",
          "form": "네 동아리 개쓸데없는거",
          "original_form": "네 동아리 개쓸데없는거",
          "speaker_id": "2",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031704.1.12",
          "form": "그러니가요",
          "original_form": "그러니가요",
          "speaker_id": "1",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031704.1.13",
          "form": "가끔씩 선배들이 했던짓생각나네요",
          "original_form": "가끔씩 선배들이 했던짓생각나네요",
          "speaker_id": "1",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031704.1.14",
          "form": "친구도 몇명만 남지 , 이젠 반친구들 얼굴 이름 다 기억 안나요",
          "original_form": "친구도 몇명만 남지 , 이젠 반친구들 얼굴 이름 다 기억 안나요",
          "speaker_id": "2",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031704.1.15",
          "form": "젊꼰들이었죠",
          "original_form": "젊꼰들이었죠",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031704.1.16",
          "form": "저두요 ㅋㅋ",
          "original_form": "저두요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031704.1.17",
          "form": "그냥 몇명만 남았네요",
          "original_form": "그냥 몇명만 남았네요",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031704.1.18",
          "form": "네 아닌애들은 빠이하고",
          "original_form": "네 아닌애들은 빠이하고",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031704.1.19",
          "form": "아 주연이 합격했나요",
          "original_form": "아 주연이 합격했나요",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031704.1.20",
          "form": "몰라요 ㅋㅋ안물어봤네요",
          "original_form": "몰라요 ㅋㅋ안물어봤네요",
          "speaker_id": "1",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031704.1.21",
          "form": "검색안되나요?",
          "original_form": "검색안되나요?",
          "speaker_id": "1",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031704.1.22",
          "form": "안해봤어요",
          "original_form": "안해봤어요",
          "speaker_id": "2",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031704.1.23",
          "form": "해보면 나오지 않을까요",
          "original_form": "해보면 나오지 않을까요",
          "speaker_id": "1",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031704.1.24",
          "form": "담에 해봐야겠네요",
          "original_form": "담에 해봐야겠네요",
          "speaker_id": "2",
          "time": "20210904 23:41"
        }
      ]
    }
