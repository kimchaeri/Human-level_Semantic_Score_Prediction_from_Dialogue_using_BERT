        },
        {
          "id": "MDRW2100009013.1.4",
          "form": "아니요 결혼하셨어요?",
          "original_form": "아니요 결혼하셨어요?",
          "speaker_id": "2",
          "time": "20210822 20:33"
        },
        {
          "id": "MDRW2100009013.1.5",
          "form": "네 했습니다",
          "original_form": "네 했습니다",
          "speaker_id": "1",
          "time": "20210822 20:36"
        },
        {
          "id": "MDRW2100009013.1.6",
          "form": "결혼은 하는게 낫다고 생각하세요?",
          "original_form": "결혼은 하는게 낫다고 생각하세요?",
          "speaker_id": "2",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100009013.1.7",
          "form": "참 어렵고 난해하네요",
          "original_form": "참 어렵고 난해하네요",
          "speaker_id": "1",
          "time": "20210822 20:59"
        },
        {
          "id": "MDRW2100009013.1.8",
          "form": "인생의 큰선택이라 고민 참 많이 돼요",
          "original_form": "인생의 큰선택이라 고민 참 많이 돼요",
          "speaker_id": "2",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100009013.1.9",
          "form": "결혼에 고민중 이신가요 ?",
          "original_form": "결혼에 고민중 이신가요 ?",
          "speaker_id": "1",
