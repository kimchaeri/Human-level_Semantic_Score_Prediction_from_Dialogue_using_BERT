      "utterance": [
        {
          "id": "MDRW2100032835.1.1",
          "form": "악플...사람들이 예민해",
          "original_form": "악플...사람들이 예민해",
          "speaker_id": "2",
          "time": "20210906 00:50"
        },
        {
          "id": "MDRW2100032835.1.2",
          "form": "진짜 너무많아 단편적으로 하나만보고",
          "original_form": "진짜 너무많아 단편적으로 하나만보고",
          "speaker_id": "1",
          "time": "20210906 00:56"
        },
        {
          "id": "MDRW2100032835.1.3",
          "form": "엄청 디스하고",
          "original_form": "엄청 디스하고",
          "speaker_id": "1",
          "time": "20210906 00:56"
        },
        {
          "id": "MDRW2100032835.1.4",
          "form": "세상 저사람을 다 아는듯이",
          "original_form": "세상 저사람을 다 아는듯이",
          "speaker_id": "1",
          "time": "20210906 00:56"
        },
        {
          "id": "MDRW2100032835.1.5",
          "form": "그치 그리고 싸우려고 덤벼드는 사람도 많고",
          "original_form": "그치 그리고 싸우려고 덤벼드는 사람도 많고",
          "speaker_id": "2",
          "time": "20210906 01:00"
        },
        {
          "id": "MDRW2100032835.1.6",
          "form": "아 그치",
          "original_form": "아 그치",
          "speaker_id": "1",
          "time": "20210906 01:08"
        },
        {
          "id": "MDRW2100032835.1.7",
          "form": "진짜 왜그럴까 답답해",
          "original_form": "진짜 왜그럴까 답답해",
          "speaker_id": "1",
          "time": "20210906 01:09"
        },
        {
          "id": "MDRW2100032835.1.8",
          "form": "일반인한테",
          "original_form": "일반인한테",
          "speaker_id": "1",
          "time": "20210906 01:09"
        },
        {
          "id": "MDRW2100032835.1.9",
          "form": "아니 그냥 인터넷에 고민상담쓴거 하나가지고",
          "original_form": "아니 그냥 인터넷에 고민상담쓴거 하나가지고",
          "speaker_id": "1",
          "time": "20210906 01:09"
        },
        {
          "id": "MDRW2100032835.1.10",
          "form": "온갖 악플을 다 달아",
          "original_form": "온갖 악플을 다 달아",
          "speaker_id": "1",
          "time": "20210906 01:09"
        },
        {
          "id": "MDRW2100032835.1.11",
          "form": "요즘엔 인스타로 세상을 보는 사람들이 많은 것 같아",
          "original_form": "요즘엔 인스타로 세상을 보는 사람들이 많은 것 같아",
          "speaker_id": "2",
          "time": "20210906 01:11"
        },
        {
          "id": "MDRW2100032835.1.12",
          "form": "인스타랑 유튜브 이런 것으로만",
          "original_form": "인스타랑 유튜브 이런 것으로만",
          "speaker_id": "2",
          "time": "20210906 01:11"
        },
        {
          "id": "MDRW2100032835.1.13",
          "form": "뭔가 사람들 시각이 좁아지는 느낌이랄까",
          "original_form": "뭔가 사람들 시각이 좁아지는 느낌이랄까",
          "speaker_id": "2",
          "time": "20210906 01:12"
        },
        {
          "id": "MDRW2100032835.1.14",
          "form": "흠 마자",
          "original_form": "흠 마자",
          "speaker_id": "1",
          "time": "20210906 01:18"
        },
        {
          "id": "MDRW2100032835.1.15",
          "form": "페북도 있고",
          "original_form": "페북도 있고",
          "speaker_id": "1",
          "time": "20210906 01:18"
        },
        {
          "id": "MDRW2100032835.1.16",
          "form": "근데 예전도 그렇긴햏는데",
          "original_form": "근데 예전도 그렇긴햏는데",
          "speaker_id": "1",
          "time": "20210906 01:18"
        },
        {
          "id": "MDRW2100032835.1.17",
          "form": "요즘엔 그게 더 심해지는 느낌이야",
          "original_form": "요즘엔 그게 더 심해지는 느낌이야",
          "speaker_id": "2",
          "time": "20210906 01:19"
        },
        {
          "id": "MDRW2100032835.1.18",
          "form": "인사이트 이런 자극적인 맛만 보고 그냥 믿고 가버리니까...",
          "original_form": "인사이트 이런 자극적인 맛만 보고 그냥 믿고 가버리니까...",
          "speaker_id": "2",
          "time": "20210906 01:20"
        },
        {
          "id": "MDRW2100032835.1.19",
          "form": "아 진짜 거긴뭐하는덴지ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 진짜 거긴뭐하는덴지ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 01:24"
        },
        {
          "id": "MDRW2100032835.1.20",
          "form": "악플을 공론화해ㅋㅋㅋ",
          "original_form": "악플을 공론화해ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 01:24"
        },
        {
          "id": "MDRW2100032835.1.21",
          "form": "유사 언론들이나 다 막았으면 해",
          "original_form": "유사 언론들이나 다 막았으면 해",
          "speaker_id": "2",
          "time": "20210906 01:25"
        },
        {
          "id": "MDRW2100032835.1.22",
          "form": "인사이트 위키 이런 곳들",
          "original_form": "인사이트 위키 이런 곳들",
          "speaker_id": "2",
          "time": "20210906 01:26"
        },
        {
          "id": "MDRW2100032835.1.23",
          "form": "아하긴 맞아 어디 조그만 글하나에 달린악플 안찾아보면 그만인데",
          "original_form": "아하긴 맞아 어디 조그만 글하나에 달린악플 안찾아보면 그만인데",
          "speaker_id": "1",
          "time": "20210906 10:20"
        },
        {
          "id": "MDRW2100032835.1.24",
          "form": "거기서 다퍼가니까",
          "original_form": "거기서 다퍼가니까",
          "speaker_id": "1",
          "time": "20210906 10:20"
        },
        {
          "id": "MDRW2100032835.1.25",
          "form": "그치만 요즘 그냥 언론도 만만치않긴함",
          "original_form": "그치만 요즘 그냥 언론도 만만치않긴함",
          "speaker_id": "1",
          "time": "20210906 10:20"
        },
        {
          "id": "MDRW2100032835.1.26",
          "form": "그건 그렇지 그래도 방송국을 막을 순 없으니까",
          "original_form": "그건 그렇지 그래도 방송국을 막을 순 없으니까",
          "speaker_id": "2",
          "time": "20210906 10:21"
        },
        {
          "id": "MDRW2100032835.1.27",
          "form": "유사 언론들이라도....",
          "original_form": "유사 언론들이라도....",
          "speaker_id": "2",
          "time": "20210906 10:21"
        },
        {
          "id": "MDRW2100032835.1.28",
          "form": "유사 언론들이 특히 젊은 세대에 끼치는 영향력이 너무 큰 것 같아서",
          "original_form": "유사 언론들이 특히 젊은 세대에 끼치는 영향력이 너무 큰 것 같아서",
          "speaker_id": "2",
          "time": "20210906 10:22"
        },
        {
          "id": "MDRW2100032835.1.29",
          "form": "커뮤니티에서 덕질빼고 못하게 하는것도 방법이 되지 않을까",
          "original_form": "커뮤니티에서 덕질빼고 못하게 하는것도 방법이 되지 않을까",
          "speaker_id": "1",
          "time": "20210906 10:30"
        },
        {
          "id": "MDRW2100032835.1.30",
          "form": "그게....불가능하지 아무래도",
          "original_form": "그게....불가능하지 아무래도",
          "speaker_id": "2",
          "time": "20210906 10:30"
        },
        {
          "id": "MDRW2100032835.1.31",
          "form": "사실 사람들 의식 수준 자체가 올라가고 그렇도록 지속적으로 교육해주는 것이 가장 이상적이지",
          "original_form": "사실 사람들 의식 수준 자체가 올라가고 그렇도록 지속적으로 교육해주는 것이 가장 이상적이지",
          "speaker_id": "2",
          "time": "20210906 10:32"
        },
        {
          "id": "MDRW2100032835.1.32",
          "form": "해외 커뮤도 악플 많은건 마찬가지 아닌가??",
          "original_form": "해외 커뮤도 악플 많은건 마찬가지 아닌가??",
          "speaker_id": "1",
          "time": "20210906 10:38"
        },
        {
          "id": "MDRW2100032835.1.33",
          "form": "응? 해외?? 해외...는 모르겠다?",
          "original_form": "응? 해외?? 해외...는 모르겠다?",
          "speaker_id": "2",
          "time": "20210906 10:43"
        },
        {
          "id": "MDRW2100032835.1.34",
          "form": "있겠지 거기도",
          "original_form": "있겠지 거기도",
          "speaker_id": "2",
          "time": "20210906 10:43"
        },
        {
          "id": "MDRW2100032835.1.35",
          "form": "근가 커뮤 악플진짜 기상천외해",
          "original_form": "근가 커뮤 악플진짜 기상천외해",
          "speaker_id": "1",
