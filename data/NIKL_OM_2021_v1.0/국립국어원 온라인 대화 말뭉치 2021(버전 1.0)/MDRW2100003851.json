      "utterance": [
        {
          "id": "MDRW2100003851.1.1",
          "form": "그랬어요?",
          "original_form": "그랬어요?",
          "speaker_id": "2",
          "time": "20210821 12:55"
        },
        {
          "id": "MDRW2100003851.1.2",
          "form": "이런적 많았던거같아요",
          "original_form": "이런적 많았던거같아요",
          "speaker_id": "1",
          "time": "20210821 12:55"
        },
        {
          "id": "MDRW2100003851.1.3",
          "form": "나이무관하게",
          "original_form": "나이무관하게",
          "speaker_id": "1",
          "time": "20210821 12:56"
        },
        {
          "id": "MDRW2100003851.1.4",
          "form": "남자들은 죄다 의심하고보는듯한?",
          "original_form": "남자들은 죄다 의심하고보는듯한?",
          "speaker_id": "1",
          "time": "20210821 12:56"
        },
        {
          "id": "MDRW2100003851.1.5",
          "form": "연애 두번해봤는데",
          "original_form": "연애 두번해봤는데",
          "speaker_id": "1",
          "time": "20210821 12:56"
        },
        {
          "id": "MDRW2100003851.1.6",
          "form": "그런가봐요 정말ㅠ",
          "original_form": "그런가봐요 정말ㅠ",
          "speaker_id": "2",
          "time": "20210821 12:56"
        },
        {
          "id": "MDRW2100003851.1.7",
          "form": "지금이 두번째거든요!",
          "original_form": "지금이 두번째거든요!",
          "speaker_id": "1",
          "time": "20210821 12:56"
        },
        {
          "id": "MDRW2100003851.1.8",
          "form": "첫번째 연애때 한살차이나는 사람 만났는데",
          "original_form": "첫번째 연애때 한살차이나는 사람 만났는데",
          "speaker_id": "1",
          "time": "20210821 12:56"
        },
        {
          "id": "MDRW2100003851.1.9",
          "form": "걔랑도 이랬어요ㅋㅋㅋ",
          "original_form": "걔랑도 이랬어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:56"
        },
        {
          "id": "MDRW2100003851.1.10",
          "form": "와 진짜 다 똑같네요",
          "original_form": "와 진짜 다 똑같네요",
          "speaker_id": "2",
          "time": "20210821 12:56"
        },
        {
          "id": "MDRW2100003851.1.11",
          "form": "사심없는 관계라는걸 이해 못하는듯",
          "original_form": "사심없는 관계라는걸 이해 못하는듯",
          "speaker_id": "1",
          "time": "20210821 12:57"
        },
        {
          "id": "MDRW2100003851.1.12",
          "form": "남자들은 이해 못하나봐요",
          "original_form": "남자들은 이해 못하나봐요",
          "speaker_id": "2",
          "time": "20210821 12:57"
        },
        {
          "id": "MDRW2100003851.1.13",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210821 12:57"
        },
        {
          "id": "MDRW2100003851.1.14",
          "form": "아니 연애하면서",
          "original_form": "아니 연애하면서",
          "speaker_id": "1",
          "time": "20210821 12:57"
        },
        {
          "id": "MDRW2100003851.1.15",
          "form": "이성친구도 만날 수 잏지",
          "original_form": "이성친구도 만날 수 잏지",
          "speaker_id": "1",
          "time": "20210821 12:57"
        },
        {
          "id": "MDRW2100003851.1.16",
          "form": "참나",
          "original_form": "참나",
          "speaker_id": "1",
          "time": "20210821 12:57"
        },
        {
          "id": "MDRW2100003851.1.17",
          "form": "어우 이거가지고는 완전 학을 뗏어요",
          "original_form": "어우 이거가지고는 완전 학을 뗏어요",
          "speaker_id": "1",
          "time": "20210821 12:57"
        },
        {
          "id": "MDRW2100003851.1.18",
          "form": "그냥 넌 얘기해라 난 친구만난다 이 마인드로 넘겨요",
          "original_form": "그냥 넌 얘기해라 난 친구만난다 이 마인드로 넘겨요",
          "speaker_id": "1",
          "time": "20210821 12:57"
        },
        {
          "id": "MDRW2100003851.1.19",
          "form": "그래야 하는수밖에 없을것 같아요",
          "original_form": "그래야 하는수밖에 없을것 같아요",
          "speaker_id": "2",
          "time": "20210821 12:58"
        },
        {
          "id": "MDRW2100003851.1.20",
          "form": "그렇게 말하는게 재밌다고 생각하는걸까요?",
          "original_form": "그렇게 말하는게 재밌다고 생각하는걸까요?",
          "speaker_id": "2",
          "time": "20210821 12:59"
        },
        {
          "id": "MDRW2100003851.1.21",
          "form": "모르겠어요 자기딴엔 반은 장난이래요",
          "original_form": "모르겠어요 자기딴엔 반은 장난이래요",
          "speaker_id": "1",
          "time": "20210821 12:59"
        },
        {
          "id": "MDRW2100003851.1.22",
          "form": "근데 듣는입장에선ㅡㅡ...",
          "original_form": "근데 듣는입장에선ㅡㅡ...",
          "speaker_id": "1",
          "time": "20210821 12:59"
        },
        {
          "id": "MDRW2100003851.1.23",
          "form": "완전 최악이죠ㅠ",
          "original_form": "완전 최악이죠ㅠ",
          "speaker_id": "2",
          "time": "20210821 12:59"
        },
        {
          "id": "MDRW2100003851.1.24",
          "form": "네ㅋㅋ연애때야 뭐 그러려니하는데",
          "original_form": "네ㅋㅋ연애때야 뭐 그러려니하는데",
          "speaker_id": "1",
          "time": "20210821 13:00"
        },
        {
          "id": "MDRW2100003851.1.25",
          "form": "결혼해서도 그얘기하면",
          "original_form": "결혼해서도 그얘기하면",
          "speaker_id": "1",
          "time": "20210821 13:00"
        },
        {
          "id": "MDRW2100003851.1.26",
          "form": "진짜 싫지않아요?",
          "original_form": "진짜 싫지않아요?",
          "speaker_id": "1",
          "time": "20210821 13:00"
        },
        {
          "id": "MDRW2100003851.1.27",
          "form": "저는 진짜 연애를 오래하고 결혼해서 그런지 남편이 예전일로 몇년째 그러니까 너무 힘들어요",
          "original_form": "저는 진짜 연애를 오래하고 결혼해서 그런지 남편이 예전일로 몇년째 그러니까 너무 힘들어요",
          "speaker_id": "2",
          "time": "20210821 13:00"
        },
        {
          "id": "MDRW2100003851.1.28",
          "form": "잊지도 않나봐요",
          "original_form": "잊지도 않나봐요",
          "speaker_id": "2",
          "time": "20210821 13:00"
        }
      ]
    }
  ]
