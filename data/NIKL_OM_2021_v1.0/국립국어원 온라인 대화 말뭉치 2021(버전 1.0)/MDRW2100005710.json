      "utterance": [
        {
          "id": "MDRW2100005710.1.1",
          "form": "안녕 요즘 운동하는 거 있어?",
          "original_form": "안녕 요즘 운동하는 거 있어?",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005710.1.2",
          "form": "아니 요즘은 못하고 있어",
          "original_form": "아니 요즘은 못하고 있어",
          "speaker_id": "2",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100005710.1.3",
          "form": "왜? 무슨 일 있어?",
          "original_form": "왜? 무슨 일 있어?",
          "speaker_id": "1",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100005710.1.4",
          "form": "코로나 4단계로 문을 닫아버렸어ㅜㅜ",
          "original_form": "코로나 4단계로 문을 닫아버렸어ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100005710.1.5",
          "form": "그렇구나ㅠㅠㅠ 나는 점핑다니는데 나도 곧 다닐 것 같아",
          "original_form": "그렇구나ㅠㅠㅠ 나는 점핑다니는데 나도 곧 다닐 것 같아",
          "speaker_id": "1",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100005710.1.6",
          "form": "무슨 운동 했어?",
          "original_form": "무슨 운동 했어?",
          "speaker_id": "1",
          "time": "20210823 01:00"
        },
        {
          "id": "MDRW2100005710.1.7",
          "form": "나는 수영 하고 있었어",
          "original_form": "나는 수영 하고 있었어",
          "speaker_id": "2",
          "time": "20210823 01:00"
        },
        {
          "id": "MDRW2100005710.1.8",
          "form": "그렇구나 수영 힘들지 않아?",
          "original_form": "그렇구나 수영 힘들지 않아?",
          "speaker_id": "1",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100005710.1.9",
          "form": "수영 처음에는 힘들어",
          "original_form": "수영 처음에는 힘들어",
          "speaker_id": "2",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100005710.1.10",
          "form": "나는 수영 못하는데 뭔가 멋있다!",
          "original_form": "나는 수영 못하는데 뭔가 멋있다!",
          "speaker_id": "1",
          "time": "20210823 01:02"
        },
        {
          "id": "MDRW2100005710.1.11",
          "form": "나도 배워볼까 생각이 드네",
          "original_form": "나도 배워볼까 생각이 드네",
          "speaker_id": "1",
          "time": "20210823 01:02"
        },
        {
          "id": "MDRW2100005710.1.12",
          "form": "수영 추천해",
          "original_form": "수영 추천해",
          "speaker_id": "2",
          "time": "20210823 01:02"
        },
        {
          "id": "MDRW2100005710.1.13",
          "form": "한번해봐",
          "original_form": "한번해봐",
          "speaker_id": "2",
          "time": "20210823 01:02"
        },
        {
          "id": "MDRW2100005710.1.14",
          "form": "잠 잘오고 좋아",
          "original_form": "잠 잘오고 좋아",
          "speaker_id": "2",
          "time": "20210823 01:02"
        },
        {
          "id": "MDRW2100005710.1.15",
          "form": "잠 잘 오려나?",
          "original_form": "잠 잘 오려나?",
          "speaker_id": "1",
          "time": "20210823 01:02"
        },
        {
          "id": "MDRW2100005710.1.16",
          "form": "좋다 나도 해볼게!!",
          "original_form": "좋다 나도 해볼게!!",
          "speaker_id": "1",
          "time": "20210823 01:02"
        },
        {
          "id": "MDRW2100005710.1.17",
          "form": "응 운동종목으로 강추야",
          "original_form": "응 운동종목으로 강추야",
          "speaker_id": "2",
          "time": "20210823 01:03"
        },
        {
          "id": "MDRW2100005710.1.18",
          "form": "점핑은 좋아?",
          "original_form": "점핑은 좋아?",
          "speaker_id": "2",
          "time": "20210823 01:03"
        },
        {
          "id": "MDRW2100005710.1.19",
          "form": "그렇구나 고마워",
          "original_form": "그렇구나 고마워",
          "speaker_id": "1",
          "time": "20210823 01:03"
        },
        {
          "id": "MDRW2100005710.1.20",
          "form": "점핑도 힘든데 땀이 많이나",
          "original_form": "점핑도 힘든데 땀이 많이나",
          "speaker_id": "1",
          "time": "20210823 01:03"
        }
