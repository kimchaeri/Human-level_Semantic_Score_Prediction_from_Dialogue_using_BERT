      "utterance": [
        {
          "id": "MDRW2100004569.1.1",
          "form": "운동 하시는거 있으세요?",
          "original_form": "운동 하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004569.1.2",
          "form": "운동은 숨쉬는거^^",
          "original_form": "운동은 숨쉬는거^^",
          "speaker_id": "2",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004569.1.3",
          "form": "시원해져서 슬슬 걷기해보려구요",
          "original_form": "시원해져서 슬슬 걷기해보려구요",
          "speaker_id": "2",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004569.1.4",
          "form": "걷기운동이 건강에 좋대요",
          "original_form": "걷기운동이 건강에 좋대요",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004569.1.5",
          "form": "담주에 개학하니 학교데려다주면 3천~3500보정도 걷더라구요",
          "original_form": "담주에 개학하니 학교데려다주면 3천~3500보정도 걷더라구요",
          "speaker_id": "2",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004569.1.6",
          "form": "만보걷기만해도 좋을텐데 쉽지않아요",
          "original_form": "만보걷기만해도 좋을텐데 쉽지않아요",
          "speaker_id": "1",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004569.1.7",
          "form": "데려다주고 도서관찍고오는게 목표인데 되려나모르겠어요.",
          "original_form": "데려다주고 도서관찍고오는게 목표인데 되려나모르겠어요.",
          "speaker_id": "2",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004569.1.8",
          "form": "날씨 선선해서 괜찮을것같은데요?",
          "original_form": "날씨 선선해서 괜찮을것같은데요?",
          "speaker_id": "1",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004569.1.9",
          "form": "산책삼아 걷고 책도빌려오고",
          "original_form": "산책삼아 걷고 책도빌려오고",
          "speaker_id": "1",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004569.1.10",
          "form": "그러려구요. 게을러서 결심대로될진 모르겠어요",
          "original_form": "그러려구요. 게을러서 결심대로될진 모르겠어요",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004569.1.11",
          "form": "저도 회사집이 가까우니 운동량 제로예요ㅠ",
          "original_form": "저도 회사집이 가까우니 운동량 제로예요ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004569.1.12",
          "form": "운동은 필요한거같아요",
          "original_form": "운동은 필요한거같아요",
          "speaker_id": "2",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004569.1.13",
          "form": "근처라도 산책다니세요",
          "original_form": "근처라도 산책다니세요",
          "speaker_id": "2",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004569.1.14",
          "form": "앞에 공원도있고 한데 나가기가 쉽지않네요",
          "original_form": "앞에 공원도있고 한데 나가기가 쉽지않네요",
          "speaker_id": "1",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004569.1.15",
          "form": "산책 많이들 다니던데 애완견데리고",
          "original_form": "산책 많이들 다니던데 애완견데리고",
          "speaker_id": "1",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004569.1.16",
          "form": "ㅎ 저도 한번 가기가 힘들더라구요.",
          "original_form": "ㅎ 저도 한번 가기가 힘들더라구요.",
          "speaker_id": "2",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004569.1.17",
          "form": "바로뒤에 강변길 잘만들어놨는데도 몇번 안가봤어요",
          "original_form": "바로뒤에 강변길 잘만들어놨는데도 몇번 안가봤어요",
          "speaker_id": "2",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004569.1.18",
          "form": "요즘 산책로도 잘해놨더라구요 걷기운동 시작해야겠어요^^",
          "original_form": "요즘 산책로도 잘해놨더라구요 걷기운동 시작해야겠어요^^",
          "speaker_id": "1",
          "time": "20210821 18:55"
        },
        {
          "id": "MDRW2100004569.1.19",
          "form": "같이 성공해요~~ 화이팅!!",
          "original_form": "같이 성공해요~~ 화이팅!!",
