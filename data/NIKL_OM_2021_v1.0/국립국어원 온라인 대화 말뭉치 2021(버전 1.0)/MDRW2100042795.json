      "utterance": [
        {
          "id": "MDRW2100042795.1.1",
          "form": "이제 점점 아침에 일어나기도 힘들겠지ㅋㅋ",
          "original_form": "이제 점점 아침에 일어나기도 힘들겠지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042795.1.2",
          "form": "아침 뭐 먹노",
          "original_form": "아침 뭐 먹노",
          "speaker_id": "1",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042795.1.3",
          "form": "워킹하면서도 한다",
          "original_form": "워킹하면서도 한다",
          "speaker_id": "1",
          "time": "20210907 09:30"
        },
        {
          "id": "MDRW2100042795.1.4",
          "form": "춥다",
          "original_form": "춥다",
          "speaker_id": "1",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042795.1.5",
          "form": "옷 정리해야 되겠네 오늘",
          "original_form": "옷 정리해야 되겠네 오늘",
          "speaker_id": "1",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042795.1.6",
          "form": "응응 ㅋㅋㅋ 옷 정리 해야지 ㅋ조금이따갘 ㅋ",
          "original_form": "응응 ㅋㅋㅋ 옷 정리 해야지 ㅋ조금이따갘 ㅋ",
          "speaker_id": "2",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042795.1.7",
          "form": "일단 좀따가 밥 좀 먹고 ㅋㅋ",
          "original_form": "일단 좀따가 밥 좀 먹고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042795.1.8",
          "form": "뭘 먹어야될지 ㅋㅋ",
          "original_form": "뭘 먹어야될지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042795.1.9",
          "form": "아침은 대충 먹어라 점심에 잔치 하잖아",
          "original_form": "아침은 대충 먹어라 점심에 잔치 하잖아",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042795.1.10",
          "form": "내 사전에 대충 이란 없다",
          "original_form": "내 사전에 대충 이란 없다",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042795.1.11",
          "form": "아 맞다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 맞다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042795.1.12",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042795.1.13",
          "form": "잔치한다 맞네 ㅋ",
          "original_form": "잔치한다 맞네 ㅋ",
          "speaker_id": "2",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042795.1.14",
          "form": "잔치를 까먹고 있었나ㅋㅋ",
          "original_form": "잔치를 까먹고 있었나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042795.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ잠시 까먹고있었네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ잠시 까먹고있었네",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042795.1.16",
          "form": "그래도 점심은 또 점심이니 ㅋ지금 당장 배가 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래도 점심은 또 점심이니 ㅋ지금 당장 배가 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042795.1.17",
          "form": "아침을 또 아침 답게 먹어줘야지ㅋㅋㅋ",
          "original_form": "아침을 또 아침 답게 먹어줘야지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042795.1.18",
          "form": "name3언니 신났던데",
          "original_form": "&name3&언니 신났던데",
          "speaker_id": "1",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042795.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ그러니깐 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러니깐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042795.1.20",
          "form": "쉬는 날이라고",
          "original_form": "쉬는 날이라고",
          "speaker_id": "1",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042795.1.21",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042795.1.22",
          "form": "아아 ㅋㅋ",
          "original_form": "아아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042795.1.23",
          "form": "아침부터 기분이 가 좋대",
