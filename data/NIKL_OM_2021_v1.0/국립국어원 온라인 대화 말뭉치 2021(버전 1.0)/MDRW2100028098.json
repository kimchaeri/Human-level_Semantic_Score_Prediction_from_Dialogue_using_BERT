      "utterance": [
        {
          "id": "MDRW2100028098.1.1",
          "form": "세계적인 질병이고",
          "original_form": "세계적인 질병이고",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028098.1.2",
          "form": "처음겪으니 힘들어",
          "original_form": "처음겪으니 힘들어",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028098.1.3",
          "form": "그래 세상이 바꼈어 질병으로",
          "original_form": "그래 세상이 바꼈어 질병으로",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028098.1.4",
          "form": "질병은 슬퍼",
          "original_form": "질병은 슬퍼",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028098.1.5",
          "form": "많은사람을 떠나보냈어",
          "original_form": "많은사람을 떠나보냈어",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028098.1.6",
          "form": "환경은 점점 나빠지고 질병은 창궐하겠지",
          "original_form": "환경은 점점 나빠지고 질병은 창궐하겠지",
          "speaker_id": "1",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028098.1.7",
          "form": "이렇게 질병속에 자신을 지키며 사는게 어려워",
          "original_form": "이렇게 질병속에 자신을 지키며 사는게 어려워",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028098.1.8",
          "form": "어떤 질병과 재난이 올까",
          "original_form": "어떤 질병과 재난이 올까",
          "speaker_id": "1",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028098.1.9",
          "form": "나는 무서워",
          "original_form": "나는 무서워",
          "speaker_id": "1",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028098.1.10",
          "form": "코로나보다 더한게 있겠지",
          "original_form": "코로나보다 더한게 있겠지",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028098.1.11",
          "form": "우리 건강을 헤치는건 너무많아",
          "original_form": "우리 건강을 헤치는건 너무많아",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028098.1.12",
          "form": "스스로를 지켜야해",
          "original_form": "스스로를 지켜야해",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028098.1.13",
          "form": "좋은환경이 되어야 질병이 사라져",
          "original_form": "좋은환경이 되어야 질병이 사라져",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028098.1.14",
          "form": "이기심을 버려야해",
          "original_form": "이기심을 버려야해",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028098.1.15",
          "form": "플라스틱도 줄이고",
          "original_form": "플라스틱도 줄이고",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028098.1.16",
          "form": "미용보단 건강이 중요하지",
          "original_form": "미용보단 건강이 중요하지",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028098.1.17",
          "form": "질병은 모든걸 앗아가 마음까지",
          "original_form": "질병은 모든걸 앗아가 마음까지",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028098.1.18",
          "form": "코로나는 중국때문이야",
          "original_form": "코로나는 중국때문이야",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028098.1.19",
          "form": "누구탓을 해도 소용없어",
          "original_form": "누구탓을 해도 소용없어",
          "speaker_id": "1",
          "time": "20210902 17:38"
        }
      ]
