      "utterance": [
        {
          "id": "MDRW2100010939.1.1",
          "form": "다이어트 콜라 먹어보셧어여?",
          "original_form": "다이어트 콜라 먹어보셧어여?",
          "speaker_id": "1",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100010939.1.2",
          "form": "제로 콜라인가요??",
          "original_form": "제로 콜라인가요??",
          "speaker_id": "2",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100010939.1.3",
          "form": "네..한번 맛보니까..",
          "original_form": "네..한번 맛보니까..",
          "speaker_id": "1",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010939.1.4",
          "form": "오히려 빠져듬.. 살 걱정없는게 더크고..",
          "original_form": "오히려 빠져듬.. 살 걱정없는게 더크고..",
          "speaker_id": "1",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010939.1.5",
          "form": "마른체질이면 아무거나 안가리시겟네여?",
          "original_form": "마른체질이면 아무거나 안가리시겟네여?",
          "speaker_id": "1",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010939.1.6",
          "form": "저 야채 싫어해서 야채는 안 먹어요!",
          "original_form": "저 야채 싫어해서 야채는 안 먹어요!",
          "speaker_id": "2",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010939.1.7",
          "form": "요리할때 아예 안 넣어요?",
          "original_form": "요리할때 아예 안 넣어요?",
          "speaker_id": "1",
          "time": "20210823 23:11"
        },
        {
          "id": "MDRW2100010939.1.8",
          "form": "마라탕 같은 거 먹을 때 안 넣어 먹고",
          "original_form": "마라탕 같은 거 먹을 때 안 넣어 먹고",
          "speaker_id": "2",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100010939.1.9",
          "form": "요리할 때 야채 있으면 빼요!",
          "original_form": "요리할 때 야채 있으면 빼요!",
          "speaker_id": "2",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100010939.1.10",
          "form": "그럼 뭐 시켜먹기 어렵겠네요?",
          "original_form": "그럼 뭐 시켜먹기 어렵겠네요?",
          "speaker_id": "1",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100010939.1.11",
          "form": "야채만 빼면 괜찮아서 의외로 쉬워요!",
          "original_form": "야채만 빼면 괜찮아서 의외로 쉬워요!",
          "speaker_id": "2",
          "time": "20210823 23:14"
        },
        {
          "id": "MDRW2100010939.1.12",
          "form": "어릴때 야채한테 물린적 있나요?",
          "original_form": "어릴때 야채한테 물린적 있나요?",
          "speaker_id": "1",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010939.1.13",
          "form": "왜 야채가 싫은거에여?",
          "original_form": "왜 야채가 싫은거에여?",
          "speaker_id": "1",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010939.1.14",
          "form": "맛이 없잖아요!!",
          "original_form": "맛이 없잖아요!!",
          "speaker_id": "2",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100010939.1.15",
          "form": "파프리카 같은거 엄청 달달하자나여?",
          "original_form": "파프리카 같은거 엄청 달달하자나여?",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100010939.1.16",
          "form": "아.. 그 유전적으로 야채싫어하는 타입인가..",
          "original_form": "아.. 그 유전적으로 야채싫어하는 타입인가..",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100010939.1.17",
          "form": "파프리카 같은 거 냄새가 별로ㅠㅠ",
          "original_form": "파프리카 같은 거 냄새가 별로ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100010939.1.18",
          "form": "좋은 건 아닌 거 같아요ㅠㅠ",
          "original_form": "좋은 건 아닌 거 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 18:16"
        },
        {
          "id": "MDRW2100010939.1.19",
          "form": "그럼 모가 젤 맛잇어요?",
          "original_form": "그럼 모가 젤 맛잇어요?",
          "speaker_id": "1",
          "time": "20210824 18:16"
        },
        {
          "id": "MDRW2100010939.1.20",
          "form": "야채 중에서는 오이가 제일 좋아요!!",
          "original_form": "야채 중에서는 오이가 제일 좋아요!!",
          "speaker_id": "2",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100010939.1.21",
          "form": "흠.. 흥미롭네요.. \r\n보통 오이를 제일 싫어하던데..",
          "original_form": "흠.. 흥미롭네요.. \r\n보통 오이를 제일 싫어하던데..",
          "speaker_id": "1",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100010939.1.22",
          "form": "토마토는요?",
          "original_form": "토마토는요?",
          "speaker_id": "1",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100010939.1.23",
          "form": "토마토는 별로 안 좋아해요!",
          "original_form": "토마토는 별로 안 좋아해요!",
          "speaker_id": "2",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100010939.1.24",
          "form": "그럼 과일은여? 바나나.. 수박.. ㅋㅋㅋ 배 사과",
          "original_form": "그럼 과일은여? 바나나.. 수박.. ㅋㅋㅋ 배 사과",
          "speaker_id": "1",
          "time": "20210824 18:19"
        },
        {
          "id": "MDRW2100010939.1.25",
          "form": "과일은 거의 다 좋아하는 편이에요!",
          "original_form": "과일은 거의 다 좋아하는 편이에요!",
          "speaker_id": "2",
          "time": "20210824 18:20"
        }
      ]
    }
  ]
