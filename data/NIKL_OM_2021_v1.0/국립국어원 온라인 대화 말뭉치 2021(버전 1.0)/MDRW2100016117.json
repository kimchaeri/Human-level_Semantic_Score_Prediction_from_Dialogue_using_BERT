      "utterance": [
        {
          "id": "MDRW2100016117.1.1",
          "form": "오늘 커피 한잔하셨나요",
          "original_form": "오늘 커피 한잔하셨나요",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016117.1.2",
          "form": "아직 못했습니다!! 요즘엔 커피마셔도 위염때문에 반샷만 넣어달라고 하거나 아예 디카페인으로 마시곤해요!!",
          "original_form": "아직 못했습니다!! 요즘엔 커피마셔도 위염때문에 반샷만 넣어달라고 하거나 아예 디카페인으로 마시곤해요!!",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016117.1.3",
          "form": "위염이있어도 땡기더라구용..",
          "original_form": "위염이있어도 땡기더라구용..",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016117.1.4",
          "form": "오늘은 넘 졸려서 이따 점심먹고 한잔 하려구요~!!",
          "original_form": "오늘은 넘 졸려서 이따 점심먹고 한잔 하려구요~!!",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016117.1.5",
          "form": "보통 하루에 커피 몇잔드시나요??",
          "original_form": "보통 하루에 커피 몇잔드시나요??",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016117.1.6",
          "form": "네 식도염에 커피안좋ㅈㅅ",
          "original_form": "네 식도염에 커피안좋ㅈㅅ",
          "speaker_id": "1",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016117.1.7",
          "form": "안좋죠",
          "original_form": "안좋죠",
          "speaker_id": "1",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016117.1.8",
          "form": "저는 지금마시고있어요",
          "original_form": "저는 지금마시고있어요",
          "speaker_id": "1",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016117.1.9",
          "form": "아이 오늘 개학해서보내고..\r\n너무 잠이쏟아져서요",
          "original_form": "아이 오늘 개학해서보내고..\r\n너무 잠이쏟아져서요",
          "speaker_id": "1",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016117.1.10",
          "form": "하루 한잔 마시려고 노력합니다",
          "original_form": "하루 한잔 마시려고 노력합니다",
          "speaker_id": "1",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100016117.1.11",
          "form": "오 뭔가 애들보내고 여유로운시간에 커피한잔..캬..좋을거같아요!!",
          "original_form": "오 뭔가 애들보내고 여유로운시간에 커피한잔..캬..좋을거같아요!!",
          "speaker_id": "2",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100016117.1.12",
          "form": "네 금방왔어요 12시20분 하교요 ㅜ",
          "original_form": "네 금방왔어요 12시20분 하교요 ㅜ",
          "speaker_id": "1",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100016117.1.13",
          "form": "어머...더있다가 좀 오지...ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어머...더있다가 좀 오지...ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016117.1.14",
          "form": "단축수업해요 코로나덕에 ㅜ.ㅜ",
          "original_form": "단축수업해요 코로나덕에 ㅜ.ㅜ",
          "speaker_id": "1",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016117.1.15",
          "form": "아 단축수업이구나 아이고ㅠㅠ 애들은 편식안하고 음식 잘먹나요?!",
          "original_form": "아 단축수업이구나 아이고ㅠㅠ 애들은 편식안하고 음식 잘먹나요?!",
          "speaker_id": "2",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016117.1.16",
          "form": "네 워낙잘먹는아이긴한데..\r\n과일을 가려요\r\n베리류를 질색해여",
          "original_form": "네 워낙잘먹는아이긴한데..\r\n과일을 가려요\r\n베리류를 질색해여",
          "speaker_id": "1",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016117.1.17",
          "form": "그래도 그정도면 많이 가리는 편은 아니네요!!!",
          "original_form": "그래도 그정도면 많이 가리는 편은 아니네요!!!",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016117.1.18",
          "form": "요즘 과일은 포도랑 복숭아 많이먹구있네요 ㅋㅋㅋㅋ",
          "original_form": "요즘 과일은 포도랑 복숭아 많이먹구있네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016117.1.19",
          "form": "어떤 과일 좋아하시나욬ㅋㅋ",
          "original_form": "어떤 과일 좋아하시나욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016117.1.20",
          "form": "아들이 복숭아 포도도 안먹네요 ㅋ",
          "original_form": "아들이 복숭아 포도도 안먹네요 ㅋ",
          "speaker_id": "1",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016117.1.21",
          "form": "저는 복숭아 참외 좋아해요",
          "original_form": "저는 복숭아 참외 좋아해요",
          "speaker_id": "1",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016117.1.22",
          "form": "아니 그 맛있는걸 !!! ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니 그 맛있는걸 !!! ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016117.1.23",
          "form": "그죠 딸기를 가장 혐오해요 ㅠ",
          "original_form": "그죠 딸기를 가장 혐오해요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016117.1.24",
          "form": "왤까요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "왤까요 ㅋㅋㅋㅋㅋㅋ",
