        },
        {
          "id": "MDRW2100046858.1.3",
          "form": "운동 좋아하시나용",
          "original_form": "운동 좋아하시나용",
          "speaker_id": "2",
          "time": "20211105 15:31"
        },
        {
          "id": "MDRW2100046858.1.4",
          "form": "숨만 쉬어요",
          "original_form": "숨만 쉬어요",
          "speaker_id": "1",
          "time": "20211105 15:31"
        },
        {
          "id": "MDRW2100046858.1.5",
          "form": "몸이 안 따라줌 ㅠ",
          "original_form": "몸이 안 따라줌 ㅠ",
          "speaker_id": "1",
          "time": "20211105 15:31"
        },
        {
          "id": "MDRW2100046858.1.6",
          "form": "앗 ㅎ",
          "original_form": "앗 ㅎ",
          "speaker_id": "2",
          "time": "20211105 15:31"
        },
        {
          "id": "MDRW2100046858.1.7",
          "form": "그럼 보시는건 좋아하시나용",
          "original_form": "그럼 보시는건 좋아하시나용",
          "speaker_id": "2",
          "time": "20211105 15:31"
        },
        {
          "id": "MDRW2100046858.1.8",
          "form": "축구란 야구 좋아해요",
          "original_form": "축구란 야구 좋아해요",
          "speaker_id": "1",
          "time": "20211105 15:32"
        },
        {
          "id": "MDRW2100046858.1.9",
          "form": "옹 해외 경기들 보시나요??",
          "original_form": "옹 해외 경기들 보시나요??",
          "speaker_id": "2",
          "time": "20211105 15:33"
        },
        {
          "id": "MDRW2100046858.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211105 15:33"
        },
        {
          "id": "MDRW2100046858.1.11",
          "form": "해외만 봄",
          "original_form": "해외만 봄",
          "speaker_id": "1",
          "time": "20211105 15:33"
        },
        {
          "id": "MDRW2100046858.1.12",
          "form": "좋아하는 팀 있으세요??",
          "original_form": "좋아하는 팀 있으세요??",
          "speaker_id": "2",
          "time": "20211105 15:33"
        },
        {
