      "utterance": [
        {
          "id": "MDRW2100028174.1.1",
          "form": "까마귀 공주풍이라고 하길래 여자아이돌인가 싶었으",
          "original_form": "까마귀 공주풍이라고 하길래 여자아이돌인가 싶었으",
          "speaker_id": "1",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100028174.1.2",
          "form": "하지만...걍 걔는 공주같아서 공주라고해",
          "original_form": "하지만...걍 걔는 공주같아서 공주라고해",
          "speaker_id": "2",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100028174.1.3",
          "form": "원어스면 남돌이 쓸만한 팀명이라고 생각했는데",
          "original_form": "원어스면 남돌이 쓸만한 팀명이라고 생각했는데",
          "speaker_id": "1",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100028174.1.4",
          "form": "역시 남돌이었군",
          "original_form": "역시 남돌이었군",
          "speaker_id": "1",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100028174.1.5",
          "form": "걔네 형제 그룹이 원위인데",
          "original_form": "걔네 형제 그룹이 원위인데",
          "speaker_id": "2",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100028174.1.6",
          "form": "밴드노래라서 좋아",
          "original_form": "밴드노래라서 좋아",
          "speaker_id": "2",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100028174.1.7",
          "form": "형제그룹도 있구나 신기하다",
          "original_form": "형제그룹도 있구나 신기하다",
          "speaker_id": "1",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100028174.1.8",
          "form": "가사가 좋음",
          "original_form": "가사가 좋음",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028174.1.9",
          "form": "난 아이돌쪽은 진짜 문외한이라",
          "original_form": "난 아이돌쪽은 진짜 문외한이라",
          "speaker_id": "1",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028174.1.10",
          "form": "Kpop도 안팠구",
          "original_form": "Kpop도 안팠구",
          "speaker_id": "1",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028174.1.11",
          "form": "뭔가 소속사가 컨셉 그렇게 잡은듯",
          "original_form": "뭔가 소속사가 컨셉 그렇게 잡은듯",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028174.1.12",
          "form": "마치 나 혼자 대한민국이 아닌 기분",
          "original_form": "마치 나 혼자 대한민국이 아닌 기분",
          "speaker_id": "1",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028174.1.13",
          "form": "합쳐서 위어스",
          "original_form": "합쳐서 위어스",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028174.1.14",
          "form": "컨셉 좋다",
          "original_form": "컨셉 좋다",
          "speaker_id": "1",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028174.1.15",
          "form": "근데 실제로도 쌍둥이 있음",
          "original_form": "근데 실제로도 쌍둥이 있음",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028174.1.16",
          "form": "오?",
          "original_form": "오?",
