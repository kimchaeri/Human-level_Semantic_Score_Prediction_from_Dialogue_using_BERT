        {
          "id": "MDRW2100043099.1.1",
          "form": "운동하는거있으세요",
          "original_form": "운동하는거있으세요",
          "speaker_id": "1",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100043099.1.2",
          "form": "저 코로나땜에 어디 다니진못하구 요즘은 집에서 홈트해요!!",
          "original_form": "저 코로나땜에 어디 다니진못하구 요즘은 집에서 홈트해요!!",
          "speaker_id": "2",
          "time": "20210907 14:16"
        },
        {
          "id": "MDRW2100043099.1.3",
          "form": "거창하게 하는건 아니지만...",
          "original_form": "거창하게 하는건 아니지만...",
          "speaker_id": "2",
          "time": "20210907 14:16"
        },
        {
          "id": "MDRW2100043099.1.4",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "1",
          "time": "20210907 14:18"
        },
        {
          "id": "MDRW2100043099.1.5",
          "form": "홈트어떤거하세요",
          "original_form": "홈트어떤거하세요",
          "speaker_id": "1",
          "time": "20210907 14:18"
        },
        {
          "id": "MDRW2100043099.1.6",
          "form": "ㅋㅋㅋㅋㅋ그냥 유튜브 틀어놓고 따라하거나..",
          "original_form": "ㅋㅋㅋㅋㅋ그냥 유튜브 틀어놓고 따라하거나..",
          "speaker_id": "2",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100043099.1.7",
          "form": "요즘은 그마저도 귀찮아서 그냥 집에있는 사이클 타요 ㅋㅋㅋㅋ",
          "original_form": "요즘은 그마저도 귀찮아서 그냥 집에있는 사이클 타요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100043099.1.8",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100043099.1.9",
          "form": "저도ㅎㅎ유투브에플랭크영상있는데",
          "original_form": "저도ㅎㅎ유투브에플랭크영상있는데",
          "speaker_id": "1",
          "time": "20210907 14:20"
        },
        {
          "id": "MDRW2100043099.1.10",
          "form": "그거10분짜리만매트피고해요ㅎㅎ",
          "original_form": "그거10분짜리만매트피고해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:20"
        },
        {
          "id": "MDRW2100043099.1.11",
          "form": "헉 플랭크 10분 내리하세요!??!",
          "original_form": "헉 플랭크 10분 내리하세요!??!",
          "speaker_id": "2",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100043099.1.12",
          "form": "1분씩끊어서10분해요ㅎㅎ",
          "original_form": "1분씩끊어서10분해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:28"
        },
        {
          "id": "MDRW2100043099.1.13",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100043099.1.14",
          "form": "아 근데 그거 좋은거같아요 ㅋㅋㅋ플랭크 혼자하면 맨날 30초하고 귀찮아서 그만두는데..",
          "original_form": "아 근데 그거 좋은거같아요 ㅋㅋㅋ플랭크 혼자하면 맨날 30초하고 귀찮아서 그만두는데..",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043099.1.15",
          "form": "저도 다음에 할때는 한번 그런거 찾아보고 해야겠어요  ㅋㅋㅋㅋ",
          "original_form": "저도 다음에 할때는 한번 그런거 찾아보고 해야겠어요  ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043099.1.16",
          "form": "싸이클만 타니까 근력운동이 부족해서 ㅋㅋㅋㅋ",
          "original_form": "싸이클만 타니까 근력운동이 부족해서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043099.1.17",
          "form": "유투브에 미서원 플랭크치면 나오는데 그거해요ㅎㅎ",
          "original_form": "유투브에 미서원 플랭크치면 나오는데 그거해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:31"
        },
        {
          "id": "MDRW2100043099.1.18",
          "form": "헉 미서원님 유명하죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "헉 미서원님 유명하죠 ㅋㅋㅋㅋㅋㅋ",
