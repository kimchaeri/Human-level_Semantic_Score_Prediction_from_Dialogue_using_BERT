      "utterance": [
        {
          "id": "MDRW2100013889.1.1",
          "form": "요새는 다들 재택하더라고요",
          "original_form": "요새는 다들 재택하더라고요",
          "speaker_id": "1",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013889.1.2",
          "form": "앞으로 일자리가 늘까요?",
          "original_form": "앞으로 일자리가 늘까요?",
          "speaker_id": "1",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013889.1.3",
          "form": "늘어나야할텐데 줄어들거같아요 ㅠㅠ 경기도 너무 어렵고",
          "original_form": "늘어나야할텐데 줄어들거같아요 ㅠㅠ 경기도 너무 어렵고",
          "speaker_id": "2",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013889.1.4",
          "form": "안돼 ㅠ 부자돼야 되는데",
          "original_form": "안돼 ㅠ 부자돼야 되는데",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013889.1.5",
          "form": "다들어려워요",
          "original_form": "다들어려워요",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013889.1.6",
          "form": "맞아요ㅠㅠ 돈많은 백수가 꿈인데 ㅠㅠㅠ 로또아니면 불가능해보여요",
          "original_form": "맞아요ㅠㅠ 돈많은 백수가 꿈인데 ㅠㅠㅠ 로또아니면 불가능해보여요",
          "speaker_id": "2",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013889.1.7",
          "form": "아휴 로또 살돈도 아까워요",
          "original_form": "아휴 로또 살돈도 아까워요",
          "speaker_id": "1",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013889.1.8",
          "form": "ㅋㅋㅋ맞아요 사지도 않으면서 1등을 꿈꾸는...",
          "original_form": "ㅋㅋㅋ맞아요 사지도 않으면서 1등을 꿈꾸는...",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013889.1.9",
          "form": "로또 돼면 뭐할거예요",
          "original_form": "로또 돼면 뭐할거예요",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013889.1.10",
          "form": "ㅋㅋㅋ음 일단 건물을 사야하지않을까요?",
          "original_form": "ㅋㅋㅋ음 일단 건물을 사야하지않을까요?",
          "speaker_id": "2",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013889.1.11",
          "form": "name1은 계획이 어떻게 되나요?",
          "original_form": "&name1&은 계획이 어떻게 되나요?",
          "speaker_id": "2",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013889.1.12",
          "form": "저는 집사고 싶어요",
          "original_form": "저는 집사고 싶어요",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013889.1.13",
          "form": "작은거라도",
          "original_form": "작은거라도",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013889.1.14",
          "form": "맞아요 일단 내 집 마련의 꿈 ㅠㅠ",
          "original_form": "맞아요 일단 내 집 마련의 꿈 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013889.1.15",
          "form": "집값이 너무 올라서 ㅜ",
          "original_form": "집값이 너무 올라서 ㅜ",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013889.1.16",
          "form": "정말 무서울정도에요",
          "original_form": "정말 무서울정도에요",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013889.1.17",
          "form": "못살거같아요",
          "original_form": "못살거같아요",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013889.1.18",
          "form": "그러니까요 집 한 채 언제쯤...ㅠㅠ",
          "original_form": "그러니까요 집 한 채 언제쯤...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013889.1.19",
