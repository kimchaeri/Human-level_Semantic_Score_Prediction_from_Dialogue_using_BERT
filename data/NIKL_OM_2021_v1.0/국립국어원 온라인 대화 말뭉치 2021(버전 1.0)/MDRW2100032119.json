      "utterance": [
        {
          "id": "MDRW2100032119.1.1",
          "form": "대출플렉스해야죠",
          "original_form": "대출플렉스해야죠",
          "speaker_id": "2",
          "time": "20210905 12:43"
        },
        {
          "id": "MDRW2100032119.1.2",
          "form": "이럴때 쓰는거라고하드라구요",
          "original_form": "이럴때 쓰는거라고하드라구요",
          "speaker_id": "2",
          "time": "20210905 12:43"
        },
        {
          "id": "MDRW2100032119.1.3",
          "form": "ㅋㅋㅋㄱ대출플렉스",
          "original_form": "ㅋㅋㅋㄱ대출플렉스",
          "speaker_id": "1",
          "time": "20210905 12:43"
        },
        {
          "id": "MDRW2100032119.1.4",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "1",
          "time": "20210905 12:44"
        },
        {
          "id": "MDRW2100032119.1.5",
          "form": "저미리대출일부상환했는데",
          "original_form": "저미리대출일부상환했는데",
          "speaker_id": "1",
          "time": "20210905 12:44"
        },
        {
          "id": "MDRW2100032119.1.6",
          "form": "후회중ㅋㅋ",
          "original_form": "후회중ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:44"
        },
        {
          "id": "MDRW2100032119.1.7",
          "form": "ㅠㅠ 왜그러셨어요",
          "original_form": "ㅠㅠ 왜그러셨어요",
          "speaker_id": "2",
          "time": "20210905 12:44"
        },
        {
          "id": "MDRW2100032119.1.8",
          "form": "대출은 갚는거아니래요",
          "original_form": "대출은 갚는거아니래요",
          "speaker_id": "2",
          "time": "20210905 12:45"
        },
        {
          "id": "MDRW2100032119.1.9",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210905 12:45"
        },
        {
          "id": "MDRW2100032119.1.10",
          "form": "늘후회하며삽니다..",
          "original_form": "늘후회하며삽니다..",
          "speaker_id": "1",
          "time": "20210905 12:45"
        },
        {
          "id": "MDRW2100032119.1.11",
          "form": "재난지원금나와서그나마다행이예요ㅋㅋ",
          "original_form": "재난지원금나와서그나마다행이예요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:45"
        },
        {
          "id": "MDRW2100032119.1.12",
          "form": "없었음 큰일날뻔",
          "original_form": "없었음 큰일날뻔",
          "speaker_id": "2",
          "time": "20210905 12:45"
        },
        {
          "id": "MDRW2100032119.1.13",
          "form": "저도여윳돈으로  신랑마통갚아주고",
          "original_form": "저도여윳돈으로  신랑마통갚아주고",
          "speaker_id": "2",
          "time": "20210905 12:46"
        },
        {
          "id": "MDRW2100032119.1.14",
          "form": "필요할때다시받기로했는데",
          "original_form": "필요할때다시받기로했는데",
          "speaker_id": "2",
          "time": "20210905 12:46"
        },
        {
          "id": "MDRW2100032119.1.15",
          "form": "큰집이사가시면",
          "original_form": "큰집이사가시면",
          "speaker_id": "1",
          "time": "20210905 12:46"
        },
        {
          "id": "MDRW2100032119.1.16",
          "form": "안줄라고해요너무짜증나요",
          "original_form": "안줄라고해요너무짜증나요",
          "speaker_id": "2",
          "time": "20210905 12:46"
        },
        {
          "id": "MDRW2100032119.1.17",
          "form": "뭐사실예정이예요?",
          "original_form": "뭐사실예정이예요?",
          "speaker_id": "1",
          "time": "20210905 12:46"
        },
        {
          "id": "MDRW2100032119.1.18",
          "form": "사실 저는 쫄보라서",
          "original_form": "사실 저는 쫄보라서",
          "speaker_id": "2",
          "time": "20210905 12:46"
        },
        {
          "id": "MDRW2100032119.1.19",
          "form": "ㅋㅋ마통요즘잘안나온다니갖고계세요",
          "original_form": "ㅋㅋ마통요즘잘안나온다니갖고계세요",
          "speaker_id": "1",
          "time": "20210905 12:46"
        },
        {
          "id": "MDRW2100032119.1.20",
          "form": "가전은일단다가져가서 망가질때까지는쓸꺼같아요",
          "original_form": "가전은일단다가져가서 망가질때까지는쓸꺼같아요",
          "speaker_id": "2",
          "time": "20210905 12:46"
        },
        {
          "id": "MDRW2100032119.1.21",
          "form": "ㅋㅋ역시미니멀라이프",
          "original_form": "ㅋㅋ역시미니멀라이프",
          "speaker_id": "1",
          "time": "20210905 12:47"
        },
        {
          "id": "MDRW2100032119.1.22",
          "form": "식탁은 사야할듯 하구요",
          "original_form": "식탁은 사야할듯 하구요",
          "speaker_id": "2",
          "time": "20210905 12:47"
        },
        {
          "id": "MDRW2100032119.1.23",
          "form": "린클이라고 음식물처리기가 있어요",
          "original_form": "린클이라고 음식물처리기가 있어요",
          "speaker_id": "2",
          "time": "20210905 12:47"
        },
        {
          "id": "MDRW2100032119.1.24",
          "form": "부업친구소개로 알게됐는데",
          "original_form": "부업친구소개로 알게됐는데",
          "speaker_id": "2",
          "time": "20210905 12:47"
        },
        {
          "id": "MDRW2100032119.1.25",
          "form": "오ㅋㅋ뭔지알아요",
          "original_form": "오ㅋㅋ뭔지알아요",
          "speaker_id": "1",
          "time": "20210905 12:47"
        },
        {
          "id": "MDRW2100032119.1.26",
          "form": "린클 부러워요",
          "original_form": "린클 부러워요",
          "speaker_id": "1",
