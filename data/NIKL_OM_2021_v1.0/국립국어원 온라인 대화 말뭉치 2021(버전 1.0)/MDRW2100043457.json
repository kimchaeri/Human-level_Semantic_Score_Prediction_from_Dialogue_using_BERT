      "utterance": [
        {
          "id": "MDRW2100043457.1.1",
          "form": "마자요 연기를 너무 자연스럽게 잘해요",
          "original_form": "마자요 연기를 너무 자연스럽게 잘해요",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043457.1.2",
          "form": "네 진짜 생활연기 장인이에요ㅋㅋ",
          "original_form": "네 진짜 생활연기 장인이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043457.1.3",
          "form": "다시 듣는 지난 노래도 좋죠?",
          "original_form": "다시 듣는 지난 노래도 좋죠?",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043457.1.4",
          "form": "네 진짜 장면 하나하나 빼놓을게 없는 드라마같아요",
          "original_form": "네 진짜 장면 하나하나 빼놓을게 없는 드라마같아요",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043457.1.5",
          "form": "예능도 좋아하세요?",
          "original_form": "예능도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043457.1.6",
          "form": "전 놀토는 챙겨봐요",
          "original_form": "전 놀토는 챙겨봐요",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043457.1.7",
          "form": "저도요~재방이라도 꼭보죠",
          "original_form": "저도요~재방이라도 꼭보죠",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043457.1.8",
          "form": "유퀴즈도 좋아하고요",
          "original_form": "유퀴즈도 좋아하고요",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043457.1.9",
          "form": "아 저도 그거 좋아해요",
          "original_form": "아 저도 그거 좋아해요",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043457.1.10",
          "form": "저랑 취향이 비슷하신것같아요",
          "original_form": "저랑 취향이 비슷하신것같아요",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043457.1.11",
          "form": "그렇네요 ㅋㅋㅋ",
          "original_form": "그렇네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043457.1.12",
          "form": "그프로그램은 호불호가 별로 없긴하죠?",
          "original_form": "그프로그램은 호불호가 별로 없긴하죠?",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043457.1.13",
          "form": "유재석의 진행도 좋아하긴해요",
          "original_form": "유재석의 진행도 좋아하긴해요",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043457.1.14",
          "form": "그건 그래요 그래서 국민엠씨ㅋㅋ",
          "original_form": "그건 그래요 그래서 국민엠씨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043457.1.15",
          "form": "요즘 재밋는거 진짜 많죠",
          "original_form": "요즘 재밋는거 진짜 많죠",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043457.1.16",
          "form": "드라마도그렇고 예능도 참 잘만드는것같아요",
          "original_form": "드라마도그렇고 예능도 참 잘만드는것같아요",
