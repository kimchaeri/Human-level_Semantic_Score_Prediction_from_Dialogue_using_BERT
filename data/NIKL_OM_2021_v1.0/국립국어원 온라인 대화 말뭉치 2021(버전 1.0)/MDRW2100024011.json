      "utterance": [
        {
          "id": "MDRW2100024011.1.1",
          "form": "저 공무원하고 싶어서~",
          "original_form": "저 공무원하고 싶어서~",
          "speaker_id": "2",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100024011.1.2",
          "form": "휴학하고 준비하려구요~~",
          "original_form": "휴학하고 준비하려구요~~",
          "speaker_id": "2",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100024011.1.3",
          "form": "학교다니면서 준비하면",
          "original_form": "학교다니면서 준비하면",
          "speaker_id": "2",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100024011.1.4",
          "form": "좀 힘들것 같아서 .. ㅋㅋㅋㅎㅎ",
          "original_form": "좀 힘들것 같아서 .. ㅋㅋㅋㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100024011.1.5",
          "form": "맞아요 학교다니면서 준비하기엔 무리가 있죠",
          "original_form": "맞아요 학교다니면서 준비하기엔 무리가 있죠",
          "speaker_id": "1",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100024011.1.6",
          "form": "그럼 영어 잘하시나요??",
          "original_form": "그럼 영어 잘하시나요??",
          "speaker_id": "1",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100024011.1.7",
          "form": "ㅋㅋㅋㅋ ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100024011.1.8",
          "form": "아뇨 ㅠㅠ",
          "original_form": "아뇨 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100024011.1.9",
          "form": "영어는",
          "original_form": "영어는",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100024011.1.10",
          "form": "지금부터 하고 있어요!",
          "original_form": "지금부터 하고 있어요!",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100024011.1.11",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100024011.1.12",
          "form": "영어 안보는 특채 있어서 ㅎㅎ",
          "original_form": "영어 안보는 특채 있어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100024011.1.13",
          "form": "그것도 준비하고 있습니당~",
          "original_form": "그것도 준비하고 있습니당~",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100024011.1.14",
          "form": "그렇군요 이번에 전공 몇 개 들으시나요?",
          "original_form": "그렇군요 이번에 전공 몇 개 들으시나요?",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024011.1.15",
          "form": "이번에",
          "original_form": "이번에",
          "speaker_id": "2",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024011.1.16",
          "form": "전공이랑 부전공 합쳐서",
          "original_form": "전공이랑 부전공 합쳐서",
          "speaker_id": "2",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024011.1.17",
          "form": "5전공 들어요!",
          "original_form": "5전공 들어요!",
          "speaker_id": "2",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024011.1.18",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024011.1.19",
          "form": "저도 5전공이요 다전공자이신건가요??",
          "original_form": "저도 5전공이요 다전공자이신건가요??",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024011.1.20",
          "form": "네 ! 부전공 듣고 있어요~",
          "original_form": "네 ! 부전공 듣고 있어요~",
          "speaker_id": "2",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100024011.1.21",
          "form": "안힘드신가요??",
          "original_form": "안힘드신가요??",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100024011.1.22",
          "form": "좀 힘들긴한데..ㅋㅋㅋ",
          "original_form": "좀 힘들긴한데..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100024011.1.23",
          "form": "관심있는 분야라서 재미있어요!!",
          "original_form": "관심있는 분야라서 재미있어요!!",
          "speaker_id": "2",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100024011.1.24",
          "form": "그래서 후회는 안해요!ㅋㅋㅋㅋ",
          "original_form": "그래서 후회는 안해요!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100024011.1.25",
          "form": "우와 언제 부전공 신청하신건가요?",
          "original_form": "우와 언제 부전공 신청하신건가요?",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024011.1.26",
          "form": "2학년 시작할 때인가?? 그 때 신청했어요!",
          "original_form": "2학년 시작할 때인가?? 그 때 신청했어요!",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024011.1.27",
          "form": "새학기여서",
          "original_form": "새학기여서",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024011.1.28",
          "form": "의지가 넘칠 때",
          "original_form": "의지가 넘칠 때",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024011.1.29",
          "form": "신청해버렸어요 .. ㅋㅋㅋㅋㅋ",
          "original_form": "신청해버렸어요 .. ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024011.1.30",
          "form": "그래도 진정한 대학생이시네요 근데 부전공 신청하면 등록금은 어떻게 선정되나요??",
          "original_form": "그래도 진정한 대학생이시네요 근데 부전공 신청하면 등록금은 어떻게 선정되나요??",
          "speaker_id": "1",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100024011.1.31",
          "form": "저희 학교는 부전공을 신청해도",
          "original_form": "저희 학교는 부전공을 신청해도",
          "speaker_id": "2",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024011.1.32",
          "form": "등록금은 똑같이 나가요!",
          "original_form": "등록금은 똑같이 나가요!",
          "speaker_id": "2",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024011.1.33",
          "form": "들을 수 있는 학점이 같아서 그런 것 같아요!",
          "original_form": "들을 수 있는 학점이 같아서 그런 것 같아요!",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024011.1.34",
          "form": "오 그렇군요!!",
