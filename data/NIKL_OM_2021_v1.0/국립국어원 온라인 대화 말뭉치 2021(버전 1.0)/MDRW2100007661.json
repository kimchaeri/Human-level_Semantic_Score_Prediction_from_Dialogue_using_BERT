      "utterance": [
        {
          "id": "MDRW2100007661.1.1",
          "form": "어제 비온게 꿈이였나",
          "original_form": "어제 비온게 꿈이였나",
          "speaker_id": "2",
          "time": "20210822 09:10"
        },
        {
          "id": "MDRW2100007661.1.2",
          "form": "뭔 날이 확개고 좋네",
          "original_form": "뭔 날이 확개고 좋네",
          "speaker_id": "2",
          "time": "20210822 09:11"
        },
        {
          "id": "MDRW2100007661.1.3",
          "form": "요새 날씨가 진짜ㅋㅋㅋ특이한거 같아ㅋㅋㅋ",
          "original_form": "요새 날씨가 진짜ㅋㅋㅋ특이한거 같아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:17"
        },
        {
          "id": "MDRW2100007661.1.4",
          "form": "맞아 기후가 바꼈대",
          "original_form": "맞아 기후가 바꼈대",
          "speaker_id": "2",
          "time": "20210822 09:32"
        },
        {
          "id": "MDRW2100007661.1.5",
          "form": "아습해",
          "original_form": "아습해",
          "speaker_id": "2",
          "time": "20210822 09:32"
        },
        {
          "id": "MDRW2100007661.1.6",
          "form": "나 머리카락이 곱슬됐다 습해서",
          "original_form": "나 머리카락이 곱슬됐다 습해서",
          "speaker_id": "2",
          "time": "20210822 09:33"
        },
        {
          "id": "MDRW2100007661.1.7",
          "form": "웃겨",
          "original_form": "웃겨",
          "speaker_id": "2",
          "time": "20210822 09:33"
        },
        {
          "id": "MDRW2100007661.1.8",
          "form": "습기에 머리카락이 민감해",
          "original_form": "습기에 머리카락이 민감해",
          "speaker_id": "2",
          "time": "20210822 09:34"
        },
        {
          "id": "MDRW2100007661.1.9",
          "form": "그나마 미세먼지 없어 좋다",
          "original_form": "그나마 미세먼지 없어 좋다",
          "speaker_id": "2",
          "time": "20210822 09:34"
        },
        {
          "id": "MDRW2100007661.1.10",
          "form": "손선해서 놀러가고싶네",
          "original_form": "손선해서 놀러가고싶네",
          "speaker_id": "2",
          "time": "20210822 09:40"
        },
        {
          "id": "MDRW2100007661.1.11",
          "form": "선선",
          "original_form": "선선",
          "speaker_id": "2",
          "time": "20210822 09:41"
        },
        {
          "id": "MDRW2100007661.1.12",
          "form": "온난화 때문이겠지ㅋㅋㅋㅋㅋ",
          "original_form": "온난화 때문이겠지ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:09"
        },
        {
          "id": "MDRW2100007661.1.13",
          "form": "맞아 날씨ㅋㅋㅋ습해지면 나도 머리 꼬부랑탱이야",
          "original_form": "맞아 날씨ㅋㅋㅋ습해지면 나도 머리 꼬부랑탱이야",
          "speaker_id": "1",
          "time": "20210822 11:09"
        },
        {
          "id": "MDRW2100007661.1.14",
          "form": "ㅋㅋ똑같네",
          "original_form": "ㅋㅋ똑같네",
          "speaker_id": "2",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007661.1.15",
          "form": "요리해서 먹었더니 덥네",
          "original_form": "요리해서 먹었더니 덥네",
          "speaker_id": "2",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007661.1.16",
          "form": "역시 낮은 뭐하면 아직 더워",
          "original_form": "역시 낮은 뭐하면 아직 더워",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007661.1.17",
          "form": "이러다 날씨가 넘 변해서 계절 사라질까 갑나네",
          "original_form": "이러다 날씨가 넘 변해서 계절 사라질까 갑나네",
          "speaker_id": "2",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007661.1.18",
          "form": "요새 날씨가 좀 그렇더라곸ㅋㅋ밤에 엄청 더운건 아닌데",
          "original_form": "요새 날씨가 좀 그렇더라곸ㅋㅋ밤에 엄청 더운건 아닌데",
          "speaker_id": "1",
          "time": "20210822 12:46"
        },
        {
          "id": "MDRW2100007661.1.19",
          "form": "선풍기 없에 자기는 뭔가 아쉬운?",
          "original_form": "선풍기 없에 자기는 뭔가 아쉬운?",
          "speaker_id": "1",
          "time": "20210822 12:46"
        },
        {
          "id": "MDRW2100007661.1.20",
          "form": "습해서 그런가",
          "original_form": "습해서 그런가",
          "speaker_id": "1",
          "time": "20210822 12:47"
        },
        {
          "id": "MDRW2100007661.1.21",
          "form": "그렇죠 습기가 문제야",
          "original_form": "그렇죠 습기가 문제야",
          "speaker_id": "2",
          "time": "20210822 12:47"
        },
        {
          "id": "MDRW2100007661.1.22",
          "form": "아 맨날 찜찜해",
          "original_form": "아 맨날 찜찜해",
          "speaker_id": "2",
          "time": "20210822 12:47"
        },
        {
          "id": "MDRW2100007661.1.23",
          "form": "이것도 이제 얼마 안남았겠지 :)",
          "original_form": "이것도 이제 얼마 안남았겠지 :)",
          "speaker_id": "1",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100007661.1.24",
          "form": "가을이 다가온다!!",
          "original_form": "가을이 다가온다!!",
          "speaker_id": "1",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100007661.1.25",
          "form": "사람들 없을때 하루 휴가내고 등산하면 딱 좋을듯",
          "original_form": "사람들 없을때 하루 휴가내고 등산하면 딱 좋을듯",
          "speaker_id": "1",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100007661.1.26",
          "form": "그럼 옷사야해",
          "original_form": "그럼 옷사야해",
          "speaker_id": "2",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100007661.1.27",
          "form": "여름 다갔어",
          "original_form": "여름 다갔어",
          "speaker_id": "2",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100007661.1.28",
          "form": "하루 휴가내서 산에가봐 요새 좀 덜더워서 괜찮을듯",
          "original_form": "하루 휴가내서 산에가봐 요새 좀 덜더워서 괜찮을듯",
          "speaker_id": "2",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100007661.1.29",
          "form": "등신복은 언니꺼로 많아ㅋㅋㅋ",
          "original_form": "등신복은 언니꺼로 많아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100007661.1.30",
          "form": "계절별로 몇벌씩 있거든ㅋㅋㅋ",
          "original_form": "계절별로 몇벌씩 있거든ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100007661.1.31",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100007661.1.32",
          "form": "언니가 많이 샀구나",
          "original_form": "언니가 많이 샀구나",
          "speaker_id": "2",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100007661.1.33",
          "form": "난 산은 ㅜㅜ못감",
          "original_form": "난 산은 ㅜㅜ못감",
          "speaker_id": "2",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100007661.1.34",
          "form": "시원하니 도전해봐",
          "original_form": "시원하니 도전해봐",
          "speaker_id": "2",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100007661.1.35",
          "form": "왜 못가!",
          "original_form": "왜 못가!",
          "speaker_id": "1",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100007661.1.36",
          "form": "힘들어 ㅋㅋ",
          "original_form": "힘들어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100007661.1.37",
          "form": "등산복 풀장착 도전해 ㅎㅎ",
          "original_form": "등산복 풀장착 도전해 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:53"
        }
      ]
