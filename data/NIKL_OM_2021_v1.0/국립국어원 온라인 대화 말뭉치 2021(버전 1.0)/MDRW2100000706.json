      "utterance": [
        {
          "id": "MDRW2100000706.1.1",
          "form": "name3 오늘 쿠팡안간데?",
          "original_form": "&name3& 오늘 쿠팡안간데?",
          "speaker_id": "1",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000706.1.2",
          "form": "잠이와서 안간데~",
          "original_form": "잠이와서 안간데~",
          "speaker_id": "2",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000706.1.3",
          "form": "알바를 지맘대로 다니니...",
          "original_form": "알바를 지맘대로 다니니...",
          "speaker_id": "1",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000706.1.4",
          "form": "그래도 대견하다. 언제커서 알바도 다 다니고",
          "original_form": "그래도 대견하다. 언제커서 알바도 다 다니고",
          "speaker_id": "2",
          "time": "20210819 17:51"
        },
        {
          "id": "MDRW2100000706.1.5",
          "form": "그건그래.",
          "original_form": "그건그래.",
          "speaker_id": "1",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000706.1.6",
          "form": "울신랑도 딸램따라서 야간알바좀 가시지?",
          "original_form": "울신랑도 딸램따라서 야간알바좀 가시지?",
          "speaker_id": "1",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000706.1.7",
          "form": "너나가라! 난 주식해서 돈벌고 일해서도 벌고있잖아",
          "original_form": "너나가라! 난 주식해서 돈벌고 일해서도 벌고있잖아",
          "speaker_id": "2",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000706.1.8",
          "form": "야간시간을 알차게써야지~~~ 가장이잘시간이 어디있어",
          "original_form": "야간시간을 알차게써야지~~~ 가장이잘시간이 어디있어",
          "speaker_id": "1",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000706.1.9",
          "form": "ㅎㅎ 넌 뭐하고?",
          "original_form": "ㅎㅎ 넌 뭐하고?",
          "speaker_id": "2",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000706.1.10",
          "form": "나야 밥하고 청소^^ 도 가끔하고 놀지~~~",
          "original_form": "나야 밥하고 청소^^ 도 가끔하고 놀지~~~",
          "speaker_id": "1",
          "time": "20210819 17:54"
        },
        {
          "id": "MDRW2100000706.1.11",
          "form": "나중에 같이가면 생각해볼께",
          "original_form": "나중에 같이가면 생각해볼께",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000706.1.12",
          "form": "그래 나도 나~~~~중에 용기를 내어볼께",
          "original_form": "그래 나도 나~~~~중에 용기를 내어볼께",
          "speaker_id": "1",
          "time": "20210819 17:56"
        },
        {
          "id": "MDRW2100000706.1.13",
          "form": "그래. 이제 애들도 다 컷으니 열심히 일해서 노후를 준비해야지",
          "original_form": "그래. 이제 애들도 다 컷으니 열심히 일해서 노후를 준비해야지",
          "speaker_id": "2",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100000706.1.14",
          "form": "응. 그래야지 애들한테 짐되지않게",
          "original_form": "응. 그래야지 애들한테 짐되지않게",
          "speaker_id": "1",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100000706.1.15",
          "form": "결혼도시키고 할일이 많다",
          "original_form": "결혼도시키고 할일이 많다",
          "speaker_id": "2",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100000706.1.16",
          "form": "ㅎ 언제 이렇게 나이를 먹었나모르겠다",
          "original_form": "ㅎ 언제 이렇게 나이를 먹었나모르겠다",
          "speaker_id": "1",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100000706.1.17",
