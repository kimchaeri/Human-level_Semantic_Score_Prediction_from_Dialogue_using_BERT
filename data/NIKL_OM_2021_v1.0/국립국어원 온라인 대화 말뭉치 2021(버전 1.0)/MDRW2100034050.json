      "utterance": [
        {
          "id": "MDRW2100034050.1.1",
          "form": "오늘 카페 가실 예정이신가요?",
          "original_form": "오늘 카페 가실 예정이신가요?",
          "speaker_id": "1",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100034050.1.2",
          "form": "오늘은 안 갈 것 같아요",
          "original_form": "오늘은 안 갈 것 같아요",
          "speaker_id": "2",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100034050.1.3",
          "form": "가실 예정이신가요?",
          "original_form": "가실 예정이신가요?",
          "speaker_id": "2",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100034050.1.4",
          "form": "저는 들르려구요!",
          "original_form": "저는 들르려구요!",
          "speaker_id": "1",
          "time": "20210827 12:55"
        },
        {
          "id": "MDRW2100034050.1.5",
          "form": "어디 브랜드 가실 예정이신가요?",
          "original_form": "어디 브랜드 가실 예정이신가요?",
          "speaker_id": "2",
          "time": "20210827 12:56"
        },
        {
          "id": "MDRW2100034050.1.6",
          "form": "개인카페로 가려구요!",
          "original_form": "개인카페로 가려구요!",
          "speaker_id": "1",
          "time": "20210827 12:56"
        },
        {
          "id": "MDRW2100034050.1.7",
          "form": "뭐 드실 예정인가요?",
          "original_form": "뭐 드실 예정인가요?",
          "speaker_id": "2",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100034050.1.8",
          "form": "아이스 밀크티 먹으려구요",
          "original_form": "아이스 밀크티 먹으려구요",
          "speaker_id": "1",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100034050.1.9",
          "form": "밀크티 좋아하시나요?",
          "original_form": "밀크티 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100034050.1.10",
          "form": "요즘 너무 맛있게 먹고 있어요",
          "original_form": "요즘 너무 맛있게 먹고 있어요",
          "speaker_id": "1",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100034050.1.11",
          "form": "초코나 딸기같이 뭐 들어간 것보다 그냥 밀크티를 가장 좋아하시나요?",
          "original_form": "초코나 딸기같이 뭐 들어간 것보다 그냥 밀크티를 가장 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100034050.1.12",
          "form": "지금은요! 예전엔 요거트스무디 많이 먹었어요",
          "original_form": "지금은요! 예전엔 요거트스무디 많이 먹었어요",
          "speaker_id": "1",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100034050.1.13",
          "form": "기본을 좋아하시나봐요",
          "original_form": "기본을 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100034050.1.14",
          "form": "그런 것 같아요ㅎㅎ",
          "original_form": "그런 것 같아요ㅎㅎ",
