      "utterance": [
        {
          "id": "MDRW2100034200.1.1",
          "form": "안녕하세요! name2님은 게임 좋아하세요?",
          "original_form": "안녕하세요! &name2&님은 게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100034200.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100034200.1.3",
          "form": "저는 폰게임만 가끔요!",
          "original_form": "저는 폰게임만 가끔요!",
          "speaker_id": "2",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100034200.1.4",
          "form": "앗 어떤 게임 하세요?!",
          "original_form": "앗 어떤 게임 하세요?!",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100034200.1.5",
          "form": "마이리틀셰프라는 요리게임인데 아세요??",
          "original_form": "마이리틀셰프라는 요리게임인데 아세요??",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100034200.1.6",
          "form": "앗 아니요! 요리하는 게임인가요?!",
          "original_form": "앗 아니요! 요리하는 게임인가요?!",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100034200.1.7",
          "form": "네 아기자기한 게임이에요 ㅋㅋㅋ",
          "original_form": "네 아기자기한 게임이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100034200.1.8",
          "form": "게임하세요?",
          "original_form": "게임하세요?",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100034200.1.9",
          "form": "네! 저는 쿠킹덤 좋아해요!!",
          "original_form": "네! 저는 쿠킹덤 좋아해요!!",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100034200.1.10",
          "form": "아시나요?!",
          "original_form": "아시나요?!",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100034200.1.11",
          "form": "아 들어봤어요!",
          "original_form": "아 들어봤어요!",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100034200.1.12",
          "form": "그거하시는 분 많은거같아요 ㅋㅋㅋ",
          "original_form": "그거하시는 분 많은거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100034200.1.13",
          "form": "맞아요 ㅎㅎ 아기자기하고 재밌는 거 같어요 완전 추천!",
          "original_form": "맞아요 ㅎㅎ 아기자기하고 재밌는 거 같어요 완전 추천!",
          "speaker_id": "1",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100034200.1.14",
          "form": "오 저도 해볼게요 ㅋㅋㅋ",
          "original_form": "오 저도 해볼게요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100034200.1.15",
          "form": "얼마나 하셨어요?",
          "original_form": "얼마나 하셨어요?",
          "speaker_id": "2",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100034200.1.16",
          "form": "저 그 처음 나왔을 때부터 계속 했어요!",
          "original_form": "저 그 처음 나왔을 때부터 계속 했어요!",
          "speaker_id": "1",
          "time": "20210827 20:56"
        },
        {
          "id": "MDRW2100034200.1.17",
          "form": "레벨이 높아지니까 지우질 못하겠더라고요 ㅎ.ㅎ",
          "original_form": "레벨이 높아지니까 지우질 못하겠더라고요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210827 20:56"
        },
        {
          "id": "MDRW2100034200.1.18",
          "form": "레벨높으면 지우기 아깝죠ㅠㅠㅜ",
          "original_form": "레벨높으면 지우기 아깝죠ㅠㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100034200.1.19",
          "form": "게다가 현질도 조금 해서 ㅎㅎ",
          "original_form": "게다가 현질도 조금 해서 ㅎㅎ",
