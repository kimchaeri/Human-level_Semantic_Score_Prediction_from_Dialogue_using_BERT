      "utterance": [
        {
          "id": "MDRW2100002642.1.1",
          "form": "도토리 사는 재미가 또 생길까요 ㅎㅎ?",
          "original_form": "도토리 사는 재미가 또 생길까요 ㅎㅎ?",
          "speaker_id": "1",
          "time": "20210820 14:20"
        },
        {
          "id": "MDRW2100002642.1.2",
          "form": "글쎄요 그땐 그시절이라 좋았던거고",
          "original_form": "글쎄요 그땐 그시절이라 좋았던거고",
          "speaker_id": "2",
          "time": "20210820 14:23"
        },
        {
          "id": "MDRW2100002642.1.3",
          "form": "지금은 어떨지 모르겠어요",
          "original_form": "지금은 어떨지 모르겠어요",
          "speaker_id": "2",
          "time": "20210820 14:23"
        },
        {
          "id": "MDRW2100002642.1.4",
          "form": "그쵸 그땐 그거 안하면  유행에 뒤쳐진다고 생각했는데 ㅋ",
          "original_form": "그쵸 그땐 그거 안하면  유행에 뒤쳐진다고 생각했는데 ㅋ",
          "speaker_id": "1",
          "time": "20210820 14:23"
        },
        {
          "id": "MDRW2100002642.1.5",
          "form": "지금은 인스타도 귀찮아서 못하겠어요 ㅎㅎ",
          "original_form": "지금은 인스타도 귀찮아서 못하겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002642.1.6",
          "form": "그렇죠 그시대에 유행이라 했죠",
          "original_form": "그렇죠 그시대에 유행이라 했죠",
          "speaker_id": "2",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002642.1.7",
          "form": "인스타 전 적응 안돼요",
          "original_form": "인스타 전 적응 안돼요",
          "speaker_id": "2",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002642.1.8",
          "form": "ㅠㅠㅋ 저도요  막 예뿌게 찍어야 하는게\r\n다 가식같아요 ㅋㅋㅋ",
          "original_form": "ㅠㅠㅋ 저도요  막 예뿌게 찍어야 하는게\r\n다 가식같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:26"
        },
        {
          "id": "MDRW2100002642.1.9",
          "form": "맞아요 전 사진 엉망이라 발로 찍어요",
          "original_form": "맞아요 전 사진 엉망이라 발로 찍어요",
          "speaker_id": "2",
          "time": "20210820 14:27"
        },
        {
          "id": "MDRW2100002642.1.10",
          "form": "아 응답하라 생각나네요",
          "original_form": "아 응답하라 생각나네요",
          "speaker_id": "2",
          "time": "20210820 14:27"
        },
        {
          "id": "MDRW2100002642.1.11",
          "form": "어떤거용?",
          "original_form": "어떤거용?",
          "speaker_id": "1",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002642.1.12",
          "form": "거기서 옛날 채팅 장면이나 그런거요",
          "original_form": "거기서 옛날 채팅 장면이나 그런거요",
          "speaker_id": "2",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002642.1.13",
          "form": "천리안 에서부터 막 변천사 나오는데",
          "original_form": "천리안 에서부터 막 변천사 나오는데",
          "speaker_id": "2",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002642.1.14",
          "form": "설명이 어렵네요 ㅋㅋ",
          "original_form": "설명이 어렵네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002642.1.15",
          "form": "아아 ㅋㅋ",
          "original_form": "아아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002642.1.16",
          "form": "천리안 전화거는 소리 띠띠딛",
          "original_form": "천리안 전화거는 소리 띠띠딛",
          "speaker_id": "1",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002642.1.17",
          "form": "추억돋네요",
          "original_form": "추억돋네요",
          "speaker_id": "2",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002642.1.18",
          "form": "맞아요 ㅋㅌ",
          "original_form": "맞아요 ㅋㅌ",
          "speaker_id": "2",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002642.1.19",
          "form": "싸이월드가 여럿 소환하네요",
          "original_form": "싸이월드가 여럿 소환하네요",
          "speaker_id": "2",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002642.1.20",
          "form": "ㅋㅋㅋ 이걸 알면 최대 30대이상 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 이걸 알면 최대 30대이상 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:30"
        },
        {
          "id": "MDRW2100002642.1.21",
          "form": "최소 ㅋㅋㅋ요 오타",
          "original_form": "최소 ㅋㅋㅋ요 오타",
          "speaker_id": "1",
          "time": "20210820 14:30"
        },
        {
          "id": "MDRW2100002642.1.22",
          "form": "저 40입니다 ㅋㅋ",
          "original_form": "저 40입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002642.1.23",
          "form": "참 그 화면에 쪼매난 이모티콘 같은것도 생각나고",
          "original_form": "참 그 화면에 쪼매난 이모티콘 같은것도 생각나고",
          "speaker_id": "2",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002642.1.24",
          "form": "전 사실 싸이월드는 별로안했어요",
          "original_form": "전 사실 싸이월드는 별로안했어요",
          "speaker_id": "2",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002642.1.25",
          "form": "근데 계속 미뤄 지던데 했나요?",
          "original_form": "근데 계속 미뤄 지던데 했나요?",
          "speaker_id": "2",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002642.1.26",
          "form": "그쵸그쵸 근데 요즘 사람들은 별로일거 같고\r\n원래 싸이월드 세대는 이제   그런거 할 나이는 지난듯한 느낌이 ㅎㅎ",
          "original_form": "그쵸그쵸 근데 요즘 사람들은 별로일거 같고\r\n원래 싸이월드 세대는 이제   그런거 할 나이는 지난듯한 느낌이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002642.1.27",
          "form": "자세히는 모르겠어요 저도 ㅎㅎ",
          "original_form": "자세히는 모르겠어요 저도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002642.1.28",
          "form": "맞아요 나이가 들고 보는것도 하는것도 다른데 좋아할지",
          "original_form": "맞아요 나이가 들고 보는것도 하는것도 다른데 좋아할지",
          "speaker_id": "2",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002642.1.29",
          "form": "오픈 해도 그냥 가서 구경하거 닫을 느낌이에요 ㅎ",
          "original_form": "오픈 해도 그냥 가서 구경하거 닫을 느낌이에요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002642.1.30",
          "form": "전 구경 안갈듯요 ㅋㅋ",
          "original_form": "전 구경 안갈듯요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002642.1.31",
          "form": "ㅋㅋㅋ 예전에 쓴글  사진 보니 오글오글 하더라구요 ㅋㅋㅋㅋ\r\n추억소환으로 보고 잇어여 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 예전에 쓴글  사진 보니 오글오글 하더라구요 ㅋㅋㅋㅋ\r\n추억소환으로 보고 잇어여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002642.1.32",
          "form": "추억은 그냥 추억인거죠",
          "original_form": "추억은 그냥 추억인거죠",
          "speaker_id": "2",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002642.1.33",
          "form": "맞아요 아 챙피해라",
          "original_form": "맞아요 아 챙피해라",
