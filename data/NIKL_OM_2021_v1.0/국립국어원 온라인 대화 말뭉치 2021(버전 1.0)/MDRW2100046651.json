      "utterance": [
        {
          "id": "MDRW2100046651.1.1",
          "form": "반갑습니다",
          "original_form": "반갑습니다",
          "speaker_id": "1",
          "time": "20211104 12:04"
        },
        {
          "id": "MDRW2100046651.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211104 12:04"
        },
        {
          "id": "MDRW2100046651.1.3",
          "form": "탕수육 부먹 vs 찍먹 어떤 걸 선호하시나요 ㅎㅎ",
          "original_form": "탕수육 부먹 vs 찍먹 어떤 걸 선호하시나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 12:04"
        },
        {
          "id": "MDRW2100046651.1.4",
          "form": "저는 당연히 찍먹입니다",
          "original_form": "저는 당연히 찍먹입니다",
          "speaker_id": "1",
          "time": "20211104 12:04"
        },
        {
          "id": "MDRW2100046651.1.5",
          "form": "부먹이 똑똑하고 잘생겼다는 연구결과가 있어요",
          "original_form": "부먹이 똑똑하고 잘생겼다는 연구결과가 있어요",
          "speaker_id": "1",
          "time": "20211104 12:05"
        },
        {
          "id": "MDRW2100046651.1.6",
          "form": "오 역시",
          "original_form": "오 역시",
          "speaker_id": "2",
          "time": "20211104 12:05"
        },
        {
          "id": "MDRW2100046651.1.7",
          "form": "찍먹이요 잘못말했네 ㅠ",
          "original_form": "찍먹이요 잘못말했네 ㅠ",
          "speaker_id": "1",
          "time": "20211104 12:05"
        },
        {
          "id": "MDRW2100046651.1.8",
          "form": "좀 배우신 분이시네요",
          "original_form": "좀 배우신 분이시네요",
          "speaker_id": "2",
          "time": "20211104 12:05"
        },
        {
          "id": "MDRW2100046651.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 12:05"
        },
        {
          "id": "MDRW2100046651.1.10",
          "form": "바본가봐요 ㅋㅋ",
          "original_form": "바본가봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211104 12:05"
        },
        {
          "id": "MDRW2100046651.1.11",
          "form": "오늘부터 부먹갑니다",
          "original_form": "오늘부터 부먹갑니다",
          "speaker_id": "1",
          "time": "20211104 12:05"
        },
        {
          "id": "MDRW2100046651.1.12",
          "form": "아핰ㅋㅋㅋ",
          "original_form": "아핰ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 12:05"
        },
        {
          "id": "MDRW2100046651.1.13",
          "form": "전 사실 다 잘먹파인데",
          "original_form": "전 사실 다 잘먹파인데",
          "speaker_id": "2",
          "time": "20211104 12:05"
        },
        {
          "id": "MDRW2100046651.1.14",
          "form": "굳이 고르자면 찍먹이에요!!",
          "original_form": "굳이 고르자면 찍먹이에요!!",
          "speaker_id": "2",
          "time": "20211104 12:06"
        },
        {
          "id": "MDRW2100046651.1.15",
          "form": "맞습니다 맛만 있으면 되는거져",
          "original_form": "맞습니다 맛만 있으면 되는거져",
          "speaker_id": "1",
          "time": "20211104 12:06"
        },
        {
          "id": "MDRW2100046651.1.16",
          "form": "부먹은 눅눅하잖아요 ㅡㅡ",
          "original_form": "부먹은 눅눅하잖아요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20211104 12:06"
        },
        {
          "id": "MDRW2100046651.1.17",
          "form": "부먹 좋아하시는 분들은 민트초코도 좋아하시더라고요 ㅡㅡ",
          "original_form": "부먹 좋아하시는 분들은 민트초코도 좋아하시더라고요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20211104 12:06"
        },
        {
          "id": "MDRW2100046651.1.18",
          "form": "민초는 맛있는데요 ㅡㅡ..",
          "original_form": "민초는 맛있는데요 ㅡㅡ..",
          "speaker_id": "2",
          "time": "20211104 12:06"
        },
        {
          "id": "MDRW2100046651.1.19",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20211104 12:06"
        },
        {
          "id": "MDRW2100046651.1.20",
          "form": "아이유님도",
          "original_form": "아이유님도",
          "speaker_id": "1",
          "time": "20211104 12:06"
        },
        {
          "id": "MDRW2100046651.1.21",
          "form": "민초파이셨다가",
          "original_form": "민초파이셨다가",
          "speaker_id": "1",
          "time": "20211104 12:07"
        },
        {
          "id": "MDRW2100046651.1.22",
          "form": "반민초파로 방향을 바꾸셨죠",
          "original_form": "반민초파로 방향을 바꾸셨죠",
          "speaker_id": "1",
          "time": "20211104 12:07"
        },
        {
          "id": "MDRW2100046651.1.23",
          "form": "치약을 맛있다고 느끼다니..\r\n안타깝네요",
          "original_form": "치약을 맛있다고 느끼다니..\r\n안타깝네요",
          "speaker_id": "1",
          "time": "20211104 12:07"
        },
        {
          "id": "MDRW2100046651.1.24",
          "form": "아이유님 그렇게 안봤는데..",
          "original_form": "아이유님 그렇게 안봤는데..",
          "speaker_id": "2",
          "time": "20211104 12:07"
        },
        {
          "id": "MDRW2100046651.1.25",
          "form": "치약이 민트향인거예요",
          "original_form": "치약이 민트향인거예요",
          "speaker_id": "2",
          "time": "20211104 12:07"
        },
        {
          "id": "MDRW2100046651.1.26",
          "form": "민트가 먼.저.예요",
          "original_form": "민트가 먼.저.예요",
          "speaker_id": "2",
          "time": "20211104 12:07"
        },
        {
          "id": "MDRW2100046651.1.27",
          "form": "제발",
          "original_form": "제발",
          "speaker_id": "1",
          "time": "20211104 12:07"
        },
        {
          "id": "MDRW2100046651.1.28",
          "form": "따로따로 드시면 될텐데",
          "original_form": "따로따로 드시면 될텐데",
          "speaker_id": "1",
          "time": "20211104 12:07"
        },
        {
          "id": "MDRW2100046651.1.29",
          "form": "똥과 오줌을 섞어먹는 느낌 아닐까요?",
          "original_form": "똥과 오줌을 섞어먹는 느낌 아닐까요?",
          "speaker_id": "1",
          "time": "20211104 12:08"
        },
        {
          "id": "MDRW2100046651.1.30",
          "form": "아님말고요~",
          "original_form": "아님말고요~",
          "speaker_id": "1",
          "time": "20211104 12:08"
        },
        {
          "id": "MDRW2100046651.1.31",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20211104 12:08"
        },
        {
          "id": "MDRW2100046651.1.32",
          "form": "ㅋㅋㅋㅋㅋ 아니 말이 넘 심하네욬ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 아니 말이 넘 심하네욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 12:08"
        },
        {
          "id": "MDRW2100046651.1.33",
          "form": "똥과 오줌....",
          "original_form": "똥과 오줌....",
          "speaker_id": "2",
          "time": "20211104 12:08"
        },
        {
          "id": "MDRW2100046651.1.34",
