      "utterance": [
        {
          "id": "MDRW2100018312.1.1",
          "form": "여행 많이다니셧나요?",
          "original_form": "여행 많이다니셧나요?",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018312.1.2",
          "form": "여행 많이는 못가봤어요~",
          "original_form": "여행 많이는 못가봤어요~",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018312.1.3",
          "form": "어디 가보셨어요?",
          "original_form": "어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018312.1.4",
          "form": "저도 별로.. 해외는 일본 중국 홍콩 정도요..",
          "original_form": "저도 별로.. 해외는 일본 중국 홍콩 정도요..",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018312.1.5",
          "form": "국내는 해남까지 가본듯.. ㅋㅋㅋ",
          "original_form": "국내는 해남까지 가본듯.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018312.1.6",
          "form": "그래도 여기저기 다녀보셨네요!",
          "original_form": "그래도 여기저기 다녀보셨네요!",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018312.1.7",
          "form": "어디가 제일 좋았나요?",
          "original_form": "어디가 제일 좋았나요?",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018312.1.8",
          "form": "국내는 지리산? ㅋㅋㅋㅋ \r\n산같은데 가보셧나여?",
          "original_form": "국내는 지리산? ㅋㅋㅋㅋ \r\n산같은데 가보셧나여?",
          "speaker_id": "1",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018312.1.9",
          "form": "오 산 어릴땐 부모님 따라 많이 다녔어요ㅋㅋ",
          "original_form": "오 산 어릴땐 부모님 따라 많이 다녔어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018312.1.10",
          "form": "와.. 영재교육 받으셧네요..",
          "original_form": "와.. 영재교육 받으셧네요..",
          "speaker_id": "1",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100018312.1.11",
          "form": "바다랑 계곡 어느쪽이세요?",
          "original_form": "바다랑 계곡 어느쪽이세요?",
          "speaker_id": "1",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100018312.1.12",
          "form": "저는 계곡!",
          "original_form": "저는 계곡!",
          "speaker_id": "2",
          "time": "20210827 10:54"
        },
        {
          "id": "MDRW2100018312.1.13",
          "form": "저도 계곡이요.. 바다는 너무 짜요.. ㅋㅋㅋ",
          "original_form": "저도 계곡이요.. 바다는 너무 짜요.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:55"
        },
        {
          "id": "MDRW2100018312.1.14",
          "form": "ㅋㅋㅋㅋ맞아요 머리 바삭해지는 느낌",
          "original_form": "ㅋㅋㅋㅋ맞아요 머리 바삭해지는 느낌",
          "speaker_id": "2",
          "time": "20210827 10:57"
        },
        {
          "id": "MDRW2100018312.1.15",
          "form": "바다는 잘 못말리면 머리 허옇게되고.. ㅋㅋㅋ",
          "original_form": "바다는 잘 못말리면 머리 허옇게되고.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:58"
        },
        {
          "id": "MDRW2100018312.1.16",
          "form": "완전....",
          "original_form": "완전....",
          "speaker_id": "2",
          "time": "20210827 17:49"
        },
        {
          "id": "MDRW2100018312.1.17",
          "form": "그 중간중간 따뜻한 곳 지나갈때 괜히 찝찝한 기분 알아요?",
          "original_form": "그 중간중간 따뜻한 곳 지나갈때 괜히 찝찝한 기분 알아요?",
          "speaker_id": "2",
          "time": "20210827 17:49"
        },
        {
          "id": "MDRW2100018312.1.18",
          "form": "누가 쉬야한거 아니에요? ㅋㅋㅋ",
          "original_form": "누가 쉬야한거 아니에요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100018312.1.19",
          "form": "ㅋㅋㄱㅋㄱㅋㄱㄱㅋ괜히 그런 기분이라 바다 별로 안좋아해요",
          "original_form": "ㅋㅋㄱㅋㄱㅋㄱㄱㅋ괜히 그런 기분이라 바다 별로 안좋아해요",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100018312.1.20",
          "form": "최근 계곡 가보셧나여?",
          "original_form": "최근 계곡 가보셧나여?",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100018312.1.21",
          "form": "아뇨~계곡 놀러간지 몇년은 된것같아요ㅠㅠ",
          "original_form": "아뇨~계곡 놀러간지 몇년은 된것같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100018312.1.22",
          "form": "놀러가고싶네요~~",
          "original_form": "놀러가고싶네요~~",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100018312.1.23",
          "form": "전 가끔 등산 다니는데.. \r\n꼭 계곡끼고 코스 잡아서.. \r\n\r\n무조건 발담그고 와요..",
          "original_form": "전 가끔 등산 다니는데.. \r\n꼭 계곡끼고 코스 잡아서.. \r\n\r\n무조건 발담그고 와요..",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100018312.1.24",
          "form": "아~완전 힐링",
          "original_form": "아~완전 힐링",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100018312.1.25",
          "form": "등산하고 계곡에 발 담그면 그렇게 상쾌하고 좋은데",
          "original_form": "등산하고 계곡에 발 담그면 그렇게 상쾌하고 좋은데",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100018312.1.26",
          "form": "저도 가고싶네용",
          "original_form": "저도 가고싶네용",
          "speaker_id": "2",
          "time": "20210827 23:40"
        }
      ]
