      "utterance": [
        {
          "id": "MDRW2100014399.1.1",
          "form": "여행 좋아하세요,",
          "original_form": "여행 좋아하세요,",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014399.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014399.1.3",
          "form": "웅ㅋㅋ 나 다음주에 남친이랑 여수여행가기로햏어!",
          "original_form": "웅ㅋㅋ 나 다음주에 남친이랑 여수여행가기로햏어!",
          "speaker_id": "2",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100014399.1.4",
          "form": "대박...진짜부럽다",
          "original_form": "대박...진짜부럽다",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014399.1.5",
          "form": "몇일 ?",
          "original_form": "몇일 ?",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014399.1.6",
          "form": "1박2일 ㅋㅋ 여수가본적있어?",
          "original_form": "1박2일 ㅋㅋ 여수가본적있어?",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014399.1.7",
          "form": "웅 옛날에 ㅋㅋㅋㅋ",
          "original_form": "웅 옛날에 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014399.1.8",
          "form": "요즘 좋은숙소 많이생겼던데..   인피니티풀...",
          "original_form": "요즘 좋은숙소 많이생겼던데..   인피니티풀...",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014399.1.9",
          "form": "바다 크으",
          "original_form": "바다 크으",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014399.1.10",
          "form": "가서뭐먹어",
          "original_form": "가서뭐먹어",
          "speaker_id": "1",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014399.1.11",
          "form": "음 일단은 간장게장?",
          "original_form": "음 일단은 간장게장?",
          "speaker_id": "2",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014399.1.12",
          "form": "친구가 거기서 게장먹었는데 진짜맛있었다고 완전 추천함",
          "original_form": "친구가 거기서 게장먹었는데 진짜맛있었다고 완전 추천함",
          "speaker_id": "2",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014399.1.13",
          "form": "오 ㅋㅋㅋ 혹시 식당 정해놨오?",
          "original_form": "오 ㅋㅋㅋ 혹시 식당 정해놨오?",
          "speaker_id": "1",
          "time": "20210825 19:25"
        },
        {
          "id": "MDRW2100014399.1.14",
          "form": "거기에 게장거리가 있대ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "거기에 게장거리가 있대ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014399.1.15",
          "form": "그중에 친구가 말해준 집 있는데 기억안나구 찾아봐야햄",
          "original_form": "그중에 친구가 말해준 집 있는데 기억안나구 찾아봐야햄",
          "speaker_id": "2",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014399.1.16",
          "form": "추천해줄 메뉴나 식당 있어?",
          "original_form": "추천해줄 메뉴나 식당 있어?",
          "speaker_id": "2",
          "time": "20210825 19:32"
        },
        {
          "id": "MDRW2100014399.1.17",
          "form": "아닝 나도 추천받을라구 물어봣오 ㅋㅋ",
          "original_form": "아닝 나도 추천받을라구 물어봣오 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:34"
        },
        {
          "id": "MDRW2100014399.1.18",
          "form": "난 10년전에가서 진짜 기억이안나네ㅠㅠ",
          "original_form": "난 10년전에가서 진짜 기억이안나네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 19:34"
        },
        {
          "id": "MDRW2100014399.1.19",
          "form": "나두 여수 첨이라 이것저것 검색해보는중!",
          "original_form": "나두 여수 첨이라 이것저것 검색해보는중!",
          "speaker_id": "2",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014399.1.20",
          "form": "오동도기억난다",
          "original_form": "오동도기억난다",
          "speaker_id": "1",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014399.1.21",
          "form": "오 오동도 들어봤어",
          "original_form": "오 오동도 들어봤어",
          "speaker_id": "2",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014399.1.22",
          "form": "맞아 여수에서 배타고 여기저기 또 들어갈수있더라",
          "original_form": "맞아 여수에서 배타고 여기저기 또 들어갈수있더라",
          "speaker_id": "2",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100014399.1.23",
          "form": "헐 그건 몰랐네 어떤데있지",
          "original_form": "헐 그건 몰랐네 어떤데있지",
          "speaker_id": "1",
          "time": "20210825 19:41"
        }
      ]
    }
  ]
