      "utterance": [
        {
          "id": "MDRW2100007711.1.1",
          "form": "오늘 점심메뉴는 정하셨나요?",
          "original_form": "오늘 점심메뉴는 정하셨나요?",
          "speaker_id": "1",
          "time": "20210822 09:50"
        },
        {
          "id": "MDRW2100007711.1.2",
          "form": "집에서 김치볶음밥이랑 콩나물국 먹으려고요!",
          "original_form": "집에서 김치볶음밥이랑 콩나물국 먹으려고요!",
          "speaker_id": "2",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007711.1.3",
          "form": "오 최고의 조합이죠 ㅠㅠ!!",
          "original_form": "오 최고의 조합이죠 ㅠㅠ!!",
          "speaker_id": "1",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100007711.1.4",
          "form": "전 남은 미역 처리하느라 미역국 끓여요 ㅎㅎ",
          "original_form": "전 남은 미역 처리하느라 미역국 끓여요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100007711.1.5",
          "form": "직접 끓이시는 건가요??",
          "original_form": "직접 끓이시는 건가요??",
          "speaker_id": "2",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100007711.1.6",
          "form": "네~ 생각보다 쉬워요 ㅎㅎ",
          "original_form": "네~ 생각보다 쉬워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 09:07"
        },
        {
          "id": "MDRW2100007711.1.7",
          "form": "아침은 드셨나요?",
          "original_form": "아침은 드셨나요?",
          "speaker_id": "1",
          "time": "20210823 09:08"
        },
        {
          "id": "MDRW2100007711.1.8",
          "form": "이제야 일어나서 빵으로 때우고 있어요ㅋㅋ",
          "original_form": "이제야 일어나서 빵으로 때우고 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:17"
        },
        {
          "id": "MDRW2100007711.1.9",
          "form": "아이코 ㅋㅋ 저도 아침은 빵으로 때울때가 많아요",
          "original_form": "아이코 ㅋㅋ 저도 아침은 빵으로 때울때가 많아요",
          "speaker_id": "1",
          "time": "20210823 15:49"
        },
        {
          "id": "MDRW2100007711.1.10",
          "form": "저녁은 뭘먹나 벌써 고민이넹ㅅ",
          "original_form": "저녁은 뭘먹나 벌써 고민이넹ㅅ",
          "speaker_id": "1",
          "time": "20210823 15:49"
        },
        {
          "id": "MDRW2100007711.1.11",
          "form": "김치볶음밥이 먹고 싶은데 귀찮아서 의욕이 안 나네요ㅜ",
          "original_form": "김치볶음밥이 먹고 싶은데 귀찮아서 의욕이 안 나네요ㅜ",
          "speaker_id": "2",
          "time": "20210823 16:14"
        },
        {
          "id": "MDRW2100007711.1.12",
          "form": "하기 귀찮죠 ㅠㅠ",
          "original_form": "하기 귀찮죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 17:10"
        },
        {
          "id": "MDRW2100007711.1.13",
          "form": "거긴 비가 많이 오나요?",
          "original_form": "거긴 비가 많이 오나요?",
          "speaker_id": "1",
          "time": "20210823 17:10"
        },
        {
          "id": "MDRW2100007711.1.14",
          "form": "비오는 날은 배달시키기도 뭔가 죄송스러워요",
          "original_form": "비오는 날은 배달시키기도 뭔가 죄송스러워요",
          "speaker_id": "1",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100007711.1.15",
          "form": "안전의 문제가 달려있으니까요ㅠ 여기는 조금씩만 오고 있어요!",
          "original_form": "안전의 문제가 달려있으니까요ㅠ 여기는 조금씩만 오고 있어요!",
          "speaker_id": "2",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100007711.1.16",
          "form": "집앞에 매주 순대트럭이 오는데 갑자기 먹고싶네요 ㅠ 오려면 아직 이틀이나 남았어요 흑",
          "original_form": "집앞에 매주 순대트럭이 오는데 갑자기 먹고싶네요 ㅠ 오려면 아직 이틀이나 남았어요 흑",
          "speaker_id": "1",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100007711.1.17",
          "form": "순대트럭 넘 맛있죠...맥주 안주로 정말 딱...ㅋㅋㅋㅋㅋ",
          "original_form": "순대트럭 넘 맛있죠...맥주 안주로 정말 딱...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:56"
        },
        {
          "id": "MDRW2100007711.1.18",
          "form": "크 좋죠 ㅎㅎ",
          "original_form": "크 좋죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100007711.1.19",
          "form": "즐겨드시는 안주가 있으신가요?",
          "original_form": "즐겨드시는 안주가 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100007711.1.20",
          "form": "저는 감자튀김을 가장 즐겨 먹어요ㅎㅎ",
          "original_form": "저는 감자튀김을 가장 즐겨 먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 10:26"
        }
      ]
