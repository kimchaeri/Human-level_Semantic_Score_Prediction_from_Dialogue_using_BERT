      "utterance": [
        {
          "id": "MDRW2100028505.1.1",
          "form": "너희는 재택근무해?",
          "original_form": "너희는 재택근무해?",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028505.1.2",
          "form": "우린 재택근무안해",
          "original_form": "우린 재택근무안해",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028505.1.3",
          "form": "그럴 기미도 없어",
          "original_form": "그럴 기미도 없어",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028505.1.4",
          "form": "우리도 재택 얘기 없어",
          "original_form": "우리도 재택 얘기 없어",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028505.1.5",
          "form": "내가 걸리는게 젤 빠르겠어",
          "original_form": "내가 걸리는게 젤 빠르겠어",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028505.1.6",
          "form": "재택해도 너의 직업은 근무하기 좋니?",
          "original_form": "재택해도 너의 직업은 근무하기 좋니?",
          "speaker_id": "1",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028505.1.7",
          "form": "재택하면 난 좀 불편해",
          "original_form": "재택하면 난 좀 불편해",
          "speaker_id": "2",
          "time": "20210902 20:33"
        },
        {
          "id": "MDRW2100028505.1.8",
          "form": "나도 사실 마냥 편하지는 않아",
          "original_form": "나도 사실 마냥 편하지는 않아",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028505.1.9",
          "form": "하지만 익숙해지면 할 만할 거 같아",
          "original_form": "하지만 익숙해지면 할 만할 거 같아",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028505.1.10",
          "form": "재택하는 여디니가 부러워",
          "original_form": "재택하는 여디니가 부러워",
          "speaker_id": "2",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028505.1.11",
          "form": "나도 재택하고 싶어",
          "original_form": "나도 재택하고 싶어",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028505.1.12",
          "form": "재택 근무하게 된다면 너의 계획을 말해봐",
          "original_form": "재택 근무하게 된다면 너의 계획을 말해봐",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028505.1.13",
          "form": "나는 10시에 일어나러 메신저 켜고",
          "original_form": "나는 10시에 일어나러 메신저 켜고",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028505.1.14",
          "form": "일하는 척하면서 씻을 거야",
          "original_form": "일하는 척하면서 씻을 거야",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028505.1.15",
          "form": "10시라니 양심적이다",
          "original_form": "10시라니 양심적이다",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028505.1.16",
          "form": "불시에 화상 연결할까 봐",
          "original_form": "불시에 화상 연결할까 봐",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028505.1.17",
          "form": "너는 재택하면 어쩔거야?",
          "original_form": "너는 재택하면 어쩔거야?",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028505.1.18",
          "form": "화상연결도 해?? 정말 지독한 놈들",
