      "utterance": [
        {
          "id": "MDRW2100025330.1.1",
          "form": "뮤비는봤으니 이제 노래를 듣고",
          "original_form": "뮤비는봤으니 이제 노래를 듣고",
          "speaker_id": "1",
          "time": "20210901 09:37"
        },
        {
          "id": "MDRW2100025330.1.2",
          "form": "밝은날 노래와 뮤비를한번에...",
          "original_form": "밝은날 노래와 뮤비를한번에...",
          "speaker_id": "1",
          "time": "20210901 09:37"
        },
        {
          "id": "MDRW2100025330.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ노래는 그렇게 무섭진 않고 적당히 괜찮아여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ노래는 그렇게 무섭진 않고 적당히 괜찮아여",
          "speaker_id": "2",
          "time": "20210901 09:43"
        },
        {
          "id": "MDRW2100025330.1.4",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210901 09:45"
        },
        {
          "id": "MDRW2100025330.1.5",
          "form": "근데 그섬 뮤비랑 안어울리지않나???",
          "original_form": "근데 그섬 뮤비랑 안어울리지않나???",
          "speaker_id": "1",
          "time": "20210901 09:45"
        },
        {
          "id": "MDRW2100025330.1.6",
          "form": "들어봐야이해가되겠군",
          "original_form": "들어봐야이해가되겠군",
          "speaker_id": "1",
          "time": "20210901 09:45"
        },
        {
          "id": "MDRW2100025330.1.7",
          "form": "어 그게",
          "original_form": "어 그게",
          "speaker_id": "2",
          "time": "20210901 09:51"
        },
        {
          "id": "MDRW2100025330.1.8",
          "form": "가사가 원망과 처절함과 청승이 담겨있긴 한데",
          "original_form": "가사가 원망과 처절함과 청승이 담겨있긴 한데",
          "speaker_id": "2",
          "time": "20210901 09:52"
        },
        {
          "id": "MDRW2100025330.1.9",
          "form": "창귀처럼 대놓고 써늘한 느낌은 아니라ㅋㅋㅋㅋㅋㅋ",
          "original_form": "창귀처럼 대놓고 써늘한 느낌은 아니라ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:52"
        },
        {
          "id": "MDRW2100025330.1.10",
          "form": "물론 그 안예은 특유의 지르는 창법이 무서운 분위기를 살려놓긴 합니다",
          "original_form": "물론 그 안예은 특유의 지르는 창법이 무서운 분위기를 살려놓긴 합니다",
          "speaker_id": "2",
          "time": "20210901 09:52"
        },
        {
          "id": "MDRW2100025330.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100025330.1.12",
          "form": "ㅋㄹㄹㅋㅋㄱ오늘 반차내고 일찍가는데",
          "original_form": "ㅋㄹㄹㅋㅋㄱ오늘 반차내고 일찍가는데",
          "speaker_id": "1",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100025330.1.13",
          "form": "가면서들어봐야지",
          "original_form": "가면서들어봐야지",
          "speaker_id": "1",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100025330.1.14",
          "form": "오늘 비오는날이라",
          "original_form": "오늘 비오는날이라",
          "speaker_id": "1",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100025330.1.15",
          "form": "음산한 노래 듣기 아주딱이네",
          "original_form": "음산한 노래 듣기 아주딱이네",
          "speaker_id": "1",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100025330.1.16",
          "form": "아 맞네ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 날이 딱",
          "original_form": "아 맞네ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 날이 딱",
          "speaker_id": "2",
          "time": "20210901 10:04"
        },
        {
          "id": "MDRW2100025330.1.17",
          "form": "공포물 즐기기 좋은 날이네여",
          "original_form": "공포물 즐기기 좋은 날이네여",
          "speaker_id": "2",
          "time": "20210901 10:04"
        },
        {
          "id": "MDRW2100025330.1.18",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025330.1.19",
          "form": "아주 분위기가 딱인듯ㅋㅋㅋ",
          "original_form": "아주 분위기가 딱인듯ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025330.1.20",
          "form": "안그래도 안예은 요새 그 만화 ost불러서",
          "original_form": "안그래도 안예은 요새 그 만화 ost불러서",
          "speaker_id": "1",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100025330.1.21",
          "form": "제피드에 자꾸뜨더라구요",
          "original_form": "제피드에 자꾸뜨더라구요",
          "speaker_id": "1",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100025330.1.22",
          "form": "창귀봐서그런가",
          "original_form": "창귀봐서그런가",
          "speaker_id": "1",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100025330.1.23",
          "form": "관련내용이뜨더라구",
          "original_form": "관련내용이뜨더라구",
          "speaker_id": "1",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100025330.1.24",
          "form": "아 야화인가 그건가보다",
          "original_form": "아 야화인가 그건가보다",
          "speaker_id": "2",
          "time": "20210901 15:30"
        },
        {
          "id": "MDRW2100025330.1.25",
          "form": "노래 좋다는데 아직 안들어봤네..",
          "original_form": "노래 좋다는데 아직 안들어봤네..",
          "speaker_id": "2",
          "time": "20210901 15:31"
        },
        {
          "id": "MDRW2100025330.1.26",
          "form": "아 정말???",
          "original_form": "아 정말???",
          "speaker_id": "1",
          "time": "20210901 15:32"
        },
        {
          "id": "MDRW2100025330.1.27",
          "form": "오웅ㅇ 이제 방금 백신도맞고",
          "original_form": "오웅ㅇ 이제 방금 백신도맞고",
          "speaker_id": "1",
          "time": "20210901 15:33"
        },
        {
          "id": "MDRW2100025330.1.28",
          "form": "노래를 들어봐야겠네여",
          "original_form": "노래를 들어봐야겠네여",
          "speaker_id": "1",
          "time": "20210901 15:33"
        },
        {
          "id": "MDRW2100025330.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ저도 곧 들어보는 것으로..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도 곧 들어보는 것으로..",
          "speaker_id": "2",
          "time": "20210901 15:34"
        },
        {
          "id": "MDRW2100025330.1.30",
          "form": "ㅋㅋㅋㄱㅋㄱ아 밝은곳을 찾아서 이제 능소화를...",
          "original_form": "ㅋㅋㅋㄱㅋㄱ아 밝은곳을 찾아서 이제 능소화를...",
          "speaker_id": "1",
          "time": "20210901 15:36"
        },
        {
          "id": "MDRW2100025330.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ오시는 지하철에서 들으셔도 되겠어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ오시는 지하철에서 들으셔도 되겠어요",
          "speaker_id": "2",
          "time": "20210901 15:48"
        },
        {
          "id": "MDRW2100025330.1.32",
          "form": "저는 창귀나 한번 더..",
          "original_form": "저는 창귀나 한번 더..",
          "speaker_id": "2",
          "time": "20210901 15:48"
        },
        {
          "id": "MDRW2100025330.1.33",
          "form": "ㅋㅋㄱㅋㄱㅋㅋ 저도 보고 다시돌아올게유",
          "original_form": "ㅋㅋㄱㅋㄱㅋㅋ 저도 보고 다시돌아올게유",
          "speaker_id": "1",
