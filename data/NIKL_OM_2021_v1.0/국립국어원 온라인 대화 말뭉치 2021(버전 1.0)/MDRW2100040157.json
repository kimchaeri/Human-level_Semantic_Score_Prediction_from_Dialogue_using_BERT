      "utterance": [
        {
          "id": "MDRW2100040157.1.1",
          "form": "비빔면은 대패랑 먹으면 좋더라고요",
          "original_form": "비빔면은 대패랑 먹으면 좋더라고요",
          "speaker_id": "2",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040157.1.2",
          "form": "짜파게티하니까 우리집에는 비빔면만 있고 비빔면하니까 고기구워서 같이먹고싶고 구운고기 생각하니까",
          "original_form": "짜파게티하니까 우리집에는 비빔면만 있고 비빔면하니까 고기구워서 같이먹고싶고 구운고기 생각하니까",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040157.1.3",
          "form": "와사비 생각나네요!!",
          "original_form": "와사비 생각나네요!!",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040157.1.4",
          "form": "ㅋㅋㅋ 끝없죠",
          "original_form": "ㅋㅋㅋ 끝없죠",
          "speaker_id": "2",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040157.1.5",
          "form": "아니 이무슨.. ㅎㅎㅎ;",
          "original_form": "아니 이무슨.. ㅎㅎㅎ;",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040157.1.6",
          "form": "그러게요ㅜㅜ",
          "original_form": "그러게요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040157.1.7",
          "form": "와사비 찍어먹으면 맛있는데",
          "original_form": "와사비 찍어먹으면 맛있는데",
          "speaker_id": "2",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040157.1.8",
          "form": "꺄",
          "original_form": "꺄",
          "speaker_id": "2",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040157.1.9",
          "form": "그쵸 초밥사이에만 맛있는건줄 알았는데",
          "original_form": "그쵸 초밥사이에만 맛있는건줄 알았는데",
          "speaker_id": "1",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100040157.1.10",
          "form": "고기랑 먹으니까 또 그렇게 맛있네요ㅠㅠ",
          "original_form": "고기랑 먹으니까 또 그렇게 맛있네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100040157.1.11",
          "form": "와사비 어디꺼로 드세요?",
          "original_form": "와사비 어디꺼로 드세요?",
          "speaker_id": "2",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100040157.1.12",
          "form": "허허...기사뜬곳에 있는거요.ㅠㅠ",
          "original_form": "허허...기사뜬곳에 있는거요.ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100040157.1.13",
          "form": "와사비가 와사비가 아니라는 기사가 떴었어용.ㅋㅋㅋ",
          "original_form": "와사비가 와사비가 아니라는 기사가 떴었어용.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100040157.1.14",
          "form": "저도 이번에 산건 기사떴더라고요 ㅎㅎㅎ",
          "original_form": "저도 이번에 산건 기사떴더라고요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100040157.1.15",
          "form": "ㅠ  음식가지고 사기치면 안되는거죠ㅠㅠㅠㅠ",
          "original_form": "ㅠ  음식가지고 사기치면 안되는거죠ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040157.1.16",
          "form": "삼광이었나 거기껀데",
          "original_form": "삼광이었나 거기껀데",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040157.1.17",
          "form": "그래도 일단  와믜사비가 아니래도 맛있으니까 먹습니다 ㅋㄱㅋ",
          "original_form": "그래도 일단  와믜사비가 아니래도 맛있으니까 먹습니다 ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040157.1.18",
          "form": "그 홈플러스에서 파는건 안걸렸더라고요",
          "original_form": "그 홈플러스에서 파는건 안걸렸더라고요",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040157.1.19",
          "form": "오정말요?",
          "original_form": "오정말요?",
          "speaker_id": "1",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100040157.1.20",
          "form": "이따 홈플갈껀데!!",
          "original_form": "이따 홈플갈껀데!!",
          "speaker_id": "1",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100040157.1.21",
          "form": "네 그래서 예전에 사먹었을때 맛있다 하고 먹었던건 역시 다르네",
          "original_form": "네 그래서 예전에 사먹었을때 맛있다 하고 먹었던건 역시 다르네",
          "speaker_id": "2",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100040157.1.22",
          "form": "하면서 봤어요 ㅋㅋㅋ",
          "original_form": "하면서 봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100040157.1.23",
          "form": "오~~~ 저도그럼 이따가서 사봐야겠어요 ㅋㅋ",
          "original_form": "오~~~ 저도그럼 이따가서 사봐야겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:24"
        },
        {
          "id": "MDRW2100040157.1.24",
          "form": "삼광은 이번에 샀는데 기사에 걸린곳이고 안걸린데는 상호명을 모르겠어요 ㅋㅋ",
          "original_form": "삼광은 이번에 샀는데 기사에 걸린곳이고 안걸린데는 상호명을 모르겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:24"
        },
        {
          "id": "MDRW2100040157.1.25",
          "form": "네 근데 맛이 진짜 달랐어요 ㅋㅋㅋ",
          "original_form": "네 근데 맛이 진짜 달랐어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:24"
        },
        {
          "id": "MDRW2100040157.1.26",
          "form": "오뚜기도걸렸더라구요ㅜ",
          "original_form": "오뚜기도걸렸더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210904 12:24"
        },
        {
          "id": "MDRW2100040157.1.27",
          "form": "오 넘신기하네요",
          "original_form": "오 넘신기하네요",
          "speaker_id": "1",
          "time": "20210904 12:24"
        },
        {
          "id": "MDRW2100040157.1.28",
          "form": "맛보기싶어지는",
          "original_form": "맛보기싶어지는",
          "speaker_id": "1",
          "time": "20210904 12:24"
        },
        {
          "id": "MDRW2100040157.1.29",
          "form": "안걸린 곳이 진짜 없던데요",
          "original_form": "안걸린 곳이 진짜 없던데요",
          "speaker_id": "2",
          "time": "20210904 12:25"
        },
        {
          "id": "MDRW2100040157.1.30",
          "form": "유명한 곳도 여러군데고",
          "original_form": "유명한 곳도 여러군데고",
          "speaker_id": "2",
          "time": "20210904 12:25"
        },
        {
          "id": "MDRW2100040157.1.31",
          "form": "전 다걸린줄알았어요 ㅋㅋ 다이러겠구나..",
          "original_form": "전 다걸린줄알았어요 ㅋㅋ 다이러겠구나..",
          "speaker_id": "1",
          "time": "20210904 12:25"
        },
        {
          "id": "MDRW2100040157.1.32",
          "form": "가격차이가 크다고하더라구요",
          "original_form": "가격차이가 크다고하더라구요",
          "speaker_id": "1",
          "time": "20210904 12:25"
        },
        {
          "id": "MDRW2100040157.1.33",
          "form": "먹을걸로 장난치다니",
          "original_form": "먹을걸로 장난치다니",
          "speaker_id": "2",
