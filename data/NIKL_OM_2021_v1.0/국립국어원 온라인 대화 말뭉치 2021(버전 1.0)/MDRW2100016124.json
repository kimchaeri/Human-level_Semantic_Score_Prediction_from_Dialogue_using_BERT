      "utterance": [
        {
          "id": "MDRW2100016124.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100016124.1.2",
          "form": "종교가있으신가요?",
          "original_form": "종교가있으신가요?",
          "speaker_id": "1",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100016124.1.3",
          "form": "아뇨 저는 없어요",
          "original_form": "아뇨 저는 없어요",
          "speaker_id": "2",
          "time": "20210826 10:16"
        },
        {
          "id": "MDRW2100016124.1.4",
          "form": "종교 갖고 계세요?",
          "original_form": "종교 갖고 계세요?",
          "speaker_id": "2",
          "time": "20210826 10:16"
        },
        {
          "id": "MDRW2100016124.1.5",
          "form": "저도 무교에요 ㅎㅎ",
          "original_form": "저도 무교에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:17"
        },
        {
          "id": "MDRW2100016124.1.6",
          "form": "가끔씩 절에 구경가기는해요 ㅎㅎ",
          "original_form": "가끔씩 절에 구경가기는해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100016124.1.7",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "2",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100016124.1.8",
          "form": "저도 어렸을때는 교회 몇번 간 적 있어요",
          "original_form": "저도 어렸을때는 교회 몇번 간 적 있어요",
          "speaker_id": "2",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100016124.1.9",
          "form": "어릴때 떡볶이준다그래서 교회갓다가",
          "original_form": "어릴때 떡볶이준다그래서 교회갓다가",
          "speaker_id": "1",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100016124.1.10",
          "form": "아하 ㅎㅎ",
          "original_form": "아하 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016124.1.11",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016124.1.12",
          "form": "앞으로도 종교가질생각은 엄써요",
          "original_form": "앞으로도 종교가질생각은 엄써요",
          "speaker_id": "1",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016124.1.13",
          "form": "넘 귀찮은것..",
          "original_form": "넘 귀찮은것..",
          "speaker_id": "1",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016124.1.14",
          "form": "군대에서는 성당 교회 절 다가봤어요",
          "original_form": "군대에서는 성당 교회 절 다가봤어요",
          "speaker_id": "2",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016124.1.15",
          "form": "종교활동을 강제해서",
          "original_form": "종교활동을 강제해서",
          "speaker_id": "2",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016124.1.16",
          "form": "아 군필이시구나~!",
          "original_form": "아 군필이시구나~!",
          "speaker_id": "1",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016124.1.17",
          "form": "저도들엇어요ㅋㅋㅋ",
          "original_form": "저도들엇어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016124.1.18",
          "form": "맛잇는거준다고?그랫던거같은뎅",
          "original_form": "맛잇는거준다고?그랫던거같은뎅",
          "speaker_id": "1",
          "time": "20210826 10:20"
        },
        {
          "id": "MDRW2100016124.1.19",
          "form": "간식 주기는 하는데 사실 지금 생각해보면",
          "original_form": "간식 주기는 하는데 사실 지금 생각해보면",
          "speaker_id": "2",
          "time": "20210826 10:21"
        },
        {
          "id": "MDRW2100016124.1.20",
          "form": "대단하지는 않아요",
          "original_form": "대단하지는 않아요",
          "speaker_id": "2",
          "time": "20210826 10:21"
        },
        {
          "id": "MDRW2100016124.1.21",
          "form": "종교가 정상적으로 동작한다면 개인들이 도덕적으로 살 수 있도록 하는 역할을 한다고 생각해요",
          "original_form": "종교가 정상적으로 동작한다면 개인들이 도덕적으로 살 수 있도록 하는 역할을 한다고 생각해요",
          "speaker_id": "2",
          "time": "20210826 10:21"
        },
        {
          "id": "MDRW2100016124.1.22",
          "form": "ㅎㅎ맞아요",
          "original_form": "ㅎㅎ맞아요",
          "speaker_id": "1",
          "time": "20210826 10:24"
        },
        {
          "id": "MDRW2100016124.1.23",
          "form": "정상적으로 동작하면 얼마나 좋으려나",
          "original_form": "정상적으로 동작하면 얼마나 좋으려나",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016124.1.24",
          "form": "그러기 쉽지않져 ㅎㅎ",
          "original_form": "그러기 쉽지않져 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016124.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 10:28"
        },
        {
          "id": "MDRW2100016124.1.26",
          "form": "아프간도 종교때문에",
          "original_form": "아프간도 종교때문에",
          "speaker_id": "2",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100016124.1.27",
          "form": "선량한 시민들만 힘들고",
          "original_form": "선량한 시민들만 힘들고",
          "speaker_id": "2",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100016124.1.28",
          "form": "여성들 억압받고",
          "original_form": "여성들 억압받고",
          "speaker_id": "2",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100016124.1.29",
          "form": "종교가 문제가 아니라",
          "original_form": "종교가 문제가 아니라",
          "speaker_id": "2",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100016124.1.30",
          "form": "사람이 문제인것 같아요",
          "original_form": "사람이 문제인것 같아요",
          "speaker_id": "2",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100016124.1.31",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100016124.1.32",
          "form": "사람이 제일 제일 나빠요",
          "original_form": "사람이 제일 제일 나빠요",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100016124.1.33",
          "form": "종교도 결국 돈 권력 욕심에.....",
          "original_form": "종교도 결국 돈 권력 욕심에.....",
