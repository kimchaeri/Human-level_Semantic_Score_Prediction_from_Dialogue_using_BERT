      "utterance": [
        {
          "id": "MDRW2100022905.1.1",
          "form": "왓챠에 재밌는거 있을까요?",
          "original_form": "왓챠에 재밌는거 있을까요?",
          "speaker_id": "2",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022905.1.2",
          "form": "나를 차버린 스파이",
          "original_form": "나를 차버린 스파이",
          "speaker_id": "1",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022905.1.3",
          "form": "이거 재밌어용!",
          "original_form": "이거 재밌어용!",
          "speaker_id": "1",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100022905.1.4",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022905.1.5",
          "form": "재밌어보여요ㅋㅋㅋㅋ",
          "original_form": "재밌어보여요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022905.1.6",
          "form": "장르가 액션코미디인데",
          "original_form": "장르가 액션코미디인데",
          "speaker_id": "1",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022905.1.7",
          "form": "미국에서 snl로 유명한",
          "original_form": "미국에서 snl로 유명한",
          "speaker_id": "1",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022905.1.8",
          "form": "케이트 맥키넌이라는 배우가 나와용",
          "original_form": "케이트 맥키넌이라는 배우가 나와용",
          "speaker_id": "1",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022905.1.9",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022905.1.10",
          "form": "재미보장이네요",
          "original_form": "재미보장이네요",
          "speaker_id": "2",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022905.1.11",
          "form": "요즘 티비 프로그램 뭐 보세용?",
          "original_form": "요즘 티비 프로그램 뭐 보세용?",
          "speaker_id": "1",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022905.1.12",
          "form": "꼬꼬무 재밌더라구요",
          "original_form": "꼬꼬무 재밌더라구요",
          "speaker_id": "2",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022905.1.13",
          "form": "엄청챙겨보진않지만",
          "original_form": "엄청챙겨보진않지만",
          "speaker_id": "2",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022905.1.14",
          "form": "아하 이거 저도 봤는데",
          "original_form": "아하 이거 저도 봤는데",
          "speaker_id": "1",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022905.1.15",
          "form": "흥미진진하더라구요ㅋㅋㅋ",
          "original_form": "흥미진진하더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022905.1.16",
          "form": "실화인데 드라마같은 이야기들이 많아요",
          "original_form": "실화인데 드라마같은 이야기들이 많아요",
          "speaker_id": "1",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022905.1.17",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022905.1.18",
          "form": "스우파도 클립으로만 보긴했지만",
          "original_form": "스우파도 클립으로만 보긴했지만",
          "speaker_id": "2",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022905.1.19",
          "form": "다들잘하고재밌더라구요~",
          "original_form": "다들잘하고재밌더라구요~",
          "speaker_id": "2",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022905.1.20",
          "form": "요즘 핫하더라구요ㅋㅋㅋ",
          "original_form": "요즘 핫하더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022905.1.21",
          "form": "그러더라구요ㅋㅋㅋㅋ",
          "original_form": "그러더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022905.1.22",
          "form": "그 청각장애를 앓고계신 예리? 출연자님 너무 대단해요",
          "original_form": "그 청각장애를 앓고계신 예리? 출연자님 너무 대단해요",
          "speaker_id": "1",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022905.1.23",
          "form": "드라마는 보시나요?",
          "original_form": "드라마는 보시나요?",
          "speaker_id": "2",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022905.1.24",
          "form": "아 그런분도계세요?",
          "original_form": "아 그런분도계세요?",
          "speaker_id": "2",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022905.1.25",
          "form": "저 드라마는 항상 종영된걸로 보고",
          "original_form": "저 드라마는 항상 종영된걸로 보고",
          "speaker_id": "1",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022905.1.26",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022905.1.27",
          "form": "본방은 안봐용ㅎㅎ",
          "original_form": "본방은 안봐용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022905.1.28",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022905.1.29",
          "form": "대단하시네요ㅜㅜ",
          "original_form": "대단하시네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022905.1.30",
          "form": "후천척 장애가 생기셨대요",
          "original_form": "후천척 장애가 생기셨대요",
          "speaker_id": "1",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022905.1.31",
          "form": "3~4살쯤ㅠㅠ",
          "original_form": "3~4살쯤ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 19:07"
        }
      ]
