      "utterance": [
        {
          "id": "MDRW2100016116.1.1",
          "form": "날씨가 좀 흐리네요",
          "original_form": "날씨가 좀 흐리네요",
          "speaker_id": "1",
          "time": "20210826 09:52"
        },
        {
          "id": "MDRW2100016116.1.2",
          "form": "여기는 날씨가 맑아요ㅋ",
          "original_form": "여기는 날씨가 맑아요ㅋ",
          "speaker_id": "2",
          "time": "20210826 09:53"
        },
        {
          "id": "MDRW2100016116.1.3",
          "form": "33도래요.",
          "original_form": "33도래요.",
          "speaker_id": "2",
          "time": "20210826 09:53"
        },
        {
          "id": "MDRW2100016116.1.4",
          "form": "비가 그치자마자 덥네요ㅋㅋ",
          "original_form": "비가 그치자마자 덥네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:54"
        },
        {
          "id": "MDRW2100016116.1.5",
          "form": "네 오늘 너무 덥네요. ㅠㅠ ㅋㅋㅋ 어제 처럼 쌀쌀할줄 알고",
          "original_form": "네 오늘 너무 덥네요. ㅠㅠ ㅋㅋㅋ 어제 처럼 쌀쌀할줄 알고",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016116.1.6",
          "form": "애들 옷....긴바지 입혔는데....실수했어요.",
          "original_form": "애들 옷....긴바지 입혔는데....실수했어요.",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016116.1.7",
          "form": "에궁... 그래도 실내에선 괜찮을거에요",
          "original_form": "에궁... 그래도 실내에선 괜찮을거에요",
          "speaker_id": "1",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016116.1.8",
          "form": "넨네 에어컨있어서 괜찮을것같아요.",
          "original_form": "넨네 에어컨있어서 괜찮을것같아요.",
          "speaker_id": "2",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100016116.1.9",
          "form": "아침 저녁엔 시원한데 낮엔 너무 더운거 같아요",
          "original_form": "아침 저녁엔 시원한데 낮엔 너무 더운거 같아요",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016116.1.10",
          "form": "맞아요 ㅠㅠ ㅋㅋㅋ 이제 가을이 온것같네요.",
          "original_form": "맞아요 ㅠㅠ ㅋㅋㅋ 이제 가을이 온것같네요.",
          "speaker_id": "2",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016116.1.11",
          "form": "해도 짧아졌어요",
          "original_form": "해도 짧아졌어요",
          "speaker_id": "2",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016116.1.12",
          "form": "더위가 덜 해져서 좋긴 하네요",
          "original_form": "더위가 덜 해져서 좋긴 하네요",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016116.1.13",
          "form": "네 너무 더운것도 싫었는데 좋아요 ㅋㅋ",
          "original_form": "네 너무 더운것도 싫었는데 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016116.1.14",
          "form": "놀러가기도 좋을 날씨인데 코로나 때문에 아쉬워요.",
          "original_form": "놀러가기도 좋을 날씨인데 코로나 때문에 아쉬워요.",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016116.1.15",
          "form": "맞아요 ㅠㅠㅠㅠ 아 정말 코로나때문에 너무 아쉽고 슬프네요",
          "original_form": "맞아요 ㅠㅠㅠㅠ 아 정말 코로나때문에 너무 아쉽고 슬프네요",
          "speaker_id": "2",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016116.1.16",
          "form": "진짜루요ㅠㅠ 넘 놀러가고 싶은데",
          "original_form": "진짜루요ㅠㅠ 넘 놀러가고 싶은데",
          "speaker_id": "1",
          "time": "20210826 11:07"
        }
      ]
