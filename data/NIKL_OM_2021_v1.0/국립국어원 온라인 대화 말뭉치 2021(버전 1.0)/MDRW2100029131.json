      "utterance": [
        {
          "id": "MDRW2100029131.1.1",
          "form": "소가전은 진짜 살만해",
          "original_form": "소가전은 진짜 살만해",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029131.1.2",
          "form": "좋아하는 물건 있어?",
          "original_form": "좋아하는 물건 있어?",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029131.1.3",
          "form": "특히 모으는거 같은거",
          "original_form": "특히 모으는거 같은거",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029131.1.4",
          "form": "나 약간 절판된 책..? 같은거 괜히 중고로 사는거 좋아해ㅋㅋ",
          "original_form": "나 약간 절판된 책..? 같은거 괜히 중고로 사는거 좋아해ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100029131.1.5",
          "form": "너는그런거 있어?",
          "original_form": "너는그런거 있어?",
          "speaker_id": "2",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100029131.1.6",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029131.1.7",
          "form": "나는 담요를 좋아하는거같아",
          "original_form": "나는 담요를 좋아하는거같아",
          "speaker_id": "1",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029131.1.8",
          "form": "있는데 자꾸 사ㅋㅋㅋ",
          "original_form": "있는데 자꾸 사ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029131.1.9",
          "form": "패턴 같은거 상관없이 그냥 다 사는거야??",
          "original_form": "패턴 같은거 상관없이 그냥 다 사는거야??",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029131.1.10",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029131.1.11",
          "form": "이쁜 패턴을 사",
          "original_form": "이쁜 패턴을 사",
          "speaker_id": "1",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029131.1.12",
          "form": "귀여운거 좋아해??",
          "original_form": "귀여운거 좋아해??",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029131.1.13",
          "form": "귀여운거보단",
          "original_form": "귀여운거보단",
          "speaker_id": "1",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029131.1.14",
          "form": "깔끔? 정갈한게좋아",
          "original_form": "깔끔? 정갈한게좋아",
          "speaker_id": "1",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029131.1.15",
          "form": "제일 최근에 산건 어떤 패턴이었어??",
          "original_form": "제일 최근에 산건 어떤 패턴이었어??",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100029131.1.16",
          "form": "체크!",
          "original_form": "체크!",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029131.1.17",
          "form": "파란체크야",
          "original_form": "파란체크야",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029131.1.18",
          "form": "귀여워ㅎㅎ",
          "original_form": "귀여워ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029131.1.19",
          "form": "상상되는것 같다ㅋㅋㅋ",
          "original_form": "상상되는것 같다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029131.1.20",
          "form": "진짜 내가 모으고 싶은거 살때면... 그냥 아무생각없이 사게돼서 통장이 텅텅 비어 ㅋㅋㅠ",
          "original_form": "진짜 내가 모으고 싶은거 살때면... 그냥 아무생각없이 사게돼서 통장이 텅텅 비어 ㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029131.1.21",
          "form": "ㅋㅋㅋㅋ맞아",
          "original_form": "ㅋㅋㅋㅋ맞아",
          "speaker_id": "1",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029131.1.22",
          "form": "근데 뭐 그런 재미라도 있어야",
          "original_form": "근데 뭐 그런 재미라도 있어야",
          "speaker_id": "1",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029131.1.23",
          "form": "살맛나는거지",
          "original_form": "살맛나는거지",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029131.1.24",
          "form": "맞아.... 돈 쓸때 약간 짜릿한거ㅋㅋㅋㅋㅋ",
          "original_form": "맞아.... 돈 쓸때 약간 짜릿한거ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029131.1.25",
          "form": "난 단거 좋아해서 마카롱 같은것도 사는데",
          "original_form": "난 단거 좋아해서 마카롱 같은것도 사는데",
          "speaker_id": "2",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029131.1.26",
          "form": "세번 먹으면...3천원이 그냥 사라지니까",
          "original_form": "세번 먹으면...3천원이 그냥 사라지니까",
          "speaker_id": "2",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029131.1.27",
          "form": "사치부릴때 사먹어",
          "original_form": "사치부릴때 사먹어",
          "speaker_id": "2",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029131.1.28",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029131.1.29",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029131.1.30",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029131.1.31",
          "form": "마카롱 맛있는데 좀비싸ㅜ",
          "original_form": "마카롱 맛있는데 좀비싸ㅜ",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029131.1.32",
          "form": "그래도 가게 지나갈때면 방앗간처럼 가서 사고있더라....",
          "original_form": "그래도 가게 지나갈때면 방앗간처럼 가서 사고있더라....",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029131.1.33",
          "form": "단거너무 좋아ㅋㅋㅋ",
          "original_form": "단거너무 좋아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029131.1.34",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029131.1.35",
          "form": "난 디저트잘안먹거든?",
          "original_form": "난 디저트잘안먹거든?",
          "speaker_id": "1",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029131.1.36",
          "form": "근데 마카롱은 가끔 사",
          "original_form": "근데 마카롱은 가끔 사",
          "speaker_id": "1",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029131.1.37",
