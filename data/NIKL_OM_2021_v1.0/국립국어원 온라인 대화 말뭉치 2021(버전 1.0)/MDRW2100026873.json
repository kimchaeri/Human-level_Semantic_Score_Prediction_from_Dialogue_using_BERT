      "utterance": [
        {
          "id": "MDRW2100026873.1.1",
          "form": "사고싶은물건 있나요",
          "original_form": "사고싶은물건 있나요",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026873.1.2",
          "form": "요즘 구매욕이 부쩍 줄었어요",
          "original_form": "요즘 구매욕이 부쩍 줄었어요",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026873.1.3",
          "form": "에어프라이기를 사고 싶습니다",
          "original_form": "에어프라이기를 사고 싶습니다",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026873.1.4",
          "form": "만두가 잘 구워진다고 합니다",
          "original_form": "만두가 잘 구워진다고 합니다",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026873.1.5",
          "form": "에프 좋지요",
          "original_form": "에프 좋지요",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026873.1.6",
          "form": "에프관련 요리책도 같이사세요",
          "original_form": "에프관련 요리책도 같이사세요",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026873.1.7",
          "form": "옷 구매욕은 줄었어요",
          "original_form": "옷 구매욕은 줄었어요",
          "speaker_id": "2",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026873.1.8",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026873.1.9",
          "form": "화장품도 안삽니다",
          "original_form": "화장품도 안삽니다",
          "speaker_id": "2",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026873.1.10",
          "form": "화장품도요",
          "original_form": "화장품도요",
          "speaker_id": "1",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026873.1.11",
          "form": "마스크껴서 안보이니",
          "original_form": "마스크껴서 안보이니",
          "speaker_id": "1",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026873.1.12",
          "form": "앵갤지수만 높아갑니다",
          "original_form": "앵갤지수만 높아갑니다",
          "speaker_id": "2",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026873.1.13",
          "form": "화장품을 덜쓰게되더라구요",
          "original_form": "화장품을 덜쓰게되더라구요",
          "speaker_id": "1",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026873.1.14",
          "form": "돈은 빼고 살은 찌지요",
          "original_form": "돈은 빼고 살은 찌지요",
          "speaker_id": "2",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026873.1.15",
          "form": "먹다보니 사이즈가 늘어나네요",
          "original_form": "먹다보니 사이즈가 늘어나네요",
          "speaker_id": "1",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026873.1.16",
          "form": "내일은 무얼살까요?",
          "original_form": "내일은 무얼살까요?",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026873.1.17",
          "form": "먹을걸 삽시다",
          "original_form": "먹을걸 삽시다",
          "speaker_id": "1",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026873.1.18",
          "form": "생수를 사야하는데",
          "original_form": "생수를 사야하는데",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026873.1.19",
          "form": "무겁다고 12개만 배달해줍니다",
          "original_form": "무겁다고 12개만 배달해줍니다",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026873.1.20",
          "form": "요즘은 배달이 잘되어서 데워먹음 편하더라구요",
          "original_form": "요즘은 배달이 잘되어서 데워먹음 편하더라구요",
          "speaker_id": "1",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026873.1.21",
          "form": "아이고 여러군데서",
          "original_form": "아이고 여러군데서",
          "speaker_id": "1",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026873.1.22",
          "form": "밀키트도 좋지요",
          "original_form": "밀키트도 좋지요",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026873.1.23",
          "form": "시켜야겠네요",
          "original_form": "시켜야겠네요",
          "speaker_id": "1",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026873.1.24",
          "form": "밀키트 편리해요",
          "original_form": "밀키트 편리해요",
          "speaker_id": "1",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026873.1.25",
          "form": "물만 배달하기엔 미안해서 이것저것 사야해요",
          "original_form": "물만 배달하기엔 미안해서 이것저것 사야해요",
          "speaker_id": "2",
          "time": "20210902 00:51"
        }
      ]
