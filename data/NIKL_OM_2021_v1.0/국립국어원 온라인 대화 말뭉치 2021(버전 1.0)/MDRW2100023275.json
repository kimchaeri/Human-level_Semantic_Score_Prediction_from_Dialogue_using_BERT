      "utterance": [
        {
          "id": "MDRW2100023275.1.1",
          "form": "안녕하세요,  스포츠 경기 좋아하시나요?",
          "original_form": "안녕하세요,  스포츠 경기 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 23:34"
        },
        {
          "id": "MDRW2100023275.1.2",
          "form": "네 좋아합니다 야구요 ㅎㅎ",
          "original_form": "네 좋아합니다 야구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 23:39"
        },
        {
          "id": "MDRW2100023275.1.3",
          "form": "직관도 자주 가시나요?",
          "original_form": "직관도 자주 가시나요?",
          "speaker_id": "1",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023275.1.4",
          "form": "요즘은 못가구",
          "original_form": "요즘은 못가구",
          "speaker_id": "2",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023275.1.5",
          "form": "예전엔 가끔 갔어요",
          "original_form": "예전엔 가끔 갔어요",
          "speaker_id": "2",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023275.1.6",
          "form": "코로나 때문이죠...ㅠ 응원하시는 팀이 있으신가요?",
          "original_form": "코로나 때문이죠...ㅠ 응원하시는 팀이 있으신가요?",
          "speaker_id": "1",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100023275.1.7",
          "form": "전 ssg요",
          "original_form": "전 ssg요",
          "speaker_id": "2",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100023275.1.8",
          "form": "이번 올림픽 야구팀에도 그 팀 선수분이 있었나여?",
          "original_form": "이번 올림픽 야구팀에도 그 팀 선수분이 있었나여?",
          "speaker_id": "1",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023275.1.9",
          "form": "음 있었는데",
          "original_form": "음 있었는데",
          "speaker_id": "2",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100023275.1.10",
          "form": "부상당해서",
          "original_form": "부상당해서",
          "speaker_id": "2",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100023275.1.11",
          "form": "못나왔....",
          "original_form": "못나왔....",
          "speaker_id": "2",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100023275.1.12",
          "form": "아이고 부상....ㅠㅠ 시즌 아웃까진 아니시죠??",
          "original_form": "아이고 부상....ㅠㅠ 시즌 아웃까진 아니시죠??",
          "speaker_id": "1",
          "time": "20210829 23:57"
        },
        {
          "id": "MDRW2100023275.1.13",
          "form": "지금은 뛰고있어요",
          "original_form": "지금은 뛰고있어요",
          "speaker_id": "2",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100023275.1.14",
          "form": "야구는 벌써 시즌 시작인가여?",
          "original_form": "야구는 벌써 시즌 시작인가여?",
          "speaker_id": "1",
          "time": "20210830 00:02"
        },
        {
          "id": "MDRW2100023275.1.15",
          "form": "이미 시작한지 오래됐져 ㅎ",
          "original_form": "이미 시작한지 오래됐져 ㅎ",
          "speaker_id": "2",
          "time": "20210830 05:51"
        },
        {
          "id": "MDRW2100023275.1.16",
          "form": "그럼 시즌 중에 추출되어 올림픽을 나간거네요...",
          "original_form": "그럼 시즌 중에 추출되어 올림픽을 나간거네요...",
          "speaker_id": "1",
          "time": "20210830 11:02"
        },
        {
          "id": "MDRW2100023275.1.17",
          "form": "네 마자요",
          "original_form": "네 마자요",
          "speaker_id": "2",
          "time": "20210830 11:08"
        },
        {
          "id": "MDRW2100023275.1.18",
          "form": "그래서 야구 시즌 3주 쉬었죠",
