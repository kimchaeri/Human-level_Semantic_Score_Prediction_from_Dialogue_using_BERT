      "utterance": [
        {
          "id": "MDRW2100041999.1.1",
          "form": "그치만 name2님 애인은 아직 학생이니까 봐줍니다",
          "original_form": "그치만 &name2&님 애인은 아직 학생이니까 봐줍니다",
          "speaker_id": "1",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041999.1.2",
          "form": "얘는 아버지 기업 물려받을거 같지 않나요",
          "original_form": "얘는 아버지 기업 물려받을거 같지 않나요",
          "speaker_id": "2",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100041999.1.3",
          "form": "대학교는 약간.. 일탈이죠",
          "original_form": "대학교는 약간.. 일탈이죠",
          "speaker_id": "2",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100041999.1.4",
          "form": "허억.....",
          "original_form": "허억.....",
          "speaker_id": "1",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100041999.1.5",
          "form": "부자애인.... 넘 좋다",
          "original_form": "부자애인.... 넘 좋다",
          "speaker_id": "1",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100041999.1.6",
          "form": "저는 재민이가 걱정돼요",
          "original_form": "저는 재민이가 걱정돼요",
          "speaker_id": "1",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100041999.1.7",
          "form": "약간...유사육아? 같은 연애인듯",
          "original_form": "약간...유사육아? 같은 연애인듯",
          "speaker_id": "1",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100041999.1.8",
          "form": "남매같은 그런..... 이상한 연애",
          "original_form": "남매같은 그런..... 이상한 연애",
          "speaker_id": "1",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041999.1.9",
          "form": "유사육아 아닐까요..",
          "original_form": "유사육아 아닐까요..",
          "speaker_id": "2",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041999.1.10",
          "form": "재민아 니가 잘해야지..",
          "original_form": "재민아 니가 잘해야지..",
          "speaker_id": "2",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041999.1.11",
          "form": "맞아요 재민아 너가 아양을 떨어봐라",
          "original_form": "맞아요 재민아 너가 아양을 떨어봐라",
          "speaker_id": "2",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100041999.1.12",
          "form": "아 근데 재현이 되게 아양떠는거 잘할거같지 않나요",
          "original_form": "아 근데 재현이 되게 아양떠는거 잘할거같지 않나요",
          "speaker_id": "2",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100041999.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 웃겨 애교머신 재민~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 웃겨 애교머신 재민~",
          "speaker_id": "1",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100041999.1.14",
          "form": "재현이는 아양을 잘떨겠죠",
          "original_form": "재현이는 아양을 잘떨겠죠",
          "speaker_id": "1",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100041999.1.15",
          "form": "약간 재현이는~ 하고 약간 현타 맞지만서도",
          "original_form": "약간 재현이는~ 하고 약간 현타 맞지만서도",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100041999.1.16",
          "form": "곧잘 할것같은 느낌인데",
          "original_form": "곧잘 할것같은 느낌인데",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100041999.1.17",
          "form": "재민이는 애교 부리면 현타가 더 심할것같은...",
          "original_form": "재민이는 애교 부리면 현타가 더 심할것같은...",
          "speaker_id": "1",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100041999.1.18",
          "form": "근데 저도 애교 좀 그래용",
          "original_form": "근데 저도 애교 좀 그래용",
          "speaker_id": "1",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100041999.1.19",
          "form": "재민이 하기전부터 현타와서",
          "original_form": "재민이 하기전부터 현타와서",
          "speaker_id": "2",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100041999.1.20",
          "form": "재민이의 후응~ 애교?",
          "original_form": "재민이의 후응~{emoji:☆} 애교?",
          "speaker_id": "1",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100041999.1.21",
          "form": "아니 잠깐만 하..,  아니.. 그냥 하지말자 이럴거같긴해요",
          "original_form": "아니 잠깐만 하..,  아니.. 그냥 하지말자 이럴거같긴해요",
          "speaker_id": "2",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100041999.1.22",
          "form": "애인이지만 너무 부담스럽...",
          "original_form": "애인이지만 너무 부담스럽...",
          "speaker_id": "1",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100041999.1.23",
          "form": "정재현 은근 사람 놀리는거 좋아하더라고요??",
          "original_form": "정재현 은근 사람 놀리는거 좋아하더라고요??",
          "speaker_id": "2",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100041999.1.24",
          "form": "헐~",
          "original_form": "헐~",
          "speaker_id": "1",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100041999.1.25",
          "form": "애인 놀리는데도 써먹나요???",
          "original_form": "애인 놀리는데도 써먹나요???",
