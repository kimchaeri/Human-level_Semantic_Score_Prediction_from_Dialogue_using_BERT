      "utterance": [
        {
          "id": "MDRW2100030623.1.1",
          "form": "유혈충돌없이 굿 마무리!",
          "original_form": "유혈충돌없이 굿 마무리!",
          "speaker_id": "2",
          "time": "20210904 10:32"
        },
        {
          "id": "MDRW2100030623.1.2",
          "form": "저런게 외교고 협상이죠",
          "original_form": "저런게 외교고 협상이죠",
          "speaker_id": "1",
          "time": "20210904 10:33"
        },
        {
          "id": "MDRW2100030623.1.3",
          "form": "저런 나쁜 놈은 죽여버려야는데",
          "original_form": "저런 나쁜 놈은 죽여버려야는데",
          "speaker_id": "1",
          "time": "20210904 10:33"
        },
        {
          "id": "MDRW2100030623.1.4",
          "form": "앗 아픈 사람이 너무 무리했어ㅜㅜ",
          "original_form": "앗 아픈 사람이 너무 무리했어ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 10:34"
        },
        {
          "id": "MDRW2100030623.1.5",
          "form": "저 두 왕이 살아있었다면 실제 역사도 좀 달라졌겠죠",
          "original_form": "저 두 왕이 살아있었다면 실제 역사도 좀 달라졌겠죠",
          "speaker_id": "1",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100030623.1.6",
          "form": "그쵸그쵸 영화적 상상력이 가미되었지만 살라딘 같은 걸출한 군주가 더 일찍 더 오래 군림했더라면",
          "original_form": "그쵸그쵸 영화적 상상력이 가미되었지만 살라딘 같은 걸출한 군주가 더 일찍 더 오래 군림했더라면",
          "speaker_id": "2",
          "time": "20210904 10:37"
        },
        {
          "id": "MDRW2100030623.1.7",
          "form": "배우들 눈이  참 깊어요 어찌 저런 연기를 할 수 있는지",
          "original_form": "배우들 눈이  참 깊어요 어찌 저런 연기를 할 수 있는지",
          "speaker_id": "1",
          "time": "20210904 10:37"
        },
        {
          "id": "MDRW2100030623.1.8",
          "form": "역사가 바뀌었을듯해요",
          "original_form": "역사가 바뀌었을듯해요",
          "speaker_id": "2",
          "time": "20210904 10:38"
        },
        {
          "id": "MDRW2100030623.1.9",
          "form": "맞아요 볼드윈4세도 같이",
          "original_form": "맞아요 볼드윈4세도 같이",
          "speaker_id": "1",
          "time": "20210904 10:38"
        },
        {
          "id": "MDRW2100030623.1.10",
          "form": "전쟁이 더 빨리 끝났을텐데",
          "original_form": "전쟁이 더 빨리 끝났을텐데",
          "speaker_id": "2",
          "time": "20210904 10:38"
        },
        {
          "id": "MDRW2100030623.1.11",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210904 10:39"
        },
        {
          "id": "MDRW2100030623.1.12",
          "form": "다시봐도 재밌네용",
          "original_form": "다시봐도 재밌네용",
          "speaker_id": "2",
          "time": "20210904 10:39"
        },
        {
          "id": "MDRW2100030623.1.13",
          "form": "알려주셔서 감사해요",
          "original_form": "알려주셔서 감사해요",
          "speaker_id": "2",
          "time": "20210904 10:39"
        },
        {
          "id": "MDRW2100030623.1.14",
          "form": "나도 같이 보니 더 재밌네요",
          "original_form": "나도 같이 보니 더 재밌네요",
          "speaker_id": "1",
          "time": "20210904 10:40"
        },
        {
          "id": "MDRW2100030623.1.15",
          "form": "히히히 말 편하게 하셔도 돼요",
          "original_form": "히히히 말 편하게 하셔도 돼요",
          "speaker_id": "2",
          "time": "20210904 10:40"
        }
      ]
