      "utterance": [
        {
          "id": "MDRW2100007370.1.1",
          "form": "그림 전시하는거",
          "original_form": "그림 전시하는거",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007370.1.2",
          "form": "법인에서",
          "original_form": "법인에서",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007370.1.3",
          "form": "우와... 진짜 대박이네",
          "original_form": "우와... 진짜 대박이네",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007370.1.4",
          "form": "어디 그것도 내나",
          "original_form": "어디 그것도 내나",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007370.1.5",
          "form": "엑스코 거기서 하시는거야?",
          "original_form": "엑스코 거기서 하시는거야?",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007370.1.6",
          "form": "그래서 우리학교 미대교수랑도 컨텍하고",
          "original_form": "그래서 우리학교 미대교수랑도 컨텍하고",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007370.1.7",
          "form": "음음 그걵나중에알려줄게",
          "original_form": "음음 그걵나중에알려줄게",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007370.1.8",
          "form": "앗 조아조아",
          "original_form": "앗 조아조아",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007370.1.9",
          "form": "아무튼 서류작성에서 사무보조역할을했어",
          "original_form": "아무튼 서류작성에서 사무보조역할을했어",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007370.1.10",
          "form": "말  조심쓰",
          "original_form": "말  조심쓰",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007370.1.11",
          "form": "전시회는원래했어",
          "original_form": "전시회는원래했어",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007370.1.12",
          "form": "ㅋㅋㅋㅋㅋ대단한 일을 맡았군!",
          "original_form": "ㅋㅋㅋㅋㅋ대단한 일을 맡았군!",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007370.1.13",
          "form": "재작년에도",
          "original_form": "재작년에도",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007370.1.14",
          "form": "헐 난 몰랐어...",
          "original_form": "헐 난 몰랐어...",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007370.1.15",
          "form": "난 왜 몰랐지...??!",
          "original_form": "난 왜 몰랐지...??!",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007370.1.16",
          "form": "대단하시다 진짜",
          "original_form": "대단하시다 진짜",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007370.1.17",
          "form": "내가안갔어",
          "original_form": "내가안갔어",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007370.1.18",
          "form": "나도안갔어",
          "original_form": "나도안갔어",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007370.1.19",
          "form": "그냥 내가관심이없었어",
          "original_form": "그냥 내가관심이없었어",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007370.1.20",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007370.1.21",
          "form": "근데 사실 전시회는",
          "original_form": "근데 사실 전시회는",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007370.1.22",
          "form": "취미가 있는 사람들이 주로 가긴해...",
          "original_form": "취미가 있는 사람들이 주로 가긴해...",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007370.1.23",
          "form": "나도 name3이라는 친구가",
          "original_form": "나도 &name3&이라는 친구가",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007370.1.24",
          "form": "그림이니까...아무래도",
          "original_form": "그림이니까...아무래도",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007370.1.25",
          "form": "전시회나 미술관가자고 하면",
          "original_form": "전시회나 미술관가자고 하면",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007370.1.26",
          "form": "좀 짜증나고 귀찮고 그랬어!",
          "original_form": "좀 짜증나고 귀찮고 그랬어!",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007370.1.27",
          "form": "그래도갔지?",
          "original_form": "그래도갔지?",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007370.1.28",
          "form": "난 예술 감각이 없어서인지",
          "original_form": "난 예술 감각이 없어서인지",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007370.1.29",
          "form": "넌참 좋은친구야",
          "original_form": "넌참 좋은친구야",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007370.1.30",
          "form": "아니 혼자 시골에서 가라고 햇어",
          "original_form": "아니 혼자 시골에서 가라고 햇어",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007370.1.31",
          "form": "청송의전시회였니?",
          "original_form": "청송의전시회였니?",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007370.1.32",
          "form": "안가더라",
          "original_form": "안가더라",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007370.1.33",
          "form": "아악!왜 말해!",
          "original_form": "아악!왜 말해!",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007370.1.34",
          "form": "지역 왜 말해!",
          "original_form": "지역 왜 말해!",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007370.1.35",
          "form": "저 친구가 보면 어쩔거야!!",
          "original_form": "저 친구가 보면 어쩔거야!!",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007370.1.36",
          "form": "나인거 들키자나!!",
          "original_form": "나인거 들키자나!!",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007370.1.37",
          "form": "저친구가볼일없어",
          "original_form": "저친구가볼일없어",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007370.1.38",
          "form": "없어!!!",
          "original_form": "없어!!!",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007370.1.39",
          "form": "왜!!",
          "original_form": "왜!!",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007370.1.40",
          "form": "아무투전시회하면놀러와",
          "original_form": "아무투전시회하면놀러와",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007370.1.41",
          "form": "책으로 나올수도 있다며!!",
          "original_form": "책으로 나올수도 있다며!!",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007370.1.42",
          "form": "이번엔나도갈거야",
          "original_form": "이번엔나도갈거야",
          "speaker_id": "1",
          "time": "20210822 01:42"
        }
      ]
    }
