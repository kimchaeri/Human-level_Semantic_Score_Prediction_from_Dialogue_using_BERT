      "utterance": [
        {
          "id": "MDRW2100034894.1.1",
          "form": "중고거래는 하시나요?",
          "original_form": "중고거래는 하시나요?",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034894.1.2",
          "form": "가끔 중고책은 알라딘 팔고",
          "original_form": "가끔 중고책은 알라딘 팔고",
          "speaker_id": "2",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100034894.1.3",
          "form": "당근은 약속잡는게 은근 귀찮아서 ㅋㅋㅋ",
          "original_form": "당근은 약속잡는게 은근 귀찮아서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100034894.1.4",
          "form": "안하게되더라구요",
          "original_form": "안하게되더라구요",
          "speaker_id": "2",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100034894.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 약속잡는게 은근 번거롭더라구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 약속잡는게 은근 번거롭더라구요",
          "speaker_id": "1",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100034894.1.6",
          "form": "자주하시나요?",
          "original_form": "자주하시나요?",
          "speaker_id": "2",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100034894.1.7",
          "form": "아주 가끔씩 대대적으로 집정리할때 필요없다 싶은 물건들 올려요ㅋㅋㅋㅋ",
          "original_form": "아주 가끔씩 대대적으로 집정리할때 필요없다 싶은 물건들 올려요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100034894.1.8",
          "form": "ㅋㅋㅋㅋ이런것도 팔리나 싶은 것들도 팔리니까 신기하더라구요",
          "original_form": "ㅋㅋㅋㅋ이런것도 팔리나 싶은 것들도 팔리니까 신기하더라구요",
          "speaker_id": "2",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100034894.1.9",
          "form": "아 맞아요 ㅋㅋㅋㅋㅋㅋ 이건 진짜 누구든 당장 사가겟다 싶은게 의외로 오랫동안 안팔리고",
          "original_form": "아 맞아요 ㅋㅋㅋㅋㅋㅋ 이건 진짜 누구든 당장 사가겟다 싶은게 의외로 오랫동안 안팔리고",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100034894.1.10",
          "form": "이걸 누가 사갈까? 그냥 버릴까? 하면서 올린게 빨리 팔리기도 하더라구요 ㅋㅋㅋㅋ",
          "original_form": "이걸 누가 사갈까? 그냥 버릴까? 하면서 올린게 빨리 팔리기도 하더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100034894.1.11",
          "form": "그래서 사람들의 민심을 잘 못읽겟어요 당근ㅋㅋㅋㅌㅋ",
          "original_form": "그래서 사람들의 민심을 잘 못읽겟어요 당근ㅋㅋㅋㅌㅋ",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100034894.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저도요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저도요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100034894.1.13",
          "form": "은근히 어려운 당근 세계 ㅋㅋㅋㅋㅋ",
          "original_form": "은근히 어려운 당근 세계 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100034894.1.14",
          "form": "제친구는 뭐하냐 물어보면 맨날 당근 거래갔다고 그래서",
          "original_form": "제친구는 뭐하냐 물어보면 맨날 당근 거래갔다고 그래서",
          "speaker_id": "2",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100034894.1.15",
          "form": "존경스러워요 ㅋㅋㅋㅋㅋ",
          "original_form": "존경스러워요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100034894.1.16",
          "form": "친구가 진짜 부지런한가봐요 ㅋㅋㅋㅋ",
          "original_form": "친구가 진짜 부지런한가봐요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100034894.1.17",
          "form": "그러니까요 당근 저렇게 잘 이용하는 사람 첨봤어요",
          "original_form": "그러니까요 당근 저렇게 잘 이용하는 사람 첨봤어요",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100034894.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ당근이 중독성이 잇나봐요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ당근이 중독성이 잇나봐요",
          "speaker_id": "1",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100034894.1.19",
          "form": "원래 당근하기전에 중고나라랑 번개장터도 늘 하고 있었고든요 친구가",
          "original_form": "원래 당근하기전에 중고나라랑 번개장터도 늘 하고 있었고든요 친구가",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100034894.1.20",
          "form": "와 중고거래 초고수네요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 중고거래 초고수네요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100034894.1.21",
          "form": "네ㅋㅋㅋㅋ친구 별명 거상이에요",
          "original_form": "네ㅋㅋㅋㅋ친구 별명 거상이에요",
          "speaker_id": "2",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100034894.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ진짜 딱인 별명이네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ진짜 딱인 별명이네요",
