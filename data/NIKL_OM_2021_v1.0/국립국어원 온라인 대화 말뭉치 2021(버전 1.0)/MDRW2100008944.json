      "utterance": [
        {
          "id": "MDRW2100008944.1.1",
          "form": "모임해여?",
          "original_form": "모임해여?",
          "speaker_id": "1",
          "time": "20210822 19:51"
        },
        {
          "id": "MDRW2100008944.1.2",
          "form": "아뇨 따로 모임 참여해있는 곳은 없어요!",
          "original_form": "아뇨 따로 모임 참여해있는 곳은 없어요!",
          "speaker_id": "2",
          "time": "20210822 19:55"
        },
        {
          "id": "MDRW2100008944.1.3",
          "form": "저는 친구계모임요",
          "original_form": "저는 친구계모임요",
          "speaker_id": "1",
          "time": "20210822 19:56"
        },
        {
          "id": "MDRW2100008944.1.4",
          "form": "오 주말마다 모이시는건가요?",
          "original_form": "오 주말마다 모이시는건가요?",
          "speaker_id": "2",
          "time": "20210822 19:59"
        },
        {
          "id": "MDRW2100008944.1.5",
          "form": "이제는 못하죠",
          "original_form": "이제는 못하죠",
          "speaker_id": "1",
          "time": "20210822 20:26"
        },
        {
          "id": "MDRW2100008944.1.6",
          "form": "아 그렇죠 시국때문에 단체활동이 힘들어서 아쉬워요",
          "original_form": "아 그렇죠 시국때문에 단체활동이 힘들어서 아쉬워요",
          "speaker_id": "2",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100008944.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 20:35"
        },
        {
          "id": "MDRW2100008944.1.8",
          "form": "친구 보구 싶아요",
          "original_form": "친구 보구 싶아요",
          "speaker_id": "1",
          "time": "20210901 10:13"
        },
        {
          "id": "MDRW2100008944.1.9",
          "form": "저두요ㅠ 자유롭게 만나고 싶은데 제약이 걸려서 안타깝네요ㅠ",
          "original_form": "저두요ㅠ 자유롭게 만나고 싶은데 제약이 걸려서 안타깝네요ㅠ",
          "speaker_id": "2",
          "time": "20210901 12:38"
        },
        {
          "id": "MDRW2100008944.1.10",
          "form": "모이시면 특별히 뭐 하는 거 있으신가요?",
          "original_form": "모이시면 특별히 뭐 하는 거 있으신가요?",
          "speaker_id": "2",
          "time": "20210901 12:38"
        },
        {
          "id": "MDRW2100008944.1.11",
          "form": "대화조 ㅎㅎ",
          "original_form": "대화조 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 12:56"
        },
        {
          "id": "MDRW2100008944.1.12",
          "form": "아 그렇죠 거의 수다가 다죠ㅎㅎ",
          "original_form": "아 그렇죠 거의 수다가 다죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100008944.1.13",
          "form": "저도 친구 만나면 카페에 죽치고 수다만 떨다가 가요ㅎㅎ",
          "original_form": "저도 친구 만나면 카페에 죽치고 수다만 떨다가 가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 14:44"
        },
        {
          "id": "MDRW2100008944.1.14",
          "form": "스트레스 풀고",
          "original_form": "스트레스 풀고",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100008944.1.15",
          "form": "맞아요ㅋㅋㅋ 수다만 떨어도 재밌긴하니까 그나마 버티는 것 같아요",
          "original_form": "맞아요ㅋㅋㅋ 수다만 떨어도 재밌긴하니까 그나마 버티는 것 같아요",
          "speaker_id": "2",
