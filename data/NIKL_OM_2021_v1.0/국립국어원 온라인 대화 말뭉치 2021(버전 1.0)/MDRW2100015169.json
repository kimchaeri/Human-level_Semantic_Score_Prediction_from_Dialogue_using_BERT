        {
          "id": "MDRW2100015169.1.1",
          "form": "공부 좋아하세요?",
          "original_form": "공부 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015169.1.2",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "1",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015169.1.3",
          "form": "공부 좋아한다기보다 ㅋㅋㅋ 저는 수업 듣는거 자체는 좋아해요 지금도 ㅋㅋ",
          "original_form": "공부 좋아한다기보다 ㅋㅋㅋ 저는 수업 듣는거 자체는 좋아해요 지금도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015169.1.4",
          "form": "오잉?",
          "original_form": "오잉?",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015169.1.5",
          "form": "훌륭하세요",
          "original_form": "훌륭하세요",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015169.1.6",
          "form": "아 그냥 뭐 물어보고 듣고 이런거 좋아해서 ㅋㅋㅋ 그래서 선생님들이 저 공부잘하는 줄 오해하신 분이 계세요 ㅋㅋㅋ",
          "original_form": "아 그냥 뭐 물어보고 듣고 이런거 좋아해서 ㅋㅋㅋ 그래서 선생님들이 저 공부잘하는 줄 오해하신 분이 계세요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015169.1.7",
          "form": "탐구한다는 자체가 멋져요",
          "original_form": "탐구한다는 자체가 멋져요",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015169.1.8",
          "form": "엄지척",
          "original_form": "엄지척",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015169.1.9",
          "form": "네 ㅎㅎ 근데 책보는 건 싫어해요 ㅋㅋㅋ",
          "original_form": "네 ㅎㅎ 근데 책보는 건 싫어해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015169.1.10",
          "form": "책 좋아하는 사람이 신기ㅎ",
          "original_form": "책 좋아하는 사람이 신기ㅎ",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015169.1.11",
          "form": "ㅋㅋㅋㅋㅋ 그쵸.. 그럼 학교다닐때 무슨 낙으로 다니셨나요?",
          "original_form": "ㅋㅋㅋㅋㅋ 그쵸.. 그럼 학교다닐때 무슨 낙으로 다니셨나요?",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015169.1.12",
          "form": "그냥 학위따는 낙이요ㅎ",
          "original_form": "그냥 학위따는 낙이요ㅎ",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015169.1.13",
          "form": "ㅎㅎㅎ 뭐 전공하셨나요?",
          "original_form": "ㅎㅎㅎ 뭐 전공하셨나요?",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015169.1.14",
          "form": "저 교육이요ㅋㅋ",
          "original_form": "저 교육이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015169.1.15",
          "form": "아니 이런 반전이 ㅋㅋㅋ",
          "original_form": "아니 이런 반전이 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015169.1.16",
          "form": "그럼 교직이수 이런 것도 하신 건가요?",
          "original_form": "그럼 교직이수 이런 것도 하신 건가요?",
