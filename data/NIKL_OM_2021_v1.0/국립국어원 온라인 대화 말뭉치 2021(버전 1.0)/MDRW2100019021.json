        },
        {
          "id": "MDRW2100019021.1.3",
          "form": "밥드셨나요",
          "original_form": "밥드셨나요",
          "speaker_id": "2",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019021.1.4",
          "form": "아직 식전입니다",
          "original_form": "아직 식전입니다",
          "speaker_id": "1",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019021.1.5",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "1",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019021.1.6",
          "form": "저도 아직이요",
          "original_form": "저도 아직이요",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019021.1.7",
          "form": "너무 배고파요",
          "original_form": "너무 배고파요",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019021.1.8",
          "form": "아직 드시지않은 이유 여쭤도 되겠습니까",
          "original_form": "아직 드시지않은 이유 여쭤도 되겠습니까",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019021.1.9",
          "form": "배달음식 시켜먹고싶은데",
          "original_form": "배달음식 시켜먹고싶은데",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019021.1.10",
          "form": "뭐먹을지 모르겠어요",
          "original_form": "뭐먹을지 모르겠어요",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019021.1.11",
          "form": "추천해주세요",
          "original_form": "추천해주세요",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019021.1.12",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019021.1.13",
          "form": "뭐 좋아하시나요",
          "original_form": "뭐 좋아하시나요",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019021.1.14",
          "form": "양식 일식이요 분식도 좋아해요",
          "original_form": "양식 일식이요 분식도 좋아해요",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019021.1.15",
          "form": "골고루 다 좋아하시는구나",
          "original_form": "골고루 다 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019021.1.16",
          "form": "음.. 더 어려운걸요 면을 선호하세요 밥을 선호하세요",
          "original_form": "음.. 더 어려운걸요 면을 선호하세요 밥을 선호하세요",
          "speaker_id": "1",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019021.1.17",
          "form": "이것도 둘 다 좋아하는데.. 밥보단 면을 더 좋아해요",
          "original_form": "이것도 둘 다 좋아하는데.. 밥보단 면을 더 좋아해요",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019021.1.18",
          "form": "음 면을 더 좋아한다라",
          "original_form": "음 면을 더 좋아한다라",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019021.1.19",
          "form": "그렇다면 면이 매콤한게 좋은가요  부드러운게 좋은가요",
          "original_form": "그렇다면 면이 매콤한게 좋은가요  부드러운게 좋은가요",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019021.1.20",
          "form": "둘 다 좋으면 어렵나요?",
          "original_form": "둘 다 좋으면 어렵나요?",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019021.1.21",
          "form": "나 둘 다 좋은데",
          "original_form": "나 둘 다 좋은데",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019021.1.22",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019021.1.23",
          "form": "로제파스타를 추천드립니다",
          "original_form": "로제파스타를 추천드립니다",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019021.1.24",
          "form": "부드럽고 매콤한 면",
          "original_form": "부드럽고 매콤한 면",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019021.1.25",
          "form": "오 로제파스타",
          "original_form": "오 로제파스타",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019021.1.26",
          "form": "진짜 좋아해요",
          "original_form": "진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019021.1.27",
          "form": "로제떡볶이도 맛있던데",
          "original_form": "로제떡볶이도 맛있던데",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019021.1.28",
          "form": "드셔보셨나요",
          "original_form": "드셔보셨나요",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019021.1.29",
          "form": "떡볶이는 안먹어보고",
          "original_form": "떡볶이는 안먹어보고",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019021.1.30",
          "form": "파스타는 먹어봤어요",
