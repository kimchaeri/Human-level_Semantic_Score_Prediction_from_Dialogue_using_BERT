      "utterance": [
        {
          "id": "MDRW2100010114.1.1",
          "form": "곰표 맥주 알아?",
          "original_form": "곰표 맥주 알아?",
          "speaker_id": "1",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010114.1.2",
          "form": "이거 맥주야?",
          "original_form": "이거 맥주야?",
          "speaker_id": "2",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010114.1.3",
          "form": "응 근데 겉포장이 곰표로 되어있어",
          "original_form": "응 근데 겉포장이 곰표로 되어있어",
          "speaker_id": "1",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010114.1.4",
          "form": "곰표가 머야?",
          "original_form": "곰표가 머야?",
          "speaker_id": "2",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100010114.1.5",
          "form": "시멘트 회사일걸?",
          "original_form": "시멘트 회사일걸?",
          "speaker_id": "1",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100010114.1.6",
          "form": "아 곰표 맥주 먹어봤어?",
          "original_form": "아 곰표 맥주 먹어봤어?",
          "speaker_id": "2",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100010114.1.7",
          "form": "아니 근데 먹어보고싶어",
          "original_form": "아니 근데 먹어보고싶어",
          "speaker_id": "1",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100010114.1.8",
          "form": "진로는 먹어봤어?",
          "original_form": "진로는 먹어봤어?",
          "speaker_id": "2",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010114.1.9",
          "form": "아니 안먹어봤어",
          "original_form": "아니 안먹어봤어",
          "speaker_id": "1",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010114.1.10",
          "form": "나 먹어봤어",
          "original_form": "나 먹어봤어",
          "speaker_id": "2",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010114.1.11",
          "form": "아 그래? 맛이 어땠어?",
          "original_form": "아 그래? 맛이 어땠어?",
          "speaker_id": "1",
          "time": "20210823 14:33"
        },
        {
          "id": "MDRW2100010114.1.12",
          "form": "알코올램프 맛 나",
          "original_form": "알코올램프 맛 나",
          "speaker_id": "2",
          "time": "20210823 14:33"
        },
        {
          "id": "MDRW2100010114.1.13",
          "form": "ㅋㅋㅋㅋㅋ알콜램프맛은 뭐지",
