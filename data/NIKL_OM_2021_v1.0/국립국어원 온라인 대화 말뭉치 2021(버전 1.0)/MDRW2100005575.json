      "utterance": [
        {
          "id": "MDRW2100005575.1.1",
          "form": "요즘 유튜브만 봄",
          "original_form": "요즘 유튜브만 봄",
          "speaker_id": "2",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005575.1.2",
          "form": "유튜브 뭐봐",
          "original_form": "유튜브 뭐봐",
          "speaker_id": "1",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005575.1.3",
          "form": "먹방",
          "original_form": "먹방",
          "speaker_id": "2",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005575.1.4",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005575.1.5",
          "form": "다이어트 하면서 처음으로 대리만족함",
          "original_form": "다이어트 하면서 처음으로 대리만족함",
          "speaker_id": "1",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005575.1.6",
          "form": "예전엔 거의 배달 메뉴 고르기였는데",
          "original_form": "예전엔 거의 배달 메뉴 고르기였는데",
          "speaker_id": "1",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005575.1.7",
          "form": "보면 더 먹고 싶어지는 게 당연하잖으아",
          "original_form": "보면 더 먹고 싶어지는 게 당연하잖으아",
          "speaker_id": "1",
          "time": "20210821 23:09"
        },
        {
          "id": "MDRW2100005575.1.8",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005575.1.9",
          "form": "요즘 마라탕 영상 찾아보는데",
          "original_form": "요즘 마라탕 영상 찾아보는데",
          "speaker_id": "1",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005575.1.10",
          "form": "별로 만족스러운 영상이 없어",
          "original_form": "별로 만족스러운 영상이 없어",
          "speaker_id": "1",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005575.1.11",
          "form": "마라탕은 먹방으로 본 기억이 없음",
          "original_form": "마라탕은 먹방으로 본 기억이 없음",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005575.1.12",
          "form": "난 맨날 ㄱ가정식 찾아봐",
          "original_form": "난 맨날 ㄱ가정식 찾아봐",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005575.1.13",
          "form": "밥 햄 김 김치찜 이런 거",
          "original_form": "밥 햄 김 김치찜 이런 거",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005575.1.14",
          "form": "아 좋다",
          "original_form": "아 좋다",
          "speaker_id": "1",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005575.1.15",
          "form": "햇님 님 김치찜 계란말이 먹방 보면 정말 행복해",
          "original_form": "햇님 님 김치찜 계란말이 먹방 보면 정말 행복해",
          "speaker_id": "1",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005575.1.16",
          "form": "와 진짜 인정",
          "original_form": "와 진짜 인정",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005575.1.17",
          "form": "나 그것만 오백번 봄",
          "original_form": "나 그것만 오백번 봄",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005575.1.18",
          "form": "햇님만 봐ㅠ",
          "original_form": "햇님만 봐ㅠ",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005575.1.19",
          "form": "진짜 레전드",
          "original_form": "진짜 레전드",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005575.1.20",
          "form": "너무 맛있어 보임",
          "original_form": "너무 맛있어 보임",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005575.1.21",
          "form": "김치찜 너무 좋아",
          "original_form": "김치찜 너무 좋아",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005575.1.22",
          "form": "나는 한식 취향인 듯 진짜",
          "original_form": "나는 한식 취향인 듯 진짜",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005575.1.23",
          "form": "다음에 같이 먹자",
          "original_form": "다음에 같이 먹자",
          "speaker_id": "1",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005575.1.24",
          "form": "나도 요즘 침흘리면서 봐",
          "original_form": "나도 요즘 침흘리면서 봐",
          "speaker_id": "1",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005575.1.25",
          "form": "좋아ㅜ",
          "original_form": "좋아ㅜ",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005575.1.26",
          "form": "짜글이는 먹어봣우?",
          "original_form": "짜글이는 먹어봣우?",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005575.1.27",
          "form": "짜글이도 안 먹어봤엉",
          "original_form": "짜글이도 안 먹어봤엉",
          "speaker_id": "1",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005575.1.28",
          "form": "김치 요리 엄청 땡겨",
          "original_form": "김치 요리 엄청 땡겨",
          "speaker_id": "1",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005575.1.29",
          "form": "할머니랑은 거의 된장찌개만 먹어서",
          "original_form": "할머니랑은 거의 된장찌개만 먹어서",
          "speaker_id": "1",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005575.1.30",
          "form": "짜글이 담에 같이 먹어볼래?",
          "original_form": "짜글이 담에 같이 먹어볼래?",
          "speaker_id": "2",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005575.1.31",
          "form": "진짜맛있음.....",
          "original_form": "진짜맛있음.....",
          "speaker_id": "2",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005575.1.32",
          "form": "김치찜도 같이 먹자",
          "original_form": "김치찜도 같이 먹자",
          "speaker_id": "2",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005575.1.33",
          "form": "아 행복해~~",
          "original_form": "아 행복해~~",
          "speaker_id": "1",
          "time": "20210821 23:25"
        }
      ]
    }
