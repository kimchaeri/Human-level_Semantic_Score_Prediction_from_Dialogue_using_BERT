      "utterance": [
        {
          "id": "MDRW2100024034.1.1",
          "form": "저 좋아 합니다 ㅋㅋㅋ",
          "original_form": "저 좋아 합니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100024034.1.2",
          "form": "잘먹지는 못하고 ㅋㅋㅋ",
          "original_form": "잘먹지는 못하고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100024034.1.3",
          "form": "어엇?ㅎㅎ 좋아하시는데 잘 드시지는 못한다니 슬퍼요ㅠㅠ",
          "original_form": "어엇?ㅎㅎ 좋아하시는데 잘 드시지는 못한다니 슬퍼요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100024034.1.4",
          "form": "ㅜㅜㅜ맞아요",
          "original_form": "ㅜㅜㅜ맞아요",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100024034.1.5",
          "form": "많이 못먹는게 너무 아쉽네요",
          "original_form": "많이 못먹는게 너무 아쉽네요",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100024034.1.6",
          "form": "저는 와인 한병은 마시는것같아요",
          "original_form": "저는 와인 한병은 마시는것같아요",
          "speaker_id": "1",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100024034.1.7",
          "form": "한 병 먹으면 혓바닥이 빨갛게 될것같아요ㅋ",
          "original_form": "한 병 먹으면 혓바닥이 빨갛게 될것같아요ㅋ",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024034.1.8",
          "form": "레드와인 좋아하세요?",
          "original_form": "레드와인 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024034.1.9",
          "form": "으허허 표현이 귀여우세요ㅋㅋㅋㅋ",
          "original_form": "으허허 표현이 귀여우세요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024034.1.10",
          "form": "네네 좋아해요~!",
          "original_form": "네네 좋아해요~!",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024034.1.11",
          "form": "스파클링도 좋아하구용",
          "original_form": "스파클링도 좋아하구용",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024034.1.12",
          "form": "오오 다양하게 드시는걸 보니",
          "original_form": "오오 다양하게 드시는걸 보니",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024034.1.13",
          "form": "잘드시나봐요 ㅎㅎ",
          "original_form": "잘드시나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024034.1.14",
          "form": "한병 넘어가면 네발로 걸어다녀요..하핫....",
          "original_form": "한병 넘어가면 네발로 걸어다녀요..하핫....",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024034.1.15",
          "form": "ㅋㅋㅋ저는 와인 너무 쓰더라고요",
          "original_form": "ㅋㅋㅋ저는 와인 너무 쓰더라고요",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024034.1.16",
          "form": "아아 약간 그렇긴 하죠",
          "original_form": "아아 약간 그렇긴 하죠",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024034.1.17",
          "form": "근데 쐬주가 더 쓴것같아옄ㅋㅋㅋㅋ",
          "original_form": "근데 쐬주가 더 쓴것같아옄ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024034.1.18",
          "form": "맞아요 그래서 저는 막걸리이런 달달한거 좋아해요",
          "original_form": "맞아요 그래서 저는 막걸리이런 달달한거 좋아해요",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024034.1.19",
          "form": "꿀밤 막걸리... 사랑해요...ㅋㅋㅋㅋ",
          "original_form": "꿀밤 막걸리... 사랑해요...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024034.1.20",
          "form": "오미자 막걸리도!",
          "original_form": "오미자 막걸리도!",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024034.1.21",
          "form": "ㅜㅜㅜ인정",
          "original_form": "ㅜㅜㅜ인정",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024034.1.22",
          "form": "너무 맛있어요",
          "original_form": "너무 맛있어요",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024034.1.23",
          "form": "요즘엔 딸기 막걸리도 있던데",
          "original_form": "요즘엔 딸기 막걸리도 있던데",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024034.1.24",
          "form": "맛있더라고요",
          "original_form": "맛있더라고요",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024034.1.25",
          "form": "딸기도 나와요????",
          "original_form": "딸기도 나와요????",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024034.1.26",
          "form": "저희 동네에 언제한번 전국 막걸리 축제가 열려서",
          "original_form": "저희 동네에 언제한번 전국 막걸리 축제가 열려서",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024034.1.27",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024034.1.28",
          "form": "오 ㅋㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024034.1.29",
          "form": "갔는데 별별 막걸리가 다있어요ㅋㅋㅋㅋ",
          "original_form": "갔는데 별별 막걸리가 다있어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
