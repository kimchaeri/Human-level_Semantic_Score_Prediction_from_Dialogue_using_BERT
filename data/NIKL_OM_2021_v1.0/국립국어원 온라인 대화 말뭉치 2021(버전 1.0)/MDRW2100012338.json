      "utterance": [
        {
          "id": "MDRW2100012338.1.1",
          "form": "애낳는거 엄청나게 별일인데",
          "original_form": "애낳는거 엄청나게 별일인데",
          "speaker_id": "1",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100012338.1.2",
          "form": "막 많은 생각은 없는 것 같았어요",
          "original_form": "막 많은 생각은 없는 것 같았어요",
          "speaker_id": "2",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100012338.1.3",
          "form": "그냥 자식을 갖고싶다는 이유로 낳다니",
          "original_form": "그냥 자식을 갖고싶다는 이유로 낳다니",
          "speaker_id": "1",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100012338.1.4",
          "form": "상상이 안가요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "상상이 안가요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100012338.1.5",
          "form": "임신 부작용",
          "original_form": "임신 부작용",
          "speaker_id": "2",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012338.1.6",
          "form": "이나",
          "original_form": "이나",
          "speaker_id": "2",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012338.1.7",
          "form": "애 키우는데 엄청난 책임감이 요구되는데 그걸 질 생각을 가볍게하다니",
          "original_form": "애 키우는데 엄청난 책임감이 요구되는데 그걸 질 생각을 가볍게하다니",
          "speaker_id": "1",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012338.1.8",
          "form": "임 신 부작용ㄷ도 무서워요",
          "original_form": "임 신 부작용ㄷ도 무서워요",
          "speaker_id": "1",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012338.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012338.1.10",
          "form": "꽤나 원하는게 확실한데",
          "original_form": "꽤나 원하는게 확실한데",
          "speaker_id": "2",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012338.1.11",
          "form": "그에따른 책임을 너무 과소평가해보였아요",
          "original_form": "그에따른 책임을 너무 과소평가해보였아요",
          "speaker_id": "2",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012338.1.12",
          "form": "다른것도 뭐 남편 잘 만나면 되는거 아니냐고 그러는데",
          "original_form": "다른것도 뭐 남편 잘 만나면 되는거 아니냐고 그러는데",
          "speaker_id": "2",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012338.1.13",
          "form": "자기기준에 맞는 좋은 사람을 찾으면 다행이죠",
          "original_form": "자기기준에 맞는 좋은 사람을 찾으면 다행이죠",
          "speaker_id": "2",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012338.1.14",
          "form": "책임을 과소평가하는 걸 볼때마다",
          "original_form": "책임을 과소평가하는 걸 볼때마다",
          "speaker_id": "1",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100012338.1.15",
          "form": "트루먼쇼를 확신하게됩니다 ㅋㅋㅋㅋ",
          "original_form": "트루먼쇼를 확신하게됩니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100012338.1.16",
          "form": "세상은 생각보다 가벼운데 너무 무겁게생각했나 하는ㅋㅋㅋ",
          "original_form": "세상은 생각보다 가벼운데 너무 무겁게생각했나 하는ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100012338.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012338.1.18",
          "form": "맞아요 나만 좀 무겁게 느껴지는건가",
          "original_form": "맞아요 나만 좀 무겁게 느껴지는건가",
          "speaker_id": "2",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012338.1.19",
          "form": "성병같은 경우도",
          "original_form": "성병같은 경우도",
          "speaker_id": "2",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012338.1.20",
          "form": "근데 금쪽같은",
          "original_form": "근데 금쪽같은",
          "speaker_id": "1",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012338.1.21",
          "form": "내/ㅎㅎ새:::끼",
          "original_form": "내/ㅎㅎ새:::끼",
          "speaker_id": "1",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012338.1.22",
          "form": "이것만 봐도 되게 무겁게 보이지않아요?",
          "original_form": "이것만 봐도 되게 무겁게 보이지않아요?",
          "speaker_id": "1",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012338.1.23",
          "form": "잘 피하면 되는거 아니냐고",
          "original_form": "잘 피하면 되는거 아니냐고",
          "speaker_id": "2",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012338.1.24",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012338.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012338.1.26",
          "form": "너무 맹목적이에요",
          "original_form": "너무 맹목적이에요",
          "speaker_id": "2",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012338.1.27",
          "form": "주체가 애한태 가있는 느낌이에요",
          "original_form": "주체가 애한태 가있는 느낌이에요",
          "speaker_id": "2",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012338.1.28",
          "form": "성병을 피한다고 피해지나욬ㅋㅋㅋ",
          "original_form": "성병을 피한다고 피해지나욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012338.1.29",
          "form": "저도 조심해야겠어요",
          "original_form": "저도 조심해야겠어요",
          "speaker_id": "2",
