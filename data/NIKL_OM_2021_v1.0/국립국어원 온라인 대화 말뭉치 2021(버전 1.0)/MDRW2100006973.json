      "utterance": [
        {
          "id": "MDRW2100006973.1.1",
          "form": "제 친구도 비숲 엄청 좋아하던데",
          "original_form": "제 친구도 비숲 엄청 좋아하던데",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006973.1.2",
          "form": "재밌어 보이긴 하더라구요",
          "original_form": "재밌어 보이긴 하더라구요",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006973.1.3",
          "form": "맞아요 정말 잘 만들었어요",
          "original_form": "맞아요 정말 잘 만들었어요",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006973.1.4",
          "form": "시그널도 안보셨나요?",
          "original_form": "시그널도 안보셨나요?",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006973.1.5",
          "form": "앗 네ㅠㅠ",
          "original_form": "앗 네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006973.1.6",
          "form": "그것도 완전 유명하잖아요",
          "original_form": "그것도 완전 유명하잖아요",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006973.1.7",
          "form": "네네 진짜 유명한건데 이것도 안보셨군요",
          "original_form": "네네 진짜 유명한건데 이것도 안보셨군요",
          "speaker_id": "2",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006973.1.8",
          "form": "추리 별로 안좋아하시나봐요?",
          "original_form": "추리 별로 안좋아하시나봐요?",
          "speaker_id": "2",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006973.1.9",
          "form": "추리물 막상 보면 완전 재밌게 보는데 그냥 제가 드라마를 별로 많이 안 본 거 같아요ㅋㅋㅠ",
          "original_form": "추리물 막상 보면 완전 재밌게 보는데 그냥 제가 드라마를 별로 많이 안 본 거 같아요ㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006973.1.10",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006973.1.11",
          "form": "기억나시는 드라마는 있으실까요?ㅎ",
          "original_form": "기억나시는 드라마는 있으실까요?ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006973.1.12",
          "form": "음 당잠사... 이것도 추리물로 치나요? 그냥 로맨스인가?",
          "original_form": "음 당잠사... 이것도 추리물로 치나요? 그냥 로맨스인가?",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006973.1.13",
          "form": "당신이 잠든 사이? 로맨스아니예요?",
          "original_form": "당신이 잠든 사이? 로맨스아니예요?",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006973.1.14",
          "form": "그런가봐요!!",
          "original_form": "그런가봐요!!",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006973.1.15",
          "form": "로맨스라 저는 패스한작품 같아요",
          "original_form": "로맨스라 저는 패스한작품 같아요",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006973.1.16",
          "form": "아하 추리물만 보시나요?",
          "original_form": "아하 추리물만 보시나요?",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006973.1.17",
          "form": "거의 그런거위주로 좋아해서요",
          "original_form": "거의 그런거위주로 좋아해서요",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006973.1.18",
          "form": "로맨스는 좀 안맞는거같더라구요 ㅎ",
          "original_form": "로맨스는 좀 안맞는거같더라구요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006973.1.19",
          "form": "갬성이 부족한가봐요",
          "original_form": "갬성이 부족한가봐요",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006973.1.20",
          "form": "갬성ㅋㅋㅋㅋ",
          "original_form": "갬성ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:20"
        }
      ]
