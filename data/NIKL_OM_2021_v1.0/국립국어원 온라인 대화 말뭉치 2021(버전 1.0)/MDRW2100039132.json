      "utterance": [
        {
          "id": "MDRW2100039132.1.1",
          "form": "작고 귀엽죠",
          "original_form": "작고 귀엽죠",
          "speaker_id": "2",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100039132.1.2",
          "form": "얼마전에는 무당벌레키웠어요",
          "original_form": "얼마전에는 무당벌레키웠어요",
          "speaker_id": "1",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100039132.1.3",
          "form": "페어리? 팅커벨?",
          "original_form": "페어리? 팅커벨?",
          "speaker_id": "2",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100039132.1.4",
          "form": "밥안줬는데 계속  제방에 들어와요",
          "original_form": "밥안줬는데 계속  제방에 들어와요",
          "speaker_id": "1",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100039132.1.5",
          "form": "무당벌레가요?",
          "original_form": "무당벌레가요?",
          "speaker_id": "2",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100039132.1.6",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "1",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100039132.1.7",
          "form": "귀여우니 망정이지..",
          "original_form": "귀여우니 망정이지..",
          "speaker_id": "1",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100039132.1.8",
          "form": "계속다가와서 노란액체를 뿜뿜해요",
          "original_form": "계속다가와서 노란액체를 뿜뿜해요",
          "speaker_id": "1",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100039132.1.9",
          "form": "무당벌레가 침도 밭나요",
          "original_form": "무당벌레가 침도 밭나요",
          "speaker_id": "2",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100039132.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100039132.1.11",
          "form": "적한테요.",
          "original_form": "적한테요.",
          "speaker_id": "1",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100039132.1.12",
          "form": "지가들어와놓고 성질내요",
          "original_form": "지가들어와놓고 성질내요",
          "speaker_id": "1",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100039132.1.13",
          "form": "적이누구죠 name1님?",
          "original_form": "적이누구죠 &name1&님?",
          "speaker_id": "2",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100039132.1.14",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039132.1.15",
          "form": "재롱인가봐요",
          "original_form": "재롱인가봐요",
          "speaker_id": "2",
          "time": "20210903 10:57"
        },
        {
          "id": "MDRW2100039132.1.16",
          "form": "귀엽네요",
          "original_form": "귀엽네요",
          "speaker_id": "1",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100039132.1.17",
          "form": "담에는 먹이를 줘야겠어요",
          "original_form": "담에는 먹이를 줘야겠어요",
          "speaker_id": "1",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100039132.1.18",
          "form": "반려동물이라",
          "original_form": "반려동물이라",
          "speaker_id": "2",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100039132.1.19",
          "form": "진딧물어디서 구하시려궁",
