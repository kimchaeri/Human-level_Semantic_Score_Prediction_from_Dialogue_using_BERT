      "utterance": [
        {
          "id": "MDRW2100024952.1.1",
          "form": "요새는 당이 들어간 음료를",
          "original_form": "요새는 당이 들어간 음료를",
          "speaker_id": "1",
          "time": "20210831 21:19"
        },
        {
          "id": "MDRW2100024952.1.2",
          "form": "절제하고 있어요",
          "original_form": "절제하고 있어요",
          "speaker_id": "1",
          "time": "20210831 21:19"
        },
        {
          "id": "MDRW2100024952.1.3",
          "form": "탄산음료도 제로 들어간 음료만 묵어용 홍홍",
          "original_form": "탄산음료도 제로 들어간 음료만 묵어용 홍홍",
          "speaker_id": "1",
          "time": "20210831 21:19"
        },
        {
          "id": "MDRW2100024952.1.4",
          "form": "아 저도 단거는 잘 못먹어서",
          "original_form": "아 저도 단거는 잘 못먹어서",
          "speaker_id": "2",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024952.1.5",
          "form": "커피도 아메리카노만 마셔요",
          "original_form": "커피도 아메리카노만 마셔요",
          "speaker_id": "2",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024952.1.6",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024952.1.7",
          "form": "아니면 그냥티백에 우린차나...",
          "original_form": "아니면 그냥티백에 우린차나...",
          "speaker_id": "2",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024952.1.8",
          "form": "아메리카노를!",
          "original_form": "아메리카노를!",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024952.1.9",
          "form": "시럽없이 드시나요!!!!",
          "original_form": "시럽없이 드시나요!!!!",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024952.1.10",
          "form": "단 음료는 아주 가끔",
          "original_form": "단 음료는 아주 가끔",
          "speaker_id": "2",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024952.1.11",
          "form": "으른이다 으릉",
          "original_form": "으른이다 으릉",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024952.1.12",
          "form": "네 ㅋㅋㄱㅋㅋㅋ 단거 들어가면 텁텁하더라구요",
          "original_form": "네 ㅋㅋㄱㅋㅋㅋ 단거 들어가면 텁텁하더라구요",
          "speaker_id": "2",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024952.1.13",
          "form": "우와..",
          "original_form": "우와..",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024952.1.14",
          "form": "전 아메리카노는 너무 쓴 것 같아요ㅠㅠ",
          "original_form": "전 아메리카노는 너무 쓴 것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024952.1.15",
          "form": "저도 성인되기전까지는 못마셨는데",
          "original_form": "저도 성인되기전까지는 못마셨는데",
          "speaker_id": "2",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100024952.1.16",
          "form": "어느순간 시럽넣어서 아메리카노 마시다가 점점 시럽을 안넣게 되더라구요 ㅋㅋ",
          "original_form": "어느순간 시럽넣어서 아메리카노 마시다가 점점 시럽을 안넣게 되더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100024952.1.17",
          "form": "지금은 아예 없는거만 마셔요",
          "original_form": "지금은 아예 없는거만 마셔요",
          "speaker_id": "2",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100024952.1.18",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100024952.1.19",
          "form": "맞아요 그런식으로",
          "original_form": "맞아요 그런식으로",
          "speaker_id": "1",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100024952.1.20",
          "form": "사람들이",
          "original_form": "사람들이",
          "speaker_id": "1",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100024952.1.21",
          "form": "에스프레소 그 사약까지도 손을 댄다고 들었어요",
          "original_form": "에스프레소 그 사약까지도 손을 댄다고 들었어요",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024952.1.22",
          "form": "커피도 자꾸 마시다보면",
          "original_form": "커피도 자꾸 마시다보면",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024952.1.23",
          "form": "지금 과도기이신 겁니다",
          "original_form": "지금 과도기이신 겁니다",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024952.1.24",
          "form": "맛이다른게 느껴지더라구용",
          "original_form": "맛이다른게 느껴지더라구용",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024952.1.25",
          "form": "신거 고소한거",
          "original_form": "신거 고소한거",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024952.1.26",
          "form": "맞아요 원두에따라",
          "original_form": "맞아요 원두에따라",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024952.1.27",
          "form": "다 다르다던데",
          "original_form": "다 다르다던데",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024952.1.28",
          "form": "또 약간 과일향나는거",
          "original_form": "또 약간 과일향나는거",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024952.1.29",
          "form": "전 그냥 고소하고쓴맛 시고쓴맛",
          "original_form": "전 그냥 고소하고쓴맛 시고쓴맛",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024952.1.30",
          "form": "초콜릿향나는거",
          "original_form": "초콜릿향나는거",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024952.1.31",
          "form": "모두 쓴맛같아요..",
          "original_form": "모두 쓴맛같아요..",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024952.1.32",
          "form": "ㅋㅋㄱㄱㄱ맞아요 저도 안마실땐 결국 쓴맛",
          "original_form": "ㅋㅋㄱㄱㄱ맞아요 저도 안마실땐 결국 쓴맛",
          "speaker_id": "2",
          "time": "20210831 21:25"
        }
      ]
