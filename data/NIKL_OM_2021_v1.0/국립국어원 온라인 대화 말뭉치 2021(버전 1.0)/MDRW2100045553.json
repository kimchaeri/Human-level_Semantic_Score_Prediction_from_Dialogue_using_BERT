      "utterance": [
        {
          "id": "MDRW2100045553.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210908 03:24"
        },
        {
          "id": "MDRW2100045553.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210908 03:25"
        },
        {
          "id": "MDRW2100045553.1.3",
          "form": "여긴 숨은 맛집이다 싶은곳있으신가요?",
          "original_form": "여긴 숨은 맛집이다 싶은곳있으신가요?",
          "speaker_id": "1",
          "time": "20210908 03:27"
        },
        {
          "id": "MDRW2100045553.1.4",
          "form": "음 맛집은 생각이 안나고 카페만 한군데 있어요!",
          "original_form": "음 맛집은 생각이 안나고 카페만 한군데 있어요!",
          "speaker_id": "2",
          "time": "20210908 03:28"
        },
        {
          "id": "MDRW2100045553.1.5",
          "form": "헛 어딘가요?",
          "original_form": "헛 어딘가요?",
          "speaker_id": "1",
          "time": "20210908 03:30"
        },
        {
          "id": "MDRW2100045553.1.6",
          "form": "성신여대 근처에 다두 라는 카페가 있어요 거기 커피 특이한 메뉴 많아요!!",
          "original_form": "성신여대 근처에 다두 라는 카페가 있어요 거기 커피 특이한 메뉴 많아요!!",
          "speaker_id": "2",
          "time": "20210908 03:33"
        },
        {
          "id": "MDRW2100045553.1.7",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210908 03:34"
        },
        {
          "id": "MDRW2100045553.1.8",
          "form": "카페다두 에 어떤 메뉴가 맛있나요?",
          "original_form": "카페다두 에 어떤 메뉴가 맛있나요?",
          "speaker_id": "1",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045553.1.9",
          "form": "전 라떼 좋아해서 라떼 젤 많이 먹고 핸드드립도 괜찮고 요리도 맛있어요 전엔 오믈렛도 있었는데 지금도 하는지 모르겠네요 제가 이사를 와서..",
          "original_form": "전 라떼 좋아해서 라떼 젤 많이 먹고 핸드드립도 괜찮고 요리도 맛있어요 전엔 오믈렛도 있었는데 지금도 하는지 모르겠네요 제가 이사를 와서..",
          "speaker_id": "2",
          "time": "20210908 03:40"
        },
        {
          "id": "MDRW2100045553.1.10",
          "form": "핸드드립 잘하는곳 드문데 가고 싶네요 ㅎㅎ",
          "original_form": "핸드드립 잘하는곳 드문데 가고 싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 03:41"
        },
        {
          "id": "MDRW2100045553.1.11",
          "form": "저도 1군데 익선동에",
          "original_form": "저도 1군데 익선동에",
          "speaker_id": "1",
          "time": "20210908 03:42"
        },
        {
          "id": "MDRW2100045553.1.12",
          "form": "오오 익선동 힙한 곳",
          "original_form": "오오 익선동 힙한 곳",
          "speaker_id": "2",
          "time": "20210908 03:44"
        },
        {
          "id": "MDRW2100045553.1.13",
          "form": "너무",
          "original_form": "너무",
          "speaker_id": "1",
          "time": "20210908 03:44"
        },
        {
          "id": "MDRW2100045553.1.14",
          "form": "맛있는곳",
          "original_form": "맛있는곳",
          "speaker_id": "1",
          "time": "20210908 03:45"
        },
        {
          "id": "MDRW2100045553.1.15",
          "form": "어딘가요!!! 궁금",
          "original_form": "어딘가요!!! 궁금",
          "speaker_id": "2",
          "time": "20210908 03:45"
        },
        {
          "id": "MDRW2100045553.1.16",
          "form": "종로스테이크라고",
          "original_form": "종로스테이크라고",
          "speaker_id": "1",
          "time": "20210908 03:47"
        },
        {
          "id": "MDRW2100045553.1.17",
          "form": "오랜된 스테이크 전문점이 있는데요",
          "original_form": "오랜된 스테이크 전문점이 있는데요",
          "speaker_id": "1",
          "time": "20210908 03:48"
        },
        {
          "id": "MDRW2100045553.1.18",
          "form": "고기가 야들야들 맛있어요",
          "original_form": "고기가 야들야들 맛있어요",
          "speaker_id": "1",
          "time": "20210908 03:48"
        },
        {
          "id": "MDRW2100045553.1.19",
          "form": "우와 검색해보고 왔어요",
          "original_form": "우와 검색해보고 왔어요",
          "speaker_id": "2",
          "time": "20210908 03:49"
        },
        {
          "id": "MDRW2100045553.1.20",
          "form": "비주얼이 ㄷㄷ",
          "original_form": "비주얼이 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210908 03:49"
        },
        {
          "id": "MDRW2100045553.1.21",
          "form": "식당분위기도 좋아요~",
          "original_form": "식당분위기도 좋아요~",
          "speaker_id": "1",
          "time": "20210908 03:49"
        },
        {
          "id": "MDRW2100045553.1.22",
          "form": "줄은 이제 좀 덜서지 않을까 싶어요ㅋㅋㅋㅋ",
          "original_form": "줄은 이제 좀 덜서지 않을까 싶어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:50"
        },
        {
          "id": "MDRW2100045553.1.23",
          "form": "식당이 뭔가 따뜻한 느낌이 드네요~",
          "original_form": "식당이 뭔가 따뜻한 느낌이 드네요~",
          "speaker_id": "2",
          "time": "20210908 03:50"
        },
        {
          "id": "MDRW2100045553.1.24",
