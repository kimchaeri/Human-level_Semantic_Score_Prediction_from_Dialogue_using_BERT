      "utterance": [
        {
          "id": "MDRW2100039277.1.1",
          "form": "엄마 우리 저녁에 치킨 먹는거 알지?",
          "original_form": "엄마 우리 저녁에 치킨 먹는거 알지?",
          "speaker_id": "1",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039277.1.2",
          "form": "치킨이 그렇게 먹고싶냐",
          "original_form": "치킨이 그렇게 먹고싶냐",
          "speaker_id": "2",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039277.1.3",
          "form": "아니 엄마가 나 공복에 치킨방송 보여줬잖아 붙잡아다",
          "original_form": "아니 엄마가 나 공복에 치킨방송 보여줬잖아 붙잡아다",
          "speaker_id": "1",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039277.1.4",
          "form": "내가 잘못했다 그래",
          "original_form": "내가 잘못했다 그래",
          "speaker_id": "2",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039277.1.5",
          "form": "ㅋㅋㅋ방송보면서 얼마나 먹고 싶었다고",
          "original_form": "ㅋㅋㅋ방송보면서 얼마나 먹고 싶었다고",
          "speaker_id": "1",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039277.1.6",
          "form": "오늘 불금이라",
          "original_form": "오늘 불금이라",
          "speaker_id": "2",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100039277.1.7",
          "form": "일찍 시켜야돼",
          "original_form": "일찍 시켜야돼",
          "speaker_id": "2",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100039277.1.8",
          "form": "ㅇㅇ한 여섯시 반쯤?",
          "original_form": "ㅇㅇ한 여섯시 반쯤?",
          "speaker_id": "1",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100039277.1.9",
          "form": "시킬라고",
          "original_form": "시킬라고",
          "speaker_id": "1",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100039277.1.10",
          "form": "그래 시킬데 미리 봐놔",
          "original_form": "그래 시킬데 미리 봐놔",
          "speaker_id": "2",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100039277.1.11",
          "form": "아 벌써 봐놨징",
          "original_form": "아 벌써 봐놨징",
          "speaker_id": "1",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100039277.1.12",
          "form": "얼씨구 잘했다",
          "original_form": "얼씨구 잘했다",
          "speaker_id": "2",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100039277.1.13",
          "form": "근데 바비큐하나 시켜도되?",
          "original_form": "근데 바비큐하나 시켜도되?",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039277.1.14",
          "form": "바베큐치킨?",
          "original_form": "바베큐치킨?",
          "speaker_id": "2",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039277.1.15",
          "form": "ㅇㅇ그거 안먹은지 엄청 오래됐는데",
          "original_form": "ㅇㅇ그거 안먹은지 엄청 오래됐는데",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039277.1.16",
          "form": "아까 진짜 맛있어보이더라",
          "original_form": "아까 진짜 맛있어보이더라",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039277.1.17",
          "form": "난 후라이드가 맛있어보이던데",
          "original_form": "난 후라이드가 맛있어보이던데",
          "speaker_id": "2",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039277.1.18",
          "form": "엄만 바베큐치킨 안먹으니까",
          "original_form": "엄만 바베큐치킨 안먹으니까",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039277.1.19",
          "form": "후라이드하나 바베큐하나 시킬라고",
          "original_form": "후라이드하나 바베큐하나 시킬라고",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039277.1.20",
          "form": "그렇게 시켜 그럼",
          "original_form": "그렇게 시켜 그럼",
          "speaker_id": "2",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039277.1.21",
          "form": "늦지않게 시켜라",
          "original_form": "늦지않게 시켜라",
          "speaker_id": "2",
