      "utterance": [
        {
          "id": "MDRW2100021017.1.1",
          "form": "요즘 여행이 미치도록 가고싶네요 ㅜㅜ",
          "original_form": "요즘 여행이 미치도록 가고싶네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 15:03"
        },
        {
          "id": "MDRW2100021017.1.2",
          "form": "맞아요ㅠㅠ 시국때문에 오래 못 갔네요",
          "original_form": "맞아요ㅠㅠ 시국때문에 오래 못 갔네요",
          "speaker_id": "2",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100021017.1.3",
          "form": "가고 싶은데 있으신가요?",
          "original_form": "가고 싶은데 있으신가요?",
          "speaker_id": "2",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100021017.1.4",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "1",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100021017.1.5",
          "form": "저는 지역은 아직 딱히 생각은 안해봤는데",
          "original_form": "저는 지역은 아직 딱히 생각은 안해봤는데",
          "speaker_id": "1",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100021017.1.6",
          "form": "그냥 호텔이면 될 것 같아요 호캉스요 ㅠ",
          "original_form": "그냥 호텔이면 될 것 같아요 호캉스요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100021017.1.7",
          "form": "좋네요. 그냥 푹 쉬기에도 괜찮구요",
          "original_form": "좋네요. 그냥 푹 쉬기에도 괜찮구요",
          "speaker_id": "2",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100021017.1.8",
          "form": "그쳐ㅓ ㅎㅎ 거기 안에서 다 해결하는거죠",
          "original_form": "그쳐ㅓ ㅎㅎ 거기 안에서 다 해결하는거죠",
          "speaker_id": "1",
          "time": "20210828 15:12"
        },
        {
          "id": "MDRW2100021017.1.9",
          "form": "조식부터 수영장 라운지바까지..",
          "original_form": "조식부터 수영장 라운지바까지..",
          "speaker_id": "1",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100021017.1.10",
          "form": "호텔은 라면도 맛있다는 얘기도 들었어요ㅋㅋ",
          "original_form": "호텔은 라면도 맛있다는 얘기도 들었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100021017.1.11",
          "form": "아까울거 같은데 먹어보고 싶기두 하네요ㅋㅋ",
          "original_form": "아까울거 같은데 먹어보고 싶기두 하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100021017.1.12",
          "form": "ㅋㅋㅋㅋ엄청 비쌀걸요",
          "original_form": "ㅋㅋㅋㅋ엄청 비쌀걸요",
          "speaker_id": "1",
          "time": "20210828 15:17"
        },
        {
          "id": "MDRW2100021017.1.13",
          "form": "신라호텔 비빔밥도 3만5천원인가 하는데 ㅜㅜ",
          "original_form": "신라호텔 비빔밥도 3만5천원인가 하는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 15:17"
        },
        {
          "id": "MDRW2100021017.1.14",
          "form": "너무 비싸네요ㅠㅠ",
          "original_form": "너무 비싸네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100021017.1.15",
          "form": "호캉스 한번 가보고싶기는 해요",
          "original_form": "호캉스 한번 가보고싶기는 해요",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100021017.1.16",
          "form": "호캉스 인생에 한번쯤은 꼭 다녀와보세요",
          "original_form": "호캉스 인생에 한번쯤은 꼭 다녀와보세요",
          "speaker_id": "1",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100021017.1.17",
          "form": "진짜 잊지못할 추억이 되실거에요",
          "original_form": "진짜 잊지못할 추억이 되실거에요",
          "speaker_id": "1",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100021017.1.18",
          "form": "놀러가면 돌아다니니까 호캉스는 나중으로 미뤘거든요ㅋㅋ",
          "original_form": "놀러가면 돌아다니니까 호캉스는 나중으로 미뤘거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100021017.1.19",
          "form": "그래서 아예 호텔에서 풀코스로 놀거를 짜셔야해요",
          "original_form": "그래서 아예 호텔에서 풀코스로 놀거를 짜셔야해요",
          "speaker_id": "1",
          "time": "20210828 15:31"
        },
        {
          "id": "MDRW2100021017.1.20",
          "form": "조식뷔페, 수영장, 바베큐파티, 라운지바 이런 식으로 알차게 ㅋㅋㅋ",
          "original_form": "조식뷔페, 수영장, 바베큐파티, 라운지바 이런 식으로 알차게 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:31"
        },
        {
          "id": "MDRW2100021017.1.21",
          "form": "한 번 가면 다 해봐야겠네요ㅋㅋㅋ",
          "original_form": "한 번 가면 다 해봐야겠네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100021017.1.22",
          "form": "이런거 보면 호캉스도 부지런해야 되나봐요 ㅋㅋ",
          "original_form": "이런거 보면 호캉스도 부지런해야 되나봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:49"
        }
      ]
    }
