      "utterance": [
        {
          "id": "MDRW2100006865.1.1",
          "form": "콩나물국밥은요",
          "original_form": "콩나물국밥은요",
          "speaker_id": "2",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100006865.1.2",
          "form": "구수하고 간편해서 좋지요. 콩나물은 그 자체로 좋아요",
          "original_form": "구수하고 간편해서 좋지요. 콩나물은 그 자체로 좋아요",
          "speaker_id": "1",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100006865.1.3",
          "form": "몸에 좋고 속도 편해요",
          "original_form": "몸에 좋고 속도 편해요",
          "speaker_id": "2",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100006865.1.4",
          "form": "해장으로 딱이에요",
          "original_form": "해장으로 딱이에요",
          "speaker_id": "1",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100006865.1.5",
          "form": "뼈해장국 최애에요",
          "original_form": "뼈해장국 최애에요",
          "speaker_id": "2",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100006865.1.6",
          "form": "아 큼직한 뼈 넣고 푹 우린거 저도 좋아해요",
          "original_form": "아 큼직한 뼈 넣고 푹 우린거 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100006865.1.7",
          "form": "넘 맛있어요",
          "original_form": "넘 맛있어요",
          "speaker_id": "2",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100006865.1.8",
          "form": "사상역 최뼈다구 아나요?",
          "original_form": "사상역 최뼈다구 아나요?",
          "speaker_id": "1",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100006865.1.9",
          "form": "물론이죠",
          "original_form": "물론이죠",
          "speaker_id": "2",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100006865.1.10",
          "form": "8천원이죠",
          "original_form": "8천원이죠",
          "speaker_id": "2",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100006865.1.11",
          "form": "거긴 여전히 손님이 많아요",
          "original_form": "거긴 여전히 손님이 많아요",
          "speaker_id": "1",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100006865.1.12",
          "form": "맛집은 손길이 끊이지않죠",
          "original_form": "맛집은 손길이 끊이지않죠",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100006865.1.13",
          "form": "사상가면 최뼈다구 가봐야죠",
          "original_form": "사상가면 최뼈다구 가봐야죠",
          "speaker_id": "1",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100006865.1.14",
          "form": "거기 삼겹살도 맛있답니나",
          "original_form": "거기 삼겹살도 맛있답니나",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100006865.1.15",
          "form": "양곱창 좋아하나요",
          "original_form": "양곱창 좋아하나요",
          "speaker_id": "1",
