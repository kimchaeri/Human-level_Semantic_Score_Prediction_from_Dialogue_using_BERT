      "utterance": [
        {
          "id": "MDRW2100007562.1.1",
          "form": "커뮤 하는 거 있어?",
          "original_form": "커뮤 하는 거 있어?",
          "speaker_id": "2",
          "time": "20210822 03:15"
        },
        {
          "id": "MDRW2100007562.1.2",
          "form": "오 커뮤",
          "original_form": "오 커뮤",
          "speaker_id": "1",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007562.1.3",
          "form": "트위터랑 여시랑 에타?",
          "original_form": "트위터랑 여시랑 에타?",
          "speaker_id": "1",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007562.1.4",
          "form": "여시는 가끔 해",
          "original_form": "여시는 가끔 해",
          "speaker_id": "1",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007562.1.5",
          "form": "어 너 여자였어? 남잔줄",
          "original_form": "어 너 여자였어? 남잔줄",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007562.1.6",
          "form": "나도 트위터해ㅋㅋ구독계로",
          "original_form": "나도 트위터해ㅋㅋ구독계로",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007562.1.7",
          "form": "또... 강사모...",
          "original_form": "또... 강사모...",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007562.1.8",
          "form": "남자같았어?",
          "original_form": "남자같았어?",
          "speaker_id": "1",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007562.1.9",
          "form": "...........",
          "original_form": "...........",
          "speaker_id": "1",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007562.1.10",
          "form": "배그한다고하니까 약간 편견이 있었네ㅠ에구",
          "original_form": "배그한다고하니까 약간 편견이 있었네ㅠ에구",
          "speaker_id": "2",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007562.1.11",
          "form": "ㅋㅋㅋㅋ 강사모는 모야? 강아지를 사랑하는 모임?",
          "original_form": "ㅋㅋㅋㅋ 강사모는 모야? 강아지를 사랑하는 모임?",
          "speaker_id": "1",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007562.1.12",
          "form": "아 맞다 네이트판도한당 가끔 눈팅으로",
          "original_form": "아 맞다 네이트판도한당 가끔 눈팅으로",
          "speaker_id": "2",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007562.1.13",
          "form": "응ㅎ.ㅎ강아지사랑모임...",
          "original_form": "응ㅎ.ㅎ강아지사랑모임...",
          "speaker_id": "2",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007562.1.14",
          "form": "배그가 쫌 남초지 ㅎㅎ 이해됨",
          "original_form": "배그가 쫌 남초지 ㅎㅎ 이해됨",
          "speaker_id": "1",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007562.1.15",
          "form": "네이트판 가끔 들리면 재밌어 기상천외 썰 많아서",
          "original_form": "네이트판 가끔 들리면 재밌어 기상천외 썰 많아서",
          "speaker_id": "1",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007562.1.16",
          "form": "오 강아지도 키우는구나",
          "original_form": "오 강아지도 키우는구나",
          "speaker_id": "1",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007562.1.17",
          "form": "맞어 약간 인생선배느낌? ㅋㅋ판은",
          "original_form": "맞어 약간 인생선배느낌? ㅋㅋ판은",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007562.1.18",
          "form": "여시는 뭔가 친구들 많이하던데 인증해야돼서ㅠ",
          "original_form": "여시는 뭔가 친구들 많이하던데 인증해야돼서ㅠ",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007562.1.19",
          "form": "ㅋㅋㄱㅋㄱㅋㄱㅋ ㅇㅈ",
          "original_form": "ㅋㅋㄱㅋㄱㅋㄱㅋ ㅇㅈ",
          "speaker_id": "1",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007562.1.20",
          "form": "인생 꿀팁 얻어가",
          "original_form": "인생 꿀팁 얻어가",
          "speaker_id": "1",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007562.1.21",
          "form": "아 마쟈..그래서 쫌 귀찮아",
          "original_form": "아 마쟈..그래서 쫌 귀찮아",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007562.1.22",
          "form": "맞어ㅋㅋ여시는 민증인증이었나?",
          "original_form": "맞어ㅋㅋ여시는 민증인증이었나?",
          "speaker_id": "2",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007562.1.23",
          "form": "나 민증분실중이라 드가고싶어도 못드간담",
          "original_form": "나 민증분실중이라 드가고싶어도 못드간담",
          "speaker_id": "2",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007562.1.24",
          "form": "민증이랑 얼굴.....",
          "original_form": "민증이랑 얼굴.....",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007562.1.25",
          "form": "술은?",
          "original_form": "술은?",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007562.1.26",
          "form": "아니 ㅁ1ㅊ도 안 받아주네 너무해",
          "original_form": "아니 ㅁ1ㅊ도 안 받아주네 너무해",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007562.1.27",
          "form": "ㅋㅋ운전면허는 있지",
          "original_form": "ㅋㅋ운전면허는 있지",
          "speaker_id": "2",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007562.1.28",
          "form": "얼굴도까야돼?허걱쓰...",
          "original_form": "얼굴도까야돼?허걱쓰...",
          "speaker_id": "2",
