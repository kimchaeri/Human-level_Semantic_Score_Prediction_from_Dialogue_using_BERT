      "utterance": [
        {
          "id": "MDRW2100017202.1.1",
          "form": "어디로 여행간게 가장 기억에 남아요?",
          "original_form": "어디로 여행간게 가장 기억에 남아요?",
          "speaker_id": "1",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100017202.1.2",
          "form": "전 짧지만 좋았던 프랑스 스위스 이탈리아 여행이요!!",
          "original_form": "전 짧지만 좋았던 프랑스 스위스 이탈리아 여행이요!!",
          "speaker_id": "2",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017202.1.3",
          "form": "name1님은 어디가 가장 기억에 남나요?",
          "original_form": "&name1&님은 어디가 가장 기억에 남나요?",
          "speaker_id": "2",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100017202.1.4",
          "form": "우와 저는 프랑스요",
          "original_form": "우와 저는 프랑스요",
          "speaker_id": "1",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017202.1.5",
          "form": "파리요",
          "original_form": "파리요",
          "speaker_id": "1",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017202.1.6",
          "form": "우와! 저도 파리 다녀왔어요!",
          "original_form": "우와! 저도 파리 다녀왔어요!",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100017202.1.7",
          "form": "파리에 오페라하우스 였나 거기가 제일 좋았어요",
          "original_form": "파리에 오페라하우스 였나 거기가 제일 좋았어요",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017202.1.8",
          "form": "오페라의 유령",
          "original_form": "오페라의 유령",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017202.1.9",
          "form": "저는 파리 센 강에서 크루즈타고 구경한게 좋았어요! 무지 추웠지만",
          "original_form": "저는 파리 센 강에서 크루즈타고 구경한게 좋았어요! 무지 추웠지만",
          "speaker_id": "2",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017202.1.10",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017202.1.11",
          "form": "그것도 좋았어요",
          "original_form": "그것도 좋았어요",
          "speaker_id": "1",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017202.1.12",
          "form": "겨울에가셨나봐요",
          "original_form": "겨울에가셨나봐요",
          "speaker_id": "1",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017202.1.13",
          "form": "네! 겨울에 가서 감기걸려서 골골 거리며 여행한 기억이 나네요 ㅋㅋㅋ",
          "original_form": "네! 겨울에 가서 감기걸려서 골골 거리며 여행한 기억이 나네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100017202.1.14",
          "form": "아 ㅋㅋ",
          "original_form": "아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017202.1.15",
          "form": "전 여름에갔었어요",
          "original_form": "전 여름에갔었어요",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017202.1.16",
          "form": "대혁명기념일 주간에요",
          "original_form": "대혁명기념일 주간에요",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017202.1.17",
          "form": "와 여름 파리가 훨씬 좋을거같아요!!",
          "original_form": "와 여름 파리가 훨씬 좋을거같아요!!",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017202.1.18",
          "form": "짐은 가벼웠어요 ㅋㅋ",
          "original_form": "짐은 가벼웠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017202.1.19",
          "form": "또 이탈리아도 겨울인가요?",
          "original_form": "또 이탈리아도 겨울인가요?",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017202.1.20",
          "form": "네! 3국 다 추웠어요! name1님은 기념품 사오셨나요?",
          "original_form": "네! 3국 다 추웠어요! &name1&님은 기념품 사오셨나요?",
          "speaker_id": "2",
          "time": "20210827 09:07"
        },
        {
          "id": "MDRW2100017202.1.21",
          "form": "아 네 ㅋㅋㅋ 도시마다 다요",
          "original_form": "아 네 ㅋㅋㅋ 도시마다 다요",
