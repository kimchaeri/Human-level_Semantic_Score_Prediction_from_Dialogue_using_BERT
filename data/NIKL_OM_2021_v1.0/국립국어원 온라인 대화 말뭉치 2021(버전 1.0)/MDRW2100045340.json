      "utterance": [
        {
          "id": "MDRW2100045340.1.1",
          "form": "안녕하세요 name2님",
          "original_form": "안녕하세요 &name2&님",
          "speaker_id": "1",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045340.1.2",
          "form": "네 날씨가 비가 오네요",
          "original_form": "네 날씨가 비가 오네요",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045340.1.3",
          "form": "여기두요 ㅎㅎ 날씨가 많이 쌀쌀해요",
          "original_form": "여기두요 ㅎㅎ 날씨가 많이 쌀쌀해요",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045340.1.4",
          "form": "우산이제 필수템이에여 ㅠ",
          "original_form": "우산이제 필수템이에여 ㅠ",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045340.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045340.1.6",
          "form": "바람이 너무 많이 불어요",
          "original_form": "바람이 너무 많이 불어요",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045340.1.7",
          "form": "가디건 같은더 챙겨 입어야 할거같아요 ㅠ",
          "original_form": "가디건 같은더 챙겨 입어야 할거같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045340.1.8",
          "form": "안그래도 어제 가디건 입었는데 가디건이 펄럭였어요..ㅋㅋㅋㅋㅋ",
          "original_form": "안그래도 어제 가디건 입었는데 가디건이 펄럭였어요..ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045340.1.9",
          "form": "오늘도 비가 올려나 모르겠네요",
          "original_form": "오늘도 비가 올려나 모르겠네요",
          "speaker_id": "1",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100045340.1.10",
          "form": "와 오늘은 안오는더같아요!",
          "original_form": "와 오늘은 안오는더같아요!",
          "speaker_id": "2",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045340.1.11",
          "form": "그러게요 여기도 안와요",
          "original_form": "그러게요 여기도 안와요",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045340.1.12",
          "form": "근데 쌀쌀한건 같네요 ㅠ",
          "original_form": "근데 쌀쌀한건 같네요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045340.1.13",
          "form": "오늘 가디건 챙기셧나용",
          "original_form": "오늘 가디건 챙기셧나용",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045340.1.14",
          "form": "아예 긴팔을 입었어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아예 긴팔을 입었어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045340.1.15",
          "form": "name2님운 뭐 입으셨나요",
          "original_form": "&name2&님운 뭐 입으셨나요",
          "speaker_id": "1",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045340.1.16",
          "form": "저는 반팔 입을 생각이예요!!!",
          "original_form": "저는 반팔 입을 생각이예요!!!",
          "speaker_id": "2",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045340.1.17",
          "form": "좋은 하루 보내세요!",
          "original_form": "좋은 하루 보내세요!",
