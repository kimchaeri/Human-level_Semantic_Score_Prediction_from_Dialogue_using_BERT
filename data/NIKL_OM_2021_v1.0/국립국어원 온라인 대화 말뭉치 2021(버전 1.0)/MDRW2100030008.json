      "utterance": [
        {
          "id": "MDRW2100030008.1.1",
          "form": "안녕하세요! 야구 좋아하세요??",
          "original_form": "안녕하세요! 야구 좋아하세요??",
          "speaker_id": "1",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100030008.1.2",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100030008.1.3",
          "form": "야구 좋아합니다",
          "original_form": "야구 좋아합니다",
          "speaker_id": "2",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100030008.1.4",
          "form": "!!",
          "original_form": "!!",
          "speaker_id": "2",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100030008.1.5",
          "form": "어느팀 좋아하세요??",
          "original_form": "어느팀 좋아하세요??",
          "speaker_id": "2",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100030008.1.6",
          "form": "저 삼성이요!!",
          "original_form": "저 삼성이요!!",
          "speaker_id": "1",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100030008.1.7",
          "form": "똑같기를 바라며...",
          "original_form": "똑같기를 바라며...",
          "speaker_id": "1",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100030008.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100030008.1.9",
          "form": "아쉽네요ㅋㅋㅋㅋ",
          "original_form": "아쉽네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100030008.1.10",
          "form": "기아...ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "기아...ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100030008.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 이럴수가ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 이럴수가ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100030008.1.12",
          "form": "기아...기아....",
          "original_form": "기아...기아....",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100030008.1.13",
          "form": "오늘 시합 있네요!!",
          "original_form": "오늘 시합 있네요!!",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100030008.1.14",
          "form": "꼭 이기길 바랄게요!!",
          "original_form": "꼭 이기길 바랄게요!!",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100030008.1.15",
          "form": "삼성이랑 같이 하는거 같은데요??",
          "original_form": "삼성이랑 같이 하는거 같은데요??",
          "speaker_id": "2",
          "time": "20210903 18:39"
        },
        {
          "id": "MDRW2100030008.1.16",
          "form": "현재 삼성 기아 2:0 인거같은데ㅋㅋㅋ",
          "original_form": "현재 삼성 기아 2:0 인거같은데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:40"
        },
        {
          "id": "MDRW2100030008.1.17",
          "form": "아...아앝....!!!!",
          "original_form": "아...아앝....!!!!",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100030008.1.18",
          "form": "과제 때문에 안보고 있었더니",
          "original_form": "과제 때문에 안보고 있었더니",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100030008.1.19",
          "form": "이런일이...!!",
          "original_form": "이런일이...!!",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100030008.1.20",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100030008.1.21",
          "form": "상황 너무 웃기네요ㅋㅋㅋㅋ",
          "original_form": "상황 너무 웃기네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100030008.1.22",
          "form": "뭐 져도 상관은 없습니다ㅋㅋㅋ",
          "original_form": "뭐 져도 상관은 없습니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100030008.1.23",
          "form": "이번 해는 마음에서 놔서ㅋㅋㅋ",
          "original_form": "이번 해는 마음에서 놔서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100030008.1.24",
          "form": "이의리 선수만 보고있거든요ㅋㅋㅋ",
          "original_form": "이의리 선수만 보고있거든요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100030008.1.25",
          "form": "ㅋㅋㅋ큐ㅠㅠㅠㅠㅠ이의리 선수 완전 잘하죠!!",
          "original_form": "ㅋㅋㅋ큐ㅠㅠㅠㅠㅠ이의리 선수 완전 잘하죠!!",
          "speaker_id": "1",
          "time": "20210903 18:44"
        },
        {
          "id": "MDRW2100030008.1.26",
          "form": "야구장에서 직관한 적 있으세용??",
          "original_form": "야구장에서 직관한 적 있으세용??",
          "speaker_id": "1",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100030008.1.27",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "2",
          "time": "20210903 18:47"
        },
        {
          "id": "MDRW2100030008.1.28",
          "form": "전 부산에 살아서 사직 몇번 다녀왔어요!!",
          "original_form": "전 부산에 살아서 사직 몇번 다녀왔어요!!",
          "speaker_id": "2",
          "time": "20210903 18:47"
        },
        {
          "id": "MDRW2100030008.1.29",
          "form": "직관 가보셨어요?",
          "original_form": "직관 가보셨어요?",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100030008.1.30",
          "form": "우와!!! 완전 부러워요!!",
          "original_form": "우와!!! 완전 부러워요!!",
          "speaker_id": "1",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100030008.1.31",
          "form": "전 야구에 빠진지 얼마 안 돼서",
          "original_form": "전 야구에 빠진지 얼마 안 돼서",
          "speaker_id": "1",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100030008.1.32",
          "form": "아직 직관은 못 가봤어요!!",
          "original_form": "아직 직관은 못 가봤어요!!",
          "speaker_id": "1",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100030008.1.33",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100030008.1.34",
          "form": "빨리 코로나가 끝나서 가기를ㅋㅋ",
          "original_form": "빨리 코로나가 끝나서 가기를ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100030008.1.35",
          "form": "3단계면 갈수있지않나여?",
          "original_form": "3단계면 갈수있지않나여?",
          "speaker_id": "2",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100030008.1.36",
          "form": "아마도요!?",
          "original_form": "아마도요!?",
          "speaker_id": "1",
          "time": "20210903 18:50"
        },
        {
          "id": "MDRW2100030008.1.37",
          "form": "근데 관중도 얼마 없을것같아서",
          "original_form": "근데 관중도 얼마 없을것같아서",
          "speaker_id": "1",
          "time": "20210903 18:50"
        },
        {
          "id": "MDRW2100030008.1.38",
          "form": "사람 많을때 가보고 싶어서요!!",
          "original_form": "사람 많을때 가보고 싶어서요!!",
          "speaker_id": "1",
          "time": "20210903 18:50"
        },
        {
          "id": "MDRW2100030008.1.39",
          "form": "솔직히 많을때가야 직관이죠ㅜ",
          "original_form": "솔직히 많을때가야 직관이죠ㅜ",
          "speaker_id": "2",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100030008.1.40",
          "form": "함성소리도 들어보고ㅠ",
          "original_form": "함성소리도 들어보고ㅠ",
          "speaker_id": "2",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100030008.1.41",
          "form": "그게 진짜 참맛이니깐요ㅜ",
          "original_form": "그게 진짜 참맛이니깐요ㅜ",
          "speaker_id": "2",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100030008.1.42",
          "form": "와아ㅏ아아ㅏㅏ맞아요!! 저 그런거 로망있어요!!",
          "original_form": "와아ㅏ아아ㅏㅏ맞아요!! 저 그런거 로망있어요!!",
          "speaker_id": "1",
          "time": "20210903 18:52"
        }
      ]
