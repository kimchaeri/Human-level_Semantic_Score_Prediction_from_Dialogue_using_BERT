      "utterance": [
        {
          "id": "MDRW2100040253.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040253.1.2",
          "form": "안녕하세용~ 백신 맞으셨나요",
          "original_form": "안녕하세용~ 백신 맞으셨나요",
          "speaker_id": "2",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040253.1.3",
          "form": "일차만 맞았어요",
          "original_form": "일차만 맞았어요",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040253.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040253.1.5",
          "form": "전 아까 2차 맞고 와서",
          "original_form": "전 아까 2차 맞고 와서",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040253.1.6",
          "form": "오늘 요양중이에요ㅋㅋㅋ",
          "original_form": "오늘 요양중이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040253.1.7",
          "form": "헉 팔 괜찮으세요?",
          "original_form": "헉 팔 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040253.1.8",
          "form": "팔 아프네요ㅋㅋㅋㅋ 근데 12시간 지나야 좀 본격 아프다고 그래서",
          "original_form": "팔 아프네요ㅋㅋㅋㅋ 근데 12시간 지나야 좀 본격 아프다고 그래서",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040253.1.9",
          "form": "그쵸 원래 이틀날이 더 아프더라구요",
          "original_form": "그쵸 원래 이틀날이 더 아프더라구요",
          "speaker_id": "1",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040253.1.10",
          "form": "네  얼마나 아플려나 걱정되네요",
          "original_form": "네  얼마나 아플려나 걱정되네요",
          "speaker_id": "2",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040253.1.11",
          "form": "그래도 팔만 아프고 지나가면 다행이죠",
          "original_form": "그래도 팔만 아프고 지나가면 다행이죠",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040253.1.12",
          "form": "백신 어떤거 맞으셨어요?",
          "original_form": "백신 어떤거 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040253.1.13",
          "form": "화이자요!",
          "original_form": "화이자요!",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040253.1.14",
          "form": "저도 일차 화이자 맞았어요",
          "original_form": "저도 일차 화이자 맞았어요",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040253.1.15",
          "form": "2차는 몸살 좀 오는 거 같더라구요",
          "original_form": "2차는 몸살 좀 오는 거 같더라구요",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040253.1.16",
          "form": "아이구 오늘 푹 쉬세요",
          "original_form": "아이구 오늘 푹 쉬세요",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040253.1.17",
          "form": "열이 있으신거예요?",
          "original_form": "열이 있으신거예요?",
          "speaker_id": "1",
          "time": "20210904 15:12"
        }
      ]
