      "utterance": [
        {
          "id": "MDRW2100009906.1.1",
          "form": "점심 머드시나요",
          "original_form": "점심 머드시나요",
          "speaker_id": "2",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009906.1.2",
          "form": "국수 하려고 생각중이에요~",
          "original_form": "국수 하려고 생각중이에요~",
          "speaker_id": "1",
          "time": "20210823 11:37"
        },
        {
          "id": "MDRW2100009906.1.3",
          "form": "저두 국수나왔네요",
          "original_form": "저두 국수나왔네요",
          "speaker_id": "2",
          "time": "20210823 11:41"
        },
        {
          "id": "MDRW2100009906.1.4",
          "form": "통했다",
          "original_form": "통했다",
          "speaker_id": "2",
          "time": "20210823 11:41"
        },
        {
          "id": "MDRW2100009906.1.5",
          "form": "와~ㅡ국수는 많이 먹어야되요 배가 금방 고프거든요",
          "original_form": "와~ㅡ국수는 많이 먹어야되요 배가 금방 고프거든요",
          "speaker_id": "1",
          "time": "20210823 13:00"
        },
        {
          "id": "MDRW2100009906.1.6",
          "form": "네 저두 야채 많이 넣어서 먹었어요 배부르네요",
          "original_form": "네 저두 야채 많이 넣어서 먹었어요 배부르네요",
          "speaker_id": "2",
          "time": "20210823 13:04"
        },
        {
          "id": "MDRW2100009906.1.7",
          "form": "식사잘하셨나요?",
          "original_form": "식사잘하셨나요?",
          "speaker_id": "2",
          "time": "20210823 13:05"
        },
        {
          "id": "MDRW2100009906.1.8",
          "form": "네~  이제 둘째 자면 커피한잔 하고 쉬고싶은데 안자네요ㅠ",
          "original_form": "네~  이제 둘째 자면 커피한잔 하고 쉬고싶은데 안자네요ㅠ",
          "speaker_id": "1",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100009906.1.9",
          "form": "엄머 둘째도 있으시군요ㅜㅜㅜ",
          "original_form": "엄머 둘째도 있으시군요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 14:54"
        },
        {
          "id": "MDRW2100009906.1.10",
          "form": "저는 동료분이 두유라떼 만들어줘서 마시고있어요",
          "original_form": "저는 동료분이 두유라떼 만들어줘서 마시고있어요",
          "speaker_id": "2",
          "time": "20210823 14:55"
        },
        {
          "id": "MDRW2100009906.1.11",
          "form": "우유말고 두유인데 참 고소하네요",
          "original_form": "우유말고 두유인데 참 고소하네요",
          "speaker_id": "2",
          "time": "20210823 14:55"
        },
        {
          "id": "MDRW2100009906.1.12",
          "form": "어떻게만든건지 ㅎㅎ 맛있겠네요",
          "original_form": "어떻게만든건지 ㅎㅎ 맛있겠네요",
          "speaker_id": "1",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100009906.1.13",
          "form": "저도 만든걸 받아서리ㅋㅋ 모르겠네요",
          "original_form": "저도 만든걸 받아서리ㅋㅋ 모르겠네요",
          "speaker_id": "2",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100009906.1.14",
          "form": "저는 방탄커피 안마셔봤는데 몽이님 드셔보셨어요?",
          "original_form": "저는 방탄커피 안마셔봤는데 몽이님 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100009906.1.15",
          "form": "아니욧. 들어보기는 했는데 못먹어봤어요",
          "original_form": "아니욧. 들어보기는 했는데 못먹어봤어요",
          "speaker_id": "1",
          "time": "20210823 16:07"
        },
        {
          "id": "MDRW2100009906.1.16",
          "form": "저두 ㅋㅋㅜㅜ 함찾아먹어봐야겠네요",
          "original_form": "저두 ㅋㅋㅜㅜ 함찾아먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100009906.1.17",
          "form": "유명카페는 드립백을 팔고있어서 그런거좀 사오면 편히 먹을수있어서 좋아요",
          "original_form": "유명카페는 드립백을 팔고있어서 그런거좀 사오면 편히 먹을수있어서 좋아요",
          "speaker_id": "2",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100009906.1.18",
          "form": "맞아요~ 드립커피는 먹기편하더라구요",
          "original_form": "맞아요~ 드립커피는 먹기편하더라구요",
          "speaker_id": "1",
          "time": "20210823 16:50"
        },
        {
          "id": "MDRW2100009906.1.19",
          "form": "숯불로 발효시킨 커피카페가있는데 잘하더라구요",
          "original_form": "숯불로 발효시킨 커피카페가있는데 잘하더라구요",
          "speaker_id": "2",
          "time": "20210823 16:52"
        }
      ]
