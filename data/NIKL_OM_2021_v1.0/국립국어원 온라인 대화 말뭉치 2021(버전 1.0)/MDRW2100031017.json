      "utterance": [
        {
          "id": "MDRW2100031017.1.1",
          "form": "여행 다니는거 좋아하시나요?",
          "original_form": "여행 다니는거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100031017.1.2",
          "form": "여행 아주 좋아합니다",
          "original_form": "여행 아주 좋아합니다",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031017.1.3",
          "form": "저도 여행 좋아해요",
          "original_form": "저도 여행 좋아해요",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031017.1.4",
          "form": "국내여행 좋아하시나요 해외여행 좋아하시나요",
          "original_form": "국내여행 좋아하시나요 해외여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031017.1.5",
          "form": "둘다 좋아요 사실",
          "original_form": "둘다 좋아요 사실",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031017.1.6",
          "form": "맞아요 여행은 ㅁ어디든 즐겁죠",
          "original_form": "맞아요 여행은 ㅁ어디든 즐겁죠",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031017.1.7",
          "form": "굳이따지자면 해외가 더 좋긴해요",
          "original_form": "굳이따지자면 해외가 더 좋긴해요",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031017.1.8",
          "form": "오 그러시군요",
          "original_form": "오 그러시군요",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031017.1.9",
          "form": "해외가 아무래도 전혀 다른 세계 온거같은 느낌이라서요",
          "original_form": "해외가 아무래도 전혀 다른 세계 온거같은 느낌이라서요",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031017.1.10",
          "form": "맞아요 공감합니다",
          "original_form": "맞아요 공감합니다",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031017.1.11",
          "form": "음식도 색다르고 볼거리도 색다르고",
          "original_form": "음식도 색다르고 볼거리도 색다르고",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031017.1.12",
          "form": "이국적인 풍경이 매력이죠",
          "original_form": "이국적인 풍경이 매력이죠",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031017.1.13",
          "form": "외국사람들 많은것도 신기하고요",
          "original_form": "외국사람들 많은것도 신기하고요",
