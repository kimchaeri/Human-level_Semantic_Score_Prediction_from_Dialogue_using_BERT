        },
        {
          "id": "MDRW2100044198.1.2",
          "form": "사람 많더라",
          "original_form": "사람 많더라",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044198.1.3",
          "form": "먹고싶다... 막창",
          "original_form": "먹고싶다... 막창",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044198.1.4",
          "form": "요 근처는 갈만한데가 없엉",
          "original_form": "요 근처는 갈만한데가 없엉",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044198.1.5",
          "form": "그 동네엔 걸작 떡볶이가 있자나ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그 동네엔 걸작 떡볶이가 있자나ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044198.1.6",
          "form": "걸작 맛있지ㅋㅋㅋㅋ",
          "original_form": "걸작 맛있지ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044198.1.7",
          "form": "부대 떡볶이는 생각보다 별로였어",
          "original_form": "부대 떡볶이는 생각보다 별로였어",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044198.1.8",
          "form": "치킨도 맛있던가요",
          "original_form": "치킨도 맛있던가요",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044198.1.9",
          "form": "평범하게 맛있었음",
          "original_form": "평범하게 맛있었음",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044198.1.10",
          "form": "딱 평타?",
          "original_form": "딱 평타?",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044198.1.11",
          "form": "오 나쁘지 않네",
          "original_form": "오 나쁘지 않네",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044198.1.12",
          "form": "떡볶이 국물에 치킨 찍어먹구ㅋㅋ",
          "original_form": "떡볶이 국물에 치킨 찍어먹구ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044198.1.13",
          "form": "그런 용도?의 치킨이면 평타로 충분하다",
          "original_form": "그런 용도?의 치킨이면 평타로 충분하다",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044198.1.14",
          "form": "그치 튀김 대신이야ㅋㅋㅋㅋ",
          "original_form": "그치 튀김 대신이야ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044198.1.15",
          "form": "네고왕 땐 결국 못 먹었댔나?",
          "original_form": "네고왕 땐 결국 못 먹었댔나?",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044198.1.16",
          "form": "ㅇㅇ금방 마감되던데",
          "original_form": "ㅇㅇ금방 마감되던데",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044198.1.17",
          "form": "사람들 네고왕하면 서버 터뜨리고....ㅋㅋㅋㅋㅋㅋㅋㅋ",
