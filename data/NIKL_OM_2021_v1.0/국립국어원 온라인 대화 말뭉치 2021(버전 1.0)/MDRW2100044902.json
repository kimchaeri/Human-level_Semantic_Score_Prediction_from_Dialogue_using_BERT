      "utterance": [
        {
          "id": "MDRW2100044902.1.1",
          "form": "과일 아니예요!",
          "original_form": "과일 아니예요!",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044902.1.2",
          "form": "혹시 안흔한건가유ㅜㅠ",
          "original_form": "혹시 안흔한건가유ㅜㅠ",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044902.1.3",
          "form": "아니요 열라 흔한 음식이예요",
          "original_form": "아니요 열라 흔한 음식이예요",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044902.1.4",
          "form": "주변에서 쉽게 구할 수 있어요",
          "original_form": "주변에서 쉽게 구할 수 있어요",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044902.1.5",
          "form": "고긴가요??",
          "original_form": "고긴가요??",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044902.1.6",
          "form": "아니요 야채입니다",
          "original_form": "아니요 야채입니다",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044902.1.7",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044902.1.8",
          "form": "틈메이러",
          "original_form": "틈메이러",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044902.1.9",
          "form": "인가요?",
          "original_form": "인가요?",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044902.1.10",
          "form": "헉 아니요! 토마토 넘 좋아하지만 오늘 점심은 아니예요ㅋㅋㅋ",
          "original_form": "헉 아니요! 토마토 넘 좋아하지만 오늘 점심은 아니예요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044902.1.11",
          "form": "오이만... 드신건아니죠?",
          "original_form": "오이만... 드신건아니죠?",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044902.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 비슷해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 비슷해요",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044902.1.13",
          "form": "오이는 아니지만",
          "original_form": "오이는 아니지만",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044902.1.14",
          "form": "이것만 먹었습니다",
          "original_form": "이것만 먹었습니다",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044902.1.15",
          "form": "헐므지",
          "original_form": "헐므지",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044902.1.16",
          "form": "오이랑 비슷한 이것",
          "original_form": "오이랑 비슷한 이것",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044902.1.17",
          "form": "당근?",
          "original_form": "당근?",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044902.1.18",
          "form": "앗 오이랑 전혀 안 비슷하게 생겼어요 제가 말을 헷갈리게 했어요",
          "original_form": "앗 오이랑 전혀 안 비슷하게 생겼어요 제가 말을 헷갈리게 했어요",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044902.1.19",
          "form": "이것만 단독으로 먹을 수 있는 게 비슷해요",
          "original_form": "이것만 단독으로 먹을 수 있는 게 비슷해요",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044902.1.20",
          "form": "딸기!!!",
          "original_form": "딸기!!!",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044902.1.21",
          "form": "당근 ㅇㅋㅋㅋ점점 초식동물이 되어가고 있어요",
          "original_form": "당근 ㅇㅋㅋㅋ점점 초식동물이 되어가고 있어요",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044902.1.22",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044902.1.23",
          "form": "고구마!!!!",
          "original_form": "고구마!!!!",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044902.1.24",
          "form": "고구마다 유레카!!!",
          "original_form": "고구마다 유레카!!!",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044902.1.25",
          "form": "아 비슷해요!!!!!!!",
          "original_form": "아 비슷해요!!!!!!!",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044902.1.26",
