      "utterance": [
        {
          "id": "MDRW2100045667.1.1",
          "form": "어떤 종류죠?",
          "original_form": "어떤 종류죠?",
          "speaker_id": "2",
          "time": "20210908 05:03"
        },
        {
          "id": "MDRW2100045667.1.2",
          "form": "아까처럼 야채 채소 범주에 드는 식재료예요",
          "original_form": "아까처럼 야채 채소 범주에 드는 식재료예요",
          "speaker_id": "1",
          "time": "20210908 05:03"
        },
        {
          "id": "MDRW2100045667.1.3",
          "form": "좋아요. 음...",
          "original_form": "좋아요. 음...",
          "speaker_id": "2",
          "time": "20210908 05:03"
        },
        {
          "id": "MDRW2100045667.1.4",
          "form": "머릿속에선 왠지 모루지만 오이가 샹각나는군요...ㅋㅋㅋㅋㅋ",
          "original_form": "머릿속에선 왠지 모루지만 오이가 샹각나는군요...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:04"
        },
        {
          "id": "MDRW2100045667.1.5",
          "form": "후후후후 오이 아닙니다",
          "original_form": "후후후후 오이 아닙니다",
          "speaker_id": "1",
          "time": "20210908 05:04"
        },
        {
          "id": "MDRW2100045667.1.6",
          "form": "그렇겠죠ㅠㅠ",
          "original_form": "그렇겠죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045667.1.7",
          "form": "매운가요?",
          "original_form": "매운가요?",
          "speaker_id": "2",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045667.1.8",
          "form": "아니요! 저언혀 안 맵습니다 호호",
          "original_form": "아니요! 저언혀 안 맵습니다 호호",
          "speaker_id": "1",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045667.1.9",
          "form": "토마토?!",
          "original_form": "토마토?!",
          "speaker_id": "2",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045667.1.10",
          "form": "땡~ 토마토 아닙니다",
          "original_form": "땡~ 토마토 아닙니다",
          "speaker_id": "1",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045667.1.11",
          "form": "음.... 너란색인가요?",
          "original_form": "음.... 너란색인가요?",
          "speaker_id": "2",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045667.1.12",
          "form": "아~니요 노란색 식재료가 아닙니다",
          "original_form": "아~니요 노란색 식재료가 아닙니다",
          "speaker_id": "1",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045667.1.13",
          "form": "음.... 하얀색인가요?",
          "original_form": "음.... 하얀색인가요?",
          "speaker_id": "2",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045667.1.14",
          "form": "네 하얀색이예요!",
          "original_form": "네 하얀색이예요!",
          "speaker_id": "1",
          "time": "20210908 05:10"
        },
        {
          "id": "MDRW2100045667.1.15",
          "form": "양파!!",
          "original_form": "양파!!",
          "speaker_id": "2",
          "time": "20210908 05:11"
        },
        {
          "id": "MDRW2100045667.1.16",
          "form": "이번엔 양파라고 믿어요!!",
          "original_form": "이번엔 양파라고 믿어요!!",
          "speaker_id": "2",
          "time": "20210908 05:12"
        },
        {
          "id": "MDRW2100045667.1.17",
          "form": "아닙니다 후후후후 양파 아니예요",
          "original_form": "아닙니다 후후후후 양파 아니예요",
          "speaker_id": "1",
          "time": "20210908 05:12"
        },
        {
          "id": "MDRW2100045667.1.18",
          "form": "그럼 마늘이요!!",
          "original_form": "그럼 마늘이요!!",
          "speaker_id": "2",
          "time": "20210908 05:12"
        },
        {
          "id": "MDRW2100045667.1.19",
          "form": "뒝~ 마늘도 아닙니다!",
          "original_form": "뒝~ 마늘도 아닙니다!",
          "speaker_id": "1",
          "time": "20210908 05:13"
        },
        {
          "id": "MDRW2100045667.1.20",
          "form": "여러 요소를 물어보세요 핫하",
          "original_form": "여러 요소를 물어보세요 핫하",
          "speaker_id": "1",
          "time": "20210908 05:13"
        },
        {
          "id": "MDRW2100045667.1.21",
          "form": "어라랏 다 빗나갔네요",
          "original_form": "어라랏 다 빗나갔네요",
          "speaker_id": "2",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045667.1.22",
          "form": "하하하 어려울것이야",
          "original_form": "하하하 어려울것이야",
          "speaker_id": "1",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045667.1.23",
          "form": "음.... 향신료 인가요?",
          "original_form": "음.... 향신료 인가요?",
          "speaker_id": "2",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045667.1.24",
          "form": "아니요 향신료는 아니예요",
          "original_form": "아니요 향신료는 아니예요",
          "speaker_id": "1",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045667.1.25",
          "form": "향신료가 아닙니다!",
          "original_form": "향신료가 아닙니다!",
          "speaker_id": "1",
          "time": "20210908 05:14"
        }
      ]
