      "utterance": [
        {
          "id": "MDRW2100005019.1.1",
          "form": "안녕하세요~ 온라인 커뮤니티에 대해 대화하라고 하시네요",
          "original_form": "안녕하세요~ 온라인 커뮤니티에 대해 대화하라고 하시네요",
          "speaker_id": "1",
          "time": "20210821 19:58"
        },
        {
          "id": "MDRW2100005019.1.2",
          "form": "보통 무슨 온라인 커뮤니티를 사용하세요?",
          "original_form": "보통 무슨 온라인 커뮤니티를 사용하세요?",
          "speaker_id": "1",
          "time": "20210821 19:58"
        },
        {
          "id": "MDRW2100005019.1.3",
          "form": "놀면뭐하니 보고 원슈타인에 빠져서",
          "original_form": "놀면뭐하니 보고 원슈타인에 빠져서",
          "speaker_id": "2",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100005019.1.4",
          "form": "처음으로 팬카페 가입해봤네요",
          "original_form": "처음으로 팬카페 가입해봤네요",
          "speaker_id": "2",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100005019.1.5",
          "form": "아 원슈타인 좋죠 ㅎㅎㅎ",
          "original_form": "아 원슈타인 좋죠 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100005019.1.6",
          "form": "저는 쇼미더머니 보고 알았어요",
          "original_form": "저는 쇼미더머니 보고 알았어요",
          "speaker_id": "1",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100005019.1.7",
          "form": "작년에 안챙겨봐서 몰랐다가ㅎㅎ 이제 보러구요",
          "original_form": "작년에 안챙겨봐서 몰랐다가ㅎㅎ 이제 보러구요",
          "speaker_id": "2",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100005019.1.8",
          "form": "온라인 커뮤니티 하시는거 있으세요?",
          "original_form": "온라인 커뮤니티 하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100005019.1.9",
          "form": "저는 좀 다양하게 하는 거 같아요 ㅋㅋ",
          "original_form": "저는 좀 다양하게 하는 거 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100005019.1.10",
          "form": "어릴 때부터 온갖 커뮤니티를 다 돌아다녔어서..",
          "original_form": "어릴 때부터 온갖 커뮤니티를 다 돌아다녔어서..",
          "speaker_id": "1",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100005019.1.11",
          "form": "요즘은 네이버 카페 열심히 합니다 ㅎㅎ",
          "original_form": "요즘은 네이버 카페 열심히 합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100005019.1.12",
          "form": "저도 네이버 팬카페에요",
          "original_form": "저도 네이버 팬카페에요",
          "speaker_id": "2",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005019.1.13",
          "form": "다른 커뮤니티는 지역 카페하구요",
          "original_form": "다른 커뮤니티는 지역 카페하구요",
          "speaker_id": "2",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005019.1.14",
          "form": "저도 지역 카페 봐요 ㅎㅎ",
          "original_form": "저도 지역 카페 봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005019.1.15",
          "form": "아무래도 정보가 많으니",
          "original_form": "아무래도 정보가 많으니",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005019.1.16",
          "form": "살기가 편하더라고요",
          "original_form": "살기가 편하더라고요",
          "speaker_id": "1",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100005019.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005019.1.18",
          "form": "모르거나 유용한 정보가 있어서 좋아요",
          "original_form": "모르거나 유용한 정보가 있어서 좋아요",
          "speaker_id": "2",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005019.1.19",
          "form": "글고 봉사같은 것도",
          "original_form": "글고 봉사같은 것도",
          "speaker_id": "1",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005019.1.20",
          "form": "은근히 많이 진행해서",
          "original_form": "은근히 많이 진행해서",
          "speaker_id": "1",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005019.1.21",
          "form": "사람사는 기분이 들어요 ㅎㅎ",
          "original_form": "사람사는 기분이 들어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005019.1.22",
          "form": "온라인 커뮤니티에서는 안해봤는데 좋네요",
          "original_form": "온라인 커뮤니티에서는 안해봤는데 좋네요",
          "speaker_id": "2",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005019.1.23",
          "form": "전 눈팅족이라ㅎㅎ",
          "original_form": "전 눈팅족이라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005019.1.24",
          "form": "아 ㅎㅎㅎㅎ 저도 눈팅족이에요 ㅋㅋ",
          "original_form": "아 ㅎㅎㅎㅎ 저도 눈팅족이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005019.1.25",
          "form": "가끔 이벤트나 참여하고 ㅋㅋ",
          "original_form": "가끔 이벤트나 참여하고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005019.1.26",
          "form": "저두요 글을 재밌게 쓰지못해서ㅠㅋㅋ",
          "original_form": "저두요 글을 재밌게 쓰지못해서ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005019.1.27",
          "form": "글 잘 쓰시는 분들은 참 ㅎㅎ 그런 글도 재밌게 쓰시더라고요. 카페 운영진 분들이나 뭐 ㅎㅎ",
          "original_form": "글 잘 쓰시는 분들은 참 ㅎㅎ 그런 글도 재밌게 쓰시더라고요. 카페 운영진 분들이나 뭐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005019.1.28",
          "form": "운영진분들 대단ㅇ해요",
          "original_form": "운영진분들 대단ㅇ해요",
