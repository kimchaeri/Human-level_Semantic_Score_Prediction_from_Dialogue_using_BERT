      "utterance": [
        {
          "id": "MDRW2100017753.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017753.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017753.1.3",
          "form": "일 하고 계신가요?",
          "original_form": "일 하고 계신가요?",
          "speaker_id": "1",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017753.1.4",
          "form": "네 저 직장인이에요~ 님은요?",
          "original_form": "네 저 직장인이에요~ 님은요?",
          "speaker_id": "2",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100017753.1.5",
          "form": "저는 취준생이에요!",
          "original_form": "저는 취준생이에요!",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017753.1.6",
          "form": "코로나19 상황에서 구직하는 소감은요?",
          "original_form": "코로나19 상황에서 구직하는 소감은요?",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017753.1.7",
          "form": "만만치 않구나.. 하고 있어요",
          "original_form": "만만치 않구나.. 하고 있어요",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017753.1.8",
          "form": "일하는곳은 어떠세요?",
          "original_form": "일하는곳은 어떠세요?",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017753.1.9",
          "form": "다른 직업에 비해서 만족하는 편이에요~",
          "original_form": "다른 직업에 비해서 만족하는 편이에요~",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017753.1.10",
          "form": "가끔 스트레스도 있긴 한데 그건 어딜가나 다 있으니까요 ㅜㅜ",
          "original_form": "가끔 스트레스도 있긴 한데 그건 어딜가나 다 있으니까요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017753.1.11",
          "form": "그쵸ㅠㅠ",
          "original_form": "그쵸ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017753.1.12",
          "form": "사람이 참 힘든거같아요",
          "original_form": "사람이 참 힘든거같아요",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017753.1.13",
          "form": "알바나 인턴 해본 적 있어요?",
          "original_form": "알바나 인턴 해본 적 있어요?",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017753.1.14",
          "form": "네! 작년까지 일했어요",
          "original_form": "네! 작년까지 일했어요",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017753.1.15",
          "form": "어떤 일 해보셨어요?",
          "original_form": "어떤 일 해보셨어요?",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017753.1.16",
          "form": "사무직이요ㅎㅎ",
          "original_form": "사무직이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017753.1.17",
          "form": "그럼 사람으로 인한 스트레스는 없었겠네요 ㅎㅎ",
          "original_form": "그럼 사람으로 인한 스트레스는 없었겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017753.1.18",
          "form": "생각보다 있더라구요ㅎㅎ",
          "original_form": "생각보다 있더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017753.1.19",
          "form": "무엇때문에요?",
          "original_form": "무엇때문에요?",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017753.1.20",
          "form": "사람이 있으면 없을수는 없더라구요",
          "original_form": "사람이 있으면 없을수는 없더라구요",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017753.1.21",
          "form": "스트레스 없으신편인가요?",
          "original_form": "스트레스 없으신편인가요?",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017753.1.22",
          "form": "저는 독립 사무실 따로 있어서 괜찮아요 ㅎㅎ",
          "original_form": "저는 독립 사무실 따로 있어서 괜찮아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017753.1.23",
          "form": "구직하기 위해서 스펙은 많이 쌓아두셨나요?",
          "original_form": "구직하기 위해서 스펙은 많이 쌓아두셨나요?",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017753.1.24",
          "form": "진로를 바꾼거라 상관이 없어 슬프네요ㅠ",
          "original_form": "진로를 바꾼거라 상관이 없어 슬프네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:36"
        }
      ]
    }
