      "utterance": [
        {
          "id": "MDRW2100046044.1.1",
          "form": "근데 신발도 넘 욕심나요",
          "original_form": "근데 신발도 넘 욕심나요",
          "speaker_id": "2",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046044.1.2",
          "form": "지네이고싶어요",
          "original_form": "지네이고싶어요",
          "speaker_id": "2",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046044.1.3",
          "form": "저는신발은",
          "original_form": "저는신발은",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046044.1.4",
          "form": "발이워낙",
          "original_form": "발이워낙",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046044.1.5",
          "form": "세상엔 예쁜 게 너무 많네요...",
          "original_form": "세상엔 예쁜 게 너무 많네요...",
          "speaker_id": "2",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046044.1.6",
          "form": "구두를오래못신어서 흑흑",
          "original_form": "구두를오래못신어서 흑흑",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046044.1.7",
          "form": "구두잘신는분들부러워영",
          "original_form": "구두잘신는분들부러워영",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046044.1.8",
          "form": "저도 그런데도 욕심이 나니",
          "original_form": "저도 그런데도 욕심이 나니",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046044.1.9",
          "form": "그게 문제예요ㅠㅠㅠㅠ",
          "original_form": "그게 문제예요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046044.1.10",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046044.1.11",
          "form": "차도 없음서ㅋㅋㅋㅋ",
          "original_form": "차도 없음서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046044.1.12",
          "form": "ㅋㅋㅋㅋㅋ저두",
          "original_form": "ㅋㅋㅋㅋㅋ저두",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046044.1.13",
          "form": "그래서 차를 사고싶네요",
          "original_form": "그래서 차를 사고싶네요",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046044.1.14",
          "form": "그게 너무 웃겨요",
          "original_form": "그게 너무 웃겨요",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046044.1.15",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046044.1.16",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046044.1.17",
          "form": "이쁜신 신으려면 차가..",
          "original_form": "이쁜신 신으려면 차가..",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046044.1.18",
          "form": "있어야..",
          "original_form": "있어야..",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046044.1.19",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046044.1.20",
          "form": "근데 차도 또 이왕사는 거 물욕생기고...",
          "original_form": "근데 차도 또 이왕사는 거 물욕생기고...",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046044.1.21",
          "form": "걍 물욕유니버스예요",
          "original_form": "걍 물욕유니버스예요",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046044.1.22",
          "form": "네 ㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046044.1.23",
          "form": "저는그래서 신발은 그래도",
          "original_form": "저는그래서 신발은 그래도",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046044.1.24",
          "form": "조은거사려고",
          "original_form": "조은거사려고",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046044.1.25",
          "form": "ㅠㅜ많이못삼",
          "original_form": "ㅠㅜ많이못삼",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046044.1.26",
          "form": "전에 자라 구두삿더니",
          "original_form": "전에 자라 구두삿더니",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046044.1.27",
          "form": "아 그거 안돼요",
          "original_form": "아 그거 안돼요",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046044.1.28",
          "form": "한번신고 무릎아파서 몇달고생",
          "original_form": "한번신고 무릎아파서 몇달고생",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046044.1.29",
          "form": "자라 신발은 진심 노답ㅜㅜ",
          "original_form": "자라 신발은 진심 노답ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046044.1.30",
          "form": "신발은 좋은 거 신어야돼요",
          "original_form": "신발은 좋은 거 신어야돼요",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046044.1.31",
          "form": "진짜요 ㅋㅋ최악",
          "original_form": "진짜요 ㅋㅋ최악",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046044.1.32",
          "form": "근데 문제는 신발은 진짜",
          "original_form": "근데 문제는 신발은 진짜",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046044.1.33",
          "form": "이쁜 건 너무 비싸더라고요",
          "original_form": "이쁜 건 너무 비싸더라고요",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046044.1.34",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046044.1.35",
          "form": "이쁘고편한거진짜",
          "original_form": "이쁘고편한거진짜",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046044.1.36",
          "form": "가격저세상",
          "original_form": "가격저세상",
          "speaker_id": "1",
          "time": "20210908 09:52"
        }
      ]
