      "utterance": [
        {
          "id": "MDRW2100038360.1.1",
          "form": "머 재밌는게 있어야지",
          "original_form": "머 재밌는게 있어야지",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038360.1.2",
          "form": "예능 본방사수는 새로운거 아닌가",
          "original_form": "예능 본방사수는 새로운거 아닌가",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038360.1.3",
          "form": "매주 새로운 편 해주는데...",
          "original_form": "매주 새로운 편 해주는데...",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038360.1.4",
          "form": "그렇게 따지면 점프도 매주 나오는데....",
          "original_form": "그렇게 따지면 점프도 매주 나오는데....",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100038360.1.5",
          "form": "그럼 된거 아닌가?",
          "original_form": "그럼 된거 아닌가?",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100038360.1.6",
          "form": "와 알고보니 나도 기력있는 사람이었어!",
          "original_form": "와 알고보니 나도 기력있는 사람이었어!",
          "speaker_id": "1",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100038360.1.7",
          "form": "다음 편 챙겨보는것만으로도 기력있는거 아님?",
          "original_form": "다음 편 챙겨보는것만으로도 기력있는거 아님?",
          "speaker_id": "2",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100038360.1.8",
          "form": "기력의 기준 그렇게 낮았다니",
          "original_form": "기력의 기준 그렇게 낮았다니",
          "speaker_id": "1",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100038360.1.9",
          "form": "존재하면 1이겠죠",
          "original_form": "존재하면 1이겠죠",
          "speaker_id": "2",
          "time": "20210902 12:12"
        },
        {
          "id": "MDRW2100038360.1.10",
          "form": "그럼 왓챠나 넷플도 안 보겠네?",
          "original_form": "그럼 왓챠나 넷플도 안 보겠네?",
