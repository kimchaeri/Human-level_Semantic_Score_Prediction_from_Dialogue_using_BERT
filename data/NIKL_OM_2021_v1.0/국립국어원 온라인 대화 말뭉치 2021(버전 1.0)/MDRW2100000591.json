      "utterance": [
        {
          "id": "MDRW2100000591.1.1",
          "form": "식사는 하셨나요~?",
          "original_form": "식사는 하셨나요~?",
          "speaker_id": "1",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000591.1.2",
          "form": "아 네네",
          "original_form": "아 네네",
          "speaker_id": "2",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000591.1.3",
          "form": "아 아직이네요 ㅎ",
          "original_form": "아 아직이네요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000591.1.4",
          "form": "앗 질문이 애매했네용~ 점심은 드셨나요~?",
          "original_form": "앗 질문이 애매했네용~ 점심은 드셨나요~?",
          "speaker_id": "1",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000591.1.5",
          "form": "ㅎㅎ 괜찮습니다",
          "original_form": "ㅎㅎ 괜찮습니다",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000591.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000591.1.7",
          "form": "저녁은 아직이시죠?",
          "original_form": "저녁은 아직이시죠?",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000591.1.8",
          "form": "ㅎㅎ네~ 6시쯤 먹는것아요~",
          "original_form": "ㅎㅎ네~ 6시쯤 먹는것아요~",
          "speaker_id": "1",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000591.1.9",
          "form": "점심은 뭐 드셨나요~?",
          "original_form": "점심은 뭐 드셨나요~?",
          "speaker_id": "1",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000591.1.10",
          "form": "카레 먹었으습니다",
          "original_form": "카레 먹었으습니다",
          "speaker_id": "2",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000591.1.11",
          "form": "회사 근처에 맛있는 카레집에 있거든요 함박 카레",
          "original_form": "회사 근처에 맛있는 카레집에 있거든요 함박 카레",
          "speaker_id": "2",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000591.1.12",
          "form": "우와 함박카레 맛있을것같아요~",
          "original_form": "우와 함박카레 맛있을것같아요~",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000591.1.13",
          "form": "저는 뼈다귀탕 먹었습니다~ 오랜만에 먹으니 맛있더라구요~",
          "original_form": "저는 뼈다귀탕 먹었습니다~ 오랜만에 먹으니 맛있더라구요~",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000591.1.14",
          "form": "와 ㅎ",
          "original_form": "와 ㅎ",
          "speaker_id": "2",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000591.1.15",
          "form": "뼈다귀 탕도 맛있었겠네요",
          "original_form": "뼈다귀 탕도 맛있었겠네요",
          "speaker_id": "2",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000591.1.16",
          "form": "맞어요~ 맛있었습니다!ㅎㅎ",
          "original_form": "맞어요~ 맛있었습니다!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000591.1.17",
          "form": "배달음식도 많이 시켜드시나요#?",
          "original_form": "배달음식도 많이 시켜드시나요#?",
          "speaker_id": "1",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000591.1.18",
          "form": "사실은 저는 요리를 못해서요 회사에서 주는 밥도 늘 맛있고 외식도 좋아하지만",
          "original_form": "사실은 저는 요리를 못해서요 회사에서 주는 밥도 늘 맛있고 외식도 좋아하지만",
          "speaker_id": "2",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000591.1.19",
          "form": "배달은 치킨 말고는 싫더라구요 일회용 쓰레기가 너무 많이 생기는 것도 별로 이고요",
          "original_form": "배달은 치킨 말고는 싫더라구요 일회용 쓰레기가 너무 많이 생기는 것도 별로 이고요",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000591.1.20",
          "form": "배달음식 많이 시켜 드시나요?",
          "original_form": "배달음식 많이 시켜 드시나요?",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000591.1.21",
          "form": "아하~ 저는 가족과 함께 살아서 밥도 많이 먹는데 주에 2-3번은 시켜먹는것같아요~",
          "original_form": "아하~ 저는 가족과 함께 살아서 밥도 많이 먹는데 주에 2-3번은 시켜먹는것같아요~",
          "speaker_id": "1",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000591.1.22",
          "form": "외식을 자주 했는데 요즘에 코로나가 심해서요ㅠ",
          "original_form": "외식을 자주 했는데 요즘에 코로나가 심해서요ㅠ",
          "speaker_id": "1",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000591.1.23",
          "form": "네 코로나 ㅜㅜ",
          "original_form": "네 코로나 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000591.1.24",
          "form": "안 시켜먹을수 없죠",
          "original_form": "안 시켜먹을수 없죠",
          "speaker_id": "2",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000591.1.25",
          "form": "맞아요ㅠㅠ 재난지원금도 거의 배달음식이나",
          "original_form": "맞아요ㅠㅠ 재난지원금도 거의 배달음식이나",
          "speaker_id": "1",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000591.1.26",
          "form": "사먹는데 든것같아요ㅠ",
          "original_form": "사먹는데 든것같아요ㅠ",
          "speaker_id": "1",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000591.1.27",
          "form": "네 먹는게 진짜 큰일!",
          "original_form": "네 먹는게 진짜 큰일!",
          "speaker_id": "2",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000591.1.28",
          "form": "요리못하는 사람은 진짜 고역 ㅜㅜ",
          "original_form": "요리못하는 사람은 진짜 고역 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 17:15"
        },
        {
          "id": "MDRW2100000591.1.29",
          "form": "맞아요ㅠ 저도 할줄아는 요리가 많이 없다보니 혼자있을때는 시켜먹는게 돈도 아깝구",
          "original_form": "맞아요ㅠ 저도 할줄아는 요리가 많이 없다보니 혼자있을때는 시켜먹는게 돈도 아깝구",
          "speaker_id": "1",
          "time": "20210819 17:17"
        }
      ]
    }
