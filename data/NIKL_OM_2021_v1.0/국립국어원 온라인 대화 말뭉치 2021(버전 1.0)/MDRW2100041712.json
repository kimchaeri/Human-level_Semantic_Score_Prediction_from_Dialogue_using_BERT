      "utterance": [
        {
          "id": "MDRW2100041712.1.1",
          "form": "네 즐겨마셔요",
          "original_form": "네 즐겨마셔요",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041712.1.2",
          "form": "좋겠어요",
          "original_form": "좋겠어요",
          "speaker_id": "1",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041712.1.3",
          "form": "전 커피향을 아주 좋아해서 마시고 싶은데 마시면 잠을 못자요",
          "original_form": "전 커피향을 아주 좋아해서 마시고 싶은데 마시면 잠을 못자요",
          "speaker_id": "1",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041712.1.4",
          "form": "그래서 커피 잘 마시는 분들 보면 부럽답니다",
          "original_form": "그래서 커피 잘 마시는 분들 보면 부럽답니다",
          "speaker_id": "1",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041712.1.5",
          "form": "직장동료는 맹물은 싫고해서 커피 1 물 9 비율로 마셔요",
          "original_form": "직장동료는 맹물은 싫고해서 커피 1 물 9 비율로 마셔요",
          "speaker_id": "2",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100041712.1.6",
          "form": "향과 약한 커피맛만 난대요",
          "original_form": "향과 약한 커피맛만 난대요",
          "speaker_id": "2",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100041712.1.7",
          "form": "대단한 비율이네요",
          "original_form": "대단한 비율이네요",
          "speaker_id": "1",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100041712.1.8",
          "form": "저는 커피좋아하는 분들 이야기 들으면서 대리만족합니다",
          "original_form": "저는 커피좋아하는 분들 이야기 들으면서 대리만족합니다",
          "speaker_id": "1",
          "time": "20210906 19:34"
        },
        {
          "id": "MDRW2100041712.1.9",
          "form": "제대로 즐기면 진짜 맛있다던데",
          "original_form": "제대로 즐기면 진짜 맛있다던데",
          "speaker_id": "1",
          "time": "20210906 19:34"
        },
        {
          "id": "MDRW2100041712.1.10",
          "form": "전 아직 촌시러워서 산미가 강한 커피는 별로",
          "original_form": "전 아직 촌시러워서 산미가 강한 커피는 별로",
          "speaker_id": "2",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100041712.1.11",
          "form": "저도 산미는 별로",
          "original_form": "저도 산미는 별로",
          "speaker_id": "1",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100041712.1.12",
          "form": "고소한 게 좋더라구요",
          "original_form": "고소한 게 좋더라구요",
          "speaker_id": "1",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100041712.1.13",
          "form": "신선한 원두가 산미가 강하다고 하더라구오",
          "original_form": "신선한 원두가 산미가 강하다고 하더라구오",
          "speaker_id": "2",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100041712.1.14",
          "form": "저도 그거 들었는데",
          "original_form": "저도 그거 들었는데",
          "speaker_id": "1",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100041712.1.15",
          "form": "그냥 덜 신선한 원두 먹으려구요..ㅎㅎㅎ",
          "original_form": "그냥 덜 신선한 원두 먹으려구요..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100041712.1.16",
          "form": "전 빡세게 로스팅한걸로",
          "original_form": "전 빡세게 로스팅한걸로",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100041712.1.17",
          "form": "그러나저러나 커피 마음대로 마실 수 있는 분들 부러워요",
          "original_form": "그러나저러나 커피 마음대로 마실 수 있는 분들 부러워요",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100041712.1.18",
          "form": "저는 어쩌다가 그것도 아침 일찍 마셔야 돼서요",
          "original_form": "저는 어쩌다가 그것도 아침 일찍 마셔야 돼서요",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100041712.1.19",
          "form": "참 아쉬워요",
          "original_form": "참 아쉬워요",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100041712.1.20",
          "form": "혹시 마시면 잠이 안오는거예요?",
          "original_form": "혹시 마시면 잠이 안오는거예요?",
          "speaker_id": "2",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100041712.1.21",
          "form": "전 오히려 안마시면...",
          "original_form": "전 오히려 안마시면...",
          "speaker_id": "2",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100041712.1.22",
          "form": "네 밤에 못자요",
          "original_form": "네 밤에 못자요",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100041712.1.23",
          "form": "그런분들 많아요",
          "original_form": "그런분들 많아요",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100041712.1.24",
          "form": "저희엄마도 커피는 좋아하시지만 밤엔 안드세요",
          "original_form": "저희엄마도 커피는 좋아하시지만 밤엔 안드세요",
          "speaker_id": "2",
          "time": "20210906 22:15"
        }
      ]
    }
