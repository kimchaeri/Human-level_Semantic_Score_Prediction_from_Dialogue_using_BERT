      "utterance": [
        {
          "id": "MDRW2100011908.1.1",
          "form": "집가면 전등 갈아야해여...",
          "original_form": "집가면 전등 갈아야해여...",
          "speaker_id": "1",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100011908.1.2",
          "form": "두꺼비집 내리고 교체하세요!",
          "original_form": "두꺼비집 내리고 교체하세요!",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011908.1.3",
          "form": "저는 어릴 때 콘센트에 뭐 넣다가 감전 된 적이 있어서 무서워서 전등 못갈아요..",
          "original_form": "저는 어릴 때 콘센트에 뭐 넣다가 감전 된 적이 있어서 무서워서 전등 못갈아요..",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011908.1.4",
          "form": "ㅠㅠㅠㅠ그래야겠어요",
          "original_form": "ㅠㅠㅠㅠ그래야겠어요",
          "speaker_id": "1",
          "time": "20210824 17:34"
        },
        {
          "id": "MDRW2100011908.1.5",
          "form": "헉..",
          "original_form": "헉..",
          "speaker_id": "1",
          "time": "20210824 17:34"
        },
        {
          "id": "MDRW2100011908.1.6",
          "form": "감전 안다치셨어요??",
          "original_form": "감전 안다치셨어요??",
          "speaker_id": "1",
          "time": "20210824 17:34"
        },
        {
          "id": "MDRW2100011908.1.7",
          "form": "다행이도 크게 다치진 않았습니다! 다만 그이후로 트라우마가 생겨서 그게 문제네요..",
          "original_form": "다행이도 크게 다치진 않았습니다! 다만 그이후로 트라우마가 생겨서 그게 문제네요..",
          "speaker_id": "2",
          "time": "20210824 17:36"
        },
        {
          "id": "MDRW2100011908.1.8",
          "form": "그래서 저는 전등 교체할 일 있음 가족한테 부탁해요 ㅋㅋ",
          "original_form": "그래서 저는 전등 교체할 일 있음 가족한테 부탁해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011908.1.9",
          "form": "ㅠㅠㅠㅠㅠㅠㅠ트라우마면 평생가는거니까요",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠ트라우마면 평생가는거니까요",
          "speaker_id": "1",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011908.1.10",
          "form": "의도치 않은 집안일 토스",
          "original_form": "의도치 않은 집안일 토스",
          "speaker_id": "2",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011908.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011908.1.12",
          "form": "무조건...부탁하세여...",
          "original_form": "무조건...부탁하세여...",
          "speaker_id": "1",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011908.1.13",
          "form": "ㅠㅠㅠㅠ저는 일체형이라",
          "original_form": "ㅠㅠㅠㅠ저는 일체형이라",
          "speaker_id": "1",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011908.1.14",
          "form": "안되겠으면ㅋㅋ집주인 부르려구요",
          "original_form": "안되겠으면ㅋㅋ집주인 부르려구요",
          "speaker_id": "1",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011908.1.15",
          "form": "아 led 전등인가봐요! 전구만 갈면 간단할텐데 일체형은 속에 전선? 을 연결해야 하더라구요.",
          "original_form": "아 led 전등인가봐요! 전구만 갈면 간단할텐데 일체형은 속에 전선? 을 연결해야 하더라구요.",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011908.1.16",
          "form": "오마자여..ㅡㅡ",
          "original_form": "오마자여..ㅡㅡ",
          "speaker_id": "1",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011908.1.17",
          "form": "집주인은 왠지 전등 마스터일거 같은",
          "original_form": "집주인은 왠지 전등 마스터일거 같은",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011908.1.18",
          "form": "저는 LED인것도몰랐어요ㅠㅠ",
          "original_form": "저는 LED인것도몰랐어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011908.1.19",
          "form": "그런 느낌적인 느낌 ㅋㅋ",
          "original_form": "그런 느낌적인 느낌 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011908.1.20",
          "form": "넼ㅋㅋㅋ이것두 첨에주인이",
          "original_form": "넼ㅋㅋㅋ이것두 첨에주인이",
          "speaker_id": "1",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011908.1.21",
          "form": "갈아주긴했는뎈ㅋㅋㅋ",
          "original_form": "갈아주긴했는뎈ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011908.1.22",
          "form": "오오!! 역시!!",
          "original_form": "오오!! 역시!!",
          "speaker_id": "2",
          "time": "20210824 17:40"
        }
      ]
