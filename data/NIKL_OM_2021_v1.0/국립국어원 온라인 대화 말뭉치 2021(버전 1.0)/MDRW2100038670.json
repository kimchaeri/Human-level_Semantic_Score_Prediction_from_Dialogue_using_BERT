      "utterance": [
        {
          "id": "MDRW2100038670.1.1",
          "form": "오늘 저녁은 무엇을 드시나요?",
          "original_form": "오늘 저녁은 무엇을 드시나요?",
          "speaker_id": "1",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100038670.1.2",
          "form": "저는 다이어트중이라서 안먹거나",
          "original_form": "저는 다이어트중이라서 안먹거나",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100038670.1.3",
          "form": "다이어트 쉐이크를 먹으려고요",
          "original_form": "다이어트 쉐이크를 먹으려고요",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100038670.1.4",
          "form": "오모 오늘 축구경기 있어서",
          "original_form": "오모 오늘 축구경기 있어서",
          "speaker_id": "1",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100038670.1.5",
          "form": "저는 치킨을 시켜버렸어요...",
          "original_form": "저는 치킨을 시켜버렸어요...",
          "speaker_id": "1",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100038670.1.6",
          "form": "어떤 맛 드세요 쉐이크",
          "original_form": "어떤 맛 드세요 쉐이크",
          "speaker_id": "1",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100038670.1.7",
          "form": "하ㅜ 제가 너무 새로운 시도를했어요",
          "original_form": "하ㅜ 제가 너무 새로운 시도를했어요",
          "speaker_id": "2",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100038670.1.8",
          "form": "민트초코맛 ㅋㅋ",
          "original_form": "민트초코맛 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100038670.1.9",
          "form": "어머 요즘 그런맛까지 나오나바욬ㅋㅌㅋㅋㅋ",
          "original_form": "어머 요즘 그런맛까지 나오나바욬ㅋㅌㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100038670.1.10",
          "form": "대단한 세계 ㄷㄷ",
          "original_form": "대단한 세계 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100038670.1.11",
          "form": "저는제가 민트초코를 좋아한다 생각했어요",
          "original_form": "저는제가 민트초코를 좋아한다 생각했어요",
          "speaker_id": "2",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100038670.1.12",
          "form": "배스킨 말하시나요? ㅎㅎㅎ",
          "original_form": "배스킨 말하시나요? ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100038670.1.13",
          "form": "저도 그건 먹는데",
          "original_form": "저도 그건 먹는데",
          "speaker_id": "1",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100038670.1.14",
          "form": "저는 민트초코가 아니라 민트초코크림을 좋아하는 거였더라고요 ㅋㅋ",
          "original_form": "저는 민트초코가 아니라 민트초코크림을 좋아하는 거였더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100038670.1.15",
          "form": "크림은 어디 붙어있어요?",
          "original_form": "크림은 어디 붙어있어요?",
          "speaker_id": "1",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100038670.1.16",
          "form": "다이어트쉐이크는 크림맛이나지않아요",
          "original_form": "다이어트쉐이크는 크림맛이나지않아요",
          "speaker_id": "2",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100038670.1.17",
          "form": "그래서 맛이없어요ㅜㅜ",
          "original_form": "그래서 맛이없어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100038670.1.18",
          "form": "닝닝해요ㅜ",
          "original_form": "닝닝해요ㅜ",
          "speaker_id": "2",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100038670.1.19",
          "form": "민초크림은 어디서 먹을수있나요?",
          "original_form": "민초크림은 어디서 먹을수있나요?",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100038670.1.20",
          "form": "ㅋㅋㅋ그러게요 겉에 민트초코크림이아니라",
          "original_form": "ㅋㅋㅋ그러게요 겉에 민트초코크림이아니라",
          "speaker_id": "2",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100038670.1.21",
          "form": "민트초코라 적혀있었을때부터 눈치까야했는데 말이죠 ㅋㅋㅋ",
          "original_form": "민트초코라 적혀있었을때부터 눈치까야했는데 말이죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100038670.1.22",
          "form": "평소 우리가 먹던 아이스크림 맛 카페의 민트초코라떼 이런거랑 아예다른 맛이에요",
          "original_form": "평소 우리가 먹던 아이스크림 맛 카페의 민트초코라떼 이런거랑 아예다른 맛이에요",
          "speaker_id": "2",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100038670.1.23",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100038670.1.24",
          "form": "그냥 화한맛인가바요 ㅋㅋㅋㅋ",
          "original_form": "그냥 화한맛인가바요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100038670.1.25",
          "form": "쉐이크 그럼 어떡하나요 ㅜㅜ",
          "original_form": "쉐이크 그럼 어떡하나요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100038670.1.26",
          "form": "다드실수 있으실지...",
          "original_form": "다드실수 있으실지...",
          "speaker_id": "1",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100038670.1.27",
          "form": "무려 2키로 짜리에요 ㅋㅋ",
          "original_form": "무려 2키로 짜리에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100038670.1.28",
          "form": "앗 그거에요 그거 뒷맛이 화해요 아 이것이 민트의 본맛인가싶은!",
          "original_form": "앗 그거에요 그거 뒷맛이 화해요 아 이것이 민트의 본맛인가싶은!",
          "speaker_id": "2",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100038670.1.29",
          "form": "치약맛 그자체일지 ㅋㅋㅋ",
          "original_form": "치약맛 그자체일지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100038670.1.30",
          "form": "윽 전 절대 다 못먹을듯",
          "original_form": "윽 전 절대 다 못먹을듯",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100038670.1.31",
          "form": "그래도 초코민트라서 초코맛이 나긴나요",
          "original_form": "그래도 초코민트라서 초코맛이 나긴나요",
          "speaker_id": "2",
          "time": "20210902 19:28"
        }
      ]
