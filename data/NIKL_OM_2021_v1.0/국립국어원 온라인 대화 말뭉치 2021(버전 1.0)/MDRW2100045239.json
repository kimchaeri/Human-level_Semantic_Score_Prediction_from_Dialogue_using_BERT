      "utterance": [
        {
          "id": "MDRW2100045239.1.1",
          "form": "반려동물과 함께 사시나요?",
          "original_form": "반려동물과 함께 사시나요?",
          "speaker_id": "1",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045239.1.2",
          "form": "옛날에는요ㅋㅋㅋㅋ 지금은 같이 안살아요 ㅎㅎ",
          "original_form": "옛날에는요ㅋㅋㅋㅋ 지금은 같이 안살아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045239.1.3",
          "form": "반려동물과 같이 생활하시나요?",
          "original_form": "반려동물과 같이 생활하시나요?",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045239.1.4",
          "form": "아니요 저는 키워본적이 없어요!!",
          "original_form": "아니요 저는 키워본적이 없어요!!",
          "speaker_id": "1",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100045239.1.5",
          "form": "혹시 키운다면 강아지파 고양이파 어느쪽이신가요",
          "original_form": "혹시 키운다면 강아지파 고양이파 어느쪽이신가요",
          "speaker_id": "1",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045239.1.6",
          "form": "둘다좋아해요",
          "original_form": "둘다좋아해요",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045239.1.7",
          "form": "강아지와 고양이 어느쪽이서요?",
          "original_form": "강아지와 고양이 어느쪽이서요?",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045239.1.8",
          "form": "저는 털만 아니라면 고양이파요+!",
          "original_form": "저는 털만 아니라면 고양이파요+!",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045239.1.9",
          "form": "고양이 귀엽죠ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "고양이 귀엽죠ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045239.1.10",
          "form": "네 새침해도 예쁘고 앵겨도 귀엽고요",
          "original_form": "네 새침해도 예쁘고 앵겨도 귀엽고요",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045239.1.11",
          "form": "얌전한듯 뻘짓할때너무귀여워요",
          "original_form": "얌전한듯 뻘짓할때너무귀여워요",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045239.1.12",
          "form": "강아지도 귀엽긴 한데요!!",
          "original_form": "강아지도 귀엽긴 한데요!!",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045239.1.13",
          "form": "맞아요 약간 고양이들이 그래서 좋아해욬ㅋㅋㅋ",
          "original_form": "맞아요 약간 고양이들이 그래서 좋아해욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045239.1.14",
          "form": "기본적으로 아기들은 다 귀엽지만요",
          "original_form": "기본적으로 아기들은 다 귀엽지만요",
          "speaker_id": "2",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045239.1.15",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045239.1.16",
          "form": "유튭에 누리라고 귀염고 똑똑한 고양이영상이 있어요",
          "original_form": "유튭에 누리라고 귀염고 똑똑한 고양이영상이 있어요",
          "speaker_id": "2",
