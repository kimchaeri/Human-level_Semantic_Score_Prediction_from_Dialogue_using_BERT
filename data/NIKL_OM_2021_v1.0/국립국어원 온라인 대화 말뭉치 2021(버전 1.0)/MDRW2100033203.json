      "utterance": [
        {
          "id": "MDRW2100033203.1.1",
          "form": "저녁 메뉴는 정했나요?",
          "original_form": "저녁 메뉴는 정했나요?",
          "speaker_id": "1",
          "time": "20210906 18:47"
        },
        {
          "id": "MDRW2100033203.1.2",
          "form": "벌써먹었네요",
          "original_form": "벌써먹었네요",
          "speaker_id": "2",
          "time": "20210906 18:48"
        },
        {
          "id": "MDRW2100033203.1.3",
          "form": "뮈드실예정이신가요?",
          "original_form": "뮈드실예정이신가요?",
          "speaker_id": "2",
          "time": "20210906 18:48"
        },
        {
          "id": "MDRW2100033203.1.4",
          "form": "저도 먹고 왔습니다",
          "original_form": "저도 먹고 왔습니다",
          "speaker_id": "1",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100033203.1.5",
          "form": "갈치구이를 반찬으로 먹었어요",
          "original_form": "갈치구이를 반찬으로 먹었어요",
          "speaker_id": "1",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100033203.1.6",
          "form": "name2님은 무엇을 드셨나요?",
          "original_form": "&name2&님은 무엇을 드셨나요?",
          "speaker_id": "1",
          "time": "20210906 18:50"
        },
        {
          "id": "MDRW2100033203.1.7",
          "form": "김치랑 고등어요.",
          "original_form": "김치랑 고등어요.",
          "speaker_id": "2",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100033203.1.8",
          "form": "name2님도 생선을 드셨군요",
          "original_form": "&name2&님도 생선을 드셨군요",
          "speaker_id": "1",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100033203.1.9",
          "form": "저는 생선요리를 가장 좋아한답니다",
          "original_form": "저는 생선요리를 가장 좋아한답니다",
          "speaker_id": "1",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100033203.1.10",
          "form": "해산물쪽을 좋아하지요",
          "original_form": "해산물쪽을 좋아하지요",
          "speaker_id": "1",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100033203.1.11",
          "form": "저는 고기좋아해요",
          "original_form": "저는 고기좋아해요",
          "speaker_id": "2",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100033203.1.12",
          "form": "육고기를 좋아하는군요",
          "original_form": "육고기를 좋아하는군요",
          "speaker_id": "1",
          "time": "20210906 18:58"
        },
        {
          "id": "MDRW2100033203.1.13",
          "form": "네..좋아하는편이예요",
          "original_form": "네..좋아하는편이예요",
          "speaker_id": "2",
          "time": "20210906 18:59"
        },
        {
          "id": "MDRW2100033203.1.14",
          "form": "아침은 챙겨 드시나요?",
          "original_form": "아침은 챙겨 드시나요?",
          "speaker_id": "1",
          "time": "20210907 05:56"
        },
        {
          "id": "MDRW2100033203.1.15",
          "form": "네..애들이있어서",
          "original_form": "네..애들이있어서",
          "speaker_id": "2",
          "time": "20210907 06:01"
        },
        {
          "id": "MDRW2100033203.1.16",
          "form": "챙겨먹어요",
          "original_form": "챙겨먹어요",
          "speaker_id": "2",
          "time": "20210907 06:03"
        },
        {
          "id": "MDRW2100033203.1.17",
          "form": "저희집도 아침을 꼬박꼬박 먹습니다",
          "original_form": "저희집도 아침을 꼬박꼬박 먹습니다",
          "speaker_id": "1",
          "time": "20210907 06:03"
        },
        {
          "id": "MDRW2100033203.1.18",
          "form": "name2 님이 아이들 아침 챙겨주시는 건가요?",
          "original_form": "&name2& 님이 아이들 아침 챙겨주시는 건가요?",
          "speaker_id": "1",
          "time": "20210907 06:03"
        },
        {
          "id": "MDRW2100033203.1.19",
          "form": "저희두요",
          "original_form": "저희두요",
          "speaker_id": "2",
          "time": "20210907 06:04"
        },
        {
          "id": "MDRW2100033203.1.20",
          "form": "꼭먹는편이예요",
          "original_form": "꼭먹는편이예요",
          "speaker_id": "2",
          "time": "20210907 06:04"
        },
        {
          "id": "MDRW2100033203.1.21",
          "form": "우리 식구들이 아침에 일찍 일어나서 아침 먹기 수월한 편입니다",
          "original_form": "우리 식구들이 아침에 일찍 일어나서 아침 먹기 수월한 편입니다",
