      "utterance": [
        {
          "id": "MDRW2100040123.1.1",
          "form": "저는 올해 서핑 시작한 게 제일ㅋㅋㅋ",
          "original_form": "저는 올해 서핑 시작한 게 제일ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:54"
        },
        {
          "id": "MDRW2100040123.1.2",
          "form": "저도 수영 못하지만 서핑 하는걸요!",
          "original_form": "저도 수영 못하지만 서핑 하는걸요!",
          "speaker_id": "2",
          "time": "20210904 10:54"
        },
        {
          "id": "MDRW2100040123.1.3",
          "form": "아 서핑 시작하셨어요?",
          "original_form": "아 서핑 시작하셨어요?",
          "speaker_id": "1",
          "time": "20210904 10:55"
        },
        {
          "id": "MDRW2100040123.1.4",
          "form": "여행지에서 해보신건가요?",
          "original_form": "여행지에서 해보신건가요?",
          "speaker_id": "1",
          "time": "20210904 10:55"
        },
        {
          "id": "MDRW2100040123.1.5",
          "form": "네네! 강원도에서요 ㅎㅎ",
          "original_form": "네네! 강원도에서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 10:56"
        },
        {
          "id": "MDRW2100040123.1.6",
          "form": "와 강습에 시간 얼마정도 걸리나요?",
          "original_form": "와 강습에 시간 얼마정도 걸리나요?",
          "speaker_id": "1",
          "time": "20210904 10:57"
        },
        {
          "id": "MDRW2100040123.1.7",
          "form": "파도 좋은 날 골라서 가면",
          "original_form": "파도 좋은 날 골라서 가면",
          "speaker_id": "2",
          "time": "20210904 10:57"
        },
        {
          "id": "MDRW2100040123.1.8",
          "form": "강습 한시간 정도 받고 강사분 도움 받으면 일어서서 탈 수 있어요!",
          "original_form": "강습 한시간 정도 받고 강사분 도움 받으면 일어서서 탈 수 있어요!",
          "speaker_id": "2",
          "time": "20210904 10:58"
        },
        {
          "id": "MDRW2100040123.1.9",
          "form": "오 생각보다 얼마 안걸리네요",
          "original_form": "오 생각보다 얼마 안걸리네요",
          "speaker_id": "1",
          "time": "20210904 10:58"
        },
        {
          "id": "MDRW2100040123.1.10",
          "form": "무섭진 않은가요??",
          "original_form": "무섭진 않은가요??",
          "speaker_id": "1",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100040123.1.11",
          "form": "저는 수영 못하는데도 별로 안 무서웠는데",
          "original_form": "저는 수영 못하는데도 별로 안 무서웠는데",
          "speaker_id": "2",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100040123.1.12",
          "form": "친구 하나는 좀 무서워하더라고요",
          "original_form": "친구 하나는 좀 무서워하더라고요",
          "speaker_id": "2",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100040123.1.13",
          "form": "재밌는 경험인거같아요",
          "original_form": "재밌는 경험인거같아요",
          "speaker_id": "1",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100040123.1.14",
          "form": "저도 기회가 되면 해보고 싶네요",
          "original_form": "저도 기회가 되면 해보고 싶네요",
          "speaker_id": "1",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100040123.1.15",
          "form": "맞아요 재미 들리지 않게 조심하세요...ㅎㅎㅎㅎ",
          "original_form": "맞아요 재미 들리지 않게 조심하세요...ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 11:51"
        },
        {
          "id": "MDRW2100040123.1.16",
          "form": "ㅋㅋㅋㅋㅋ 혹시 한번 강습받고 타는데 비용은 얼마정도 되는지 알려주실 수 있나요?",
          "original_form": "ㅋㅋㅋㅋㅋ 혹시 한번 강습받고 타는데 비용은 얼마정도 되는지 알려주실 수 있나요?",
          "speaker_id": "1",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100040123.1.17",
          "form": "강습 한번 받고 장비 빌리고 그 날 하루 종일 자유롭게 장비 사용해서 타는 것까지 해서 5만원 정도 하더라고요!",
          "original_form": "강습 한번 받고 장비 빌리고 그 날 하루 종일 자유롭게 장비 사용해서 타는 것까지 해서 5만원 정도 하더라고요!",
          "speaker_id": "2",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100040123.1.18",
          "form": "와 생각보다 저렴하네요",
          "original_form": "와 생각보다 저렴하네요",
          "speaker_id": "1",
          "time": "20210904 11:55"
        },
        {
          "id": "MDRW2100040123.1.19",
          "form": "그쵸!",
          "original_form": "그쵸!",
          "speaker_id": "2",
          "time": "20210904 11:55"
        }
      ]
