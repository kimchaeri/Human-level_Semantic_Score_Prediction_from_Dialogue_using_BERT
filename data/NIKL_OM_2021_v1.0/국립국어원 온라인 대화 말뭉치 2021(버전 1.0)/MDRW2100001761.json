      "utterance": [
        {
          "id": "MDRW2100001761.1.1",
          "form": "둘 다  남녀공학이였어. 너는?",
          "original_form": "둘 다  남녀공학이였어. 너는?",
          "speaker_id": "2",
          "time": "20210819 18:39"
        },
        {
          "id": "MDRW2100001761.1.2",
          "form": "나는 남중남고 나와서 진짜 재미가 없었어",
          "original_form": "나는 남중남고 나와서 진짜 재미가 없었어",
          "speaker_id": "1",
          "time": "20210819 18:39"
        },
        {
          "id": "MDRW2100001761.1.3",
          "form": "그랬구나 그래도 옷갈아 입을때 불편하고 그런건 없었겠네ㅋㅋ",
          "original_form": "그랬구나 그래도 옷갈아 입을때 불편하고 그런건 없었겠네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:39"
        },
        {
          "id": "MDRW2100001761.1.4",
          "form": "맨날 늦게 까지 컴퓨터 게임하고 학교에선 졸고 그랬었지 ㅋㅋㅋ",
          "original_form": "맨날 늦게 까지 컴퓨터 게임하고 학교에선 졸고 그랬었지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:39"
        },
        {
          "id": "MDRW2100001761.1.5",
          "form": "체육복 화장실에서 갈아입는거 진짜 귀찮았음!!",
          "original_form": "체육복 화장실에서 갈아입는거 진짜 귀찮았음!!",
          "speaker_id": "2",
          "time": "20210819 18:40"
        },
        {
          "id": "MDRW2100001761.1.6",
          "form": "남자들만 있으니까 불편한건 전혀 없었지",
          "original_form": "남자들만 있으니까 불편한건 전혀 없었지",
          "speaker_id": "1",
          "time": "20210819 18:40"
        },
        {
          "id": "MDRW2100001761.1.7",
          "form": "그래도 남녀공학이 재미 있지 않아?",
          "original_form": "그래도 남녀공학이 재미 있지 않아?",
          "speaker_id": "1",
          "time": "20210819 18:40"
        },
        {
          "id": "MDRW2100001761.1.8",
          "form": "ㅋ 나는 예체능이여서 공부는 일찍이 패스!",
          "original_form": "ㅋ 나는 예체능이여서 공부는 일찍이 패스!",
          "speaker_id": "2",
          "time": "20210819 18:40"
        },
        {
          "id": "MDRW2100001761.1.9",
          "form": "재미있긴하지ㅋㅋ",
          "original_form": "재미있긴하지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:40"
        },
        {
          "id": "MDRW2100001761.1.10",
          "form": "나는 그냥 인문계 였어서 항상 공부에 대한 압박이 있었는데 ㅜㅜ",
          "original_form": "나는 그냥 인문계 였어서 항상 공부에 대한 압박이 있었는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 18:41"
        },
        {
          "id": "MDRW2100001761.1.11",
          "form": "글쿤 나는 맨날 레슨핑계로 땡땡이치고",
          "original_form": "글쿤 나는 맨날 레슨핑계로 땡땡이치고",
          "speaker_id": "2",
          "time": "20210819 18:41"
        },
        {
          "id": "MDRW2100001761.1.12",
          "form": "밥먹고 연습실에서 낮잠자고 그랬음ㅋ",
          "original_form": "밥먹고 연습실에서 낮잠자고 그랬음ㅋ",
          "speaker_id": "2",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100001761.1.13",
          "form": "연습실에서 방해 없이 낮잠을 잘 수 있었다니 부럽구만 ㅋㅋㅋ",
          "original_form": "연습실에서 방해 없이 낮잠을 잘 수 있었다니 부럽구만 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:43"
        },
        {
          "id": "MDRW2100001761.1.14",
          "form": "그러다 걸리면 엄청 혼나고 그랬었음ㅋㅋ",
          "original_form": "그러다 걸리면 엄청 혼나고 그랬었음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:43"
        },
        {
          "id": "MDRW2100001761.1.15",
          "form": "나도 수업시간에 졸다가 참 많이 혼났지 ㅋㅋㅋㅋㅋ",
          "original_form": "나도 수업시간에 졸다가 참 많이 혼났지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:44"
        },
        {
          "id": "MDRW2100001761.1.16",
          "form": "요즘은 다시 학생으로 돌아가고싶음ㅋㅋ그때가 제일 재미있었던거같아ㅋㅋ",
          "original_form": "요즘은 다시 학생으로 돌아가고싶음ㅋㅋ그때가 제일 재미있었던거같아ㅋㅋ",
          "speaker_id": "2",
