      "utterance": [
        {
          "id": "MDRW2100004758.1.1",
          "form": "name2님은 볶음밥을 좋아하시는건가여?",
          "original_form": "&name2&님은 볶음밥을 좋아하시는건가여?",
          "speaker_id": "1",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004758.1.2",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "2",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004758.1.3",
          "form": "김치볶음밥도 좋구",
          "original_form": "김치볶음밥도 좋구",
          "speaker_id": "2",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004758.1.4",
          "form": "그래서 닉네임이 name2이시구나..!!",
          "original_form": "그래서 닉네임이 &name2&이시구나..!!",
          "speaker_id": "1",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004758.1.5",
          "form": "허닭 볶음밥 드셔보셨어요??",
          "original_form": "허닭 볶음밥 드셔보셨어요??",
          "speaker_id": "1",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004758.1.6",
          "form": "오 네네 먹어봤어요!",
          "original_form": "오 네네 먹어봤어요!",
          "speaker_id": "2",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004758.1.7",
          "form": "다이어트 한다고 먹었는데 속세의 맛이었습니다",
          "original_form": "다이어트 한다고 먹었는데 속세의 맛이었습니다",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004758.1.8",
          "form": "앜ㅋㅋㅋㅋㅋ완전 공감..",
          "original_form": "앜ㅋㅋㅋㅋㅋ완전 공감..",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004758.1.9",
          "form": "저도 다이어트 한다고 사뒀는데, 왜그랬는지는 모르겠지만 불닭소스 더해서 맛있게 먹었어요",
          "original_form": "저도 다이어트 한다고 사뒀는데, 왜그랬는지는 모르겠지만 불닭소스 더해서 맛있게 먹었어요",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004758.1.10",
          "form": "거기에 치즈까지 넣으면 행복했어요",
          "original_form": "거기에 치즈까지 넣으면 행복했어요",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004758.1.11",
          "form": "깍두기볶음밥 진짜 맛있어요",
          "original_form": "깍두기볶음밥 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004758.1.12",
          "form": "그냥.. 그냥 볶음밥",
          "original_form": "그냥.. 그냥 볶음밥",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004758.1.13",
          "form": "헐 치즈 생각도 못했어요",
          "original_form": "헐 치즈 생각도 못했어요",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004758.1.14",
          "form": "우와 깍두기는 안 먹어봤어요!",
          "original_form": "우와 깍두기는 안 먹어봤어요!",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004758.1.15",
          "form": "그거 양도 꽤 많은 거 같아요. 먹고나면 든든해요",
          "original_form": "그거 양도 꽤 많은 거 같아요. 먹고나면 든든해요",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004758.1.16",
          "form": "깍두기에 불닭소스 좀 더해서 먹으면 완전 속세의 맛이에요...",
          "original_form": "깍두기에 불닭소스 좀 더해서 먹으면 완전 속세의 맛이에요...",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004758.1.17",
          "form": "헉 너무 맛있겠다",
          "original_form": "헉 너무 맛있겠다",
          "speaker_id": "2",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004758.1.18",
          "form": "저 다음에 깍두기볶음밥 사먹어볼래요",
          "original_form": "저 다음에 깍두기볶음밥 사먹어볼래요",
          "speaker_id": "2",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004758.1.19",
          "form": "깍두기볶음밥 추천하는 사람 많더라구요.",
          "original_form": "깍두기볶음밥 추천하는 사람 많더라구요.",
          "speaker_id": "1",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004758.1.20",
          "form": "베스트셀러인가봐요..!!",
          "original_form": "베스트셀러인가봐요..!!",
          "speaker_id": "1",
