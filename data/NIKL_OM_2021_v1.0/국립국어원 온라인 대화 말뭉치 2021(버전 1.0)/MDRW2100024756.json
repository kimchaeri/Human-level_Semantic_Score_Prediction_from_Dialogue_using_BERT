      "utterance": [
        {
          "id": "MDRW2100024756.1.1",
          "form": "언니 공차 어때요?",
          "original_form": "언니 공차 어때요?",
          "speaker_id": "1",
          "time": "20210831 18:09"
        },
        {
          "id": "MDRW2100024756.1.2",
          "form": "꺄~~~",
          "original_form": "꺄~~~",
          "speaker_id": "2",
          "time": "20210831 18:09"
        },
        {
          "id": "MDRW2100024756.1.3",
          "form": "공차 너무 좋죠!!!",
          "original_form": "공차 너무 좋죠!!!",
          "speaker_id": "2",
          "time": "20210831 18:10"
        },
        {
          "id": "MDRW2100024756.1.4",
          "form": "제 소울푸드 !!!!",
          "original_form": "제 소울푸드 !!!!",
          "speaker_id": "2",
          "time": "20210831 18:10"
        },
        {
          "id": "MDRW2100024756.1.5",
          "form": "방금 먹고왔어요ㅋㅋ",
          "original_form": "방금 먹고왔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024756.1.6",
          "form": "무슨 맛 드시고 오셨나요????",
          "original_form": "무슨 맛 드시고 오셨나요????",
          "speaker_id": "2",
          "time": "20210831 18:20"
        },
        {
          "id": "MDRW2100024756.1.7",
          "form": "저는 제주그린티스무디를",
          "original_form": "저는 제주그린티스무디를",
          "speaker_id": "2",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100024756.1.8",
          "form": "아주 좋아해요.....",
          "original_form": "아주 좋아해요.....",
          "speaker_id": "2",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100024756.1.9",
          "form": "저는 히비스커스패션후르츠였나 그거먹었어요",
          "original_form": "저는 히비스커스패션후르츠였나 그거먹었어요",
          "speaker_id": "1",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100024756.1.10",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100024756.1.11",
          "form": "오 그런 맛도 공차에 있었군요!!!!",
          "original_form": "오 그런 맛도 공차에 있었군요!!!!",
          "speaker_id": "2",
          "time": "20210831 18:27"
        },
        {
          "id": "MDRW2100024756.1.12",
          "form": "펄은 어떤걸로 드셨나요",
          "original_form": "펄은 어떤걸로 드셨나요",
          "speaker_id": "2",
          "time": "20210831 18:27"
        },
        {
          "id": "MDRW2100024756.1.13",
          "form": "생일이라고 친구가 사준건가요",
          "original_form": "생일이라고 친구가 사준건가요",
          "speaker_id": "2",
          "time": "20210831 18:27"
        },
        {
          "id": "MDRW2100024756.1.14",
          "form": "펄없었구 직접 사먹었어요ㅋㅋㅋㅋ",
          "original_form": "펄없었구 직접 사먹었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100024756.1.15",
          "form": "앗 그랬군요",
          "original_form": "앗 그랬군요",
          "speaker_id": "2",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024756.1.16",
          "form": "다 틀렸네요 머쓱",
          "original_form": "다 틀렸네요 머쓱",
          "speaker_id": "2",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024756.1.17",
          "form": "저는 공차에서 타피오카 펄도 좋아하지만",
          "original_form": "저는 공차에서 타피오카 펄도 좋아하지만",
          "speaker_id": "2",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024756.1.18",
          "form": "화이트펄도 좋아해요!!!",
          "original_form": "화이트펄도 좋아해요!!!",
          "speaker_id": "2",
          "time": "20210831 18:54"
        },
        {
          "id": "MDRW2100024756.1.19",
          "form": "화이트펄은 안먹어봤어요!!",
          "original_form": "화이트펄은 안먹어봤어요!!",
          "speaker_id": "1",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100024756.1.20",
          "form": "타피오카펄보다",
          "original_form": "타피오카펄보다",
          "speaker_id": "2",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100024756.1.21",
          "form": "건강에 좋을 것 같더라구요",
          "original_form": "건강에 좋을 것 같더라구요",
          "speaker_id": "2",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100024756.1.22",
          "form": "해초로 만들어진거라 하던데~~~",
          "original_form": "해초로 만들어진거라 하던데~~~",
          "speaker_id": "2",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100024756.1.23",
          "form": "헐 신기하다",
          "original_form": "헐 신기하다",
          "speaker_id": "1",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100024756.1.24",
          "form": "담에 먹어봐야겠어요",
          "original_form": "담에 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210831 18:56"
        },
        {
          "id": "MDRW2100024756.1.25",
          "form": "배고파요",
          "original_form": "배고파요",
          "speaker_id": "1",
          "time": "20210831 18:56"
        },
        {
          "id": "MDRW2100024756.1.26",
          "form": "그러시겠어요ㅠ",
          "original_form": "그러시겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210831 18:56"
        },
        {
          "id": "MDRW2100024756.1.27",
          "form": "오늘 저녁엔 친구랑 뭐 드세요~?",
          "original_form": "오늘 저녁엔 친구랑 뭐 드세요~?",
          "speaker_id": "2",
          "time": "20210831 18:56"
        },
        {
          "id": "MDRW2100024756.1.28",
          "form": "아직 안정해서 정해야해요!",
          "original_form": "아직 안정해서 정해야해요!",
