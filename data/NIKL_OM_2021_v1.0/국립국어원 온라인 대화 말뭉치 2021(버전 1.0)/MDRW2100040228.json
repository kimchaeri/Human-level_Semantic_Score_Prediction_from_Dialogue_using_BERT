      "utterance": [
        {
          "id": "MDRW2100040228.1.1",
          "form": "옷 쇼핑 조아하세요?",
          "original_form": "옷 쇼핑 조아하세요?",
          "speaker_id": "1",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100040228.1.2",
          "form": "계절 바뀔때마다 옷쇼핑 했는데",
          "original_form": "계절 바뀔때마다 옷쇼핑 했는데",
          "speaker_id": "2",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100040228.1.3",
          "form": "요즘은 옷장이 포화상태라 자제중이예요",
          "original_form": "요즘은 옷장이 포화상태라 자제중이예요",
          "speaker_id": "2",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100040228.1.4",
          "form": "ㅋㅋ 저랑 같네요",
          "original_form": "ㅋㅋ 저랑 같네요",
          "speaker_id": "1",
          "time": "20210904 14:24"
        },
        {
          "id": "MDRW2100040228.1.5",
          "form": "ㅎㅎ근데 옷장은 가득 찼는데 입을 옷은 딱히 없고..",
          "original_form": "ㅎㅎ근데 옷장은 가득 찼는데 입을 옷은 딱히 없고..",
          "speaker_id": "2",
          "time": "20210904 14:24"
        },
        {
          "id": "MDRW2100040228.1.6",
          "form": "그렇지 않나요?",
          "original_form": "그렇지 않나요?",
          "speaker_id": "2",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100040228.1.7",
          "form": "그죠... 미스테리",
          "original_form": "그죠... 미스테리",
          "speaker_id": "1",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100040228.1.8",
          "form": "파신적은요?",
          "original_form": "파신적은요?",
          "speaker_id": "1",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100040228.1.9",
          "form": "중ㄱㅎ",
          "original_form": "중ㄱㅎ",
          "speaker_id": "1",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100040228.1.10",
          "form": "중고",
          "original_form": "중고",
          "speaker_id": "1",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100040228.1.11",
          "form": "아 중고로 팔진 않고",
          "original_form": "아 중고로 팔진 않고",
          "speaker_id": "2",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100040228.1.12",
          "form": "아름다운가게에 기부는 해봤어요",
          "original_form": "아름다운가게에 기부는 해봤어요",
          "speaker_id": "2",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100040228.1.13",
          "form": "오호 기부하면 조은게잇나용",
          "original_form": "오호 기부하면 조은게잇나용",
          "speaker_id": "1",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100040228.1.14",
          "form": "그냥 깨끗한 옷 필요한 분이 입으시면 좋겠다 싶어서",
          "original_form": "그냥 깨끗한 옷 필요한 분이 입으시면 좋겠다 싶어서",
          "speaker_id": "2",
          "time": "20210904 14:29"
        },
        {
          "id": "MDRW2100040228.1.15",
          "form": "그리고 연말에 기부금영수증 처리 해주기도 하고요",
          "original_form": "그리고 연말에 기부금영수증 처리 해주기도 하고요",
          "speaker_id": "2",
          "time": "20210904 14:29"
        },
        {
          "id": "MDRW2100040228.1.16",
          "form": "아하 좋네요 버리는 것 보다",
          "original_form": "아하 좋네요 버리는 것 보다",
          "speaker_id": "1",
          "time": "20210904 14:33"
        },
        {
          "id": "MDRW2100040228.1.17",
          "form": "네 낡은 옷은 버리기도 하지만요ㅎㅎ",
          "original_form": "네 낡은 옷은 버리기도 하지만요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:34"
        },
        {
          "id": "MDRW2100040228.1.18",
          "form": "최근엔 옷 안사셧나요",
          "original_form": "최근엔 옷 안사셧나요",
          "speaker_id": "1",
          "time": "20210904 14:36"
        },
        {
          "id": "MDRW2100040228.1.19",
          "form": "이번 여름엔 안 사고 지나갔어요",
          "original_form": "이번 여름엔 안 사고 지나갔어요",
          "speaker_id": "2",
          "time": "20210904 14:37"
        },
        {
          "id": "MDRW2100040228.1.20",
          "form": "ㅋㅋㅋㅋ 자제하셧나요",
          "original_form": "ㅋㅋㅋㅋ 자제하셧나요",
          "speaker_id": "1",
          "time": "20210904 14:37"
        },
        {
          "id": "MDRW2100040228.1.21",
          "form": "네ㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:37"
        },
        {
          "id": "MDRW2100040228.1.22",
          "form": "여름에 휴가를 안가서",
          "original_form": "여름에 휴가를 안가서",
          "speaker_id": "2",
          "time": "20210904 14:37"
        },
        {
          "id": "MDRW2100040228.1.23",
          "form": "ㅋㅋ 가을옷은요",
          "original_form": "ㅋㅋ 가을옷은요",
          "speaker_id": "1",
          "time": "20210904 14:38"
        },
        {
          "id": "MDRW2100040228.1.24",
          "form": "가을옷은 아직은 안사고 있는데",
          "original_form": "가을옷은 아직은 안사고 있는데",
          "speaker_id": "2",
          "time": "20210904 14:39"
        },
        {
          "id": "MDRW2100040228.1.25",
          "form": "가을이 워낙 짧아서 요즘ㅠ",
          "original_form": "가을이 워낙 짧아서 요즘ㅠ",
          "speaker_id": "2",
          "time": "20210904 14:39"
        },
        {
          "id": "MDRW2100040228.1.26",
          "form": "ㅜㅜ그죠 사도 못입을때도잇어요",
          "original_form": "ㅜㅜ그죠 사도 못입을때도잇어요",
          "speaker_id": "1",
          "time": "20210904 14:40"
        }
      ]
    }
  ]
