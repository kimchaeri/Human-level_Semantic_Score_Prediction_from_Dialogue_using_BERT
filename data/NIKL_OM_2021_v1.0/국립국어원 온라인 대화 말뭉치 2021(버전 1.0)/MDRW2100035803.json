      "utterance": [
        {
          "id": "MDRW2100035803.1.1",
          "form": "안녕하세요 주말에 맛있는거 많이 드셨어요~?",
          "original_form": "안녕하세요 주말에 맛있는거 많이 드셨어요~?",
          "speaker_id": "1",
          "time": "20210830 03:02"
        },
        {
          "id": "MDRW2100035803.1.2",
          "form": "ㅋㅋ 금요일날 치킨먹고 어제는 계란말이 먹엇어요 ㅎ\r\nname1님은요?",
          "original_form": "ㅋㅋ 금요일날 치킨먹고 어제는 계란말이 먹엇어요 ㅎ\r\n&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 03:03"
        },
        {
          "id": "MDRW2100035803.1.3",
          "form": "앗 저도 치킨 먹었어요!",
          "original_form": "앗 저도 치킨 먹었어요!",
          "speaker_id": "1",
          "time": "20210830 03:03"
        },
        {
          "id": "MDRW2100035803.1.4",
          "form": "저는 뿌링클 먹었는데",
          "original_form": "저는 뿌링클 먹었는데",
          "speaker_id": "1",
          "time": "20210830 03:03"
        },
        {
          "id": "MDRW2100035803.1.5",
          "form": "어디꺼 드셨어용?",
          "original_form": "어디꺼 드셨어용?",
          "speaker_id": "1",
          "time": "20210830 03:03"
        },
        {
          "id": "MDRW2100035803.1.6",
          "form": "오 저는 치킨더홈이요 ㅎㅎㅎ",
          "original_form": "오 저는 치킨더홈이요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 03:05"
        },
        {
          "id": "MDRW2100035803.1.7",
          "form": "뿌링클 좋아하시나봐요",
          "original_form": "뿌링클 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210830 03:05"
        },
        {
          "id": "MDRW2100035803.1.8",
          "form": "거기 약간 건강한 치킨 이미지이던데 안짜고 맛있나용?",
          "original_form": "거기 약간 건강한 치킨 이미지이던데 안짜고 맛있나용?",
          "speaker_id": "1",
          "time": "20210830 03:05"
        },
        {
          "id": "MDRW2100035803.1.9",
          "form": "뿌링클 주기적으로 먹어요ㅎㅎ",
          "original_form": "뿌링클 주기적으로 먹어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 03:06"
        },
        {
          "id": "MDRW2100035803.1.10",
          "form": "매장마다 조금 차이가잇긴해요 ㅎㅎ 가끔 너무 바쁘면 염지가 좀 부족하기도하고 그렇더라고료.",
          "original_form": "매장마다 조금 차이가잇긴해요 ㅎㅎ 가끔 너무 바쁘면 염지가 좀 부족하기도하고 그렇더라고료.",
          "speaker_id": "2",
          "time": "20210830 03:06"
        },
        {
          "id": "MDRW2100035803.1.11",
          "form": "아항ㅠㅠ",
          "original_form": "아항ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 03:06"
        },
        {
          "id": "MDRW2100035803.1.12",
          "form": "뿌링클이 치킨중에 최고로 좋으세오??",
          "original_form": "뿌링클이 치킨중에 최고로 좋으세오??",
          "speaker_id": "2",
          "time": "20210830 03:06"
        },
        {
          "id": "MDRW2100035803.1.13",
          "form": "아ㅋㅋㅋ 제가 제일 좋아하는 치킨은",
          "original_form": "아ㅋㅋㅋ 제가 제일 좋아하는 치킨은",
          "speaker_id": "1",
          "time": "20210830 03:06"
        },
        {
          "id": "MDRW2100035803.1.14",
          "form": "따로 있어용!",
          "original_form": "따로 있어용!",
          "speaker_id": "1",
          "time": "20210830 03:06"
        },
        {
          "id": "MDRW2100035803.1.15",
          "form": "치킨매니아라는 브랜드의",
          "original_form": "치킨매니아라는 브랜드의",
          "speaker_id": "1",
          "time": "20210830 03:07"
        },
        {
          "id": "MDRW2100035803.1.16",
          "form": "새우치킨인데 혹시 드셔보셨어용?",
          "original_form": "새우치킨인데 혹시 드셔보셨어용?",
          "speaker_id": "1",
          "time": "20210830 03:07"
        },
        {
          "id": "MDRW2100035803.1.17",
          "form": "오 저도 먹어보긴햇엇어요 ㅎㅎㅎ",
          "original_form": "오 저도 먹어보긴햇엇어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 03:07"
        },
        {
          "id": "MDRW2100035803.1.18",
          "form": "거기 메뉴 좋아하시는군요 ㅋㅋㅋ",
          "original_form": "거기 메뉴 좋아하시는군요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 03:07"
        },
        {
          "id": "MDRW2100035803.1.19",
          "form": "약간 깐풍기스러운 느낌인데 넘 맛있더라구요ㅎㅎ",
          "original_form": "약간 깐풍기스러운 느낌인데 넘 맛있더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 03:07"
        },
        {
          "id": "MDRW2100035803.1.20",
          "form": "배달음식 자주 드세요?",
          "original_form": "배달음식 자주 드세요?",
          "speaker_id": "1",
          "time": "20210830 03:08"
        },
        {
          "id": "MDRW2100035803.1.21",
          "form": "저희집은 한달에 4번정도 시켜요 ㅎㅎ\r\nname1님은요?",
          "original_form": "저희집은 한달에 4번정도 시켜요 ㅎㅎ\r\n&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 03:08"
        },
        {
          "id": "MDRW2100035803.1.22",
          "form": "거의 주에 한번이군용",
          "original_form": "거의 주에 한번이군용",
          "speaker_id": "1",
          "time": "20210830 03:08"
        },
        {
          "id": "MDRW2100035803.1.23",
          "form": "저는 배달음식말고 밀키트 자주 먹어용!ㅎㅎ",
          "original_form": "저는 배달음식말고 밀키트 자주 먹어용!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 03:09"
        },
        {
          "id": "MDRW2100035803.1.24",
