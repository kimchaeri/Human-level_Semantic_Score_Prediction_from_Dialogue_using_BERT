        },
        {
          "id": "MDRW2100042423.1.2",
          "form": "앉아있을때 끼면 좋을거같아서",
          "original_form": "앉아있을때 끼면 좋을거같아서",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042423.1.3",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042423.1.4",
          "form": "사볼까 고민중",
          "original_form": "사볼까 고민중",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042423.1.5",
          "form": "아 근데 난 사이즈땜에",
          "original_form": "아 근데 난 사이즈땜에",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042423.1.6",
          "form": "못샀어",
          "original_form": "못샀어",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042423.1.7",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042423.1.8",
          "form": "그것도 사이즈 있다길래",
          "original_form": "그것도 사이즈 있다길래",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042423.1.9",
          "form": "나도 종아리 두꺼운데",
          "original_form": "나도 종아리 두꺼운데",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042423.1.10",
          "form": "뭐 사야할지 몰라서",
          "original_form": "뭐 사야할지 몰라서",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042423.1.11",
          "form": "근데 집에 있었네",
          "original_form": "근데 집에 있었네",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042423.1.12",
          "form": "아 사이즈있구나",
          "original_form": "아 사이즈있구나",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042423.1.13",
          "form": "오ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042423.1.14",
          "form": "아니 어쩐지 거등학교때 애들이 하고다니는거",
          "original_form": "아니 어쩐지 거등학교때 애들이 하고다니는거",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042423.1.15",
          "form": "난 잘 안들어가더라ㅋㅋㅋㅋㅋ",
          "original_form": "난 잘 안들어가더라ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042423.1.16",
          "form": "우리집에 있는거 먼저 써볼래?",
          "original_form": "우리집에 있는거 먼저 써볼래?",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042423.1.17",
          "form": "우리 언제 한번 만나서 같이 공부하자",
          "original_form": "우리 언제 한번 만나서 같이 공부하자",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042423.1.18",
          "form": "그리고 단백질 그거 만들자",
          "original_form": "그리고 단백질 그거 만들자",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042423.1.19",
          "form": "오오오오오오",
