      "utterance": [
        {
          "id": "MDRW2100025796.1.1",
          "form": "감자빵 어땠엌ㅋㅋ",
          "original_form": "감자빵 어땠엌ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100025796.1.2",
          "form": "아주 귀엽지?",
          "original_form": "아주 귀엽지?",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025796.1.3",
          "form": "먹을만 하더라",
          "original_form": "먹을만 하더라",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025796.1.4",
          "form": "근데 감자는 이제 뭐하지마",
          "original_form": "근데 감자는 이제 뭐하지마",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025796.1.5",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025796.1.6",
          "form": "먹을만하다면서",
          "original_form": "먹을만하다면서",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025796.1.7",
          "form": "그거  먹는 건 잠깐이고 만드는덴 오래 걸리잖아",
          "original_form": "그거  먹는 건 잠깐이고 만드는덴 오래 걸리잖아",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025796.1.8",
          "form": "별로 안 힘든데?",
          "original_form": "별로 안 힘든데?",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025796.1.9",
          "form": "너 오늘 일 겁나 많이 했다",
          "original_form": "너 오늘 일 겁나 많이 했다",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025796.1.10",
          "form": "그런소리 할거면 좀 도와주던가",
          "original_form": "그런소리 할거면 좀 도와주던가",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100025796.1.11",
          "form": "아 도와줄거 있음 말하라니까 자기가 안해놓고",
          "original_form": "아 도와줄거 있음 말하라니까 자기가 안해놓고",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100025796.1.12",
          "form": "그렇게 손 많이 가는 건 없었어",
          "original_form": "그렇게 손 많이 가는 건 없었어",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100025796.1.13",
          "form": "설거지가 많더라",
          "original_form": "설거지가 많더라",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100025796.1.14",
          "form": "그러니까 설거지 거리 많이 나와서 고생했다",
          "original_form": "그러니까 설거지 거리 많이 나와서 고생했다",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025796.1.15",
          "form": "엄마가 땅콩먹고 그대로 내놔서 그래",
          "original_form": "엄마가 땅콩먹고 그대로 내놔서 그래",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025796.1.16",
          "form": "ㅋㅋㅋ배불러 죽겠다",
          "original_form": "ㅋㅋㅋ배불러 죽겠다",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025796.1.17",
          "form": "저녁 어떻게 먹을래",
          "original_form": "저녁 어떻게 먹을래",
          "speaker_id": "2",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025796.1.18",
