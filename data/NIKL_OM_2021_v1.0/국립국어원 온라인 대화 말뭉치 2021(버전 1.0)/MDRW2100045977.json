      "utterance": [
        {
          "id": "MDRW2100045977.1.1",
          "form": "식사하셧나여",
          "original_form": "식사하셧나여",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045977.1.2",
          "form": "저는 출근하자마자 아메리카노 때려넣엇어여",
          "original_form": "저는 출근하자마자 아메리카노 때려넣엇어여",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045977.1.3",
          "form": "아직이요 name1님은 식사하셨나요?",
          "original_form": "아직이요 &name1&님은 식사하셨나요?",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045977.1.4",
          "form": "저는 아침안먹고",
          "original_form": "저는 아침안먹고",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045977.1.5",
          "form": "바로 출근해여ㅋㅋ",
          "original_form": "바로 출근해여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045977.1.6",
          "form": "밥보다 잠이라서",
          "original_form": "밥보다 잠이라서",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045977.1.7",
          "form": "아아 저도 그런기는 한데 빈속에 아메리카노 괜찮으신가요?ㅠㅠ",
          "original_form": "아아 저도 그런기는 한데 빈속에 아메리카노 괜찮으신가요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045977.1.8",
          "form": "얼마전에 위내시경 받앗는데",
          "original_form": "얼마전에 위내시경 받앗는데",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045977.1.9",
          "form": "말짱하데요 ㅎㅎ",
          "original_form": "말짱하데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045977.1.10",
          "form": "이러다 훅갈꺼같아여ㅋㅋㅋ",
          "original_form": "이러다 훅갈꺼같아여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045977.1.11",
          "form": "미리미리 조심하세요ㅠㅠ",
          "original_form": "미리미리 조심하세요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045977.1.12",
          "form": "커피조아하세요?",
          "original_form": "커피조아하세요?",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045977.1.13",
          "form": "저도 빈속에 아메리카노 먹고 이것저것 넣었다가 어느 날 갑자기 속이 많이 쓰더라구요ㅠㅠ",
          "original_form": "저도 빈속에 아메리카노 먹고 이것저것 넣었다가 어느 날 갑자기 속이 많이 쓰더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045977.1.14",
          "form": "땡기거나 피곤하다 싶으면 마시는 편이에요",
          "original_form": "땡기거나 피곤하다 싶으면 마시는 편이에요",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045977.1.15",
          "form": "저도 카페인 잘들어서",
          "original_form": "저도 카페인 잘들어서",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045977.1.16",
          "form": "잠깰려구 마셔여ㅜㅜㅜㅜ",
          "original_form": "잠깰려구 마셔여ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045977.1.17",
          "form": "잠 깨는게 아니라 맛으로 천천히 여유롭게 즐길 수 있으면 더 좋을 거 같은데 아쉽네요ㅠㅠ",
          "original_form": "잠 깨는게 아니라 맛으로 천천히 여유롭게 즐길 수 있으면 더 좋을 거 같은데 아쉽네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045977.1.18",
          "form": "일할때는 어쩔수없어요 흑흑",
          "original_form": "일할때는 어쩔수없어요 흑흑",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100045977.1.19",
          "form": "일만 아니었어도 사람들은 여유롭게 즐길 텐데 진짜 너무 아쉬워요ㅠㅠ",
          "original_form": "일만 아니었어도 사람들은 여유롭게 즐길 텐데 진짜 너무 아쉬워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045977.1.20",
          "form": "그렇다고 회사에서 즐길 수도 없는 노릇이잖아요ㅠㅠ",
          "original_form": "그렇다고 회사에서 즐길 수도 없는 노릇이잖아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045977.1.21",
          "form": "맞아여ㅜㅜㅜㅜㅜ",
          "original_form": "맞아여ㅜㅜㅜㅜㅜ",
