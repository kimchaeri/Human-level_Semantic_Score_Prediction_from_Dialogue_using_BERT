      "utterance": [
        {
          "id": "MDRW2100044654.1.1",
          "form": "혼자사세요?",
          "original_form": "혼자사세요?",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044654.1.2",
          "form": "아뇨 가족들이랑 살아요",
          "original_form": "아뇨 가족들이랑 살아요",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044654.1.3",
          "form": "집안일 해요?",
          "original_form": "집안일 해요?",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044654.1.4",
          "form": "조금은요",
          "original_form": "조금은요",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044654.1.5",
          "form": "어떤거하세여",
          "original_form": "어떤거하세여",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044654.1.6",
          "form": "딱 정해선 아니고",
          "original_form": "딱 정해선 아니고",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044654.1.7",
          "form": "제가 할 수 있는 걸 그때그때 해요",
          "original_form": "제가 할 수 있는 걸 그때그때 해요",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044654.1.8",
          "form": "가끔만요",
          "original_form": "가끔만요",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044654.1.9",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044654.1.10",
          "form": "가끔이라면 가끔이고",
          "original_form": "가끔이라면 가끔이고",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044654.1.11",
          "form": "자주라면 자주..",
          "original_form": "자주라면 자주..",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044654.1.12",
          "form": "아 저희 식기세척기가 있어요",
          "original_form": "아 저희 식기세척기가 있어요",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044654.1.13",
          "form": "우왘ㅋㅋ ㅋ부럽다",
          "original_form": "우왘ㅋㅋ ㅋ부럽다",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044654.1.14",
          "form": "그럼 빨래?",
          "original_form": "그럼 빨래?",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044654.1.15",
          "form": "빨래는 세탁기랑 건조기가",
          "original_form": "빨래는 세탁기랑 건조기가",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044654.1.16",
          "form": "손빨래할 건 제 건 제가 하고요",
          "original_form": "손빨래할 건 제 건 제가 하고요",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044654.1.17",
          "form": "집안일 많이 하시나요?",
          "original_form": "집안일 많이 하시나요?",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044654.1.18",
          "form": "ㅋㅋㅋㅋ 저요..자취해서요",
          "original_form": "ㅋㅋㅋㅋ 저요..자취해서요",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044654.1.19",
          "form": "다해야해요..",
          "original_form": "다해야해요..",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044654.1.20",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044654.1.21",
          "form": "부러워용 ...",
          "original_form": "부러워용 ...",
          "speaker_id": "1",
          "time": "20210908 00:06"
        }
      ]
