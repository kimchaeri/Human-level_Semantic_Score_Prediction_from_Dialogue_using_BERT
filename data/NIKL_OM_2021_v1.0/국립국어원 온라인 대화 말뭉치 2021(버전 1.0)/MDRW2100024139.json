      "utterance": [
        {
          "id": "MDRW2100024139.1.1",
          "form": "지금도 아빠가 골프왕 틀어놓고 씻으러 갔어요",
          "original_form": "지금도 아빠가 골프왕 틀어놓고 씻으러 갔어요",
          "speaker_id": "1",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024139.1.2",
          "form": "요즘 볼게없으니 골프방송 틀어놓으세요",
          "original_form": "요즘 볼게없으니 골프방송 틀어놓으세요",
          "speaker_id": "1",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024139.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ골프왕",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ골프왕",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024139.1.4",
          "form": "골프 배워보고는 싶네요",
          "original_form": "골프 배워보고는 싶네요",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024139.1.5",
          "form": "지금 주제가 식음료인가요 영상,음악 인가뇨?",
          "original_form": "지금 주제가 식음료인가요 영상,음악 인가뇨?",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024139.1.6",
          "form": "식음료네요?",
          "original_form": "식음료네요?",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024139.1.7",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024139.1.8",
          "form": "몰랐는뎈ㅋㅋ",
          "original_form": "몰랐는뎈ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024139.1.9",
          "form": "요즘 추천 음료는 있으신가요?",
          "original_form": "요즘 추천 음료는 있으신가요?",
          "speaker_id": "1",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024139.1.10",
          "form": "아무것도 안눌렀는데 바뀌어버렸어요",
          "original_form": "아무것도 안눌렀는데 바뀌어버렸어요",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024139.1.11",
          "form": "저는 페퍼민트차를 마셔요",
          "original_form": "저는 페퍼민트차를 마셔요",
          "speaker_id": "1",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024139.1.12",
          "form": "소화에 좋답니다",
          "original_form": "소화에 좋답니다",
          "speaker_id": "1",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024139.1.13",
          "form": "음료를 잘 안마시는데 둥궁레 티백을 마시는걸 좋아합니다",
          "original_form": "음료를 잘 안마시는데 둥궁레 티백을 마시는걸 좋아합니다",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100024139.1.14",
          "form": "회사에서도 드시고 계시나여?",
          "original_form": "회사에서도 드시고 계시나여?",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024139.1.15",
          "form": "네에~ 날이 선선해지니까 따뜻한게 끌리더라고요",
          "original_form": "네에~ 날이 선선해지니까 따뜻한게 끌리더라고요",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024139.1.16",
          "form": "페퍼민트차가 소화에 좋다니 몰랐네요",
          "original_form": "페퍼민트차가 소화에 좋다니 몰랐네요",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024139.1.17",
          "form": "뱃속이 꾸룩꾸룩할때 좋아요",
          "original_form": "뱃속이 꾸룩꾸룩할때 좋아요",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024139.1.18",
          "form": "제가 좀 그렇거든요",
          "original_form": "제가 좀 그렇거든요",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024139.1.19",
          "form": "민트차는 거의 다 좋다고 하더라고요",
          "original_form": "민트차는 거의 다 좋다고 하더라고요",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024139.1.20",
          "form": "아하 잘 기억해두겠습니다",
          "original_form": "아하 잘 기억해두겠습니다",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024139.1.21",
          "form": "그런데 name1님.너무 졸립네요",
          "original_form": "그런데 &name1&님.너무 졸립네요",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024139.1.22",
          "form": "어이쿠 힘드신가봐요",
          "original_form": "어이쿠 힘드신가봐요",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024139.1.23",
          "form": "내일 카페인 많이 드시지 말고요",
          "original_form": "내일 카페인 많이 드시지 말고요",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024139.1.24",
          "form": "주무세요",
          "original_form": "주무세요",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024139.1.25",
          "form": "졸고있었네요 ㅎㅎ 내일은 조금 일찍 만나요",
          "original_form": "졸고있었네요 ㅎㅎ 내일은 조금 일찍 만나요",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024139.1.26",
          "form": "식사메뉴로 찾아뵐게요",
          "original_form": "식사메뉴로 찾아뵐게요",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024139.1.27",
          "form": "ㅋㅋ 조리법을 여쭤봐야겠네여",
          "original_form": "ㅋㅋ 조리법을 여쭤봐야겠네여",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024139.1.28",
          "form": "네에~ 급하게 나가 죄송해요 낼 만나요~",
          "original_form": "네에~ 급하게 나가 죄송해요 낼 만나요~",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024139.1.29",
          "form": "안녕 내일봬요",
          "original_form": "안녕 내일봬요",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024139.1.30",
          "form": "괜찮아여",
          "original_form": "괜찮아여",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024139.1.31",
          "form": "네에 주무세용",
          "original_form": "네에 주무세용",
          "speaker_id": "2",
          "time": "20210830 23:04"
        }
      ]
