      "utterance": [
        {
          "id": "MDRW2100046090.1.1",
          "form": "아눀ㅋㅋㅋ",
          "original_form": "아눀ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046090.1.2",
          "form": "저는 알바하기 바빠서",
          "original_form": "저는 알바하기 바빠서",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046090.1.3",
          "form": "저는 지금 고민 중이에요",
          "original_form": "저는 지금 고민 중이에요",
          "speaker_id": "1",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046090.1.4",
          "form": "동아리 할지 말지",
          "original_form": "동아리 할지 말지",
          "speaker_id": "1",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046090.1.5",
          "form": "어떤동아리요?",
          "original_form": "어떤동아리요?",
          "speaker_id": "2",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046090.1.6",
          "form": "만화동아리?",
          "original_form": "만화동아리?",
          "speaker_id": "1",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046090.1.7",
          "form": "전공이랑 같네요!",
          "original_form": "전공이랑 같네요!",
          "speaker_id": "2",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046090.1.8",
          "form": "잘하실거같은데??",
          "original_form": "잘하실거같은데??",
          "speaker_id": "2",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046090.1.9",
          "form": "과제도 많은데",
          "original_form": "과제도 많은데",
          "speaker_id": "1",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046090.1.10",
          "form": "동아리 열심히 할 수 있을지...",
          "original_form": "동아리 열심히 할 수 있을지...",
          "speaker_id": "1",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046090.1.11",
          "form": "음....다른동아리는",
          "original_form": "음....다른동아리는",
          "speaker_id": "2",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046090.1.12",
          "form": "관심없어요?",
          "original_form": "관심없어요?",
          "speaker_id": "2",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046090.1.13",
          "form": "딱히",
          "original_form": "딱히",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046090.1.14",
          "form": "공부 동아리?",
          "original_form": "공부 동아리?",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046090.1.15",
          "form": "왘ㅋㅋㅋ진짜 열심히시구나",
          "original_form": "왘ㅋㅋㅋ진짜 열심히시구나",
          "speaker_id": "2",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046090.1.16",
          "form": "동아리도 공부쪽으로!",
          "original_form": "동아리도 공부쪽으로!",
          "speaker_id": "2",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046090.1.17",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046090.1.18",
          "form": "할 만한게 없어서",
          "original_form": "할 만한게 없어서",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046090.1.19",
          "form": "꼭 안해도돼요",
          "original_form": "꼭 안해도돼요",
          "speaker_id": "2",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046090.1.20",
          "form": "하면재밌지만 안해도돼욬ㅋㅋㅋㅋ",
          "original_form": "하면재밌지만 안해도돼욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046090.1.21",
          "form": "제 친구들도",
          "original_form": "제 친구들도",
          "speaker_id": "1",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046090.1.22",
          "form": "안 하더라고요",
          "original_form": "안 하더라고요",
          "speaker_id": "1",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046090.1.23",
          "form": "제친구들도",
          "original_form": "제친구들도",
          "speaker_id": "2",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046090.1.24",
          "form": "그랬어요ㅋㅋㅋㅋ",
          "original_form": "그랬어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:18"
        }
      ]
