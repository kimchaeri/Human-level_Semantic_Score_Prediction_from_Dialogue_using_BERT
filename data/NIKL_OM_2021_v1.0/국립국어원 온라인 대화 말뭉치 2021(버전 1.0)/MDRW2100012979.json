      "utterance": [
        {
          "id": "MDRW2100012979.1.1",
          "form": "할인 소식이라해서 생각난건데",
          "original_form": "할인 소식이라해서 생각난건데",
          "speaker_id": "2",
          "time": "20210825 10:08"
        },
        {
          "id": "MDRW2100012979.1.2",
          "form": "지금 더쿠에서 바이린샵 이라는 곳이 할인한다고 엄청 사재끼고있어",
          "original_form": "지금 더쿠에서 바이린샵 이라는 곳이 할인한다고 엄청 사재끼고있어",
          "speaker_id": "2",
          "time": "20210825 10:08"
        },
        {
          "id": "MDRW2100012979.1.3",
          "form": "난 드가봤는데 뭐 그렇게 좋은지 싼지 모르겠던데",
          "original_form": "난 드가봤는데 뭐 그렇게 좋은지 싼지 모르겠던데",
          "speaker_id": "2",
          "time": "20210825 10:08"
        },
        {
          "id": "MDRW2100012979.1.4",
          "form": "괜찮은 사이트인가바",
          "original_form": "괜찮은 사이트인가바",
          "speaker_id": "2",
          "time": "20210825 10:09"
        },
        {
          "id": "MDRW2100012979.1.5",
          "form": "오 신기하네",
          "original_form": "오 신기하네",
          "speaker_id": "1",
          "time": "20210825 13:47"
        },
        {
          "id": "MDRW2100012979.1.6",
          "form": "나도 방금 드가봤는데",
          "original_form": "나도 방금 드가봤는데",
          "speaker_id": "1",
          "time": "20210825 13:47"
        },
        {
          "id": "MDRW2100012979.1.7",
          "form": "트렌치코트가 130만원인데",
          "original_form": "트렌치코트가 130만원인데",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100012979.1.8",
          "form": "여기맞나...?",
          "original_form": "여기맞나...?",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100012979.1.9",
          "form": "헐....???",
          "original_form": "헐....???",
          "speaker_id": "2",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100012979.1.10",
          "form": "뭐노",
          "original_form": "뭐노",
          "speaker_id": "2",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100012979.1.11",
          "form": "세일중인거 맞나",
          "original_form": "세일중인거 맞나",
          "speaker_id": "2",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100012979.1.12",
          "form": "더쿠에는 다들 부자들만 사나....",
          "original_form": "더쿠에는 다들 부자들만 사나....",
          "speaker_id": "2",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100012979.1.13",
          "form": "돈많네...",
          "original_form": "돈많네...",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100012979.1.14",
          "form": "이런부자사이트는 소외감만느껴져...",
          "original_form": "이런부자사이트는 소외감만느껴져...",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100012979.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100012979.1.16",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100012979.1.17",
          "form": "싼거보니 5만원짜리 니트도 있긴하네",
          "original_form": "싼거보니 5만원짜리 니트도 있긴하네",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100012979.1.18",
          "form": "나는 지금 최저시급도 안되는",
          "original_form": "나는 지금 최저시급도 안되는",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100012979.1.19",
          "form": "심심이 노동중인데!",
          "original_form": "심심이 노동중인데!",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100012979.1.20",
          "form": "그런거보고 좋다고하는건가?",
          "original_form": "그런거보고 좋다고하는건가?",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100012979.1.21",
          "form": "5만원 짜리가 그나마 싼거구나...",
          "original_form": "5만원 짜리가 그나마 싼거구나...",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100012979.1.22",
          "form": "너는지금 일하는도중아님?",
          "original_form": "너는지금 일하는도중아님?",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100012979.1.23",
          "form": "그렇겠지?? 더쿠가 130만원 짜리 지를 그런 사이트도 아니구...",
          "original_form": "그렇겠지?? 더쿠가 130만원 짜리 지를 그런 사이트도 아니구...",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100012979.1.24",
          "form": "일함+심심이노동중=최저시급향상",
          "original_form": "일함+심심이노동중=최저시급향상",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100012979.1.25",
          "form": "나 수업중인데 쉬는시간인거야!!",
          "original_form": "나 수업중인데 쉬는시간인거야!!",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100012979.1.26",
          "form": "바주라 ㅋㅋ",
          "original_form": "바주라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100012979.1.27",
          "form": "ㅋㅋㅋ하긴 그럼 그냥 저 쇼핑몰이 비싼 것이구나",
          "original_form": "ㅋㅋㅋ하긴 그럼 그냥 저 쇼핑몰이 비싼 것이구나",
          "speaker_id": "1",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100012979.1.28",
          "form": "흥...",
          "original_form": "흥...",
          "speaker_id": "1",
