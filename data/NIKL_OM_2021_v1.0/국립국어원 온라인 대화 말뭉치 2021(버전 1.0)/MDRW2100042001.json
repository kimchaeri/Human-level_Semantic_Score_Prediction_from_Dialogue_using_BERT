      "utterance": [
        {
          "id": "MDRW2100042001.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100042001.1.2",
          "form": "아니요 아직ㅠㅠ 뭐 먹을까 고민하고 있어요",
          "original_form": "아니요 아직ㅠㅠ 뭐 먹을까 고민하고 있어요",
          "speaker_id": "2",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100042001.1.3",
          "form": "님은 드셨나요?",
          "original_form": "님은 드셨나요?",
          "speaker_id": "2",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100042001.1.4",
          "form": "넹 부대찌개 먹었어요",
          "original_form": "넹 부대찌개 먹었어요",
          "speaker_id": "1",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100042001.1.5",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100042001.1.6",
          "form": "배달 시킨건가요?",
          "original_form": "배달 시킨건가요?",
          "speaker_id": "2",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100042001.1.7",
          "form": "포장해왔어요!",
          "original_form": "포장해왔어요!",
          "speaker_id": "1",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100042001.1.8",
          "form": "아하! 점심은 뭐 드셨나요??",
          "original_form": "아하! 점심은 뭐 드셨나요??",
          "speaker_id": "2",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100042001.1.9",
          "form": "점심은 스벅에서 샌드위치랑 피지오 사왔어요",
          "original_form": "점심은 스벅에서 샌드위치랑 피지오 사왔어요",
          "speaker_id": "1",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100042001.1.10",
          "form": "와 맛있겠다",
          "original_form": "와 맛있겠다",
          "speaker_id": "2",
          "time": "20210906 18:36"
        },
        {
          "id": "MDRW2100042001.1.11",
          "form": "저는 수업때문에 점심 못 먹었어요ㅠㅠ",
          "original_form": "저는 수업때문에 점심 못 먹었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:36"
        },
        {
          "id": "MDRW2100042001.1.12",
          "form": "그래서 저녁은 마라탕 먹으려구요",
          "original_form": "그래서 저녁은 마라탕 먹으려구요",
          "speaker_id": "2",
          "time": "20210906 18:36"
        },
        {
          "id": "MDRW2100042001.1.13",
          "form": "헉 맛있겠다",
          "original_form": "헉 맛있겠다",
          "speaker_id": "1",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100042001.1.14",
          "form": "매운거 잘 드세요?",
          "original_form": "매운거 잘 드세요?",
          "speaker_id": "1",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100042001.1.15",
          "form": "아뇨ㅠㅠ 맨날 1단계 먹어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아뇨ㅠㅠ 맨날 1단계 먹어요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100042001.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:47"
        },
        {
          "id": "MDRW2100042001.1.17",
          "form": "뭐 넣어 드실 건가요",
          "original_form": "뭐 넣어 드실 건가요",
          "speaker_id": "1",
          "time": "20210906 18:47"
        },
        {
          "id": "MDRW2100042001.1.18",
          "form": "저는 목이버섯 많이 넣어요!!",
          "original_form": "저는 목이버섯 많이 넣어요!!",
          "speaker_id": "2",
          "time": "20210906 18:48"
        },
        {
          "id": "MDRW2100042001.1.19",
          "form": "맛있겠어요...... 건두부도 넣어 주세요...",
          "original_form": "맛있겠어요...... 건두부도 넣어 주세요...",
          "speaker_id": "1",
          "time": "20210906 18:58"
        },
        {
          "id": "MDRW2100042001.1.20",
          "form": "건두부 당연히 넣었죠!!",
          "original_form": "건두부 당연히 넣었죠!!",
