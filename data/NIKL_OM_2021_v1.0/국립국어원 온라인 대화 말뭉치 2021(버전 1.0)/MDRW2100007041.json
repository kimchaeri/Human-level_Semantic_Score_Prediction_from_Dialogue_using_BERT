      "utterance": [
        {
          "id": "MDRW2100007041.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007041.1.2",
          "form": "안녕하세요 직장인이신가요?",
          "original_form": "안녕하세요 직장인이신가요?",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007041.1.3",
          "form": "아니요 이직준비중이에요!",
          "original_form": "아니요 이직준비중이에요!",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007041.1.4",
          "form": "이제 곧 백수 입니다!",
          "original_form": "이제 곧 백수 입니다!",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007041.1.5",
          "form": "직장인이신가요?",
          "original_form": "직장인이신가요?",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007041.1.6",
          "form": "아 더 좋은데 가시려나보네요!",
          "original_form": "아 더 좋은데 가시려나보네요!",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007041.1.7",
          "form": "저는 직장인입니당",
          "original_form": "저는 직장인입니당",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007041.1.8",
          "form": "네 더 좋은 곳에 가고 싶어요 하하",
          "original_form": "네 더 좋은 곳에 가고 싶어요 하하",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007041.1.9",
          "form": "요즘 일은 어떠세요?",
          "original_form": "요즘 일은 어떠세요?",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007041.1.10",
          "form": "이번주까지 쉬어서 담주에 나가려니 막막하네요ㅠ",
          "original_form": "이번주까지 쉬어서 담주에 나가려니 막막하네요ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007041.1.11",
          "form": "휴가셨나보군요! 쉬다가 다시 일하려면 힘들죠ㅠㅠㅋㅋ",
          "original_form": "휴가셨나보군요! 쉬다가 다시 일하려면 힘들죠ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007041.1.12",
          "form": "그래서 지금 잠도 못주무시나봐요",
          "original_form": "그래서 지금 잠도 못주무시나봐요",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007041.1.13",
          "form": "그쵸 이직은 지금업무랑 비슷한곳으로 가시려는건가요?",
          "original_form": "그쵸 이직은 지금업무랑 비슷한곳으로 가시려는건가요?",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007041.1.14",
          "form": "아니요 완전 다른 곳으로 갈 예정이에요 지금 일이 너무 힘들어서요ㅋㅋ",
          "original_form": "아니요 완전 다른 곳으로 갈 예정이에요 지금 일이 너무 힘들어서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007041.1.15",
          "form": "휴가는 잘 보내셨나요? 코로나 때문에 놀러가기도 힘드셨죠?",
          "original_form": "휴가는 잘 보내셨나요? 코로나 때문에 놀러가기도 힘드셨죠?",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007041.1.16",
          "form": "코로나 땜에 계속 있었어요 예전에 대중교통땜에 격리한적 있어서요ㅠ",
          "original_form": "코로나 땜에 계속 있었어요 예전에 대중교통땜에 격리한적 있어서요ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007041.1.17",
          "form": "정말 고생 많으셨겠어요ㅠㅠ 코로나 때문에 휴가가 휴가 같지도 않네요",
          "original_form": "정말 고생 많으셨겠어요ㅠㅠ 코로나 때문에 휴가가 휴가 같지도 않네요",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007041.1.18",
          "form": "전 재택업무 중이라서 집에서 넷플릭스랑 왓챠만 봤어요",
          "original_form": "전 재택업무 중이라서 집에서 넷플릭스랑 왓챠만 봤어요",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007041.1.19",
          "form": "휴가 때 뭐 하셨어요?",
          "original_form": "휴가 때 뭐 하셨어요?",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007041.1.20",
          "form": "음ㅎ 집에서 계속 티비만본거같아요 재택업무면 출퇴근은 유연한가요?",
          "original_form": "음ㅎ 집에서 계속 티비만본거같아요 재택업무면 출퇴근은 유연한가요?",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007041.1.21",
          "form": "그렇진 않아요 회사에서 나오라면 나와야해서요",
          "original_form": "그렇진 않아요 회사에서 나오라면 나와야해서요",
          "speaker_id": "1",
          "time": "20210822 00:34"
        }
      ]
